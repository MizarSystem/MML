begin

definition
  let X be non empty set;
  let Y be RealNormSpace;
  let IT be Function of X, the carrier of Y;
  attr IT is bounded means :Def4:
  ex K being Real st 0 <= K &
   for x being Element of X holds
   ||. IT.x .|| <= K;
end;

theorem Th6:
  for X be non empty set
    for Y be RealNormSpace holds
      for f be Function of X,the carrier of Y
         st (for x be Element of X holds f.x=0.Y)
        holds f is bounded
 proof
    let X be non empty set; let Y be RealNormSpace;
    let f be Function of X,the carrier of Y such that
        A1: for x be Element of X holds f.x=0.Y;
    A2:
      now
        let x be Element of X;
        thus ||. f.x .|| = ||. 0.Y .|| by A1
         .=0;
       end;
     take 0;
     thus thesis by A2;
  end;

registration
  let X be non empty set;
  let Y be RealNormSpace;
  cluster bounded Function of X,the carrier of Y;
existence
proof
  set f=X --> 0.Y;
  reconsider f as Function of X,the carrier of Y by FUNCOP_1:58;
  take f;
      for x be Element of X holds f.x =0.Y by FUNCOP_1:13;
  hence thesis by Th6;
 end;
end;

definition
  let X be non empty set;
  let Y be RealNormSpace;
  func BoundedFunctions(X,Y) ->
    Subset of RealVectSpace(X,Y) means :Def5:
   for x being set holds x in it
     iff
   x is bounded Function of X,the carrier of Y;
existence
proof
  defpred P[set] means $1 is bounded Function of X,the carrier of Y;
  consider IT being set such that
  A1:for x being set holds x in IT
    iff x in Funcs(X,the carrier of Y)  & P[x] from XBOOLE_0:sch 1;
A2:  IT is Subset of Funcs(X,the carrier of Y)
proof
         for x be set st x in IT holds x
     in Funcs(X,the carrier of Y) by A1;
     hence thesis by TARSKI:def 3;
   end;
  A3: the carrier of RealVectSpace(X,Y) = Funcs(X,the carrier of Y)
  proof
      RealVectSpace(X,Y) = RLSStruct(#Funcs(X,the carrier of Y),
              (FuncZero(X,Y)),FuncAdd(X,Y),FuncExtMult(X,Y)#)
              by LOPBAN_1:def 4;
  hence thesis;
  end;
  take IT;
  thus IT is Subset of RealVectSpace(X,Y) by A2,A3;
  let x be set;
  thus x in IT implies x is bounded Function of X,the carrier of Y by A1;
 assume
A4:  x is bounded Function of X,the carrier of Y;
  then x in Funcs(X,the carrier of Y) by FUNCT_2:11;
  hence thesis by A1,A4;
end;
uniqueness
proof
   let X1,X2 be Subset of RealVectSpace(X,Y);
 assume that
A5: for x being set holds x in X1 iff
     x is bounded Function of X,the carrier of Y and
A6: for x being set holds x in X2 iff
     x is bounded Function of X,the carrier of Y;
        for x being set st x in X1 holds x in X2
    proof
      let x be set;
    assume x in X1;
      then x is bounded Function of X,the carrier of Y by A5;
      hence thesis by A6;
    end;
then A7: X1 c= X2 by TARSKI:def 3;
        for x being set st x in X2 holds x in X1
    proof
      let x be set;
      assume x in X2;
      then x is bounded Function of X,the carrier of Y by A6;
      hence thesis by A5;
    end;
    then X2 c= X1 by TARSKI:def 3;
    hence thesis by A7,XBOOLE_0:def 10;
  end;
end;

registration
  let X be non empty set;
  let Y be RealNormSpace;
  cluster BoundedFunctions(X,Y) -> non empty;
  coherence
   proof
     set f=X --> 0.Y;
     reconsider f as  Function of X,the carrier of Y
     by FUNCOP_1:58;
         f is bounded
     proof
           for x be Element of X holds f.x =0.Y by FUNCOP_1:13;
       hence thesis by Th6;
     end;
     hence thesis by Def5;
   end;
end;

theorem Th7:
  for X be non empty set for Y be RealNormSpace holds
  BoundedFunctions(X,Y) is linearly-closed
proof
  let X be non empty set; let Y be RealNormSpace;
  set W = BoundedFunctions(X,Y);
A1:  RealVectSpace(X,Y) =
  RLSStruct(#Funcs(X,the carrier of Y),
              (FuncZero(X,Y)),FuncAdd(X,Y),FuncExtMult(X,Y)#)
              by LOPBAN_1:def 4;
A2:for v,u be VECTOR of RealVectSpace(X,Y)  st
    v in W & u in W
    holds v + u in W
   proof
     let v,u be VECTOR of RealVectSpace(X,Y)  such that
     A3: v in W & u in W;
     reconsider f=v+u as Function of X,the carrier of Y
     by A1,FUNCT_2:121;
          f is bounded
     proof
     reconsider u1=u as bounded Function of X,
     the carrier of Y by A3,Def5;
     reconsider v1=v as bounded Function of X,
     the carrier of Y by A3,Def5;
     consider K1 be Real such that
       A4: 0 <= K1 &
     for x be Element of X holds ||. u1.x .||   <= K1 by Def4;
     consider K2 be Real such that
       A5: 0 <= K2 &
     for x be Element of X holds ||. v1.x .|| <= K2 by Def4;
     take K3=K1+K2;
         A6: 0 + 0 <= K1+K2 by A4,A5,XREAL_1:9;
           now let x be Element of X;
        A7: ||. f.x .|| =||. v1.x+u1.x .|| by LOPBAN_1:14;
        A8: ||. u1.x+v1.x .|| <= ||. u1.x .||+ ||. v1.x .|| by NORMSP_1:def 2;
        A9:||. u1.x .|| <= K1 by A4;
           ||. v1.x .|| <= K2 by A5;
        then ||. u1.x .|| + ||. v1.x .|| <= K1 +K2 by A9,XREAL_1:9;
       hence ||. f.x .|| <= K3 by A7,A8,XXREAL_0:2;
        end;
     hence thesis by A6;
     end;
     hence thesis by Def5;
  end;

    for a be Real for v be VECTOR of RealVectSpace(X,Y)
    st v in W holds a * v in W
    proof
     let a be Real;
     let v be VECTOR of RealVectSpace(X,Y) such that
           A10: v in W;
          reconsider f=a*v as Function of X,the carrier of Y by A1,FUNCT_2:121;
              f is bounded
          proof
     reconsider v1=v as bounded Function of X,
     the carrier of Y by A10,Def5;
     consider K be Real such that
       A11: 0 <= K &
        for x be Element of X holds ||. v1.x .|| <= K by Def4;
      take K1=abs(a)*K;
      A12: 0 <= K & 0 <=abs(a) by A11,COMPLEX1:132;
          now let x be Element of X;
        A13: ||. f.x .|| =||. a*v1.x .|| by LOPBAN_1:15;
        A14: ||. a*v1.x .|| = abs(a)* ||. v1.x .|| by NORMSP_1:def 2;
        A15:||. v1.x .|| <= K by A11;
            0 <=abs(a) by COMPLEX1:132;
       hence ||. f.x .|| <= abs(a)* K by A13,A14,A15,XREAL_1:66;
     end;
     hence thesis by A12,REAL_2:121;
     end;
     hence thesis by Def5;
     end;
 hence thesis by A2,RLSUB_1:def 1;
end;

theorem Th8:
  for X be non empty set for Y be RealNormSpace holds
RLSStruct (# BoundedFunctions(X,Y),
        Zero_(BoundedFunctions(X,Y),
        RealVectSpace(X,Y)),
         Add_(BoundedFunctions(X,Y),
         RealVectSpace(X,Y)),
        Mult_(BoundedFunctions(X,Y),
        RealVectSpace(X,Y)) #)
is Subspace of RealVectSpace(X,Y) by Th7,RSSPACE:13;

registration
  let X be non empty set;
  let Y be RealNormSpace;
  cluster RLSStruct (# BoundedFunctions(X,Y),
        Zero_(BoundedFunctions(X,Y),
        RealVectSpace(X,Y)),
         Add_(BoundedFunctions(X,Y),
         RealVectSpace(X,Y)),
        Mult_(BoundedFunctions(X,Y),
        RealVectSpace(X,Y)) #)
 -> Abelian add-associative right_zeroed right_complementable
        RealLinearSpace-like;
  coherence by Th8;
end;

definition
  let X be non empty set;
  let Y be RealNormSpace;
  func R_VectorSpace_of_BoundedFunctions(X,Y) -> RealLinearSpace equals

 RLSStruct (# BoundedFunctions(X,Y),
        Zero_(BoundedFunctions(X,Y),
        RealVectSpace(X,Y)),
         Add_(BoundedFunctions(X,Y),
         RealVectSpace(X,Y)),
        Mult_(BoundedFunctions(X,Y),
        RealVectSpace(X,Y)) #);
  coherence;
end;

registration
  let X be non empty set; let Y be RealNormSpace;
  cluster R_VectorSpace_of_BoundedFunctions(X,Y) -> strict;
  coherence;
end;

canceled;

theorem Th10:
     for X be non empty set for  Y be RealNormSpace
     for f,g,h be VECTOR of R_VectorSpace_of_BoundedFunctions(X,Y)
       for f',g',h' be bounded Function of X,the carrier of Y
        st f'=f & g'=g & h'=h holds
      (h = f+g iff (for x be Element of X holds (h'.x = f'.x + g'.x)) )
proof
     let X be non empty set;
     let Y be RealNormSpace;
     let f,g,h be VECTOR of R_VectorSpace_of_BoundedFunctions(X,Y);
     let f',g',h' be bounded Function of X,the carrier of Y such that
       A1: f'=f and
       A2: g'=g and
       A3: h'=h;
      A4:R_VectorSpace_of_BoundedFunctions(X,Y) is
           Subspace of RealVectSpace(X,Y) by Th8;
        then reconsider f1=f as VECTOR
           of RealVectSpace(X,Y) by RLSUB_1:18;
       reconsider g1=g as VECTOR
          of RealVectSpace(X,Y) by A4,RLSUB_1:18;
       reconsider h1=h as VECTOR
         of RealVectSpace(X,Y) by A4,RLSUB_1:18;
       reconsider f1'=f', g1'=g', h1'=h' as Element
            of Funcs(X, the carrier of Y) by FUNCT_2:11;
      A5: now assume
            A6: h = f+g;
             let x be Element of X;
                 h1=f1+g1 by A4,A6,RLSUB_1:21;
             hence h'.x=f'.x+g'.x by A1,A2,A3,LOPBAN_1:14;
    end;
        now assume
                for x be Element of X holds h'.x=f'.x+g'.x;
           then h1=f1+g1 by A1,A2,A3,LOPBAN_1:14;
           hence h =f+g by A4,RLSUB_1:21;
     end;
     hence thesis by A5;
 end;

theorem Th11:
     for X be non empty set for  Y be RealNormSpace
     for f,h be VECTOR of R_VectorSpace_of_BoundedFunctions(X,Y)
     for f',h' be bounded Function of X,the carrier of Y
         st f'=f & h'=h
     for a be Real holds
      h = a*f iff for x be Element of X holds h'.x = a*f'.x
  proof
     let X be non empty set;
     let Y be RealNormSpace;
     let f,h be VECTOR of R_VectorSpace_of_BoundedFunctions(X,Y);
     let f',h' be bounded Function of X,the carrier of Y such that
       A1: f'=f and
       A2: h'=h;
     let a be Real;
      A3:R_VectorSpace_of_BoundedFunctions(X,Y) is
           Subspace of RealVectSpace(X,Y) by Th8;
        then reconsider f1=f, h1=h as VECTOR
                   of RealVectSpace(X,Y) by RLSUB_1:18;
       reconsider f1'=f', h1'=h' as Element
            of Funcs(X, the carrier of Y) by FUNCT_2:11;
      A4: now assume
            A5: h = a*f;
             let x be Element of X;
                 h1=a*f1 by A3,A5,RLSUB_1:22;
             hence h'.x=a*f'.x by A1,A2,LOPBAN_1:15;
    end;
         now assume
                for x be Element of X holds h'.x=a*f'.x;
           then h1=a*f1 by A1,A2,LOPBAN_1:15;
           hence h =a*f by A3,RLSUB_1:22;
     end;
     hence thesis by A4;
 end;

theorem Th12:
     for X be non empty set for Y be RealNormSpace holds
          0.R_VectorSpace_of_BoundedFunctions(X,Y)
          =(X -->0.Y)
     proof
     let X be non empty set; let Y be RealNormSpace;
      A1:R_VectorSpace_of_BoundedFunctions(X,Y) is
           Subspace of RealVectSpace(X,Y) by Th8;
         0.RealVectSpace(X,Y)
          =(X -->0.Y) by LOPBAN_1:16;
     hence thesis by A1,RLSUB_1:19;
    end;

definition
  let X be non empty set; let Y be RealNormSpace;
  let f be set such that
  A1: f in BoundedFunctions(X,Y);
  func modetrans(f,X,Y)
  -> bounded Function of X,the carrier of Y equals:Def7:
    f;
  coherence by A1,Def5;
end;

definition
   let X be non empty set; let Y be RealNormSpace;
   let u be Function of X,the carrier of Y;
   func PreNorms(u) -> non empty Subset of REAL equals
   {||.u.t.|| where t is Element of X : not contradiction  };
 coherence
 proof
   A1: now let x be set;
             now assume
               x in {||.u.t.|| where t is Element of X
           : not contradiction };
           then consider t be Element of X such that
            A2:   x=||.u.t.||;
          thus x in REAL by A2;
         end;
         hence x in {||.u.t.|| where t is Element of X
         : not contradiction } implies x in REAL;
       end;
     consider x be Element of X;
        ||.u.x.|| in {||.u.t.|| where t is Element of X
    : not contradiction };
    hence thesis by A1,TARSKI:def 3;
  end;
end;

theorem Th13:
    for X be non empty set for Y be RealNormSpace
    for g be bounded Function of X,the carrier of Y
      holds PreNorms(g) is non empty bounded_above
   proof
       let X be non empty set; let Y be RealNormSpace;
       let g be bounded Function of X,the carrier of Y;
       PreNorms(g) is bounded_above
   proof
  consider K be Real such that
         0 <= K and
    A1: for x be Element of X holds
       ||. g.x .|| <= K by Def4;
  A2:now let r be Real such that
        A3:  r in PreNorms(g);
       consider t be Element of X such that
          A4:   r=||.g.t.|| by A3;
        thus r <=K by A1,A4;
      end;
   take K;
   thus thesis by A2;
   end;
 hence thesis;
 end;

theorem
        for X be non empty set for Y be RealNormSpace
    for g be Function of X,the carrier of Y
      holds g is bounded iff PreNorms(g) is bounded_above
   proof
       let X be non empty set; let Y be RealNormSpace;
       let g be Function of X,the carrier of Y;

     now assume
    A1: PreNorms(g) is bounded_above;
         reconsider K=sup PreNorms(g) as Real;
    A2:  0 <= K
    proof
    A3:now let r be Real such that
        A4: r in PreNorms(g);
        consider t be Element of X such that
          A5: r=||.g.t.|| by A4;
          thus 0 <= r by A5,NORMSP_1:8;
    end;
  consider r0 be set such that
  A6: r0 in PreNorms(g) by XBOOLE_0:def 1;
  reconsider r0 as Real by A6;
      0 <= r0 by A3,A6;
    hence thesis by A1,A6,SEQ_4:def 4;
    end;
    A7: now let t be Element of X;
           ||.g.t.|| in PreNorms(g);
          hence ||.g.t.|| <= K by A1,SEQ_4:def 4;
        end;
 take K;
 thus g is bounded by A2,A7,Def4;
 end;
hence thesis by Th13;
end;

theorem Th15:
   for X be non empty set for Y be RealNormSpace
    ex NORM be Function of BoundedFunctions(X,Y),REAL st
          for f be set st f in BoundedFunctions(X,Y) holds
        NORM.f = sup PreNorms(modetrans(f,X,Y))
proof
   let X be non empty set; let Y be RealNormSpace;
  deffunc F(set) = sup PreNorms(modetrans($1,X,Y));
A1:for z be set st z in BoundedFunctions(X,Y) holds F(z) in REAL;
       ex f being Function of BoundedFunctions(X,Y),REAL st
     for x being set st x in BoundedFunctions(X,Y) holds
       f.x = F(x) from FUNCT_2:sch 2(A1);
   hence thesis;
end;

definition
   let X be non empty set; let Y be RealNormSpace;
   func BoundedFunctionsNorm(X,Y)
     -> Function of BoundedFunctions(X,Y), REAL means :Def9:
  for x be set st x in BoundedFunctions(X,Y) holds
    it.x = sup PreNorms(modetrans(x,X,Y));
existence by Th15;
uniqueness
proof
   let NORM1,NORM2 be Function of BoundedFunctions(X,Y), REAL
   such that
A1: (for x be set st x in BoundedFunctions(X,Y) holds
        NORM1.x = sup PreNorms(modetrans(x,X,Y))) and
A2:(for x be set st x in BoundedFunctions(X,Y) holds
     NORM2.x = sup PreNorms(modetrans(x,X,Y)));
A3:dom NORM1 = BoundedFunctions(X,Y) &
     dom NORM2 = BoundedFunctions(X,Y) by FUNCT_2:def 1;
       for z be set st z in BoundedFunctions(X,Y) holds NORM1.z = NORM2.z
   proof
     let z be set such that
A4:  z in BoundedFunctions(X,Y);
         NORM1.z = sup PreNorms(modetrans(z,X,Y)) by A1,A4;
     hence thesis by A2,A4;
  end;
  hence thesis by A3,FUNCT_1:9;
end;
end;

theorem Th16:
   for X be non empty set for Y be RealNormSpace
   for f be bounded Function of X,the carrier of Y holds modetrans(f,X,Y)=f
  proof
     let X be non empty set; let Y be RealNormSpace;
     let f be bounded Function of X,the carrier of Y;
         f in BoundedFunctions(X,Y) by Def5;
     hence thesis by Def7;
  end;

theorem Th17:
   for X be non empty set for Y be RealNormSpace
   for f be bounded Function of X,the carrier of Y holds
   BoundedFunctionsNorm(X,Y).f = sup PreNorms(f)
  proof
       let X be non empty set;let Y be RealNormSpace;
       let f be bounded Function of X,the carrier of Y;
       A1: f in BoundedFunctions(X,Y) by Def5;
      reconsider f'=f as set;
      thus BoundedFunctionsNorm(X,Y).f
        = sup PreNorms(modetrans(f',X,Y)) by A1,Def9
       .= sup PreNorms(f) by Th16;
  end;

definition let X be non empty set;let Y be RealNormSpace;
 func R_NormSpace_of_BoundedFunctions(X,Y) -> non empty NORMSTR equals
   NORMSTR (# BoundedFunctions(X,Y),
        Zero_(BoundedFunctions(X,Y),
        RealVectSpace(X,Y)),
         Add_(BoundedFunctions(X,Y),
         RealVectSpace(X,Y)),
          Mult_(BoundedFunctions(X,Y),
          RealVectSpace(X,Y)),
          BoundedFunctionsNorm(X,Y) #);
  coherence;
end;

theorem Th18:
    for X be non empty set for Y be RealNormSpace
       holds (X --> 0.Y) =
        0.R_NormSpace_of_BoundedFunctions(X,Y)
 proof
     let X be non empty set; let Y be RealNormSpace;
              (X --> 0.Y)
            =0.R_VectorSpace_of_BoundedFunctions(X,Y) by Th12
            .=0.R_NormSpace_of_BoundedFunctions(X,Y);
  hence thesis;
end;

theorem Th19:
    for X be non empty set for Y be RealNormSpace
    for f being Point of
       R_NormSpace_of_BoundedFunctions(X,Y)
    for g be bounded Function of X,the carrier of Y st g=f holds
      for t be Element of X holds ||.g.t.|| <= ||.f.||
   proof
       let X be non empty set; let Y be RealNormSpace;
       let f being Point of
          R_NormSpace_of_BoundedFunctions(X,Y);
      let g be bounded Function of X,the carrier of Y such that
     A1:g=f;
 A2: PreNorms(g) is non empty bounded_above by Th13;
        now let t be Element of X;
           ||.g.t.|| in PreNorms(g);
          then ||.g.t.|| <= sup PreNorms(g) by A2,SEQ_4:def 4;
           hence ||.g.t.|| <= ||.f.|| by A1,Th17;
    end;
    hence thesis;
end;

theorem
        for X be non empty set for Y be RealNormSpace
    for f being Point of
       R_NormSpace_of_BoundedFunctions(X,Y)
    holds 0 <= ||.f.||
proof
       let X be non empty set; let Y be RealNormSpace;
       let f being Point of
          R_NormSpace_of_BoundedFunctions(X,Y);
    reconsider g=f as bounded Function of X,the carrier of Y by Def5;
 A1: now let r be Real such that
        A2:  r in PreNorms(g);
        consider t be Element of X such that
          A3: r=||.g.t.|| by A2;
          thus 0 <= r by A3,NORMSP_1:8;
 end;
A4:PreNorms(g) is non empty bounded_above by Th13;
 consider r0 be set such that
  A5: r0 in PreNorms(g) by XBOOLE_0:def 1;
  reconsider r0 as Real by A5;
      0 <= r0 by A1,A5;
  then  0 <=sup PreNorms(g) by A4,A5,SEQ_4:def 4;
    hence thesis by Th17;
  end;

theorem Th21:
    for X be non empty set for Y be RealNormSpace
    for f being Point of
       R_NormSpace_of_BoundedFunctions(X,Y)
       st f = 0.R_NormSpace_of_BoundedFunctions(X,Y)
    holds 0 = ||.f.||
proof
    let X be non empty set; let Y be RealNormSpace;
    let f being Point of
       R_NormSpace_of_BoundedFunctions(X,Y) such that
A1:       f = 0.R_NormSpace_of_BoundedFunctions(X,Y);
  thus ||.f.|| = 0
    proof
        set z = X --> 0.Y;
        reconsider z as Function of X, the carrier of Y by FUNCOP_1:58;
        reconsider g=f as bounded Function of X,
        the carrier of Y by Def5;
       A2: z=g by A1,Th18;
       A3:
      now let r be Real such that
        A4:  r in PreNorms(g);
        consider t be Element of X such that
          A5:   r=||.g.t.|| by A4;
              ||.g.t.|| = ||.0.Y.|| by A2,FUNCOP_1:13
                  .= 0;
          hence 0 <= r & r <=0 by A5;
      end;
     A6:PreNorms(g) is non empty bounded_above by Th13;
     consider r0 be set such that
      A7: r0 in PreNorms(g) by XBOOLE_0:def 1;
     reconsider r0 as Real by A7;
     A8: 0 <= r0 & r0<=0 by A3,A7;
         (for s be real number st s in PreNorms(g) holds s <= 0)
         implies sup PreNorms(g) <= 0 by SEQ_4:62;
       then sup PreNorms(g) = 0 by A3,A6,A7,A8,SEQ_4:def 4;
      hence thesis by Th17;
      end;
end;

theorem Th22:
     for X be non empty set for Y be RealNormSpace
     for f,g,h be Point of R_NormSpace_of_BoundedFunctions(X,Y)
       for f',g',h' be bounded Function of X,the carrier of Y
        st f'=f & g'=g & h'=h holds
      (h = f+g iff
          (for x be Element of X holds (h'.x = f'.x + g'.x)) )
proof
     let X be non empty set;let Y be RealNormSpace;
     let f,g,h be Point of R_NormSpace_of_BoundedFunctions(X,Y);
     let f',g',h' be bounded Function of X,the carrier of Y such that
       A1: f'=f and
       A2: g'=g and
       A3: h'=h;
 reconsider f1=f, g1=g, h1=h as VECTOR of
 R_VectorSpace_of_BoundedFunctions(X,Y);
     h=f+g iff h1=f1+g1;
hence thesis by A1,A2,A3,Th10;
end;

theorem Th23:
     for X be non empty set for Y be RealNormSpace
     for f,h be Point of R_NormSpace_of_BoundedFunctions(X,Y)
     for f',h' be bounded Function of X,the carrier of Y st f'=f & h'=h
     for a be Real holds
      h = a*f iff for x be Element of X holds h'.x = a*f'.x
proof
     let X be non empty set; let Y be RealNormSpace;
     let f,h be Point of
     R_NormSpace_of_BoundedFunctions(X,Y);
     let f',h' be bounded Function of X,the carrier of Y such that
       A1: f'=f and
       A2: h'=h;
     let a be Real;
 reconsider f1=f as VECTOR of
 R_VectorSpace_of_BoundedFunctions(X,Y);
 reconsider h1=h as VECTOR of
 R_VectorSpace_of_BoundedFunctions(X,Y);
    h=a*f iff h1=a*f1;
hence thesis by A1,A2,Th11;
end;

theorem Th24:
    for X be non empty set
    for Y be RealNormSpace
    for f, g being Point of
       R_NormSpace_of_BoundedFunctions(X,Y)
       for a be Real holds
  ( ||.f.|| = 0 iff
  f = 0.R_NormSpace_of_BoundedFunctions(X,Y) ) &
  ||.a*f.|| = abs(a) * ||.f.|| &
  ||.f+g.|| <= ||.f.|| + ||.g.||
proof
    let X be non empty set;
    let Y be RealNormSpace;
    let f, g being Point of R_NormSpace_of_BoundedFunctions(X,Y);
    let a be Real;
A1:
now assume
   A2: ||.f.|| = 0;
   reconsider g=f as bounded Function of X,the carrier of Y by Def5;
   set z = X --> 0.Y;
   reconsider z as Function of X, the carrier of Y by FUNCOP_1:58;
      now let t be Element of X;
                 ||.g.t.|| <= ||.f.||  by Th19;
              then ||.g.t.|| = 0 by A2,NORMSP_1:8;
           hence g.t =0.Y by NORMSP_1:def 2
                 .=z.t by FUNCOP_1:13;
       end;
     then g=z by FUNCT_2:113;
   hence f=0.R_NormSpace_of_BoundedFunctions(X,Y) by Th18;
end;

A3:
now assume
   A4: f = 0.R_NormSpace_of_BoundedFunctions(X,Y);
  thus ||.f.|| = 0
  proof
     set z = X --> 0.Y;
     reconsider z as Function of X, the carrier of Y
    by FUNCOP_1:58;
    reconsider g=f as bounded Function of X,the carrier of Y by Def5;
      A5: z=g by A4,Th18;
      A6:
      now let r be Real such that
        A7:  r in PreNorms(g);
        consider t be Element of X such that
          A8:   r=||.g.t.|| by A7;
              ||.g.t.|| = ||.0.Y.|| by A5,FUNCOP_1:13
                  .= 0;
          hence 0 <= r & r <=0 by A8;
    end;
A9:PreNorms(g) is non empty bounded_above by Th13;
 consider r0 be set such that
  A10: r0 in PreNorms(g) by XBOOLE_0:def 1;
  reconsider r0 as Real by A10;
  A11: 0<=r0 by A6,A10;
    (for s be real number st s in PreNorms(g) holds s <= 0)
implies sup PreNorms(g) <= 0 by SEQ_4:62;
     then sup PreNorms(g) = 0 by A6,A9,A10,A11,SEQ_4:def 4;
    hence thesis by Th17;
  end;
end;

A12:  ||.a*f.|| = abs(a) * ||.f.||
proof
reconsider f1=f, h1=a*f as bounded Function of X,
the carrier of Y by Def5;
A13:
now let t be Element of X;
 A14:  ||.h1.t.||= ||.a*f1.t.|| by Th23;
 A15:  ||.a*f1.t.|| =abs(a)*||.f1.t.|| by NORMSP_1:def 2;
 A16:   ||.f1.t.||<= ||.f.|| by Th19;
     0<= abs(a) by COMPLEX1:132;
    hence ||.h1.t.|| <= abs(a)*||.f.|| by A14,A15,A16,XREAL_1:66;
end;
A17:
     now let r be Real such that
        A18:  r in PreNorms(h1);
        consider t be Element of X such that
          A19: r=||.h1.t.|| by A18;
          thus r <= abs(a)*||.f.|| by A13,A19;
  end;

A20:
(for s be real number st s in PreNorms(h1) holds s <= abs(a)*||.f.|| )
implies sup PreNorms(h1) <=  abs(a)*||.f.|| by SEQ_4:62;
   A21: BoundedFunctionsNorm(X,Y).(a*f)
    = sup PreNorms(h1) by Th17;
    now per cases;
case A22: a <> 0;
 A23: now let t be Element of X;
      h1.t=a*f1.t by Th23;
  then A24: a"*h1.t =( a"* a)*f1.t by RLVECT_1:def 9
             .=1*f1.t by A22,XCMPLX_0:def 7
             .=f1.t by RLVECT_1:def 9;
 A25:  ||.a"*h1.t.|| =abs(a")*||.h1.t.|| by NORMSP_1:def 2;
 A26:   ||.h1.t.||<= ||.a*f.|| by Th19;
 A27: 0<= abs(a") by COMPLEX1:132;
       abs(a") =abs(1*a") .=abs( 1/a) by XCMPLX_0:def 9
                 .=1/abs(a) by ABSVALUE:15
                 .=1*abs(a)" by XCMPLX_0:def 9
                 .=abs(a)";
  hence ||.f1.t.|| <= abs(a)"*||.a*f.|| by A24,A25,A26,A27,XREAL_1:66;
end;
A28:
     now let r be Real such that
        A29:  r in PreNorms(f1);
        consider t be Element of X such that
          A30:  r=||.f1.t.|| by A29;
          thus r <= abs(a)"*||.a*f.|| by A23,A30;
  end;
A31: abs(a) <>0 by A22,COMPLEX1:133;
 A32:
(for s be real number st s in PreNorms(f1) holds s <= abs(a)"*||.a*f.|| )
implies sup PreNorms(f1) <= abs(a)"*||.a*f.|| by SEQ_4:62;
    A33: BoundedFunctionsNorm(X,Y).(f)
    = sup PreNorms(f1) by Th17;
     0 <= abs(a) by COMPLEX1:132;
 then abs(a)*||.f.|| <=abs(a)*(abs(a)"*||.a*f.||) by A28,A32,A33,XREAL_1:66;
 then abs(a)*||.f.|| <=(abs(a)*abs(a)")*||.a*f.||;
 then abs(a)*||.f.|| <=1*||.a*f.|| by A31,XCMPLX_0:def 7;
 hence abs(a)* ||.f.|| <=||.a*f.||;
 end;
 case A34: a=0;
  reconsider fz=f as VECTOR
     of R_VectorSpace_of_BoundedFunctions(X,Y);
     A35: a*f =a*fz
        .=0.R_VectorSpace_of_BoundedFunctions(X,Y)
 by A34,RLVECT_1:23
        .=0.R_NormSpace_of_BoundedFunctions(X,Y);
  thus abs(a)* ||.f.|| =0 * ||.f.|| by A34,ABSVALUE:7
                      .=||.a*f.|| by A35,Th21;
  end;
end;
hence thesis by A17,A20,A21,XXREAL_0:1;
end;
 ||.f+g.|| <= ||.f.|| + ||.g.||
proof
reconsider f1=f, g1=g, h1=f+g
as bounded Function of X,the carrier of Y by Def5;
A36:
now let t be Element of X;
A37:  ||.h1.t.||= ||.f1.t+g1.t.|| by Th22;
A38:  ||.f1.t+g1.t.|| <=||.f1.t.||+||.g1.t.|| by NORMSP_1:def 2;
A39:   ||.f1.t.||<= ||.f.|| by Th19;
      ||.g1.t.||<= ||.g.|| by Th19;
   then ||.f1.t.||+||.g1.t.|| <= ||.f.|| + ||.g.|| by A39,XREAL_1:9;
    hence ||.h1.t.|| <= ||.f.|| + ||.g.|| by A37,A38,XXREAL_0:2;
end;
A40:
     now let r be Real such that
        A41:  r in PreNorms(h1);
        consider t be Element of X such that
          A42:   r=||.h1.t.|| by A41;
          thus r <= ||.f.|| + ||.g.|| by A36,A42;
  end;

(for s be real number st s in PreNorms(h1) holds s <= ||.f.|| + ||.g.||)
implies sup PreNorms(h1) <= ||.f.|| + ||.g.|| by SEQ_4:62;
hence ||.f+g.|| <=||.f.|| + ||.g.|| by A40,Th17;
end;
hence thesis by A1,A3,A12;
end;

theorem Th25:
  for X be non empty set for Y be RealNormSpace holds
  R_NormSpace_of_BoundedFunctions(X,Y) is RealNormSpace-like
      proof
       let X be non empty set; let Y be RealNormSpace;
        for x, y being Point of R_NormSpace_of_BoundedFunctions(X,Y)
       for a be Real holds
  ( ||.x.|| = 0 iff
  x = 0.R_NormSpace_of_BoundedFunctions(X,Y) ) &
  ||.a*x.|| = abs(a) * ||.x.|| &
  ||.x+y.|| <= ||.x.|| +  ||.y.|| by Th24;
   hence thesis by NORMSP_1:def 2;
  end;

theorem Th26:
  for X be non empty set for Y be RealNormSpace holds
    R_NormSpace_of_BoundedFunctions(X,Y) is RealNormSpace
     proof
    let X be non empty set; let Y be RealNormSpace;
        RLSStruct (# BoundedFunctions(X,Y),
            Zero_(BoundedFunctions(X,Y),RealVectSpace(X,Y)),
             Add_(BoundedFunctions(X,Y),
             RealVectSpace(X,Y)),
            Mult_(BoundedFunctions(X,Y),
            RealVectSpace(X,Y)) #) is RealLinearSpace;
      hence thesis by Th25,RSSPACE3:4;
     end;

registration
   let X be non empty set; let Y be RealNormSpace;
   cluster R_NormSpace_of_BoundedFunctions(X,Y) ->
               RealNormSpace-like RealLinearSpace-like
               Abelian add-associative right_zeroed right_complementable;
   coherence by Th26;
end;

theorem Th27:
     for X be non empty set for Y be RealNormSpace
     for f,g,h be Point of R_NormSpace_of_BoundedFunctions(X,Y)
       for f',g',h' be bounded Function of X,the carrier of Y
        st f'=f & g'=g & h'=h holds
      (h = f-g iff
          (for x be Element of X holds (h'.x = f'.x - g'.x)) )
proof
     let X be non empty set; let Y be RealNormSpace;
     let f,g,h be Point of R_NormSpace_of_BoundedFunctions(X,Y);
     let f',g',h' be bounded Function of X,the carrier of Y such that
       A1: f'=f and
       A2: g'=g and
       A3: h'=h;
A4: now assume h=f-g;
     then h+g=f-(g-g) by RLVECT_1:43;
     then h+g=f-0.R_NormSpace_of_BoundedFunctions(X,Y) by RLVECT_1:28;
     then A5: h+g=f by RLVECT_1:26;
         now let x be Element of X;
           f'.x=h'.x +  g'.x by A1,A2,A3,A5,Th22;
       then f'.x-g'.x=h'.x + (g'.x-g'.x) by RLVECT_1:def 6;
       then f'.x-g'.x=h'.x + 0.Y by RLVECT_1:28;
       hence f'.x-g'.x=h'.x by RLVECT_1:10;
     end;
     hence for x be Element of X holds (h'.x = f'.x - g'.x);
end;
     now assume
     A6: for x be Element of X holds (h'.x = f'.x - g'.x);
         now let x be Element of X;
           h'.x = f'.x - g'.x by A6;
       then h'.x + g'.x= f'.x - (g'.x- g'.x) by RLVECT_1:43;
       then h'.x + g'.x= f'.x - 0.Y by RLVECT_1:28;
       hence h'.x + g'.x= f'.x by RLVECT_1:26;
     end;
     then f=h+g by A1,A2,A3,Th22;
     then f-g=h+(g-g) by RLVECT_1:def 6;
     then f-g=h+0.R_NormSpace_of_BoundedFunctions(X,Y) by RLVECT_1:28;
     hence f-g=h by RLVECT_1:10;
end;
hence thesis by A4;
end;

Lm8:
for e be Real, seq be Real_Sequence st ( seq is convergent &
 ex k be Element of NAT st
   (for i be Element of NAT st k <= i holds seq.i <=e ) ) holds
  lim seq <=e
proof
   let e be Real;
   let seq be Real_Sequence such that
A1:seq is convergent and
A2:ex k be Element of NAT st
   (for i be Element of NAT st k <= i holds seq.i <=e );
   reconsider cseq = NAT --> e as Real_Sequence by FUNCOP_1:57;
   reconsider e1=e as real number;
A3:cseq is convergent by SEQ_4:39;
   consider k be Element of NAT such that
A4:for i be Element of NAT st k <= i holds seq.i <=e by A2;
A5:(seq ^\k) is convergent by A1,SEQ_4:33;
A6:lim (seq ^\k)=lim seq by A1,SEQ_4:33;
A7:now let i be Element of NAT;
A8: (seq ^\k).i = seq.(k+i) by SEQM_3:def 9;
         k <= k+i by NAT_1:11;
     then seq.(k+i) <=e by A4;
     hence (seq ^\k) .i <= cseq.i by A8,FUNCOP_1:13;
  end;
      lim cseq = cseq.0 by SEQ_4:41 .= e by FUNCOP_1:13;
  hence thesis by A3,A5,A6,A7,SEQ_2:32;
end;

theorem Th28:
    for X be non empty set for Y be RealNormSpace st Y is complete
      for seq be sequence of R_NormSpace_of_BoundedFunctions(X,Y)
               st seq is Cauchy_sequence_by_Norm
 holds seq is convergent
proof
    let X be non empty set; let Y be RealNormSpace such that
 A1:   Y is complete;
    let vseq be sequence of R_NormSpace_of_BoundedFunctions(X,Y)
     such that
A2: vseq is Cauchy_sequence_by_Norm;
A3:  ||.vseq.|| is convergent
proof
     now let e1 be real number such that
   A4: e1 >0;
   reconsider e =e1 as Real by XREAL_0:def 1;
          consider k be Element of NAT such that
   A5:      for n, m be Element of NAT st n >= k & m >= k holds
                ||.(vseq.n) - (vseq.m).|| < e by A2,A4,RSSPACE3:10;
A6: now
      let m be Element of NAT such that
      A7:   m >= k;
      A8:  abs( ||.vseq.m.||- ||.vseq.k.|| )
         <= ||.(vseq.m) - (vseq.k).|| by NORMSP_1:13;
      A9:   ||.vseq.k.||= ||.vseq.||.k by NORMSP_1:def 10;
      A10:   ||.vseq.m.||= ||.vseq.||.m by NORMSP_1:def 10;
             ||.(vseq.m) - (vseq.k).|| <e by A5,A7;
      hence abs( ||.vseq.||.m - ||.vseq.||.k ) <e1
      by A8,A9,A10,XXREAL_0:2;
   end;
   take k;
   thus for m be Element of NAT st m >= k holds
             abs(||.vseq.||.m - ||.vseq.||.k ) < e1 by A6;
 end;
 hence ||.vseq.|| is convergent by SEQ_4:58;
end;
    defpred P[set, set] means ex xseq be sequence of Y st
    (for n be Element of NAT holds xseq.n=modetrans((vseq.n),X,Y).$1)
    & xseq is convergent & $2= lim xseq;
A11: for x be Element of X ex y be Element of Y st  P[x,y]
   proof
       let x be Element of X;
       deffunc F(Element of NAT) = modetrans((vseq.$1),X,Y).x;
       consider xseq be sequence of Y such that
A12:  for n be Element of NAT holds xseq.n = F(n) from FUNCT_2:sch 4;
A13: for m,k be Element of NAT holds
             ||.xseq.m-xseq.k.|| <= ||.vseq.m - vseq.k.||
      proof
      let m,k be Element of NAT;
             vseq.m is bounded Function of X,the carrier of Y
         by Def5;
then A14:         modetrans((vseq.m),X,Y)=vseq.m by Th16;
              vseq.k is bounded Function of X,the carrier of Y
          by Def5;
then A15:         modetrans((vseq.k),X,Y)=vseq.k by Th16;
         reconsider h1=vseq.m-vseq.k as bounded Function of X,
           the carrier of Y by Def5;
           A16:  xseq.m =modetrans((vseq.m),X,Y).x by A12;
                xseq.k =modetrans((vseq.k),X,Y).x by A12;
           then xseq.m - xseq.k = h1.x by A14,A15,A16,Th27;
          hence thesis by Th19;
         end;
A17: xseq is convergent
     proof
            now
            let e be Real such that
A18:          e > 0;
               thus ex k be Element of NAT st
                for n, m be Element of NAT st n >= k & m >= k
                  holds ||.xseq.n -xseq.m.|| < e
                proof
                   consider k be Element of NAT such that
A19:                for n, m be Element of NAT st n >= k & m >= k holds
                     ||.(vseq.n) - (vseq.m).|| < e
                        by A2,A18,RSSPACE3:10;
                   take k;
                   thus for n, m be Element of NAT st ( n >= k & m >= k )
                     holds ||.xseq.n-xseq.m.|| < e
                   proof
                     let n,m be Element of NAT such that
A20:                  n >=k & m >= k;
A21:                 ||.xseq.n-xseq.m.||  <=
                     ||.(vseq.n) - (vseq.m).||  by A13;
                         ||.(vseq.n) - (vseq.m).|| < e by A19,A20;
                     hence ||.xseq.n-xseq.m.|| < e by A21,XXREAL_0:2;
                 end;
               end;
            end;
            then xseq is Cauchy_sequence_by_Norm by RSSPACE3:10;
         hence thesis by A1,LOPBAN_1:def 16;
         end;
  take y = lim xseq;
  thus thesis by A12,A17;
  end;
   consider f be Function of X,the carrier of Y such that
A22:for x be Element of X holds P[x,f.x] from FUNCT_2:sch 3(A11);
   reconsider tseq=f as Function of X,the carrier of Y;
A23: tseq is bounded
proof
A24: now
       let x be Element of X;
       consider xseq be sequence of Y such that
  A25: (for n be Element of NAT holds xseq.n=modetrans((vseq.n),X,Y).x)
       & xseq is convergent
       & tseq.x = lim xseq by A22;
A26: for m be Element of NAT holds ||.xseq.m.|| <= ||.vseq.m.||
      proof
      let m be Element of NAT;
        vseq.m is bounded Function of X,the carrier of Y
by Def5;
then A27:         modetrans((vseq.m),X,Y)=vseq.m by Th16;
         reconsider h1=vseq.m as bounded Function of X,the carrier of Y by Def5
;
              xseq.m =modetrans((vseq.m),X,Y).x by A25;
          hence thesis by A27,Th19;
         end;
  A28:     ||.xseq.|| is convergent by A25,LOPBAN_1:47;
  A29:     ||.tseq.x.|| = lim ||.xseq.|| by A25,LOPBAN_1:47;
              for n be Element of NAT holds ||.xseq.||.n  <=(||.vseq.||).n
            proof
            let n be Element of NAT;
             A30: ||.xseq.||.n = ||.(xseq.n).|| by NORMSP_1:def 10;
                 ||.(xseq.n).|| <= ||.vseq.n.|| by A26;
             hence thesis by A30,NORMSP_1:def 10;
            end;
   hence ||.tseq.x.|| <= lim (||.vseq.|| ) by A3,A28,A29,SEQ_2:32;
    end;
   take K= lim (||.vseq.|| );
        K >=0
    proof
         now let n be Element of NAT;
             ||.vseq.n.|| >=0 by NORMSP_1:8;
         hence ||.vseq.||.n >=0 by NORMSP_1:def 10;
      end;
     hence thesis by A3,SEQ_2:31;
    end;
    hence thesis by A24;
end;

A31:
   for e be Real st
    e > 0 ex k be Element of NAT st for n be Element of NAT st
     n >= k holds
        for x be Element of X holds
            ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e
   proof
     let e be Real such that
      A32:e > 0;
      consider k be Element of NAT such that
      A33:  for n, m be Element of NAT st ( n >= k & m >= k ) holds
             ||.(vseq.n) - (vseq.m).|| < e by A2,A32,RSSPACE3:10;
      A34: now let n be Element of NAT such that
         A35: n >= k;
               now let x be Element of X;
              consider xseq be sequence of Y such that
              A36:
              (for n be Element of NAT holds  xseq.n=modetrans((vseq.n),X,Y).x)
                & xseq is convergent
                & tseq.x = lim xseq by A22;
               A37:
                for m,k be Element of NAT holds
                 ||.xseq.m-xseq.k.|| <= ||.vseq.m - vseq.k.||
                proof
                let m,k be Element of NAT;
                    vseq.m is bounded Function of X,the carrier of Y
                by Def5;
                 then A38:           modetrans((vseq.m),X,Y)=vseq.m by Th16;
                    vseq.k is bounded Function of X,the carrier of Y
                by Def5;
                then A39:            modetrans((vseq.k),X,Y)=vseq.k by Th16;
                reconsider h1=vseq.m-vseq.k
              as bounded Function of X,the carrier of Y by Def5;
                A40:  xseq.m =modetrans((vseq.m),X,Y).x by A36;
                     xseq.k =modetrans((vseq.k),X,Y).x by A36;
                 then xseq.m - xseq.k =h1.x by A38,A39,A40,Th27;
                 hence thesis by Th19;
                 end;
           A41:  for m be Element of NAT st m >=k holds
                    ||.xseq.n-xseq.m.|| <= e
                    proof
                      let m be Element of NAT such that
                      A42:  m >=k;
                      A43:  ||.xseq.n-xseq.m.||
                      <= ||.vseq.n - vseq.m.|| by A37;
                          ||.vseq.n - vseq.m.|| <e by A33,A35,A42;
                      hence thesis by A43,XXREAL_0:2;
                    end;
                      ||.xseq.n-tseq.x.|| <= e
                  proof
                    deffunc F(Element of NAT) = ||.xseq.$1 - xseq.n.||;
                    consider rseq be Real_Sequence such that
  A44:              for m be Element of NAT holds rseq.m = F(m)
                      from SEQ_1:sch 1;
  A45:            rseq is convergent & lim rseq = ||.tseq.x-xseq.n.||
                    proof
                        A46: xseq - xseq.n is convergent by A36,NORMSP_1:36;
                        A47: lim (xseq-xseq.n)= tseq.x - xseq.n
                    by A36,NORMSP_1:44;
                              rseq = ||.xseq - xseq.n.||
                         proof
                              now let x be set such that
                           A48:  x in NAT;
                           reconsider k=x as Element of NAT by A48;
                           thus rseq.x = ||.xseq.k - xseq.n.|| by A44
                             .= ||.(xseq - xseq.n).k.|| by NORMSP_1:def 7
                             .= ||.(xseq - xseq.n).||.x by NORMSP_1:def 10;
                         end;
                         hence thesis by SEQ_1:8;
                         end;
                         hence thesis by A46,A47,LOPBAN_1:24;
                     end;
                for m be Element of NAT st m >= k holds rseq.m <= e
                    proof
                      let m be Element of NAT such that
                      A49:  m >=k;
                          rseq.m = ||.xseq.m-xseq.n.|| by A44
                                     .= ||.xseq.n-xseq.m.|| by NORMSP_1:11;
                      hence thesis by A41,A49;
                    end;
                   then lim rseq <= e by A45,Lm8;
                   hence thesis by A45,NORMSP_1:11;
                  end;
                 hence ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e by A36;
           end;
         hence for x be Element of X holds
          ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e;
     end;
     take k;
     thus thesis by A34;
   end;
  reconsider tseq as bounded Function of X,the carrier of Y by A23;
  reconsider tv=tseq as Point of
   R_NormSpace_of_BoundedFunctions(X,Y) by Def5;

A50:
   for e be Real st
    e > 0 ex k be Element of NAT st for n be Element of NAT st
     n >= k holds ||.vseq.n - tv.|| <= e
   proof
   let e be Real such that
      A51: e > 0;
      consider k be Element of NAT such that
      A52: for n be Element of NAT st n >= k holds
          for x be Element of X holds
  ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e by A31,A51;
          now let n be Element of NAT such that
        A53: n >= k;
        set f1=modetrans((vseq.n),X,Y);
        set g1=tseq;
        reconsider h1=vseq.n-tv
        as bounded Function of X,the carrier of Y by Def5;
       A54: now
       let t be Element of X;
        vseq.n is bounded Function of X,the carrier of Y by Def5;
       then modetrans((vseq.n),X,Y)=vseq.n by Th16;
       then ||.h1.t.||= ||.f1.t-g1.t.|| by Th27;
       hence ||.h1.t.|| <=e by A52,A53;
      end;
     A55:
       now let r be Real such that
        A56:  r in PreNorms(h1);
        consider t be Element of X such that
          A57: r=||.h1.t.|| by A56;
          thus r <=e by A54,A57;
      end;

     (for s be real number st s in PreNorms(h1) holds s <= e)
      implies sup PreNorms(h1) <=e by SEQ_4:62;
     hence ||.vseq.n-tv.|| <=e by A55,Th17;
      end;
     hence thesis;
end;
      for e be Real st
    e > 0 ex m be Element of NAT st for n be Element of NAT st
     n >= m holds ||.(vseq.n) - tv.|| < e
   proof
    let e be Real such that
      A58: e > 0;
      A59: e/2 > 0 by A58,SEQ_2:3;
      A60: e/2<e by A58,XREAL_1:218;
      consider m be Element of NAT such that
          A61: for n be Element of NAT st
          n >= m holds ||.(vseq.n) - tv.|| <= e/2 by A50,A59;
              now let n be Element of NAT such that
        A62: n >= m;
                ||.(vseq.n) - tv.|| <= e/2 by A61,A62;
            hence ||.(vseq.n) - tv.|| < e by A60,XXREAL_0:2;
          end;
    hence thesis;
   end;
   hence thesis by NORMSP_1:def 9;
end;

theorem Th29:
  for X be non empty set
  for Y be RealBanachSpace holds
     R_NormSpace_of_BoundedFunctions(X,Y) is RealBanachSpace
proof
    let X be non empty set;
    let Y be RealBanachSpace;
        for seq be sequence of R_NormSpace_of_BoundedFunctions(X,Y)
      st seq is Cauchy_sequence_by_Norm holds seq is convergent by Th28;
    hence thesis by LOPBAN_1:def 16;
end;

registration
  let X be non empty set;
  let Y be RealBanachSpace;
  cluster R_NormSpace_of_BoundedFunctions (X,Y) -> complete;
  coherence by Th29;
end;
