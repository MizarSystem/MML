then A29:       abs(seq_id((vseq.m) - (vseq.n))) is bounded &
             sup rng abs seq_id((vseq.m) - (vseq.n)) <= e by A15,A24;
then A30:       abs(seq_id((vseq.m) - (vseq.n))).i
             <=sup rng abs seq_id((vseq.m) - (vseq.n)) by Lm2;
               rseq.m = abs(seq_id((vseq.m) - (vseq.n))).i by A26;
           hence thesis by A29,A30,XXREAL_0:2;
         end;
        hence thesis by A27,A28,RSSPACE2:6;
  end;
      abs(seq_id tseq-seq_id(vseq.n)) is bounded
  proof
A31:  0 + 0 < 1 + e by A13,XREAL_1:10;
A32:  0 + e < 1 + e by XREAL_1:10;
             now let i be Element of NAT;
            A33: abs((seq_id tseq -seq_id(vseq.n))).i <= e by A25;
                abs ((seq_id tseq -seq_id(vseq.n))).i
