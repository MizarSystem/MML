theorem Th10:
  PSO T /\ D(c,ps)(T) = the topology of T
proof
  thus PSO T /\ D(c,ps)(T) c= the topology of T
  proof
    let x be set;
    assume
A1: x in PSO T /\ D(c,ps)(T);
