A52:                lm+1 <= card I +1 by INT_1:20;
                    card I + 1 < card I + 3 by XREAL_1:8;
                    then lm+1 < card I +3 by A52,XXREAL_0:2;
                    then lm+1 < card pWHL by Lm1;
