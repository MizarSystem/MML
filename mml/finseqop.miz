      .= h.(F.(f.c,f9.c)) by FUNCOP_1:48
      .= H.(h.(f.c),h.(f9.c)) by A1
      .= H.((h*f).c,h.(f9.c)) by FUNCT_2:21
      .= H.((h*f).c,(h*f9).c) by FUNCT_2:21
