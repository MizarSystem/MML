:: Auxiliary theorems

theorem Th1:
  for f being Function holds <:{},f:> = {} & <:f,{}:> = {}
proof
  let f be Function;
  dom <:{},f:> = dom {} /\ dom f & dom <:f,{}:> = dom f /\ dom {} by
  FUNCT_3:def 8;
  hence thesis;
end;

theorem
  for f being Function holds [:{},f:] = {} & [:f,{}:] = {}
proof
  let f be Function;
  dom [:{},f:] = [:dom {},dom f:] & dom [:f,{}:] = [:dom f,dom {}:] by
  FUNCT_3:def 9;
  hence thesis by ZFMISC_1:113;
end;

canceled;

theorem Th4:
  for F,f being Function holds F.:({},f) = {} & F.:(f,{}) = {}
proof
  let F,f be Function;
  F.:({},f) = F*{} by Th1;
  hence thesis by Th1;
end;

theorem
  for F being Function holds F[:]({},x) = {}
proof
  let F be Function;
  F[:]({},x) = F.:({}, dom {} --> x);
  hence thesis by Th4;
end;

theorem
  for F being Function holds F[;](x,{}) = {}
proof
  let F be Function;
  F[;](x,{}) = F.:(dom {} --> x, {});
  hence thesis by Th4;
end;

theorem Th7:
  for X being set, x1,x2 being set holds <:X-->x1,X-->x2:> = X -->[ x1,x2]
proof
  let X be set, x1,x2 be set;
  set f = X-->x1, g = X-->x2;
  set fg = <:f,g:>;
  now
    per cases;
    suppose
A1:   X = {};
      then f = {} --> x1;
      hence thesis by A1,Th1;
    end;
    suppose
A2:   X <> {};
      rng fg c= [:{x1},{x2}:];
      then
A3:   rng fg c= {[x1,x2]} by ZFMISC_1:35;
      consider x being Element of X;
A4:   dom f = X & dom g = X by FUNCOP_1:19;
      then
A5:   dom fg = X by FUNCT_3:70;
      f.x = x1 & g.x = x2 by A2,FUNCOP_1:13;
      then fg.x = [x1,x2] by A2,A4,FUNCT_3:69;
      then [x1,x2] in rng fg by A2,A5,FUNCT_1:def 5;
      then {[x1,x2]} c= rng fg by ZFMISC_1:37;
      then rng fg = {[x1,x2]} by A3,XBOOLE_0:def 10;
      hence thesis by A5,FUNCOP_1:15;
    end;
  end;
  hence thesis;
end;

theorem Th8:
  for F being Function,X being set, x1,x2 being set st [x1,x2] in
  dom F holds F.:(X-->x1,X-->x2) = X --> F.(x1,x2)
proof
  let F be Function, X be set, x1,x2 be set such that
A1: [x1,x2] in dom F;
  set f = X-->x1, g = X-->x2;
  thus F.:(f,g) = F*(X-->[x1,x2]) by Th7
    .= X --> F.(x1,x2) by A1,FUNCOP_1:23;
end;

reserve i,j for Nat;
reserve F for Function of [:D,D9:],E;
reserve p,q for FinSequence of D,
  p9,q9 for FinSequence of D9;

definition
  let D,D9,E,F,p,p9;
  redefine func F.:(p,p9) -> FinSequence of E;
  coherence by FINSEQ_2:84;
end;

definition
  let D,D9,E,F,p,d9;
  redefine func F[:](p,d9) -> FinSequence of E;
  coherence by FINSEQ_2:97;
end;

definition
  let D,D9,E,F,d,p9;
  redefine func F[;](d,p9) -> FinSequence of E;
  coherence by FINSEQ_2:91;
end;

definition
  let D,i,d;
  redefine func i|-> d -> Element of i-tuples_on D;
  coherence by FINSEQ_2:132;
end;

reserve f,f9 for Function of C,D,
  h for Function of D,E;

definition
  let D,E be set, p be FinSequence of D, h be Function of D,E;
  redefine func h*p -> FinSequence of E;
  coherence by FINSEQ_2:36;
end;

theorem Th9:
  h*(p^<*d*>) = (h*p)^<*h.d*>
proof
  set q = h*(p^<*d*>);
  set r = (h*p)^<*h.d*>;
  set i = len p + 1;
A1: len q = len(p^<*d*>) by FINSEQ_2:37;
A2: len(h*p) = len p by FINSEQ_2:37;
  len(p^<*d*>) = i by FINSEQ_2:19;
  then
A3: dom q = Seg i by A1,FINSEQ_1:def 3;
A4: now
    let j be Nat;
A5: Seg len p = dom p by FINSEQ_1:def 3;
A6: Seg len(h*p) = dom(h*p) by FINSEQ_1:def 3;
    assume
A7: j in dom q;
    now
      per cases by A3,A7,A5,FINSEQ_2:8;
      suppose
A8:     j in dom p;
        hence h.((p^<*d*>).j) = h.(p.j) by FINSEQ_1:def 7
          .= (h*p).j by A2,A5,A6,A8,FUNCT_1:22
          .= r.j by A2,A5,A6,A8,FINSEQ_1:def 7;
      end;
      suppose
A9:     j = i;
        hence h.((p^<*d*>).j) = h.d by FINSEQ_1:59
          .= r.j by A2,A9,FINSEQ_1:59;
      end;
    end;
    hence q.j = r.j by A7,FUNCT_1:22;
  end;
  len r = len(h*p)+1 by FINSEQ_2:19;
  hence thesis by A1,A2,A4,FINSEQ_2:10,19;
end;

theorem
  h*(p^q) = (h*p)^(h*q)
proof
  defpred P[FinSequence of D] means h*(p^$1) = (h*p)^(h*$1);
A1: for q,d st P[q] holds P[q^<*d*>]
  proof
    let q,d such that
A2: h*(p^q) = (h*p)^(h*q);
    thus h*(p^(q^<*d*>)) = h*((p^q)^<*d*>) by FINSEQ_1:45
      .= (h*(p^q))^<*h.d*> by Th9
      .= (h*p)^((h*q)^<*h.d*>) by A2,FINSEQ_1:45
      .= (h*p)^(h*(q^<*d*>)) by Th9;
  end;
  h*(p^<*>D) = h*p by FINSEQ_1:47
    .= (h*p)^(h*(<*>D)) by FINSEQ_1:47;
  then
A3: P[<*>D];
  for q holds P[q] from FINSEQ_2:sch 2(A3,A1);
  hence thesis;
end;

reserve T,T1,T2,T3 for Tuple of i,D;
reserve T9 for Tuple of i, D9;
reserve S for Tuple of j, D;
reserve S9 for Tuple of j, D9;

Lm1: for T being Tuple of 0, D holds F.:(T,T9) = <*>E
proof
  let T be Tuple of 0, D;
  T = <*>D;
  hence thesis by FINSEQ_2:87;
end;

Lm2: for T9 being Tuple of 0, D9 holds F[;](d,T9) = <*>E
proof
  let T9 be Tuple of 0, D9;
  T9 = <*>D9;
  hence thesis by FINSEQ_2:93;
end;

Lm3: for T being Tuple of 0, D holds F[:](T,d9) = <*>E
proof
  let T be Tuple of 0, D;
  T = <*>D;
  hence thesis by FINSEQ_2:99;
end;

theorem Th11:
  F.:(T^<*d*>,T9^<*d9*>) = (F.:(T,T9))^<*F.(d,d9)*>
proof
  set p = T^<*d*>, q = T9^<*d9*>, pq = F.:(T,T9);
  set r = F.:(p,q), s = pq^<*F.(d,d9)*>;
A1: len T9 = i by FINSEQ_1:def 18;
  then
A2: len q = i+1 by FINSEQ_2:19;
A3: len T = i by FINSEQ_1:def 18;
  then
A4: len pq = i by A1,FINSEQ_2:86;
  len p = i+1 by A3,FINSEQ_2:19;
  then
A5: len r = i+1 by A2,FINSEQ_2:86;
  then
A6: dom r = Seg(i+1) by FINSEQ_1:def 3;
A7: now
    let j be Nat;
    assume
A8: j in dom r;
    now
      per cases by A6,A8,FINSEQ_2:8;
      suppose
A9:     j in Seg i;
        Seg len T = dom T by FINSEQ_1:def 3;
        then
A10:    p.j = T.j by A3,A9,FINSEQ_1:def 7;
A11:    Seg len pq = dom pq by FINSEQ_1:def 3;
        Seg len T9 = dom T9 by FINSEQ_1:def 3;
        then
A12:    q.j = T9.j by A1,A9,FINSEQ_1:def 7;
        j in dom pq by A4,A9,FINSEQ_1:def 3;
        hence F.(p.j,q.j) = pq.j by A10,A12,FUNCOP_1:28
          .= s.j by A4,A9,A11,FINSEQ_1:def 7;
      end;
      suppose
A13:    j = i+1;
        then p.j = d & q.j = d9 by A3,A1,FINSEQ_1:59;
        hence F.(p.j,q.j) = s.j by A4,A13,FINSEQ_1:59;
      end;
    end;
    hence r.j = s.j by A8,FUNCOP_1:28;
  end;
  len s = len pq + 1 by FINSEQ_2:19;
  hence thesis by A5,A4,A7,FINSEQ_2:10;
end;

theorem
  F.:(T^S,T9^S9) = (F.:(T,T9))^(F.:(S,S9))
proof
  defpred P[Nat] means for S being Tuple of $1, D, S9 being Tuple
  of $1, D9 holds F.:(T^S,T9^S9) = (F.:(T,T9))^(F.:(S,S9));
  now
    let j such that
A1: for S,S9 holds F.:(T^S,T9^S9) = (F.:(T,T9))^(F.:(S,S9));
    let S be Tuple of j+1, D;
    let S9 be Tuple of j+1, D9;
    consider S1 being Element of j-tuples_on D, d such that
A2: S = S1^<*d*> by FINSEQ_2:137;
A3: T^S = T^S1^<*d*> by A2,FINSEQ_1:45;
    reconsider S1 as Tuple of j, D;
    consider S19 being Element of j-tuples_on D9, d9 such that
A4: S9 = S19^<*d9*> by FINSEQ_2:137;
A5: T9^S9 = T9^S19^<*d9*> by A4,FINSEQ_1:45;
    reconsider S19 as Tuple of j, D9;
    T^S1 is Tuple of i+j, D & T9^S19 is Tuple of i+j, D9 by FINSEQ_2:127;
    hence F.:(T^S,T9^S9) = (F.:(T^S1,T9^S19))^<*F.(d,d9)*> by A3,A5,Th11
      .= (F.:(T,T9))^(F.:(S1,S19))^<*F.(d,d9)*> by A1
      .= (F.:(T,T9))^((F.:(S1,S19))^<*F.(d,d9)*>) by FINSEQ_1:45
      .= (F.:(T,T9))^(F.:(S,S9)) by A2,A4,Th11;
  end;
  then
A6: for j st P[j] holds P[j+1];
  now
    let S be Tuple of 0, D;
    let S9 be Tuple of 0, D9;
    S = <*>D;
    then
A7: F.:(S,S9) = <*>E by FINSEQ_2:87;
    T^S = T & T9^S9 = T9 by FINSEQ_2:115;
    hence F.:(T^S,T9^S9) = (F.:(T,T9))^(F.:(S,S9)) by A7,FINSEQ_1:47;
  end;
  then
A8: P[0];
  for j holds P[j] from NAT_1:sch 2(A8,A6);
  hence thesis;
end;

theorem Th13:
  F[;](d,p9^<*d9*>) = (F[;](d,p9))^<*F.(d,d9)*>
proof
  set pd = p9^<*d9*>, q = F[;](d,p9);
  set r = F[;](d,pd), s = q^<*F.(d,d9)*>;
  set i = len p9;
A1: len q = i by FINSEQ_2:92;
  len pd = i + 1 by FINSEQ_2:19;
  then
A2: len r = i + 1 by FINSEQ_2:92;
  then
A3: dom r = Seg(i+1) by FINSEQ_1:def 3;
A4: now
    let j be Nat;
    assume
A5: j in dom r;
    now
      per cases by A3,A5,FINSEQ_2:8;
      suppose
A6:     j in Seg i;
        then
A7:     j in dom q by A1,FINSEQ_1:def 3;
A8:     Seg len q = dom q by FINSEQ_1:def 3;
        Seg len p9 = dom p9 by FINSEQ_1:def 3;
        hence F.(d,pd.j) = F.(d,p9.j) by A6,FINSEQ_1:def 7
          .= q.j by A7,FUNCOP_1:42
          .= s.j by A1,A6,A8,FINSEQ_1:def 7;
      end;
      suppose
A9:     j = i+1;
        hence F.(d,pd.j) = F.(d,d9) by FINSEQ_1:59
          .= s.j by A1,A9,FINSEQ_1:59;
      end;
    end;
    hence r.j = s.j by A5,FUNCOP_1:42;
  end;
  len s = len q + 1 by FINSEQ_2:19;
  hence thesis by A1,A2,A4,FINSEQ_2:10;
end;

theorem
  F[;](d,p9^q9) = (F[;](d,p9))^(F[;](d,q9))
proof
  defpred P[FinSequence of D9] means F[;](d,p9^($1)) = (F[;](d,p9))^(F[;](d,$1
  ));
A1: for q9,d9 st P[q9] holds P[q9^<*d9*>]
  proof
    let q9,d9 such that
A2: F[;](d,p9^q9) = (F[;](d,p9))^(F[;](d,q9));
    thus F[;](d,p9^(q9^<*d9*>)) = F[;](d,(p9^q9)^<*d9*>) by FINSEQ_1:45
      .= (F[;](d,p9^q9))^<*F.(d,d9)*> by Th13
      .= (F[;](d,p9))^((F[;](d,q9))^<*F.(d,d9)*>) by A2,FINSEQ_1:45
      .= (F[;](d,p9))^(F[;](d,q9^<*d9*>)) by Th13;
  end;
  F[;](d,p9^(<*>D9)) = F[;](d,p9) by FINSEQ_1:47
    .= (F[;](d,p9))^(<*>E) by FINSEQ_1:47
    .= (F[;](d,p9))^(F[;](d,<*>D9)) by FINSEQ_2:93;
  then
A3: P[<*>D9];
  for q9 holds P[q9] from FINSEQ_2:sch 2(A3,A1);
  hence thesis;
end;

theorem Th15:
  F[:](p^<*d*>,d9) = (F[:](p,d9))^<*F.(d,d9)*>
proof
  set pd = p^<*d*>, q = F[:](p,d9);
  set r = F[:](pd,d9), s = q^<*F.(d,d9)*>;
  set i = len p;
A1: len q = i by FINSEQ_2:98;
  len pd = i + 1 by FINSEQ_2:19;
  then
A2: len r = i + 1 by FINSEQ_2:98;
  then
A3: dom r = Seg(i+1) by FINSEQ_1:def 3;
A4: now
    let j be Nat;
    assume
A5: j in dom r;
    now
      per cases by A3,A5,FINSEQ_2:8;
      suppose
A6:     j in Seg i;
        then
A7:     j in dom q by A1,FINSEQ_1:def 3;
        Seg len p = dom p by FINSEQ_1:def 3;
        hence F.(pd.j,d9) = F.(p.j,d9) by A6,FINSEQ_1:def 7
          .= q.j by A7,FUNCOP_1:35
          .= s.j by A7,FINSEQ_1:def 7;
      end;
      suppose
A8:     j = i+1;
        hence F.(pd.j,d9) = F.(d,d9) by FINSEQ_1:59
          .= s.j by A1,A8,FINSEQ_1:59;
      end;
    end;
    hence r.j = s.j by A5,FUNCOP_1:35;
  end;
  len s = len q + 1 by FINSEQ_2:19;
  hence thesis by A1,A2,A4,FINSEQ_2:10;
end;

theorem
  F[:](p^q,d9) = (F[:](p,d9))^(F[:](q,d9))
proof
  defpred P[FinSequence of D] means F[:](p^($1),d9) = (F[:](p,d9))^(F[:]($1,d9
  ));
A1: for q,d st P[q] holds P[q^<*d*>]
  proof
    let q,d such that
A2: F[:](p^q,d9) = (F[:](p,d9))^(F[:](q,d9));
    thus F[:](p^(q^<*d*>),d9) = F[:]((p^q)^<*d*>,d9) by FINSEQ_1:45
      .= (F[:](p^q,d9))^<*F.(d,d9)*> by Th15
      .= (F[:](p,d9))^((F[:](q,d9))^<*F.(d,d9)*>) by A2,FINSEQ_1:45
      .= (F[:](p,d9))^(F[:](q^<*d*>,d9)) by Th15;
  end;
  F[:](p^(<*>D),d9) = F[:](p,d9) by FINSEQ_1:47
    .= (F[:](p,d9))^(<*>E) by FINSEQ_1:47
    .= (F[:](p,d9))^(F[:](<*>D,d9)) by FINSEQ_2:99;
  then
A3: P[<*>D];
  for q holds P[q] from FINSEQ_2:sch 2(A3,A1);
  hence thesis;
end;

theorem
  for h being Function of D,E holds h*(i|->d) = i|->(h.d)
proof
  let h be Function of D,E;
  d in D;
  then d in dom h by FUNCT_2:def 1;
  hence thesis by FUNCOP_1:23;
end;

theorem Th18:
  F.:(i|->d,i|->d9) = i |-> (F.(d,d9))
proof
  [d,d9] in [:D,D9:] by ZFMISC_1:def 2;
  then [d,d9] in dom F by FUNCT_2:def 1;
  hence thesis by Th8;
end;

theorem
  F[;](d,i|->d9) = i |-> (F.(d,d9))
proof
  thus F[;](d,i|->d9) = F.:(i |->d,i|->d9) by FUNCOP_1:19
    .= i |-> (F.(d,d9)) by Th18;
end;

theorem
  F[:](i|->d,d9) = i |-> (F.(d,d9))
proof
  thus F[:](i|->d,d9) = F.:(i |->d,i|->d9) by FUNCOP_1:19
    .= i |-> (F.(d,d9)) by Th18;
end;

theorem
  F.:(i|->d,T9) = F[;](d,T9)
proof
  dom T9 = Seg len T9 by FINSEQ_1:def 3
    .= Seg i by FINSEQ_1:def 18;
  hence thesis;
end;

theorem
  F.:(T,i|->d) = F[:](T,d)
proof
  dom T = Seg len T by FINSEQ_1:def 3
    .= Seg i by FINSEQ_1:def 18;
  hence thesis;
end;

theorem
  F[;](d,T9) = F[;](d,id D9)*T9
proof
  rng T9 c= D9;
  hence F[;](d,T9) = F[;](d,(id D9)*T9) by RELAT_1:79
    .= F[;](d,(id D9))*T9 by FUNCOP_1:44;
end;

theorem
  F[:](T,d) = F[:](id D,d)*T
proof
  rng T c= D;
  hence F[:](T,d) = F[:]((id D)*T,d) by RELAT_1:79
    .= F[:]((id D),d)*T by FUNCOP_1:37;
end;
 
:: Binary Operations

reserve F,G for BinOp of D;
reserve u for UnOp of D;
reserve H for BinOp of E;

Lm4: T is Function of Seg i,D
proof
  len T = i by FINSEQ_1:def 18;
  then Seg i = dom T by FINSEQ_1:def 3;
  hence thesis by FINSEQ_2:30;
end;

theorem Th25:
  F is associative implies F[;](d,id D)*(F.:(f,f9)) = F.:(F[;](d, id D)*f,f9)
proof
  assume
A1: F is associative;
  now
    let c;
    thus (F[;](d,id D)*(F.:(f,f9))).c = (F[;](d,id D)).((F.:(f,f9)).c) by
    FUNCT_2:21
      .= (F[;](d,id D)).(F.(f.c,f9.c)) by FUNCOP_1:48
      .= F.(d,(id D).(F.(f.c,f9.c))) by FUNCOP_1:66
      .= F.(d,F.(f.c,f9.c)) by FUNCT_1:35
      .= F.(F.(d,f.c),f9.c) by A1,BINOP_1:def 3
      .= F.((F[;](d,f)).c,f9.c) by FUNCOP_1:66
      .= F.(((F[;](d,id D))*f).c,f9.c) by FUNCOP_1:69
      .= (F.:(F[;](d,id D)*f,f9)).c by FUNCOP_1:48;
  end;
  hence thesis by FUNCT_2:113;
end;

theorem Th26:
  F is associative implies F[:](id D,d)*(F.:(f,f9)) = F.:(f,F[:]( id D,d)*f9)
proof
  assume
A1: F is associative;
  now
    let c;
    thus (F[:](id D,d)*(F.:(f,f9))).c = (F[:](id D,d)).((F.:(f,f9)).c) by
    FUNCT_2:21
      .= (F[:](id D,d)).(F.(f.c,f9.c)) by FUNCOP_1:48
      .= F.((id D).(F.(f.c,f9.c)),d) by FUNCOP_1:60
      .= F.(F.(f.c,f9.c),d) by FUNCT_1:35
      .= F.(f.c,F.(f9.c,d)) by A1,BINOP_1:def 3
      .= F.(f.c,(F[:](f9,d)).c) by FUNCOP_1:60
      .= F.(f.c,((F[:](id D,d))*f9).c) by FUNCOP_1:63
      .= (F.:(f,F[:](id D,d)*f9)).c by FUNCOP_1:48;
  end;
  hence thesis by FUNCT_2:113;
end;

