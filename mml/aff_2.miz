environ

 vocabularies DIRAF, AFF_1, ANALOAF, INCSP_1, AFF_2;
 notations STRUCT_0, ANALOAF, DIRAF, AFF_1;
 constructors AFF_1;
 theorems AFF_1;

begin

reserve AP for AffinPlane,
  a,a',b,b',c,c',x,y,o,p,q,r,s for Element of AP,
  A,C
  ,C',D,K,M,N,P,T for Subset of AP;

definition
  let AP;
  attr AP is satisfying_PPAP means
  :Def1:
  for M,N,a,b,c,a',b',c' st M is
  being_line & N is being_line & a in M & b in M & c in M & a' in N & b' in N &
  c' in N & a,b' // b,a' & b,c' // c,b' holds a,c' // c,a';
end;

definition
  let AP be AffinSpace;
  attr AP is Pappian means
  :Def2:
  for M,N being Subset of AP, o,a,b,c,a',b',c'
being Element of AP st M is being_line & N is being_line & M<>N & o in M & o in
N & o<>a & o<>a' & o<>b & o<>b' & o<>c & o<>c' & a in M & b in M & c in M & a'
  in N & b' in N & c' in N & a,b' // b,a' & b,c' // c,b' holds a,c' // c,a';
end;

definition
  let AP;
  attr AP is satisfying_PAP_1 means
  :Def3:
  for M,N,o,a,b,c,a',b',c' st M is
being_line & N is being_line & M<>N & o in M & o in N & o<>a & o<>a' & o<>b & o
<>b' & o<>c & o<>c' & a in M & b in M & c in M & b' in N & c' in N & a,b' // b,
  a' & b,c' // c,b' & a,c' // c,a' & b<>c holds a' in N;
end;

definition
  let AP be AffinSpace;
  attr AP is Desarguesian means
  :Def4:
  for A,P,C being Subset of AP, o,a,b,c,
a',b',c' being Element of AP st o in A & o in P & o in C & o<>a & o<>b & o<>c &
a in A & a' in A & b in P & b' in P & c in C & c' in C & A is being_line & P is
being_line & C is being_line & A<>P & A<>C & a,b // a',b' & a,c // a',c' holds
  b,c // b',c';
end;

definition
  let AP;
  attr AP is satisfying_DES_1 means
  :Def5:
  for A,P,C,o,a,b,c,a',b',c' st o in
A & o in P & o<>a & o<>b & o<>c & a in A & a' in A & b in P & b' in P & c in C
& c' in C & A is being_line & P is being_line & C is being_line & A<>P & A<>C &
a,b // a',b' & a,c // a',c' & b,c // b',c' & not LIN a,b,c & c <>c' holds o in
  C;
end;

definition
  let AP;
  attr AP is satisfying_DES_2 means
  for A,P,C,o,a,b,c,a',b',c' st o in A & o
in P & o in C & o<>a & o<>b & o<>c & a in A & a' in A & b in P & b' in P & c in
C & A is being_line & P is being_line & C is being_line & A<>P & A<>C & a,b //
  a',b' & a,c // a',c' & b,c // b',c' holds c' in C;
end;

definition
  let AP be AffinSpace;
  attr AP is Moufangian means
  :Def7:
  for K being Subset of AP, o,a,b,c,a',b',
  c' being Element of AP st K is being_line & o in K & c in K & c' in K & not a
in K & o<>c & a<>b & LIN o,a,a' & LIN o,b,b' & a,b // a',b' & a,c // a',c' & a,
  b // K holds b,c // b',c';
end;

definition
  let AP;
  attr AP is satisfying_TDES_1 means
  :Def8:
  for K,o,a,b,c,a',b',c' st K is
being_line & o in K & c in K & c' in K & not a in K & o<>c & a<>b & LIN o,a,a'
  & a,b // a',b' & b,c // b',c' & a,c // a',c' & a,b // K holds LIN o,b,b';
end;

definition
  let AP;
  attr AP is satisfying_TDES_2 means
  :Def9:
  for K,o,a,b,c,a',b',c' st K is
being_line & o in K & c in K & c' in K & not a in K & o<>c & a<>b & LIN o,a,a'
  & LIN o,b,b' & b,c // b',c' & a,c // a',c' & a,b // K holds a,b // a',b';
end;

definition
  let AP;
  attr AP is satisfying_TDES_3 means
  :Def10:
  for K,o,a,b,c,a',b',c' st K is
being_line & o in K & c in K & not a in K & o<>c & a<>b & LIN o,a,a' & LIN o,b,
  b' & a,b // a',b' & a,c // a',c' & b,c // b',c' & a,b // K holds c' in K;
end;

definition
  let AP be AffinSpace;
  attr AP is translational means
  :Def11:
  for A,P,C being Subset of AP, a,b,c,
  a',b',c' being Element of AP st A // P & A // C & a in A & a' in A & b in P &
  b' in P & c in C & c' in C & A is being_line & P is being_line & C is
  being_line & A<>P & A<>C & a,b // a',b' & a,c // a',c' holds b,c // b',c';
end;

definition
  let AP;
  attr AP is satisfying_des_1 means
  :Def12:
  for A,P,C,a,b,c,a',b',c' st A // P
& a in A & a' in A & b in P & b' in P & c in C & c' in C & A is being_line & P
is being_line & C is being_line & A<>P & A<>C & a,b // a',b' & a,c // a',c' & b
  ,c // b',c' & not LIN a,b,c & c <>c' holds A // C;
end;

definition
  let AP be AffinSpace;
  attr AP is satisfying_pap means
  :Def13:
  for M,N being Subset of AP, a,b,c,a'
,b',c' being Element of AP st M is being_line & N is being_line & a in M & b in
M & c in M & M // N & M<>N & a' in N & b' in N & c' in N & a,b' // b,a' & b,c'
  // c,b' holds a,c' // c,a';
end;

definition
  let AP;
  attr AP is satisfying_pap_1 means
  :Def14:
  for M,N,a,b,c,a',b',c' st M is
being_line & N is being_line & a in M & b in M & c in M & M // N & M<>N & a' in
N & b' in N & a,b' // b,a' & b,c' // c,b' & a,c' // c,a' & a'<>b' holds c' in N
  ;
end;

canceled 14;

theorem
  AP is Pappian iff AP is satisfying_PAP_1
proof
A1: now
    assume
A2: AP is satisfying_PAP_1;
    thus AP is Pappian
    proof
      let M,N,o,a,b,c,a',b',c';
      assume that
A3:   M is being_line and
A4:   N is being_line and
A5:   M<>N & o in M & o in N and
A6:   o<>a and
A7:   o<>a' and
A8:   o<>b and
A9:   o<>b' and
A10:  o<>c & o<>c' and
A11:  a in M and
A12:  b in M and
A13:  c in M and
A14:  a' in N and
A15:  b' in N and
A16:  c' in N and
A17:  a,b' // b,a' and
A18:  b,c' // c,b';
      set A=Line(a,c'), P=Line(b,a');
A19:  b<>a' by A3,A4,A5,A7,A12,A14,AFF_1:30;
      then
A20:  b in P by AFF_1:38;
      assume
A21:  not a,c' // c,a';
      then
A22:  a<>c' by AFF_1:12;
      then
A23:  a in A & c' in A by AFF_1:38;
A24:  a' in P by A19,AFF_1:38;
      A is being_line by A22,AFF_1:38;
      then consider K such that
A25:  c in K and
A26:  A // K by AFF_1:63;
A27:  b<>c
      proof
        assume
A28:    b=c;
        then LIN b,c',b' by A18,AFF_1:def 1;
        then LIN b',c',b by AFF_1:15;
        then b'=c' or b in N by A4,A15,A16,AFF_1:39;
        hence contradiction by A3,A4,A5,A8,A12,A17,A21,A28,AFF_1:30;
      end;
A29:  b'<>c'
      proof
        assume
        b'=c';
        then b',b // b',c by A18,AFF_1:13;
        then LIN b',b,c by AFF_1:def 1;
        then LIN b,c,b' by AFF_1:15;
        then b' in M by A3,A12,A13,A27,AFF_1:39;
        hence contradiction by A3,A4,A5,A9,A15,AFF_1:30;
      end;
A30:  not P // K
      proof
        assume
        P // K;
        then P // A by A26,AFF_1:58;
        then b,a' // a,c' by A23,A20,A24,AFF_1:53;
        then a,b' // a,c' by A17,A19,AFF_1:14;
        then LIN a,b',c' by AFF_1:def 1;
        then LIN b',c',a by AFF_1:15;
        then a in N by A4,A15,A16,A29,AFF_1:39;
        hence contradiction by A3,A4,A5,A6,A11,AFF_1:30;
      end;
A31:  P is being_line by A19,AFF_1:38;
      K is being_line by A26,AFF_1:50;
      then consider x such that
A32:  x in P and
A33:  x in K by A31,A30,AFF_1:72;
A34:  a,c' // c,x by A23,A25,A26,A33,AFF_1:53;
      LIN b,x,a' by A31,A20,A24,A32,AFF_1:33;
      then b,x // b,a' by AFF_1:def 1;
      then a,b' // b,x by A17,A19,AFF_1:14;
      then x in N by A2,A3,A4,A5,A6,A8,A9,A10,A11,A12,A13,A15,A16,A18
      ,A27,A34,Def3;
      then N=P by A4,A14,A21,A31,A24,A32,A34,AFF_1:30;
      hence contradiction by A3,A4,A5,A8,A12,A20,AFF_1:30;
    end;
  end;
  now
    assume
A35: AP is Pappian;
    thus AP is satisfying_PAP_1
    proof
      let M,N,o,a,b,c,a',b',c';
      assume that
A36:  M is being_line and
A37:  N is being_line and
A38:  M<>N and
A39:  o in M and
A40:  o in N and
A41:  o<>a and
      o<>a' and
A42:  o<>b and
A43:  o<>b' and
A44:  o<>c and
A45:  o<>c' and
A46:  a in M and
A47:  b in M and
A48:  c in M and
A49:  b' in N and
A50:  c' in N and
A51:  a,b' // b,a' and
A52:  b,c' // c,b' and
A53:  a,c' // c,a' and
A54:  b<>c;
A55:  a<>c' by A36,A37,A38,A39,A40,A41,A46,A50,AFF_1:30;
A56:  b<>a'
      proof
        assume
        b=a';
        then c,b // a,c' by A53,AFF_1:13;
        then c' in M by A36,A46,A47,A48,A54,AFF_1:62;
        hence contradiction by A36,A37,A38,A39,A40,A45,A50,AFF_1:30;
      end;
      not b,a' // N
      proof
        assume
A57:    b,a' // N;
        b,a' // a,b' by A51,AFF_1:13;
        then a,b' // N by A56,A57,AFF_1:46;
        then b',a // N by AFF_1:48;
        then a in N by A37,A49,AFF_1:37;
        hence contradiction by A36,A37,A38,A39,A40,A41,A46,AFF_1:30;
      end;
      then consider x such that
A58:  x in N and
A59:  LIN b,a',x by A37,AFF_1:73;
A60:  b,a' // b,x by A59,AFF_1:def 1;
A61:  o<>x
      proof
        assume
        o=x;
        then b,o // a,b' by A51,A56,A60,AFF_1:14;
        then b' in M by A36,A39,A42,A46,A47,AFF_1:62;
        hence contradiction by A36,A37,A38,A39,A40,A43,A49,AFF_1:30;
      end;
      a,b' // b,x by A51,A56,A60,AFF_1:14;
      then a,c' // c,x by A35,A36,A37,A38,A39,A40,A41,A42,A43,A44,A45,A46,A47
      ,A48,A49
      ,A50,A52,A58,A61,Def2;
      then c,a' // c,x by A53,A55,AFF_1:14;
      then LIN c,a',x by AFF_1:def 1;
      then
A62:  LIN a',x,c by AFF_1:15;
A63:  LIN a',x,x by AFF_1:16;
      assume
A64:  not a' in N;
      LIN a',x,b by A59,AFF_1:15;
      then x in M by A36,A47,A48,A54,A64,A58,A62,A63,AFF_1:17,39;
      hence contradiction by A36,A37,A38,A39,A40,A58,A61,AFF_1:30;
    end;
  end;
  hence thesis by A1;
end;

theorem
  AP is Desarguesian iff AP is satisfying_DES_1
proof
A1: now
    assume
A2: AP is satisfying_DES_1;
    thus AP is Desarguesian
    proof
      let A,P,C,o,a,b,c,a',b',c';
      assume that
A3:   o in A and
A4:   o in P and
A5:   o in C and
A6:   o<>a and
A7:   o<>b and
A8:   o<>c and
A9:   a in A and
A10:  a' in A and
A11:  b in P and
A12:  b' in P and
A13:  c in C and
A14:  c' in C and
A15:  A is being_line and
A16:  P is being_line and
A17:  C is being_line and
A18:  A<>P and
A19:  A<>C and
A20:  a,b // a',b' and
A21:  a,c // a',c';
      assume
A22:  not b,c // b',c';
A23:  a'<>b'
      proof
A24:    a',c' // c,a by A21,AFF_1:13;
        assume
A25:    a'=b';
        then a' in C by A3,A4,A5,A10,A12,A15,A16,A18,AFF_1:30;
        then a in C or a'=c' by A13,A14,A17,A24,AFF_1:62;
        hence contradiction by A3,A5,A6,A9,A15,A17,A19,A22,A25, AFF_1
        :12,30;
      end;
A26:  a'<>c'
      proof
        assume
        a'=c';
        then
A27:    a' in P by A3,A4,A5,A10,A14,A15,A17,A19,AFF_1:30;
        a',b' // b,a by A20,AFF_1:13;
        then a in P by A11,A12,A16,A23,A27,AFF_1:62;
        hence contradiction by A3,A4,A6,A9,A15,A16,A18,AFF_1:30;
      end;
A28:  o<>c'
      proof
        assume
A29:    o=c';
        a',c' // a,c by A21,AFF_1:13;
        then c in A by A3,A9,A10,A15,A26,A29,AFF_1:62;
        hence contradiction by A3,A5,A8,A13,A15,A17,A19,AFF_1:30;
      end;
      set K=Line(a',c'), M=Line(a,c), N=Line(b',c');
A30:  a<>c by A3,A5,A6,A9,A13,A15,A17,A19,AFF_1:30;
      then
A31:  c in M by AFF_1:38;
A32:  a<>b by A3,A4,A6,A9,A11,A15,A16,A18,AFF_1:30;
A33:  not LIN a',b',c'
      proof
        assume
A34:    LIN a',b',c';
        then a',b' // a',c' by AFF_1:def 1;
        then a',b' // a,c by A21,A26,AFF_1:14;
        then a,b // a,c by A20,A23,AFF_1:14;
        then LIN a,b,c by AFF_1:def 1;
        then LIN b,c,a by AFF_1:15;
        then b,c // b,a by AFF_1:def 1;
        then b,c // a,b by AFF_1:13;
        then
A35:    b,c // a',b' by A20,A32,AFF_1:14;
        LIN b',c',a' by A34,AFF_1:15;
        then b',c' // b',a' by AFF_1:def 1;
        then b',c' // a',b' by AFF_1:13;
        hence contradiction by A22,A23,A35,AFF_1:14;
      end;
A36:  b'<>c' by A22,AFF_1:12;
      then
A37:  b' in N & c' in N by AFF_1:38;
      N is being_line by A36,AFF_1:38;
      then consider D such that
A38:  b in D and
A39:  N // D by AFF_1:63;
A40:  a in M by A30,AFF_1:38;
A41:  not M // D
      proof
        assume
        M // D;
        then M // N by A39,AFF_1:58;
        then a,c // b',c' by A40,A31,A37,AFF_1:53;
        then a',c' // b',c' by A21,A30,AFF_1:14;
        then c',a' // c',b' by AFF_1:13;
        then LIN c',a',b' by AFF_1:def 1;
        hence contradiction by A33,AFF_1:15;
      end;
A42:  M is being_line by A30,AFF_1:38;
      D is being_line by A39,AFF_1:50;
      then consider x such that
A43:  x in M and
A44:  x in D by A42,A41,AFF_1:72;
      LIN a,c,x by A42,A40,A31,A43,AFF_1:33;
      then a,c // a,x by AFF_1:def 1;
      then
A45:  a,x // a',c' by A21,A30,AFF_1:14;
      set T=Line(c',x);
A46:  a<>a'
      proof
        assume
A47:    a=a';
        then LIN a,c,c' by A21,AFF_1:def 1;
        then LIN c,c',a by AFF_1:15;
        then
A48:    c =c' or a in C by A13,A14,A17,AFF_1:39;
        LIN a,b,b' by A20,A47,AFF_1:def 1;
        then LIN b,b',a by AFF_1:15;
        then b=b' or a in P by A11,A12,A16,AFF_1:39;
        hence contradiction by A3,A4,A5,A6,A9,A15,A16,A17,A18,A19,A22,
        A48,AFF_1:11,30;
      end;
A49:  x<>c'
      proof
        assume
        x=c';
        then c',a // c',a' by A45,AFF_1:13;
        then LIN c',a,a' by AFF_1:def 1;
        then LIN a,a',c' by AFF_1:15;
        then c' in A by A9,A10,A15,A46,AFF_1:39;
        hence contradiction by A3,A5,A14,A15,A17,A19,A28,AFF_1:30;
      end;
      then
A50:  T is being_line & c' in T by AFF_1:38;
A51:  b,x // b',c' by A37,A38,A39,A44,AFF_1:53;
A52:  x in T by A49,AFF_1:38;
A53:  a<>x
      proof
        assume
        a=x;
        then a,b // b',c' by A37,A38,A39,A44,AFF_1:53;
        then a',b' // b',c' by A20,A32,AFF_1:14;
        then b',a' // b',c' by AFF_1:13;
        then LIN b',a',c' by AFF_1:def 1;
        hence contradiction by A33,AFF_1:15;
      end;
      not LIN a,b,x
      proof
        assume
        LIN a,b,x;
        then a,b // a,x by AFF_1:def 1;
        then a,b // a',c' by A45,A53,AFF_1:14;
        then a',b' // a',c' by A20,A32,AFF_1:14;
        hence contradiction by A33,AFF_1:def 1;
      end;
      then o in T by A2,A3,A4,A6,A7,A9,A10,A11,A12,A15,A16,A18,A20,A45
      ,A51,A49,A50,A52,Def5;
      then x in C by A5,A14,A17,A28,A50,A52,AFF_1:30;
      then C=M by A13,A17,A22,A42,A31,A43,A51,AFF_1:30;
      hence contradiction by A3,A5,A6,A9,A15,A17,A19,A40,AFF_1:30;
    end;
  end;
  now
    assume
A54: AP is Desarguesian;
    thus AP is satisfying_DES_1
    proof
      let A,P,C,o,a,b,c,a',b',c';
      assume that
A55:  o in A and
A56:  o in P and
A57:  o<>a and
A58:  o<>b and
      o<>c and
A59:  a in A and
A60:  a' in A and
A61:  b in P and
A62:  b' in P and
A63:  c in C and
A64:  c' in C and
A65:  A is being_line and
A66:  P is being_line and
A67:  C is being_line and
A68:  A<>P and
A69:  A<>C and
A70:  a,b // a',b' and
A71:  a,c // a',c' and
A72:  b,c // b',c' and
A73:  not LIN a,b,c and
A74:  c <>c';
      set K=Line(o,c');
      assume
A75:  not o in C;
      then
A76:  K is being_line by A64,AFF_1:38;
A77:  a'<>c'
      proof
        assume
A78:    a'=c';
        then b,c // a',b' by A72,AFF_1:13;
        then a,b // b,c or a'=b' by A70,AFF_1:14;
        then b,a // b,c or a'=b' by AFF_1:13;
        then LIN b,a,c or a'=b' by AFF_1:def 1;
        hence contradiction by A55,A56,A60,A62,A64,A65,A66,A68,A73,A75,A78,
        AFF_1:15,30;
      end;
A79:  A<>K
      proof
        assume
        A=K;
        then
A80:    c' in A by A55,AFF_1:38;
        a',c' // a,c by A71,AFF_1:13;
        then c in A by A59,A60,A65,A77,A80,AFF_1:62;
        hence contradiction by A63,A64,A65,A67,A69,A74,A80,AFF_1:30;
      end;
A81:  a<>c by A73,AFF_1:16;
A82:  o in K by A64,A75,AFF_1:38;
A83:  c' in K by A64,A75,AFF_1:38;
      not a,c // K
      proof
        assume
        a,c // K;
        then a',c' // K by A71,A81,AFF_1:46;
