        then a'=b' or LIN a,c,b by AFF_1:def 1;
        then b,c // a,c by A71,A72,A73,A77,AFF_1:14,15;
        then c,b // c,a by AFF_1:13;
        then LIN c,b,a by AFF_1:def 1;
