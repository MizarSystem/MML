A84:    a' in P by A55,A56,A60,A65,A76,A82,A79,AFF_1:30;
        a',b' // b,a by A70,AFF_1:13;
        then a'=b' or a in P by A61,A62,A66,A84,AFF_1:62;
        then a,c // b,c by A55,A56,A57,A59,A65,A66,A68,A71,A72,A77,
