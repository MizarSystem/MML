        then a,b // a,c by A47,A64,AFF_1:5;
        hence contradiction by A56,AFF_1:def 1;
      end;
A66:  not K // M
      proof
        assume K // M;
        then K // N by A62,AFF_1:44;
        then a,c // b9,c9 by A51,A59,A54,A63,AFF_1:39;
        then a9,c9 // b9,c9 by A47,A50,AFF_1:5;
        then c9,a9 // c9,b9 by AFF_1:4;
