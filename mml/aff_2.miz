      end;
A59:  c in K by A50,AFF_1:24;
A60:  N is being_line by A53,AFF_1:24;
      then consider M such that
A61:  b in M and
A62:  N // M by AFF_1:49;
A63:  c9 in N by A53,AFF_1:24;
A64:  a9<>c9 by A34,A36,A40,A45,AFF_1:45;
