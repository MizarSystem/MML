,A45,A46,A738,XREAL_1:11;
                  then j-'len h11+2<=len h2-2+2 by A21,A875,XREAL_1:8;
                  then A880:1<=(j-'len h11 +2-'1) & (j-'len h11 +2-'1)<=len h21
                                    by A23,A878,Lm1,BINARITH:60,62;
                  A881:j-'len h11+2-'1=j-'len h11+2-1
                      by A879,Lm1,BINARITH:57;
                  set k=j-'len h11+2-'1;
                     len h11+1-len h11<=j-len h11 by A21,A873,XREAL_1:11;
                  then j-'len h11=j-len h11 by BINARITH:57;
                  then i+1-'len h11<j-'len h11 by A748,A869,XREAL_1:11;
                  then i+1-'len h11+2<j-'len h11+2 by XREAL_1:8;
                  then A882:(i+1)-'len h11+2-'1<j-'len h11+2-'1
                            by A868,A881,XREAL_1:11;
                  A883:k in dom h2 by A22,A880,FINSEQ_3:27;
                     j in dom h0 by A738,A741,FINSEQ_3:27;
                  then h0/.j=h0.j by FINSEQ_4:def 4;
                 hence contradiction
      by A22,A67,A828,A864,A870,A871,A877,A882,A883,FUNCT_1:def 8;
            end;
          end;
     hence Segment(h0/.i,h0/.(i+1),P) misses Segment(h0/.j,h0/.(j+1),P)
                   by A1,A744,A745,A750,A753,Th13;
    end;
   let i be Element of NAT such that
A884: 1 < i and
A885: i+1 < len h0;
A886:  i<len h0 by A885,NAT_1:13;
A887: LE h0/.i,h0/.(i+1),P by A546,A884,A885;
A888: 1<i+1 by A884,NAT_1:13;
A889: h0/.i<>h0/.(i+1) by A546,A884,A885;
A890: i in dom h0 by A884,A886,FINSEQ_3:27;
     then h0/.i = h0.i by FINSEQ_4:def 4;
then A891: h0/.i<>W-min P by A39,A63,A68,A884,A890,FUNCT_1:def 8;
A892: i+1 in dom h0 by A885,A888,FINSEQ_3:27;
A893: len h0-len h11 <= len (mid(h21,2,len h21 -'1)) by A40;
      set k=len h0-'len h11+2-'1;
         0<=len h0-'len h11 by NAT_1:2;
      then 0+2<=len h0-'len h11 +2 by XREAL_1:8;
then A894: 2-'1<=len h0-'len h11 +2-'1 by BINARITH:60;
         len h0-'len h11+2<=len h2-2+2 by A23,A40,A42,A45,A46,A56;
      then (len h0-'len h11 +2-'1)<=len h21 by A23,BINARITH:62;
      then A895: (len h0-'len h11 +2-'1) in dom h21 by A894,Lm1,FINSEQ_3:27;
   h0.len h0=mid(h21,2,len h21 -'1).(len h0 -len h11)
                        by A21,A40,A50,FINSEQ_1:36;
then A896: h0.len h0=h21.(len h0-'len h11 +2-'1)
             by A21,A43,A44,A55,A56,A893,JORDAN3:27;
A897: h21.k=g2.(h2.k) by A22,A895,FUNCT_1:23;
      A898: h2.k in rng h2 by A22,A895,FUNCT_1:def 5;
then A899: h0.len h0 in Lower_Arc(P) by A13,A15,A16,A896,A897,BORSUK_1:83
,FUNCT_1:def 5;
A900:LE h0/.(i+1),h0/.len h0,P
      proof
       per cases;
       suppose A901:i+1<=len h1;
then A902:      h0.(i+1)=h11.(i+1) by A21,A888,FINSEQ_1:85;
            i+1 in dom h1 by A888,A901,FINSEQ_3:27;
         then A903:      h11.(i+1)=g1.(h1.(i+1)) by FUNCT_1:23;
                     i+1 in dom h1 by A888,A901,FINSEQ_3:27;
                  then A904: h1.(i+1) in rng h1 by FUNCT_1:def 5;
                  A905:h0/.(i+1)=h0.(i+1) by A892,FINSEQ_4:def 4;
                          g1.(h1.(i+1)) in rng g1 by A6,A9,A904,BORSUK_1:83
,FUNCT_1:def 5;
        hence thesis by A4,A54,A125,A899,A902,A903,A905,JORDAN6:def 10;
       end;
       suppose A906:i+1>len h1;
A907:             0<=len h0-'len h11 by A21,A51,A56,XREAL_1:50;
                  A908:len h0-len h11
                      <= len (mid(h21,2,len h21 -'1)) by A40;
                   0+1<=len h0-'len h11+1+1-1 by A907,XREAL_1:8;
                  then A909:len h0-'len h11+2-'1=len h0-'len h11+2-1 by
BINARITH:57;
                     len h0-'len h11+2<=len h2-2+2
                        by A23,A40,A42,A45,A46,A56;
then A910: (len h0-'len h11 +2-'1)<=len h21 by A23,BINARITH:62;
                  then A911:len h0-'len h11 +2-'1 in dom h21
                  by A894,Lm1,FINSEQ_3:27;
A912:             len h11+1<=i+1 by A21,A906,NAT_1:13;
A913:             i+1<len h11 + len (mid(h21,2,len h21 -'1)) by A885,
FINSEQ_1:35;
                    A914: i+1-len h11<len h11 + len (mid(h21,2,len h21 -'1)) -
len h11
                             by A40,A885,XREAL_1:11;
                     len h1+1<=i+1 by A906,NAT_1:13;
                  then A915: len h1+1-len h1<=i+1-len h1 by XREAL_1:11;
then A916:             i+1-'len h11=i+1-len h11 by A21,BINARITH:57;
A917:              h0.(i+1) = mid(h21,2,len h21 -'1).(i+1 -len h11)
                                      by A912,A913,FINSEQ_1:36
                          .= h21.(i+1-'len h11+2-'1)
                    by A21,A43,A44,A914,A915,A916,JORDAN3:27;
A918:             (i+1)-len h11=(i+1)-'len h11 by A21,A906,BINARITH:50;
A919:             (len h0-'len h11 +2-'1) in dom h21 by A894,A910,Lm1,
FINSEQ_3:27;
                    h0.len h0=(mid(h21,2,len h21 -'1)).(len h0 -len h11)
                                      by A21,A40,A50,FINSEQ_1:36;
                  then A920:h0.len h0=h21.(len h0-'len h11 +2-'1)
                    by A21,A43,A44,A55,A56,A908,JORDAN3:27;
                     0<=(i+1)-'len h11 by NAT_1:2;
                  then 0+2<=(i+1)-'len h11 +2 by XREAL_1:8;
                  then A921:2-'1<=((i+1)-'len h11 +2-'1) by BINARITH:60;
                     (i+1)-len h11<=len h11+(len h2-2)-len h11
                                    by A23,A40,A42,A45,A46,A885,XREAL_1:11;
                  then (i+1)-'len h11+2<=len h2-2+2 by A918,XREAL_1:8;
then A922:           (i+1)-'len h11 +2-'1 <= len h21 by A23,BINARITH:62;
                  A923:(i+1)-'len h11+2-'1=(i+1)-'len h11+2-1
                                  by A921,Lm1,BINARITH:57;
                  set k=len h0-'len h11+2-'1;
                  set j0=(i+1)-'len h11+2-'1;
                     (i+1)-len h11<len h0-len h11 by A885,XREAL_1:11;
                  then (i+1)-'len h11+2<len h0-'len h11+2 by A56,A918,XREAL_1:8
;
                  then A924:j0<k by A909,A923,XREAL_1:11;
                  A925:h21.k=g2.(h2.k) by A22,A911,FUNCT_1:23;
A926:              h2.k in rng h2 by A22,A919,FUNCT_1:def 5;
A927:             h2.k<=1 by A16,A926,BORSUK_1:83,RCOMP_1:48;
                  A928:j0 in dom h2 by A22,A921,A922,Lm1,FINSEQ_3:27;
                  then A929:h0.(i+1)=g2.(h2.j0) by A917,FUNCT_1:23;
A930:              h2.j0 in rng h2 by A928,FUNCT_1:def 5;
then A931:             0<=h2.j0 by A16,BORSUK_1:83,RCOMP_1:48;
A932:             h2.j0<=1 by A16,A930,BORSUK_1:83,RCOMP_1:48;
                  A933:h2.(j0)<h2.(k)
                  by A16,A22,A919,A924,A928,GOBOARD1:def 1;
                  A934:h0/.(i+1)=h0.(i+1) by A892,FINSEQ_4:def 4;
                    g2.(h2.j0) in rng g2 by A15,A16,A930,BORSUK_1:83
,FUNCT_1:def 5;
                  then A935:h0.(i+1) in Lower_Arc(P) by A13,A917,A928,
FUNCT_1:23;
                    h0.(len h0) in Lower_Arc(P) by A13,A15,A16,A896,A897,A898,
BORSUK_1:83,FUNCT_1:def 5;
then A936:       h0/.len h0 in Lower_Arc P by A53,FINSEQ_4:def 4;
           LE h0/.(i+1),h0/.len h0,Lower_Arc(P),E-max(P),W-min(P)
             by A11,A12,A13,A14,A54,A920,A925,A927,A929,A931,A932,A933,A934,
Th19;
       hence thesis by A125,A934,A935,A936,JORDAN6:def 10;
      end;
      end;
             now assume A937:h0/.(i+1)=h0/.len h0;
            per cases;
            suppose A938:i+1<=len h1;
                   i+1 in dom h0 by A885,A888,FINSEQ_3:27;
                then A939:h0/.(i+1)=h0.(i+1) by FINSEQ_4:def 4;
                A940:h0.(i+1)=h11.(i+1) by A21,A888,A938,FINSEQ_1:85;
                A941:i+1 in dom h1 by A888,A938,FINSEQ_3:27;
                then A942:h0.(i+1)=g1.(h1.(i+1)) by A940,FUNCT_1:23;
                   h1.(i+1) in rng h1 by A941,FUNCT_1:def 5;
                then A943:h0.(i+1) in Upper_Arc(P)
                             by A4,A6,A9,A942,BORSUK_1:83,FUNCT_1:def 5;
A944: h0.len h0=(mid(h21,2,len h21 -'1)).(len h0 -len h11)
              by A21,A40,A50,FINSEQ_1:36;
                    len h0-len h11<= len (mid(h21,2,len h21 -'1))
                                             by A40;
                  then A945:h0.len h0=h21.(len h0-'len h11 +2-'1)
                             by A21,A43,A44,A55,A56,A944,JORDAN3:27;
                     0<=len h0-'len h11 by NAT_1:2;
                  then 0+2<=len h0-'len h11 +2 by XREAL_1:8;
                  then A946:2-'1<=(len h0-'len h11 +2-'1) by BINARITH:60;
                     len h0-'len h11+2<=len h2-2+2
                           by A23,A40,A42,A45,A46,A56;
then A947:            len h0-'len h11 +2-'1 <= len h21 by A23,BINARITH:62;
                  A948:len h0-'len h11+2-'1=len h0-'len h11+2-1
                     by A946,Lm1,BINARITH:57;
                  set k=len h0-'len h11+2-'1;
                    A949: len h0-'len h11+2<=len h2-2+2
                      by A23,A40,A42,A45,A46,A56;
                    A950: len h0-'len h11+2-'1<len h0-'len h11+2-'1+1 by
NAT_1:13;
                     1+2<=len h0-'len h11+2 by A57,XREAL_1:8;
                  then 1+2-1<=len h0-'len h11+2-1 by XREAL_1:11;
                  then A951:1<k by A948,XXREAL_0:2;
                  A952:k in dom h2 by A22,A946,A947,Lm1,FINSEQ_3:27;
                  then A953:h0.len h0=g2.(h2.k) by A945,FUNCT_1:23;
                     h2.k in rng h2 by A952,FUNCT_1:def 5;
                  then g2.(h2.k) in rng g2 by A15,A16,BORSUK_1:83,FUNCT_1:def 5
;
                  then h0.len h0 in Upper_Arc(P) /\ Lower_Arc(P)
                   by A13,A54,A937,A939,A943,A953,XBOOLE_0:def 3;
then A954:         h0.len h0 in {W-min(P),E-max(P)} by A1,JORDAN6:def 9;
A955:         h0.len h0 <> W-min P
                     by A22,A29,A31,A67,A945,A948,A949,A950,A952,FUNCT_1:def 8;
                  h0.len h0 <> E-max P
                    by A22,A28,A30,A67,A945,A951,A952,FUNCT_1:def 8;
              hence contradiction by A954,A955,TARSKI:def 2;
            end;
            suppose A956:i+1>len h1;
                  then A957:len h1 +1<=i+1 by NAT_1:13;
A958:        i+1 <= len h11 + len mid(h21,2,len h21 -'1) by A885,FINSEQ_1:35;
                  A959:h0.(i+1)=(mid(h21,2,len h21 -'1)).((i+1) -len h11)
                                     by A21,A40,A885,A957,FINSEQ_1:36;
                  A960:(i+1)-len h11=(i+1)-'len h11 by A21,A956,BINARITH:50;
                    A961: i+1-len h11
                   <=len h11 + len mid(h21,2,len h21 -'1)-len h11
                                         by A958,XREAL_1:11;
                     len h1 +1-len h1<=(i+1)-len h1 by A957,XREAL_1:11;
                  then A962:h0.(i+1)=h21.((i+1)-'len h11 +2-'1)
                             by A21,A43,A44,A959,A960,A961,JORDAN3:27;
                     0<=(i+1)-'len h11 by NAT_1:2;
                  then 0+2<=(i+1)-'len h11 +2 by XREAL_1:8;
                  then A963:2-'1<=((i+1)-'len h11 +2-'1) by BINARITH:60;
                     (i+1)-len h11<=len h1+(len h2-2)-len h11
                            by A21,A23,A40,A42,A45,A46,A885,XREAL_1:11;
                  then (i+1)-'len h11+2<=len h2-2+2 by A21,A960,XREAL_1:8;
then A964:            (i+1)-'len h11 +2-'1 <= len h21 by A23,BINARITH:62;
                  A965:(i+1)-'len h11+2-'1=(i+1)-'len h11+2-1
                                 by A963,Lm1,BINARITH:57;
                  set j0=(i+1)-'len h11+2-'1;
                     len h11+1-len h11<=(i+1)-len h11
                           by A21,A957,XREAL_1:11;
                  then A966:(i+1)-'len h11=(i+1)-len h11 by BINARITH:57;
                 A967:j0 in dom h2 by A22,A963,A964,Lm1,FINSEQ_3:27;
                     1<=i+1 by A884,NAT_1:13;
                  then i+1 in dom h0 by A885,FINSEQ_3:27;
                  then A968:h0/.(i+1)=h0.(i+1) by FINSEQ_4:def 4;
A969: h0.len h0=(mid(h21,2,len h21 -'1)).(len h0 -len h11)
                     by A21,A40,A50,FINSEQ_1:36;
                    len h0-len h11<= len (mid(h21,2,len h21 -'1))
                                             by A40;
                  then A970:h0.len h0=h21.(len h0-'len h11 +2-'1)
                             by A43,A44,A56,A57,A969,JORDAN3:27;
                     0<=len h0-'len h11 by NAT_1:2;
                  then 0+2<=len h0-'len h11 +2 by XREAL_1:8;
                  then A971:2-'1<=(len h0-'len h11 +2-'1) by BINARITH:60;
                     len h0-'len h11+2<=len h2-2+2
                    by A23,A40,A42,A45,A46,A56;
then A972:            len h0-'len h11 +2-'1 <= len h21 by A23,BINARITH:62;
                  A973:len h0-'len h11+2-'1=len h0-'len h11+2-1
                      by A971,Lm1,BINARITH:57;
                  set k=len h0-'len h11+2-'1;
                     len h0-'len h11=len h0-len h11 by A23,A40,A42,A45,A46,A49,
BINARITH:57;
                  then i+1-'len h11<len h0-'len h11 by A885,A966,XREAL_1:11;
                  then i+1-'len h11+2<len h0-'len h11+2 by XREAL_1:8;
                  then A974: (i+1)-'len h11+2-'1<k by A965,A973,XREAL_1:11;
                   k in dom h2 by A22,A971,A972,Lm1,FINSEQ_3:27;
              hence contradiction by A22,A54,A67,A937,A962,A967,A968,A970,A974,
FUNCT_1:def 8;
            end;
          end;
   hence Segment(h0/.len h0,h0/.1,P) misses Segment(h0/.i,h0/.(i+1),P)
                   by A1,A39,A64,A887,A889,A891,A900,Th14;
  end;
