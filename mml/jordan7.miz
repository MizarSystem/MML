        then
A809:   h0/.j=h0.(j) by PARTFUN1:def 8;
        len h11+1<=i+1 by A39,A780,NAT_1:13;
        then h0.(i+1)=(mid(h21,2,len h21 -'1)).(i+1 -len h11) by A804
        , FINSEQ_1:36
