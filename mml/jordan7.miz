        then j-'len h11+2<=len h2-2+2 by A39,A729,XREAL_1:8;
        then
A730:   (j-'len h11 +2-'1)<=len h21 by A47,NAT_D:44;
A731:   0+2<=j-'len h11 +2 by XREAL_1:8;
        then
A732:   j-'len h11+2-'1=j-'len h11+2-1 by Lm1,NAT_D:39,42;
