        end;
      end;
      suppose
A720:   i+1>len h1;
        then
A721:   j>len h1 by A691,XXREAL_0:2;
        then
A722:   len h1 +1<=j by NAT_1:13;
        then
A723:   len h1 +1-len h1<=j-len h1 by XREAL_1:11;
