        then
A806:   h0/.(i+1)=h0.(i+1) by PARTFUN1:def 8;
        len h1+1<=i+1 by A780,NAT_1:13;
        then
A807:   len h1+1-len h1<=i+1-len h1 by XREAL_1:11;
        then
A808:   i+1-'len h11=i+1-len h11 by A39,NAT_D:39;
