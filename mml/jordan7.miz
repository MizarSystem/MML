            end;
            end;
           hence x in g2.:([.h2/.j,h2/.(j+1).]) by FUNCT_1:def 12;
          end;
            g2.:([.h2/.j,h2/.(j+1).]) c= Segment(h0/.i,h0/.1,P)
          proof let x be set;assume x in g2.:([.h2/.j,h2/.(j+1).]);
            then consider y being set such that
      A453: y in dom g2 & y in [.h2/.j,h2/.(j+1).] & x=g2.y by FUNCT_1:def 12;
            A454:x in Lower_Arc(P) by A13,A453,FUNCT_1:def 5;
            then reconsider p1=x as Point of TOP-REAL 2;
            reconsider sy=y as Real by A453;
             A455:h2/.j<=sy & sy<=h2/.(j+1) by A453,RCOMP_1:48;
             A456:0<=sy & sy<=1 by A15,A453,RCOMP_1:48;
            A457:h0/.1=W-min(P) by A39,A63,FINSEQ_4:def 4;
A458:      Upper_Arc(P) \/ Lower_Arc(P)=P by A1,JORDAN6:def 9;
A459:        LE h0/.i,p1,Lower_Arc(P),E-max(P),W-min(P)
               by A11,A12,A13,A14,A430,A437,A438,A439,A440,A453,A455,A456,Th19;
               now per cases;
            case p1=W-min(P);
             hence LE h0/.i,p1,P or  h0/.i in P & p1=W-min(P)
              by A434,A437,A458,XBOOLE_0:def 2;
            end;
            case p1<>W-min(P);
             hence LE h0/.i,p1,P or  h0/.i in P & p1=W-min(P)
                              by A434,A437,A454,A459,JORDAN6:def 10;
            end;
            end;
            then x in {p: LE h0/.i,p,P or h0/.i in P & p=W-min(P)};
           hence x in Segment(h0/.i,h0/.1,P) by A457,Def1;
          end;
         then W=g2.:Q1 by A413,A442,XBOOLE_0:def 10;
        hence diameter(W)<e by A16,A420,A427,Lm1;
