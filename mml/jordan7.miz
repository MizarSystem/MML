                  then A530:h0.((i+1)+1)=h21.((i+1)+1-'len h11 +2-'1)
                    by A43,A44,A527,A528,JORDAN3:27;
                     1<(i+1)+1-'len h11+(2-1) by A529,NAT_1:13;
                  then A531: 0<(i+1)+1-'len h11+2-1;
                     0<=(i+1)-'len h11 by NAT_1:2;
                  then 0+2<=(i+1)-'len h11 +2 by XREAL_1:8;
                  then A532:2-'1<=((i+1)-'len h11 +2-'1) by BINARITH:60;
                     (i+1)-len h11<=len h1+(len h2-2)-len h11
                               by A21,A23,A40,A42,A45,A46,A461,XREAL_1:11;
                  then A533: (i+1)-'len h11+2<=len h2-2+2 by A21,A523,A525,
XREAL_1:8;
                  set j=(i+1)-'len h11+2-'1;
                     (i+1)-len h11<len h11+(len h2-2)-len h11
                  by A23,A40,A42,A45,A46,A461,XREAL_1:11;
                  then A534: (i+1)-len h11+2<len h2-2+2 by XREAL_1:8;
                  A535:j+1=(i+1)-'len h11+(1+1)-1+1 by A532,Lm1,BINARITH:57
