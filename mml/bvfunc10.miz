theorem  for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' b) '&' (b 'imp' c) '<' (a 'imp' (b '&' c))
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a 'imp' b) '&' (b 'imp' c)).z=TRUE;
    A2: ((a 'imp' b) '&' (b 'imp' c)).z
   =(a 'imp' b).z '&' (b 'imp' c).z by VALUAT_1:def 6
  .=('not' a 'or' b).z '&' (b 'imp' c).z by BVFUNC_4:8
  .=('not' a 'or' b).z '&' ('not' b 'or' c).z by BVFUNC_4:8
  .=(('not' a).z 'or' (b).z) '&' ('not' b 'or' c).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (b).z) '&' (('not'
b).z 'or' (c).z) by BVFUNC_1:def 7;
       now assume
      A3: (a 'imp' (b '&' c)).z<>TRUE;
      A4: (a 'imp' (b '&' c)).z
     =('not' a 'or' (b '&' c)).z by BVFUNC_4:8
    .=('not' a).z 'or' (b '&' c).z by BVFUNC_1:def 7
    .=('not' a).z 'or' ((b).z '&' (c).z) by VALUAT_1:def 6
    .='not' (a).z 'or' ((b).z '&' (c).z) by VALUAT_1:def 5;
      A5:'not' (a).z=TRUE or 'not' (a).z=FALSE by XBOOLEAN:def 3;
      A6: ((b).z '&' (c).z)=TRUE or ((b).z '&' (c).z)=FALSE
        by XBOOLEAN:def 3;
A7:  ('not' a).z=FALSE by A3,A4,A5,VALUAT_1:def 5;
         now per cases by A3,A4,A6,MARGREL1:45;
      case (b).z=FALSE;
      then (('not' a).z 'or' (b).z) '&' (('not' b).z 'or' (c).z)
     =FALSE by A7;
      hence thesis by A1,A2;
      end;
      case (c).z=FALSE;
      then (('not' a).z 'or' (b).z) '&' (('not' b).z 'or' (c).z)
     =(b).z '&' 'not' (b).z by  A7,VALUAT_1:def 5
    .=FALSE by XBOOLEAN:138;
      hence thesis by A1,A2;
      end;
      end;
      hence thesis;
    end;
    hence thesis;
end;

theorem  for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' b) '&' (b 'imp' c) '<' ((a 'or' b) 'imp' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a 'imp' b) '&' (b 'imp' c)).z=TRUE;
    A2: ((a 'imp' b) '&' (b 'imp' c)).z
   =(a 'imp' b).z '&' (b 'imp' c).z by VALUAT_1:def 6
  .=('not' a 'or' b).z '&' (b 'imp' c).z by BVFUNC_4:8
  .=('not' a 'or' b).z '&' ('not' b 'or' c).z by BVFUNC_4:8
  .=(('not' a).z 'or' (b).z) '&' ('not' b 'or' c).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (b).z) '&' (('not'
b).z 'or' (c).z) by BVFUNC_1:def 7;
       now assume
      A3: ((a 'or' b) 'imp' c).z<>TRUE;
      A4: ((a 'or' b) 'imp' c).z
     =('not'( a 'or' b) 'or' c).z by BVFUNC_4:8
    .=(('not' a '&' 'not' b) 'or' c).z by BVFUNC_1:16
    .=('not' a '&' 'not' b).z 'or' (c).z by BVFUNC_1:def 7
    .=(('not' a).z '&' ('not' b).z) 'or' (c).z by VALUAT_1:def 6;
      A5:(('not' a).z '&' ('not' b).z)=TRUE or (('not' a).z '&' ('not'
b).z)=FALSE
       by XBOOLEAN:def 3;
      A6: (c).z=TRUE or (c).z=FALSE
        by XBOOLEAN:def 3;
         now per cases by A3,A4,A5,MARGREL1:45;
      case ('not' a).z=FALSE;
      then (('not' a).z 'or' (b).z) '&' (('not' b).z 'or' (c).z)
     =(b).z '&' 'not' (b).z by  A3,A4,A6,VALUAT_1:def 5
    .=FALSE by XBOOLEAN:138;
      hence thesis by A1,A2;
      end;
      case ('not' b).z=FALSE;
      then (('not' a).z 'or' (b).z) '&' (('not' b).z 'or' (c).z)
     =(('not' a).z 'or' (b).z) '&' FALSE by A3,A4,XBOOLEAN:def 3
    .=FALSE;
      hence thesis by A1,A2;
      end;
      end;
      hence thesis;
    end;
    hence thesis;
end;

theorem Th19: for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' b) '&' (b 'imp' c) '<' (a 'imp' (b 'or' c))
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a 'imp' b) '&' (b 'imp' c)).z=TRUE;
    A2: ((a 'imp' b) '&' (b 'imp' c)).z
   =(a 'imp' b).z '&' (b 'imp' c).z by VALUAT_1:def 6
  .=('not' a 'or' b).z '&' (b 'imp' c).z by BVFUNC_4:8
  .=('not' a 'or' b).z '&' ('not' b 'or' c).z by BVFUNC_4:8
  .=(('not' a).z 'or' (b).z) '&' ('not' b 'or' c).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (b).z) '&' (('not'
b).z 'or' (c).z) by BVFUNC_1:def 7;
       now assume
      A3: (a 'imp' (b 'or' c)).z<>TRUE;
      A4: (a 'imp' (b 'or' c)).z
     =('not' a 'or' (b 'or' c)).z by BVFUNC_4:8
    .=('not' a).z 'or' (b 'or' c).z by BVFUNC_1:def 7
    .=('not' a).z 'or' ((b).z 'or' (c).z) by BVFUNC_1:def 7;
      A5:('not' a).z=TRUE or ('not' a).z=FALSE
       by XBOOLEAN:def 3;
      A6: ((b).z 'or' (c).z)=TRUE or ((b).z 'or' (c).z)=FALSE
