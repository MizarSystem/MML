            case
A10:          'not' (a).z=FALSE;
              now
                per cases by A5,A7,A6,A10,MARGREL1:45;
                case
                  'not' (b).z=FALSE;
                  hence thesis by A3,A2,A7,A10;
                end;
                case
                  (c).z=FALSE;
                  hence thesis by A3,A2,A7,A10;
                end;
              end;
              hence thesis;
            end;
            case
A11:          (b).z=FALSE;
