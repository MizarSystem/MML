    .=('not' a '&' b '&' c).z 'or' (a '&' 'not' b '&' c).z 'or'
      ((a).z '&' (b).z '&' ('not' c).z)
      by VALUAT_1:def 6
    .=('not' a '&' b '&' c).z 'or'
      ((a '&' 'not' b).z '&' (c).z) 'or'
      ((a).z '&' (b).z '&' ('not' c).z)
      by VALUAT_1:def 6
    .=('not' a '&' b '&' c).z 'or'
      ((a).z '&' ('not' b).z '&' (c).z) 'or'
      ((a).z '&' (b).z '&' ('not' c).z)
      by VALUAT_1:def 6
    .=(('not' a '&' b).z '&' (c).z) 'or'
      ((a).z '&' ('not' b).z '&' (c).z) 'or'
      ((a).z '&' (b).z '&' ('not' c).z)
      by VALUAT_1:def 6
    .=(('not' a).z '&' (b).z '&' (c).z) 'or'
      ((a).z '&' ('not' b).z '&' (c).z) 'or'
      ((a).z '&' (b).z '&' ('not' c).z)
      by VALUAT_1:def 6
    .=('not' (a).z '&' (b).z '&' (c).z) 'or'
      ((a).z '&' ('not' b).z '&' (c).z) 'or'
      ((a).z '&' (b).z '&' ('not' c).z)
      by VALUAT_1:def 5
    .=('not' (a).z '&' (b).z '&' (c).z) 'or'
      ((a).z '&' 'not' (b).z '&' (c).z) 'or'
      ((a).z '&' (b).z '&' ('not' c).z)
      by VALUAT_1:def 5
    .=('not' (a).z '&' (b).z '&' (c).z) 'or'
      ((a).z '&' 'not' (b).z '&' (c).z) 'or'
      ((a).z '&' (b).z '&' 'not' (c).z)
      by VALUAT_1:def 5;
then A5:  ('not' (a).z '&' (b).z '&' (c).z) 'or'
      ((a).z '&' 'not' (b).z '&' (c).z) 'or'
      ((a).z '&' (b).z '&' 'not' (c).z)=FALSE by A3,XBOOLEAN:def 3;
      A6:('not' (a).z '&' (b).z '&' (c).z) 'or'
      ((a).z '&' 'not' (b).z '&' (c).z)=TRUE or
      ('not' (a).z '&' (b).z '&' (c).z) 'or'
      ((a).z '&' 'not' (b).z '&' (c).z)=FALSE by XBOOLEAN:def 3;
      A7: ((a).z '&' (b).z '&' 'not' (c).z)=TRUE or
      ((a).z '&' (b).z '&' 'not' (c).z)=FALSE by XBOOLEAN:def 3;
      A8: ((a).z '&' 'not' (b).z '&' (c).z)=TRUE or
      ((a).z '&' 'not' (b).z '&' (c).z)=FALSE by XBOOLEAN:def 3;
then A9:  ('not' (a).z '&' (b).z '&' (c).z)=FALSE &
      ((a).z '&' 'not' (b).z '&' (c).z)=FALSE &
      ((a).z '&' (b).z '&' 'not' (c).z)=FALSE
      by A3,A4,A6,A7;
         now per cases by A9,MARGREL1:45;
      case A10: 'not' (a).z '&' (b).z=FALSE;
         now per cases by A10,MARGREL1:45;
        case A11:'not' (a).z=FALSE;
        then A12:(a).z=TRUE;
        then A13: 'not' (b).z '&' (c).z=FALSE by A9;
           now per cases by A13,MARGREL1:45;
        case 'not' (b).z=FALSE;
then A14:    (b).z=TRUE;
        then (a).z '&' (b).z '&' 'not' (c).z
       ='not' (c).z by A12;
        then (c).z=TRUE by A3,A4,A7;
        then ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
        ((c).z 'or' (a).z) '&' ('not' (a).z 'or' 'not' (b).z 'or' 'not'
(c).z)
       =FALSE by A11,A14;
        hence thesis by A1,A2;
        end;
        case (c).z=FALSE;
        then (a).z '&' (b).z '&' 'not' (c).z
       =(b).z by A12;
        then ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
        ((c).z 'or' (a).z) '&' ('not' (a).z 'or' 'not' (b).z 'or' 'not'
(c).z)
       =FALSE by A5,A6;
        hence thesis by A1,A2;
        end;
        end;
        hence thesis;
        end;
        case A15:(b).z=FALSE;
        then (a).z '&' 'not' (b).z '&' (c).z
       =((a).z '&' (c).z);
        then A16: (a).z '&' (c).z=FALSE by A3,A4,A8;
           now per cases by A16,MARGREL1:45;
          case (a).z=FALSE;
          then ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
          ((c).z 'or' (a).z) '&' ('not' (a).z 'or' 'not'
          (b).z 'or' 'not'
(c).z)
         =FALSE by A15;
          hence thesis by A1,A2;
          end;
          case (c).z=FALSE;
          then ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
          ((c).z 'or' (a).z) '&' ('not' (a).z 'or' 'not'
          (b).z 'or' 'not'
(c).z)
         =FALSE by A15;
          hence thesis by A1,A2;
        end;
        end;
        hence thesis;
      end;
      end;
      hence thesis;
      end;
      case A17:(c).z=FALSE;
      then A18: (a).z '&' (b).z '&' 'not' (c).z
     =((a).z '&' (b).z);
         now per cases by A3,A4,A7,A18,MARGREL1:45;
        case (a).z=FALSE;
        then ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
        ((c).z 'or' (a).z) '&' ('not' (a).z 'or' 'not' (b).z 'or' 'not'
(c).z)
       =FALSE by A17;
        hence thesis by A1,A2;
        end;
        case (b).z=FALSE;
        then ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
        ((c).z 'or' (a).z) '&' ('not' (a).z 'or' 'not' (b).z 'or' 'not'
(c).z)
       =FALSE by A17;
        hence thesis by A1,A2;
      end;
      end;
      hence thesis;
      end;
      end;
      hence thesis;
    end;
    hence thesis;
  end;
then A19:(a 'or' b) '&' (b 'or' c) '&' (c 'or' a) '&' 'not'( a '&' b '&' c) '<'
  ('not' a '&' b '&' c) 'or' (a '&' 'not' b '&' c) 'or' (a '&' b '&' 'not' c)
