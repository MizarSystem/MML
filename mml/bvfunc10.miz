environ

 vocabularies FUNCT_2, MARGREL1, ZF_LANG, BVFUNC_1, PARTIT1, FUNCT_1, NAT_1;
 notations XBOOLE_0, SUBSET_1, XBOOLEAN, MARGREL1, VALUAT_1, FUNCT_2, FRAENKEL,
      BVFUNC_1;
 constructors BINARITH, BVFUNC_1, NAT_D;
 registrations MARGREL1, VALUAT_1, FRAENKEL, XBOOLEAN;
 requirements BOOLE, SUBSET, NUMERALS, ARITHM;
 definitions BVFUNC_1, XBOOLEAN;
 theorems MARGREL1, BVFUNC_1, BVFUNC_4, BVFUNC_5, BVFUNC_6, BVFUNC_7, VALUAT_1,
      XBOOLEAN;

begin

::Chap. 1  Propositional Calculus

reserve Y for non empty set;

theorem  for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a '&' b) 'or' (b '&' c) 'or' (c '&' a)=
(a 'or' b) '&' (b 'or' c) '&' (c 'or' a)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
     for z being Element of Y st
    ((a '&' b) 'or' (b '&' c) 'or' (c '&' a)).z=TRUE
    holds
    ((a 'or' b) '&' (b 'or' c) '&' (c 'or' a)).z=TRUE
  proof
    let z be Element of Y;
    assume A1:((a '&' b) 'or' (b '&' c) 'or' (c '&' a)).z=TRUE;
A2:((a '&' b) 'or' (b '&' c) 'or' (c '&' a)).z
   =((a '&' b) 'or' (b '&' c)).z 'or' (c '&' a).z by BVFUNC_1:def 7
  .=(a '&' b).z 'or' (b '&' c).z 'or' (c '&' a).z by BVFUNC_1:def 7
  .=((a).z '&' (b).z) 'or' (b '&' c).z 'or' (c '&' a).z
   by VALUAT_1:def 6
  .=((a).z '&' (b).z) 'or' ((b).z '&' (c).z) 'or' (c '&' a).z
   by VALUAT_1:def 6
  .=((a).z '&' (b).z) 'or' ((b).z '&' (c).z) 'or' ((c).z '&' (a).z)
   by VALUAT_1:def 6;
       now assume A3: ((a 'or' b) '&' (b 'or' c) '&' (c 'or' a)).z<>TRUE;
         ((a 'or' b) '&' (b 'or' c) '&' (c 'or' a)).z
     =((a 'or' b) '&' (b 'or' c)).z '&' (c 'or' a).z by VALUAT_1:def 6
    .=(a 'or' b).z '&' (b 'or' c).z '&' (c 'or' a).z by VALUAT_1:def 6
    .=((a).z 'or' (b).z) '&' (b 'or' c).z '&' (c 'or' a).z
     by BVFUNC_1:def 7
    .=((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&' (c 'or' a).z
     by BVFUNC_1:def 7
    .=((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
      ((c).z 'or' (a).z)
     by BVFUNC_1:def 7;
  then A4: ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
      ((c).z 'or' (a).z) = FALSE by A3,XBOOLEAN:def 3;
         now per cases by A4,MARGREL1:45;
        case A5: ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z)=FALSE;
           now per cases by A5,MARGREL1:45;
          case A6:((a).z 'or' (b).z)=FALSE;
             (b).z=TRUE or (b).z=FALSE by XBOOLEAN:def 3;
          then (a).z=FALSE & (b).z=FALSE by A6;
          then ((a).z '&' (b).z) 'or' ((b).z '&' (c).z) 'or'
          ((c).z '&' (a).z)
         =FALSE;
          hence thesis by A1,A2;
          end;
          case A7:((b).z 'or' (c).z)=FALSE;
             (c).z=TRUE or (c).z=FALSE by XBOOLEAN:def 3;
          then (b).z=FALSE & (c).z=FALSE by A7;
          then ((a).z '&' (b).z) 'or' ((b).z '&' (c).z) 'or'
          ((c).z '&' (a).z)
         =FALSE;
          hence thesis by A1,A2;
        end;
        end;
        hence thesis;
        end;
        case A8:((c).z 'or' (a).z)=FALSE;
           (a).z=TRUE or (a).z=FALSE by XBOOLEAN:def 3;
        then (c).z=FALSE & (a).z=FALSE by A8;
        then ((a).z '&' (b).z) 'or' ((b).z '&' (c).z) 'or'
        ((c).z '&' (a).z)
       =FALSE;
        hence thesis by A1,A2;
      end;
      end;
      hence thesis;
    end;
    hence thesis;
  end;
  then A9:(a '&' b) 'or' (b '&' c) 'or' (c '&' a) '<'
  (a 'or' b) '&' (b 'or' c) '&' (c 'or' a) by BVFUNC_1:def 15;
     for z being Element of Y st
    ((a 'or' b) '&' (b 'or' c) '&' (c 'or' a)).z=TRUE
    holds
    ((a '&' b) 'or' (b '&' c) 'or' (c '&' a)).z=TRUE
  proof
    let z be Element of Y;
    assume A10:((a 'or' b) '&' (b 'or' c) '&' (c 'or' a)).z=TRUE;
A11:((a 'or' b) '&' (b 'or' c) '&' (c 'or' a)).z
   =((a 'or' b) '&' (b 'or' c)).z '&' (c 'or' a).z by VALUAT_1:def 6
  .=(a 'or' b).z '&' (b 'or' c).z '&' (c 'or' a).z by VALUAT_1:def 6
  .=((a).z 'or' (b).z) '&' (b 'or' c).z '&' (c 'or' a).z
   by BVFUNC_1:def 7
  .=((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&' (c 'or' a).z
   by BVFUNC_1:def 7
  .=((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
    ((c).z 'or' (a).z)
   by BVFUNC_1:def 7;
       now assume A12: ((a '&' b) 'or' (b '&' c) 'or' (c '&' a)).z<>TRUE;
      A13: ((a '&' b) 'or' (b '&' c) 'or' (c '&' a)).z
     =((a '&' b) 'or' (b '&' c)).z 'or' (c '&' a).z by BVFUNC_1:def 7
    .=(a '&' b).z 'or' (b '&' c).z 'or' (c '&' a).z by BVFUNC_1:def 7
    .=((a).z '&' (b).z) 'or' (b '&' c).z 'or' (c '&' a).z
     by VALUAT_1:def 6
    .=((a).z '&' (b).z) 'or' ((b).z '&' (c).z) 'or' (c '&' a).z
     by VALUAT_1:def 6
    .=((a).z '&' (b).z) 'or' ((b).z '&' (c).z) 'or'
      ((c).z '&' (a).z)
     by VALUAT_1:def 6;
     A14:(((a).z '&' (b).z) 'or' ((b).z '&' (c).z))=TRUE or
     (((a).z '&' (b).z) 'or' ((b).z '&' (c).z))=FALSE by XBOOLEAN:def 3;
     A15: ((c).z '&' (a).z)=TRUE or ((c).z '&' (a).z)=FALSE
       by XBOOLEAN:def 3;
        ((b).z '&' (c).z)=TRUE or ((b).z '&' (c).z)=FALSE
       by XBOOLEAN:def 3;
then A16: ((a).z '&' (b).z)=FALSE & ((b).z '&' (c).z)=FALSE
       by A12,A13,A14;
         now per cases by A12,A13,A15,A16,MARGREL1:45;
      case (a).z=FALSE & (b).z=FALSE;
      then ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
      ((c).z 'or' (a).z)
     =FALSE;
      hence thesis by A10,A11;
      end;
      case (b).z=FALSE & (c).z=FALSE;
      then ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
      ((c).z 'or' (a).z)
     =FALSE;
      hence thesis by A10,A11;
      end;
      case (c).z=FALSE & (a).z=FALSE;
      then ((a).z 'or' (b).z) '&' ((b).z 'or' (c).z) '&'
      ((c).z 'or' (a).z)
     =FALSE;
      hence thesis by A10,A11;
      end;
      end;
      hence thesis;
    end;
    hence thesis;
  end;
  then (a 'or' b) '&' (b 'or' c) '&' (c 'or' a) '<'
  (a '&' b) 'or' (b '&' c) 'or' (c '&' a) by BVFUNC_1:def 15;
  hence thesis by A9,BVFUNC_1:18;
end;

Lm1: for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a '&' 'not' b) 'or' (b '&' 'not' c) 'or' (c '&' 'not' a) '<'
(b '&' 'not' a) 'or' (c '&' 'not' b) 'or' (a '&' 'not' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a '&' 'not' b) 'or' (b '&' 'not' c) 'or' (c '&' 'not' a)).z=TRUE;
    A2: ((a '&' 'not' b) 'or' (b '&' 'not' c) 'or' (c '&' 'not' a)).z
   =((a '&' 'not' b) 'or' (b '&' 'not' c)).z 'or' (c '&' 'not'
a).z by BVFUNC_1:def 7
  .=(a '&' 'not' b).z 'or' (b '&' 'not' c).z 'or' (c '&' 'not'
a).z by BVFUNC_1:def 7
  .=((a).z '&' ('not' b).z) 'or' (b '&' 'not' c).z 'or'
    (c '&' 'not' a).z by VALUAT_1:def 6
  .=((a).z '&' ('not' b).z) 'or' ((b).z '&' ('not' c).z) 'or'
    (c '&' 'not' a).z by VALUAT_1:def 6
  .=((a).z '&' ('not' b).z) 'or' ((b).z '&' ('not' c).z) 'or'
    ((c).z '&' ('not' a).z) by VALUAT_1:def 6
  .=((a).z '&' 'not' (b).z) 'or' ((b).z '&' ('not' c).z) 'or'
    ((c).z '&' ('not' a).z) by VALUAT_1:def 5
  .=((a).z '&' 'not' (b).z) 'or' ((b).z '&' 'not' (c).z) 'or'
    ((c).z '&' ('not' a).z) by VALUAT_1:def 5
  .=((a).z '&' 'not' (b).z) 'or' ((b).z '&' 'not' (c).z) 'or'
    ((c).z '&' 'not' (a).z) by VALUAT_1:def 5;
       now assume
      A3: ((b '&' 'not' a) 'or' (c '&' 'not' b) 'or'
      (a '&' 'not' c)).z<>TRUE;
      A4: ((b '&' 'not' a) 'or' (c '&' 'not' b) 'or' (a '&' 'not' c)).z
     =((b '&' 'not' a) 'or' (c '&' 'not' b)).z 'or' (a '&' 'not'
c).z by BVFUNC_1:def 7
    .=(b '&' 'not' a).z 'or' (c '&' 'not' b).z 'or'
      (a '&' 'not' c).z by BVFUNC_1:def 7
    .=((b).z '&' ('not' a).z) 'or' (c '&' 'not' b).z 'or'
      (a '&' 'not' c).z by VALUAT_1:def 6
    .=((b).z '&' ('not' a).z) 'or' ((c).z '&' ('not' b).z) 'or'
      (a '&' 'not' c).z by VALUAT_1:def 6
    .=((b).z '&' ('not' a).z) 'or' ((c).z '&' ('not' b).z) 'or'
      ((a).z '&' ('not' c).z) by VALUAT_1:def 6;
      A5:(((b).z '&' ('not' a).z) 'or' ((c).z '&' ('not' b).z))=TRUE or
          (((b).z '&' ('not' a).z) 'or' ((c).z '&' ('not' b).z))=FALSE
        by XBOOLEAN:def 3;
      A6: ((a).z '&' ('not' c).z)=TRUE or ((a).z '&' ('not' c).z)=FALSE
        by XBOOLEAN:def 3;
         ((c).z '&' ('not' b).z)=TRUE or ((c).z '&' ('not' b).z)=FALSE
       by XBOOLEAN:def 3;
then A7:  ((b).z '&' ('not' a).z)=FALSE & ((c).z '&' ('not' b).z)=FALSE
       by A3,A4,A5;
then A8:  (b).z=FALSE or ('not' a).z=FALSE by MARGREL1:45;
A9:  (c).z=FALSE or ('not' b).z=FALSE by A7,MARGREL1:45;
         now per cases by A3,A4,A6,MARGREL1:45;
      case A10:(a).z=FALSE;
      then 'not' (a).z=TRUE;
      then  'not' (a).z<>FALSE;
      then 'not' (b).z=TRUE by A8,VALUAT_1:def 5;
      then 'not' (b).z<>FALSE;
      then 'not' (c).z=TRUE by A9,VALUAT_1:def 5;
      then ((a).z '&' 'not' (b).z) 'or' ((b).z '&' 'not' (c).z) 'or'
      ((c).z '&' 'not' (a).z)
     =FALSE by A8,A10,VALUAT_1:def 5;
      hence thesis by A1,A2;
      end;
      case ('not' c).z=FALSE;
then A11:  'not' (c).z=FALSE by VALUAT_1:def 5;
then   (c).z=TRUE;
      then 'not' (b).z=FALSE by A9,VALUAT_1:def 5;
then A12:  (b).z=TRUE;
      then 'not' (a).z=FALSE by A8,VALUAT_1:def 5;
      then (a).z=TRUE;
      then ((a).z '&' 'not' (b).z) 'or' ((b).z '&' 'not' (c).z) 'or'
      ((c).z '&' 'not' (a).z)
     =FALSE by A11,A12;
      hence thesis by A1,A2;
      end;
      end;
      hence thesis;
    end;
    hence thesis;
end;

theorem  for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a '&' 'not' b) 'or' (b '&' 'not' c) 'or' (c '&' 'not' a)=
(b '&' 'not' a) 'or' (c '&' 'not' b) 'or' (a '&' 'not' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
A1:(a '&' 'not' b) 'or' (b '&' 'not' c) 'or' (c '&' 'not' a) '<'
  (b '&' 'not' a) 'or' (c '&' 'not' b) 'or' (a '&' 'not' c) by Lm1;
     (b '&' 'not' a) 'or' (c '&' 'not' b) 'or' (a '&' 'not' c) '<'
  (a '&' 'not' b) 'or' (b '&' 'not' c) 'or' (c '&' 'not' a) by Lm1;
  hence thesis by A1,BVFUNC_1:18;
end;

Lm2: for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'or' 'not' b) '&' (b 'or' 'not' c) '&' (c 'or' 'not' a) '<'
(b 'or' 'not' a) '&' (c 'or' 'not' b) '&' (a 'or' 'not' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a 'or' 'not' b) '&' (b 'or' 'not' c) '&' (c 'or' 'not' a)).z=TRUE;
    A2: ((a 'or' 'not' b) '&' (b 'or' 'not' c) '&' (c 'or' 'not' a)).z
   =((a 'or' 'not' b) '&' (b 'or' 'not' c)).z '&' (c 'or' 'not'
a).z by VALUAT_1:def 6
  .=(a 'or' 'not' b).z '&' (b 'or' 'not' c).z '&' (c 'or' 'not'
a).z by VALUAT_1:def 6
  .=((a).z 'or' ('not' b).z) '&' (b 'or' 'not' c).z '&'
    (c 'or' 'not' a).z by BVFUNC_1:def 7
  .=((a).z 'or' ('not' b).z) '&' ((b).z 'or' ('not' c).z) '&'
    (c 'or' 'not' a).z by BVFUNC_1:def 7
  .=((a).z 'or' ('not' b).z) '&' ((b).z 'or' ('not' c).z) '&'
    ((c).z 'or' ('not' a).z) by BVFUNC_1:def 7
  .=((a).z 'or' 'not' (b).z) '&' ((b).z 'or' ('not' c).z) '&'
    ((c).z 'or' ('not' a).z) by VALUAT_1:def 5
  .=((a).z 'or' 'not' (b).z) '&' ((b).z 'or' 'not' (c).z) '&'
    ((c).z 'or' ('not' a).z) by VALUAT_1:def 5
  .=((a).z 'or' 'not' (b).z) '&' ((b).z 'or' 'not' (c).z) '&'
    ((c).z 'or' 'not' (a).z) by VALUAT_1:def 5;
       now assume
      A3: ((b 'or' 'not' a) '&' (c 'or' 'not' b) '&'
      (a 'or' 'not' c)).z<>TRUE;
         ((b 'or' 'not' a) '&' (c 'or' 'not' b) '&' (a 'or' 'not' c)).z
     =((b 'or' 'not' a) '&' (c 'or' 'not' b)).z '&' (a 'or' 'not'
c).z by VALUAT_1:def 6
    .=(b 'or' 'not' a).z '&' (c 'or' 'not' b).z '&'
      (a 'or' 'not' c).z by VALUAT_1:def 6
    .=((b).z 'or' ('not' a).z) '&' (c 'or' 'not' b).z '&'
      (a 'or' 'not' c).z by BVFUNC_1:def 7
    .=((b).z 'or' ('not' a).z) '&' ((c).z 'or' ('not' b).z) '&'
      (a 'or' 'not' c).z by BVFUNC_1:def 7
    .=((b).z 'or' ('not' a).z) '&' ((c).z 'or' ('not' b).z) '&'
      ((a).z 'or' ('not' c).z) by BVFUNC_1:def 7;
  then A4: ((b).z 'or' ('not' a).z) '&' ((c).z 'or' ('not' b).z) '&'
      ((a).z 'or' ('not' c).z)=FALSE by A3,XBOOLEAN:def 3;
         now per cases by A4,MARGREL1:45;
      case A5: (((b).z 'or' ('not' a).z) '&'
      ((c).z 'or' ('not' b).z))=FALSE;
         now per cases by A5,MARGREL1:45;
        case A6:((b).z 'or' ('not' a).z)=FALSE;
        reconsider cz = c.z as boolean set;
           ('not' a).z=TRUE or ('not' a).z=FALSE by XBOOLEAN:def 3;
then A7:    (b).z=FALSE & ('not' a).z=FALSE by A6;
        then 'not' (a).z=FALSE by VALUAT_1:def 5;
        then (a).z=TRUE;
        then ((a).z 'or' 'not' (b).z) '&' ((b).z 'or' 'not' (c).z) '&'
        ((c).z 'or' 'not' (a).z)
      =FALSE by  A7,XBOOLEAN:138;
        hence thesis by A1,A2;
        end;
        case A8:((c).z 'or' ('not' b).z)=FALSE;
           ('not' b).z=TRUE or ('not' b).z=FALSE by XBOOLEAN:def 3;
then A9:    (c).z=FALSE & ('not' b).z=FALSE by A8;
        then 'not' (b).z=FALSE by VALUAT_1:def 5;
        then ((a).z 'or' 'not' (b).z) '&' ((b).z 'or' 'not' (c).z) '&'
        ((c).z 'or' 'not' (a).z)
       =FALSE by  A9,XBOOLEAN:138;
        hence thesis by A1,A2;
      end;
      end;
      hence thesis;
      end;
      case A10:((a).z 'or' ('not' c).z)=FALSE;
        reconsider bz = b.z as boolean set;
           ('not' c).z=TRUE or ('not' c).z=FALSE by XBOOLEAN:def 3;
then A11:    (a).z=FALSE & ('not' c).z=FALSE by A10;
        then 'not' (c).z=FALSE by VALUAT_1:def 5;
        then ((a).z 'or' 'not' (b).z) '&' ((b).z 'or' 'not' (c).z) '&'
        ((c).z 'or' 'not' (a).z)
       =FALSE by  A11,XBOOLEAN:138;
      hence thesis by A1,A2;
      end;
      end;
      hence thesis;
    end;
    hence thesis;
end;

theorem  for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'or' 'not' b) '&' (b 'or' 'not' c) '&' (c 'or' 'not' a)=
(b 'or' 'not' a) '&' (c 'or' 'not' b) '&' (a 'or' 'not' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
A1:(a 'or' 'not' b) '&' (b 'or' 'not' c) '&' (c 'or' 'not' a) '<'
  (b 'or' 'not' a) '&' (c 'or' 'not' b) '&' (a 'or' 'not' c) by Lm2;
     (b 'or' 'not' a) '&' (c 'or' 'not' b) '&' (a 'or' 'not' c) '<'
  (a 'or' 'not' b) '&' (b 'or' 'not' c) '&' (c 'or' 'not' a) by Lm2;
  hence thesis by A1,BVFUNC_1:18;
end;

theorem
   for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(c 'imp' a)=I_el(Y) & (c 'imp' b)=I_el(Y) implies
c 'imp' (a 'or' b)=I_el(Y)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
  assume A1:(c 'imp' a)=I_el(Y) & (c 'imp' b)=I_el(Y);
     c 'imp' (a 'or' b)
 =(c 'imp' a) 'or' (c 'imp' b) by BVFUNC_7:3
.=I_el(Y) by A1;
  hence thesis;
end;

theorem  for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' c)=I_el(Y) & (b 'imp' c)=I_el(Y) implies
(a '&' b) 'imp' c = I_el(Y)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
     (a 'imp' c)=I_el(Y) & (b 'imp' c)=I_el(Y) implies
  (a '&' b) 'imp' (c '&' c)=I_el(Y) by BVFUNC_6:21;
  hence thesis;
end;

theorem  for a1,a2,b1,b2,c1,c2 being Element of Funcs(Y,BOOLEAN) holds
(a1 'imp' a2) '&' (b1 'imp' b2) '&' (c1 'imp' c2) '&'
(a1 'or' b1 'or' c1)
'<'
(a2 'or' b2 'or' c2)
proof
  let a1,a2,b1,b2,c1,c2 be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a1 'imp' a2) '&' (b1 'imp' b2) '&' (c1 'imp' c2) '&'
       (a1 'or' b1 'or' c1)).z=TRUE;
A2:((a1 'imp' a2) '&' (b1 'imp' b2) '&' (c1 'imp' c2) '&'
       (a1 'or' b1 'or' c1)).z
   =((a1 'imp' a2) '&' (b1 'imp' b2) '&' (c1 'imp' c2)).z '&'
    (a1 'or' b1 'or' c1).z by VALUAT_1:def 6
  .=((a1 'imp' a2) '&' (b1 'imp' b2)).z '&' (c1 'imp' c2).z '&'
    (a1 'or' b1 'or' c1).z by VALUAT_1:def 6
  .=(a1 'imp' a2).z '&' (b1 'imp' b2).z '&' (c1 'imp' c2).z '&'
    (a1 'or' b1 'or' c1).z by VALUAT_1:def 6
  .=('not' a1 'or' a2).z '&' (b1 'imp' b2).z '&' (c1 'imp' c2).z '&'
    (a1 'or' b1 'or' c1).z by BVFUNC_4:8
  .=('not' a1 'or' a2).z '&' ('not' b1 'or' b2).z '&' (c1 'imp' c2).z '&'
    (a1 'or' b1 'or' c1).z by BVFUNC_4:8
  .=('not' a1 'or' a2).z '&' ('not' b1 'or' b2).z '&' ('not'
c1 'or' c2).z '&'
    (a1 'or' b1 'or' c1).z by BVFUNC_4:8
  .=(('not' a1).z 'or' (a2).z) '&' ('not' b1 'or' b2).z '&' ('not'
c1 'or' c2).z '&'
    (a1 'or' b1 'or' c1).z by BVFUNC_1:def 7
  .=(('not' a1).z 'or' (a2).z) '&' (('not' b1).z 'or' (b2).z) '&'
    ('not' c1 'or' c2).z '&' (a1 'or' b1 'or' c1).z by BVFUNC_1:def 7
  .=(('not' a1).z 'or' (a2).z) '&' (('not' b1).z 'or' (b2).z) '&'
    (('not'
c1).z 'or' (c2).z) '&' (a1 'or' b1 'or' c1).z by BVFUNC_1:def 7
  .=(('not' a1).z 'or' (a2).z) '&' (('not' b1).z 'or' (b2).z) '&'
    (('not' c1).z 'or' (c2).z) '&' ((a1 'or' b1).z 'or' (c1).z)
   by BVFUNC_1:def 7
  .=(('not' a1).z 'or' (a2).z) '&' (('not' b1).z 'or' (b2).z) '&'
    (('not' c1).z 'or' (c2).z) '&' ((a1).z 'or' (b1).z 'or' (c1).z)
   by BVFUNC_1:def 7
  .=('not' (a1).z 'or' (a2).z) '&' (('not' b1).z 'or' (b2).z) '&'
    (('not' c1).z 'or' (c2).z) '&' ((a1).z 'or' (b1).z 'or' (c1).z)
   by VALUAT_1:def 5
  .=('not' (a1).z 'or' (a2).z) '&' ('not' (b1).z 'or' (b2).z) '&'
    (('not' c1).z 'or' (c2).z) '&' ((a1).z 'or' (b1).z 'or' (c1).z)
   by VALUAT_1:def 5
  .=('not' (a1).z 'or' (a2).z) '&' ('not' (b1).z 'or' (b2).z) '&'
    ('not' (c1).z 'or' (c2).z) '&' ((a1).z 'or' (b1).z 'or' (c1).z)
   by VALUAT_1:def 5;
       now assume A3: (a2 'or' b2 'or' c2).z<>TRUE;
      A4: (a2 'or' b2 'or' c2).z
     =(a2 'or' b2).z 'or' (c2).z by BVFUNC_1:def 7
    .=(a2).z 'or' (b2).z 'or' (c2).z by BVFUNC_1:def 7;
      A5:((a2).z 'or' (b2).z)=TRUE or
          ((a2).z 'or' (b2).z)=FALSE by XBOOLEAN:def 3;
      A6: (c2).z=TRUE or (c2).z=FALSE by XBOOLEAN:def 3;
         (b2).z=TRUE or (b2).z=FALSE by XBOOLEAN:def 3;
      then (a2).z=FALSE & (b2).z=FALSE by A3,A4,A5;
then A7:  ('not' (a1).z 'or' (a2).z) '&' ('not' (b1).z 'or' (b2).z) '&'
      ('not' (c1).z 'or' (c2).z) '&' ((a1).z 'or' (b1).z 'or' (c1).z)
     =(('not' a1).z '&' 'not' (b1).z '&' 'not' (c1).z) '&'
      ((a1).z 'or' (b1).z 'or' (c1).z)
     by  A3,A4,A6,VALUAT_1:def 5
    .=(('not' a1).z '&' ('not' b1).z '&' 'not' (c1).z) '&'
      ((a1).z 'or' (b1).z 'or' (c1).z)
     by VALUAT_1:def 5
    .=(('not' a1).z '&' ('not' b1).z '&' ('not' c1).z) '&'
      ((a1).z 'or' (b1).z 'or' (c1).z)
     by VALUAT_1:def 5
    .=(('not' a1).z '&' ('not' b1).z '&' ('not' c1).z) '&'
      ((a1 'or' b1).z 'or' (c1).z)
     by BVFUNC_1:def 7
    .=(('not' a1).z '&' ('not' b1).z '&' ('not' c1).z) '&'
      ((a1 'or' b1 'or' c1).z)
     by BVFUNC_1:def 7
    .=(('not' a1 '&' 'not' b1).z '&' ('not' c1).z) '&'
      ((a1 'or' b1 'or' c1).z)
     by VALUAT_1:def 6
    .=(('not' a1 '&' 'not' b1 '&' 'not' c1).z) '&' ((a1 'or' b1 'or' c1).z)
     by VALUAT_1:def 6
    .=(('not' a1 '&' 'not' b1 '&' 'not' c1) '&' (a1 'or' b1 'or' c1)).z
     by VALUAT_1:def 6;
         ('not' a1 '&' 'not' b1 '&' 'not' c1) '&' (a1 'or' b1 'or' c1)
     =('not' a1 '&' 'not' b1 '&' 'not' c1) '&' (a1 'or' b1) 'or'
      ('not' a1 '&' 'not' b1 '&' 'not' c1) '&' c1 by BVFUNC_1:15
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' (a1 'or' b1) 'or'
      ('not' a1 '&' 'not' b1 '&' ('not' c1 '&' c1)) by BVFUNC_1:7
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' (a1 'or' b1) 'or'
      ('not' a1 '&' 'not' b1 '&' O_el(Y)) by BVFUNC_4:5
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' (a1 'or' b1) 'or'
      O_el(Y) by BVFUNC_1:8
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' (a1 'or' b1)
     by BVFUNC_1:12
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' a1 'or'
      ('not' a1 '&' 'not' b1 '&' 'not' c1) '&' b1
     by BVFUNC_1:15
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' a1 'or'
      'not' a1 '&' 'not' c1 '&' 'not' b1 '&' b1
     by BVFUNC_1:7
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' a1 'or'
      'not' a1 '&' 'not' c1 '&' ('not' b1 '&' b1)
     by BVFUNC_1:7
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' a1 'or'
      'not' a1 '&' 'not' c1 '&' O_el(Y)
     by BVFUNC_4:5
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' a1 'or'
      O_el(Y)
     by BVFUNC_1:8
    .=('not' a1 '&' 'not' b1 '&' 'not' c1) '&' a1
     by BVFUNC_1:12
    .='not' b1 '&' 'not' c1 '&' 'not' a1 '&' a1
     by BVFUNC_1:7
    .='not' b1 '&' 'not' c1 '&' ('not' a1 '&' a1)
     by BVFUNC_1:7
    .='not' b1 '&' 'not' c1 '&' O_el(Y)
     by BVFUNC_4:5
    .=O_el(Y) by BVFUNC_1:8;
      then ((a1 'imp' a2) '&' (b1 'imp' b2) '&' (c1 'imp' c2) '&'
       (a1 'or' b1 'or' c1)).z
     =FALSE by A2,A7,BVFUNC_1:def 13;
      hence contradiction by A1;
    end;
    hence thesis;
end;

Lm3: for a1,a2,b1,b2 being Element of Funcs(Y,BOOLEAN) holds
(a1 'imp' b1) '&' (a2 'imp' b2) '&' (a1 'or' a2) '&' 'not'( b1 '&' b2) '<'
(b1 'imp' a1) '&' (b2 'imp' a2) '&' (b1 'or' b2) '&' 'not'( a1 '&' a2)
proof
  let a1,a2,b1,b2 be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a1 'imp' b1) '&' (a2 'imp' b2) '&' (a1 'or' a2) '&'
      'not'( b1 '&' b2)).z=TRUE;
    A2: ((a1 'imp' b1) '&' (a2 'imp' b2) '&' (a1 'or' a2) '&'
    'not'( b1 '&' b2)).z
   =(('not' a1 'or' b1) '&' (a2 'imp' b2) '&' (a1 'or' a2) '&' 'not'
(b1 '&' b2)).z
   by BVFUNC_4:8
  .=(('not' a1 'or' b1) '&' ('not' a2 'or' b2) '&' (a1 'or' a2) '&' 'not'
(b1 '&' b2)).z
   by BVFUNC_4:8
  .=(('not' a1 'or' b1) '&' ('not' a2 'or' b2) '&' (a1 'or' a2)).z '&'
    ('not'( b1 '&' b2)).z
   by VALUAT_1:def 6
  .=(('not' a1 'or' b1) '&' ('not' a2 'or' b2)).z '&'
    (a1 'or' a2).z '&' ('not'( b1 '&' b2)).z
   by VALUAT_1:def 6
  .=(('not' a1 'or' b1) '&' ('not' a2 'or' b2)).z '&'
    (a1 'or' a2).z '&' ('not' b1 'or' 'not' b2).z
   by BVFUNC_1:17
  .=('not' a1 'or' b1).z '&' ('not' a2 'or' b2).z '&'
    (a1 'or' a2).z '&' ('not' b1 'or' 'not' b2).z
   by VALUAT_1:def 6
  .=(('not' a1).z 'or' (b1).z) '&' ('not' a2 'or' b2).z '&'
    (a1 'or' a2).z '&' ('not' b1 'or' 'not' b2).z
   by BVFUNC_1:def 7
  .=(('not' a1).z 'or' (b1).z) '&' (('not' a2).z 'or' (b2).z) '&'
    (a1 'or' a2).z '&' ('not' b1 'or' 'not' b2).z
   by BVFUNC_1:def 7
  .=(('not' a1).z 'or' (b1).z) '&' (('not' a2).z 'or' (b2).z) '&'
    ((a1).z 'or' (a2).z) '&' ('not' b1 'or' 'not' b2).z
   by BVFUNC_1:def 7
  .=(('not' a1).z 'or' (b1).z) '&' (('not' a2).z 'or' (b2).z) '&'
    ((a1).z 'or' (a2).z) '&' (('not' b1).z 'or' ('not' b2).z)
   by BVFUNC_1:def 7
  .=('not' (a1).z 'or' (b1).z) '&' (('not' a2).z 'or' (b2).z) '&'
    ((a1).z 'or' (a2).z) '&' (('not' b1).z 'or' ('not' b2).z)
   by VALUAT_1:def 5
  .=('not' (a1).z 'or' (b1).z) '&' ('not' (a2).z 'or' (b2).z) '&'
    ((a1).z 'or' (a2).z) '&' (('not' b1).z 'or' ('not' b2).z)
   by VALUAT_1:def 5
  .=('not' (a1).z 'or' (b1).z) '&' ('not' (a2).z 'or' (b2).z) '&'
    ((a1).z 'or' (a2).z) '&' ('not' (b1).z 'or' ('not' b2).z)
   by VALUAT_1:def 5
  .=('not' (a1).z 'or' (b1).z) '&' ('not' (a2).z 'or' (b2).z) '&'
    ((a1).z 'or' (a2).z) '&' ('not' (b1).z 'or' 'not' (b2).z)
   by VALUAT_1:def 5;
       now assume A3: ((b1 'imp' a1) '&' (b2 'imp' a2) '&' (b1 'or' b2) '&'
    'not'( a1 '&' a2)).z<>TRUE;
       ((b1 'imp' a1) '&' (b2 'imp' a2) '&' (b1 'or' b2) '&'
    'not'( a1 '&' a2)).z
   =(('not' b1 'or' a1) '&' (b2 'imp' a2) '&' (b1 'or' b2) '&' 'not'
(a1 '&' a2)).z
   by BVFUNC_4:8
  .=(('not' b1 'or' a1) '&' ('not' b2 'or' a2) '&' (b1 'or' b2) '&' 'not'
(a1 '&' a2)).z by BVFUNC_4:8
  .=(('not' b1 'or' a1) '&' ('not' b2 'or' a2) '&' (b1 'or' b2)).z '&'
    ('not'( a1 '&' a2)).z by VALUAT_1:def 6
  .=(('not' b1 'or' a1) '&' ('not' b2 'or' a2)).z '&' (b1 'or' b2).z '&'
    ('not'( a1 '&' a2)).z by VALUAT_1:def 6
  .=('not' b1 'or' a1).z '&' ('not' b2 'or' a2).z '&' (b1 'or' b2).z '&'
    ('not'( a1 '&' a2)).z
   by VALUAT_1:def 6
  .=('not' b1 'or' a1).z '&' ('not' b2 'or' a2).z '&' (b1 'or' b2).z '&'
    (('not' a1 'or' 'not' a2)).z
   by BVFUNC_1:17
  .=(('not' b1).z 'or' (a1).z) '&' ('not'
b2 'or' a2).z '&' (b1 'or' b2).z '&'
    (('not' a1 'or' 'not' a2)).z
   by BVFUNC_1:def 7
  .=(('not' b1).z 'or' (a1).z) '&' (('not' b2).z 'or' (a2).z) '&'
    (b1 'or' b2).z '&' (('not' a1 'or' 'not' a2)).z
   by BVFUNC_1:def 7
  .=(('not' b1).z 'or' (a1).z) '&' (('not' b2).z 'or' (a2).z) '&'
    ((b1).z 'or' (b2).z) '&' (('not' a1 'or' 'not' a2)).z
   by BVFUNC_1:def 7
  .=(('not' b1).z 'or' (a1).z) '&' (('not' b2).z 'or' (a2).z) '&'
    ((b1).z 'or' (b2).z) '&' (('not' a1).z 'or' ('not' a2).z)
   by BVFUNC_1:def 7;
then A4: (('not' b1).z 'or' (a1).z) '&' (('not' b2).z 'or' (a2).z) '&'
    ((b1).z 'or' (b2).z) '&' (('not' a1).z 'or' ('not'
a2).z)=FALSE by A3,XBOOLEAN:def 3;
       now per cases by A4,MARGREL1:45;
    case A5: (('not' b1).z 'or' (a1).z) '&' (('not' b2).z 'or' (a2).z)
'&'
    ((b1).z 'or' (b2).z)=FALSE;
         now per cases by A5,MARGREL1:45;
      case A6: (('not' b1).z 'or' (a1).z) '&' (('not'
b2).z 'or' (a2).z)=FALSE;
           now per cases by A6,MARGREL1:45;
        case A7:(('not' b1).z 'or' (a1).z)=FALSE;
           (a1).z=TRUE or (a1).z=FALSE by XBOOLEAN:def 3;
then A8:    ('not' b1).z=FALSE & (a1).z=FALSE by A7;
        then 'not' (b1).z=FALSE by VALUAT_1:def 5;
        then (b1).z=TRUE;
        then ('not' (a1).z 'or' (b1).z) '&' ('not' (a2).z 'or' (b2).z)
'&'
        ((a1).z 'or' (a2).z) '&' ('not' (b1).z 'or' 'not' (b2).z)
       =((a2).z '&' 'not' (a2).z 'or' (a2).z '&' (b2).z)
        '&' 'not' (b2).z by  A8,XBOOLEAN:8
      .=(FALSE 'or' (a2).z '&' (b2).z)
        '&' 'not' (b2).z by XBOOLEAN:138
      .=((a2).z '&' (b2).z) '&' 'not' (b2).z
      .=(a2).z '&' ((b2).z '&' 'not' (b2).z)
      .=FALSE '&' (a2).z by XBOOLEAN:138
      .=FALSE;
        hence thesis by A1,A2;
        end;
        case A9:(('not' b2).z 'or' (a2).z)=FALSE;
           (a2).z=TRUE or (a2).z=FALSE by XBOOLEAN:def 3;
then A10:    ('not' b2).z=FALSE & (a2).z=FALSE by A9;
        then 'not' (b2).z=FALSE by VALUAT_1:def 5;
        then (b2).z=TRUE;
        then ('not' (a1).z 'or' (b1).z) '&' ('not' (a2).z 'or' (b2).z)
'&'
        ((a1).z 'or' (a2).z) '&' ('not' (b1).z 'or' 'not' (b2).z)
       =((a1).z '&' 'not' (a1).z 'or' (a1).z '&' (b1).z) '&'
        'not' (b1).z by  A10,XBOOLEAN:8
      .=(FALSE 'or' (a1).z '&' (b1).z) '&'
        'not' (b1).z by XBOOLEAN:138
      .=((a1).z '&' (b1).z) '&'
        'not' (b1).z
      .=(a1).z '&' ((b1).z '&' 'not' (b1).z)
      .=FALSE '&' (a1).z by XBOOLEAN:138
      .=FALSE;
        hence thesis by A1,A2;
        end;
        end;
      hence thesis;
      end;
      case A11:((b1).z 'or' (b2).z)=FALSE;
        reconsider a1z = a1.z as boolean set;
        reconsider a2z = a2.z as boolean set;
           (b1).z=TRUE or (b1).z=FALSE by XBOOLEAN:def 3;
        then (b1).z=FALSE & (b2).z=FALSE by A11;
        then ('not' (a1).z 'or' (b1).z) '&' ('not' (a2).z 'or' (b2).z)
'&'
        ((a1).z 'or' (a2).z) '&' ('not' (b1).z 'or' 'not' (b2).z)
       ='not' (a1).z '&'
        ('not' (a2).z '&' ((a1).z 'or' (a2).z))
      .='not' (a1).z '&'
        ('not' (a2).z '&' (a1).z 'or' 'not' a2z '&' a2z)
       by XBOOLEAN:8
      .='not' (a1).z '&'
        ('not' (a2).z '&' (a1).z 'or' FALSE)
       by XBOOLEAN:138
      .='not' (a1).z '&' ((a1).z '&' 'not' (a2).z)
      .='not' a1z '&' a1z '&' 'not' (a2).z
      .=FALSE '&' 'not' (a2).z
       by XBOOLEAN:138
      .=FALSE;
      hence thesis by A1,A2;
      end;
      end;
    hence thesis;
    end;
    case A12:(('not' a1).z 'or' ('not' a2).z)=FALSE;
           ('not' a2).z=TRUE or ('not' a2).z=FALSE by XBOOLEAN:def 3;
then A13:    ('not' a1).z=FALSE & ('not' a2).z=FALSE by A12;
        then A14: 'not' (a1).z=FALSE by VALUAT_1:def 5;
           'not' (a2).z=FALSE by A13,VALUAT_1:def 5;
        then (a2).z=TRUE;
        then ('not' (a1).z 'or' (b1).z) '&' ('not' (a2).z 'or' (b2).z)
'&'
        ((a1).z 'or' (a2).z) '&' ('not' (b1).z 'or' 'not' (b2).z)
       =(b1).z '&' ((b2).z '&'
        ('not' (b1).z 'or' 'not' (b2).z)) by A14
      .=(b1).z '&'
        ((b2).z '&' 'not' (b1).z 'or' (b2).z '&' 'not'
(b2).z) by XBOOLEAN:8
      .=(b1).z '&'
        ((b2).z '&' 'not' (b1).z 'or' FALSE) by XBOOLEAN:138
      .=(b1).z '&' ('not' (b1).z '&' (b2).z)
      .=(b1).z '&' 'not' (b1).z '&' (b2).z
      .=FALSE '&' (b2).z by XBOOLEAN:138
      .=FALSE;
    hence thesis by A1,A2;
    end;
    end;
    hence thesis;
    end;
    hence thesis;
end;

theorem  for a1,a2,b1,b2 being Element of Funcs(Y,BOOLEAN) holds
(a1 'imp' b1) '&' (a2 'imp' b2) '&' (a1 'or' a2) '&' 'not'( b1 '&' b2)=
(b1 'imp' a1) '&' (b2 'imp' a2) '&' (b1 'or' b2) '&' 'not'( a1 '&' a2)
proof
  let a1,a2,b1,b2 be Element of Funcs(Y,BOOLEAN);
A1:(a1 'imp' b1) '&' (a2 'imp' b2) '&' (a1 'or' a2) '&' 'not'( b1 '&' b2) '<'
  (b1 'imp' a1) '&' (b2 'imp' a2) '&' (b1 'or' b2) '&' 'not'( a1 '&' a2)
  by Lm3;
     (b1 'imp' a1) '&' (b2 'imp' a2) '&' (b1 'or' b2) '&' 'not'( a1 '&' a2) '<'
  (a1 'imp' b1) '&' (a2 'imp' b2) '&' (a1 'or' a2) '&' 'not'( b1 '&' b2)
  by Lm3;
  hence thesis by A1,BVFUNC_1:18;
end;

theorem  for a,b,c,d being Element of Funcs(Y,BOOLEAN) holds
(a 'or' b) '&' (c 'or' d) =
(a '&' c) 'or' (a '&' d) 'or' (b '&' c) 'or' (b '&' d)
proof
  let a,b,c,d be Element of Funcs(Y,BOOLEAN);
     (a 'or' b) '&' (c 'or' d)
 =((a 'or' b) '&' c) 'or' ((a 'or' b) '&' d) by BVFUNC_1:15
.=(a '&' c) 'or' (b '&' c) 'or' ((a 'or' b) '&' d) by BVFUNC_1:15
.=(a '&' c) 'or' (b '&' c) 'or' ((a '&' d) 'or' (b '&' d)) by BVFUNC_1:15
.=(a '&' c) 'or' (b '&' c) 'or' (a '&' d) 'or' (b '&' d) by BVFUNC_1:11
.=(a '&' c) 'or' (a '&' d) 'or' (b '&' c) 'or' (b '&' d) by BVFUNC_1:11;
  hence thesis;
end;

theorem  for a1,a2,b1,b2,b3 being Element of Funcs(Y,BOOLEAN) holds
(a1 '&' a2) 'or' (b1 '&' b2 '&' b3)=
(a1 'or' b1) '&' (a1 'or' b2) '&' (a1 'or' b3) '&'
(a2 'or' b1) '&' (a2 'or' b2) '&' (a2 'or' b3)
proof
  let a1,a2,b1,b2,b3 be Element of Funcs(Y,BOOLEAN);
     (a1 'or' b1) '&' (a1 'or' b2) '&' (a1 'or' b3) '&'
  (a2 'or' b1) '&' (a2 'or' b2) '&' (a2 'or' b3)
 =(a1 'or' (b1 '&' b2)) '&' (a1 'or' b3) '&'
  (a2 'or' b1) '&' (a2 'or' b2) '&' (a2 'or' b3) by BVFUNC_1:14
.=(a1 'or' (b1 '&' b2 '&' b3)) '&'
  (a2 'or' b1) '&' (a2 'or' b2) '&' (a2 'or' b3) by BVFUNC_1:14
.=(a1 'or' (b1 '&' b2 '&' b3)) '&'
  ((a2 'or' b1) '&' (a2 'or' b2)) '&' (a2 'or' b3) by BVFUNC_1:7
.=(a1 'or' (b1 '&' b2 '&' b3)) '&'
  (((a2 'or' b1) '&' (a2 'or' b2)) '&' (a2 'or' b3)) by BVFUNC_1:7
.=(a1 'or' (b1 '&' b2 '&' b3)) '&'
  ((a2 'or' (b1 '&' b2)) '&' (a2 'or' b3)) by BVFUNC_1:14
.=(a1 'or' (b1 '&' b2 '&' b3)) '&'
  (a2 'or' (b1 '&' b2 '&' b3)) by BVFUNC_1:14
.=(a1 '&' a2) 'or' (b1 '&' b2 '&' b3) by BVFUNC_1:14;
  hence thesis;
end;

theorem  for a,b,c,d being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d)=
(a 'imp' (b '&' c '&' d)) '&' (b 'imp' (c '&' d)) '&' (c 'imp' d)
proof
  let a,b,c,d be Element of Funcs(Y,BOOLEAN);
A1:(a 'imp' (b '&' c '&' d)) '&' (b 'imp' (c '&' d)) '&' (c 'imp' d)
 =('not' a 'or' (b '&' c '&' d)) '&' (b 'imp' (c '&' d)) '&' (c 'imp' d)
 by BVFUNC_4:8
.=('not' a 'or' (b '&' c '&' d)) '&' ('not' b 'or' (c '&' d)) '&' (c 'imp' d)
 by BVFUNC_4:8
.=('not' a 'or' (b '&' c '&' d)) '&' ('not' b 'or'
(c '&' d)) '&' ('not' c 'or' d) by BVFUNC_4:8
.=(('not' a 'or' b) '&' ('not' a 'or' c) '&' ('not' a 'or' d))
  '&' ('not' b 'or' (c '&' d)) '&' ('not' c 'or' d) by BVFUNC_5:40
.=(('not' a 'or' b) '&' ('not' a 'or' c) '&' ('not' a 'or' d))
  '&' (('not' b 'or' c) '&' ('not' b 'or' d)) '&' ('not' c 'or' d)
  by BVFUNC_1:14
.=('not' a 'or' b) '&' ('not' a 'or' c) '&' ('not' a 'or' d) '&'
  ('not' b 'or' c) '&' ('not' b 'or' d) '&' ('not' c 'or' d) by BVFUNC_1:7
.=('not' a 'or' b) '&' ('not' a 'or' c) '&' ('not' b 'or' c) '&'
  ('not' a 'or' d) '&' ('not' b 'or' d) '&' ('not' c 'or' d) by BVFUNC_1:7
.=('not' a 'or' b) '&' ('not' b 'or' c) '&' ('not' a 'or' c) '&'
  ('not' a 'or' d) '&' ('not' b 'or' d) '&' ('not' c 'or' d) by BVFUNC_1:7
.=('not' a 'or' b) '&' ('not' b 'or' c) '&' ('not' a 'or' c) '&'
  ('not' a 'or' d) '&' ('not' c 'or' d) '&' ('not' b 'or' d) by BVFUNC_1:7
.=('not' a 'or' b) '&' ('not' b 'or' c) '&' ('not' a 'or' c) '&'
  ('not' c 'or' d) '&' ('not' a 'or' d) '&' ('not' b 'or' d) by BVFUNC_1:7
.=('not' a 'or' b) '&' ('not' b 'or' c) '&' ('not' c 'or' d) '&'
  ('not' a 'or' c) '&' ('not' a 'or' d) '&' ('not' b 'or' d) by BVFUNC_1:7
.=(a 'imp' b) '&' ('not' b 'or' c) '&' ('not' c 'or' d) '&'
  ('not' a 'or' c) '&' ('not' a 'or' d) '&' ('not' b 'or' d) by BVFUNC_4:8
.=(a 'imp' b) '&' (b 'imp' c) '&' ('not' c 'or' d) '&'
  ('not' a 'or' c) '&' ('not' a 'or' d) '&' ('not' b 'or' d) by BVFUNC_4:8
.=(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d) '&'
  ('not' a 'or' c) '&' ('not' a 'or' d) '&' ('not' b 'or' d) by BVFUNC_4:8
.=(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d) '&'
  (a 'imp' c) '&' ('not' a 'or' d) '&' ('not' b 'or' d) by BVFUNC_4:8
.=(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d) '&'
  (a 'imp' c) '&' (a 'imp' d) '&' ('not' b 'or' d) by BVFUNC_4:8
.=(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d) '&'
  (a 'imp' c) '&' (a 'imp' d) '&' (b 'imp' d) by BVFUNC_4:8;
     (a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d)
 =(a 'imp' b) '&' (b 'imp' c) '&' (a 'imp' c) '&' (c 'imp' d)
 by BVFUNC_7:12
.=(a 'imp' b) '&' (b 'imp' c) '&' ((a 'imp' c) '&' (c 'imp' d))
 by BVFUNC_1:7
.=(a 'imp' b) '&' (b 'imp' c) '&' ((a 'imp' c) '&'
  (c 'imp' d) '&' (a 'imp' d))
 by BVFUNC_7:12
.=(a 'imp' b) '&' (b 'imp' c) '&' ((a 'imp' c) '&'
  (c 'imp' d)) '&' (a 'imp' d)
 by BVFUNC_1:7
.=(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d) '&'
  (a 'imp' c) '&' (a 'imp' d)
 by BVFUNC_1:7
.=(a 'imp' b) '&' ((b 'imp' c) '&' (c 'imp' d)) '&'
  (a 'imp' c) '&' (a 'imp' d)
 by BVFUNC_1:7
.=(a 'imp' b) '&' (((b 'imp' c) '&' (c 'imp' d)) '&' (b 'imp' d)) '&'
  (a 'imp' c) '&' (a 'imp' d)
 by BVFUNC_7:12
.=(a 'imp' b) '&' ((b 'imp' c) '&' (c 'imp' d)) '&' (b 'imp' d) '&'
  (a 'imp' c) '&' (a 'imp' d)
 by BVFUNC_1:7
.=(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d) '&' (b 'imp' d) '&'
  (a 'imp' c) '&' (a 'imp' d)
 by BVFUNC_1:7
.=(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d) '&' (a 'imp' c) '&'
  (b 'imp' d) '&' (a 'imp' d)
 by BVFUNC_1:7
.=(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' d) '&' (a 'imp' c) '&'
  (a 'imp' d) '&' (b 'imp' d)
 by BVFUNC_1:7;
  hence thesis by A1;
end;

theorem  for a,b,c,d being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' c) '&' (b 'imp' d) '&' (a 'or' b) '<' (c 'or' d)
proof
  let a,b,c,d be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a 'imp' c) '&' (b 'imp' d) '&' (a 'or' b)).z=TRUE;
    A2: ((a 'imp' c) '&' (b 'imp' d) '&' (a 'or' b)).z
   =((a 'imp' c) '&' (b 'imp' d)).z '&' (a 'or' b).z by VALUAT_1:def 6
  .=(a 'imp' c).z '&' (b 'imp' d).z '&' (a 'or' b).z by VALUAT_1:def 6
  .=('not' a 'or' c).z '&' (b 'imp' d).z '&' (a 'or' b).z by BVFUNC_4:8
  .=('not' a 'or' c).z '&' ('not'
b 'or' d).z '&' (a 'or' b).z by BVFUNC_4:8
  .=(('not' a).z 'or' (c).z) '&' ('not' b 'or' d).z '&'
    (a 'or' b).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (c).z) '&' (('not' b).z 'or' (d).z) '&'
    (a 'or' b).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (c).z) '&' (('not' b).z 'or' (d).z) '&'
    ((a).z 'or' (b).z) by BVFUNC_1:def 7
  .=('not' (a).z 'or' (c).z) '&' (('not' b).z 'or' (d).z) '&'
    ((a).z 'or' (b).z) by VALUAT_1:def 5
  .=('not' (a).z 'or' (c).z) '&' ('not' (b).z 'or' (d).z) '&'
    ((a).z 'or' (b).z) by VALUAT_1:def 5;
        reconsider az = a.z as boolean set;
        reconsider bz = b.z as boolean set;
       now assume
         (c 'or' d).z<>TRUE;
      then (c 'or' d).z=FALSE by XBOOLEAN:def 3;
then A3:  (c).z 'or' (d).z=FALSE by BVFUNC_1:def 7;
         (d).z=TRUE or (d).z=FALSE by XBOOLEAN:def 3;
      then (c).z=FALSE & (d).z=FALSE by A3;
      then ('not' (a).z 'or' (c).z) '&' ('not' (b).z 'or' (d).z) '&'
      ((a).z 'or' (b).z)
     ='not' (a).z '&' ('not' (b).z '&'
      ((b).z 'or' (a).z))
    .='not' (a).z '&' (
      ('not' bz '&' bz 'or' 'not' (b).z '&' (a).z))
      by XBOOLEAN:8
    .='not' (a).z '&' (
      (FALSE 'or' 'not' (b).z '&' (a).z)) by XBOOLEAN:138
    .='not' (a).z '&' ((a).z '&' 'not' (b).z)
    .='not' az '&' az '&' 'not' (b).z
    .=FALSE '&' 'not' (b).z by XBOOLEAN:138
    .=FALSE;
      hence thesis by A1,A2;
    end;
    hence thesis;
end;

theorem  for a,b,c being Element of Funcs(Y,BOOLEAN) holds
((a '&' b) 'imp' 'not' c) '&' a '&' c '<' 'not' b
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    (((a '&' b) 'imp' 'not' c) '&' a '&' c).z=TRUE;
    A2: (((a '&' b) 'imp' 'not' c) '&' a '&' c).z
   =(((a '&' b) 'imp' 'not' c) '&' a).z '&' (c).z by VALUAT_1:def 6
  .=((a '&' b) 'imp' 'not' c).z '&' (a).z '&' (c).z by VALUAT_1:def 6
  .=('not'( a '&' b) 'or' 'not' c).z '&' (a).z '&' (c).z by BVFUNC_4:8
  .=(('not' a 'or' 'not' b) 'or' 'not'
c).z '&' (a).z '&' (c).z by BVFUNC_1:17
  .=(('not' a 'or' 'not' b).z 'or' ('not'
c).z) '&' (a).z '&' (c).z by BVFUNC_1:def 7
  .=((('not' a).z 'or' ('not' b).z) 'or' ('not' c).z) '&'
    (a).z '&' (c).z by BVFUNC_1:def 7;
        reconsider cz = c.z as boolean set;
       now assume ('not' b).z<>TRUE;
      then ('not' b).z=FALSE by XBOOLEAN:def 3;
      then ((('not' a).z 'or' ('not' b).z) 'or' ('not' c).z) '&'
      (a).z '&' (c).z
     =('not' (a).z 'or' ('not' c).z) '&'
      (a).z '&' (c).z by VALUAT_1:def 5
    .=(a).z '&' ('not' (a).z 'or' 'not' (c).z) '&'
      (c).z by VALUAT_1:def 5
    .=((a).z '&' 'not' (a).z 'or' (a).z '&' 'not' (c).z) '&'
      (c).z by XBOOLEAN:8
    .=(FALSE 'or' (a).z '&' 'not' (c).z) '&'
      (c).z by XBOOLEAN:138
    .=((a).z '&' 'not' (c).z) '&'
      (c).z
    .=(a).z '&' ('not' cz '&' cz)
    .=FALSE '&' (a).z by XBOOLEAN:138
    .=FALSE;
      hence thesis by A1,A2;
    end;
    hence thesis;
end;

theorem  for a1,a2,a3,b1,b2,b3 being Element of Funcs(Y,BOOLEAN) holds
(a1 '&' a2 '&' a3) 'imp' (b1 'or' b2 'or' b3)=
('not' b1 '&' 'not' b2 '&' a3) 'imp' ('not' a1 'or' 'not' a2 'or' b3)
proof
  let a1,a2,a3,b1,b2,b3 be Element of Funcs(Y,BOOLEAN);
     ('not' b1 '&' 'not' b2 '&' a3) 'imp' ('not' a1 'or' 'not' a2 'or' b3)
 ='not'( 'not' b1 '&' 'not' b2 '&' a3) 'or' ('not' a1 'or' 'not'
a2 'or' b3) by BVFUNC_4:8
.=('not' 'not' b1 'or' 'not' 'not' b2 'or' 'not' a3) 'or' ('not' a1 'or' 'not'
a2 'or' b3) by BVFUNC_5:38
.=b1 'or' b2 'or' 'not' a3 'or' ('not' a1 'or' 'not' a2) 'or' b3 by BVFUNC_1:11
.=b1 'or' b2 'or' (('not' a1 'or' 'not' a2) 'or' 'not' a3) 'or' b3
by BVFUNC_1:11
.=(('not' a1 'or' 'not' a2) 'or' 'not'
a3) 'or' ((b1 'or' b2) 'or' b3) by BVFUNC_1:11
.='not'( a1 '&' a2 '&' a3) 'or' (b1 'or' b2 'or' b3) by BVFUNC_5:38
.=(a1 '&' a2 '&' a3) 'imp' (b1 'or' b2 'or' b3) by BVFUNC_4:8;
  hence thesis;
end;

theorem  for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' a) =
(a '&' b '&' c) 'or' ('not' a '&' 'not' b '&' 'not' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
     for z being Element of Y st
    ((a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' a)).z=TRUE
    holds
    ((a '&' b '&' c) 'or' ('not' a '&' 'not' b '&' 'not' c)).z=TRUE
  proof
    let z be Element of Y;
    assume A1:
    ((a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' a)).z=TRUE;
    A2: ((a 'imp' b) '&' (b 'imp' c) '&' (c 'imp' a)).z
   =((a 'imp' b) '&' (b 'imp' c)).z '&' (c 'imp' a).z by VALUAT_1:def 6
  .=(a 'imp' b).z '&' (b 'imp' c).z '&' (c 'imp' a).z by VALUAT_1:def 6
  .=('not' a 'or' b).z '&' (b 'imp' c).z '&' (c 'imp' a).z by BVFUNC_4:8
  .=('not' a 'or' b).z '&' ('not'
b 'or' c).z '&' (c 'imp' a).z by BVFUNC_4:8
  .=('not' a 'or' b).z '&' ('not' b 'or' c).z '&' ('not'
c 'or' a).z by BVFUNC_4:8
  .=(('not' a).z 'or' (b).z) '&' ('not' b 'or' c).z '&'
    ('not' c 'or' a).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (b).z) '&' (('not' b).z 'or' (c).z) '&'
    ('not' c 'or' a).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (b).z) '&' (('not' b).z 'or' (c).z) '&'
    (('not' c).z 'or' (a).z) by BVFUNC_1:def 7
  .=('not' (a).z 'or' (b).z) '&' (('not' b).z 'or' (c).z) '&'
    (('not' c).z 'or' (a).z) by VALUAT_1:def 5
  .=('not' (a).z 'or' (b).z) '&' ('not' (b).z 'or' (c).z) '&'
    (('not' c).z 'or' (a).z) by VALUAT_1:def 5
  .=('not' (a).z 'or' (b).z) '&' ('not' (b).z 'or' (c).z) '&'
    ('not' (c).z 'or' (a).z) by VALUAT_1:def 5;
       now assume
      A3: ((a '&' b '&' c) 'or' ('not' a '&' 'not' b '&' 'not' c)).z<>TRUE;
      A4: ((a '&' b '&' c) 'or' ('not' a '&' 'not' b '&' 'not' c)).z
     =(a '&' b '&' c).z 'or' ('not' a '&' 'not' b '&' 'not'
c).z by BVFUNC_1:def 7
    .=((a '&' b).z '&' (c).z) 'or'
      ('not' a '&' 'not' b '&' 'not' c).z by VALUAT_1:def 6
    .=((a).z '&' (b).z '&' (c).z) 'or'
      ('not' a '&' 'not' b '&' 'not' c).z by VALUAT_1:def 6
    .=((a).z '&' (b).z '&' (c).z) 'or'
      (('not' a '&' 'not' b).z '&' ('not' c).z) by VALUAT_1:def 6
    .=((a).z '&' (b).z '&' (c).z) 'or'
      (('not' a).z '&' ('not' b).z '&' ('not' c).z) by VALUAT_1:def 6
    .=((a).z '&' (b).z '&' (c).z) 'or'
      ('not' (a).z '&' ('not' b).z '&' ('not' c).z) by VALUAT_1:def 5
    .=((a).z '&' (b).z '&' (c).z) 'or'
      ('not' (a).z '&' 'not' (b).z '&' ('not' c).z) by VALUAT_1:def 5
    .=((a).z '&' (b).z '&' (c).z) 'or'
      ('not' (a).z '&' 'not' (b).z '&' 'not' (c).z)
      by VALUAT_1:def 5;
      A5:((a).z '&' (b).z '&' (c).z)=TRUE or
      ((a).z '&' (b).z '&' (c).z)=FALSE by XBOOLEAN:def 3;
      A6: ('not' (a).z '&' 'not' (b).z '&' 'not' (c).z)=TRUE or
      ('not' (a).z '&' 'not' (b).z '&' 'not'
(c).z)=FALSE by XBOOLEAN:def 3;
        reconsider az = a.z as boolean set;
         now per cases by A3,A4,A5,MARGREL1:45;
      case A7: ((a).z '&' (b).z)=FALSE;
         now per cases by A7,MARGREL1:45;
      case A8:(a).z=FALSE;
      then A9: 'not' (a).z '&' 'not' (b).z '&' 'not' (c).z
     ='not' (b).z '&' 'not' (c).z;
         now per cases by A3,A4,A6,A9,MARGREL1:45;
        case 'not' (b).z=FALSE;
        then (b).z=TRUE;
        then ('not' (a).z 'or' (b).z) '&' ('not' (b).z 'or' (c).z) '&'
        ('not' (c).z 'or' (a).z)
       =FALSE by  A8,XBOOLEAN:138;
        hence thesis by A1,A2;
        end;
        case 'not' (c).z=FALSE;
        then (c).z=TRUE;
        then ('not' (a).z 'or' (b).z) '&' ('not' (b).z 'or' (c).z) '&'
        ('not' (c).z 'or' (a).z)
       =FALSE by A8;
        hence thesis by A1,A2;
      end;
      end;
      hence thesis;
      end;
      case A10:(b).z=FALSE;
      then A11: 'not' (a).z '&' 'not' (b).z '&' 'not' (c).z
     ='not' (a).z '&' 'not' (c).z;
         now per cases by A3,A4,A6,A11,MARGREL1:45;
        case 'not' (a).z=FALSE;
        then ('not' (a).z 'or' (b).z) '&' ('not' (b).z 'or' (c).z) '&'
        ('not' (c).z 'or' (a).z)
       =FALSE by A10;
        hence thesis by A1,A2;
        end;
        case 'not' (c).z=FALSE;
        then (c).z=TRUE;
        then ('not' (a).z 'or' (b).z) '&' ('not' (b).z 'or' (c).z) '&'
        ('not' (c).z 'or' (a).z)
       =FALSE by  A10,XBOOLEAN:138;
        hence thesis by A1,A2;
      end;
      end;
      hence thesis;
      end;
      end;
      hence thesis;
      end;
      case A12:(c).z=FALSE;
      then A13: 'not' (a).z '&' 'not' (b).z '&' 'not' (c).z
     ='not' (a).z '&' 'not' (b).z;
         now per cases by A3,A4,A6,A13,MARGREL1:45;
        case 'not' (a).z=FALSE;
        then ('not' (a).z 'or' (b).z) '&' ('not' (b).z 'or' (c).z) '&'
