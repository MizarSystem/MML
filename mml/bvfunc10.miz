A18:          ('not' (b).z 'or' (c).z)=FALSE;
              (c).z=TRUE or (c).z=FALSE by XBOOLEAN:def 3;
              hence thesis by A13,A12,A18;
            end;
          end;
          hence thesis;
        end;
        case
A19:      ('not' (c).z 'or' (a).z)=FALSE;
          (a).z=TRUE or (a).z=FALSE by XBOOLEAN:def 3;
          hence thesis by A13,A12,A19;
        end;
      end;
      hence thesis;
    end;
    hence thesis;
  end;
  then (a '&' b '&' c) 'or' ('not' a '&' 'not' b '&' 'not' c) '<' (a 'imp' b)
  '&' (b 'imp' c) '&' (c 'imp' a) by BVFUNC_1:def 15;
  hence thesis by A11,BVFUNC_1:18;
end;

theorem
