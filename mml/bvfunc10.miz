(a 'imp' b) '&' (b 'imp' c) '<' (a 'imp' (b 'or' 'not' c))
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a 'imp' b) '&' (b 'imp' c)).z=TRUE;
    A2: ((a 'imp' b) '&' (b 'imp' c)).z
   =(a 'imp' b).z '&' (b 'imp' c).z by VALUAT_1:def 6
  .=('not' a 'or' b).z '&' (b 'imp' c).z by BVFUNC_4:8
  .=('not' a 'or' b).z '&' ('not' b 'or' c).z by BVFUNC_4:8
  .=(('not' a).z 'or' (b).z) '&' ('not' b 'or' c).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (b).z) '&' (('not'
b).z 'or' (c).z) by BVFUNC_1:def 7;
       now assume
      A3: (a 'imp' (b 'or' 'not' c)).z<>TRUE;
      A4: (a 'imp' (b 'or' 'not' c)).z
     =('not' a 'or' (b 'or' 'not' c)).z by BVFUNC_4:8
    .=('not' a).z 'or' (b 'or' 'not' c).z by BVFUNC_1:def 7
    .=('not' a).z 'or' ((b).z 'or' ('not' c).z) by BVFUNC_1:def 7;
      A5:('not' a).z=TRUE or ('not' a).z=FALSE
       by XBOOLEAN:def 3;
      A6: ((b).z 'or' ('not' c).z)=TRUE or ((b).z 'or' ('not' c).z)=
FALSE
        by XBOOLEAN:def 3;
         ('not' c).z=TRUE or ('not' c).z=FALSE by XBOOLEAN:def 3;
      then (b).z=FALSE & ('not' c).z=FALSE by A3,A4,A6;
      hence thesis by A1,A2,A4,A5;
    end;
    hence thesis;
end;

theorem Th21: for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' b) '&' (b 'imp' c) '<' (b 'imp' (c 'or' a))
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a 'imp' b) '&' (b 'imp' c)).z=TRUE;
    A2: ((a 'imp' b) '&' (b 'imp' c)).z
   =(a 'imp' b).z '&' (b 'imp' c).z by VALUAT_1:def 6
  .=('not' a 'or' b).z '&' (b 'imp' c).z by BVFUNC_4:8
  .=('not' a 'or' b).z '&' ('not' b 'or' c).z by BVFUNC_4:8
  .=(('not' a).z 'or' (b).z) '&' ('not' b 'or' c).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (b).z) '&' (('not'
b).z 'or' (c).z) by BVFUNC_1:def 7;
       now assume
      A3: (b 'imp' (c 'or' a)).z<>TRUE;
      A4: (b 'imp' (c 'or' a)).z
     =('not' b 'or' (c 'or' a)).z by BVFUNC_4:8
    .=('not' b).z 'or' (c 'or' a).z by BVFUNC_1:def 7
    .=('not' b).z 'or' ((c).z 'or' (a).z) by BVFUNC_1:def 7;
      A5:('not' b).z=TRUE or ('not' b).z=FALSE
       by XBOOLEAN:def 3;
      A6: ((c).z 'or' (a).z)=TRUE or ((c).z 'or' (a).z)=FALSE
        by XBOOLEAN:def 3;
         (a).z=TRUE or (a).z=FALSE by XBOOLEAN:def 3;
      then (c).z=FALSE & (a).z=FALSE by A3,A4,A6;
      hence thesis by A1,A2,A4,A5;
    end;
    hence thesis;
end;

theorem Th22: for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' b) '&' (b 'imp' c) '<' (b 'imp' (c 'or' 'not' a))
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
    let z be Element of Y;
    assume A1:
    ((a 'imp' b) '&' (b 'imp' c)).z=TRUE;
    A2: ((a 'imp' b) '&' (b 'imp' c)).z
   =(a 'imp' b).z '&' (b 'imp' c).z by VALUAT_1:def 6
  .=('not' a 'or' b).z '&' (b 'imp' c).z by BVFUNC_4:8
  .=('not' a 'or' b).z '&' ('not' b 'or' c).z by BVFUNC_4:8
  .=(('not' a).z 'or' (b).z) '&' ('not' b 'or' c).z by BVFUNC_1:def 7
  .=(('not' a).z 'or' (b).z) '&' (('not'
b).z 'or' (c).z) by BVFUNC_1:def 7;
       now assume
      A3: (b 'imp' (c 'or' 'not' a)).z<>TRUE;
      A4: (b 'imp' (c 'or' 'not' a)).z
     =('not' b 'or' (c 'or' 'not' a)).z by BVFUNC_4:8
    .=('not' b).z 'or' (c 'or' 'not' a).z by BVFUNC_1:def 7
    .=('not' b).z 'or' ((c).z 'or' ('not' a).z) by BVFUNC_1:def 7;
      A5:('not' b).z=TRUE or ('not' b).z=FALSE
       by XBOOLEAN:def 3;
      A6: ((c).z 'or' ('not' a).z)=TRUE or ((c).z 'or' ('not' a).z)=
FALSE
        by XBOOLEAN:def 3;
         ('not' a).z=TRUE or ('not' a).z=FALSE by XBOOLEAN:def 3;
      then (c).z=FALSE & ('not' a).z=FALSE by A3,A4,A6;
      hence thesis by A1,A2,A4,A5;
    end;
    hence thesis;
end;

theorem  for a,b,c being Element of Funcs(Y,BOOLEAN) holds
(a 'imp' b) '&' (b 'imp' c) '<' (a 'imp' b) '&' (b 'imp' (c 'or' a))
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
     (a 'imp' b) '&' (b 'imp' c) '<' (b 'imp' (c 'or' a)) by Th21;
then A1:(a 'imp' b) '&' (b 'imp' c) 'imp' (b 'imp' (c 'or' a))=I_el(Y)
  by BVFUNC_1:19;
     (a 'imp' b) '&' (b 'imp' c) 'imp' (a 'imp' b)=I_el(Y)
  by BVFUNC_6:39;
  then (a 'imp' b) '&' (b 'imp' c) 'imp'
  (a 'imp' b) '&' (b 'imp' (c 'or' a))=I_el(Y)
  by A1,BVFUNC_6:18;
  hence thesis by BVFUNC_1:19;
end;

