                      .= f.x "\/" sup y1 by YELLOW_2:def 3
                      .= sup x1 "\/" sup y1 by YELLOW_2:def 3
                      .= sup (x1 "\/" y1) by Th4
                      .= sup downarrow (x1 "\/" y1) by A2,WAYBEL_0:33
                      .= f.(downarrow (x1 "\/" y1)) by YELLOW_2:def 3
                      .= f.(x "\/" y) by YELLOW_4:30
                      .= f.sup {x,y} by YELLOW_0:41;
