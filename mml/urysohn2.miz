::                    Properties of the Intervals

canceled;

theorem Th2:
  for A being Subset of REAL holds for x being Real st x <> 0 holds
  x" ** (x ** A) = A
proof
  let A be Subset of REAL;
  let x be Real;
  assume
A1: x <> 0;
  thus x" ** (x ** A) c= A
  proof
    let y be set;
    assume
A2: y in x" ** (x ** A);
    then reconsider y as Real;
    consider z being Real such that
A3: z in x ** A & y = x" * z by A2,INTEGRA2:def 2;
    consider t being Real such that
A4: t in A & z = x * t by A3,INTEGRA2:def 2;
    y = (x" * x) * t by A3,A4
      .= 1 * t by A1,XCMPLX_0:def 7
      .= t;
    hence thesis by A4;
  end;
  let y be set;
  assume
A5: y in A;
  then reconsider y as Real;
  set t = y / x";
A6: y = x" * t by A1,XCMPLX_1:88,203;
   t in x ** A by A5,INTEGRA2:def 2;
  hence thesis by A6,INTEGRA2:def 2;
end;

theorem Th3:
  for x being Real st x <> 0 holds for A being Subset of REAL holds
  A = REAL implies x ** A = A
proof
  let x be Real;
  assume
A1: x <> 0;
  let A be Subset of REAL;
  assume
A2: A = REAL;
  for y being set st y in A holds y in x ** A
  proof
    let y be set;
    assume y in A;
    then reconsider y as Real;
    reconsider z = y/x as Real;
    y = x * z by A1,XCMPLX_1:88;
    hence thesis by A2,INTEGRA2:def 2;
  end;
  then A c= x ** A by TARSKI:def 3;
  hence thesis by A2,XBOOLE_0:def 10;
end;

theorem Th4:
  for A being Subset of REAL st A <> {} holds 0 ** A = {0}
proof
  let A be Subset of REAL;
  assume
A1: A <> {};
A2: {0} c= 0 ** A
  proof
    let y be set;
    assume
A3: y in {0};
    consider t being set such that
A4: t in A by A1,XBOOLE_0:def 1;
    reconsider t as Element of A by A4;
    reconsider t as Real by A4;
    y = 0 * t by A3,TARSKI:def 1;
    hence thesis by A4,INTEGRA2:def 2;
  end;
  0 ** A c= {0}
  proof
    let y be set;
    assume
A5: y in 0 ** A;
    then reconsider y as Real;
    consider z being Real such that
A6: z in A & y = 0 * z by A5,INTEGRA2:def 2;
    thus thesis by A6,TARSKI:def 1;
  end;
  hence thesis by A2,XBOOLE_0:def 10;
end;

theorem Th5:
  for x being Real holds x ** {} = {}
proof
  let x be Real;
  x ** {} c= {}
  proof
    let y be set;
    assume
A2: y in x ** {};
    then reconsider y as Real;
    consider z being Real such that
A3: z in {} & y = x * z by A2,INTEGRA2:def 2;
    thus thesis by A3;
  end;
  hence thesis;
end;

theorem Th6:
  for a,b being R_eal st a <= b holds
  (a = -infty & b = -infty) or (a = -infty & b in REAL) or
  (a = -infty & b = +infty) or (a in REAL & b in REAL) or
  (a in REAL & b = +infty) or (a = +infty & b = +infty)
proof
  let a,b be R_eal;
  assume
A1: a <= b;
  (a in REAL or a in {-infty,+infty}) & (b in REAL or b in {-infty,+infty} )
  by XBOOLE_0:def 3,XXREAL_0:def 4;
  then (a in REAL or a = -infty or a = +infty) &
  (b in REAL or b = -infty or b = +infty) by TARSKI:def 2;
  hence thesis by A1,XXREAL_0:9,12;
end;

canceled;

theorem Th8:
  for A being Interval holds 0 ** A is connected
proof
  let A be Interval;
  per cases;
  suppose A = {};
    hence thesis by Th5;
  end;
  suppose A <> {};
    then 0 ** A = {0} by Th4;
    then 0 ** A = [.0,0 .] by XXREAL_1:17;
    hence thesis;
  end;
end;

theorem Th9:
  for A being Interval holds for x being Real st x<>0 holds
  A is open_interval implies x ** A is open_interval
proof
  let A be Interval;
  let x be Real;
  assume
A1: x <> 0;
  assume
A2: A is open_interval;
  then consider a,b being R_eal such that
A3: a <= b & A = ].a,b.[ by MEASURE5:def 5;
  now per cases;
    case
A4:   x < 0;
      now per cases by A3,Th6;
        case a = -infty & b = -infty;
          then
A5:       ].a,b.[ = {};
          then x ** A = {} by A3,Th5;
          hence thesis by A3,A5,MEASURE5:def 5;
        end;
        case
A6:       a = -infty & b in REAL;
          then reconsider s = b as Real;
          set c = +infty;
          x * s is R_eal by XXREAL_0:def 1;
          then consider d being R_eal such that
A7:       d = x * s;
A8:       x ** A = ].d,c.[
          proof
A9:         x ** A c= ].d,c.[
            proof
              let q be set;
              assume
A10:          q in x ** A;
              then reconsider q as Real;
              consider z2 being Real such that
A11:          z2 in A & q = x * z2 by A10,INTEGRA2:def 2;
              reconsider z2 as R_eal by XXREAL_0:def 1;
              reconsider q as R_eal by XXREAL_0:def 1;
              a < z2 & z2 < b by A3,A11,MEASURE5:def 2;
              then consider r,o being Real such that
A12:          r = z2 & o = b & r <= o by A6;
A13:          r < o by A3,A11,A12,MEASURE5:def 2;
              reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
A14:          o1 < r1 by A4,A13,XREAL_1:71;
              q < +infty by XXREAL_0:9;
              hence thesis by A7,A11,A12,A14,MEASURE5:def 2;
            end;
            ].d,c.[ c= x ** A
            proof
              let q be set;
              assume
A15:          q in ].d,c.[;
              then reconsider q as Real;
              reconsider q1 = q as R_eal by XXREAL_0:def 1;
A16:          d < q1 & q1 < c & q1 in REAL by A15,MEASURE5:def 2;
A17:          q = x * (q / x) by A1,XCMPLX_1:88;
              set q2 = q / x;
              q2 in A
              proof
                reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                a < q3 & q3 < b & q3 in REAL
                proof
                  x * q2 = q by A1,XCMPLX_1:88;
                  hence thesis by A4,A6,A7,A16,XREAL_1:67,XXREAL_0:12;
                end;
                hence thesis by A3,MEASURE5:def 2;
              end;
              hence thesis by A17,INTEGRA2:def 2;
            end;
            hence thesis by A9,XBOOLE_0:def 10;
          end;
          d <= c by XXREAL_0:4;
          hence thesis by A8,MEASURE5:def 5;
        end;
        case a = -infty & b = +infty;
          hence thesis by A2,A3,A4,Th3,XXREAL_1:224;
        end;
        case
A18:      a in REAL & b in REAL;
          then reconsider s = a, r = b as Real;
          reconsider d = x * s, g = x * r as R_eal by XXREAL_0:def 1;
A19:      x ** A = ].g,d.[
          proof
A20:        x ** A c= ].g,d.[
            proof
              let q be set;
              assume
A21:          q in x ** A;
              then reconsider q as Real;
              consider z2 being Real such that
A22:          z2 in A & q = x * z2 by A21,INTEGRA2:def 2;
              reconsider z2 as R_eal by XXREAL_0:def 1;
A23:          a < z2 & z2 < b by A3,A22,MEASURE5:def 2;
              then consider 1o,1r being Real such that
A24:          1o= a & 1r = z2 & 1o <= 1r by A18;
              consider 2o,2r being Real such that
A25:          2o= z2 & 2r = b & 2o <= 2r by A18,A23;
A26:          1o < 1r by A3,A22,A24,MEASURE5:def 2;
A27:          2o< 2r by A3,A22,A25,MEASURE5:def 2;
              reconsider 1o1 = x * 1o, 1r1 = x * 1r,
              2o1 = x * 2o, 2r1 = x * 2r as R_eal by XXREAL_0:def 1;
A28:          1r1 < 1o1 by A4,A26,XREAL_1:71;
              2r1 < 2o1 by A4,A27,XREAL_1:71;
              hence thesis by A22,A24,A25,A28,MEASURE5:def 2;
            end;
            ].g,d.[ c= x ** A
            proof
              let q be set;
              assume
A29:          q in ].g,d.[;
              then reconsider q as Real;
              reconsider q1 = q as R_eal by XXREAL_0:def 1;
A30:          g < q1 & q1 < d & q1 in REAL by A29,MEASURE5:def 2;
A31:          q = x * (q / x) by A1,XCMPLX_1:88;
              set q2 = q / x;
              q2 in A
              proof
                reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                a < q3 & q3 < b & q3 in REAL
                proof
A32:              q3 < b
                  proof
                    q/x < (x * r)/x by A4,A30,XREAL_1:77;
                    hence thesis by A4,XCMPLX_1:90;
                  end;
                  a < q3
                  proof
                    x * q2 = q by A1,XCMPLX_1:88;
                    hence thesis by A4,A30,XREAL_1:67;
                  end;
                  hence thesis by A32;
                end;
                hence thesis by A3,MEASURE5:def 2;
              end;
              hence thesis by A31,INTEGRA2:def 2;
            end;
            hence thesis by A20,XBOOLE_0:def 10;
          end;
          x * r <= x * s by A3,A4,XREAL_1:67;
          hence thesis by A19,MEASURE5:def 5;
        end;
        case
A33:      a in REAL & b = +infty;
          then reconsider s = a as Real;
          set c = -infty;
          reconsider d = x * s as R_eal by XXREAL_0:def 1;
A34:      x ** A = ].c,d.[
          proof
A35:        x ** A c= ].c,d.[
            proof
              let q be set;
              assume
A36:          q in x ** A;
              then reconsider q as Real;
              consider z2 being Real such that
A37:          z2 in A & q = x * z2 by A36,INTEGRA2:def 2;
              reconsider z2,q as R_eal by XXREAL_0:def 1;
              a < z2 & z2 < b by A3,A37,MEASURE5:def 2;
              then consider o,r being Real such that
A38:          o = a & r = z2 & o <= r by A33;
A39:          o < r by A3,A37,A38,MEASURE5:def 2;
              reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
A40:          r1 < o1 by A4,A39,XREAL_1:71;
              -infty < q by XXREAL_0:12;
              hence thesis by A37,A38,A40,MEASURE5:def 2;
            end;
            ].c,d.[ c= x ** A
            proof
              let q be set;
              assume
A41:          q in ].c,d.[;
              then reconsider q as Real;
              reconsider q1 = q as R_eal by XXREAL_0:def 1;
A42:          q = x * (q / x) by A1,XCMPLX_1:88;
              set q2 = q / x;
              q2 in A
              proof
                reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                a < q3 & q3 < b & q3 in REAL
                proof
                  a < q3
                  proof
                    q1 <= d & q1 <> d by A41,MEASURE5:def 2;
                    then x * q2 < x * s by A42,XXREAL_0:1;
                    hence thesis by A4,XREAL_1:67;
                  end;
                  hence thesis by A33,XXREAL_0:9;
                end;
                hence thesis by A3,MEASURE5:def 2;
              end;
              hence thesis by A42,INTEGRA2:def 2;
            end;
            hence thesis by A35,XBOOLE_0:def 10;
          end;
          c <= d by XXREAL_0:5;
          hence thesis by A34,MEASURE5:def 5;
        end;
        case a = +infty & b = +infty;
          then
A43:      ].a,b.[ = {};
          then x ** A = {} by A3,Th5;
          hence thesis by A3,A43,MEASURE5:def 5;
        end;
      end;
      hence thesis;
    end;
    case x = 0;
      hence thesis by A1;
    end;
    case
A44:  0 < x;
      now per cases by A3,Th6;
        case a = -infty & b = -infty;
          then
A45:      ].a,b.[ = {};
          then x ** A = {} by A3,Th5;
          hence thesis by A3,A45,MEASURE5:def 5;
        end;
        case
A46:      a = -infty & b in REAL;
          then reconsider s = b as Real;
          set c = -infty;
          reconsider d = x * s as R_eal by XXREAL_0:def 1;
A47:      x ** A = ].c,d.[
          proof
A48:        x ** A c= ].c,d.[
            proof
              let q be set;
              assume
A49:          q in x ** A;
              then reconsider q as Real;
              consider z2 being Real such that
A50:          z2 in A & q = x * z2 by A49,INTEGRA2:def 2;
              reconsider z2,q as R_eal by XXREAL_0:def 1;
              a < z2 & z2 < b by A3,A50,MEASURE5:def 2;
              then consider r,o being Real such that
A51:          r = z2 & o = b & r <= o by A46;
A52:          r < o by A3,A50,A51,MEASURE5:def 2;
              reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
A53:          r1 < o1 by A44,A52,XREAL_1:70;
              -infty < q by XXREAL_0:12;
              hence thesis by A50,A51,A53,MEASURE5:def 2;
            end;
            ].c,d.[ c= x ** A
            proof
              let q be set;
              assume
A54:          q in ].c,d.[;
              then reconsider q as Real;
              reconsider q1 = q as R_eal by XXREAL_0:def 1;
A55:          c < q1 & q1 < d & q1 in REAL by A54,MEASURE5:def 2;
A56:          q = x * (q / x) by A1,XCMPLX_1:88;
              set q2 = q / x;
              q2 in A
              proof
                reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                a < q3 & q3 < b & q3 in REAL
                proof
                  q3 < b
                  proof
                    x * q2 = q by A1,XCMPLX_1:88;
                    hence thesis by A44,A55,XREAL_1:66;
                  end;
                  hence thesis by A46,XXREAL_0:12;
                end;
                hence thesis by A3,MEASURE5:def 2;
              end;
              hence thesis by A56,INTEGRA2:def 2;
            end;
            hence thesis by A48,XBOOLE_0:def 10;
          end;
          c <= d by XXREAL_0:5;
          hence thesis by A47,MEASURE5:def 5;
        end;
        case a = -infty & b = +infty;
          hence thesis by A2,A3,A44,Th3,XXREAL_1:224;
        end;
        case
A57:      a in REAL & b in REAL;
          then reconsider s = a, r = b as Real;
          reconsider d = x * s as R_eal by XXREAL_0:def 1;
          reconsider g = x * r as R_eal by XXREAL_0:def 1;
A58:      x ** A = ].d,g.[
          proof
A59:        x ** A c= ].d,g.[
            proof
              let q be set;
              assume
A60:          q in x ** A;
              then reconsider q as Real;
              consider z2 being Real such that
A61:          z2 in A & q = x * z2 by A60,INTEGRA2:def 2;
              reconsider z2 as R_eal by XXREAL_0:def 1;
A62:          a < z2 & z2 < b by A3,A61,MEASURE5:def 2;
              then consider 1o,1r being Real such that
A63:          1o= a & 1r = z2 & 1o <= 1r by A57;
              consider 2o,2r being Real such that
A64:          2o= z2 & 2r = b & 2o <= 2r by A57,A62;
A65:          1o< 1r by A3,A61,A63,MEASURE5:def 2;
A66:          2o< 2r by A3,A61,A64,MEASURE5:def 2;
              reconsider 1o1 = x * 1o, 1r1 = x * 1r as R_eal by XXREAL_0:def 1;
              reconsider 2o1 = x * 2o, 2r1 = x * 2r as R_eal by XXREAL_0:def 1;
A67:          1o1 < 1r1 by A44,A65,XREAL_1:70;
              2o1 < 2r1 by A44,A66,XREAL_1:70;
              hence thesis by A61,A63,A64,A67,MEASURE5:def 2;
            end;
            ].d,g.[ c= x ** A
            proof
              let q be set;
              assume
A68:          q in ].d,g.[;
              then reconsider q as Real;
              q is R_eal by XXREAL_0:def 1;
              then consider q1 being R_eal such that
A69:          q1 = q;
A70:          d < q1 & q1 < g & q1 in REAL by A68,A69,MEASURE5:def 2;
A71:          q = x * (q / x) by A1,XCMPLX_1:88;
              set q2 = q / x;
              q2 in A
              proof
                reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                a < q3 & q3 < b & q3 in REAL
                proof
A72:              q3 < b
                  proof
                    q/x < (x * r)/x by A44,A69,A70,XREAL_1:76;
                    hence thesis by A44,XCMPLX_1:90;
                  end;
                  a < q3
                  proof
                    x * q2 = q by A1,XCMPLX_1:88;
                    hence thesis by A44,A69,A70,XREAL_1:66;
                  end;
                  hence thesis by A72;
                end;
                hence thesis by A3,MEASURE5:def 2;
              end;
              hence thesis by A71,INTEGRA2:def 2;
            end;
            hence thesis by A59,XBOOLE_0:def 10;
          end;
          x * s <= x * r by A3,A44,XREAL_1:66;
          hence thesis by A58,MEASURE5:def 5;
        end;
        case
A73:      a in REAL & b = +infty;
          then reconsider s = a as Real;
          set c = +infty;
          reconsider d = x * s as R_eal by XXREAL_0:def 1;
A74:      x ** A = ].d,c.[
          proof
A75:        x ** A c= ].d,c.[
            proof
              let q be set;
              assume
A76:          q in x ** A;
              then reconsider q as Real;
              consider z2 being Real such that
A77:          z2 in A & q = x * z2 by A76,INTEGRA2:def 2;
              reconsider z2 as R_eal by XXREAL_0:def 1;
              reconsider q as R_eal by XXREAL_0:def 1;
              a < z2 & z2 < b by A3,A77,MEASURE5:def 2;
              then consider o,r being Real such that
A78:          o = a & r = z2 & o <= r by A73;
A79:          o < r by A3,A77,A78,MEASURE5:def 2;
              reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
A80:          o1 < r1 by A44,A79,XREAL_1:70;
              q < +infty by XXREAL_0:9;
              hence thesis by A77,A78,A80,MEASURE5:def 2;
            end;
            ].d,c.[ c= x ** A
            proof
              let q be set;
              assume
A81:          q in ].d,c.[;
              then reconsider q as Real;
              reconsider q1 = q as R_eal by XXREAL_0:def 1;
A82:          d < q1 & q1 < c & q1 in REAL by A81,MEASURE5:def 2;
A83:          q = x * (q / x) by A1,XCMPLX_1:88;
              set q2 = q / x;
              q2 in A
              proof
                reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                a < q3 & q3 < b & q3 in REAL
                by A44,A73,A82,A83,XREAL_1:66,XXREAL_0:9;
                hence thesis by A3,MEASURE5:def 2;
              end;
              hence thesis by A83,INTEGRA2:def 2;
            end;
            hence thesis by A75,XBOOLE_0:def 10;
          end;
          d <= c by XXREAL_0:4;
          hence thesis by A74,MEASURE5:def 5;
        end;
        case a = +infty & b = +infty;
          then
A84:      ].a,b.[ = {};
          then x ** A = {} by A3,Th5;
          hence thesis by A3,A84,MEASURE5:def 5;
        end;
      end;
      hence thesis;
    end;
  end;
  hence thesis;
end;

theorem Th10:
  for A being Interval holds for x being Real st x<>0 holds
  A is closed_interval implies x ** A is closed_interval
proof
  let A be Interval;
  let x be Real;
  assume
A1: x <> 0;
  assume
    A is closed_interval;
  then consider a,b being real number such that
A3: a <= b & A = [.a,b.] by MEASURE5:def 6;
  reconsider a,b as R_eal by XXREAL_0:def 1;
  now per cases;
    case
A4:   x < 0;
      now
A17:      a in REAL & b in REAL by Th6;
          reconsider s = a, r = b as Real by Th6;
          reconsider d = x * s as R_eal by XXREAL_0:def 1;
          reconsider g = x * r as R_eal by XXREAL_0:def 1;
