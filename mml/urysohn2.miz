Lx1:
 for A being non empty Subset of REAL, x being Real
  st x > 0 & x**A is bounded_above
  holds A is bounded_above
proof
 let A be non empty Subset of REAL, x be Real such that
Z1: x > 0 and
Z2: x**A is bounded_above;
  consider r being real number such that
W: r in A by MEMBERED:9;
R:  sup(x**A) <> +infty by Z2,XXREAL_2:77;
 per cases by R,XXREAL_0:14;
 suppose
S:  sup(x**A) = -infty;
 take 0;
T: -infty is UpperBound of x**A by S,XXREAL_2:def 3;
  x*r in x**A by W,INTEGRA2:def 2;
  hence thesis by T,XXREAL_0:6,XXREAL_2:def 1;
 end;
 suppose sup(x**A) in REAL;
  then reconsider r = sup(x**A) as Real;
 take r/x;
 let z be ext-realnumber;
 assume
Z: z in A;
   then reconsider z1=z as Real;
   x" ** (x ** A) = A by Z1,Th2;
   then consider s being Real such that
W1: s in x**A and
W2: z = x" * s by Z,INTEGRA2:def 2;
  s <= r by W1,XXREAL_2:4;
  then s/x <= r/x by Z1,XREAL_1:74;
 hence thesis by W2;
 end;
end;

theorem Tx1:
 for A being non empty Subset of REAL, x being Real, y being R_eal
   st x = y & 0 <= y
  holds sup(x ** A) = y * sup A
proof let A be non empty Subset of REAL, x being Real, y being R_eal such that
Z1: x = y and
Z2: 0 <= y;
   reconsider Y = x ** A as non empty Subset of REAL by INTEGRA2:7;
 per cases;
 suppose
SS:  A is not bounded_above;
  per cases by Z2;
  suppose
S:  y = 0;
   then x ** A = {0} by Z1,INTEGRA2:42;
  hence sup(x ** A) = 0 by XXREAL_2:11
  .= y * sup A by S;
  end;
  suppose
S:  y > 0;
   Y is not bounded_above by Lx1,S,SS,Z1;
  hence sup(x ** A) = +infty by XXREAL_2:77
        .= y * +infty by S,XXREAL_3:def 5
        .= y * sup A by SS,XXREAL_2:77;
  end;
 end;
 suppose
    A is bounded_above;
  then reconsider X = A as non empty bounded_above (real-membered set);
   reconsider u = upper_bound X as Real by XREAL_0:def 1;
   x ** X is bounded_above by Z1,Z2,INTEGRA2:9;
   then reconsider Y as non empty bounded_above (real-membered set);
  thus sup(x ** A) = upper_bound Y
    .= x * u by Z1,Z2,INTEGRA2:13
    .= y * sup A by Z1,EXTREAL1:13;
  end;
end;

Lx2:
 for A being non empty Subset of REAL, x being Real
  st x > 0 & x**A is bounded_below
  holds A is bounded_below
proof
 let A be non empty Subset of REAL, x be Real such that
Z1: x > 0 and
Z2: x**A is bounded_below;
  consider r being real number such that
W: r in A by MEMBERED:9;
R:  inf(x**A) <> -infty by Z2,XXREAL_2:78;
 per cases by R,XXREAL_0:14;
 suppose
S:  inf(x**A) = +infty;
 take 0;
T: +infty is LowerBound of x**A by S,XXREAL_2:def 4;
  x*r in x**A by W,INTEGRA2:def 2;
  hence thesis by T,XXREAL_0:4,XXREAL_2:def 2;
 end;
 suppose inf(x**A) in REAL;
  then reconsider r = inf(x**A) as Real;
 take r/x;
 let z be ext-realnumber;
 assume
Z: z in A;
   then reconsider z1=z as Real;
   x" ** (x ** A) = A by Z1,Th2;
   then consider s being Real such that
W1: s in x**A and
W2: z = x" * s by Z,INTEGRA2:def 2;
  r/x <= s/x by Z1,W1,XREAL_1:74,XXREAL_2:3;
 hence thesis by W2;
