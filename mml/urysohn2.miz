                  hence thesis by A2;
               end;
               case
                 ^^A = +infty;
                  hence thesis by A17,A18,MEASURE6:54;
               end;
               end;
               hence thesis;
            end;
            case
              not A^^ is Real;
            then A20:A^^ in {-infty,+infty} by XXREAL_0:def 4,XBOOLE_0:def 2;
                  now per cases by A20,TARSKI:def 2;
               case
                A^^ = -infty;
                  hence thesis by A17,A18,MEASURE6:54;
               end;
               case
                A^^ = +infty;
              then vol(A) = +infty by A6,A8,A9,XXREAL_0:1,SUPINF_2:6;
                  hence thesis by A2;
               end;
               end;
               hence thesis;
            end;
            end;
            hence thesis;
         end;
         then consider s,t being Real such that
      A21:t = A^^ & s = ^^A;
      A22:^^B = x * s & B^^ = x * t
         proof
               now per cases by A3,MEASURE6:52;
            case A = ].^^A,A^^.[;
               hence thesis by A3,A5,A10,A21,Th17;
            end;
            case A = ].^^A,A^^.];
               hence thesis by A3,A5,A10,A21,Th16;
            end;
            case A = [.^^A,A^^.];
               hence thesis by A3,A5,A10,A21,Th15;
            end;
            case A = [.^^A,A^^.[;
               hence thesis by A3,A5,A10,A21,Th18;
            end;
            end;
            hence thesis;
         end;
            now per cases by A8,XXREAL_0:1;
         case
           A^^ = ^^A;
            hence thesis by A2,A9,A10,A11,A21,A22,Th1,SUPINF_2:def 1;
         end;
         case
         A23:^^A < A^^;
         then A24:vol(A) = t - s by A9,A21,SUPINF_2:5;
            consider s1,t1 being Real such that
         A25:s1 = ^^A & t1 = A^^ & s1 <= t1 by A15,A23;
               s1 < t1 by A23,A25;
         then x * s1 <= x * t1 & x * s1 <> x * t1 by A5,XREAL_1:70;
            then ^^B <= B^^ & ^^B <> B^^ by A21,A22,A25;
            then vol(B) = x * t - x * s by A14,A22,XXREAL_0:1,SUPINF_2:5
                   .= x * y by A2,A24;
            hence thesis by A10;
         end;
         end;
         hence thesis;
      end;
      end;
      hence thesis;
end;
end;

