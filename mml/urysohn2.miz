begin

::                    Properties of the Intervals

theorem Th1:
   for A being Interval st A <> {} holds
   (^^A < A^^ implies vol(A) = A^^ - ^^A) &
   (A^^ = ^^A implies vol(A) = 0.)
proof
   let A be Interval;
   assume
A1:A <> {};
   set aa = ^^A, bb = A^^;
A2:A^^ = ^^A implies vol(A) = 0.
   proof
      assume A3:A^^ = ^^A;
          now per cases by MEASURE5:def 9;
      case
      A4:A is open_interval;
         then A = ].aa,bb.[ & aa <= bb by A1,MEASURE6:48;
         hence thesis by A3,A4,MEASURE5:53;
      end;
      case
      A5:A is closed_interval;
         then A = [.aa,bb.] & aa <= bb by A1,MEASURE6:49;
         hence thesis by A3,A5,MEASURE5:54;
      end;
      case
      A6:A is right_open_interval;
         then A = [.aa,bb.[ & aa <= bb by A1,MEASURE6:50;
         hence thesis by A3,A6,MEASURE5:55;
      end;
      case
      A7:A is left_open_interval;
         then A = ].^^A,A^^.] by A1,MEASURE6:51;
         hence thesis by A3,A7,MEASURE5:56;
      end;
      end;
      hence thesis;
   end;
       ^^A < A^^ implies vol(A) = A^^ - ^^A
   proof
      assume
   A8:^^A < A^^;
          now per cases by MEASURE5:def 9;
      case
      A9:A is open_interval;
         then A = ].^^A,A^^.[ by A1,MEASURE6:48;
         hence thesis by A8,A9,MEASURE5:53;
      end;
      case
      A10:A is closed_interval;
         then A = [.aa,bb.] & aa <= bb by A1,MEASURE6:49;
         hence thesis by A8,A10,MEASURE5:54;
      end;
      case
      A11:A is right_open_interval;
         then A = [.^^A,A^^.[ by A1,MEASURE6:50;
         hence thesis by A8,A11,MEASURE5:55;
      end;
      case
      A12:A is left_open_interval;
         then A = ].^^A,A^^.] by A1,MEASURE6:51;
         hence thesis by A8,A12,MEASURE5:56;
      end;
      end;
      hence thesis;
   end;
   hence thesis by A2;
end;

theorem
  for A being Subset of REAL holds
   for x being Real st x <> 0 holds
   x" ** (x ** A) = A
proof
   let A be Subset of REAL;
   let x be Real;
   assume
A1:x <> 0;
   thus x" ** (x ** A) c= A
   proof
      let y be set;
      assume
   A2:y in x" ** (x ** A);
      then reconsider y as Real;
      consider z being Real such that
   A3:z in x ** A & y = x" * z by A2,INTEGRA2:def 2;
      consider t being Real such that
   A4:t in A & z = x * t by A3,INTEGRA2:def 2;
          y = (x" * x) * t by A3,A4
       .= 1 * t by A1,XCMPLX_0:def 7
       .= t;
      hence thesis by A4;
   end;
   let y be set;
   assume
A5:y in A;
   then reconsider y as Real;
   set t = y / x";
       x" <> 0 by A1,XCMPLX_1:203;
then A6:y = x" * t by XCMPLX_1:88;
    t = 1 * t
    .= x * x" * t by A1,XCMPLX_0:def 7
    .= x * y by A6;
   then t in x ** A by A5,INTEGRA2:def 2;
   hence thesis by A6,INTEGRA2:def 2;
end;

theorem Th3:
   for x being Real st x <> 0 holds
   for A being Subset of REAL holds
   A = REAL implies x ** A = A
proof
   let x be Real;
   assume
A1:x <> 0;
   let A be Subset of REAL;
   assume
A2:A = REAL;
   for y being set st y in A holds y in x ** A
   proof
      let y be set;
      assume y in A;
      then reconsider y as Real;
      reconsider z = y/x as Real;
      y = x * z by A1,XCMPLX_1:88;
      hence thesis by A2,INTEGRA2:def 2;
   end;
   then A c= x ** A by TARSKI:def 3;
   hence thesis by A2,XBOOLE_0:def 10;
end;

theorem Th4:
   for A being Subset of REAL st A <> {} holds 0 ** A = {0}
proof
   let A be Subset of REAL;
   assume
A1:A <> {};
A2:{0} c= 0 ** A
   proof
      let y be set;
      assume A3:y in {0};
      consider t being set such that
   A4:t in A by A1,XBOOLE_0:def 1;
      reconsider t as Element of A by A4;
      reconsider t as Real by A4;
      y = 0 * t by A3,TARSKI:def 1;
      hence thesis by A4,INTEGRA2:def 2;
   end;
   0 ** A c= {0}
   proof
      let y be set;
      assume
   A5:y in 0 ** A;
      then reconsider y as Real;
      consider z being Real such that
   A6:z in A & y = 0 * z by A5,INTEGRA2:def 2;
      thus thesis by A6,TARSKI:def 1;
   end;
   hence thesis by A2,XBOOLE_0:def 10;
end;

theorem Th5:
   for x being Real holds
   x ** {} = {}
proof
   let x be Real;
A1:{} c= x ** {} by XBOOLE_1:2;
   x ** {} c= {}
   proof
      let y be set;
      assume
   A2:y in x ** {};
      then reconsider y as Real;
      consider z being Real such that
   A3:z in {} & y = x * z by A2,INTEGRA2:def 2;
      thus thesis by A3;
   end;
   hence thesis by A1,XBOOLE_0:def 10;
end;

theorem Th6:
   for a,b being R_eal st a <= b holds
   (a = -infty & b = -infty) or (a = -infty & b in REAL) or
   (a = -infty & b = +infty) or (a in REAL & b in REAL) or
   (a in REAL & b = +infty) or (a = +infty & b = +infty)
proof
   let a,b be R_eal;
   assume
A1:a <= b;
      (a in REAL or a in {-infty,+infty}) & (b in REAL or b in {-infty,+infty}
)
      by XBOOLE_0:def 2,XXREAL_0:def 4;
   then (a in REAL or a = -infty or a = +infty) &
   (b in REAL or b = -infty or b = +infty) by TARSKI:def 2;
   hence thesis by A1,XXREAL_0:9,12;
end;

theorem Th7:
   for x being R_eal holds
   [.x,x.] is Interval
proof
   let x be R_eal;
   [.x,x.] is closed_interval by MEASURE5:def 6;
   hence thesis by MEASURE5:def 9;
end;

theorem Th8:
   for A being Interval holds
   0 ** A is Interval
proof
   let A be Interval;
   consider s being R_eal such that
A1:s = 0.;
   per cases;
   suppose A = {};
      hence thesis by Th5;
   end;
   suppose
   A2:A <> {};
      0 ** A = [.s,s.]
      proof
         0 ** A = {0} by A2,Th4;
         hence thesis by A1,MEASURE5:14;
      end;
      hence thesis by Th7;
end;
end;

theorem Th9:
   for A being Interval holds
   for x being Real st x<>0 holds
   A is open_interval implies x ** A is open_interval
proof
   let A be Interval;
   let x be Real;
   assume
A1:x <> 0;
   assume
A2:A is open_interval;
   then consider a,b being R_eal such that
A3:a <= b & A = ].a,b.[ by MEASURE5:def 5;
   now per cases;
   case
   A4:x < 0;
   now per cases by A3,Th6;
      case a = -infty & b = -infty;
      then A5: ].a,b.[ = {} by MEASURE5:15;
         then x ** A = {} by A3,Th5;
         hence thesis by A3,A5,MEASURE5:def 5;
      end;
      case
      A6:a = -infty & b in REAL;
         then reconsider s = b as Real;
         set c = +infty;
         x * s is R_eal by XXREAL_0:def 1;
         then consider d being R_eal such that
      A7: d = x * s;
      A8:x ** A = ].d,c.[
         proof
         A9:x ** A c= ].d,c.[
             proof
                let q be set;
                assume
             A10:q in x ** A;
                then reconsider q as Real;
                consider z2 being Real such that
             A11:z2 in A & q = x * z2 by A10,INTEGRA2:def 2;
                reconsider z2 as R_eal by XXREAL_0:def 1;
                reconsider q as R_eal by XXREAL_0:def 1;
                 a < z2 & z2 < b by A3,A11,MEASURE5:def 2;
                then consider r,o being Real such that
             A12:r = z2 & o = b & r <= o by A6;
                   A13: r < o by A3,A11,A12,MEASURE5:def 2;
                 reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
             A14:o1 < r1 by A4,A13,XREAL_1:71;
                    q < +infty by XXREAL_0:9;
                hence thesis by A7,A11,A12,A14,MEASURE5:def 2;
             end;
                 ].d,c.[ c= x ** A
             proof
                let q be set;
                assume
             A15:q in ].d,c.[;
                then reconsider q as Real;
                reconsider q1 = q as R_eal by XXREAL_0:def 1;
             A16:d < q1 & q1 < c & q1 in REAL by A15,MEASURE5:def 2;
             A17:q = x * (q / x) by A1,XCMPLX_1:88;
                set q2 = q / x;
                    q2 in A
                proof
                   reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                   a < q3 & q3 < b & q3 in REAL
                   proof
                      x * q2 = q by A1,XCMPLX_1:88;
                      hence thesis by A4,A6,A7,A16,XREAL_1:67,XXREAL_0:12;
                   end;
                   hence thesis by A3,MEASURE5:def 2;
                end;
                hence thesis by A17,INTEGRA2:def 2;
             end;
             hence thesis by A9,XBOOLE_0:def 10;
         end;
         d <= c by XXREAL_0:4;
         hence thesis by A8,MEASURE5:def 5;
      end;
      case a = -infty & b = +infty;
         hence thesis by A2,A3,A4,Th3,MEASURE6:35;
      end;
      case
      A18:a in REAL & b in REAL;
         then reconsider s = a, r = b as Real;
         reconsider d = x * s, g = x * r as R_eal by XXREAL_0:def 1;
      A19:x ** A = ].g,d.[
         proof
         A20:x ** A c= ].g,d.[
             proof
                let q be set;
                assume
             A21:q in x ** A;
                then reconsider q as Real;
                consider z2 being Real such that
             A22:z2 in A & q = x * z2 by A21,INTEGRA2:def 2;
                reconsider z2 as R_eal by XXREAL_0:def 1;
                A23: a < z2 & z2 < b by A3,A22,MEASURE5:def 2;
                then consider 1o,1r being Real such that
             A24:1o= a & 1r = z2 & 1o <= 1r by A18;
                consider 2o,2r being Real such that
             A25:2o= z2 & 2r = b & 2o <= 2r by A18,A23;
                   A26: 1o < 1r by A3,A22,A24,MEASURE5:def 2;
                   A27: 2o< 2r by A3,A22,A25,MEASURE5:def 2;
                 reconsider 1o1 = x * 1o, 1r1 = x * 1r,
                 2o1 = x * 2o, 2r1 = x * 2r as R_eal by XXREAL_0:def 1;
             A28:1r1 < 1o1 by A4,A26,XREAL_1:71;
              2r1 < 2o1 by A4,A27,XREAL_1:71;
                hence thesis by A22,A24,A25,A28,MEASURE5:def 2;
             end;
                 ].g,d.[ c= x ** A
             proof
                let q be set;
                assume
             A29:q in ].g,d.[;
                then reconsider q as Real;
                reconsider q1 = q as R_eal by XXREAL_0:def 1;
             A30:g < q1 & q1 < d & q1 in REAL by A29,MEASURE5:def 2;
             A31:q = x * (q / x) by A1,XCMPLX_1:88;
                set q2 = q / x;
                q2 in A
                proof
                   reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                   a < q3 & q3 < b & q3 in REAL
                   proof
                   A32:q3 < b
                      proof
                         q/x < (x * r)/x by A4,A30,REAL_1:74;
                         hence thesis by A4,XCMPLX_1:90;
                      end;
                      a < q3
                      proof
                        x * q2 = q by A1,XCMPLX_1:88;
                        hence thesis by A4,A30,XREAL_1:67;
                      end;
                      hence thesis by A32;
                   end;
                   hence thesis by A3,MEASURE5:def 2;
                end;
                hence thesis by A31,INTEGRA2:def 2;
             end;
             hence thesis by A20,XBOOLE_0:def 10;
         end;
          x * r <= x * s by A3,A4,XREAL_1:67;
         hence thesis by A19,MEASURE5:def 5;
      end;
      case
      A33:a in REAL & b = +infty;
         then reconsider s = a as Real;
         set c = -infty;
         reconsider d = x * s as R_eal by XXREAL_0:def 1;
     A34:x ** A = ].c,d.[
         proof
         A35:x ** A c= ].c,d.[
             proof
                let q be set;
                assume
             A36:q in x ** A;
                then reconsider q as Real;
                consider z2 being Real such that
             A37:z2 in A & q = x * z2 by A36,INTEGRA2:def 2;
                reconsider z2,q as R_eal by XXREAL_0:def 1;
                 a < z2 & z2 < b by A3,A37,MEASURE5:def 2;
                then consider o,r being Real such that
             A38:o = a & r = z2 & o <= r by A33;
             A39: o < r by A3,A37,A38,MEASURE5:def 2;
                 reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
             A40:r1 < o1 by A4,A39,XREAL_1:71;
                -infty < q by XXREAL_0:12;
                hence thesis by A37,A38,A40,MEASURE5:def 2;
             end;
             ].c,d.[ c= x ** A
             proof
                let q be set;
                assume
             A41:q in ].c,d.[;
                then reconsider q as Real;
                reconsider q1 = q as R_eal by XXREAL_0:def 1;
             A42:q = x * (q / x) by A1,XCMPLX_1:88;
                set q2 = q / x;
                q2 in A
                proof
                   reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                   a < q3 & q3 < b & q3 in REAL
                   proof
                      a < q3
                      proof
                      A43:q1 <= d & q1 <> d by A41,MEASURE5:def 2;
                         x * q2 < x * s by A42,A43,REAL_1:def 5;
                         hence thesis by A4,XREAL_1:67;
                      end;
                      hence thesis by A33,XXREAL_0:9;
                   end;
                   hence thesis by A3,MEASURE5:def 2;
                end;
                hence thesis by A42,INTEGRA2:def 2;
             end;
             hence thesis by A35,XBOOLE_0:def 10;
         end;
         c <= d by XXREAL_0:5;
         hence thesis by A34,MEASURE5:def 5;
      end;
      case a = +infty & b = +infty;
      then A44: ].a,b.[ = {} by MEASURE5:15;
         then x ** A = {} by A3,Th5;
         hence thesis by A3,A44,MEASURE5:def 5;
      end;
      end;
      hence thesis;
   end;
   case x = 0;hence thesis by A1;
   end;
   case
   A45:0 < x;
   now per cases by A3,Th6;
      case a = -infty & b = -infty;
      then A46: ].a,b.[ = {} by MEASURE5:15;
         then x ** A = {} by A3,Th5;
         hence thesis by A3,A46,MEASURE5:def 5;
      end;
      case
      A47:a = -infty & b in REAL;
         then reconsider s = b as Real;
         set c = -infty;
         reconsider d = x * s as R_eal by XXREAL_0:def 1;
      A48:x ** A = ].c,d.[
         proof
         A49:x ** A c= ].c,d.[
             proof
                let q be set;
                assume
            A50:q in x ** A;
                then reconsider q as Real;
                consider z2 being Real such that
             A51:z2 in A & q = x * z2 by A50,INTEGRA2:def 2;
                reconsider z2,q as R_eal by XXREAL_0:def 1;
                 a < z2 & z2 < b by A3,A51,MEASURE5:def 2;
                then consider r,o being Real such that
             A52:r = z2 & o = b & r <= o by A47;
             A53: r < o by A3,A51,A52,MEASURE5:def 2;
                reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
            A54:r1 < o1 by A45,A53,XREAL_1:70;
                -infty < q by XXREAL_0:12;
                hence thesis by A51,A52,A54,MEASURE5:def 2;
             end;
             ].c,d.[ c= x ** A
             proof
                let q be set;
                assume
            A55:q in ].c,d.[;
                then reconsider q as Real;
                reconsider q1 = q as R_eal by XXREAL_0:def 1;
             A56:c < q1 & q1 < d & q1 in REAL by A55,MEASURE5:def 2;
             A57:q = x * (q / x) by A1,XCMPLX_1:88;
                set q2 = q / x;
                q2 in A
                proof
                   reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                   a < q3 & q3 < b & q3 in REAL
                   proof
                      q3 < b
                      proof
                         x * q2 = q by A1,XCMPLX_1:88;
                         hence thesis by A45,A56,XREAL_1:66;
                      end;
                      hence thesis by A47,XXREAL_0:12;
                   end;
                   hence thesis by A3,MEASURE5:def 2;
                end;
                hence thesis by A57,INTEGRA2:def 2;
             end;
             hence thesis by A49,XBOOLE_0:def 10;
         end;
         c <= d by XXREAL_0:5;
         hence thesis by A48,MEASURE5:def 5;
      end;
      case a = -infty & b = +infty;
         hence thesis by A2,A3,A45,Th3,MEASURE6:35;
      end;
      case
      A58:a in REAL & b in REAL;
         then reconsider s = a, r = b as Real;
         reconsider d = x * s as R_eal by XXREAL_0:def 1;
         reconsider g = x * r as R_eal by XXREAL_0:def 1;
      A59:x ** A = ].d,g.[
         proof
         A60:x ** A c= ].d,g.[
             proof
                let q be set;
                assume
            A61:q in x ** A;
                then reconsider q as Real;
                consider z2 being Real such that
             A62:z2 in A & q = x * z2 by A61,INTEGRA2:def 2;
                reconsider z2 as R_eal by XXREAL_0:def 1;
                A63: a < z2 & z2 < b by A3,A62,MEASURE5:def 2;
                then consider 1o,1r being Real such that
             A64:1o= a & 1r = z2 & 1o <= 1r by A58;
                consider 2o,2r being Real such that
             A65:2o= z2 & 2r = b & 2o <= 2r by A58,A63;
                   A66: 1o< 1r by A3,A62,A64,MEASURE5:def 2;
                   A67: 2o< 2r by A3,A62,A65,MEASURE5:def 2;
                 reconsider 1o1 = x * 1o, 1r1 = x * 1r as R_eal by
XXREAL_0:def 1;
                 reconsider 2o1 = x * 2o, 2r1 = x * 2r as R_eal by
XXREAL_0:def 1;
             A68:1o1 < 1r1 by A45,A66,XREAL_1:70;
              2o1 < 2r1 by A45,A67,XREAL_1:70;
                hence thesis by A62,A64,A65,A68,MEASURE5:def 2;
             end;
             ].d,g.[ c= x ** A
             proof
                let q be set;
                assume
             A69:q in ].d,g.[;
                then reconsider q as Real;
                q is R_eal by XXREAL_0:def 1;
                then consider q1 being R_eal such that
             A70:q1 = q;
             A71:d < q1 & q1 < g & q1 in REAL by A69,A70,MEASURE5:def 2;
             A72:q = x * (q / x) by A1,XCMPLX_1:88;
                set q2 = q / x;
                q2 in A
                proof
                   reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                   a < q3 & q3 < b & q3 in REAL
                   proof
                   A73:q3 < b
                      proof
                         q/x < (x * r)/x by A45,A70,A71,REAL_1:73;
                         hence thesis by A45,XCMPLX_1:90;
                      end;
                      a < q3
                      proof
                         x * q2 = q by A1,XCMPLX_1:88;
                         hence thesis by A45,A70,A71,XREAL_1:66;
                      end;
                      hence thesis by A73;
                   end;
                   hence thesis by A3,MEASURE5:def 2;
                end;
                hence thesis by A72,INTEGRA2:def 2;
             end;
             hence thesis by A60,XBOOLE_0:def 10;
         end;
          x * s <= x * r by A3,A45,XREAL_1:66;
         hence thesis by A59,MEASURE5:def 5;
      end;
      case
      A74:a in REAL & b = +infty;
         then reconsider s = a as Real;
         set c = +infty;
         reconsider d = x * s as R_eal by XXREAL_0:def 1;
      A75:x ** A = ].d,c.[
         proof
         A76:x ** A c= ].d,c.[
             proof
                let q be set;
                assume
