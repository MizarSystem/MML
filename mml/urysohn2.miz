theorem Th14:
  for A being Interval holds for x being Real st x < 0 holds
  A is left_open_interval implies x ** A is right_open_interval
proof
  let A be Interval;
  let x be Real;
  assume
A1: x < 0;
  assume A is left_open_interval;
  then consider a being R_eal,b being real number such that
A2: a <= b & A = ].a,b.] by MEASURE5:def 8;
  reconsider b as R_eal by XXREAL_0:def 1;
  now per cases by A2,Th6;
    case
      a = -infty & b = -infty;
      hence thesis;
    end;
    case
A4:   a = -infty & b in REAL;
      then consider s being Real such that
A5:   s = b;
      consider c being R_eal such that
A6:   c = +infty;
      x * s is R_eal by XXREAL_0:def 1;
      then consider d being R_eal such that
A7:   d = x * s;
A8:   x ** A = [.d,c.[
