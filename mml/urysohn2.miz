 schemes REAL_1, NAT_1;

begin

::                    Properties of the Intervals

theorem Th1:
   for A being Interval st A <> {} holds
   (^^A < A^^ implies vol(A) = A^^ - ^^A) &
   (A^^ = ^^A implies vol(A) = 0.)
proof
   let A be Interval;
   assume
A1:A <> {};
   set aa = ^^A, bb = A^^;
A2:A^^ = ^^A implies vol(A) = 0.
   proof
      assume A3:A^^ = ^^A;
         now per cases by MEASURE5:def 9;
      case
      A4:A is open_interval;
         then A = ].aa,bb.[ & aa <= bb by A1,MEASURE6:48;
         hence thesis by A3,A4,MEASURE5:53;
      end;
      case
      A5:A is closed_interval;
         then A = [.aa,bb.] & aa <= bb by A1,MEASURE6:49;
         hence thesis by A3,A5,MEASURE5:54;
      end;
      case
      A6:A is right_open_interval;
         then A = [.aa,bb.[ & aa <= bb by A1,MEASURE6:50;
         hence thesis by A3,A6,MEASURE5:55;
      end;
      case
      A7:A is left_open_interval;
         then A = ].^^A,A^^.] by A1,MEASURE6:51;
         hence thesis by A3,A7,MEASURE5:56;
      end;
      end;
      hence thesis;
   end;
      ^^A < A^^ implies vol(A) = A^^ - ^^A
   proof
      assume
   A8:^^A < A^^;
         now per cases by MEASURE5:def 9;
      case
      A9:A is open_interval;
         then A = ].^^A,A^^.[ by A1,MEASURE6:48;
         hence thesis by A8,A9,MEASURE5:53;
      end;
      case
      A10:A is closed_interval;
         then A = [.aa,bb.] & aa <= bb by A1,MEASURE6:49;
         hence thesis by A8,A10,MEASURE5:54;
      end;
      case
      A11:A is right_open_interval;
         then A = [.^^A,A^^.[ by A1,MEASURE6:50;
         hence thesis by A8,A11,MEASURE5:55;
      end;
      case
      A12:A is left_open_interval;
         then A = ].^^A,A^^.] by A1,MEASURE6:51;
         hence thesis by A8,A12,MEASURE5:56;
      end;
      end;
      hence thesis;
   end;
   hence thesis by A2;
end;

theorem
      for A being Subset of REAL holds
   for x being Real st x <> 0 holds
   x" * (x * A) = A
proof
   let A be Subset of REAL;
   let x be Real;
   assume
A1:x <> 0;
   thus x" * (x * A) c= A
   proof
      let y be set;
      assume
   A2:y in x" * (x * A);
      then reconsider y as Real;
      consider z being Real such that
   A3:z in x * A & y = x" * z by A2,INTEGRA2:def 2;
      consider t being Real such that
   A4:t in A & z = x * t by A3,INTEGRA2:def 2;
         y = (x" * x) * t by A3,A4
       .= 1 * t by A1,XCMPLX_0:def 7
       .= t;
      hence thesis by A4;
   end;
   let y be set;
   assume
A5:y in A;
   then reconsider y as Real;
   set t = y / x";
      x" <> 0 by A1,XCMPLX_1:203;
then A6:y = x" * t by XCMPLX_1:88;
      t = 1 * t
    .= x * x" * t by A1,XCMPLX_0:def 7
    .= x * y by A6;
   then t in x * A by A5,INTEGRA2:def 2;
   hence thesis by A6,INTEGRA2:def 2;
end;

theorem Th3:
   for x being Real st x <> 0 holds
   for A being Subset of REAL holds
   A = REAL implies x * A = A
proof
   let x be Real;
   assume
A1:x <> 0;
   let A be Subset of REAL;
   assume
A2:A = REAL;
      for y being set holds
   y in A implies y in x * A
   proof
      let y be set;
      assume y in A;
      then reconsider y as Real;
      reconsider z = y/x as Real;
         y = x * z by A1,XCMPLX_1:88;
      hence thesis by A2,INTEGRA2:def 2;
   end;
   then A c= x * A by TARSKI:def 3;
   hence thesis by A2,XBOOLE_0:def 10;
end;

theorem Th4:
   for A being Subset of REAL st A <> {} holds 0 * A = {0}
proof
   let A be Subset of REAL;
   assume
A1:A <> {};
A2:{0} c= 0 * A
   proof
      let y be set;
      assume A3:y in {0};
      consider t being set such that
   A4:t in A by A1,XBOOLE_0:def 1;
      reconsider t as Element of A by A4;
      reconsider t as Real by A4;
         y = 0 * t by A3,TARSKI:def 1;
      hence thesis by A4,INTEGRA2:def 2;
   end;
      0 * A c= {0}
   proof
      let y be set;
      assume
   A5:y in 0 * A;
      then reconsider y as Real;
      consider z being Real such that
   A6:z in A & y = 0 * z by A5,INTEGRA2:def 2;
      thus thesis by A6,TARSKI:def 1;
   end;
   hence thesis by A2,XBOOLE_0:def 10;
end;

theorem Th5:
   for x being Real holds
   x * {} = {}
proof
   let x be Real;
A1:{} c= x * {} by XBOOLE_1:2;
      x * {} c= {}
   proof
      let y be set;
      assume
   A2:y in x * {};
      then reconsider y as Real;
      consider z being Real such that
   A3:z in {} & y = x * z by A2,INTEGRA2:def 2;
      thus thesis by A3;
   end;
   hence thesis by A1,XBOOLE_0:def 10;
end;

theorem Th6:
   for a,b being R_eal st a <= b holds
   (a = -infty & b = -infty) or (a = -infty & b in REAL) or
   (a = -infty & b = +infty) or (a in REAL & b in REAL) or
   (a in REAL & b = +infty) or (a = +infty & b = +infty)
proof
   let a,b be R_eal;
   assume
A1:a <= b;
      (a in REAL or a in {-infty,+infty}) & (b in REAL or b in {-infty,+infty})
      by XXREAL_0:def 4,XBOOLE_0:def 2;
   then (a in REAL or a = -infty or a = +infty) &
   (b in REAL or b = -infty or b = +infty) by TARSKI:def 2;
   hence thesis by A1,XXREAL_0:12,XXREAL_0:9;
end;

theorem Th7:
   for x being R_eal holds
   [.x,x.] is Interval
proof
   let x be R_eal;
      [.x,x.] is closed_interval by MEASURE5:def 6;
   hence thesis by MEASURE5:def 9;
end;

theorem Th8:
   for A being Interval holds
   0 * A is Interval
proof
   let A be Interval;
   consider s being R_eal such that
A1:s = 0.;
   per cases;
   suppose A = {};
      hence thesis by Th5;
   end;
   suppose
   A2:A <> {};
         0 * A = [.s,s.]
      proof
            0 * A = {0} by A2,Th4;
         hence thesis by A1,MEASURE5:14,SUPINF_2:def 1;
      end;
      hence thesis by Th7;
end;
end;

theorem Th9:
   for A being Interval holds
   for x being Real st x<>0 holds
   A is open_interval implies x * A is open_interval
proof
   let A be Interval;
   let x be Real;
   assume
A1:x <> 0;
   assume
A2:A is open_interval;
   then consider a,b being R_eal such that
A3:a <= b & A = ].a,b.[ by MEASURE5:def 5;
      now per cases;
   case
   A4:x < 0;
         now per cases by A3,Th6;
      case
        a = -infty & b = -infty;
      then A5: ].a,b.[ = {} by MEASURE5:15;
         then x * A = {} by A3,Th5;
         hence thesis by A3,A5,MEASURE5:def 5;
      end;
      case
      A6:a = -infty & b in REAL;
         then reconsider s = b as Real;
         set c = +infty;
            x * s is R_eal by XXREAL_0:def 1;
         then consider d being R_eal such that
      A7: d = x * s;
      A8:x * A = ].d,c.[
         proof
         A9:x * A c= ].d,c.[
             proof
                let q be set;
                assume
             A10:q in x * A;
                then reconsider q as Real;
                consider z2 being Real such that
             A11:z2 in A & q = x * z2 by A10,INTEGRA2:def 2;
                reconsider z2 as R_eal by XXREAL_0:def 1;
                reconsider q as R_eal by XXREAL_0:def 1;
                A12: a < z2 & z2 < b by A3,A11,MEASURE5:def 2;
                then consider r,o being Real such that
             A13:r = z2 & o = b & r <= o by A6;
                   r < o by A12,A13;
             then A14:x * o <= x * r & x * o <> x * r by A4,XREAL_1:71;
                 reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
             A15:o1 < r1 by A14,XXREAL_0:1;
                   q < +infty by SUPINF_1:31;
                hence thesis by A7,A11,A13,A15,MEASURE5:def 2;
             end;
                ].d,c.[ c= x * A
             proof
                let q be set;
                assume
             A16:q in ].d,c.[;
                then reconsider q as Real;
                reconsider q1 = q as R_eal by XXREAL_0:def 1;
             A17:d < q1 & q1 < c & q1 in REAL by A16,MEASURE5:def 2;
             A18:q = x * (q / x) by A1,XCMPLX_1:88;
                set q2 = q / x;
                   q2 in A
                proof
                   reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                      a < q3 & q3 < b & q3 in REAL
                   proof
                      consider r,o being Real such that
                   A19:r = d & o = q1 & r <= o by A7,A17;
                   A20:x * q2 = q by A1,XCMPLX_1:88;
                         r < o by A17,A19;
                   then q2 <= s & q2 <> s by A4,A7,A19,A20,XREAL_1:67;
                      then q3 <= b;
                      hence thesis by A6,A7,A17,A20,XXREAL_0:1,SUPINF_1:31;
                   end;
                   hence thesis by A3,MEASURE5:def 2;
                end;
                hence thesis by A18,INTEGRA2:def 2;
             end;
             hence thesis by A9,XBOOLE_0:def 10;
         end;
            d <= c by XXREAL_0:4;
         hence thesis by A8,MEASURE5:def 5;
      end;
      case a = -infty & b = +infty;
         hence thesis by A2,A3,A4,Th3,MEASURE6:35;
      end;
      case
      A21:a in REAL & b in REAL;
         then reconsider s = a, r = b as Real;
         reconsider d = x * s, g = x * r as R_eal by XXREAL_0:def 1;
      A22:x * A = ].g,d.[
         proof
         A23:x * A c= ].g,d.[
             proof
                let q be set;
                assume
             A24:q in x * A;
                then reconsider q as Real;
                consider z2 being Real such that
             A25:z2 in A & q = x * z2 by A24,INTEGRA2:def 2;
                reconsider z2 as R_eal by XXREAL_0:def 1;
                A26: a < z2 & z2 < b by A3,A25,MEASURE5:def 2;
                then consider 1o,1r being Real such that
             A27:1o= a & 1r = z2 & 1o <= 1r by A21;
                consider 2o,2r being Real such that
             A28:2o= z2 & 2r = b & 2o <= 2r by A21,A26;
                   1o < 1r by A26,A27;
             then A29:x * 1r <= x * 1o & x * 1r <> x * 1o by A4,XREAL_1:71;
                   2o< 2r by A26,A28;
             then A30:x * 2r <= x * 2o & x * 2r <> x * 2o by A4,XREAL_1:71;
                 reconsider 1o1 = x * 1o, 1r1 = x * 1r,
                     2o1 = x * 2o, 2r1 = x * 2r as R_eal by XXREAL_0:def 1;
             A31:1r1 < 1o1 by A29,XXREAL_0:1;
             2r1 < 2o1 by A30,XXREAL_0:1;
                hence thesis by A25,A27,A28,A31,MEASURE5:def 2;
             end;
                ].g,d.[ c= x * A
             proof
                let q be set;
                assume
             A32:q in ].g,d.[;
                then reconsider q as Real;
                reconsider q1 = q as R_eal by XXREAL_0:def 1;
             A33:g < q1 & q1 < d & q1 in REAL by A32,MEASURE5:def 2;
             A34:q = x * (q / x) by A1,XCMPLX_1:88;
                set q2 = q / x;
                   q2 in A
                proof
                   reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                      a < q3 & q3 < b & q3 in REAL
                   proof
                   A35:q3 < b
                      proof
                         consider p,o being Real such that
                      A36:p = g & o = q1 & p <= o by A33;
                            p < o by A33,A36;
                         then q/x < (x * r)/x by A4,A36,REAL_1:74;
                      then A37:q2 < r by A4,XCMPLX_1:90;
                         thus thesis by A37;
                      end;
                         a < q3
                      proof
                         consider r,o being Real such that
                    A38:r = q1 & o = d & r <= o by A33;
                    A39:x * q2 = q by A1,XCMPLX_1:88;
                            r < o by A33,A38;
                      then s <= q2 & q2 <> s by A4,A38,A39,XREAL_1:67;
                         then a <= q3;
                         hence thesis by A33,A39,XXREAL_0:1;
                      end;
                      hence thesis by A35;
                   end;
                   hence thesis by A3,MEASURE5:def 2;
                end;
                hence thesis by A34,INTEGRA2:def 2;
             end;
             hence thesis by A23,XBOOLE_0:def 10;
         end;
            s <= r by A3;
         then x * r <= x * s by A4,XREAL_1:67;
         then g <= d;
         hence thesis by A22,MEASURE5:def 5;
      end;
      case
      A40:a in REAL & b = +infty;
         then reconsider s = a as Real;
         set c = -infty;
         reconsider d = x * s as R_eal by XXREAL_0:def 1;
      A41:x * A = ].c,d.[
         proof
         A42:x * A c= ].c,d.[
             proof
                let q be set;
                assume
             A43:q in x * A;
                then reconsider q as Real;
                consider z2 being Real such that
             A44:z2 in A & q = x * z2 by A43,INTEGRA2:def 2;
                reconsider z2,q as R_eal by XXREAL_0:def 1;
                A45: a < z2 & z2 < b by A3,A44,MEASURE5:def 2;
                then consider o,r being Real such that
             A46:o = a & r = z2 & o <= r by A40;
                   o < r by A45,A46;
             then A47:x * r <= x * o & x * r <> x * o by A4,XREAL_1:71;
                 reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
             A48:r1 < o1 by A47,XXREAL_0:1;
                   -infty < q by SUPINF_1:31;
                hence thesis by A44,A46,A48,MEASURE5:def 2;
             end;
                ].c,d.[ c= x * A
             proof
                let q be set;
                assume
             A49:q in ].c,d.[;
                then reconsider q as Real;
                reconsider q1 = q as R_eal by XXREAL_0:def 1;
             A50:q = x * (q / x) by A1,XCMPLX_1:88;
                set q2 = q / x;
                   q2 in A
                proof
                   reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                      a < q3 & q3 < b & q3 in REAL
                   proof
                         a < q3
                      proof
                      A51:q1 <= d & q1 <> d by A49,MEASURE5:def 2;
                         consider r,o being Real;
                            x * q2 < x * s by A50,A51,REAL_1:def 5;
                      then s <= q2 & q2 <> s by A4,XREAL_1:67;
                         then a <= q3;
                         hence thesis by A50,A51,XXREAL_0:1;
                      end;
                      hence thesis by A40,SUPINF_1:31;
                   end;
                   hence thesis by A3,MEASURE5:def 2;
                end;
                hence thesis by A50,INTEGRA2:def 2;
             end;
             hence thesis by A42,XBOOLE_0:def 10;
         end;
            c <= d by XXREAL_0:5;
         hence thesis by A41,MEASURE5:def 5;
      end;
      case
        a = +infty & b = +infty;
      then A52: ].a,b.[ = {} by MEASURE5:15;
         then x * A = {} by A3,Th5;
         hence thesis by A3,A52,MEASURE5:def 5;
      end;
      end;
      hence thesis;
   end;
   case x = 0;hence thesis by A1;
   end;
   case
   A53:0 < x;
         now per cases by A3,Th6;
      case a = -infty & b = -infty;
      then A54: ].a,b.[ = {} by MEASURE5:15;
         then x * A = {} by A3,Th5;
         hence thesis by A3,A54,MEASURE5:def 5;
      end;
      case
      A55:a = -infty & b in REAL;
         then reconsider s = b as Real;
         set c = -infty;
         reconsider d = x * s as R_eal by XXREAL_0:def 1;
      A56:x * A = ].c,d.[
         proof
         A57:x * A c= ].c,d.[
             proof
                let q be set;
                assume
             A58:q in x * A;
                then reconsider q as Real;
                consider z2 being Real such that
             A59:z2 in A & q = x * z2 by A58,INTEGRA2:def 2;
                reconsider z2,q as R_eal by XXREAL_0:def 1;
                A60: a < z2 & z2 < b by A3,A59,MEASURE5:def 2;
                then consider r,o being Real such that
             A61:r = z2 & o = b & r <= o by A55;
                   r < o by A60,A61;
             then A62:x * r <= x * o & x * r <> x * o by A53,XREAL_1:70;
                 reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
             A63:r1 < o1 by A62,XXREAL_0:1;
                   -infty < q by SUPINF_1:31;
                hence thesis by A59,A61,A63,MEASURE5:def 2;
             end;
                ].c,d.[ c= x * A
             proof
                let q be set;
                assume
            A64:q in ].c,d.[;
                then reconsider q as Real;
                reconsider q1 = q as R_eal by XXREAL_0:def 1;
             A65:c < q1 & q1 < d & q1 in REAL by A64,MEASURE5:def 2;
             A66:q = x * (q / x) by A1,XCMPLX_1:88;
                set q2 = q / x;
                   q2 in A
                proof
                   reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
                      a < q3 & q3 < b & q3 in REAL
                   proof
                         q3 < b
                      proof
                      A67:q1 <= d & q1 <> d by A64,MEASURE5:def 2;
                         consider r,o being Real such that
                      A68:r = q1 & o = d & r <= o by A65;
                      A69:x * q2 = q by A1,XCMPLX_1:88;
                            r < o by A65,A68;
                      then q2 <= s & q2 <> s by A53,A68,A69,XREAL_1:66;
                         then q3 <= b;
                         hence thesis by A67,A69,XXREAL_0:1;
                      end;
                      hence thesis by A55,SUPINF_1:31;
                   end;
                   hence thesis by A3,MEASURE5:def 2;
                end;
                hence thesis by A66,INTEGRA2:def 2;
             end;
             hence thesis by A57,XBOOLE_0:def 10;
         end;
            c <= d by XXREAL_0:5;
         hence thesis by A56,MEASURE5:def 5;
      end;
      case a = -infty & b = +infty;
         hence thesis by A2,A3,A53,Th3,MEASURE6:35;
      end;
      case
      A70:a in REAL & b in REAL;
         then reconsider s = a, r = b as Real;
         reconsider d = x * s as R_eal by XXREAL_0:def 1;
         reconsider g = x * r as R_eal by XXREAL_0:def 1;
      A71:x * A = ].d,g.[
         proof
         A72:x * A c= ].d,g.[
             proof
                let q be set;
                assume
             A73:q in x * A;
                then reconsider q as Real;
                consider z2 being Real such that
             A74:z2 in A & q = x * z2 by A73,INTEGRA2:def 2;
                reconsider z2 as R_eal by XXREAL_0:def 1;
                A75: a < z2 & z2 < b by A3,A74,MEASURE5:def 2;
                then consider 1o,1r being Real such that
             A76:1o= a & 1r = z2 & 1o <= 1r by A70;
                consider 2o,2r being Real such that
             A77:2o= z2 & 2r = b & 2o <= 2r by A70,A75;
                   1o< 1r by A75,A76;
             then A78:x * 1o <= x * 1r & x * 1o <> x * 1r by A53,XREAL_1:70
;
                   2o< 2r by A75,A77;
             then A79:x * 2o <= x * 2r & x * 2o <> x * 2r by A53,XREAL_1:70
;
                 reconsider 1o1 = x * 1o, 1r1 = x * 1r as R_eal by
XXREAL_0:def 1;
                 reconsider 2o1 = x * 2o, 2r1 = x * 2r as R_eal by
XXREAL_0:def 1;
             A80:1o1 < 1r1 by A78,XXREAL_0:1;
             2o1 < 2r1 by A79,XXREAL_0:1;
                hence thesis by A74,A76,A77,A80,MEASURE5:def 2;
             end;
                ].d,g.[ c= x * A
             proof
                let q be set;
                assume
             A81:q in ].d,g.[;
                then reconsider q as Real;
                   q is R_eal by XXREAL_0:def 1;
                then consider q1 being R_eal such that
