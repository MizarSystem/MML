end;

theorem
       for A being Interval
   for x being Real st 0 <= x
   for y being Real st y = vol(A) holds
   x * y = vol(x ** A)
proof
   let A be Interval;
   let x be Real;
   assume
A1:0 <= x;
   let y be Real;
   assume
A2:y = vol(A);
   per cases;
   suppose A = {};
      hence thesis by A2,Th5,MEASURE5:60;
   end;
   suppose
   A3:not A = {};
          now per cases by A1;
      case
      A4:x = 0;
      then x ** A = {0.} by A3,Th4;
         then x ** A = [.0.,0..] by MEASURE5:14;
         hence thesis by A4,MEASURE5:def 10;
      end;
      case
      A5:0 < x;
      A6:not ((^^A = +infty & A^^ = +infty)
         or (^^A = -infty & A^^ = -infty))
         proof
            assume
         A7:(^^A = +infty & A^^ = +infty)
            or (^^A = -infty & A^^ = -infty);
            A = ].^^A,A^^.[ or A = ].^^A,A^^.] or
            A = [.^^A,A^^.] or A = [.^^A,A^^.[ by A3,MEASURE6:52;
            hence thesis by A3,A7,MEASURE5:13,14,XXREAL_1:20;
         end;
         A8: ^^A <= A^^ by A3,MEASURE6:52;
      A9:((^^A < A^^) implies (vol(A) = A^^ - ^^A)) &
         (A^^ = ^^A implies vol(A) = 0. ) by A3,Th1;
         reconsider B = x ** A as Interval;
         A10: B <> {}
         proof
            consider a being set such that
        A11:a in A by A3,XBOOLE_0:def 1;
            reconsider a as Real by A11;
            x * a is Real;
            hence thesis by A11,INTEGRA2:def 2;
         end;
      then A12:(^^B < B^^ implies vol(B) = B^^ - ^^B) &
        (B^^ = ^^B implies vol(B) = 0. ) by Th1;
      ^^A is Real & A^^ is Real
         proof
            assume
        A13:not ^^A is Real or not A^^ is Real;
            consider a being set such that
         A14:a in A by A3,XBOOLE_0:def 1;
            reconsider a as Real by A14;
            R_EAL a = a by MEASURE6:def 1;
         then A15:-infty < R_EAL a & R_EAL a < +infty by XXREAL_0:9,12;
         now per cases by A13;
            case not ^^A is Real;
              then A16: ^^A in {-infty,+infty} by XBOOLE_0:def 2,XXREAL_0:def 4
;
            per cases by A16,TARSKI:def 2;
               case ^^A = -infty;
                  hence thesis by A2,A6,A8,A9,SUPINF_2:7,XXREAL_0:1;
               end;
               case ^^A = +infty;
                  hence thesis by A14,A15,MEASURE6:54;
               end;
            end;
            case not A^^ is Real;
            then A17:A^^ in {-infty,+infty} by XBOOLE_0:def 2,XXREAL_0:def 4;
            now per cases by A17,TARSKI:def 2;
               case A^^ = -infty;
                  hence thesis by A14,A15,MEASURE6:54;
               end;
               case A^^ = +infty;
                  hence thesis by A2,A6,A8,A9,SUPINF_2:6,XXREAL_0:1;
               end;
               end;
               hence thesis;
            end;
            end;
            hence thesis;
         end;
         then consider s,t being Real such that
      A18:t = A^^ & s = ^^A;
      A19:^^B = x * s & B^^ = x * t
         proof
          now per cases by A3,MEASURE6:52;
            case A = ].^^A,A^^.[;
               hence thesis by A3,A5,A18,Th17;
            end;
            case A = ].^^A,A^^.];
               hence thesis by A3,A5,A18,Th16;
            end;
            case A = [.^^A,A^^.];
               hence thesis by A3,A5,A18,Th15;
            end;
            case A = [.^^A,A^^.[;
               hence thesis by A3,A5,A18,Th18;
            end;
            end;
            hence thesis;
         end;
         now per cases by A8,XXREAL_0:1;
         case A^^ = ^^A;
            hence thesis by A2,A9,A10,A18,A19,Th1;
         end;
         case
         A20:^^A < A^^;
         then A21:vol(A) = t - s by A9,A18,SUPINF_2:5;
            consider s1,t1 being Real;
            vol(B) = x * t - x * s by A5,A12,A18,A19,A20,SUPINF_2:5,XREAL_1:70
                   .= x * y by A2,A21;
            hence thesis;
         end;
         end;
         hence thesis;
      end;
      end;
      hence thesis;
end;
end;

