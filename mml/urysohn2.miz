theorem Th28:
  for a,b being Real st a < b & 0 <= a & b <= 1 holds
  ex c being Real st c in DYADIC & a < c & c < b
proof
  let a,b be Real;
  assume
A1: a < b & 0 <= a & b <= 1;
  set eps = b - a;
  consider n being Element of NAT such that
A2: 1 < 2|^n * eps by Th23,A1,XREAL_1:52;
  set aa = 2|^n * a, bb = 2|^n * b;
A3: 1 < bb - aa by A2;
A4: 0 < 2|^n by NEWTON:102;
  consider m being Element of NAT such that
A5: aa < m & m < bb by A3,Th24,A1;
  a < m / 2|^n & m / 2|^n < b by A4,A5,XREAL_1:83,85;
  then m / 2|^n < 1 by A1,XXREAL_0:2;
  then
A6: 0 <= m & m < 2|^n by A4,XREAL_1:183;
  set x = m / 2|^n;
A7: x in dyadic(n) by A6,URYSOHN1:def 3;
  take x;
  thus thesis by A4,A5,A7,URYSOHN1:def 4,XREAL_1:83,85;
end;

theorem Th29:
  for a,b being Real st a < b ex c being Real st c in DOM & a < c & c < b
proof
  let a,b be Real;
  assume
A1: a < b;
  per cases;
  suppose
A2: a < 0 & b <= 1;
    now per cases;
      case
A3:     b <= 0;
        consider c being real number such that
A4:     a < c & c < b by A1,XREAL_1:7;
A5:     halfline 0 = {g where g is Real : g<0} by XXREAL_1:229;
        reconsider c as Real by XREAL_0:def 1;
        c in halfline 0 by A3,A4,A5;
        then c in (halfline 0) \/ DYADIC by XBOOLE_0:def 3;
        then c in DOM by URYSOHN1:def 5,XBOOLE_0:def 3;
        hence thesis by A4;
      end;
      case
A6:     0 < b;
        set a1 = 0;
        consider c being Real such that
A7:     c in DYADIC & a1 < c & c < b by A2,A6,Th28;
        c in (halfline 0) \/ DYADIC by A7,XBOOLE_0:def 3;
        then c in DOM by URYSOHN1:def 5,XBOOLE_0:def 3;
        hence thesis by A2,A7;
      end;
    end;
    hence thesis;
  end;
  suppose
A8: a < 0 & 1 < b;
    consider a1,b1 being Real such that
A9: a1 = 0 & b1 = 1;
    consider c being Real such that
A10: c in DYADIC & a1 < c & c < b1 by A9,Th28;
A11: c in (halfline 0) \/ DYADIC by A10,XBOOLE_0:def 3;
    take c;
    thus thesis by A8,A9,A10,A11,URYSOHN1:def 5,XBOOLE_0:def 3,XXREAL_0:2;
  end;
  suppose 0 <= a & b <= 1;
    then consider c being Real such that
A12: c in DYADIC & a < c & c < b by A1,Th28;
A13: c in (halfline 0) \/ DYADIC by A12,XBOOLE_0:def 3;
    take c;
    thus thesis by A12,A13,URYSOHN1:def 5,XBOOLE_0:def 3;
  end;
  suppose
A14: 0 <= a & 1 < b;
    now per cases;
      case
A15:    1 <= a;
        consider c being real number such that
A16:    a < c & c < b by A1,XREAL_1:7;
A17:    right_open_halfline 1 = {g where g is Real : 1<g} by XXREAL_1:230;
        reconsider c as Real by XREAL_0:def 1;
        1 < c by A15,A16,XXREAL_0:2;
        then c in right_open_halfline 1 by A17;
        then c in DYADIC \/ (right_open_halfline 1) by XBOOLE_0:def 3;
        then c in (halfline 0) \/ (DYADIC \/ right_open_halfline 1)
        by XBOOLE_0:def 3;
        then c in DOM by URYSOHN1:def 5,XBOOLE_1:4;
        hence thesis by A16;
      end;
      case
A18:    a < 1;
        set b1 = 1;
        consider c being Real such that
A19:    c in DYADIC & a < c & c < b1 by A14,A18,Th28;
        c in (halfline 0) \/ DYADIC by A19,XBOOLE_0:def 3;
        then
A20:    c in DOM by URYSOHN1:def 5,XBOOLE_0:def 3;
        a < c & c < b by A14,A19,XXREAL_0:2;
        hence thesis by A20;
      end;
    end;
    hence thesis;
  end;
end;

theorem
  for A being non empty Subset of ExtREAL holds
  for a,b being R_eal st A c= [.a,b.] holds a <= inf A & sup A <= b
proof
  let A be non empty Subset of ExtREAL;
  let a,b be R_eal;
  assume
A1: A c= [.a,b.];
  reconsider B = [.a,b.] as non empty Subset of ExtREAL
  by A1,MEMBERED:2;
A3: a <= inf A
  proof
    for x being ext-real number holds (x in B implies a <= x)
    by XXREAL_1:1;
    then a is LowerBound of B by XXREAL_2:def 2;
    then a is LowerBound of A by A1,XXREAL_2:5;
    hence thesis by XXREAL_2:def 4;
  end;
  sup A <= b
  proof
    for x being ext-real number st x in B holds x <= b by XXREAL_1:1;
    then b is UpperBound of B by XXREAL_2:def 1;
    then b is UpperBound of A by A1,XXREAL_2:6;
    hence thesis by XXREAL_2:def 3;
  end;
  hence thesis by A3;
end;

