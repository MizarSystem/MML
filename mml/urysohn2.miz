theorem Th28:
   for a,b being Real st a < b & 0 <= a & b <= 1 holds
   ex c being Real st c in DYADIC & a < c & c < b
proof
   let a,b be Real;
   assume
A1:a < b & 0 <= a & b <= 1;
   set eps = b - a;
   0 < eps by A1,XREAL_1:52;
   then consider n being Element of NAT such that
A2:1 < 2|^n * eps by Th23;
   set aa = 2|^n * a, bb = 2|^n * b;
A3:1 < bb - aa by A2;
A4:0 < 2|^n by NEWTON:102;
    0 <= 2|^n * a by A1,REAL_2:121;
   then consider m being Element of NAT such that
A5:aa < m & m < bb by A3,Th24;
   a < m / 2|^n & m / 2|^n < b by A4,A5,REAL_2:178;
    then m / 2|^n < 1 by A1,XXREAL_0:2;
   then A6:0 <= m & m < 2|^n by A4,REAL_2:117;
   set x = m / 2|^n;
   A7: x in dyadic(n) by A6,URYSOHN1:def 3;
   take x;
   thus thesis by A4,A5,A7,REAL_2:178,URYSOHN1:def 4;
end;

theorem Th29:
   for a,b being Real st a < b
   ex c being Real st c in DOM & a < c & c < b
proof
   let a,b be Real;
   assume
A1:a < b;
   per cases;
   suppose
   A2:a < 0 & b <= 1;
   now per cases;
      case
      A3:b <= 0;
         consider c being real number such that
      A4:a < c & c < b by A1,XREAL_1:7;
A5:  halfline 0 = {g where g is Real : g<0} by LIMFUNC1:151;
         reconsider c as Real by XREAL_0:def 1;
         c in halfline 0 by A3,A4,A5;
         then c in (halfline 0) \/ DYADIC by XBOOLE_0:def 2;
         then c in DOM by URYSOHN1:def 5,XBOOLE_0:def 2;
         hence thesis by A4;
      end;
      case
      A6:0 < b;
         set a1 = 0;
         consider c being Real such that
      A7:c in DYADIC & a1 < c & c < b by A2,A6,Th28;
         c in (halfline 0) \/ DYADIC by A7,XBOOLE_0:def 2;
         then c in DOM by URYSOHN1:def 5,XBOOLE_0:def 2;
         hence thesis by A2,A7;
      end;
      end;
      hence thesis;
   end;
   suppose
   A8:a < 0 & 1 < b;
      consider a1,b1 being Real such that
   A9:a1 = 0 & b1 = 1;
      consider c being Real such that
   A10:c in DYADIC & a1 < c & c < b1 by A9,Th28;
      A11: c in (halfline 0) \/ DYADIC by A10,XBOOLE_0:def 2;
      take c;
      thus thesis by A8,A9,A10,A11,URYSOHN1:def 5,XBOOLE_0:def 2,XXREAL_0:2;
   end;
   suppose
      0 <= a & b <= 1;
      then consider c being Real such that
   A12:c in DYADIC & a < c & c < b by A1,Th28;
      A13: c in (halfline 0) \/ DYADIC by A12,XBOOLE_0:def 2;
      take c;
      thus thesis by A12,A13,URYSOHN1:def 5,XBOOLE_0:def 2;
   end;
   suppose
   A14:0 <= a & 1 < b;
   now per cases;
      case
      A15:1 <= a;
         consider c being real number such that
      A16:a < c & c < b by A1,XREAL_1:7;
A17:     right_open_halfline 1 = {g where g is Real : 1<g} by LIMFUNC1:152;
         reconsider c as Real by XREAL_0:def 1;
             1 < c by A15,A16,XXREAL_0:2;
         then c in right_open_halfline 1 by A17;
         then c in DYADIC \/ (right_open_halfline 1) by XBOOLE_0:def 2;
         then c in (halfline 0) \/ (DYADIC \/ right_open_halfline 1)
           by XBOOLE_0:def 2;
      then c in DOM by URYSOHN1:def 5,XBOOLE_1:4;
         hence thesis by A16;
      end;
      case
      A18:a < 1;
         set b1 = 1;
         consider c being Real such that
      A19:c in DYADIC & a < c & c < b1 by A14,A18,Th28;
         c in (halfline 0) \/ DYADIC by A19,XBOOLE_0:def 2;
         then A20:c in DOM by URYSOHN1:def 5,XBOOLE_0:def 2;
         a < c & c < b by A14,A19,XXREAL_0:2;
         hence thesis by A20;
      end;
      end;
      hence thesis;
end;
end;

theorem
       for A being non empty Subset of ExtREAL holds
   for a,b being R_eal st A c= [.a,b.] holds
   a <= inf A & sup A <= b
proof
   let A be non empty Subset of ExtREAL;
   let a,b be R_eal;
   assume
A1:A c= [.a,b.];
   consider q being set such that
A2:q in A by XBOOLE_0:def 1;
   reconsider B = [.a,b.] as non empty Subset of ExtREAL
     by A1,A2,NUMBERS:31,XBOOLE_1:1;
A3:a <= inf A
   proof
      for x being ext-real number holds (x in B implies a <= x)
     by MEASURE5:def 1;
      then a is minorant of B by SUPINF_1:def 10;
      then a is minorant of A by A1,SUPINF_1:39;
      hence thesis by SUPINF_1:def 17;
   end;
   sup A <= b
   proof
      for x being ext-real number st x in B holds x <= b by MEASURE5:def 1;
      then b is majorant of B by SUPINF_1:def 9;
      then b is majorant of A by A1,SUPINF_1:34;
      hence thesis by SUPINF_1:def 16;
   end;
   hence thesis by A3;
end;

theorem
  0 in DYADIC & 1 in DYADIC
proof
   0 in dyadic(0) & 1 in dyadic(0) by URYSOHN1:12;
   hence thesis by URYSOHN1:def 4;
end;

theorem
  for a,b being R_eal st a = 0 & b = 1 holds
    DYADIC c= [.a,b.]
proof
   let a,b being R_eal;
   assume
A1:a = 0 & b = 1;
   let x be set;
   assume
A2:x in DYADIC;
   then reconsider x as Real;
   consider n being Element of NAT such that
A3:x in dyadic(n) by A2,URYSOHN1:def 4;
   reconsider x as R_eal by XXREAL_0:def 1;
   a <= x & x <= b by A1,A3,URYSOHN1:5;
   hence thesis by MEASURE5:def 1;
end;

theorem
   for n,k being Element of NAT st n <= k holds
   dyadic(n) c= dyadic(k)
proof
   let n,k be Element of NAT;
   assume
A1:n <= k;
A2:for s being Element of NAT holds dyadic(n) c= dyadic(n+s)
   proof
   defpred P[Element of NAT] means dyadic(n) c= dyadic(n+$1);
   A3: P[0];
   A4:for k being Element of NAT st P[k] holds P[(k+1)]
      proof
         let k be Element of NAT;
         assume
      A5:dyadic(n) c= dyadic(n+k);
         dyadic(n+k) c= dyadic(n+k+1) by URYSOHN1:11;
         hence thesis by A5,XBOOLE_1:1;
      end;
      for k be Element of NAT holds P[k] from NAT_1:sch 1(A3,A4);
      hence thesis;
   end;
   consider s being Nat such that
A6:k = n + s by A1,NAT_1:10;
   s in NAT by ORDINAL1:def 13;
   hence thesis by A2,A6;
end;

theorem Th34: :: JGRAPH_1:31
   for a,b,c,d being Real st a < c & c < b & a < d & d < b holds
   abs(d - c) < b - a
proof
   let a,b,c,d be Real;
   assume
A1:a < c & c < b & a < d & d < b;
then A2:a + d < c + b by XREAL_1:10;
then A3:d - c < b - a by XREAL_1:23;
A4:c + a < b + d by A1,XREAL_1:10;
then c - d <> b - a & c - d <= b - a by XREAL_1:23;
     then -(b - a) <= -(c - d) by XREAL_1:26;
then A5:abs(d - c) <= b - a by A3,ABSVALUE:12;
   abs(d - c) <> b - a
   proof
      assume
   A6:abs(d - c) = b - a;
   A7:d - c = b - a or d - c = -(b - a)
      proof
      per cases;
         suppose 0 <= d - c;
           hence thesis by A6,ABSVALUE:def 1;
         end;
         suppose not 0 <= d - c;
           then b - a = -(d - c) by A6,ABSVALUE:def 1;
           hence thesis;
         end;
      end;
      now per cases by A7;
      case d - c = b - a;
         hence thesis by A2;
      end;
      case d - c = -(b - a);
         hence thesis by A4;
      end;
      end;
      hence thesis;
   end;
   hence thesis by A5,REAL_1:def 5;
end;

theorem
  for eps being Real st 0 < eps holds
   for d being Real st 0 < d & d <= 1 holds
   ex r1,r2 being Real st
     r1 in DYADIC \/ (right_open_halfline 1) &
     r2 in DYADIC \/ (right_open_halfline 1) &
     0 < r1 & r1 < d & d < r2 & r2 - r1 < eps
proof
   let eps be Real;
   assume
A1:0 < eps;
   let d be Real;
   assume
A2:0 < d & d <= 1;
   consider eps1 being real number such that
A3:0 < eps1 & eps1 < eps by A1,XREAL_1:7;
   reconsider eps1 as Real by XREAL_0:def 1;
   per cases;
   suppose
   A4:eps1 < d;
      d - eps1 < d - 0 by A3,REAL_1:92;
      then ex c being Real st c in DOM & d - eps1 < c & c < d by Th29;
      then consider r1 being Real such that
   A5:d - eps1 < r1 & r1 < d & r1 in DOM;
      eps - eps < eps - eps1 by A3,REAL_1:92;
      then d + 0 < d + (eps - eps1) by XREAL_1:10;
      then ex c being Real st c in DOM & d < c & c < d + (eps - eps1) by Th29;
      then consider r2 being Real such that
   A6:d < r2 & r2 < d + (eps - eps1) & r2 in DOM;
      take r1,r2;
  A7: 0 < d - eps1 by A4,XREAL_1:52;
      (r1 in (halfline 0) \/ DYADIC or
       r1 in right_open_halfline 1) &
      (r2 in halfline 0 \/ DYADIC or r2 in right_open_halfline 1)
      by A5,A6,URYSOHN1:def 5,XBOOLE_0:def 2;
      then A8: (r1 in halfline 0 or r1 in DYADIC or
      r1 in right_open_halfline 1) &
      (r2 in halfline 0 or r2 in DYADIC or
        r2 in right_open_halfline 1) by XBOOLE_0:def 2;
   A9:r1 < r2 by A5,A6,XXREAL_0:2;
   then A10:r1 < d + (eps - eps1) by A6,XXREAL_0:2;
   A11:d - eps1 < r2 by A5,A9,XXREAL_0:2;
       (d + (eps - eps1)) - (d - eps1) = eps;
   then A12:abs(r2 - r1) < eps by A5,A6,A10,A11,Th34;
      0 <= r2 - r1 by A9,XREAL_1:50;
      hence thesis by A5,A6,A7,A8,A12,ABSVALUE:def 1,URYSOHN1:1,XBOOLE_0:def 2
;
   end;
   suppose
   A13:d <= eps1;
      consider r1 being Real such that
   A14:r1 in DOM & 0 < r1 & r1 < d by A2,Th29;
      0 + d < r1 + eps1 by A13,A14,XREAL_1:10;
      then ex c being Real st c in DOM & d < c & c < r1 + eps1 by Th29;
      then consider r2 being Real such that
   A15:d < r2 & r2 < r1 + eps1 & r2 in DOM;
      take r1,r2;
   A16:not r1 in halfline 0 by A14,URYSOHN1:1;
   A17:not r2 in halfline 0 by A2,A15,URYSOHN1:1;
      (r1 in halfline 0 \/ DYADIC or r1 in right_open_halfline 1) &
      (r2 in halfline 0 \/ DYADIC or r2 in right_open_halfline 1)
      by A14,A15,URYSOHN1:def 5,XBOOLE_0:def 2;
      then A18: (r1 in DYADIC or r1 in right_open_halfline 1) &
      (r2 in DYADIC or r2 in right_open_halfline 1)
        by A16,A17,XBOOLE_0:def 2;
      r2 - r1 < r1 + eps1 - r1 by A15,XREAL_1:11;
      hence thesis by A3,A14,A15,A18,XBOOLE_0:def 2,XXREAL_0:2;
end;
end;

