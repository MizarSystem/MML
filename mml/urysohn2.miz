theorem
  for A being Interval for x being Real st 0 <= x
  for y being Real st y = diameter(A) holds x * y = diameter(x ** A)
proof
  let A be Interval;
  let x be Real such that
Z1: 0 <= x;
  let y be Real such that
Z2: y = diameter(A);
  per cases;
  suppose
S:  A is empty;
X: now assume x ** A is non empty;
     then consider y being real number such that
W:    y in x ** A by MEMBERED:9;
     reconsider y as Real by XREAL_0:def 1;
     ex z being Real st z in A & y = x * z by W,INTEGRA2:def 2;
    hence contradiction by S;
   end;
   thus x * y = 0 by S,Z2,MEASURE5:60
      .= diameter(x ** A) by X,MEASURE5:60;
  end;
  suppose
S:  A is non empty;
   then reconsider AA = A as non empty Subset of REAL;
    consider z being real number such that
W:    z in A by S,MEMBERED:9;
      reconsider z as Real by XREAL_0:def 1;
XX:   x * z in x ** A by W,INTEGRA2:def 2;
   reconsider z = x as R_eal by XXREAL_0:def 1;
    reconsider u = x as R_eal by XXREAL_0:def 1;
Y2: inf(x ** AA) = u * inf AA by Tx2,Z1;
  thus x * y = z * diameter A by Z2,EXTREAL1:13
     .= z * (sup A - inf A) by S,MEASURE5:def 10
     .= z * sup A - (z * inf A) by XXREAL_3:112
     .= sup(x ** A) - inf(x ** A) by Tx1,Z1,Y2
     .= diameter(x ** A) by XX,MEASURE5:def 10;
  end;
end;

theorem Th23:
  for eps being Real st 0 < eps holds
  ex n being Element of NAT st 1 < 2|^n * eps
proof
  let eps be Real;
  assume
A1: 0 < eps;
  consider n being Element of NAT such that
A2: 1 / eps < n by SEQ_4:10;
  1/eps < 2|^n by A2,NEWTON:105,XXREAL_0:2;
  then
A3: 1/eps * eps < 2|^n * eps by A1,XREAL_1:70;
  take n;
  thus thesis by A1,A3,XCMPLX_1:88;
end;

theorem Th24:
  for a,b being Real st 0 <= a & 1 < b - a holds
  ex n being Element of NAT st a < n & n < b
proof
  let a,b be Real;
  assume that
A1: 0 <= a and
A2: 1 < b - a;
A3:  a < [\a/]+1 by INT_1:52;
   then reconsider n = [\a/]+1 as Element of NAT by A1,INT_1:16;
  take n;
  thus a < n by A3;
A4:  1+a < b by A2,XREAL_1:22;
   [\a/] <= a by INT_1:def 4;
   then [\a/]+1 <= a+1 by XREAL_1:8;
  hence thesis by A4,XXREAL_0:2;
end;

