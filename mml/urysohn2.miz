Lm1: for A being non empty Subset of REAL, x being Real st x > 0 & x**A is
bounded_above holds A is bounded_above

proof
  let A be non empty Subset of REAL, x be Real such that
A1: x > 0 and
A2: x**A is bounded_above;
A3: sup(x**A) <> +infty by A2,XXREAL_2:77;
  consider r being real number such that
A4: r in A by MEMBERED:9;
  per cases by A3,XXREAL_0:14;
  suppose
A5: sup(x**A) = -infty;
    take 0;
A6: x*r in x**A by A4,INTEGRA2:def 2;
    -infty is UpperBound of x**A by A5,XXREAL_2:def 3;
    hence thesis by A6,XXREAL_0:6,XXREAL_2:def 1;
  end;
  suppose
    sup(x**A) in REAL;
    then reconsider r = sup(x**A) as Real;
    take r/x;
    let z be ext-realnumber;
    assume
A7: z in A;
    then reconsider z1=z as Real;
    x" ** (x ** A) = A by A1,Th2;
    then consider s being Real such that
A8: s in x**A and
A9: z = x" * s by A7,INTEGRA2:def 2;
    s <= r by A8,XXREAL_2:4;
    then s/x <= r/x by A1,XREAL_1:74;
    hence thesis by A9;
  end;
end;

theorem Th20:
  for A being non empty Subset of REAL, x being Real, y being
  R_eal st x = y & 0 <= y holds sup(x ** A) = y * sup A
proof
  let A be non empty Subset of REAL, x being Real, y being R_eal such that
A1: x = y and
A2: 0 <= y;
  reconsider Y = x ** A as non empty Subset of REAL by INTEGRA2:7;
  per cases;
  suppose
A3: A is not bounded_above;
    per cases by A2;
    suppose
A4:   y = 0;
      then x ** A = {0} by A1,INTEGRA2:42;
      hence sup(x ** A) = 0 by XXREAL_2:11
        .= y * sup A by A4;
    end;
    suppose
A5:   y > 0;
      then Y is not bounded_above by A1,A3,Lm1;
      hence sup(x ** A) = +infty by XXREAL_2:77
        .= y * +infty by A5,XXREAL_3:def 5
        .= y * sup A by A3,XXREAL_2:77;
    end;
  end;
  suppose
    A is bounded_above;
    then reconsider X = A as non empty bounded_above (real-membered set);
    reconsider u = upper_bound X as Real by XREAL_0:def 1;
    x ** X is bounded_above by A1,A2,INTEGRA2:9;
    then reconsider Y as non empty bounded_above (real-membered set);
    thus sup(x ** A) = upper_bound Y
      .= x * u by A1,A2,INTEGRA2:13
      .= y * sup A by A1,EXTREAL1:13;
  end;
end;

Lm2: for A being non empty Subset of REAL, x being Real st x > 0 & x**A is
bounded_below holds A is bounded_below

proof
  let A be non empty Subset of REAL, x be Real such that
A1: x > 0 and
A2: x**A is bounded_below;
A3: inf(x**A) <> -infty by A2,XXREAL_2:78;
  consider r being real number such that
A4: r in A by MEMBERED:9;
  per cases by A3,XXREAL_0:14;
  suppose
A5: inf(x**A) = +infty;
    take 0;
A6: x*r in x**A by A4,INTEGRA2:def 2;
    +infty is LowerBound of x**A by A5,XXREAL_2:def 4;
    hence thesis by A6,XXREAL_0:4,XXREAL_2:def 2;
  end;
  suppose
    inf(x**A) in REAL;
    then reconsider r = inf(x**A) as Real;
    take r/x;
    let z be ext-realnumber;
    assume
A7: z in A;
    then reconsider z1=z as Real;
    x" ** (x ** A) = A by A1,Th2;
    then consider s being Real such that
A8: s in x**A and
A9: z = x" * s by A7,INTEGRA2:def 2;
    r/x <= s/x by A1,A8,XREAL_1:74,XXREAL_2:3;
    hence thesis by A9;
  end;
end;

theorem Th21:
  for A being non empty Subset of REAL, x being Real, y being
  R_eal st x = y & 0 <= y holds inf(x ** A) = y * inf A
proof
  let A be non empty Subset of REAL, x being Real, y being R_eal such that
A1: x = y and
A2: 0 <= y;
  reconsider Y = x ** A as non empty Subset of REAL by INTEGRA2:7;
  per cases;
  suppose
A3: A is not bounded_below;
    per cases by A2;
    suppose
A4:   y = 0;
      then x ** A = {0} by A1,INTEGRA2:42;
      hence inf(x ** A) = 0 by XXREAL_2:13
        .= y * inf A by A4;
    end;
    suppose
A5:   y > 0;
      then Y is not bounded_below by A1,A3,Lm2;
      hence inf(x ** A) = -infty by XXREAL_2:78
        .= y * -infty by A5,XXREAL_3:def 5
        .= y * inf A by A3,XXREAL_2:78;
    end;
  end;
  suppose
    A is bounded_below;
    then reconsider X = A as non empty bounded_below (real-membered set);
    reconsider u = lower_bound X as Real by XREAL_0:def 1;
    x ** X is bounded_below by A1,A2,INTEGRA2:11;
    then reconsider Y as non empty bounded_below (real-membered set);
    thus inf(x ** A) = lower_bound Y
      .= x * u by A1,A2,INTEGRA2:15
      .= y * inf A by A1,EXTREAL1:13;
  end;
end;

theorem
  for A being Interval for x being Real st 0 <= x for y being Real st y
  = diameter(A) holds x * y = diameter(x ** A)
proof
  let A be Interval;
  let x be Real such that
A1: 0 <= x;
  let y be Real such that
A2: y = diameter(A);
  per cases;
  suppose
A3: A is empty;
A4: now
      assume
      x ** A is non empty;
      then consider y being real number such that
A5:   y in x ** A by MEMBERED:9;
      reconsider y as Real by XREAL_0:def 1;
      ex z being Real st z in A & y = x * z by A5,INTEGRA2:def 2;
      hence contradiction by A3;
    end;
    thus x * y = 0 by A2,A3,MEASURE5:60
      .= diameter(x ** A) by A4,MEASURE5:60;
  end;
  suppose
A6: A is non empty;
    then consider z being real number such that
A7: z in A by MEMBERED:9;
    reconsider z as Real by XREAL_0:def 1;
A8: x * z in x ** A by A7,INTEGRA2:def 2;
    reconsider AA = A as non empty Subset of REAL by A6;
    reconsider u = x as R_eal by XXREAL_0:def 1;
A9: inf(x ** AA) = u * inf AA by A1,Th21;
    reconsider z = x as R_eal by XXREAL_0:def 1;
    thus x * y = z * diameter A by A2,EXTREAL1:13
      .= z * (sup A - inf A) by A6,MEASURE5:def 10
      .= z * sup A - (z * inf A) by XXREAL_3:112
      .= sup(x ** A) - inf(x ** A) by A1,A9,Th20
      .= diameter(x ** A) by A8,MEASURE5:def 10;
  end;
end;

theorem Th23:
  for eps being Real st 0 < eps holds ex n being Element of NAT st
  1 < 2|^n * eps
proof
  let eps be Real;
  assume
A1: 0 < eps;
  consider n being Element of NAT such that
A2: 1 / eps < n by SEQ_4:10;
  take n;
  1/eps < 2|^n by A2,NEWTON:105,XXREAL_0:2;
  then 1/eps * eps < 2|^n * eps by A1,XREAL_1:70;
  hence thesis by A1,XCMPLX_1:88;
end;

theorem Th24:
  for a,b being Real st 0 <= a & 1 < b - a holds ex n being
  Element of NAT st a < n & n < b
proof
  let a,b be Real;
  assume that
A1: 0 <= a and
A2: 1 < b - a;
  a < [\a/]+1 by INT_1:52;
  then reconsider n = [\a/]+1 as Element of NAT by A1,INT_1:16;
  take n;
  thus a < n by INT_1:52;
  [\a/] <= a by INT_1:def 4;
  then
A3: [\a/]+1 <= a+1 by XREAL_1:8;
  1+a < b by A2,XREAL_1:22;
  hence thesis by A3,XXREAL_0:2;
end;

