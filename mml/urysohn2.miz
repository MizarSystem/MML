theorem Th23:
   for eps being Real st 0 < eps holds
   ex n being Element of NAT st 1 < 2|^n * eps
proof
   let eps be Real;
   assume
A1:0 < eps;
   consider n being Element of NAT such that
A2:1 / eps < n by SEQ_4:10;
   n <= 2|^n by NEWTON:105;
   then 1/eps < 2|^n by A2,XXREAL_0:2;
   then A3:1/eps * eps < 2|^n * eps by A1,XREAL_1:70;
   take n;
   thus thesis by A1,A3,XCMPLX_1:88;
end;

theorem Th24:
   for a,b being Real st 0 <= a & 1 < b - a holds
   ex n being Element of NAT st a < n & n < b
proof
   let a,b be Real;
   assume
A1:0 <= a & 1 < b - a;
      assume A2: not ex n being Element of NAT st a < n & n < b;
      defpred W1[Real] means $1 <= a & $1 in NAT;
      defpred W2[Real] means b <= $1 & $1 in NAT;
      consider N being Subset of REAL such that
   A3:for n being Real holds n in N iff W1[n] from REAL_1:sch 1;
      consider M being Subset of REAL such that
   A4:for n being Real holds n in M iff W2[n] from REAL_1:sch 1;
   A5:N \/ M c= REAL;
      N c= NAT
      proof
         for n being set holds n in N implies n in NAT by A3;
         hence thesis by TARSKI:def 3;
      end;
      then reconsider N as Subset of NAT;
      M c= NAT
      proof
         for n being set holds n in M implies n in NAT by A4;
         hence thesis by TARSKI:def 3;
      end;
      then reconsider M as Subset of NAT;
   A6:NAT = N \/ M
      proof
         NAT c= N \/ M
         proof
            reconsider A = N \/ M as Subset of REAL by A5;
A7:         0 in A & for x being Real st x in A holds x + 1 in A
            proof
           A8: 0 in N by A1,A3;
                for x being Real st x in A holds x + 1 in A
               proof
                  let x be Real;
                  assume
               A9:x in A;
                   x + 1 in A
                  proof
                     per cases by A9,XBOOLE_0:def 2;
                     suppose x in N;
                      then reconsider x as Element of NAT;
                        per cases by A2;
                        suppose x + 1 <= a;
                           then x + 1 in N by A3;
                           hence thesis by XBOOLE_0:def 2;
                        end;
                        suppose b <= x + 1;
                           then x + 1 in M by A4;
                           hence thesis by XBOOLE_0:def 2;
                        end;
                     end;
                     suppose x in M;
                        then reconsider x as Element of NAT;
                        per cases by A2;
                        suppose x + 1 <= a;
                           then x + 1 in N by A3;
                           hence thesis by XBOOLE_0:def 2;
                        end;
                        suppose b <= x + 1;
                           then x + 1 in M by A4;
                           hence thesis by XBOOLE_0:def 2;
                        end;
                  end;
                  end;
                  hence thesis;
               end;
               hence thesis by A8,XBOOLE_0:def 2;
            end;
             NAT c= A
            proof
              let x be set; assume x in NAT;
              then reconsider x as Nat by ORDINAL1:def 13;
               x in A by A7,NAT_1:1;
              hence thesis;
            end;
            hence thesis;
         end;
         hence thesis by XBOOLE_0:def 10;
      end;
       ex n being Element of NAT st n in N & not n + 1 in N
      proof
         set n = [\ a /];
      A10:n <= a & a - 1 < n by INT_1:def 4;
      A11:n is Element of NAT
         proof
            per cases;
            suppose a < 1;
               then 0 <= a & a - 1 < 0 by A1,REAL_2:105;
               hence thesis by INT_1:def 4;
            end;
            suppose 1 <= a;
               then A12: a - 1 < [\ a /] & 0 <= a - 1
                 by INT_1:def 4,XREAL_1:50;
             now per cases by A12;
               case n = 0; hence thesis;
               end;
               case
               A13:0 < n;
                  consider k being Element of NAT such that
               A14:n = k or n = - k by INT_1:8;
               now per cases by A14;
                  case n = k; hence thesis;
                  end;
                  case n = -k;
                     hence thesis by A13;
                  end;
                  end;
                  hence thesis;
            end;
            end;
            hence thesis;
         end;
         end;
         A15: a +(- 1) + 1 < n + 1 by A10,XREAL_1:8;
         reconsider n as Element of NAT by A11;
         take n;
         thus thesis by A3,A10,A15;
      end;
      then consider n being Element of NAT such that
   A16:n in N & not n + 1 in N;
   A17:n + 1 in M by A6,A16,XBOOLE_0:def 2;
A18:  n + 1 - n = 1;
      W1[n] & W2[n + 1] by A3,A4,A16,A17;
      then n + b <= a + (n + 1) by XREAL_1:9;
      hence thesis by A1,A18,XREAL_1:23;
end;

