begin

::                    Properties of the Intervals

canceled;

theorem Th2:
  for A being Subset of REAL holds for x being Real st x <> 0 holds
  x" ** (x ** A) = A
proof
  let A be Subset of REAL;
  let x be Real;
  assume
A1: x <> 0;
  thus x" ** (x ** A) c= A
  proof
    let y be set;
    assume
A2: y in x" ** (x ** A);
    then reconsider y as Real;
    consider z being Real such that
A3: z in x ** A and
A4: y = x" * z by A2,INTEGRA2:def 2;
    consider t being Real such that
A5: t in A and
A6: z = x * t by A3,INTEGRA2:def 2;
    y = (x" * x) * t by A4,A6
      .= 1 * t by A1,XCMPLX_0:def 7
      .= t;
    hence thesis by A5;
  end;
  let y be set;
  assume
A7: y in A;
  then reconsider y as Real;
  set t = y / x";
A8: t in x ** A by A7,INTEGRA2:def 2;
  y = x" * t by A1,XCMPLX_1:88,203;
  hence thesis by A8,INTEGRA2:def 2;
end;

theorem Th3:
  for x being Real st x <> 0 holds for A being Subset of REAL holds
  A = REAL implies x ** A = A
proof
  let x be Real;
  assume
A1: x <> 0;
  let A be Subset of REAL;
  assume
A2: A = REAL;
  for y being set st y in A holds y in x ** A
  proof
    let y be set;
    assume
    y in A;
    then reconsider y as Real;
    reconsider z = y/x as Real;
    y = x * z by A1,XCMPLX_1:88;
    hence thesis by A2,INTEGRA2:def 2;
  end;
  then A c= x ** A by TARSKI:def 3;
  hence thesis by A2,XBOOLE_0:def 10;
end;

theorem Th4:
  for A being Subset of REAL st A <> {} holds 0 ** A = {0}
proof
  let A be Subset of REAL;
  assume
A1: A <> {};
A2: {0} c= 0 ** A
  proof
    let y be set;
    consider t being set such that
A3: t in A by A1,XBOOLE_0:def 1;
    reconsider t as Element of A by A3;
    reconsider t as Real by A3;
    assume
    y in {0};
    then y = 0 * t by TARSKI:def 1;
    hence thesis by A3,INTEGRA2:def 2;
  end;
  0 ** A c= {0}
  proof
    let y be set;
    assume
A4: y in 0 ** A;
    then reconsider y as Real;
    ex z being Real st z in A & y = 0 * z by A4,INTEGRA2:def 2;
    hence thesis by TARSKI:def 1;
  end;
  hence thesis by A2,XBOOLE_0:def 10;
end;

theorem Th5:
  for x being Real holds x ** {} = {}
proof
  let x be Real;
  x ** {} c= {}
  proof
    let y be set;
    assume
A1: y in x ** {};
    then reconsider y as Real;
    ex z being Real st z in {} & y = x * z by A1,INTEGRA2:def 2;
    hence thesis;
  end;
  hence thesis;
end;

theorem Th6:
  for a,b being R_eal st a <= b holds a = -infty & b = -infty or a
  = -infty & b in REAL or a = -infty & b = +infty or a in REAL & b in REAL or a
  in REAL & b = +infty or a = +infty & b = +infty
proof
  let a,b be R_eal;
  a in REAL or a in {-infty,+infty} by XBOOLE_0:def 3,XXREAL_0:def 4;
  then
A1: a in REAL or a = -infty or a = +infty by TARSKI:def 2;
  b in REAL or b in {-infty,+infty} by XBOOLE_0:def 3,XXREAL_0:def 4;
  then
A2: b in REAL or b = -infty or b = +infty by TARSKI:def 2;
  assume
  a <= b;
  hence thesis by A1,A2,XXREAL_0:9,12;
end;

canceled;

theorem Th8:
  for A being Interval holds 0 ** A is connected
proof
  let A be Interval;
  per cases;
  suppose
    A = {};
    hence thesis by Th5;
  end;
  suppose
    A <> {};
    then 0 ** A = {0} by Th4;
    then 0 ** A = [.0,0 .] by XXREAL_1:17;
    hence thesis;
  end;
end;

theorem Th9:
  for A being Interval holds for x being Real st x<>0 holds A is
  open_interval implies x ** A is open_interval
proof
  let A be Interval;
  let x be Real;
  assume
A1: x <> 0;
  assume
A2: A is open_interval;
  then consider a,b being R_eal such that
A3: a <= b and
A4: A = ].a,b.[ by MEASURE5:def 5;
  now
    per cases;
    case
A5:   x < 0;
      now
        per cases by A3,Th6;
        case
          a = -infty & b = -infty;
          then
A6:       ].a,b.[ = {};
          then x ** A = {} by A4,Th5;
          hence thesis by A3,A6,MEASURE5:def 5;
        end;
        case
A7:       a = -infty & b in REAL;
          then reconsider s = b as Real;
          set c = +infty;
          x * s is R_eal by XXREAL_0:def 1;
          then consider d being R_eal such that
A8:       d = x * s;
A9:       ].d,c.[ c= x ** A
          proof
            let q be set;
            assume
A10:        q in ].d,c.[;
            then reconsider q as Real;
            set q2 = q / x;
            reconsider q1 = q as R_eal by XXREAL_0:def 1;
A11:        d < q1 by A10,MEASURE5:def 2;
A12:        q2 in A
            proof
              reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
              x * q2 = q by A1,XCMPLX_1:88;
              then
A13:          q3 < b by A5,A8,A11,XREAL_1:67;
              a < q3 by A7,XXREAL_0:12;
              hence thesis by A4,A13,MEASURE5:def 2;
            end;
            q = x * (q / x) by A1,XCMPLX_1:88;
            hence thesis by A12,INTEGRA2:def 2;
          end;
A14:      d <= c by XXREAL_0:4;
          x ** A c= ].d,c.[
          proof
            let q be set;
            assume
A15:        q in x ** A;
            then reconsider q as Real;
            consider z2 being Real such that
A16:        z2 in A and
A17:        q = x * z2 by A15,INTEGRA2:def 2;
            reconsider q as R_eal by XXREAL_0:def 1;
A18:        q < +infty by XXREAL_0:9;
            reconsider z2 as R_eal by XXREAL_0:def 1;
            z2 < b by A4,A16,MEASURE5:def 2;
            then consider r,o being Real such that
A19:        r = z2 & o = b and
            r <= o by A7;
            reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
            r < o by A4,A16,A19,MEASURE5:def 2;
            then o1 < r1 by A5,XREAL_1:71;
            hence thesis by A8,A17,A19,A18,MEASURE5:def 2;
          end;
          then x ** A = ].d,c.[ by A9,XBOOLE_0:def 10;
          hence thesis by A14,MEASURE5:def 5;
        end;
        case
          a = -infty & b = +infty;
          hence thesis by A2,A4,A5,Th3,XXREAL_1:224;
        end;
        case
A20:      a in REAL & b in REAL;
          then reconsider s = a, r = b as Real;
          reconsider d = x * s, g = x * r as R_eal by XXREAL_0:def 1;
A21:      x * r <= x * s by A3,A5,XREAL_1:67;
A22:      ].g,d.[ c= x ** A
          proof
            let q be set;
            assume
A23:        q in ].g,d.[;
            then reconsider q as Real;
            set q2 = q / x;
            reconsider q1 = q as R_eal by XXREAL_0:def 1;
A24:        q1 < d by A23,MEASURE5:def 2;
A25:        g < q1 by A23,MEASURE5:def 2;
A26:        q2 in A
            proof
              reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
              x * q2 = q by A1,XCMPLX_1:88;
              then
A27:          a < q3 by A5,A24,XREAL_1:67;
              q/x < (x * r)/x by A5,A25,XREAL_1:77;
              then q3 < b by A5,XCMPLX_1:90;
              hence thesis by A4,A27,MEASURE5:def 2;
            end;
            q = x * (q / x) by A1,XCMPLX_1:88;
            hence thesis by A26,INTEGRA2:def 2;
          end;
          x ** A c= ].g,d.[
          proof
            let q be set;
            assume
A28:        q in x ** A;
            then reconsider q as Real;
            consider z2 being Real such that
A29:        z2 in A and
A30:        q = x * z2 by A28,INTEGRA2:def 2;
            reconsider z2 as R_eal by XXREAL_0:def 1;
            a < z2 by A4,A29,MEASURE5:def 2;
            then consider 1o,1ra being Real such that
A31:        1o= a & 1ra = z2 and
            1o <= 1ra by A20;
            z2 < b by A4,A29,MEASURE5:def 2;
            then consider 2o,2r being Real such that
A32:        2o= z2 & 2r = b and
            2o <= 2r by A20;
            reconsider 1o1 = x * 1o, 1r1 = x * 1ra, 2o1 = x * 2o, 2r1 = x * 2r
            as R_eal by XXREAL_0:def 1;
            2o< 2r by A4,A29,A32,MEASURE5:def 2;
            then
A33:        2r1 < 2o1 by A5,XREAL_1:71;
            1o < 1ra by A4,A29,A31,MEASURE5:def 2;
            then 1r1 < 1o1 by A5,XREAL_1:71;
            hence thesis by A30,A31,A32,A33,MEASURE5:def 2;
          end;
          then x ** A = ].g,d.[ by A22,XBOOLE_0:def 10;
          hence thesis by A21,MEASURE5:def 5;
        end;
        case
A34:      a in REAL & b = +infty;
          then reconsider s = a as Real;
          set c = -infty;
          reconsider d = x * s as R_eal by XXREAL_0:def 1;
A35:      ].c,d.[ c= x ** A
          proof
            let q be set;
            assume
A36:        q in ].c,d.[;
            then reconsider q as Real;
            set q2 = q / x;
            reconsider q1 = q as R_eal by XXREAL_0:def 1;
A37:        q = x * (q / x) by A1,XCMPLX_1:88;
            q2 in A
            proof
              reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
              q1 <= d by A36,MEASURE5:def 2;
              then x * q2 < x * s by A36,A37,MEASURE5:def 2;
              then
A38:          a < q3 by A5,XREAL_1:67;
              q3 < b by A34,XXREAL_0:9;
              hence thesis by A4,A38,MEASURE5:def 2;
            end;
            hence thesis by A37,INTEGRA2:def 2;
          end;
A39:      c <= d by XXREAL_0:5;
          x ** A c= ].c,d.[
          proof
            let q be set;
            assume
A40:        q in x ** A;
            then reconsider q as Real;
            consider z2 being Real such that
A41:        z2 in A and
A42:        q = x * z2 by A40,INTEGRA2:def 2;
            reconsider z2,q as R_eal by XXREAL_0:def 1;
            a < z2 by A4,A41,MEASURE5:def 2;
            then consider o,r being Real such that
A43:        o = a & r = z2 and
            o <= r by A34;
            reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
A44:        -infty < q by XXREAL_0:12;
            o < r by A4,A41,A43,MEASURE5:def 2;
            then r1 < o1 by A5,XREAL_1:71;
            hence thesis by A42,A43,A44,MEASURE5:def 2;
          end;
          then x ** A = ].c,d.[ by A35,XBOOLE_0:def 10;
          hence thesis by A39,MEASURE5:def 5;
        end;
        case
          a = +infty & b = +infty;
          then
A45:      ].a,b.[ = {};
          then x ** A = {} by A4,Th5;
          hence thesis by A3,A45,MEASURE5:def 5;
        end;
      end;
      hence thesis;
    end;
    case
      x = 0;
      hence thesis by A1;
    end;
    case
A46:  0 < x;
      now
        per cases by A3,Th6;
        case
          a = -infty & b = -infty;
          then
A47:      ].a,b.[ = {};
          then x ** A = {} by A4,Th5;
          hence thesis by A3,A47,MEASURE5:def 5;
        end;
        case
A48:      a = -infty & b in REAL;
          then reconsider s = b as Real;
          set c = -infty;
          reconsider d = x * s as R_eal by XXREAL_0:def 1;
A49:      ].c,d.[ c= x ** A
          proof
            let q be set;
            assume
A50:        q in ].c,d.[;
            then reconsider q as Real;
            set q2 = q / x;
            reconsider q1 = q as R_eal by XXREAL_0:def 1;
A51:        q1 < d by A50,MEASURE5:def 2;
A52:        q2 in A
            proof
              reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
              x * q2 = q by A1,XCMPLX_1:88;
              then
A53:          q3 < b by A46,A51,XREAL_1:66;
              a < q3 by A48,XXREAL_0:12;
              hence thesis by A4,A53,MEASURE5:def 2;
            end;
            q = x * (q / x) by A1,XCMPLX_1:88;
            hence thesis by A52,INTEGRA2:def 2;
          end;
A54:      c <= d by XXREAL_0:5;
          x ** A c= ].c,d.[
          proof
            let q be set;
            assume
A55:        q in x ** A;
            then reconsider q as Real;
            consider z2 being Real such that
A56:        z2 in A and
A57:        q = x * z2 by A55,INTEGRA2:def 2;
            reconsider z2,q as R_eal by XXREAL_0:def 1;
            z2 < b by A4,A56,MEASURE5:def 2;
            then consider r,o being Real such that
A58:        r = z2 & o = b and
            r <= o by A48;
            reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
A59:        -infty < q by XXREAL_0:12;
            r < o by A4,A56,A58,MEASURE5:def 2;
            then r1 < o1 by A46,XREAL_1:70;
            hence thesis by A57,A58,A59,MEASURE5:def 2;
          end;
          then x ** A = ].c,d.[ by A49,XBOOLE_0:def 10;
          hence thesis by A54,MEASURE5:def 5;
        end;
        case
          a = -infty & b = +infty;
          hence thesis by A2,A4,A46,Th3,XXREAL_1:224;
        end;
        case
A60:      a in REAL & b in REAL;
          then reconsider s = a, r = b as Real;
          reconsider d = x * s as R_eal by XXREAL_0:def 1;
          reconsider g = x * r as R_eal by XXREAL_0:def 1;
A61:      x * s <= x * r by A3,A46,XREAL_1:66;
A62:      ].d,g.[ c= x ** A
          proof
            let q be set;
            assume
A63:        q in ].d,g.[;
            then reconsider q as Real;
            set q2 = q / x;
            q is R_eal by XXREAL_0:def 1;
            then consider q1 being R_eal such that
A64:        q1 = q;
A65:        q1 < g by A63,A64,MEASURE5:def 2;
A66:        d < q1 by A63,A64,MEASURE5:def 2;
A67:        q2 in A
            proof
              reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
              x * q2 = q by A1,XCMPLX_1:88;
              then
A68:          a < q3 by A46,A64,A66,XREAL_1:66;
              q/x < (x * r)/x by A46,A64,A65,XREAL_1:76;
              then q3 < b by A46,XCMPLX_1:90;
              hence thesis by A4,A68,MEASURE5:def 2;
            end;
            q = x * (q / x) by A1,XCMPLX_1:88;
            hence thesis by A67,INTEGRA2:def 2;
          end;
          x ** A c= ].d,g.[
          proof
            let q be set;
            assume
A69:        q in x ** A;
            then reconsider q as Real;
            consider z2 being Real such that
A70:        z2 in A and
A71:        q = x * z2 by A69,INTEGRA2:def 2;
            reconsider z2 as R_eal by XXREAL_0:def 1;
            z2 < b by A4,A70,MEASURE5:def 2;
            then consider 2o,2r being Real such that
A72:        2o= z2 & 2r = b and
            2o <= 2r by A60;
            reconsider 2o1 = x * 2o, 2r1 = x * 2r as R_eal by XXREAL_0:def 1;
            2o< 2r by A4,A70,A72,MEASURE5:def 2;
            then
A73:        2o1 < 2r1 by A46,XREAL_1:70;
            a < z2 by A4,A70,MEASURE5:def 2;
            then consider 1o,1ra being Real such that
A74:        1o= a & 1ra = z2 and
            1o <= 1ra by A60;
            reconsider 1o1 = x * 1o, 1r1 = x * 1ra as R_eal by XXREAL_0:def 1;
            1o< 1ra by A4,A70,A74,MEASURE5:def 2;
            then 1o1 < 1r1 by A46,XREAL_1:70;
            hence thesis by A71,A74,A72,A73,MEASURE5:def 2;
          end;
          then x ** A = ].d,g.[ by A62,XBOOLE_0:def 10;
          hence thesis by A61,MEASURE5:def 5;
        end;
        case
A75:      a in REAL & b = +infty;
          then reconsider s = a as Real;
          set c = +infty;
          reconsider d = x * s as R_eal by XXREAL_0:def 1;
A76:      x ** A c= ].d,c.[
          proof
            let q be set;
            assume
A77:        q in x ** A;
            then reconsider q as Real;
            consider z2 being Real such that
A78:        z2 in A and
A79:        q = x * z2 by A77,INTEGRA2:def 2;
            reconsider q as R_eal by XXREAL_0:def 1;
A80:        q < +infty by XXREAL_0:9;
            reconsider z2 as R_eal by XXREAL_0:def 1;
            a < z2 by A4,A78,MEASURE5:def 2;
            then consider o,r being Real such that
A81:        o = a & r = z2 and
            o <= r by A75;
            reconsider o1 = x * o, r1 = x * r as R_eal by XXREAL_0:def 1;
            o < r by A4,A78,A81,MEASURE5:def 2;
            then o1 < r1 by A46,XREAL_1:70;
            hence thesis by A79,A81,A80,MEASURE5:def 2;
          end;
A82:      d <= c by XXREAL_0:4;
          ].d,c.[ c= x ** A
          proof
            let q be set;
            assume
A83:        q in ].d,c.[;
            then reconsider q as Real;
            set q2 = q / x;
            reconsider q1 = q as R_eal by XXREAL_0:def 1;
A84:        q = x * (q / x) by A1,XCMPLX_1:88;
A85:        d < q1 by A83,MEASURE5:def 2;
            q2 in A
            proof
              reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
              a < q3 & q3 < b by A46,A75,A85,A84,XREAL_1:66,XXREAL_0:
              9;
              hence thesis by A4,MEASURE5:def 2;
            end;
            hence thesis by A84,INTEGRA2:def 2;
          end;
          then x ** A = ].d,c.[ by A76,XBOOLE_0:def 10;
          hence thesis by A82,MEASURE5:def 5;
        end;
        case
          a = +infty & b = +infty;
          then
A86:      ].a,b.[ = {};
          then x ** A = {} by A4,Th5;
          hence thesis by A3,A86,MEASURE5:def 5;
        end;
      end;
      hence thesis;
    end;
  end;
  hence thesis;
end;

theorem Th10:
  for A being Interval holds for x being Real st x<>0 holds A is
  closed_interval implies x ** A is closed_interval
proof
  let A be Interval;
  let x be Real;
  assume
A1: x <> 0;
  assume
  A is closed_interval;
  then consider a,b being real number such that
A2: a <= b and
A3: A = [.a,b.] by MEASURE5:def 6;
  reconsider a,b as R_eal by XXREAL_0:def 1;
  now
    per cases;
    case
A4:   x < 0;
      now
        reconsider s = a, r = b as Real by Th6;
        reconsider d = x * s as R_eal by XXREAL_0:def 1;
        reconsider g = x * r as R_eal by XXREAL_0:def 1;
A5:     b in REAL by Th6;
A6:     x * r <= x * s by A2,A4,XREAL_1:67;
A7:     [.g,d.] c= x ** A
        proof
          let q be set;
          assume
A8:       q in [.g,d.];
          then reconsider q as Real by XREAL_0:def 1;
          set q2 = q / x;
          reconsider q1 = q as R_eal by XXREAL_0:def 1;
A9:       g <= q1 by A8,XXREAL_1:1;
