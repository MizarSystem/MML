      x * s is R_eal by XXREAL_0:def 1;
      then consider d being R_eal such that
A9:   d = x * s;
      x * t is R_eal by XXREAL_0:def 1;
      then consider g being R_eal such that
A10:  g = x * t;
A11:  [.d,g.[ c= x ** A
      proof
        let q be set;
        assume
A12:    q in [.d,g.[;
        then reconsider q as Real by A9,XREAL_0:def 1;
        set q2 = q / x;
        reconsider q1 = q as R_eal by XXREAL_0:def 1;
A13:    q1 < g by A12,XXREAL_1:3;
A14:    q2 in A
        proof
          reconsider q3 = q2 as R_eal by XXREAL_0:def 1;
          inf A <= q3 & q3 < sup A & q3 in REAL
          proof
A15:        q3 < sup A
            proof
              consider p,o being Real such that
A16:          p = q1 & o = g and
              p <= o by A10,A13;
              q1 < g by A12,XXREAL_1:3;
              then p/x < o/x by A1,A16,XREAL_1:76;
              hence thesis by A1,A7,A10,A16,XCMPLX_1:90;
            end;
            d <= q1 & x * q2 = q by A1,A12,XCMPLX_1:88,XXREAL_1:3;
            hence thesis by A1,A6,A9,A15,XREAL_1:70;
          end;
          hence thesis by A4,XXREAL_1:3;
        end;
        q = x * (q / x) by A1,XCMPLX_1:88;
        hence thesis by A14,INTEGRA2:def 2;
      end;
      x ** A c= [.d,g.[
      proof
        let q be set;
        assume
A17:    q in x ** A;
        then reconsider q as Real;
        consider z2 being Real such that
A18:    z2 in A and
A19:    q = x * z2 by A17,INTEGRA2:def 2;
        reconsider z2 as R_eal by XXREAL_0:def 1;
        z2 <= sup A by A4,A18,XXREAL_1:3;
        then consider 2o,2r being Real such that
A20:    2o= z2 & 2r = sup A and
        2o <= 2r by A7;
        x * 2o is R_eal & x * 2r is R_eal by XXREAL_0:def 1;
        then consider 2o1,2r1 being R_eal such that
A21:    2o1 = x * 2o & 2r1 = x * 2r;
        2o< 2r by A4,A18,A20,XXREAL_1:3;
        then
A22:    2o1 < 2r1 by A1,A21,XREAL_1:70;
        inf A <= z2 by A4,A18,XXREAL_1:3;
        then consider 1o,1ra being Real such that
A23:    1o= inf A & 1ra = z2 and
A24:    1o <= 1ra by A6;
        x * 1o <= x * 1ra by A1,A24,XREAL_1:66;
        hence thesis by A6,A7,A9,A10,A19,A23,A20,A21,A22,XXREAL_1:3;
      end;
      then x ** A = [.d,g.[ by A11,XBOOLE_0:def 10;
      hence thesis by A2,A9,A10,A8,MEASURE5:def 7,MEASURE6:41,47;
    end;
    hence thesis;
  end;
  A = [.a,sup A.[ by A4;
  then A is right_open_interval by A3,MEASURE5:def 7;
  then x ** A is right_open_interval by A1,Th11;
  hence thesis by A2,A5,MEASURE6:50;
end;

theorem Th19:
  for A being Interval holds for x being Real holds x ** A is
  Interval
proof
  let A be Interval;
  let x be Real;
  per cases;
  suppose
    x = 0;
    hence thesis by Th8;
  end;
  suppose
A1: x <> 0;
    now
      per cases by MEASURE5:def 9;
      case
        A is open_interval;
        then x ** A is open_interval by A1,Th9;
        hence thesis by MEASURE5:def 9;
      end;
      case
        A is closed_interval;
        then x ** A is closed_interval by A1,Th10;
        hence thesis by MEASURE5:def 9;
      end;
      case
A2:     A is right_open_interval;
        per cases by A1;
        case
          x < 0;
          then x ** A is left_open_interval by A2,Th12;
          hence thesis by MEASURE5:def 9;
        end;
        case
          0 < x;
          then x ** A is right_open_interval by A2,Th11;
          hence thesis by MEASURE5:def 9;
        end;
      end;
      case
A3:     A is left_open_interval;
        now
          per cases by A1;
          case
            x < 0;
            then x ** A is right_open_interval by A3,Th14;
            hence thesis by MEASURE5:def 9;
          end;
          case
            0 < x;
            then x ** A is left_open_interval by A3,Th13;
            hence thesis by MEASURE5:def 9;
          end;
        end;
        hence thesis;
      end;
    end;
    hence thesis;
  end;
end;

