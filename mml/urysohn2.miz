 theorems TARSKI, NAT_1, REAL_1, REAL_2, SEQ_4, ABSVALUE, URYSOHN1, XREAL_0,
      SUPINF_1, SUPINF_2, MEASURE5, MEASURE6, INT_1, HEINE, INTEGRA2, XBOOLE_0,
