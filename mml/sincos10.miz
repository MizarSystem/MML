    A3: rr in ].PI/2,PI.] & rr in cos"{0} by A2,XBOOLE_0:def 4;
    A5: cos.rr <> 0 by A3,Lm2,COMPTRIG:29;
        cos.(rr) in {0} by A3,FUNCT_1:def 13;
