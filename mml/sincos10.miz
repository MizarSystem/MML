    st x in dom (sec | [.0,PI/4.]) & y = (sec | [.0,PI/4.]).x
    proof
        assume
    A1: y in [.1,sqrt 2.];
        then reconsider y1=y as Real;
        [.0,PI/4.] c= [.0,PI/2.[
        by Lm9,XXREAL_2:def 12;
