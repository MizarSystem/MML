theorem Th29:
  sin.(PI/4) = 1/sqrt 2 & cos.(PI/4) = 1/sqrt 2
proof
A1: 1 = (sin.(PI/4))^2 + (sin.(PI/4))^2 by SIN_COS:31,78
    .= 2*(sin.(PI/4))^2;
    (sqrt(1/2))^2 = 1/2 by SQUARE_1:def 4;then
A2: sin.(PI/4) = sqrt(1/2) or sin.(PI/4) = -sqrt(1/2) by A1,SQUARE_1:109;
    PI/4 < PI/1 by XREAL_1:78;then
A5: PI/4 in ].0,PI.[;
    sqrt(1/2) > 0 by SQUARE_1:93;then
    -sqrt(1/2) <= -0;
    hence thesis by A2,A5,COMPTRIG:23,SIN_COS:78,SQUARE_1:101;
end;

theorem Th30:
  sin.(-PI/4) = -1/sqrt 2 & cos.(-PI/4) = 1/sqrt 2 &
  sin.(3/4*PI) = 1/sqrt 2 & cos.(3/4*PI) = -1/sqrt 2
proof
A1: sin.(-PI/4) = -1/sqrt 2 by Th29,SIN_COS:33;
A2: cos.(-PI/4) = 1/sqrt 2 by Th29,SIN_COS:33;
A3: sin.(3/4*PI) = sin.(PI+(-PI/4))
    .= -(-1/sqrt 2) by A1,SIN_COS:83
    .= 1/sqrt 2;
    cos.(3/4*PI) = cos.(PI+(-PI/4))
    .= -1/sqrt 2 by A2,SIN_COS:83;
    hence thesis by A3,Th29,SIN_COS:33;
end;

theorem Th31:
  sec.0 = 1 & sec.(PI/4) = sqrt 2 &
  sec.(3/4*PI) = -sqrt 2 & sec.PI = -1
proof
A2: sec.0 = 1/1 by Lm9,Th1,RFUNCT_1:def 8,SIN_COS:33
    .= 1;
A4: sec.(PI/4) = 1/(1/sqrt 2) by Lm9,Th1,Th29,RFUNCT_1:def 8
    .= sqrt 2;
A6: sec.(3/4*PI) = 1/(-1/sqrt 2) by Lm10,Th2,Th30,RFUNCT_1:def 8
    .= -1/(1/sqrt 2) by XCMPLX_1:189
    .= -sqrt 2;
    sec.PI = 1/(-1) by Lm10,Th2,RFUNCT_1:def 8,SIN_COS:81
    .= -1;
    hence thesis by A2,A4,A6;
end;

theorem Th32:
  cosec.(-PI/2) = -1 & cosec.(-PI/4) = -sqrt 2 &
  cosec.(PI/4) = sqrt 2 & cosec.(PI/2) = 1
proof
A2: cosec.(-PI/2) = 1/sin.(-PI/2) by Lm11,Th3,RFUNCT_1:def 8
    .= 1/(-1) by SIN_COS:33,81
    .= -1;
A4: cosec.(-PI/4) = 1/(-1/sqrt 2) by Lm11,Th3,Th30,RFUNCT_1:def 8
    .= -1/(1/sqrt 2) by XCMPLX_1:189
    .= -sqrt 2;
A6: cosec.(PI/4) = 1/(1/sqrt 2) by Lm12,Th4,Th29,RFUNCT_1:def 8
    .= sqrt 2;
    cosec.(PI/2) = 1/1 by Lm12,Th4,RFUNCT_1:def 8,SIN_COS:81
    .= 1;
    hence thesis by A2,A4,A6;
end;

theorem Th33:
  for x be set st x in [.0,PI/4.] holds sec.x in [.1,sqrt 2.]
proof
    let x be set;
    assume
A1: x in [.0,PI/4.];
    x in ].0,PI/4.[ \/ {0,PI/4} by A1,XXREAL_1:128;then
A4: x in ].0,PI/4.[ or x in {0,PI/4} by XBOOLE_0:def 3;
    per cases by A4,TARSKI:def 2;
      suppose
      A5:  x in ].0,PI/4.[;
      A6:  0 in [.0,PI/2.[;
           PI/4 < PI/2 by XREAL_1:78;then
        PI/4 in [.0,PI/2.[;then
      A7:  [.0,PI/4.] c= [.0,PI/2.[ by A6,XXREAL_2:def 12;
      then
      A8:  sec|[.0,PI/4.] is increasing
           by Th17,RFUNCT_2:60;
      A9:  0 in [.0,PI/4.];
      A10: [.0,PI/4.] /\ dom sec = [.0,PI/4.]
           by A7,Th1,XBOOLE_1:1,28;
      A11: ].0,PI/4.[ c= [.0,PI/4.] by XXREAL_1:25;
           x in { s where s is Real: 0 < s & s < PI/4 }
           by A5;
           then ex s be Real st s=x & 0 < s & s < PI/4;then
      A12: 1 < sec.x by A5,A8,A9,A10,A11,Th31,RFUNCT_2:43;
      A13: PI/4 in [.0,PI/4.] /\ dom sec by A10;
           x in { s where s is Real: 0 < s & s < PI/4 }
           by A5;
           then ex s be Real st s=x & 0 < s & s < PI/4;
           then sec.x < sqrt 2 by A5,A8,A10,A11,A13,Th31,RFUNCT_2:43;then
      A14: sec.x in ].1,sqrt 2.[ by A12;
           ].1,sqrt 2.[ c= [.1,sqrt 2.] by XXREAL_1:25;
           hence thesis by A14;
      end;
      suppose x = 0;
        hence thesis by Th31,SQUARE_1:84;
      end;
      suppose x = PI/4;
        hence thesis by Th31,SQUARE_1:84;
      end;
end;

theorem Th34:
  for x be set st x in [.3/4*PI,PI.] holds sec.x in [.-sqrt 2,-1.]
proof
    let x be set;
    assume
A1: x in [.3/4*PI,PI.];
A3: PI/4 < PI/2 by XREAL_1:78;then
A4: PI/4+PI/2 < PI/2+PI/2 by XREAL_1:10;
    then x in ].3/4*PI,PI.[ \/ {3/4*PI,PI} by A1,XXREAL_1:128;then
A5: x in ].3/4*PI,PI.[ or x in {3/4*PI,PI} by XBOOLE_0:def 3;
    per cases by A5,TARSKI:def 2;
      suppose
      A6:  x in ].3/4*PI,PI.[;
           PI/4+PI/4 < PI/2+PI/4 by A3,XREAL_1:10;then
      A7:  3/4*PI in ].PI/2,PI.] by A4;
        PI in ].PI/2,PI.] by COMPTRIG:21;then
      A8:  [.3/4*PI,PI.] c= ].PI/2,PI.] by A7,XXREAL_2:def 12;
      then
      A9:  sec|[.3/4*PI,PI.] is increasing
           by Th18,RFUNCT_2:60;
      A10: 3/4*PI in [.3/4*PI,PI.] by A4;
      A11: [.3/4*PI,PI.] /\ dom sec = [.3/4*PI,PI.]
           by A8,Th2,XBOOLE_1:1,28;
      A12: ].3/4*PI,PI.[ c= [.3/4*PI,PI.] by XXREAL_1:25;
           x in { s where s is Real: 3/4*PI < s & s < PI }
           by A6;
           then ex s be Real st s=x & 3/4*PI < s & s < PI;then
      A13: -sqrt 2 < sec.x by A6,A9,A10,A11,A12,Th31,RFUNCT_2:43;
           PI in {s where s is Real: 3/4*PI <= s & s <= PI} by A4;then
      A14: PI in [.3/4*PI,PI.] /\ dom sec by A11;
           x in { s where s is Real: 3/4*PI < s & s < PI }
           by A6;
           then ex s be Real st s=x & 3/4*PI < s & s < PI;
           then sec.x < -1 by A6,A9,A11,A12,A14,Th31,RFUNCT_2:43;then
      A15: sec.x in ].-sqrt 2,-1.[ by A13;
           ].-sqrt 2,-1.[ c= [.-sqrt 2,-1.] by XXREAL_1:25;
           hence thesis by A15;
      end;
      suppose
      A16: x = 3/4*PI;
           -sqrt 2 < -1 by SQUARE_1:84,XREAL_1:26;
           hence thesis by A16,Th31;
      end;
      suppose
      A17: x = PI;
           -sqrt 2 < -1 by SQUARE_1:84,XREAL_1:26;
           hence thesis by A17,Th31;
      end;
end;

theorem Th35:
  for x be set st x in [.-PI/2,-PI/4.] holds cosec.x in [.-sqrt 2,-1.]
proof
    let x be set;
    assume
A1: x in [.-PI/2,-PI/4.];
    PI/4 < PI/2 by XREAL_1:78;then
A3: -PI/2 < -PI/4 by XREAL_1:26;
    then x in ].-PI/2,-PI/4.[ \/ {-PI/2,-PI/4} by A1,XXREAL_1:128;then
A4: x in ].-PI/2,-PI/4.[ or x in {-PI/2,-PI/4} by XBOOLE_0:def 3;
    per cases by A4,TARSKI:def 2;
      suppose
      A5:  x in ].-PI/2,-PI/4.[;
      A6:  -PI/2 in [.-PI/2,0.[;
        -PI/4 in [.-PI/2,0.[ by A3;then
      A7:  [.-PI/2,-PI/4.] c= [.-PI/2,0.[ by A6,XXREAL_2:def 12;
      then
      A8:  cosec|[.-PI/2,-PI/4.] is decreasing
           by Th19,RFUNCT_2:61;
      A9:  -PI/2 in [.-PI/2,-PI/4.] by A3;
      A10: [.-PI/2,-PI/4.] /\ dom cosec = [.-PI/2,-PI/4.]
           by A7,Th3,XBOOLE_1:1,28;
      A11: ].-PI/2,-PI/4.[ c= [.-PI/2,-PI/4.] by XXREAL_1:25;
           x in { s where s is Real: -PI/2 < s & s < -PI/4 }
           by A5;
           then ex s be Real st s=x & -PI/2 < s & s < -PI/4;then
      A12: -1 > cosec.x by A5,A8,A9,A10,A11,Th32,RFUNCT_2:44;
           -PI/4 in {s where s is Real: -PI/2 <= s & s <= -PI/4} by A3;then
      A13: -PI/4 in [.-PI/2,-PI/4.] /\ dom cosec by A10;
           x in { s where s is Real: -PI/2 < s & s < -PI/4 }
           by A5;
           then ex s be Real st s=x & -PI/2 < s & s < -PI/4;
           then cosec.x > -sqrt 2 by A5,A8,A10,A11,A13,Th32,RFUNCT_2:44;then
      A14: cosec.x in ].-sqrt 2,-1.[ by A12;
           ].-sqrt 2,-1.[ c= [.-sqrt 2,-1.] by XXREAL_1:25;
           hence thesis by A14;
      end;
      suppose
      A15: x = -PI/2;
           -sqrt 2 < -1 by SQUARE_1:84,XREAL_1:26;
           hence thesis by A15,Th32;
      end;
      suppose
      A16: x = -PI/4;
           -sqrt 2 < -1 by SQUARE_1:84,XREAL_1:26;
           hence thesis by A16,Th32;
      end;
end;

theorem Th36:
  for x be set st x in [.PI/4,PI/2.] holds cosec.x in [.1,sqrt 2.]
proof
    let x be set;
    assume
A1: x in [.PI/4,PI/2.];
A3: PI/4 < PI/2 by XREAL_1:78;
    then x in ].PI/4,PI/2.[ \/ {PI/4,PI/2} by A1,XXREAL_1:128;then
A4: x in ].PI/4,PI/2.[ or x in {PI/4,PI/2} by XBOOLE_0:def 3;
    per cases by A4,TARSKI:def 2;
      suppose
      A5:  x in ].PI/4,PI/2.[;
      A6:  PI/4 in ].0,PI/2.] by A3;
        PI/2 in ].0,PI/2.];then
      A7:  [.PI/4,PI/2.] c= ].0,PI/2.] by A6,XXREAL_2:def 12;
      then
      A8:  cosec|[.PI/4,PI/2.] is decreasing
           by Th20,RFUNCT_2:61;
      A9:  PI/4 in [.PI/4,PI/2.] by A3;
      A10: [.PI/4,PI/2.] /\ dom cosec = [.PI/4,PI/2.]
           by A7,Th4,XBOOLE_1:1,28;
      A11: ].PI/4,PI/2.[ c= [.PI/4,PI/2.] by XXREAL_1:25;
           x in { s where s is Real: PI/4 < s & s < PI/2 }
           by A5;
           then ex s be Real st s=x & PI/4 < s & s < PI/2;then
      A12: sqrt 2 > cosec.x by A5,A8,A9,A10,A11,Th32,RFUNCT_2:44;
           PI/2 in {s where s is Real: PI/4 <= s & s <= PI/2} by A3;then
      A13: PI/2 in [.PI/4,PI/2.] /\ dom cosec by A10;
           x in { s where s is Real: PI/4 < s & s < PI/2 }
           by A5;
           then ex s be Real st s=x & PI/4 < s & s < PI/2;
           then cosec.x > 1 by A5,A8,A10,A11,A13,Th32,RFUNCT_2:44;then
      A14: cosec.x in ].1,sqrt 2.[ by A12;
           ].1,sqrt 2.[ c= [.1,sqrt 2.] by XXREAL_1:25;
           hence thesis by A14;
      end;
      suppose x = PI/4;
        hence thesis by Th32,SQUARE_1:84;
      end;
      suppose x = PI/2;
        hence thesis by Th32,SQUARE_1:84;
      end;
end;

Lm13:
