    .= 1/sin.x by A5,A2,RFUNCT_1:def 8;
  PI/2 in [.-PI/2,PI/2.];
  then [.PI/4,PI/2.] c= [.-PI/2,PI/2.] by A1,XXREAL_2:def 12;
