A1: r in ].PI/2,PI.];
A2: dom (sec | ].PI/2,PI.]) = ].PI/2,PI.] by Th2,RELAT_1:91;
    arcsec2 sec.r = arcsec2.((sec|].PI/2,PI.]).r) by A1,FUNCT_1:72
    .= (id ].PI/2,PI.]).r by A1,A2,Th66,FUNCT_1:23
    .= r by A1,FUNCT_1:35;
    hence thesis;
end;

