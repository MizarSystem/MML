A2: dom (cosec | ].0,PI/2.]) = ].0,PI/2.] by Th4,RELAT_1:91;
    arccosec2 cosec.r = arccosec2.((cosec|].0,PI/2.]).r) by A1,FUNCT_1:72
    .= (id ].0,PI/2.]).r by A1,A2,Th68,FUNCT_1:23
