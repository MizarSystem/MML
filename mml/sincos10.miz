  end;
  ].0,PI/2.[ c= ].0,PI.[ by A1,XXREAL_1:46;
  then ].0,PI/2.[ \/ {PI/2} c= ].0,PI.[ by A3,XBOOLE_1:8;
  hence thesis by XXREAL_1:132;
end;

