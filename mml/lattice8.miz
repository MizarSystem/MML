    suppose
A47:  u in A & q = {A} & p = {A};
      then f.(p,q)"\/"f.(q,u) = Bottom L"\/"f.(q,u) by Def5
        .= f.(p,u) by A47,WAYBEL_1:4;
      hence thesis;
    end;
    suppose
A48:  u in A & q = {A} & p = {{A}};
      then reconsider u' = u as Element of A;
      a <= a "\/" d.(x,y) by YELLOW_0:22;
      then
A49:  a"\/"((d.(x,y)"\/"a)"/\"b) = (a"\/"b)"/\"(a"\/" d.(x,y)) by A1,
      YELLOW11:def 3;
      d.(u',y) <= d.(u',x)"\/"d.(x,y) by A3,LATTICE5:def 8;
      then
A50:  d.(u',y)"\/"a <= (d.(u',x)"\/"d.(x,y))"\/"a by YELLOW_5:7;
      a <= a;
      then d.(x,y)"\/"a <= (a"\/"b)"\/"a by A12,YELLOW_3:3;
      then (d.(x,y)"\/"a)"/\"(d.(x,y)"\/"a) <= (d.(x,y)"\/"a)"/\"((a"\/"b)
      "\/"a) by YELLOW_5:6;
      then
A51:  d.(x,y)"\/"a = (d.(x,y)"\/"a)"/\"(d.(x,y)"\/"a) & d.(u',x)"\/"((d.
(x,y)"\/"a )"/\"(d.(x,y)"\/"a)) <= d.(u',x)"\/"((d.(x,y)"\/"a)"/\"((a"\/"b)"\/"
      a)) by WAYBEL_1:3,YELLOW_5:2;
      f.(p,q) = (d.(x,y)"\/"a)"/\"b & f.(q,u) = d.(u',x)"\/"a by A48,
      Def5;
      then f.(p,q) "\/" f.(q,u) = d.(u',x)"\/"((a"\/"b)"/\"(a"\/" d.(x,y)))
      by A49,LATTICE3:14
        .= d.(u',x)"\/"((d.(x,y)"\/"a)"/\"((a"\/"a)"\/"b)) by YELLOW_5:1
        .= d.(u',x)"\/"((d.(x,y)"\/"a)"/\"((a"\/"b)"\/" a)) by LATTICE3:14;
      then (d.(u',x)"\/"d.(x,y))"\/"a <= f.(p,q) "\/" f.(q,u) by A51,
      LATTICE3:14;
      then d.(u',y)"\/"a <= f.(p,q) "\/" f.(q,u) by A50,ORDERS_2:26;
      hence thesis by A48,Def5;
    end;
    suppose
A52:  u in A & q = {{A}} & p = {A};
      then reconsider u' = u as Element of A;
      a <= a "\/" d.(x,y) by YELLOW_0:22;
      then
A53:  a"\/"((d.(x,y)"\/"a)"/\"b) = (a"\/"b)"/\"(a"\/" d.(x,y)) by A1,
      YELLOW11:def 3;
      a <= a;
      then d.(x,y)"\/"a <= (a"\/"b)"\/"a by A12,YELLOW_3:3;
      then (d.(x,y)"\/"a)"/\"(d.(x,y)"\/"a) <= (d.(x,y)"\/"a)"/\"((a"\/"b)
      "\/"a) by YELLOW_5:6;
      then
A54:  d.(x,y)"\/"a = (d.(x,y)"\/"a)"/\"(d.(x,y)"\/"a) & d.(u',y)"\/"((d.
(x,y)"\/"a )"/\"(d.(x,y)"\/"a)) <= d.(u',y)"\/"((d.(x,y)"\/"a)"/\"((a"\/"b)"\/"
      a)) by WAYBEL_1:3,YELLOW_5:2;
      d.(y,x) = d.(x,y) by A2,LATTICE5:def 6;
      then d.(u',x) <= d.(u',y)"\/"d.(x,y) by A3,LATTICE5:def 8;
      then
A55:  d.(u',x)"\/"a <= (d.(u',y)"\/"d.(x,y))"\/"a by YELLOW_5:7;
      f.(p,q) = (d.(x,y)"\/"a)"/\"b & f.(q,u) = d.(u',y)"\/"a by A52,
      Def5;
      then f.(p,q) "\/" f.(q,u) = d.(u',y)"\/"((a"\/"b)"/\"(a"\/" d.(x,y)))
      by A53,LATTICE3:14
        .= d.(u',y)"\/"((d.(x,y)"\/"a)"/\"((a"\/"a)"\/"b)) by YELLOW_5:1
        .= d.(u',y)"\/"((d.(x,y)"\/"a)"/\"((a"\/"b)"\/" a)) by LATTICE3:14;
      then (d.(u',y)"\/"d.(x,y))"\/"a <= f.(p,q) "\/" f.(q,u) by A54,
      LATTICE3:14;
      then d.(u',x)"\/"a <= f.(p,q) "\/" f.(q,u) by A55,ORDERS_2:26;
