A57: for p,q,u being Element of new_set2 A st p in B & q in A & u in A holds
  f.(p,u) <= f.(p,q) "\/" f.(q,u)
  proof
    let p,q,u be Element of new_set2 A;
    assume that
A58: p in B and
A59: q in A & u in A;
    reconsider q' = q, u' = u as Element of A by A59;
    per cases by A58,A59,TARSKI:def 2;
    suppose
A60:  p = {A} & q in A & u in A;
      d.(u',x) <= d.(u',q') "\/" d.(q',x) by A3,LATTICE5:def 8;
      then d.(u',x) <= d.(q',u') "\/" d.(q',x) by A2,LATTICE5:def 6;
      then d.(u',x)"\/"a <= (d.(q',x)"\/"d.(q',u'))"\/"a by WAYBEL_1:3;
      then
A61:  d.(u',x)"\/"a <= (d.(q',x)"\/"a)"\/"d.(q',u') by LATTICE3:14;
A62:  f.(q,u) = d.(q',u') by Def5;
      f.(p,q) = d.(q',x)"\/"a by A60,Def5;
      hence thesis by A60,A62,A61,Def5;
    end;
    suppose
A63:  p = {{A}} & q in A & u in A;
      d.(u',y) <= d.(u',q') "\/" d.(q',y) by A3,LATTICE5:def 8;
      then d.(u',y) <= d.(q',u') "\/" d.(q',y) by A2,LATTICE5:def 6;
      then d.(u',y)"\/"a <= (d.(q',y)"\/"d.(q',u'))"\/"a by WAYBEL_1:3;
      then
A64:  d.(u',y)"\/"a <= (d.(q',y)"\/"a)"\/"d.(q',u') by LATTICE3:14;
A65:  f.(q,u) = d.(q',u') by Def5;
      f.(p,q) = d.(q',y)"\/"a by A63,Def5;
      hence thesis by A63,A65,A64,Def5;
    end;
  end;
A66: for p,q,u being Element of new_set2 A st p in A & q in B & u in A holds
  f.(p,u) <= f.(p,q) "\/" f.(q,u)
  proof
    let p,q,u be Element of new_set2 A;
    assume
A67: p in A & q in B & u in A;
    per cases by A67,TARSKI:def 2;
    suppose
A68:  p in A & u in A & q = {A};
      then reconsider p' = p, u' = u as Element of A;
      d.(p',u') <= d.(p',x) "\/" d.(x,u') by A3,LATTICE5:def 8;
      then
A69:  d.(p',x) "\/" d.(u',x) <= (d.(p',x) "\/" d.(u',x))"\/"a & d.(p',u')
      <= d.(p',x) "\/" d.(u',x) by A2,LATTICE5:def 6,YELLOW_0:22;
      (d.(p',x)"\/"d.(u',x))"\/"a = d.(p',x)"\/"(d.(u',x)"\/"a) by
      LATTICE3:14
        .= d.(p',x)"\/"(d.(u',x)"\/"(a"\/"a)) by YELLOW_5:1
        .= d.(p',x)"\/"((d.(u',x)"\/"a)"\/"a) by LATTICE3:14
        .= (d.(p',x)"\/"a) "\/" (d.(u',x)"\/"a) by LATTICE3:14;
      then
A70:  d.(p',u') <= (d.(p',x)"\/"a) "\/" (d.(u',x)"\/"a) by A69,
      ORDERS_2:26;
      f.(p,q) = d.(p',x)"\/"a & f.(q,u) = d.(u',x)"\/"a by A68,Def5;
      hence thesis by A70,Def5;
    end;
    suppose
A71:  p in A & u in A & q = {{A}};
      then reconsider p' = p, u' = u as Element of A;
      d.(p',u') <= d.(p',y) "\/" d.(y,u') by A3,LATTICE5:def 8;
      then
A72:  d.(p',y) "\/" d.(u',y) <= (d.(p',y) "\/" d.(u',y))"\/"a & d.(p',u')
      <= d.(p',y) "\/" d.(u',y) by A2,LATTICE5:def 6,YELLOW_0:22;
      (d.(p',y)"\/"d.(u',y))"\/"a = d.(p',y)"\/"(d.(u',y)"\/"a) by
      LATTICE3:14
        .= d.(p',y)"\/"(d.(u',y)"\/"(a"\/"a)) by YELLOW_5:1
        .= d.(p',y)"\/"((d.(u',y)"\/"a)"\/"a) by LATTICE3:14
        .= (d.(p',y)"\/"a)"\/"(d.(u',y)"\/"a) by LATTICE3:14;
      then
A73:  d.(p',u') <= (d.(p',y)"\/"a) "\/" (d.(u',y)"\/"a) by A72,
      ORDERS_2:26;
      f.(p,q) = d.(p',y)"\/"a & f.(q,u) = d.(u',y)"\/"a by A71,Def5;
      hence thesis by A73,Def5;
    end;
  end;
A74: for p,q,u being Element of new_set2 A st p in A & q in A & u in A holds
  f.(p,u) <= f.(p,q) "\/" f.(q,u)
  proof
    let p,q,u be Element of new_set2 A;
    assume
    p in A & q in A & u in A;
    then reconsider p' = p, q' = q, u' = u as Element of A;
A75: f.(q,u) = d.(q',u') by Def5;
    f.(p,u) = d.(p',u') & f.(p,q) = d.(p',q') by Def5;
    hence thesis by A3,A75,LATTICE5:def 8;
  end;
  for p,q,u being Element of new_set2 A holds f.(p,u) <= f.(p,q) "\/" f.
  ( q, u )
  proof
    let p,q,u be Element of new_set2 A;
    per cases by XBOOLE_0:def 3;
    suppose
      p in A & q in A & u in A;
      hence thesis by A74;
    end;
    suppose
      p in A & q in A & u in B;
      hence thesis by A4;
    end;
    suppose
      p in A & q in B & u in A;
      hence thesis by A66;
    end;
    suppose
      p in A & q in B & u in B;
      hence thesis by A13;
    end;
    suppose
      p in B & q in A & u in A;
      hence thesis by A57;
    end;
    suppose
      p in B & q in A & u in B;
      hence thesis by A25;
    end;
    suppose
      p in B & q in B & u in A;
      hence thesis by A45;
    end;
    suppose
      p in B & q in B & u in B;
      hence thesis by A35;
    end;
  end;
  hence thesis by LATTICE5:def 8;
end;

