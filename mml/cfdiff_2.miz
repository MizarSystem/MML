Lm1: for seq be Real_Sequence, cseq be Complex_Sequence st seq = cseq holds
seq is convergent iff cseq is convergent
proof
  deffunc F(Element of NAT) = 0;
  consider bseq being Real_Sequence such that
A1: for n holds bseq.n = F(n) from SEQ_1:sch 1;
  let seq be Real_Sequence, cseq be Complex_Sequence such that
A2: seq = cseq;
A3: for n holds Re (cseq.n) = seq.n & Im (cseq.n) = bseq.n
  proof
    let n;
    thus Re (cseq.n) = seq.n by A2,COMPLEX1:def 2;
    Im (cseq.n) = Im (seq.n) by A2;
    then Im (cseq.n) = 0 by COMPLEX1:def 3;
    hence Im (cseq.n) = bseq.n by A1;
  end;
  now
    let n be Nat;
    n in NAT by ORDINAL1:def 13;
    hence bseq.n = 0 by A1;
  end;
  then bseq is constant by VALUED_0:def 18;
  hence thesis by A3,COMSEQ_3:38;
end;

Lm2: for seq be Real_Sequence, cseq be Complex_Sequence, rlim be real number
st seq=cseq & seq is convergent & lim seq = rlim holds lim cseq = rlim
proof
  let seq be Real_Sequence, cseq be Complex_Sequence, rlim be real number such
  that
A1: seq = cseq & seq is convergent & lim seq = rlim;
  reconsider clim = rlim as Complex by XCMPLX_0:def 2;
  cseq is convergent & for p be Real st 0<p ex n st for m st n<=m holds
  abs( cseq.m-clim)<p by A1,Lm1,SEQ_2:def 7;
  hence thesis by COMSEQ_2:def 5;
end;

Lm3: for seq be Real_Sequence, cseq be Complex_Sequence, rlim be Real st seq=
cseq & cseq is convergent & lim cseq = rlim holds lim seq = rlim
proof
  let seq be Real_Sequence, cseq be Complex_Sequence, rlim be Real such that
A1: seq = cseq and
A2: cseq is convergent and
A3: lim cseq = rlim;
  reconsider clim = rlim as Complex by XCMPLX_0:def 2;
A4: now
    let p be real number;
    assume
A5: 0 < p;
    p is Real by XREAL_0:def 1;
    then consider n such that
A6: for m st n<=m holds |.cseq.m - clim.| < p by A2,A3,A5,COMSEQ_2:def 5;
    take n;
    thus for m st n<=m holds |.seq.m - rlim.| < p by A1,A6;
  end;
  seq is convergent by A1,A2,Lm1;
  hence lim seq = rlim by A4,SEQ_2:def 7;
end;

Lm4: for seq be Real_Sequence, cseq be Complex_Sequence st seq=cseq holds seq
is convergent_to_0 iff cseq is convergent_to_0
proof
  let seq be Real_Sequence, cseq be Complex_Sequence such that
A1: seq = cseq;
  thus seq is convergent_to_0 implies cseq is convergent_to_0
  proof
    assume
A2: seq is convergent_to_0;
    then
A3: seq is convergent by FDIFF_1:def 1;
    lim seq = 0 by A2,FDIFF_1:def 1;
    then
A4: lim cseq = 0 by A1,A3,Lm2;
A5: cseq is non-zero by A1,A2,FDIFF_1:def 1;
    cseq is convergent by A1,A3,Lm1;
    hence cseq is convergent_to_0 by A5,A4,CFDIFF_1:def 1;
  end;
  assume
A6: cseq is convergent_to_0;
  then lim cseq = 0 by CFDIFF_1:def 1;
  then
A7: lim seq = 0 by A1,A6,Lm3;
  seq is convergent by A1,A6,Lm1;
  hence seq is convergent_to_0 by A1,A6,A7,FDIFF_1:def 1;
end;

theorem Th1:
  for f be PartFunc of COMPLEX,COMPLEX st f is total holds dom (Re
  f) = COMPLEX & dom (Im f) = COMPLEX
proof
  let f be PartFunc of COMPLEX,COMPLEX;
  assume f is total;
  then dom f = COMPLEX by PARTFUN1:def 4;
  hence thesis by COMSEQ_3:def 3,COMSEQ_3:def 4;
end;

Lm5: for seq, cseq be Complex_Sequence st cseq = <i> (#) seq holds seq is
non-zero iff cseq is non-zero
proof
  let seq, cseq be Complex_Sequence such that
A1: cseq = <i> (#) seq;
  thus seq is non-zero implies cseq is non-zero by A1,COMSEQ_1:41;
A2: <i> (#) seq is non-zero implies seq is non-zero
  proof
    assume
A3: <i> (#) seq is non-zero;
    now
      let n;
      (<i> (#) seq).n <> 0c by A3,COMSEQ_1:4;
      then <i> * seq.n <> 0c by VALUED_1:6;
      hence seq.n <> 0c;
    end;
    hence thesis by COMSEQ_1:4;
  end;
  assume cseq is non-zero;
  hence seq is non-zero by A1,A2;
end;

Lm6: for seq, cseq be Complex_Sequence st cseq = <i> (#) seq holds seq is
convergent_to_0 iff cseq is convergent_to_0
proof
  let seq, cseq be Complex_Sequence such that
A1: cseq = <i> (#) seq;
  thus seq is convergent_to_0 implies cseq is convergent_to_0
  proof
    assume
A2: seq is convergent_to_0;
    then lim seq = 0 by CFDIFF_1:def 1;
    then
A3: lim (<i> (#) seq) = <i> * 0 by A2,COMSEQ_2:18
      .= 0;
    <i> (#) seq is non-zero & <i> (#) seq is convergent by A2,COMSEQ_1:41
,COMSEQ_2:17;
    hence cseq is convergent_to_0 by A1,A3,CFDIFF_1:def 1;
  end;
  assume
A4: cseq is convergent_to_0;
  then
A5: seq is non-zero by A1,Lm5;
  (-<i>) (#) (<i> (#) seq ) is convergent by A1,A4,COMSEQ_2:17;
  then ((-<i>) * <i> ) (#) seq is convergent by CFUNCT_1:31;
  then
A6: seq is convergent by CFUNCT_1:35;
  lim (<i> (#) seq) = 0 by A1,A4,CFDIFF_1:def 1;
  then <i> * lim seq = 0 by A6,COMSEQ_2:18;
  hence seq is convergent_to_0 by A6,A5,CFDIFF_1:def 1;
end;
 
:: Cauchy-Riemann

theorem Th2:
  for f be PartFunc of COMPLEX, COMPLEX, u,v be PartFunc of REAL 2,
  REAL, z0 be Complex, x0, y0 be Real, xy0 be Element of REAL 2 st (for x, y be
Real st x+y*<i> in dom f holds <*x,y*> in dom u & u.<*x,y*> = (Re f).(x+y*<i>))
& (for x,y be Real st x+y*<i> in dom f holds <*x,y*> in dom v & v.<*x,y*> = (Im
  f).(x+y*<i>)) & z0 = x0+y0*<i> & xy0 = <*x0,y0*> & f is_differentiable_in z0
holds u is_partial_differentiable_in xy0,1 & u is_partial_differentiable_in xy0
,2 & v is_partial_differentiable_in xy0,1 & v is_partial_differentiable_in xy0,
2 & Re diff(f,z0) = partdiff(u,xy0,1) & Re diff(f,z0) = partdiff(v,xy0,2) & Im
  diff(f,z0) =-partdiff(u,xy0,2) & Im diff(f,z0) = partdiff(v,xy0,1)
proof
  let f be PartFunc of COMPLEX,COMPLEX, u,v be PartFunc of REAL 2,REAL, z0 be
  Complex, x0,y0 be Real,xy0 be Element of REAL 2 such that
A1: for x,y be Real st x+y*<i> in dom f holds <*x,y*> in dom u & u.<*x,y
  *> = (Re f).(x+y*<i>) and
A2: for x,y be Real st x+y*<i> in dom f holds <*x,y*> in dom v & v.<*x,y
  *> = (Im f).(x+y*<i>) and
A3: z0 = x0+y0*<i> and
A4: xy0 = <*x0,y0*> and
A5: f is_differentiable_in z0;
  deffunc LDF2(Real) = (Im diff(f,z0))* $1;
  consider LD2 be Function of REAL, REAL such that
A6: for x be Real holds LD2.x = LDF2(x) from FUNCT_2:sch 4;
  reconsider LD2 as LINEAR by A6,FDIFF_1:def 4;
  deffunc LDF1(Real) = (Re diff(f,z0)) * $1;
  consider LD1 be Function of REAL, REAL such that
A7: for x be Real holds LD1.x = LDF1(x) from FUNCT_2:sch 4;
A8: for y be Real holds (v*reproj(2,xy0)).y = v.<*x0,y*>
  proof
    let y be Real;
    y in REAL;
    then y in dom (reproj(2,xy0)) by PDIFF_1:def 5;
    hence (v*reproj(2,xy0)).y = v.((reproj(2,xy0)).y) by FUNCT_1:23
      .=v.(Replace(xy0,2,y)) by PDIFF_1:def 5
      .=v.<*x0,y*> by A4,FINSEQ_7:16;
  end;
A9: for y be Real holds (u*reproj(2,xy0)).y=u.<*x0,y*>
  proof
    let y be Real;
    y in REAL;
    then y in dom (reproj(2,xy0)) by PDIFF_1:def 5;
    hence (u*reproj(2,xy0)).y = u.((reproj(2,xy0)).y) by FUNCT_1:23
      .= u.(Replace(xy0,2,y)) by PDIFF_1:def 5
      .= u.<*x0,y*> by A4,FINSEQ_7:16;
  end;
A10: proj(2,2).xy0 = xy0.2 by PDIFF_1:def 1
    .= y0 by A4,FINSEQ_1:61;
  reconsider LD1 as LINEAR by A7,FDIFF_1:def 4;
  deffunc LDF3(Real) = -(Im diff(f,z0)) * $1;
  consider LD3 be Function of REAL,REAL such that
A11: for x be Real holds LD3.x = LDF3(x) from FUNCT_2:sch 4;
  for x be Real holds LD3.x = (-(Im diff(f,z0)))* x
  proof
    let x be Real;
    thus LD3.x = -(Im diff(f,z0)) * x by A11
      .= (-(Im diff(f,z0)))* x;
  end;
  then reconsider LD3 as LINEAR by FDIFF_1:def 4;
  consider N being Neighbourhood of z0 such that
A12: N c= dom f and
A13: ex L be C_LINEAR, R be C_REST st diff(f,z0) = L/.1r & for z be
Complex st z in N holds f/.z-f/.z0 = L/.(z-z0)+R/.(z-z0) by A5,CFDIFF_1:def 7;
  consider L be C_LINEAR, R be C_REST such that
A14: diff(f,z0) = L/.1r & for z be Complex st z in N holds f/.z-f/.z0 = L
  /.(z-z0) +R/.(z-z0) by A13;
  deffunc RF4(Real) = (Im R).($1*<i>);
  consider R4 be Function of REAL, REAL such that
A15: for y be Real holds R4.y =RF4(y) from FUNCT_2:sch 4;
a1:  for z be Complex st z in N holds f/.z-f/.z0 = diff(f,z0)*(z-z0)+R/.(z-z0)
  proof
    let z be Complex;
    assume
A16: z in N;
    consider a0 be Complex such that
A17: for w be Complex holds L/.w = a0*w by CFDIFF_1:def 4;
    L/.(1r*(z-z0)) = a0*1r*(z-z0) by A17
      .= (L/.1r)*(z-z0) by A17;
    hence thesis by A14,A16;
  end;
A18: for x,y be Real st x+y*<i> in N & x0+y0*<i> in N holds
     f.(x+y*<i>)-f.(x0+y0*<i>) =
  diff(f,z0)*((x+y*<i>)-(x0+y0*<i>)) + R/.((x+y*<i>)-(x0+y0*<i>))
  proof
    let x,y be Real;
    assume
b1: x+y*<i> in N & x0+y0*<i> in N;
    then x+y*<i> in dom f & x0+y0*<i> in dom f by A12;
    then f.(x+y*<i>) = f/.(x+y*<i>) & f.(x0+y0*<i>) = f/.(x0+y0*<i>)
    by PARTFUN1:def 8;
    hence thesis by a1,b1,A3;
  end;
b2: dom R = COMPLEX by PARTFUN1:def 4;
  for h be convergent_to_0 Real_Sequence holds (h")(#)(R4/*h) is
  convergent & lim ((h")(#)(R4/*h)) = 0
  proof
    let h be convergent_to_0 Real_Sequence;
    rng h c= COMPLEX by NUMBERS:11,XBOOLE_1:1;
    then reconsider hz0 =h as Complex_Sequence by FUNCT_2:8;
    reconsider hz0 as convergent_to_0 Complex_Sequence by Lm4;
    set hz=<i>(#)hz0;
    reconsider hz as convergent_to_0 Complex_Sequence by Lm6;
    now
A19:  rng hz c= dom R by b2;
      dom R4=REAL by PARTFUN1:def 4;
      then
A20:  rng h c= dom R4;
      let n be Element of NAT;
A21:  Im ((h.n)" ) = 0 & Re ((h.n)") = (h.n)" by COMPLEX1:def 2,def 3;
A22:  hz.n = (h.n)*<i> by VALUED_1:6;
      (h.n)*<i> in COMPLEX by XCMPLX_0:def 2;
      then
A23:  (h.n)*<i> in dom Im R by Th1;
      thus ((h")(#)(R4/*h)).n = (h").n*((R4/*h).n) by SEQ_1:12
        .= (h.n)"*((R4/*h).n) by VALUED_1:10
        .= (h.n)"*(R4.(h.n)) by A20,FUNCT_2:185
        .= (h.n)"* (Im R).((h.n)*<i>) by A15
        .= Re ((h.n)" )* Im (R.((h.n)*<i>)) + Re (R.((h.n)*<i>)) * Im ((h.
      n)") by A23,A21,COMSEQ_3:def 4
        .= Im (((hz.n)/<i>) "* (R.(hz.n))) by A22,COMPLEX1:24
        .= Im ((<i> /(hz.n)) * (R.(hz.n))) by XCMPLX_1:215
        .= Im ( <i>*(hz").n * (R.(hz.n))) by VALUED_1:10
        .= Im ( <i>*((hz").n * (R.(hz.n))))
        .= Re ( <i> ) * Im ((hz").n * (R/.(hz.n))) + Re ((hz").n * (R/.(hz.n
      )))*Im ( <i> ) by COMPLEX1:24
        .= Re ((hz").n * (R/*hz).n ) by A19,COMPLEX1:17,FUNCT_2:186
        .= Re (( (hz")(#)(R/*hz)).n ) by VALUED_1:5;
    end;
    then
A24: (h")(#)(R4/*h) = Re ((hz")(#)(R/*hz)) by COMSEQ_3:def 5;
    (hz")(#)(R/*hz) is convergent & lim ((hz")(#)(R/*hz)) = 0 by CFDIFF_1:def 3
;
    hence thesis by A24,COMPLEX1:12,COMSEQ_3:41;
  end;
  then reconsider R4 as REST by FDIFF_1:def 3;
  deffunc RF2(Real) = (Re R).($1*<i>);
A25: dom R = COMPLEX by PARTFUN1:def 4;
  consider R2 be Function of REAL,REAL such that
A26: for y be Real holds R2.y =RF2(y) from FUNCT_2:sch 4;
  for h be convergent_to_0 Real_Sequence holds (h")(#)(R2/*h) is
  convergent & lim ((h")(#)(R2/*h)) = 0
  proof
    let h be convergent_to_0 Real_Sequence;
    rng h c= COMPLEX by NUMBERS:11,XBOOLE_1:1;
    then reconsider hz0 =h as Complex_Sequence by FUNCT_2:8;
    reconsider hz0 as convergent_to_0 Complex_Sequence by Lm4;
    set hz=<i>(#)hz0;
    reconsider hz as convergent_to_0 Complex_Sequence by Lm6;
A27: (hz")(#)(R/*hz) is convergent by CFDIFF_1:def 3;
    now
      dom R= COMPLEX by PARTFUN1:def 4;
      then
A28:  rng hz c= dom R;
      dom R2=REAL by PARTFUN1:def 4;
      then
A29:  rng h c= dom R2;
      let n be Element of NAT;
A30:  Im ((h.n)" ) = 0 & Re ((h.n)") = (h.n)" by COMPLEX1:def 2,def 3;
A31:  hz.n=(h.n)*<i> by VALUED_1:6;
      dom Re R = COMPLEX by Th1;
      then
A32:  (h.n)*<i> in dom Re R by XCMPLX_0:def 2;
e1:   R.(hz.n) = R/.(hz.n);
      thus ((h")(#)(R2/*h)).n = (h").n*((R2/*h).n) by SEQ_1:12
        .= (h.n)"*((R2/*h).n) by VALUED_1:10
        .= (h.n)"*(R2.(h.n)) by A29,FUNCT_2:185
        .= (h.n)"* (Re R).((h.n)*<i>) by A26
        .= Re ((h.n)" )* Re (R.((h.n)*<i>)) -Im ((h.n)") *Im (R.((h.n)*<i>
      )) by A32,A30,COMSEQ_3:def 3
        .= Re (((hz.n)/<i>) "* (R.(hz.n))) by A31,COMPLEX1:24
        .= Re ((<i> /(hz.n)) * (R.(hz.n))) by XCMPLX_1:215
        .= Re ( <i>*(hz").n * (R.(hz.n))) by VALUED_1:10
        .= Re ( <i>*((hz").n * (R.(hz.n))))
        .= Re <i> * Re ((hz").n * (R.(hz.n))) -
           Im <i> * Im ((hz").n * (R.(hz.n))) by COMPLEX1:24
        .= -Im ((hz").n * (R/*hz).n ) by e1,A28,COMPLEX1:17,FUNCT_2:186
        .= -Im (( (hz")(#)(R/*hz)).n ) by VALUED_1:5
        .= - (Im ((hz")(#)(R/*hz))).n by COMSEQ_3:def 6;
    end;
    then
A33: (h")(#)(R2/*h) = - Im ((hz")(#)(R/*hz)) by SEQ_1:14;
    lim ((hz")(#)(R/*hz)) = 0 by CFDIFF_1:def 3;
    then lim Im ((hz")(#)(R/*hz)) = Im 0 by A27,COMSEQ_3:41;
    then lim ((h")(#)(R2/*h)) = -Im 0 by A33,A27,SEQ_2:24
      .= 0 by COMPLEX1:12;
    hence thesis by A33,A27,SEQ_2:23;
  end;
  then reconsider R2 as REST by FDIFF_1:def 3;
  consider r0 be Real such that
A34: 0<r0 and
A35: {y where y is Complex: |.y-z0.| < r0} c= N by CFDIFF_1:def 5;
  set Ny0 = ].y0-r0,y0+r0.[;
  reconsider Ny0 as Neighbourhood of y0 by A34,RCOMP_1:def 7;
A36: for y be Real st y in Ny0 holds x0+y*<i> in N
  proof
    let y be Real;
    |. (x0+y*<i>) - z0 .| = |.(y-y0)*<i>.| by A3;
    then
A37: |. (x0+y*<i>) - z0 .| = |.(y-y0).|*|.<i>.| by COMPLEX1:151;
    assume y in Ny0;
    then x0+y*<i> is Complex & |. (x0+y*<i>) - z0 .| < r0 by A37,COMPLEX1:135
,RCOMP_1:8,XCMPLX_0:def 2;
    then (x0+y*<i>) in {w where w is Complex : |.w-z0.| < r0};
    hence (x0+y*<i>) in N by A35;
  end;
A38: for x, y be Real holds diff(f,z0)*((x+y*<i>)-(x0+y0*<i>)) = ((Re diff(f
,z0)) * (x-x0)-(Im diff(f,z0))*(y-y0)) + ((Im diff(f,z0)) * (x-x0)+(Re diff(f,
  z0))*(y-y0))*<i>
  proof
    let x, y be Real;
    thus diff(f,z0)*((x+y*<i>)-(x0+y0*<i>)) = ( Re ( diff(f,z0)) + (Im diff(f,
    z0)) * <i> )*((x-x0)+(y-y0)*<i>) by COMPLEX1:29
      .= ((Re diff(f,z0)) * (x-x0)-(Im diff(f,z0))*(y-y0)) + ((Im diff(f,z0)
    ) * (x-x0)+(Re diff(f,z0))*(y-y0))*<i>;
  end;
A39: for x,y be Real holds Re (diff(f,z0)*((x+y*<i>)-(x0+y0*<i>))) = (Re
  diff(f,z0)) * (x-x0)-(Im diff(f,z0))*(y-y0)
  proof
    let x, y be Real;
    thus Re (diff(f,z0)*((x+y*<i>)-(x0+y0*<i>))) = Re (((Re diff(f,z0)) * (x-
x0)-(Im diff(f,z0))*(y-y0)) + ((Im diff(f,z0)) * (x-x0)+(Re diff(f,z0))*(y-y0))
