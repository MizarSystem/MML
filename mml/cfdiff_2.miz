    then consider x being Element of REAL such that
A109: x in Nx0 and
A110: s = (reproj(1,xy0)).x by FUNCT_2:116;
A111: x+y0*<i> in N by A82,A109;
    s=Replace(xy0,1,x) by A110,PDIFF_1:def 5
      .= <* x, y0 *> by A4,FINSEQ_7:15;
    hence s in dom v by A2,A12,A111;
  end;
  then dom (reproj(1,xy0)) = REAL & (reproj(1,xy0)).:Nx0 c= dom v by
FUNCT_2:def 1,TARSKI:def 3;
  then
A112: Nx0 c= dom (v*reproj(1,xy0)) by FUNCT_3:3;
  then
A113: v*reproj(1,xy0) is_differentiable_in proj(1,2).xy0 by A72,A90,
FDIFF_1:def 5;
  now
    let s be set;
    assume s in (reproj(1,xy0)).:Nx0;
    then consider x being Element of REAL such that
A114: x in Nx0 and
A115: s = (reproj(1,xy0)).x by FUNCT_2:116;
A116: x+y0*<i> in N by A82,A114;
    s=Replace(xy0,1,x) by A115,PDIFF_1:def 5
      .= <* x, y0 *> by A4,FINSEQ_7:15;
    hence s in dom u by A1,A12,A116;
  end;
  then dom (reproj(1,xy0)) = REAL & (reproj(1,xy0)).:Nx0 c= dom u by
FUNCT_2:def 1,TARSKI:def 3;
  then
A117: Nx0 c= dom (u*reproj(1,xy0)) by FUNCT_3:3;
  then u*reproj(1,xy0) is_differentiable_in proj(1,2).xy0 by A72,A107,
FDIFF_1:def 5;
  hence thesis by A72,A10,A107,A90,A57,A117,A69,A70,A98,A112,A113,A71,A62,A63,
FDIFF_1:def 6,PDIFF_1:def 11;
end;

Lm7: for x,y be Real,vx, vy be Point of REAL-NS 1 st vx=<*x*> & vy=<*y*> holds
vx+vy=<*x+y*> & vx-vy = <*x-y*>
proof
  let x, y be Real,vx, vy be Point of REAL-NS 1;
  assume
A1: vx=<*x*> & vy=<*y*>;
  reconsider vx1= vx, vy1 =vy as Element of REAL 1 by REAL_NS1:def 4;
  thus vx+vy = vx1+vy1 by REAL_NS1:2
    .= <*x+y*> by A1,RVSUM_1:29;
  thus vx-vy = vx1-vy1 by REAL_NS1:5
    .= <*x-y*> by A1,RVSUM_1:50;
end;

Lm8: for x, y, z, w be Real, vx, vy be Element of REAL 2 st vx = <*x,y*> & vy
= <*z,w*> holds vx+vy = <*x+z,y+w*> & vx-vy = <*x-z,y-w*>
proof
  let x, y, z, w be Real, vx, vy be Element of REAL 2;
  reconsider x1=x,y1=y,z1=z,w1=w as Element of REAL;
  assume
A1: vx = <*x,y*> & vy = <*z,w*>;
  hence vx + vy = <* addreal.(x1,z1), addreal.(y1,w1) *> by FINSEQ_2:89
    .= <*x1+z1,addreal.(y1,w1) *> by BINOP_2:def 9
    .= <*x+z,y+w*> by BINOP_2:def 9;
  thus vx - vy = <* diffreal.(x1,z1), diffreal.(y1,w1) *> by A1,FINSEQ_2:89
    .= <*x1-z1,diffreal.(y1,w1) *> by BINOP_2:def 10
    .= <*x-z,y-w*> by BINOP_2:def 10;
end;

Lm9: for x, y, a be Real, vx be Element of REAL 2 st vx=<*x,y*> holds a*vx=<*
a*x,a*y *>
proof
  let x, y, a be Real, vx be Element of REAL 2;
  reconsider x1=x,y1=y as Element of REAL;
  assume vx = <*x,y*>;
  hence a*vx = <* (a multreal).x1, (a multreal).y1 *> by FINSEQ_2:40
    .= <*a*x1,(a multreal).y1 *> by RVSUM_1:19
    .= <*a*x,a*y*> by RVSUM_1:19;
end;

Lm10: for x,y be Real holds <*x,y*> is Point of REAL-NS 2
proof
  let x, y be Real;
  reconsider x1=x, y1=y as Element of REAL;
  <*x1,y1*> in REAL 2 by FINSEQ_2:121;
  hence thesis by REAL_NS1:def 4;
end;

Lm11: for x, y, z, w be Real, vx, vy be Point of REAL-NS 2 st vx = <*x,y*> &
vy = <*z,w*> holds vx+vy = <*x+z,y+w*> & vx-vy = <*x-z,y-w*>
proof
  let x, y, z, w be Real, vx, vy be Point of REAL-NS 2;
  assume
A1: vx = <*x,y*> & vy = <*z,w*>;
  reconsider vx1 = vx, vy1 = vy as Element of REAL 2 by REAL_NS1:def 4;
  thus vx+vy = vx1+vy1 by REAL_NS1:2
    .= <*x+z,y+w*> by A1,Lm8;
  thus vx-vy = vx1-vy1 by REAL_NS1:5
    .= <*x-z,y-w*> by A1,Lm8;
end;

Lm12: for x,y,a be Real,vx be Point of REAL-NS 2 st vx=<*x,y*> holds a*vx = <*
a*x,a*y *>
proof
  let x, y, a be Real, vx be Point of REAL-NS 2;
  assume
A1: vx = <*x,y*>;
  reconsider vx1 = vx as Element of REAL 2 by REAL_NS1:def 4;
  thus a*vx = a*vx1 by REAL_NS1:3
    .= <*a*x, a*y *> by A1,Lm9;
end;

Lm13: for u be PartFunc of REAL 2, REAL, xy be Element of REAL 2 st xy in dom
u holds (<>*u).xy = <*u.xy*>
proof
  let u be PartFunc of REAL 2, REAL, xy be Element of REAL 2;
  assume xy in dom u;
  hence (<>*u).xy = (proj(1,1) qua Function" ).(u.xy) by FUNCT_1:23
    .= <*u.xy*> by PDIFF_1:1;
end;

Lm14: for u be PartFunc of REAL 2, REAL, x, y be Real st <*x,y*> in dom u
holds (<>*u)/.(<*x,y*>) =<*u/.(<*x,y*>)*>
proof
  set W = proj(1,1)qua Function";
  let u be PartFunc of REAL 2, REAL;
  let x, y be Real;
  assume
A1: <*x,y*> in dom u;
  rng u c= dom W by PDIFF_1:2;
  then dom <>*u = dom u by RELAT_1:46;
  hence (<>*u)/.(<*x,y*>) = (<>*u).(<*x,y*>) by A1,PARTFUN1:def 8
    .= (proj(1,1) qua Function" ).(u.(<*x,y*>)) by A1,FUNCT_1:23
    .= <*u.(<*x,y*>)*> by PDIFF_1:1
    .= <*u/.(<*x,y*>)*> by A1,PARTFUN1:def 8;
end;

Lm15: for x,y be Real,z be Complex,v be Element of REAL-NS 2 st z=x+y*<i> & v=
<* x,y *> holds |.z.|= ||.v.||
proof
  let x, y be Real,z be Complex,v be Element of REAL-NS 2;
  assume that
A1: z=x+y*<i> and
A2: v = <* x,y *>;
A3: Im z = y by A1,COMPLEX1:28;
  reconsider xx = <* x,y *> as Element of REAL 2 by FINSEQ_2:121;
A4: |.xx.| = ||.v.|| & Re z = x by A1,A2,COMPLEX1:28,REAL_NS1:1;
  reconsider xx1 = xx as Point of TOP-REAL 2 by EUCLID:25;
  xx1`2 = y by FINSEQ_1:61;
  then
A5: (sqr xx).2 = y^2 by VALUED_1:11;
  xx1`1 = x by FINSEQ_1:61;
  then len sqr xx = 2 & (sqr xx).1 = x^2 by FINSEQ_1:def 18,VALUED_1:11;
  then sqr xx = <*x^2,y^2*> by A5,FINSEQ_1:61;
  hence thesis by A4,A3,RVSUM_1:107;
end;

Lm16: for x,y be Real,z be Complex st z=x+y*<i> holds |.z.| <= abs(x)+abs(y)
proof
  let x, y be Real,z be Complex;
  assume z = x+y*<i>;
  then Re z = x & Im z = y by COMPLEX1:28;
  hence thesis by COMPLEX1:164;
end;

Lm17: for x, y be Real holds abs(x) <= |.x+y*<i>.| & abs(y) <= |.x+y*<i>.|
proof
  let x, y be Real;
  set z = x+y*<i>;
  Re z = x & Im z = y by COMPLEX1:28;
  hence thesis by COMPLEX1:165;
end;

Lm18: for x,y be Real,v be Element of REAL-NS 2 st v=<* x,y *> holds ||.v.||
<= abs(x)+abs(y)
proof
  let x, y be Real,v be Element of REAL-NS 2;
  reconsider z = x+y*<i> as Complex by XCMPLX_0:def 2;
  assume v = <* x,y *>;
  then |.z.|= ||.v.|| by Lm15;
  hence thesis by Lm16;
end;

theorem Th3:
  for seq be Real_Sequence holds seq is convergent & lim seq = 0
  iff abs seq is convergent & lim abs seq = 0
proof
  let seq be Real_Sequence;
  thus seq is convergent & lim seq = 0 implies abs seq is convergent & lim abs
  seq = 0
  proof
    assume that
A1: seq is convergent and
A2: lim seq = 0;
    lim abs seq = abs(0) by A1,A2,SEQ_4:27
      .= 0 by ABSVALUE:7;
    hence thesis by A1,SEQ_4:26;
  end;
  thus thesis by SEQ_4:28;
end;

theorem Th4:
  for X be RealNormSpace, seq be sequence of X holds seq is
convergent & lim seq = 0.X iff ||. seq .|| is convergent & lim ||. seq .|| = 0
proof
  let X be RealNormSpace, seq be sequence of X;
  thus seq is convergent & lim seq = 0.X implies ||. seq .|| is convergent &
  lim ||. seq .|| = 0
  proof
    assume
A1: seq is convergent & lim seq = 0.X;
    then lim ||. seq .|| = ||.0.X.|| by LOPBAN_1:24;
    hence thesis by A1,LOPBAN_1:24,NORMSP_1:5;
  end;
  assume
A2: ||. seq .|| is convergent & lim ||. seq .|| = 0;
A3: now
    let p be Real;
    assume 0 < p;
    then consider m be Element of NAT such that
A4: for n be Element of NAT st m <= n holds abs(||. seq .||.n - 0) < p
    by A2,SEQ_2:def 7;
    take m;
    hereby
      let n be Element of NAT;
      assume m <= n;
      then abs(||. seq .||.n - 0) < p by A4;
      then
A5:   abs ||. seq.n .|| < p by NORMSP_0:def 4;
      0 <= ||. seq.n .|| by NORMSP_1:8;
      then ||. seq.n .|| < p by A5,ABSVALUE:def 1;
      hence ||.seq.n-0.X.|| < p by RLVECT_1:26;
    end;
  end;
  then seq is convergent by NORMSP_1:def 9;
  hence thesis by A3,NORMSP_1:def 11;
end;

Lm19: for x be Real, vx be Element of REAL-NS 2 st vx = <* x,0 *> holds ||.vx
.|| = abs x
proof
  let x be Real, vx be Element of REAL-NS 2;
  reconsider xx = <* x,0 *> as Element of REAL 2 by FINSEQ_2:121;
  reconsider xx1 = xx as Point of TOP-REAL 2 by EUCLID:25;
  assume vx = <* x,0 *>;
  then
A1: ||.vx.|| = |.xx.| by REAL_NS1:1;
  xx1`2 = 0 by FINSEQ_1:61;
  then
A2: (sqr xx).2 = 0^2 by VALUED_1:11;
  xx1`1 = x by FINSEQ_1:61;
  then len sqr xx = 2 & (sqr xx).1 = x^2 by FINSEQ_1:def 18,VALUED_1:11;
  then sqr xx = <*x^2,0^2*> by A2,FINSEQ_1:61;
  then sqrt Sum sqr xx = sqrt ( x^2+0^2 ) by RVSUM_1:107
    .= sqrt ( x^2+0 * 0) by SQUARE_1:def 3;
  hence ||.vx.|| = abs x by A1,COMPLEX1:158;
end;

Lm20: for x be Real,vx be Element of REAL-NS 2 st vx = <* 0,x*> holds ||.vx.||
= abs x
proof
  let x be Real, vx be Element of REAL-NS 2;
  reconsider xx = <* 0,x *> as Element of REAL 2 by FINSEQ_2:121;
  reconsider xx1 = xx as Point of TOP-REAL 2 by EUCLID:25;
  assume vx = <* 0,x *>;
  then
A1: ||.vx.|| = |.xx.| by REAL_NS1:1;
  xx1`2 = x by FINSEQ_1:61;
  then
A2: (sqr xx).2 = x^2 by VALUED_1:11;
  xx1`1 = 0 by FINSEQ_1:61;
  then len sqr xx = 2 & (sqr xx).1 = 0^2 by FINSEQ_1:def 18,VALUED_1:11;
  then sqr xx = <*0^2,x^2*> by A2,FINSEQ_1:61;
  then sqrt Sum sqr xx = sqrt ( 0^2+x^2 ) by RVSUM_1:107
    .= sqrt ( (0 * 0)+x^2 ) by SQUARE_1:def 3
    .= sqrt ( x^2 );
  hence ||.vx.|| = abs x by A1,COMPLEX1:158;
end;

Lm21: for x be Real,vx be Element of REAL-NS 1 st vx = <* x *> holds ||.vx.||
= abs x
proof
  let x be Real, vx be Element of REAL-NS 1;
  reconsider xx = vx as Element of REAL 1 by REAL_NS1:def 4;
  assume vx = <* x *>;
  then sqrt Sum sqr xx = sqrt Sum <*x^2*> by RVSUM_1:81;
  then
A1: sqrt Sum sqr xx = sqrt (x^2) by RVSUM_1:103;
  ||.vx.|| = |.xx.| by REAL_NS1:1;
  hence ||.vx.|| = abs x by A1,COMPLEX1:158;
end;

Lm22: for v be Element of REAL-NS 1 holds abs( proj(1,1).v ) = ||.v.||
proof
  let v be Element of REAL-NS 1;
  reconsider x=proj(1,1).v as Real;
  reconsider w=v as Element of REAL 1 by REAL_NS1:def 4;
  len w = 1 & w.1 = x by FINSEQ_1:def 18,PDIFF_1:def 1;
  then <* x *> = w by FINSEQ_1:57;
  hence thesis by Lm21;
end;

theorem Th5:
  for u be PartFunc of REAL 2, REAL, x0, y0 be Real, xy0 be Element
  of REAL 2 st xy0 = <*x0,y0*> & <>*u is_differentiable_in xy0 holds u
is_partial_differentiable_in xy0,1 & u is_partial_differentiable_in xy0,2 & <*
  partdiff(u,xy0,1) *> = diff(<>*u,xy0).(<* 1,0 *>) & <* partdiff(u,xy0,2) *> =
  diff(<>*u,xy0).(<* 0,1 *>)
proof
  reconsider ey0 = <* 0, 1 *> as Point of REAL-NS 2 by Lm10;
  reconsider ex0 = <* 1, 0 *> as Point of REAL-NS 2 by Lm10;
  let u be PartFunc of REAL 2,REAL, x0, y0 be Real, xy0 be Element of REAL 2
  such that
A1: xy0 = <*x0,y0*> and
A2: <>*u is_differentiable_in xy0;
  set W = proj(1,1)qua Function";
  consider g be PartFunc of REAL-NS 2,REAL-NS 1, gxy0 be Point of REAL-NS 2
  such that
A3: <>*u=g and
A4: xy0=gxy0 and
A5: g is_differentiable_in gxy0 by A2,PDIFF_1:def 7;
  consider N being Neighbourhood of gxy0 such that
A6: N c= dom g and
A7: ex R be REST of REAL-NS 2,REAL-NS 1 st for xy be Point of REAL-NS 2
  st xy in N holds g/.xy-g/.gxy0 = diff(g,gxy0).(xy-gxy0) + R/.(xy-gxy0) by A5,
NDIFF_1:def 7;
  consider R be REST of REAL-NS 2,REAL-NS 1 such that
A8: for xy be Point of REAL-NS 2 st xy in N holds g/.xy-g/.gxy0 = diff(
  g,gxy0).(xy-gxy0) + R/.(xy-gxy0) by A7;
  consider r0 be Real such that
A9: 0 < r0 and
A10: {xy where xy is Point of REAL-NS 2: ||.xy-gxy0.|| < r0} c= N by
NFCONT_1:def 3;
  consider f be PartFunc of REAL-NS 2,REAL-NS 1, fxy0 be Point of REAL-NS 2
  such that
A11: <>*u=f & xy0=fxy0 and
A12: diff(<>*u,xy0)=diff(f,fxy0) by A2,PDIFF_1:def 8;
  rng u c= dom W by PDIFF_1:2;
  then
A13: dom <>*u = dom u by RELAT_1:46;
A14: for xy be Element of REAL 2, L1, R1 be Real, z be Element of REAL 2 st
xy in N & z=xy-xy0 & <*L1*>=diff(<>*u,xy0).z & <*R1*>=R.z holds u.xy-u.xy0 = L1
  +R1
  proof
    let xy be Element of REAL 2, L1, R1 be Real, z be Element of REAL 2;
    assume that
A15: xy in N and
A16: z=xy-xy0 and
A17: <*L1*>=diff(<>*u,xy0).z and
A18: <*R1*>=R.z;
    reconsider zxy=xy as Point of REAL-NS 2 by REAL_NS1:def 4;
A19: zxy-gxy0 = z by A4,A16,REAL_NS1:5;
    R is total by NDIFF_1:def 5;
    then dom R = the carrier of REAL-NS 2 by PARTFUN1:def 4;
    then R/.(zxy-gxy0) = <*R1*> by A18,A19,PARTFUN1:def 8;
    then
A20: diff(g,gxy0).(zxy-gxy0) + R/.(zxy-gxy0) = <*L1+R1*> by A3,A4,A11,A12,A17
,A19,Lm7;
A21: xy0 in N by A4,NFCONT_1:4;
    then
A22: g/.gxy0 = g.xy0 by A4,A6,PARTFUN1:def 8
      .=(<>*u)/.xy0 by A3,A6,A21,PARTFUN1:def 8;
A23: g/.zxy = g.xy by A6,A15,PARTFUN1:def 8
      .=(<>*u)/.xy by A3,A6,A15,PARTFUN1:def 8;
A24: g/.zxy-g/.gxy0 = diff(g,gxy0).(zxy-gxy0) + R/.(zxy-gxy0) by A8,A15;
A25: (<>*u)/.xy0 = (<>*u).xy0 by A3,A6,A21,PARTFUN1:def 8
      .= <*u.xy0*> by A3,A6,A13,A21,Lm13;
    (<>*u)/.xy = (<>*u).xy by A3,A6,A15,PARTFUN1:def 8
      .= <*u.xy*> by A3,A6,A13,A15,Lm13;
    then g/.zxy-g/.gxy0 = <*u.xy*> -<*u.xy0*> by A23,A22,A25,REAL_NS1:5
      .= <*u.xy*> +<*-u.xy0*> by RVSUM_1:38
      .= <* u.xy-u.xy0 *> by RVSUM_1:29;
    hence u.xy - u.xy0 = (<*L1+R1*>).1 by A24,A20,FINSEQ_1:def 8
      .= L1+R1 by FINSEQ_1:def 8;
  end;
  set Nx0 = ].x0-r0,x0+r0.[;
  reconsider Nx0 as Neighbourhood of x0 by A9,RCOMP_1:def 7;
A26: for x be Real st x in Nx0 holds <*x,y0*> in N
  proof
    let x be Real;
    reconsider xy=<*x,y0*> as Point of REAL-NS 2 by Lm10;
    reconsider xx=<* x-x0,0 *> as Element of REAL 2 by FINSEQ_2:121;
    reconsider xx1 =xx as Point of TOP-REAL 2 by EUCLID:25;
    assume x in Nx0;
    then
A27: |.x-x0.| < r0 by RCOMP_1:8;
    xx1`2 = 0 by FINSEQ_1:61;
    then
A28: abs(xx1`2) = 0 by ABSVALUE:7;
    xy-gxy0 =<*x-x0,y0-y0*> by A1,A4,Lm11
      .=<* x-x0,0 *>;
    then
A29: ||. xy-gxy0 .|| = |.xx.| by REAL_NS1:1;
    abs(xx1`1) = |.x-x0.| & |. xx .| <= abs(xx1`1)+abs(xx1`2) by FINSEQ_1:61
,JGRAPH_1:48;
    then ||. xy-gxy0 .|| < r0 by A27,A28,A29,XXREAL_0:2;
    then xy in {z where z is Point of REAL-NS 2: ||.z-gxy0.|| < r0};
    hence thesis by A10;
  end;
  now
    let s be set;
    assume s in (reproj(1,xy0)).:Nx0;
    then consider x being Element of REAL such that
A30: x in Nx0 and
A31: s = (reproj(1,xy0)).x by FUNCT_2:116;
A32: <* x,y0 *> in N by A26,A30;
    s=Replace(xy0,1,x) by A31,PDIFF_1:def 5
      .= <* x, y0 *> by A1,FINSEQ_7:15;
    hence s in dom u by A3,A6,A13,A32;
  end;
  then dom (reproj(1,xy0)) = REAL & (reproj(1,xy0)).:Nx0 c= dom u by
FUNCT_2:def 1,TARSKI:def 3;
  then
A33: Nx0 c= dom (u*reproj(1,xy0)) by FUNCT_3:3;
  defpred P1[Element of REAL,Element of REAL] means ex vx be Element of REAL 2
  st vx=<*$1,0 *> & <*$2*> = R.vx;
A34: now
    let x be Element of REAL;
    reconsider vx = <*x,0 *> as Element of REAL 2 by FINSEQ_2:121;
    R is total by NDIFF_1:def 5;
    then
A35: dom R = the carrier of REAL-NS 2 by PARTFUN1:def 4;
    vx is Element of REAL-NS 2 by REAL_NS1:def 4;
    then R.vx in the carrier of REAL-NS 1 by A35,PARTFUN1:27;
    then R.vx is Element of REAL 1 by REAL_NS1:def 4;
    then consider y being Element of REAL such that
A36: <*y*> = R.vx by FINSEQ_2:117;
    take y;
    thus P1[x,y] by A36;
  end;
  consider R1 be Function of REAL,REAL such that
A37: for x being Element of REAL holds P1[x, R1.x] from FUNCT_2:sch 10(
  A34);
A38: now
    let x be Real, vx be Element of REAL 2;
    assume
A39: vx=<*x,0 *>;
    ex vx1 be Element of REAL 2 st vx1=<*x,0 *> & <*R1.x*> = R.vx1 by A37;
    hence <*R1.x*> = R.vx by A39;
  end;
  for h be convergent_to_0 Real_Sequence holds (h")(#)(R1/*h) is
  convergent & lim ((h")(#)(R1/*h)) = 0
  proof
    let h be convergent_to_0 Real_Sequence;
    defpred H1[Element of NAT,Element of REAL-NS 2] means $2 = <* h.$1,0 *>;
A40: now
      let n be Element of NAT;
      <* h.n,0 *> in REAL 2 by FINSEQ_2:121;
      then reconsider y = <* h.n, 0 *> as Element of REAL-NS 2 by
REAL_NS1:def 4;
      take y;
      thus H1[n,y];
    end;
    consider h1 be Function of NAT,REAL-NS 2 such that
A41: for n being Element of NAT holds H1[n, h1.n] from FUNCT_2:sch 10(
    A40);
    reconsider h1 as sequence of REAL-NS 2;
    now
      let n be Element of NAT;
A42:  h1.n =<*h.n,0 *> by A41;
      thus ||.h1.|| .n = ||. h1.n .|| by NORMSP_0:def 4
        .= abs (h.n) by A42,Lm19
        .= (abs(h)).n by VALUED_1:18;
    end;
    then
A43: ||.h1.|| = abs h by FUNCT_2:113;
    set g = (||.h1.||")(#)(R/*h1);
    now
      let n be Element of NAT;
      reconsider v = h1.n as Element of REAL 2 by REAL_NS1:def 4;
      dom R1 = REAL by PARTFUN1:def 4;
      then
A44:  rng h c= dom R1;
