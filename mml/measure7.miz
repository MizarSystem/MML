definition
  let F be Function of NAT,bool REAL;
  let G be Interval_Covering of F;
  let H be Function of NAT,[:NAT,NAT:] such that
A1: rng H = [:NAT,NAT:];
  func On(G,H) -> Interval_Covering of union rng F means
