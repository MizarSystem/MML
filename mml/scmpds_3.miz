 Cs1i1.IC SCMPDS & (Cs2i1|dom NPP p).IC SCMPDS =
  Cs2i1. IC SCMPDS by FUNCT_1:72;
A7: Cs2i1 = Following(P2,Cs2i) by EXTPRO_1:4
    .= Exec (CurInstr(P2,Cs2i), Cs2i);
  assume that
A8: I = (a,k1)<=0_goto k2 and
A9: m= IC Cs1i & m+k2 >= 0 & k2 <> 1;
A10: I = CurInstr(P2,Comput(P2,s2,i))
 by A1,AMISTD_5:7,A2;
A11: now
    assume that
A12: Comput(P2,s2,i).DataLoc(Cs2i.a,k1) > 0 and
A13: Comput(P1,s1,i).DataLoc(Cs1i.a,k1) <= 0;
A14: Cs1i1.IC SCMPDS = ICplusConst(Cs1i,k2) by A4,A8,A13,SCMPDS_2:68;
    Cs2i1.IC SCMPDS = succ IC Cs2i by A10,A7,A8,A12,SCMPDS_2:68
      .=ICplusConst(Cs2i,1) by Th20;
    hence contradiction by A6,A3,A9,A5,A14,Th18;
  end;
  now
    assume that
A15: Comput(P1,s1,i).DataLoc(Cs1i.a,k1) > 0 and
A16: Comput(P2,s2,i).DataLoc(Cs2i.a,k1) <= 0;
A17: Cs2i1.IC SCMPDS = ICplusConst(Cs2i,k2) by A10,A7,A8,A16,SCMPDS_2:68;
    Cs1i1.IC SCMPDS = succ IC Cs1i by A4,A8,A15,SCMPDS_2:68
      .=ICplusConst(Cs1i,1) by Th20;
    hence contradiction by A6,A3,A9,A5,A17,Th18;
