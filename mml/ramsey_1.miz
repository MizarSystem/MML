    end;
    suppose
A25:  n+1=2;
      set r=m+1;
      take r;
A26:  n+1-1 >= 2-1 by A5,XREAL_1:11;
      (m+1)+(n+1) >= 2+2 by A4,A5,XREAL_1:9;
      then (m+1)+(n+1)-2 >= 4-2 by XREAL_1:11;
