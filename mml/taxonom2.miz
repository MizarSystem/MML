        end;
      end;
      hence thesis;
    end;
    suppose
      h in union C;
      hence thesis;
    end;
  end;
  take C;
  union C c= H
  proof
    let h be set;
    assume h in union C;
    then consider P be set such that
A136: h in P and
A137: P in C by TARSKI:def 4;
    P c= H by A11,A28,A137;
    hence thesis by A136;
