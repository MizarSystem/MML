              end;
            end;
            suppose
              P1 in {PS1};
              then
A130:         P1 = PS1 by TARSKI:def 1;
              per cases by A126,XBOOLE_0:def 3;
              suppose
                P2 in C;
                hence P1 is_finer_than P2 or P2 is_finer_than P1 by A123,A130;
              end;
              suppose
                P2 in {PS1};
                hence P1 is_finer_than P2 or P2 is_finer_than P1 by A130,
                TARSKI:def 1;
              end;
            end;
          end;
A131:     PS1 is a_partition of X by A40,A42,A120,A112,A113,A114,A121,
          Th18;
          {PS1} c= PARTITIONS(X)
          proof
            let s be set;
