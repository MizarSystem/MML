then A35: for b be set st b in B holds min' misses b & Y <> {} by A7;
set C = B \/ {min'};
A36: C is mutually-disjoint Subset-Family of Y
       & union C <> union B by A1,A17,A35,Th12;
    {min'} c= H
proof
  let s be set such that A37: s in {min'};
