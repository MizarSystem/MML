theorem th1: x #Z 2=x^2 
 proof
   x #Z 2 = x |^ 2 by PREPOWER:46 .= x^2 by NEWTON:100;
   hence thesis;
 end;

theorem th2:x>0 implies x #R (1/2)=sqrt x 
  proof
   assume A1:x>0;then
A2:x #R (1/2)>0 by PREPOWER:95;
   x = x #R ((1/2)*2) by PREPOWER:86,A1
    .=x #R (1/2) #R 2 by PREPOWER:105,A1
    .=(x #R (1/2)) #Q 2 by A2,PREPOWER:88
    .=(x #R (1/2)) to_power 2 by A2,POWER:51
    .=(x #R (1/2))^2 by POWER:53;
   hence x #R (1/2)=sqrt x by A2,SQUARE_1:def 4,A1;
  end;

theorem th5:x>0 implies x #R (-1/2)=1/sqrt x 
 proof
   assume A1:x>0;
   hence x #R (-1/2)=1 / x #R (1/2) by PREPOWER:90
    .=1 / sqrt x by th2,A1;
 end;

Lm6:2*(cos.(x/2))^2=1+cos.x
 proof
A2:(cos(x/2))^2=(1+cos(2*(x/2)))/2 by SIN_COS5:21
     .=(1+cos(x))/2;
   2*(cos.(x/2))^2 = 2*((1+cos(x))/2) by A2,SIN_COS:def 23
    .=1+cos.x by SIN_COS:def 23;
   hence thesis;
 end;

Lm7:2*(sin.(x/2))^2=1-cos.x
 proof
A2:(sin(x/2))^2=(1-cos(2*(x/2)))/2 by SIN_COS5:20
     .=(1-cos(x))/2;
   2*(sin.(x/2))^2=2*((1-cos(x))/2) by A2,SIN_COS:def 21
     .=1-cos.x by SIN_COS:def 23;
   hence thesis;
 end;

::f.x=r*arcsinx
theorem 
 Z c=]. -1,1 .[ & Z c= dom (r(#)arcsin) implies
 r(#)arcsin is_differentiable_on Z & 
 for x st x in Z holds ((r(#)arcsin)`|Z).x = r / sqrt(1-x^2)
 proof
   assume that A1:Z c= ]. -1,1 .[ and A2:Z c= dom (r(#)arcsin);
A4:arcsin is_differentiable_on Z by A1,SIN_COS6:84,FDIFF_1:34;
   for x st x in Z holds ((r(#)arcsin)`|Z).x = r / sqrt(1-x^2)
   proof
     let x;
     assume B1:x in Z; then
A6:  -1 < x & x < 1 by RCOMP_1:47,A1;
     ((r(#)arcsin)`|Z).x = r*diff(arcsin,x) by A4,A2,FDIFF_1:28,B1
       .= r*(1 / sqrt(1-x^2)) by SIN_COS6:84,A6
       .=r / sqrt(1-x^2) by XCMPLX_1:100;
     hence thesis;
   end;
   hence thesis by A4,A2,FDIFF_1:28;
 end;

::f.x=r*arccosx
theorem 
 Z c=]. -1,1 .[ & Z c= dom (r(#)arccos) implies
 r(#)arccos is_differentiable_on Z & 
 for x st x in Z holds ((r(#)arccos)`|Z).x =- r / sqrt(1-x^2)
 proof
   assume that A1:Z c= ]. -1,1 .[ and A2:Z c= dom (r(#)arccos);
A4:arccos is_differentiable_on Z by A1,SIN_COS6:108,FDIFF_1:34;
   for x st x in Z holds ((r(#)arccos)`|Z).x = -r / sqrt(1-x^2)
   proof
     let x;
     assume B1:x in Z;then
A6:  -1 < x & x < 1 by A1,RCOMP_1:47;
     ((r(#)arccos)`|Z).x = r*diff(arccos,x) by A4,A2,FDIFF_1:28,B1
       .= r*(-1 / sqrt(1-x^2)) by SIN_COS6:108,A6
       .=-r*(1 / sqrt(1-x^2))
       .=-r / sqrt(1-x^2) by XCMPLX_1:100;
     hence thesis;
   end;
   hence thesis by A4,A2,FDIFF_1:28;
 end;

theorem th3:
 f is_differentiable_in x & f.x > -1 & f.x < 1 implies
 (arcsin)*f is_differentiable_in x & 
 diff((arcsin)*f,x) = diff(f,x)/sqrt(1-(f.x)^2)
 proof
   assume that A1:f is_differentiable_in x and A2:f.x > -1 & f.x < 1;
   f.x in ]. -1,1 .[ by A2; then
A4:arcsin is_differentiable_in f.x by SIN_COS6:84,FDIFF_1:16;
   diff((arcsin)*f,x) = diff((arcsin),f.x)*diff(f,x) by A1,A4,FDIFF_2:13
   .=diff(f,x)*(1 / sqrt(1-(f.x)^2)) by SIN_COS6:84,A2
  .=diff(f,x)/sqrt(1-(f.x)^2) by XCMPLX_1:100;
  hence thesis by A1,A4,FDIFF_2:13;
 end;

theorem th4:
 f is_differentiable_in x & f.x > -1 & f.x < 1 implies
 (arccos)*f is_differentiable_in x & 
 diff((arccos)*f,x) = -diff(f,x)/sqrt(1-(f.x)^2)
 proof
   assume that A1:f is_differentiable_in x and A2:f.x > -1 & f.x < 1;
   f.x in ]. -1,1 .[ by A2;
   then A4:arccos is_differentiable_in f.x by SIN_COS6:108,FDIFF_1:16;
   diff(arccos*f,x) = diff(arccos,f.x)*diff(f,x) by A1,A4,FDIFF_2:13
   .=(-1 / sqrt(1-(f.x)^2))*diff(f,x) by SIN_COS6:108,A2
   .=-diff(f,x)*(1 / sqrt(1-(f.x)^2))
   .=-diff(f,x)/sqrt(1-(f.x)^2) by XCMPLX_1:100;
   hence thesis by A1,A4,FDIFF_2:13;
 end;

::f.x=ln(arcsin.x)
theorem 
 Z c= dom ((log_ number_e)*(arcsin)) & Z c= ]. -1,1 .[ &
 (for x st x in Z holds arcsin.x>0) implies 
 (log_ number_e)*(arcsin) is_differentiable_on Z & for x st x in Z holds 
 (((log_ number_e)*(arcsin))`|Z).x=1 / (sqrt(1-x^2)*arcsin.x)
 proof
   assume that A1:Z c= dom ((log_ number_e)*arcsin) and
A2:Z c= ]. -1,1 .[ and A3:for x st x in Z holds arcsin.x > 0;
A8:for x st x in Z holds (log_ number_e)*arcsin is_differentiable_in x
  proof
   let x;
   assume B1:x in Z;then 
A5: arcsin is_differentiable_in x by FDIFF_1:16,A2,SIN_COS6:84;
   arcsin.x >0 by A3,B1;
   hence (log_ number_e)*arcsin is_differentiable_in x by TAYLOR_1:20,A5;
  end; then
A:(log_ number_e)*arcsin is_differentiable_on Z by A1,FDIFF_1:16;
  for x st x in Z holds 
    (((log_ number_e)*(arcsin))`|Z).x = 1 / (sqrt(1-x^2)*arcsin.x)
  proof
   let x;
   assume B1:x in Z;
   then A6:-1 < x & x < 1 by A2,RCOMP_1:47;
A5:arcsin is_differentiable_in x by FDIFF_1:16,A2,SIN_COS6:84,B1;
A7:arcsin.x >0 by A3,B1;
   diff((log_ number_e)*arcsin,x)
  =diff(arcsin,x)/arcsin.x by TAYLOR_1:20,A7,A5
  .=(1 / sqrt(1-x^2))/arcsin.x by SIN_COS6:84,A6
  .=1 / (sqrt(1-x^2)*arcsin.x) by XCMPLX_1:79;
   hence thesis by B1,A,FDIFF_1:def 8;
  end;
  hence thesis by A8,A1,FDIFF_1:16;
 end;

