::f.x=(arcsin)|^n
theorem th7:
 Z c= dom (( #Z n)*(arcsin)) & Z c=]. -1,1 .[ implies
 ( #Z n)*(arcsin) is_differentiable_on Z & for x st x in Z holds 
 ((( #Z n)*arcsin)`|Z).x =n*(arcsin.x) #Z (n-1) / sqrt(1-x^2)
 proof
   assume that A1:Z c= dom (( #Z n)*arcsin) and A2: Z c=]. -1,1 .[;
A8:for x st x in Z holds ( #Z n)*arcsin is_differentiable_in x
   proof
    let x;
    assume x in Z;
    then arcsin is_differentiable_in x by FDIFF_1:16,A2,SIN_COS6:84;
    hence thesis by TAYLOR_1:3;
   end;
  then A7:( #Z n)*(arcsin) is_differentiable_on Z by A1,FDIFF_1:16;
  for x st x in Z holds 
    ((( #Z n)*(arcsin))`|Z).x = n*(arcsin.x) #Z (n-1) / sqrt(1-x^2)
  proof
    let x;
    assume B1:x in Z;then
A6:-1 < x & x < 1 by A2,RCOMP_1:47;
A5:arcsin is_differentiable_in x by FDIFF_1:16,A2,SIN_COS6:84,B1;
   ((( #Z n)*arcsin)`|Z).x = diff(( #Z n)*arcsin,x) by B1,FDIFF_1:def 8,A7
  .=(n*((arcsin.x) #Z (n-1))) * diff(arcsin,x) by TAYLOR_1:3,A5
  .=(n*((arcsin.x) #Z (n-1))) *(1 / sqrt(1-x^2)) by SIN_COS6:84,A6
  .=n*(arcsin.x) #Z (n-1) / sqrt(1-x^2) by XCMPLX_1:100;
   hence thesis;
  end;
  hence thesis by A8,A1,FDIFF_1:16;
 end;

