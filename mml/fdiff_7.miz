  arccos is_differentiable_in x by FDIFF_1:16,A2,SIN_COS6:108,B1;
  hence thesis by TAYLOR_1:3;
 end;then
A7:( #Z n)*arccos is_differentiable_on Z by A1,FDIFF_1:16;
  for x st x in Z holds 
    ((( #Z n)*arccos)`|Z).x =-n*(arccos.x) #Z (n-1) / sqrt(1-x^2)
  proof
   let x;
   assume B1:x in Z;
   then A6:-1 < x & x < 1 by A2,RCOMP_1:47;
A5:arccos is_differentiable_in x by FDIFF_1:16,A2,SIN_COS6:108,B1;
   ((( #Z n)*(arccos))`|Z).x 
   =diff(( #Z n)*(arccos),x) by B1,FDIFF_1:def 8,A7
  .=(n*((arccos.x) #Z (n-1))) * diff(arccos,x) by TAYLOR_1:3,A5
  .=(n*((arccos.x) #Z (n-1))) *(-(1 / sqrt(1-x^2))) by SIN_COS6:108,A6
  .=-(n*(arccos.x) #Z (n-1)) *(1/ sqrt(1-x^2))
  .=-n*(arccos.x) #Z (n-1) / sqrt(1-x^2) by XCMPLX_1:100;
