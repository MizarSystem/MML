theorem Th29:
for x,y,z being Element of CAlgebra(A), a,b holds
  x + y = y + x &
  (x + y) + z = x + (y + z) &
  x + (0.CAlgebra(A)) = x &
  (ex t being Element of CAlgebra(A) st x + t=(0.CAlgebra(A))) &
  x * y = y * x &
  (x * y) * z = x * (y * z) &
  x * (1.CAlgebra(A)) = x &
  x * (y + z) = x * y + x * z &
  a * (x * y) = (a * x) * y &
  a * (x + y) = a * x + a * y &
  (a + b) * x = a * x + b * x &
  (a * b) * x = a * (b * x)
proof
   let x,y,z be Element of CAlgebra(A);
   let a,b;
A1:CAlgebra(A)= ComplexAlgebraStr(#
     Funcs(A,COMPLEX),ComplexFuncMult(A),ComplexFuncAdd(A),
     ComplexFuncExtMult(A),(ComplexFuncUnit(A)),(ComplexFuncZero(A))#)
       by Def8;
   set IT = CAlgebra(A);
   reconsider f=x, g=y, h=z as Element of Funcs(A,COMPLEX) by A1;
   thus x+y = (ComplexFuncAdd(A)).(f,g) by A1,RLVECT_1:5
           .= (the add of IT).(g,f) by A1,Th7
           .= y+x by RLVECT_1:5;
   thus (x+y)+z = (the add of IT).(x+y,z) by RLVECT_1:5
               .= (ComplexFuncAdd(A)).((ComplexFuncAdd(A)).(f,g),h)
                   by A1,RLVECT_1:5
               .= (ComplexFuncAdd(A)).(f,(the add of IT).(y,z)) by A1,Th8
               .= (ComplexFuncAdd(A)).(f,y+z) by RLVECT_1:5
               .= x+(y+z) by A1,RLVECT_1:5;
   thus x+(0.CAlgebra(A)) = (ComplexFuncAdd(A)).(f,0.IT) by A1,RLVECT_1:5
                     .= (ComplexFuncAdd(A)).(f,ComplexFuncZero(A))
                          by A1,RLVECT_1:def 2
                     .= (ComplexFuncAdd(A)).(ComplexFuncZero(A),f) by Th7
                     .= x by Th12;
   thus ex t being Element of CAlgebra(A) st x+t=(0.CAlgebra(A))
   proof
     set h = (ComplexFuncExtMult(A)).[-1r,f];
     reconsider t=h as Element of IT by A1;
     take t;
     thus x+t = (ComplexFuncAdd(A)).(f,h) by A1,RLVECT_1:5
             .= ComplexFuncZero(A) by Th13
             .= 0.IT by A1,RLVECT_1:def 2;
   end;
   thus x*y = (ComplexFuncMult(A)).(f,g) by A1,GROUP_1:def 1
           .= (ComplexFuncMult(A)).(g,f) by Th9
           .= y*x by A1,GROUP_1:def 1;
   thus (x*y)*z = (ComplexFuncMult(A)).(x*y,h) by A1,GROUP_1:def 1
              .= (ComplexFuncMult(A)).((ComplexFuncMult(A)).(f,g),h)
                  by A1,GROUP_1:def 1
              .= (ComplexFuncMult(A)).(f,(ComplexFuncMult(A)).(g,h)) by Th10
              .= (ComplexFuncMult(A)).(f,y*z) by A1,GROUP_1:def 1
              .= x*(y*z) by A1,GROUP_1:def 1;
   thus x*(1.CAlgebra(A)) = (ComplexFuncMult(A)).(f,1.IT)
                by A1,GROUP_1:def 1
              .= (ComplexFuncMult(A)).(f,ComplexFuncUnit(A)) by Lm23
                     .= (ComplexFuncMult(A)).(ComplexFuncUnit(A),f) by Th9
                     .= x by Th11;
   thus x*(y+z) = (ComplexFuncMult(A)).(f,y+z) by A1,GROUP_1:def 1
         .= (ComplexFuncMult(A)).(f,(ComplexFuncAdd(A)).(g,h))
             by A1,RLVECT_1:5
         .= (ComplexFuncAdd(A)).
            ((ComplexFuncMult(A)).(f,g),(ComplexFuncMult(A)).(f,h)) by Th17
         .= (ComplexFuncAdd(A)).(x*y,(ComplexFuncMult(A)).(f,h))
                   by A1,GROUP_1:def 1
         .= (ComplexFuncAdd(A)).(x*y,x*z) by A1,GROUP_1:def 1
         .= x*y + x*z by A1,RLVECT_1:5;
   thus a*(x*y) = (ComplexFuncExtMult(A)).[a,x*y] by A1,CLVECT_1:def 1
         .= (ComplexFuncExtMult(A)).[a,(ComplexFuncMult(A)).(f,g)]
            by A1,GROUP_1:def 1
         .= (ComplexFuncMult(A)).((ComplexFuncExtMult(A)).[a,f],g) by Th18
         .= (ComplexFuncMult(A)).(a*x,g) by A1,CLVECT_1:def 1
         .= (a*x)*y by A1,GROUP_1:def 1;
   thus a*(x+y) = (ComplexFuncExtMult(A)).[a,x+y] by A1,CLVECT_1:def 1
               .= (ComplexFuncExtMult(A)).[a,(ComplexFuncAdd(A)).(f,g)]
                by A1,RLVECT_1:5
               .= (ComplexFuncAdd(A)).
               ((ComplexFuncExtMult(A)).[a,f],(ComplexFuncExtMult(A)).[a,g])
                                                                 by Lm2
               .= (ComplexFuncAdd(A)).(a*x,(ComplexFuncExtMult(A)).[a,g])
                                                  by A1,CLVECT_1:def 1
               .= (ComplexFuncAdd(A)).(a*x,a*y) by A1,CLVECT_1:def 1
               .= (a*x) + (a*y) by A1,RLVECT_1:5;
   thus (a+b)*x = (ComplexFuncExtMult(A)).[a+b,f] by A1,CLVECT_1:def 1
               .= (ComplexFuncAdd(A)).
                ((ComplexFuncExtMult(A)).[a,f],(ComplexFuncExtMult(A)).[b,f])
                                                                  by Th16
               .= (ComplexFuncAdd(A)).(a*x,(ComplexFuncExtMult(A)).[b,f])
                                                   by A1,CLVECT_1:def 1
               .= (ComplexFuncAdd(A)).(a*x,b*x) by A1,CLVECT_1:def 1
               .= (a*x) + (b*x) by A1,RLVECT_1:5;
    thus (a*b)*x = (ComplexFuncExtMult(A)).[a*b,f] by A1,CLVECT_1:def 1
                .= (ComplexFuncExtMult(A)).
                              [a,(ComplexFuncExtMult(A)).[b,f]] by Th15
                .= (ComplexFuncExtMult(A)).[a,b*x] by A1,CLVECT_1:def 1
                .= a*(b*x) by A1,CLVECT_1:def 1;
   end;

 definition let IT be non empty ComplexAlgebraStr;
  attr IT is ComplexAlgebra-like means :Def9:
  for x,y,z being Element of IT
    for a,b holds
     x * (1.IT) = x &
     x * (y + z) = x * y + x * z &
     a * (x * y) = (a * x) * y &
     a * (x + y) = a * x + a * y &
     (a + b) * x = a * x + b * x &
     (a * b) * x = a * (b * x);
end;

registration
 cluster strict Abelian add-associative right_zeroed right_complementable
    commutative associative ComplexAlgebra-like (non empty ComplexAlgebraStr);
existence
proof
   consider A;
    for x,y,z being Element of CAlgebra(A)
   for a,b holds
    x+y = y+x &
    (x+y)+z = x+(y+z) &
    x+(0.CAlgebra(A)) = x &
    (ex t being Element of CAlgebra(A) st x+t=(0.CAlgebra(A))) &
    x*y = y*x &
    (x*y)*z = x*(y*z) &
    x*(1.CAlgebra(A)) = x &
    x*(y+z) = x*y + x*z &
    a*(x*y) = (a*x)*y &
    a*(x+y) = a*x + a*y &
    (a+b)*x = a*x + b*x &
    (a*b)*x = a*(b*x) by Th29;
    then CAlgebra(A) is Abelian add-associative right_zeroed
right_complementable
     ComplexAlgebra-like commutative associative
      by Def9,GROUP_1:def 4,def 16,RLVECT_1:def 5,def 6,def 7,def 8;
    hence thesis;
   end;
end;

definition
  mode ComplexAlgebra is Abelian add-associative right_zeroed
    right_complementable commutative associative ComplexAlgebra-like
    (non empty ComplexAlgebraStr);
end;

theorem
 CAlgebra(A) is ComplexAlgebra
proof
    for x,y,z being Element of CAlgebra(A)
   for a,b holds
    x+y = y+x &
    (x+y)+z = x+(y+z) &
    x+(0.CAlgebra(A)) = x &
    (ex t being Element of CAlgebra(A) st x+t=(0.CAlgebra(A))) &
    x*y = y*x &
    (x*y)*z = x*(y*z) &
    x*(1.CAlgebra(A)) = x &
    x*(y+z) = x*y + x*z &
    a*(x*y) = (a*x)*y &
    a*(x+y) = a*x + a*y &
    (a+b)*x = a*x + b*x &
    (a*b)*x = a*(b*x) by Th29;
   hence thesis
    by Def9,GROUP_1:def 4,def 16,RLVECT_1:def 5,def 6,def 7,def 8;
end;

