Lm23: now let A;
  CAlgebra A = ComplexAlgebraStr(#Funcs(A,COMPLEX),ComplexFuncMult(A),
            ComplexFuncAdd(A),ComplexFuncExtMult(A),(ComplexFuncUnit(A)),
            (ComplexFuncZero(A))#) by Def8;
  then reconsider e = ComplexFuncUnit(A) as Element of CAlgebra(A);
