theorem Th27:
for x,y,z being Element of CRing(A) holds
  x+y = y+x &
  (x+y)+z = x+(y+z) &
  x+(0.CRing(A)) = x &
  (ex t being Element of CRing(A) st x+t=(0.CRing(A))) &
  x*y = y*x &
  (x*y)*z = x*(y*z) &
  x*(1.CRing(A)) = x &
  (1.CRing(A))*x = x &
  x*(y+z) = x*y + x*z &
  (y+z)*x = y*x + z*x
proof
   let x,y,z be Element of CRing(A);
A1:CRing(A)=doubleLoopStr(#Funcs(A,COMPLEX),ComplexFuncAdd(A),
      ComplexFuncMult(A),(ComplexFuncUnit(A)),(ComplexFuncZero(A))#) by Def7;
   set IT = CRing(A);
   reconsider f=x, g=y, h=z as Element of Funcs(A,COMPLEX) by A1;
   thus x+y = (ComplexFuncAdd(A)).(f,g) by A1,RLVECT_1:5
          .= (the add of IT).(g,f) by A1,Th7
          .= y+x by RLVECT_1:5;
   thus (x+y)+z = (the add of IT).(x+y,z) by RLVECT_1:5
              .= (ComplexFuncAdd(A)).((ComplexFuncAdd(A)).(f,g),h)
                  by A1,RLVECT_1:5
              .= (ComplexFuncAdd(A)).(f,(the add of IT).(y,z)) by A1,Th8
              .= (ComplexFuncAdd(A)).(f,y+z) by RLVECT_1:5
              .= x+(y+z) by A1,RLVECT_1:5;
   thus x+(0.CRing(A)) = (ComplexFuncAdd(A)).(f,0.IT) by A1,RLVECT_1:5
                     .= (ComplexFuncAdd(A)).(f,ComplexFuncZero(A))
                          by A1,RLVECT_1:def 2
                     .= (ComplexFuncAdd(A)).(ComplexFuncZero(A),f) by Th7
                     .= x by Th12;
   thus ex t being Element of CRing(A) st x+t=(0.CRing(A))
   proof
     set h = (ComplexFuncExtMult(A)).[-1r,f];
     reconsider t=h as Element of IT by A1;
     take t;
     thus x+t = (ComplexFuncAdd(A)).(f,h) by A1,RLVECT_1:5
            .= ComplexFuncZero(A) by Th13
            .= 0.IT by A1,RLVECT_1:def 2;
   end;
   thus x*y = y*x by Lm19;
   thus (x*y)*z = (ComplexFuncMult(A)).(x*y,h) by A1,GROUP_1:def 1
              .= (ComplexFuncMult(A)).((ComplexFuncMult(A)).(f,g),h)
                   by A1,GROUP_1:def 1
              .= (ComplexFuncMult(A)).(f,(ComplexFuncMult(A)).(g,h)) by Th10
              .= (ComplexFuncMult(A)).(f,y*z) by A1,GROUP_1:def 1
              .= x*(y*z) by A1,GROUP_1:def 1;
   thus x*(1.CRing(A)) = (ComplexFuncMult(A)).(f,1.IT) by A1,GROUP_1:def 1
                     .= (ComplexFuncMult(A)).(f,ComplexFuncUnit(A)) by Lm22
                     .= (ComplexFuncMult(A)).(ComplexFuncUnit(A),f) by Th9
                     .= x by Th11;
   hence (1.CRing(A))*x = x by Lm19;
   thus x*(y+z) = (ComplexFuncMult(A)).(f,y+z) by A1,GROUP_1:def 1
              .= (ComplexFuncMult(A)).(f,(ComplexFuncAdd(A)).(g,h))
                  by A1,RLVECT_1:5
              .= (ComplexFuncAdd(A)).
              ((ComplexFuncMult(A)).(f,g),(ComplexFuncMult(A)).(f,h)) by Th17
              .= (ComplexFuncAdd(A)).(x*y,(ComplexFuncMult(A)).(f,h))
                by A1,GROUP_1:def 1
              .= (ComplexFuncAdd(A)).(x*y,x*z) by A1,GROUP_1:def 1
              .= x*y + x*z by A1,RLVECT_1:5;
   hence (y+z)*x = x*y + x*z by Lm19
              .= y*x + x*z by Lm19
              .= y*x + z*x by Lm19;
end;

theorem  CRing(A) is commutative Ring
proof
    for x,y,z being Element of CRing(A) holds
        x+y = y+x &
        (x+y)+z = x+(y+z) &
        x+(0.CRing(A)) = x &
        (ex t being Element of CRing(A) st x+t=(0.CRing(A))) &
        x*y = y*x &
        (x*y)*z = x*(y*z) &
        x*(1.CRing(A)) = x &
        (1.CRing(A))*x = x &
        x*(y+z) = x*y + x*z &
        (y+z)*x = y*x + z*x by Th27;
   hence thesis by GROUP_1:def 4,def 16,RLVECT_1:def 5,def 6,def 7,def 8
,VECTSP_1:def 18;
end;

definition
 struct(doubleLoopStr,CLSStruct) ComplexAlgebraStr (#
           carrier -> set,
           mult,add -> (BinOp of the carrier),
           Mult -> (Function of [:COMPLEX,the carrier:],the carrier),
           unity,Zero -> Element of the carrier #);
end;

registration
