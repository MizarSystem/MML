          .= ((p/3) |^ (1+1))*(p/3) by NEWTON:11
          .= ((((p/3) |^ 1)*(p/3)))*(p/3) by NEWTON:11
          .= ((p/3) |^ 1)*((p/3)*(p/3))
          .= ((p/3) |^ 1)*(p/3)^2 by SQUARE_1:def 3;
         then (p/3) |^ 3 = ((p/3) to_power 1)*(p/3)^2by POWER:47;
          then (-p/3) |^ 3 = -((p/3) |^ 3) by A97,POWER:30;
         then A98: z2 = (-q)*2" +sqrt(q^2/4+((p/3) |^ 3)) by A96,XCMPLX_0:def 9
          .= -(q*2") +sqrt(q^2/4+((p/3) |^ 3));
         then A99:  v |^ 3 = -q/2 +sqrt(q^2/4+((p/3) |^ 3)) by XCMPLX_0:def 9;
                   now per cases by REAL_1:def 5;
                case v >0;
                  then A100: v > 0 & (-q/2 +sqrt(q^2/4+(p/3) |^ 3))> 0
                       by A99,PREPOWER:13;
                     v |^ 3 = -q/2 +sqrt(q^2/4+(p/3) |^ 3) by A98,
XCMPLX_0:def 9;
                   then v = 3 -Root (-q/2 +sqrt(q^2/4+(p/3) |^ 3))
                          by A100,PREPOWER:def 3;hence
                      v = 3-root(-q/2 +sqrt(q^2/4+(p/3) |^ 3))
                           by A100,POWER:def 1;
               end;
               case A101:v=0;
                  then v |^ 3 = 0 by NEWTON:16;
                  then (-q/2 +sqrt(q^2/4+(p/3) |^ 3)) = 0
                         by A98,XCMPLX_0:def 9; hence
                      v = 3-root(-q/2 +sqrt(q^2/4+(p/3) |^ 3))
                         by A101,POWER:6;
               end;
               case v<0;
                  then A102: -v > 0 by REAL_1:66;
                  then A103:  (-v) |^ 3 > 0 by PREPOWER:13;
                      (-v) |^ 3 = (-v) |^ (2+1)
                   .= ((-v) |^ (1+1))*(-v) by NEWTON:11
                   .= ((((-v) |^ 1)*(-v)))*(-v) by NEWTON:11
                   .= ((-v) |^ 1)*((-v)*(-v))
                   .= ((-v) |^ 1)*(-v)^2 by SQUARE_1:def 3
