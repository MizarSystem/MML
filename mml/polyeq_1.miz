                   .= (v |^ 1)*(v*v)
                   .= (v |^ 1)*v^2 by SQUARE_1:def 3;
                  then A105: v |^ 3 = (v to_power 1)*v^2 by POWER:47;
                  then A106: (-v) |^ 3 = -(v |^ 3) by A104,POWER:30;
                     -(v |^ 3)> 0 by A103,A104,A105,POWER:30;
                  then A107:  -(-q/2 +sqrt(q^2/4+(p/3) |^ 3))> 0
                          by A98,XCMPLX_0:def 9;
                     (-v) |^ 3 = -(-q/2 +sqrt(q^2/4+(p/3) |^ 3))
                                by A98,A106,XCMPLX_0:def 9;
                  then A108: (-v) = 3 -Root(-(-q/2 +sqrt(q^2/4+(p/3) |^ 3)))
                              by A102,A107,PREPOWER:def 3;
                   set r = (-q/2 +sqrt(q^2/4+(p/3) |^ 3));
                  A109: (-v) = 3-root((-1)*(r)) by A107,A108,POWER:def 1;
                  A110: 3 = 2*1+1;
                  then A111: (-v) = (3-root(-1))*(3-root r) by A109,POWER:12;
                     (3-root (-1)) = -1 by A110,POWER:9;
                  then -(-v) = (3-root r) by A111;hence
                     v = 3-root(-q/2 +sqrt(q^2/4+(p/3) |^ 3));
               end;
               end; hence
                      y = 3-root(-q/2 +sqrt(q^2/4+(p/3) |^ 3))
                       +3-root(-q/2 -sqrt(q^2/4+(p/3) |^ 3)) by A2,A64;
