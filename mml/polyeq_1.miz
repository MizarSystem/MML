          u |^ 3 = u |^ (2+1);
          then u |^ 3 = (u |^ (1+1))*u by NEWTON:11;
          then u |^ 3 = ((u |^ 1)*u)*u by NEWTON:11;
          then u |^ 3 = (u |^ 1)*(u*u);
          then
A63:      u |^ 3 = (u to_power 1)*u^2 by POWER:46;
          then -(-q/2 -sqrt(q^2/4+(p/3) |^ 3))> 0 by A54,A55,A61,A62,POWER:30;
          then
A64:      3 -Root(-(-q/2 -sqrt(q^2/4+(p/3) |^ 3))) = 3-root(-(-q/2 -sqrt
          (q^2/4+(p/3) |^ 3))) by POWER:def 1;
          set r = (-q/2 -sqrt(q^2/4+(p/3) |^ 3));
          (-u) |^ 3 = -(-q/2 -sqrt(q^2/4+(p/3) |^ 3)) by A54,A55,A62,A63,
POWER:30;
          then (-u) = 3-root((-1)*(r)) by A60,A61,A64,PREPOWER:def 3;
          then
A65:      (-u) = (3-root (-1))*(3-root r) by Lm1,POWER:12;
          (3-root (-1)) = -1 by Lm1,POWER:9;
          hence u = 3-root(-q / 2 -sqrt(q^2/4+(p/3) |^ 3)) by A65;
        end;
      end;
      now
        per cases by A8,A10,Th5;
        case
A66:      z2 = (-e2+sqrt delta(e1,e2,e3))/(2*e1);
          (-p/3) |^ 3 = (-p/3) |^ (2+1);
          then (-p/3) |^ 3 = ((-p/3) |^ (1+1))*(-p/3) by NEWTON:11;
          then (-p/3) |^ 3 = ((((-p/3) |^ 1)*(-p/3)))*(-p/3) by NEWTON:11;
          then (-p/3) |^ 3 = ((-p/3) |^ 1)*((-p/3)*(-p/3));
          then (-p/3) |^ 3 = ((-p/3) to_power 1)*(-p/3)^2by POWER:46;
          then (-p/3) |^ 3 = (-p/3)*(-p/3)^2 by POWER:30;
          then
A67:      (-p/3) |^ 3 = -((p/3)*(p/3)^2);
          (p/3) |^ 3 = (p/3) |^ (2+1);
          then (p/3) |^ 3 = ((p/3) |^ (1+1))*(p/3) by NEWTON:11;
          then (p/3) |^ 3 = ((((p/3) |^ 1)*(p/3)))*(p/3) by NEWTON:11;
          then (p/3) |^ 3 = ((p/3) |^ 1)*((p/3)*(p/3));
          then (p/3) |^ 3 = ((p/3) to_power 1)*(p/3)^2by POWER:46;
          then
A68:      (-p/3) |^ 3 = -((p/3) |^ 3) by A67,POWER:30;
          z2 = (-e2+sqrt(e2^2-4*e1*e3))/(2*e1) by A66,QUIN_1:def 1;
          then z2 = (-q)/2 +sqrt(q^2-4*(-p/3) |^ 3)/sqrt 4 by SQUARE_1:85
,XCMPLX_1:63;
          then
A69:      z2 = (-q)/2 +sqrt((q^2-4*(-p/3) |^ 3)/4) by A7,SQUARE_1:99
            .= (-q)/2 +sqrt(q^2/4-1*(-p/3) |^ 3);
          now
            per cases by XXREAL_0:1;
            case
A70:          v >0;
              then
A71:          (-q/2 +sqrt(q^2/4+(p/3) |^ 3))> 0 by A69,A68,PREPOWER:13;
              then v = 3 -Root (-q/2 +sqrt(q^2/4+(p/3) |^ 3)) by A69,A68,A70,
PREPOWER:def 3;
              hence v = 3-root(-q/2 + sqrt(q^2/4+(p/3) |^ 3)) by A71,
POWER:def 1;
            end;
            case
A72:          v=0;
              then (-q/2 +sqrt(q^2/4+(p/3) |^ 3)) = 0 by A69,A68,NEWTON:16;
              hence v = 3-root(-q/2 +sqrt(q^2/ 4+(p/3) |^ 3)) by A72,POWER:6;
            end;
            case
              v<0;
              then
A73:          -v > 0 by XREAL_1:60;
              then
A74:          (-v) |^ 3 > 0 by PREPOWER:13;
              (-v) |^ 3 = (-v) |^ (2+1);
              then (-v) |^ 3 = ((-v) |^ (1+1))*(-v) by NEWTON:11;
              then (-v) |^ 3 = ((((-v) |^ 1)*(-v)))*(-v) by NEWTON:11;
              then (-v) |^ 3 = ((-v) |^ 1)*((-v)*(-v));
              then (-v) |^ 3 = ((-v) to_power 1)*(-v)^2by POWER:46;
              then (-v) |^ 3 = (-v)*(-v)^2 by POWER:30;
              then
A75:          (-v) |^ 3 = -(v*v^2);
              v |^ 3 = v |^ (2+1);
              then v |^ 3 = (v |^ (1+1))*v by NEWTON:11;
              then v |^ 3 = ((v |^ 1)*v)*v by NEWTON:11;
              then v |^ 3 = (v |^ 1)*(v*v);
              then
A76:          v |^ 3 = (v to_power 1)*v^2 by POWER:46;
              then
A77:          -(-q/2 +sqrt(q^2/4+(p/3) |^ 3))> 0 by A69,A68,A74,A75,POWER:30;
              set r = (-q/2 +sqrt(q^2/4+(p/3) |^ 3));
A78:          (3-root (-1)) = -1 by Lm1,POWER:9;
              v |^ 3 = v*v^2 by A76,POWER:30;
              then (-v) = 3 -Root(-(-q/2 +sqrt(q^2/4+(p/3) |^ 3))) by A69,A68
,A73,A74,A75,PREPOWER:def 3;
              then (-v) = 3-root((-1)*(r)) by A77,POWER:def 1;
              then (-v) = (3-root(-1))*(3-root r) by Lm1,POWER:12;
              hence v = 3-root(-q/2 +sqrt(q^2/4+(p/3) |^ 3)) by A78;
            end;
          end;
          hence thesis by A2,A56;
        end;
        case
A79:      z2 = (-e2-sqrt delta(e1,e2,e3))/(2*e1);
          e2^2 = (z1+z2)^2 by A5,SQUARE_1:61;
          then
A80:      e2^2-(4*e1*e3) = (z1-z2)^2 by A11;
          (-p/3) |^ 3 = (-p/3) |^ (2+1)
            .= ((-p/3) |^ (1+1))*(-p/3) by NEWTON:11
            .= ((((-p/3) |^ 1)*(-p/3)))*(-p/3) by NEWTON:11
            .= ((-p/3) |^ 1)*((-p/3)*(-p/3));
          then (-p/3) |^ 3 = ((-p/3) to_power 1)*(-p/3)^2by POWER:46
            .= (-p/3)*(p/3)^2 by POWER:30;
          then
A81:      (-p/3) |^ 3 = -((p/3)*(p/3)^2);
          (p/3) |^ 3 = (p/3) |^ (2+1) .= ((p/3) |^ (1+1))*(p/3) by NEWTON:11
            .= ((((p/3) |^ 1)*(p/3)))*(p/3) by NEWTON:11
            .= ((p/3) |^ 1)*(p/3)^2;
          then (p/3) |^ 3 = ((p/3) to_power 1)*(p/3)^2by POWER:46;
          then
A82:      (-p/3) |^ 3 = -((p/3) |^ 3) by A81,POWER:30;
          z2 = (-e2-sqrt(e2^2-4*e1*e3))/(2*e1) by A79,QUIN_1:def 1;
          then
A83:      z2 = (-q)/2 +sqrt((q^2-4*(-p/3) |^ 3)/4) by A52,A79,A80,SQUARE_1:82
            .= (-q)/2 +sqrt(q^2/4-1*(-p/3) |^ 3);
          now
            per cases by XXREAL_0:1;
            case
A84:          v >0;
              then
A85:          (-q/2 +sqrt(q^2/4+(p/3) |^ 3))> 0 by A83,A82,PREPOWER:13;
              then v = 3 -Root (-q/2 +sqrt(q^2/4+(p/3) |^ 3)) by A83,A82,A84,
PREPOWER:def 3;
              hence v = 3-root(-q/2 +sqrt (q^2/4+(p/3) |^ 3)) by A85,
POWER:def 1;
            end;
            case
A86:          v=0;
              then v |^ 3 = 0 by NEWTON:16;
              hence
              v = 3-root(-q/2 +sqrt(q ^2/4+(p/3) |^ 3)) by A83,A82,A86,POWER:6;
            end;
            case
              v<0;
              then
A87:          -v > 0 by XREAL_1:60;
              then
A88:          (-v) |^ 3 > 0 by PREPOWER:13;
              set r = (-q/2 +sqrt(q^2/4+(p/3) |^ 3));
              v |^ 3 = v |^ (2+1) .= (v |^ (1+1))*v by NEWTON:11
                .= ((v |^ 1)*v)*v by NEWTON:11
                .= (v |^ 1)*v^2;
              then
A89:          v |^ 3 = (v to_power 1)*v^2 by POWER:46;
              (-v) |^ 3 = (-v) |^ (2+1) .= ((-v) |^ (1+1))*(-v) by NEWTON:11
                .= ((((-v) |^ 1)*(-v)))*(-v) by NEWTON:11
                .= ((-v) |^ 1)*(-v)^2
                .= ((-v) to_power 1)*(-v)^2by POWER:46
                .= (-v)*v^2 by POWER:30;
              then
A90:          (-v) |^ 3 = -(v*v^2);
              then
A91:          -(v |^ 3)> 0 by A88,A89,POWER:30;
A92:          (3-root (-1)) = -1 by Lm1,POWER:9;
              (-v) |^ 3 = -(v |^ 3) by A90,A89,POWER:30;
              then (-v) = 3 -Root(-(-q/2 +sqrt(q^2/4+(p/3) |^ 3))) by A83,A82
,A87,A88,PREPOWER:def 3;
              then (-v) = 3-root((-1)*(r)) by A83,A82,A91,POWER:def 1;
              then (-v) = (3-root(-1))*(3-root r) by Lm1,POWER:12;
              hence v = 3-root(-q/2 +sqrt(q^2/4+(p/3) |^ 3)) by A92;
            end;
          end;
          hence
          y = 3-root(-q/2 +sqrt(q^2/4+(p/3) |^ 3)) +3-root(-q/2 -sqrt(q^2
          /4+(p/3) |^ 3)) by A2,A56;
        end;
      end;
      hence thesis;
    end;
  end;
  hence thesis;
end;

theorem
  b <> 0 & delta(b,c,d) > 0 & Polynom(0,b,c,d,x) = 0 implies x = (-c+
  sqrt delta(b,c,d))/(2*b) or x = (-c-sqrt delta(b,c,d))/(2*b)
proof
  assume
A1: b <> 0 & delta(b,c,d)>0;
  assume Polynom(0,b,c,d,x) = 0;
  then Polynom(b,c,d,x) = 0;
  hence thesis by A1,Th5;
end;

theorem
  a <> 0 & p = c/a & q = d/a & Polynom(a,0,c,d,x) = 0 implies for u,v st
x = u+v & 3*v*u+p = 0 holds x = 3-root(-d/(2*a)+sqrt((d^2/(4*a^2))+(c/(3*a)) |^
3)) +3-root(-d/(2*a)-sqrt((d^2/(4*a^2))+(c/(3*a)) |^ 3)) or x = 3-root(-d/(2*a)
+sqrt((d^2/(4*a^2))+(c/(3*a)) |^ 3)) +3-root(-d/(2*a)+sqrt((d^2/(4*a^2))+(c/(3*
a)) |^ 3)) or x = 3-root(-d/(2*a)-sqrt((d^2/(4*a^2))+(c/(3*a)) |^ 3)) +3-root(-
  d/(2*a)-sqrt((d^2/(4*a^2))+(c/(3*a)) |^ 3))
proof
  assume that
A1: a <> 0 and
A2: p = c/a and
A3: q = d/a;
A4: p/3 = c/(3*a) & -q/2 = -d/(2*a) by A2,A3,XCMPLX_1:79;
  assume Polynom(a,0,c,d,x) = 0;
  then a"*(a*(x |^ 3)+c*x +d) = 0;
  then (a"*a)*(x |^ 3)+a"*(c*x +d)= 0;
  then 1*(x |^ 3)+a"*(c*x +d)= 0 by A1,XCMPLX_0:def 7;
  then (x |^ 3)+((a"*c)*x +a"*d) = 0;
  then (x |^ 3)+((c/a)*x +a"*d) = 0 by XCMPLX_0:def 9;
  then (x |^ 3)+((c/a)*x +d/a) = 0 by XCMPLX_0:def 9;
  then
A5: Polynom(1,0,p,q,x) = 0 by A2,A3;
  q^2/4 = d^2/a^2/4 by A3,XCMPLX_1:77;
  then
A6: q^2/4 = d^2/(4*a^2) by XCMPLX_1:79;
  let u,v;
  assume x = u+v & 3*v*u+p = 0;
  hence thesis by A5,A4,A6,Th19;
end;

theorem
  a <> 0 & delta(a,b,c) >= 0 & Polynom(a,b,c,0,x) = 0 implies x = 0 or x
  = (-b+sqrt delta(a,b,c))/(2*a) or x = (-b-sqrt delta(a,b,c))/(2*a)
proof
  assume
A1: a <> 0 & delta(a,b,c)>= 0;
  x |^ 3 = x |^ (2+1);
  then x |^ 3 = (x |^ (1+1))*x by NEWTON:11;
  then
A2: x |^ 3 = ((x |^ 1)*x)*x by NEWTON:11;
  x |^ 1 = x to_power 1 by POWER:46;
  then
A3: x |^ 3 = x^2*x by A2,POWER:30;
  assume Polynom(a,b,c,0,x) = 0;
  then (a*x^2 +b*x+c)*x = 0 by A3;
  then x = 0 or Polynom(a,b,c,x) = 0 by XCMPLX_1:6;
  hence thesis by A1,Th5;
end;

theorem
  a <> 0 & c/a < 0 & Polynom(a,0,c,0,x) = 0 implies x = 0 or x = sqrt -c
  /a or x = -sqrt(-c/a)
proof
  assume that
A1: a <> 0 and
A2: c/a < 0;
  x |^ 3 = x |^ (2+1);
  then x |^ 3 = (x |^ (1+1))*x by NEWTON:11;
  then
A3: x |^ 3 = ((x |^ 1)*x)*x by NEWTON:11;
  x |^ 1 = x to_power 1 by POWER:46;
  then
A4: x |^ 3 = x^2*x by A3,POWER:30;
  assume Polynom(a,0,c,0,x) = 0;
  then (a*x^2+c)*x = 0 by A4;
  then
A5: x = 0 or (a*x^2+c) = 0 by XCMPLX_1:6;
  now
    per cases by XXREAL_0:1;
    case
A6:   x > 0;
      x |^ 2 = (x |^ (1+1));
      then x |^ 2 = ((x |^ 1)*x) by NEWTON:11;
      then x |^ 2 = ((x to_power 1)*x) by POWER:46;
      then
A7:   x |^ 2 = x*x by POWER:30;
A8:   (-c/a) > 0 by A2,XREAL_1:60;
      x^2 = (-c)/a by A1,A5,A6,XCMPLX_1:90;
      then x^2 = ((-c)*a") by XCMPLX_0:def 9;
      then x^2 = -(c*a");
      then x^2 = -c/a by XCMPLX_0:def 9;
      then x = 2 -Root (-c/a) by A6,A8,A7,PREPOWER:def 3;
      hence thesis by A8,PREPOWER:41;
    end;
    case
A9:   x < 0;
      (-x) |^ 2 = ((-x) |^ (1+1));
      then (-x) |^ 2 = (((-x) |^ 1)*(-x)) by NEWTON:11;
      then (-x) |^ 2 = (((-x) to_power 1)*(-x)) by POWER:46;
      then
A10:  (-x) |^ 2 = (-x)*(-x) by POWER:30;
      x^2 = (-c)/a by A1,A5,A9,XCMPLX_1:90;
      then x^2 = ((-c)*a") by XCMPLX_0:def 9;
      then x^2 = -(c*a");
      then
A11:  (-x)^2=-c/a by XCMPLX_0:def 9;
A12:  (-c/a) > 0 by A2,XREAL_1:60;
      -x>0 by A9,XREAL_1:60;
      then -x = (2 -Root (-c/a)) by A11,A12,A10,PREPOWER:def 3;
      then -x = sqrt(-c/a) by A12,PREPOWER:41;
      hence thesis;
    end;
    case
      x=0;
      hence thesis;
    end;
  end;
  hence thesis;
end;
