begin :: Polynomial of Degree 1 and 2

reserve a, a9, a1, a2, a3, b, b9, c, c9, d, d9, h, p, q, x, x1, x2, x3, u, v,
  y, z for real number;

definition
  let a, b, x be complex number;
  func Polynom (a,b,x) equals
  a*x+b;
  coherence;
end;

registration
  let a, b, x be complex number;
  cluster Polynom(a,b,x) -> complex;
  coherence;
end;

registration
