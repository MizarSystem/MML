definition
  let P be preProgram of SCM+FSA;
  let l be Element of NAT;
  func Directed(P,l) -> preProgram of SCM+FSA equals
  P +~ (halt SCM+FSA,goto l);
  coherence;
end;

definition
  let P be preProgram of SCM+FSA;
  func Directed P -> preProgram of SCM+FSA equals
  Directed(P, card P);
  coherence;
end;

