theorem Th70:
  for p holds for v,w holds v|still_not-bound_in p = w|
  still_not-bound_in p implies (J,v |= p iff J,w |= p)
proof
  defpred P[Element of CQC-WFF] means for v,w holds v|still_not-bound_in $1 =
  w|still_not-bound_in $1 implies (J,v |= $1 iff J,w |= $1);
A1: for p,q,x,k for l being CQC-variable_list of k for P being
QC-pred_symbol of k holds P[VERUM] & P[P!l] & (P[p] implies P['not' p]) & (P[p]
& P[q] implies P[p '&' q]) & (P[p] implies P[All(x,p)]) by Th61,Th62,Th63,Th68,
  VALUAT_1:44;
  thus for p holds P[p] from CQC_LANG:sch 1(A1);
end;

theorem Th71:
  [S,x] is quantifiable implies (v.((S_Bound(@CQCSub_All([S,x],xSQ
)))|a)). (NEx_Val(v,S,x,xSQ)+*(x|a))|still_not-bound_in S`1 = (v.(NEx_Val(v,S,x
  ,xSQ)+*(x|a)))|still_not-bound_in S`1
proof
  set S1 = CQCSub_All([S,x],xSQ);
  set z = S_Bound(@S1);
  set finSub = RestrictSub(x,All(x,S`1),xSQ);
  set V1 = (v.(z|a)).(NEx_Val(v,S,x,xSQ)+*(x|a));
  set V2 = v.(NEx_Val(v,S,x,xSQ)+*(x|a));
  set X = still_not-bound_in S`1;
A1: dom V1 = dom (v.(z|a)) \/ dom (NEx_Val(v,S,x,xSQ)+*(x|a)) by FUNCT_4:def
  1;
  dom (v.(z|a)) = bound_QC-variables by Th59;
  then dom (v.(z|a)) = dom v by Th59;
  then
A2: dom V1 = dom V2 by A1,FUNCT_4:def 1;
  assume
A3: [S,x] is quantifiable;
A4: now
    assume not x in rng finSub;
    then
A5: z = x by A3,Th53;
    for b st b in dom V1 holds V1.b = V2.b
    proof
      let b such that
A6:   b in dom V1;
A7:   now
        assume
A8:     b <> z;
A9:     now
          not b in {x} by A5,A8,TARSKI:def 1;
          then
A10:      not b in dom (x|a) by Th59;
          assume not b in dom NEx_Val(v,S,x,xSQ);
          then not b in dom NEx_Val(v,S,x,xSQ) \/ dom (x|a) by A10,
          XBOOLE_0:def 3;
          then
A11:      not b in dom (NEx_Val(v,S,x,xSQ)+*(x|a)) by
          FUNCT_4:def 1;
          reconsider x = b as bound_QC-variable by A6;
          V1.b = (v.(z|a)).b by A11,FUNCT_4:12;
          then V1.b = v.x by A8,Th49;
          hence thesis by A11,FUNCT_4:12;
        end;
        now
          dom (NEx_Val(v,S,x,xSQ)+*(x|a)) = dom NEx_Val(v,S,x,xSQ) \/ dom
          (x|a) by FUNCT_4:def 1;
          then
A12:      dom NEx_Val(v,S,x,xSQ) c= dom (NEx_Val(v,S,x,xSQ)+*(x| a)) by
          XBOOLE_1:7;
          assume
A13:      b in dom NEx_Val(v,S,x,xSQ);
          then V1.b = (NEx_Val(v,S,x,xSQ)+*(x|a)).b by A12,FUNCT_4:14;
          hence thesis by A13,A12,FUNCT_4:14;
        end;
        hence thesis by A9;
      end;
      now
        assume b = z;
        then b in {x} by A5,TARSKI:def 1;
        then
A14:    b in dom (x|a) by Th59;
        dom (NEx_Val(v,S,x,xSQ)+*(x|a)) = dom NEx_Val(v,S,x,xSQ) \/ dom (
        x|a) by FUNCT_4:def 1;
        then
A15:    dom (x|a) c= dom (NEx_Val(v,S,x,xSQ)+*(x|a)) by XBOOLE_1:7;
        then V1.b = (NEx_Val(v,S,x,xSQ)+*(x|a)).b by A14,FUNCT_4:14;
        hence thesis by A14,A15,FUNCT_4:14;
      end;
      hence thesis by A7;
    end;
    hence thesis by A2,FUNCT_1:9;
  end;
  now
    assume
A16: x in rng finSub;
A17: dom (V1|X) = (dom (v.(z|a)) \/ dom (NEx_Val(v,S,x,xSQ)+*(x|a))) /\ X
    by A1,RELAT_1:90;
A18: for b st b in dom (V1|X) holds V1|X.b = V2|X.b
    proof
A19:  X c= Bound_Vars(S`1) by Th48;
      let b such that
A20:  b in dom (V1|X);
      b in X by A17,A20,XBOOLE_0:def 4;
      then
A21:  b <> z by A3,A16,A19,Th39;
A22:  V2|X = v|X +* (NEx_Val(v,S,x,xSQ)+*(x|a))|X by FUNCT_4:75;
A23:  V1|X = (v.(z|a))|X +* (NEx_Val(v,S,x,xSQ)+*(x|a))|X by FUNCT_4:75;
      then
A24:  dom (V1|X) = dom (v.(z|a)|X) \/ dom ((NEx_Val(v,S,x,xSQ)+*(x|a))|X)
      by FUNCT_4:def 1;
A25:  now
        assume
A26:    not b in dom ((NEx_Val(v,S,x,xSQ)+*(x|a))|X);
        then
A27:    b in dom (v.(z|a)|X) by A20,A24,XBOOLE_0:def 3;
        then b in dom (v.(z|a)) /\ X by RELAT_1:90;
        then
A28:    b in X by XBOOLE_0:def 4;
        b in bound_QC-variables by A20;
        then b in dom v by Th59;
        then
A29:    b in dom v /\ X by A28,XBOOLE_0:def 4;
        V1|X.b = (v.(z|a))|X.b by A23,A26,FUNCT_4:12;
        then V1|X.b = v.(z|a).b by A27,FUNCT_1:70;
        then
A30:    V1|X.b = v.b by A21,Th49;
        V2|X.b = v|X.b by A22,A26,FUNCT_4:12;
        hence thesis by A30,A29,FUNCT_1:71;
      end;
      now
        assume
A31:    b in dom ((NEx_Val(v,S,x,xSQ)+*(x|a))|X);
        then V1|X.b = (NEx_Val(v,S,x,xSQ)+*(x|a))|X.b by A23,FUNCT_4:14;
        hence thesis by A22,A31,FUNCT_4:14;
      end;
      hence thesis by A25;
    end;
    dom (V2|X) = dom (V1) /\ X by A2,RELAT_1:90;
    hence thesis by A18,FUNCT_1:9,RELAT_1:90;
  end;
  hence thesis by A4;
end;

theorem Th72:
  [S,x] is quantifiable implies ((for a holds J,(v.((S_Bound(@
CQCSub_All([S,x],xSQ)))|a)).(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S) iff for a holds J
  ,v.(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S )
proof
  set S1 = CQCSub_All([S,x],xSQ);
  set z = S_Bound(@S1);
  assume
A1: [S,x] is quantifiable;
  thus (for a holds J,(v.(z|a)).(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S) implies for
  a holds J,v.(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S
  proof
    set X = still_not-bound_in S`1;
    assume
A2: for a holds J,(v.(z|a)).(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S;
    let a;
    set V1 = (v.(z|a)).(NEx_Val(v,S,x,xSQ)+*(x|a));
    set V2 = v.(NEx_Val(v,S,x,xSQ)+*(x|a));
    V1|X = V2|X by A1,Th71;
    then
A3: J,V1 |= S`1 iff J,V2 |= S`1 by Th70;
    J,V1 |= S by A2;
    hence thesis by A3,Def3;
  end;
  thus (for a holds J,v.(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S) implies for a holds
  J,(v.(z|a)).(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S
  proof
    set X = still_not-bound_in S`1;
    assume
A4: for a holds J,v.(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S;
    let a;
    set V1 = (v.(z|a)).(NEx_Val(v,S,x,xSQ)+*(x|a));
    set V2 = v.(NEx_Val(v,S,x,xSQ)+*(x|a));
    V1|X = V2|X by A1,Th71;
    then
A5: J,V1 |= S`1 iff J,V2 |= S`1 by Th70;
    J,V2 |= S by A4;
    hence thesis by A5,Def3;
  end;
end;

theorem Th73:
  dom NEx_Val(v,S,x,xSQ) = dom RestrictSub(x,All(x,S`1),xSQ)
proof
  rng (@RestrictSub(x,All(x,S`1),xSQ)) c= bound_QC-variables;
  then rng (@RestrictSub(x,All(x,S`1),xSQ)) c= dom v by Th59;
  then dom NEx_Val(v,S,x,xSQ) = dom (@RestrictSub(x,All(x,S`1),xSQ)) by
  RELAT_1:46;
  hence thesis by SUBSTUT1:def 2;
end;

