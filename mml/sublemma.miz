theorem Th1:
  for f,g,h,h1,h2 being Function st dom h1 c= dom h & dom h2 c= dom
  h holds f+*g+*h = (f+*h1)+*(g+*h2)+*h
proof
  let f,g,h,h1,h2 be Function such that
A1: dom h1 c= dom h and
A2: dom h2 c= dom h;
  dom (f+*h1) = (dom f \/ dom h1) & dom (g+*h2) = (dom g \/ dom h2) by
  FUNCT_4:def 1;
  then dom ((f+*h1)+*(g+*h2)) = ((dom f \/ dom h1) \/ (dom g \/ dom h2)) by
  FUNCT_4:def 1;
  then
  dom ((f+*h1)+*(g+*h2)+*h) = ((dom f \/ dom h1) \/ (dom g \/ dom h2)) \/
  dom h by FUNCT_4:def 1;
  then dom ((f+*h1)+*(g+*h2)+*h) = (dom f \/ dom h1) \/ ((dom g \/ dom h2) \/
  dom h) by XBOOLE_1:4;
  then dom ((f+*h1)+*(g+*h2)+*h) = (dom f \/ dom h1) \/ (dom g \/ (dom h2 \/
  dom h)) by XBOOLE_1:4;
  then
  dom ((f+*h1)+*(g+*h2)+*h) = (dom f \/ dom h1) \/ (dom g \/ dom h) by A2
  ,XBOOLE_1:12;
  then dom ((f+*h1)+*(g+*h2)+*h) = ((dom f \/ dom h1) \/ dom h) \/ dom g by
  XBOOLE_1:4;
  then dom ((f+*h1)+*(g+*h2)+*h) = (dom f \/ (dom h1 \/ dom h)) \/ dom g by
  XBOOLE_1:4;
  then
A3: dom ((f+*h1)+*(g+*h2)+*h) = (dom f \/ dom h) \/ dom g by A1,
  XBOOLE_1:12;
A4: for b st b in dom (f+*g+*h) holds (f+*g+*h).b = ((f+*h1)+*(g+*h2)+*h).b
  proof
    let b such that
    b in dom (f+*g+*h);
A5: now
      assume
A6:   not b in dom h;
      then
A7:   ((f+*h1)+*(g+*h2)+*h).b = ((f+*h1)+*(g+*h2)).b by FUNCT_4:12;
A8:   (f+*g+*h).b = (f+*g).b by A6,FUNCT_4:12;
A9:   now
A10:    not b in dom h2 by A2,A6;
        assume
A11:    b in dom g;
        dom g c= (dom g \/ dom h2) by XBOOLE_1:7;
        then b in dom g \/ dom h2 by A11;
        then b in dom (g+*h2) by FUNCT_4:def 1;
        then
A12:    ((f+*h1)+*(g+*h2)+*h).b = (g+*h2).b by A7,FUNCT_4:14;
        (f+*g+*h).b = g.b by A8,A11,FUNCT_4:14;
        hence thesis by A12,A10,FUNCT_4:12;
      end;
      now
A13:    not b in dom h1 by A1,A6;
        assume
A14:    not b in dom g;
        not b in dom h2 by A2,A6;
        then not b in dom g \/ dom h2 by A14,XBOOLE_0:def 3;
        then not b in dom (g+*h2) by FUNCT_4:def 1;
        then
A15:    ((f+*h1)+*(g+*h2)+*h).b = (f+*h1).b by A7,FUNCT_4:12;
        (f+*g+*h).b = f.b by A8,A14,FUNCT_4:12;
        hence thesis by A15,A13,FUNCT_4:12;
      end;
      hence thesis by A9;
    end;
    now
      assume
A16:  b in dom h;
      then (f+*g+*h).b = h.b by FUNCT_4:14;
      hence thesis by A16,FUNCT_4:14;
    end;
    hence thesis by A5;
  end;
  dom (f+*g) = dom f \/ dom g by FUNCT_4:def 1;
  then dom (f+*g+*h) = (dom f \/ dom g) \/ dom h by FUNCT_4:def 1;
  hence thesis by A3,A4,FUNCT_1:9,XBOOLE_1:4;
end;

theorem Th2:
  for vS1 being Function st x in dom vS1 holds (vS1|((dom vS1) \ {x
  })) +* (x .--> vS1.x) = vS1
proof
  let vS1 be Function;
  assume x in dom vS1;
  then (dom vS1 \ {x}) \/ {x} = dom vS1 \/ {x} & {x} c= dom vS1 by
  XBOOLE_1:
  39 ,ZFMISC_1:37;
  then (dom vS1 \ {x}) \/ {x} = dom vS1 by XBOOLE_1:12;
  hence thesis by FUNCT_7:15;
end;

definition
  let A;
  mode Val_Sub of A is PartFunc of bound_QC-variables,A;
end;

reserve vS,vS1,vS2 for Val_Sub of A;

notation
  let A, v, vS;
  synonym v.vS for v +* vS;
end;

definition
  let A, v, vS;
  redefine func v.vS -> Element of Valuations_in A;
  coherence
  proof
    v is Element of Funcs(bound_QC-variables,A) by VALUAT_1:def 1;
    then ex f st v = f & dom f = bound_QC-variables & rng f c= A by
    FUNCT_2:
    def 2;
    then dom (v +* vS) = bound_QC-variables \/ dom vS by FUNCT_4:def 1;
    then
A1: dom (v +* vS) = bound_QC-variables by XBOOLE_1:12;
    rng (v +* vS) c= rng v \/ rng vS by FUNCT_4:18;
    then ex g st v +* vS = g & dom g = bound_QC-variables & rng g c= A by A1;
    then v +* vS in Funcs(bound_QC-variables,A) by FUNCT_2:def 2;
    hence thesis by VALUAT_1:def 1;
  end;
end;

definition
  let S;
  redefine func S`1 -> Element of CQC-WFF;
  coherence
  proof
    S in CQC-Sub-WFF;
    then ex S1 being Element of QC-Sub-WFF st S = S1 & S1`1 is Element of
    CQC-WFF by SUBSTUT1:def 39;
    hence thesis;
  end;
end;

definition
  canceled;
  let S, A, v;
  func Val_S(v,S) -> Val_Sub of A equals
  (@S`2)*v;
  coherence;
end;

theorem Th3:
  S is Sub_VERUM implies CQC_Sub(S) = VERUM
proof
  ex F being Function of QC-Sub-WFF,QC-WFF st CQC_Sub(S) = F.S & for S9
being Element of QC-Sub-WFF holds (S9 is Sub_VERUM implies F.S9 = VERUM) & ( S9
  is Sub_atomic implies F.S9 = (the_pred_symbol_of ((S9)`1))! CQC_Subst(
Sub_the_arguments_of S9,(S9)`2)) & (S9 is Sub_negative implies F.S9 = 'not' (F.
  (Sub_the_argument_of S9))) & (S9 is Sub_conjunctive implies F.S9 = (F.
  Sub_the_left_argument_of S9) '&' (F.Sub_the_right_argument_of S9)) & (S9 is
  Sub_universal implies F.S9 = Quant(S9,F.Sub_the_scope_of S9)) by SUBSTUT1:def
  38;
  hence thesis;
end;

definition
  let S, A, v, J;
  pred J,v |= S means
  :Def3:
  J,v |= S`1;
end;

theorem Th4:
  S is Sub_VERUM implies for v holds (J,v |= CQC_Sub(S) iff J,v.
  Val_S(v,S) |= S)
proof
  assume
A1: S is Sub_VERUM;
  let v;
  ex Sub st S = [VERUM,Sub] by A1,SUBSTUT1:def 19;
  then S`1 = VERUM by MCART_1:7;
  then J,v.Val_S(v,S) |= VERUM iff J,v.Val_S(v,S) |= S by Def3;
  hence J,v |= CQC_Sub(S) implies J,v.Val_S(v,S) |= S by VALUAT_1:44;
  J,v.Val_S(v,S) |= S implies J,v |= VERUM by VALUAT_1:44;
  hence thesis by A1,Th3;
end;

theorem Th5:
  i in dom ll implies ll.i is bound_QC-variable
proof
  assume i in dom ll;
  then
A1: ll.i in rng ll by FUNCT_1:12;
  rng ll c= bound_QC-variables by RELAT_1:def 19;
  hence thesis by A1;
end;

theorem Th6:
  S is Sub_atomic implies CQC_Sub(S) = (the_pred_symbol_of S`1)!
  CQC_Subst(Sub_the_arguments_of S,S`2)
proof
  ex F being Function of QC-Sub-WFF,QC-WFF st CQC_Sub(S) = F.S & for S9
being Element of QC-Sub-WFF holds (S9 is Sub_VERUM implies F.S9 = VERUM) & ( S9
  is Sub_atomic implies F.S9 = (the_pred_symbol_of ((S9)`1))! CQC_Subst(
Sub_the_arguments_of S9,(S9)`2)) & (S9 is Sub_negative implies F.S9 = 'not' (F.
  (Sub_the_argument_of S9))) & (S9 is Sub_conjunctive implies F.S9 = (F.
  Sub_the_left_argument_of S9) '&' (F.Sub_the_right_argument_of S9)) & (S9 is
  Sub_universal implies F.S9 = Quant(S9,F.Sub_the_scope_of S9)) by SUBSTUT1:def
  38;
  hence thesis;
end;

theorem
  Sub_the_arguments_of Sub_P(P,ll,Sub) = Sub_the_arguments_of Sub_P(P9,
  ll9,Sub9) implies ll = ll9
proof
  assume
A1: Sub_the_arguments_of Sub_P(P,ll,Sub) = Sub_the_arguments_of Sub_P(P9
  ,ll9,Sub9);
  consider k1 being Element of NAT, P1 being (QC-pred_symbol of k1), ll1 being
  QC-variable_list of k1, e1 being Element of vSUB such that
A2: Sub_the_arguments_of Sub_P(P,ll,Sub) = ll1 and
A3: Sub_P(P,ll,Sub) = Sub_P(P1,ll1,e1) by SUBSTUT1:def 29;
A4: P!ll = <*P*>^ll & P1!ll1 = <*P1*>^ll1 by QC_LANG1:23;
  Sub_P(P,ll,Sub) = [P!ll,Sub] by SUBSTUT1:9;
  then [P!ll,Sub] = [P1!ll1,e1] by A3,SUBSTUT1:9;
  then
A5: <*P1*>^ll1 = <*P*>^ll by A4,ZFMISC_1:33;
  (<*P1*>^ll1).1 = P1 & (<*P*>^ll).1 = P by FINSEQ_1:58;
  then
A6: ll1 = ll by A5,FINSEQ_1:46;
  consider k2 being Element of NAT, P2 being (QC-pred_symbol of k2), ll2 being
  QC-variable_list of k2, e2 being Element of vSUB such that
A7: Sub_the_arguments_of Sub_P(P9,ll9,Sub9) = ll2 and
A8: Sub_P(P9,ll9,Sub9) = Sub_P(P2,ll2,e2) by SUBSTUT1:def 29;
A9: P9!ll9 = <*P9*>^ll9 & P2!ll2 = <*P2*>^ll2 by QC_LANG1:23;
  Sub_P(P9,ll9,Sub9) = [P9!ll9,Sub9] by SUBSTUT1:9;
  then [P9!ll9,Sub9] = [P2!ll2,e2] by A8,SUBSTUT1:9;
  then
A10: <*P2*>^ll2 = <*P9*>^ll9 by A9,ZFMISC_1:33;
  (<*P2*>^ll2).1 = P2 & (<*P9*>^ll9).1 = P9 by FINSEQ_1:58;
  hence thesis by A1,A2,A7,A6,A10,FINSEQ_1:46;
end;

definition
  let k, P, ll, Sub;
  redefine func Sub_P(P,ll,Sub) -> Element of CQC-Sub-WFF;
  coherence
  proof
    Sub_P(P,ll,Sub) = [P!ll,Sub] by SUBSTUT1:9;
    then Sub_P(P,ll,Sub)`1 = P!ll by MCART_1:7;
    then Sub_P(P,ll,Sub) in CQC-Sub-WFF by SUBSTUT1:def 39;
    hence thesis;
  end;
end;

canceled;

theorem Th9:
  CQC_Sub(Sub_P(P,ll,Sub)) = P!CQC_Subst(ll,Sub)
proof
A1: P!ll is atomic by QC_LANG1:def 17;
A2: Sub_P(P,ll,Sub) = [P!ll,Sub] by SUBSTUT1:9;
  then
A3: Sub_P(P,ll,Sub)`2 = Sub by MCART_1:7;
  Sub_P(P,ll,Sub)`1 = P!ll by A2,MCART_1:7;
  then
  Sub_the_arguments_of Sub_P(P,ll,Sub) = ll & the_pred_symbol_of (Sub_P(P,
  ll, Sub)`1) = P by A1,QC_LANG1:def 21,SUBSTUT1:def 29;
  hence thesis by A3,Th6;
end;

theorem
  P!CQC_Subst(ll,Sub) is Element of CQC-WFF
proof
  CQC_Sub(Sub_P(P,ll,Sub)) = P!CQC_Subst(ll,Sub) by Th9;
  hence thesis;
end;

theorem Th11:
  CQC_Subst(ll,Sub) is CQC-variable_list of k
proof
  reconsider ll as FinSequence of bound_QC-variables by SUBSTUT1:34;
  reconsider s = CQC_Subst(ll,Sub) as FinSequence of bound_QC-variables;
A1: s = CQC_Subst(@ll,Sub) by SUBSTUT1:def 5;
  len ll = k by FINSEQ_1:def 18;
  then len @ll = k by SUBSTUT1:def 4;
  then len s = k by A1,SUBSTUT1:def 3;
  then s is CQC-variable_list of k by SUBSTUT1:34;
  hence thesis by A1,SUBSTUT1:def 4;
end;

registration
  let k, ll, Sub;
  cluster CQC_Subst(ll,Sub) -> bound_QC-variables -valued k-long;
  coherence by Th11;
end;

theorem Th12:
  not x in dom S`2 implies (v.Val_S(v,S)).x = v.x
proof
  assume not x in dom(S`2);
  then
A1: not x in dom @S`2 by SUBSTUT1:def 2;
  dom(@S`2*v) c= dom(@S`2) by RELAT_1:44;
  then not x in dom Val_S(v,S) by A1;
  hence thesis by FUNCT_4:12;
end;

theorem Th13:
  x in dom S`2 implies (v.Val_S(v,S)).x = Val_S(v,S).x
proof
  assume x in dom S`2;
  then
A1: x in dom @S`2 by SUBSTUT1:def 2;
  rng @S`2 c= bound_QC-variables & dom v = bound_QC-variables by FUNCT_2:
  def 1;
  then x in dom Val_S(v,S) by A1,RELAT_1:46;
  hence thesis by FUNCT_4:14;
end;

theorem Th14:
  (v.Val_S(v,Sub_P(P,ll,Sub)))*'ll = v*'(CQC_Subst(ll,Sub))
proof
  set S9 = Sub_P(P,ll,Sub);
  set ll9 = CQC_Subst(ll,Sub);
A1: len ll = k by FINSEQ_1:def 18;
  S9 = [P!ll,Sub] by SUBSTUT1:9;
  then
A2: (S9)`2 = Sub by MCART_1:7;
A3: len ((v.Val_S(v,S9))*'ll) = k by VALUAT_1:def 8;
  then
A4: dom ((v.Val_S(v,S9))*'ll) = Seg k by FINSEQ_1:def 3;
A5: for j be Nat st j in dom ((v.Val_S(v,S9))*'ll) holds (v.Val_S(v,S9))*'ll
  .j = v*'(CQC_Subst(ll,Sub)).j
  proof
    let j be Nat such that
A6: j in dom ((v.Val_S(v,S9))*'ll);
A7: 1 <= j & j <= k by A4,A6,FINSEQ_1:3;
    reconsider j as Element of NAT by ORDINAL1:def 13;
    j in Seg (len ll) by A4,A6,FINSEQ_1:def 18;
    then j in dom ll by FINSEQ_1:def 3;
    then reconsider x = ll.j as bound_QC-variable by Th5;
A8: now
      assume
A9:   ll.j in dom Sub;
      then
      (v.Val_S(v,S9)).(ll.j) = Val_S(v,S9).x & ll.j in dom @(S9)`2 by A2,
      Th13,SUBSTUT1:def 2;
      then (v.Val_S(v,S9)).(ll.j) = v.((@(S9)`2).(ll.j)) by FUNCT_1:23;
      then
A10:  (v.Val_S(v,S9)).(ll.j) = v.(((S9)`2).(ll.j)) by
      SUBSTUT1:def 2;
A11:  ((v.Val_S(v,S9))*'ll).j = (v.Val_S(v,S9)).(ll.j) by A7, VALUAT_1:
      def 8;
      v.(ll9.j) = v.(((S9)`2).(ll.j)) by A2,A1,A7,A9,SUBSTUT1:def 3;
      hence thesis by A7,A10,A11,VALUAT_1:def 8;
    end;
    now
      assume not ll.j in dom Sub;
      then
A12:  v.(ll9.j) = v.(ll.j) & (v.Val_S(v,S9)).(ll.j) = v.x by A2,A1,A7,
      Th12, SUBSTUT1:def 3;
      (v*'ll9).j = v.(ll9.j) by A7,VALUAT_1:def 8;
      hence thesis by A7,A12,VALUAT_1:def 8;
    end;
    hence thesis by A8;
  end;
  len (v*'ll9) = k by VALUAT_1:def 8;
  hence thesis by A3,A5,FINSEQ_2:10;
end;

theorem Th15:
  Sub_P(P,ll,Sub)`1 = P!ll
proof
  Sub_P(P,ll,Sub) = [P!ll,Sub] by SUBSTUT1:9;
  hence thesis by MCART_1:7;
end;

theorem Th16:
  for v holds (J,v |= CQC_Sub(Sub_P(P,ll,Sub)) iff J,v.Val_S(v,
  Sub_P(P,ll,Sub)) |= Sub_P(P,ll,Sub))
proof
  set S9 = Sub_P(P,ll,Sub);
  set ll9 = CQC_Subst(ll,Sub);
  reconsider p = P!ll9 as Element of CQC-WFF;
  reconsider ll9 as CQC-variable_list of k;
  let v;
A1: Valid(p,J).v = TRUE iff v*'ll9 in J.P by VALUAT_1:16;
A2: (v.Val_S(v,S9))*'ll in J.P iff Valid(P!ll,J).((v.Val_S(v,S9))) = TRUE by
  VALUAT_1:16;
A3: J,v.Val_S(v,S9) |= P!ll iff J,v.Val_S(v,S9) |= Sub_P(P,ll,Sub)`1 by Th15;
  J,v |= CQC_Sub(Sub_P(P,ll,Sub)) iff J,v |= p by Th9;
  hence thesis by A1,A2,A3,Def3,Th14,VALUAT_1:def 12;
end;

theorem Th17:
  (Sub_not S)`1 = 'not' S`1 & (Sub_not S)`2 = S`2
proof
  Sub_not S = ['not' S`1,S`2] by SUBSTUT1:def 20;
  hence thesis by MCART_1:7;
end;

definition
  let S;
  redefine func Sub_not S -> Element of CQC-Sub-WFF;
  coherence
  proof
    'not' S`1 in CQC-WFF;
    then (Sub_not S)`1 in CQC-WFF by Th17;
    then Sub_not S in CQC-Sub-WFF by SUBSTUT1:def 39;
    hence thesis;
  end;
end;

theorem Th18:
  not J,v.Val_S(v,S) |= S iff J,v.Val_S(v,S) |= Sub_not S
proof
A1: not J,v.Val_S(v,S) |= S`1 iff J,v.Val_S(v,S) |= 'not' S`1 by VALUAT_1:28;
  J,v.Val_S(v,S) |= 'not' S`1 iff J,v.Val_S(v,S) |= (Sub_not S)`1 by Th17;
  hence thesis by A1,Def3;
end;

theorem Th19:
  Val_S(v,S) = Val_S(v,Sub_not S)
proof
  Sub_not S = ['not' S`1,S`2] by SUBSTUT1:def 20;
  hence thesis by MCART_1:7;
end;

theorem Th20:
  (for v holds (J,v |= CQC_Sub(S) iff J,v.Val_S(v,S) |= S))
  implies for v holds (J,v |= CQC_Sub(Sub_not S) iff J,v.Val_S(v,Sub_not S) |=
  Sub_not S)
proof
  assume
A1: for v holds (J,v |= CQC_Sub(S) iff J,v.Val_S(v,S) |= S);
  let v;
A2: J,v |= 'not' CQC_Sub(S) iff not J,v |= CQC_Sub(S) by VALUAT_1:28;
  not J,v.Val_S(v,S) |= S iff J,v.Val_S(v,S) |= Sub_not S by Th18;
  hence thesis by A1,A2,Th19,SUBSTUT1:29;
end;

definition
  let S1, S2;
  assume
A1: S1`2 = S2`2;
  func CQCSub_&(S1,S2) -> Element of CQC-Sub-WFF equals
  :Def4:
  Sub_&(S1,S2);
  coherence
  proof
    Sub_&(S1,S2) = [(S1`1) '&' (S2`1),S1`2] by A1,SUBSTUT1:def 21;
    then Sub_&(S1,S2)`1 = (S1`1) '&' (S2`1) by MCART_1:7;
    then Sub_&(S1,S2) in CQC-Sub-WFF by SUBSTUT1:def 39;
    hence thesis;
  end;
end;

theorem Th21:
  S1`2 = S2`2 implies CQCSub_&(S1,S2)`1 = (S1`1) '&' (S2`1) &
  CQCSub_&(S1,S2)`2 = S1`2
proof
  assume
A1: S1`2 = S2`2;
  then Sub_&(S1,S2) = [(S1`1) '&' (S2`1),S1`2] by SUBSTUT1:def 21;
  then CQCSub_&(S1,S2) = [(S1`1) '&' (S2`1),S1`2] by A1,Def4;
  hence thesis by MCART_1:7;
end;

theorem Th22:
  S1`2 = S2`2 implies CQCSub_&(S1,S2)`2 = S1`2
proof
  assume
A1: S1`2 = S2`2;
  then CQCSub_&(S1,S2) = Sub_&(S1,S2) by Def4;
  then CQCSub_&(S1,S2) = [(S1`1) '&' (S2`1),S1`2] by A1,SUBSTUT1:def 21;
  hence thesis by MCART_1:7;
end;

theorem
  S1`2 = S2`2 implies Val_S(v,S1) = Val_S(v,CQCSub_&(S1,S2)) & Val_S(v,
  S2) = Val_S(v,CQCSub_&(S1,S2)) by Th22;

theorem Th24:
  S1`2 = S2`2 implies CQC_Sub(CQCSub_&(S1,S2)) = (CQC_Sub(S1)) '&'
  (CQC_Sub(S2))
proof
  assume
A1: S1`2 = S2`2;
  then CQCSub_&(S1,S2) = Sub_&(S1,S2) by Def4;
  hence thesis by A1,SUBSTUT1:31;
end;

theorem Th25:
  S1`2 = S2`2 implies (J,v.Val_S(v,S1) |= S1 & J,v.Val_S(v,S2) |=
  S2 iff J,v.Val_S(v,CQCSub_&(S1,S2)) |= CQCSub_&(S1,S2))
proof
  assume
A1: S1`2 = S2`2;
  then Val_S(v,S1) = Val_S(v,CQCSub_&(S1,S2)) by Th22;
  then
A2: J,v.Val_S(v,S1) |= S1`1 & J,v.Val_S(v,S1) |= S2`1 iff J,v.Val_S(v,
  CQCSub_&(S1,S2)) |= (S1`1) '&' (S2`1) by VALUAT_1:29;
  J,v.Val_S(v,CQCSub_&(S1,S2)) |= (S1`1) '&' (S2`1) iff J,v.Val_S(v,
  CQCSub_&(S1,S2)) |= CQCSub_&(S1,S2)`1 by A1,Th21;
  hence thesis by A1,A2,Def3;
end;

theorem Th26:
  S1`2 = S2`2 & (for v holds (J,v |= CQC_Sub(S1) iff J,v.Val_S(v,
  S1) |= S1)) & (for v holds (J,v |= CQC_Sub(S2) iff J,v.Val_S(v,S2) |= S2))
implies for v holds (J,v |= CQC_Sub(CQCSub_&(S1,S2)) iff J,v.Val_S(v,CQCSub_&(
  S1,S2)) |= CQCSub_&(S1,S2))
proof
  assume that
A1: S1`2 = S2`2 and
A2: ( for v holds (J,v |= CQC_Sub(S1) iff J,v.Val_S(v,S1) |= S1))& for v
  holds (J, v |= CQC_Sub(S2) iff J,v.Val_S(v,S2) |= S2);
  let v;
A3: J,v |= (CQC_Sub(S1)) & J,v |= (CQC_Sub(S2)) iff J,v.Val_S(v,S1) |= S1 &
  J,v.Val_S(v,S2) |= S2 by A2;
  J,v |= CQC_Sub(CQCSub_&(S1,S2)) iff J,v |= (CQC_Sub(S1)) '&' (CQC_Sub(S2
  )) by A1,Th24;
  hence thesis by A1,A3,Th25,VALUAT_1:29;
end;

reserve B for Element of [:QC-Sub-WFF,bound_QC-variables:],
  SQ for second_Q_comp of B;

theorem Th27:
  B is quantifiable implies Sub_All(B,SQ)`1 = All(B`2,(B`1)`1) &
  Sub_All(B,SQ)`2 = SQ
proof
  assume B is quantifiable;
  then Sub_All(B,SQ) = [All(B`2,(B`1)`1),SQ] by SUBSTUT1:def 24;
  hence thesis by MCART_1:7;
end;

definition
  let B be Element of [:QC-Sub-WFF,bound_QC-variables:];
  attr B is CQC-WFF-like means
  :Def5:
  B`1 in CQC-Sub-WFF;
end;

registration
  cluster CQC-WFF-like Element of [:QC-Sub-WFF,bound_QC-variables:];
  existence
  proof
    consider Sub,x;
    set B = [[VERUM,Sub],x];
    reconsider S = [VERUM,Sub] as Element of QC-Sub-WFF by QC_LANG1:def 13,
    SUBSTUT1:def 16;
    [VERUM,Sub] in QC-Sub-WFF by QC_LANG1:def 13,SUBSTUT1:def 16;
    then reconsider B as Element of [:QC-Sub-WFF,bound_QC-variables:] by
    ZFMISC_1:106;
    take B;
    S`1 is Element of CQC-WFF by MCART_1:7;
    then S in CQC-Sub-WFF by SUBSTUT1:def 39;
    then B`1 in CQC-Sub-WFF by MCART_1:7;
    hence thesis by Def5;
  end;
end;

definition
  let S, x;
  redefine func [S,x] -> CQC-WFF-like Element of [:QC-Sub-WFF,
  bound_QC-variables:];
  coherence
  proof
    [S,x]`1 = S by MCART_1:7;
    hence thesis by Def5;
  end;
end;

reserve B for CQC-WFF-like Element of [:QC-Sub-WFF,bound_QC-variables:],
  xSQ for second_Q_comp of [S,x],
  SQ for second_Q_comp of B;

definition
  let B;
  redefine func B`1 -> Element of CQC-Sub-WFF;
  coherence by Def5;
end;

definition
  let B, SQ;
  assume
A1: B is quantifiable;
  func CQCSub_All(B,SQ) -> Element of CQC-Sub-WFF equals
  :Def6:
  Sub_All(B,SQ);
  coherence
  proof
    All(B`2,(B`1)`1) in CQC-WFF;
    then Sub_All(B,SQ)`1 in CQC-WFF by A1,Th27;
    then Sub_All(B,SQ) in CQC-Sub-WFF by SUBSTUT1:def 39;
    hence thesis;
  end;
end;

theorem Th28:
  B is quantifiable implies CQCSub_All(B,SQ) is Sub_universal
proof
  assume
A1: B is quantifiable;
  then Sub_All(B,SQ) is Sub_universal by SUBSTUT1:14;
  hence thesis by A1,Def6;
end;

definition
  let S such that
A1: S is Sub_universal;
  func CQCSub_the_scope_of S -> Element of CQC-Sub-WFF equals
  :Def7:
  Sub_the_scope_of S;
  coherence
  proof
    consider B being Element of [:QC-Sub-WFF,bound_QC-variables:], SQ being
    second_Q_comp of B such that
A2: S = Sub_All(B,SQ) and
A3: B`1 = Sub_the_scope_of S and
A4: B is quantifiable by A1,SUBSTUT1:def 34;
    S`1 = All(B`2,(B`1)`1) by A2,A4,Th27;
    then (B`1)`1 is Element of CQC-WFF by CQC_LANG:23;
    then B`1 in CQC-Sub-WFF by SUBSTUT1:def 39;
    hence thesis by A3;
  end;
end;

definition
  let S1, p;
  assume that
A1: S1 is Sub_universal and
A2: p = CQC_Sub(CQCSub_the_scope_of S1);
  func CQCQuant(S1,p) -> Element of CQC-WFF equals
  :Def8:
  Quant(S1,p);
  coherence
  proof
    CQCSub_the_scope_of S1 = Sub_the_scope_of S1 by A1,Def7;
    then CQC_Sub(S1) = Quant(S1,p) by A1,A2,SUBSTUT1:32;
    hence thesis;
  end;
end;

theorem Th29:
  S is Sub_universal implies CQC_Sub(S) = CQCQuant(S,CQC_Sub(
  CQCSub_the_scope_of S))
proof
  assume
A1: S is Sub_universal;
  then CQCSub_the_scope_of S = Sub_the_scope_of S by Def7;
  then CQCQuant(S,CQC_Sub(CQCSub_the_scope_of S)) = Quant(S,CQC_Sub(
  Sub_the_scope_of S)) by A1,Def8;
  hence thesis by A1,SUBSTUT1:32;
end;

theorem Th30:
  B is quantifiable implies CQCSub_the_scope_of(CQCSub_All(B,SQ)) = B`1
proof
  assume
A1: B is quantifiable;
  then
A2: CQCSub_All(B,SQ) = Sub_All(B,SQ) by Def6;
  then CQCSub_All(B,SQ) is Sub_universal by A1,SUBSTUT1:14;
  then
  CQCSub_the_scope_of(CQCSub_All(B,SQ)) = Sub_the_scope_of(Sub_All(B,SQ))
  by A2,Def7;
  hence thesis by A1,SUBSTUT1:21;
end;

begin :: The Substitution Lemma

theorem Th31:
  [S,x] is quantifiable implies CQCSub_the_scope_of(CQCSub_All([S,
  x],xSQ)) = S & CQCQuant(CQCSub_All([S,x],xSQ),CQC_Sub(CQCSub_the_scope_of
  CQCSub_All([S,x],xSQ))) = CQCQuant(CQCSub_All([S,x],xSQ),CQC_Sub(S))
proof
  assume [S,x] is quantifiable;
  then CQCSub_the_scope_of(CQCSub_All([S,x],xSQ)) = [S,x]`1 by Th30;
  hence thesis by MCART_1:7;
end;

theorem Th32:
  [S,x] is quantifiable implies CQCQuant(CQCSub_All([S,x],xSQ),
  CQC_Sub(S)) = All(S_Bound(@CQCSub_All([S,x],xSQ)),CQC_Sub(S))
proof
  set S1 = CQCSub_All([S,x],xSQ);
  set p = CQC_Sub(CQCSub_the_scope_of S1);
A1: Quant(S1,p) = All(S_Bound(@S1),p) by SUBSTUT1:def 37;
  assume
A2: [S,x] is quantifiable;
  then CQCSub_All([S,x],xSQ) = Sub_All([S,x],xSQ) by Def6;
  then CQCSub_All([S,x],xSQ) is Sub_universal by A2,SUBSTUT1:14;
  then
A3: CQCQuant(S1,p) = Quant(S1,p) by Def8;
  CQCQuant(S1,CQC_Sub(S)) = CQCQuant(S1,p) by A2,Th31;
  hence thesis by A2,A3,A1,Th31;
end;

theorem
  x in dom S`2 implies v.((@S`2).x) = v.Val_S(v,S).x
proof
  assume x in dom S`2;
  then v.Val_S(v,S).x = Val_S(v,S).x & x in dom @S`2 by
  Th13,SUBSTUT1:def 2;
  hence thesis by FUNCT_1:23;
end;

theorem
  x in dom (@S`2) implies (@S`2).x is bound_QC-variable
proof
  assume x in dom (@S`2);
  then (@S`2).x in rng @S`2 by FUNCT_1:12;
  hence thesis;
end;

theorem Th35:
  [:QC-WFF,vSUB:] c= dom QSub
proof
  let a;
  assume a in [:QC-WFF,vSUB:];
  then consider b,c such that
A1: b in QC-WFF and
A2: c in vSUB and
A3: a = [b,c] by ZFMISC_1:def 2;
  reconsider Sub = c as CQC_Substitution by A2;
  reconsider p = b as Element of QC-WFF by A1;
A4: now
    set b = {};
    set a = [[p,Sub],b];
    assume not p is universal;
    then p,Sub PQSub b by SUBSTUT1:def 14;
    then a in QSub by SUBSTUT1:def 15;
    hence thesis by A3,FUNCT_1:8;
  end;
  now
    set b = ExpandSub(bound_in p,the_scope_of p, RestrictSub(bound_in p,p,Sub)
    );
    set a = [[p,Sub],b];
    assume p is universal;
    then p,Sub PQSub b by SUBSTUT1:def 14;
    then a in QSub by SUBSTUT1:def 15;
    hence thesis by A3,FUNCT_1:8;
  end;
  hence thesis by A4;
end;

reserve B1 for Element of [:QC-Sub-WFF,bound_QC-variables:];
reserve SQ1 for second_Q_comp of B1;

theorem Th36:
  B is quantifiable & B1 is quantifiable & Sub_All(B,SQ) = Sub_All
  (B1,SQ1) implies B`2 = B1`2 & SQ = SQ1
proof
  assume that
A1: B is quantifiable and
A2: B1 is quantifiable & Sub_All(B,SQ) = Sub_All(B1,SQ1);
  Sub_All(B,SQ) = [All(B`2,(B`1)`1),SQ] by A1,SUBSTUT1:def 24;
  then
A3: [All(B`2,(B`1)`1),SQ] = [All(B1`2,(B1`1)`1),SQ1] by A2,SUBSTUT1:def
  24;
  then All(B`2,(B`1)`1) = All(B1`2,(B1`1)`1) by ZFMISC_1:33;
  hence thesis by A3,QC_LANG2:6,ZFMISC_1:33;
end;

theorem Th37:
  B is quantifiable & B1 is quantifiable & CQCSub_All(B,SQ) =
  Sub_All(B1,SQ1) implies B`2 = B1`2 & SQ = SQ1
proof
  assume that
A1: B is quantifiable and
A2: B1 is quantifiable and
A3: CQCSub_All(B,SQ) = Sub_All(B1,SQ1);
  Sub_All(B,SQ) = Sub_All(B1,SQ1) by A1,A3,Def6;
  hence thesis by A1,A2,Th36;
end;

theorem
  [S,x] is quantifiable implies Sub_the_bound_of CQCSub_All([S,x],xSQ) = x
proof
  set S1 = CQCSub_All([S,x],xSQ);
  assume
A1: [S,x] is quantifiable;
  then S1 = Sub_All([S,x],xSQ) by Def6;
  then S1 is Sub_universal by A1,SUBSTUT1:14;
  then consider B being Element of [:QC-Sub-WFF,bound_QC-variables:], SQ being
  second_Q_comp of B such that
A2: S1 = Sub_All(B,SQ) and
A3: B`2 = Sub_the_bound_of S1 and
A4: B is quantifiable by SUBSTUT1:def 33;
  [S,x]`2 = B`2 by A1,A2,A4,Th37;
  hence thesis by A3,MCART_1:7;
end;

theorem Th39:
  [S,x] is quantifiable & x in rng RestrictSub(x,All(x,S`1),xSQ)
implies not S_Bound(@CQCSub_All([S,x],xSQ)) in rng RestrictSub(x,All(x,S`1),xSQ
  ) & not S_Bound(@CQCSub_All([S,x],xSQ)) in Bound_Vars(S`1)
proof
  set S1 = CQCSub_All([S,x],xSQ);
  assume that
A1: [S,x] is quantifiable and
A2: x in rng RestrictSub(x,All(x,S`1),xSQ);
A3: S1 = Sub_All([S,x],xSQ) by A1,Def6;
  then S1`1 = All([S,x]`2,([S,x]`1)`1) by A1,Th27;
  then
A4: S1`1 = All(x,([S,x]`1)`1) by MCART_1:7;
  then
A5: bound_in S1`1 = x by QC_LANG2:8;
  set finSub = RestrictSub(bound_in S1`1,S1`1,S1`2);
A6: Dom_Bound_Vars(the_scope_of S1`1) = {i : x.i in Bound_Vars(the_scope_of
  S1`1)} by SUBSTUT1:def 9;
  S1`2 = xSQ by A1,A3,Th27;
  then
A7: finSub = RestrictSub(x,All(x,S`1),xSQ) by A4,A5,MCART_1:7;
  set Y = Dom_Bound_Vars(the_scope_of S1`1) \/ Sub_Var(finSub);
  set n = min NSub(the_scope_of S1`1,finSub);
A8: upVar(finSub,the_scope_of S1`1) = n by SUBSTUT1:def 12;
  NSub(the_scope_of S1`1,finSub) = NAT \ Y by SUBSTUT1:def 11;
  then reconsider X = NAT \ Y as non empty Subset of NAT;
A9: n = min X by SUBSTUT1:def 11;
  Dom_Bound_Vars(the_scope_of S1`1) c= Dom_Bound_Vars(the_scope_of S1`1)
\/ Sub_Var(finSub) & min X in NAT\(Dom_Bound_Vars(the_scope_of S1`1) \/ Sub_Var
  ( finSub)) by XBOOLE_1:7,XXREAL_2:def 7;
  then not n in Dom_Bound_Vars(the_scope_of S1`1) by A9,XBOOLE_0:def 5;
  then
A10: not x.n in Bound_Vars(the_scope_of S1`1) by A6;
  S1`1 = All(x,S`1) by A4,MCART_1:7;
  then
A11: not x.upVar(finSub,the_scope_of S1`1) in Bound_Vars(S`1) by A8,A10,
  QC_LANG2:8;
  Sub_Var(finSub) c= Y & min X in NAT\(Dom_Bound_Vars(the_scope_of S1`1)
  \/ Sub_Var(finSub)) by XBOOLE_1:7,XXREAL_2:def 7;
  then
A12: not n in Sub_Var(finSub) by A9,XBOOLE_0:def 5;
  Sub_Var(finSub) = {i : x.i in rng finSub} by SUBSTUT1:def 10;
  then
A13: not x.upVar(finSub,the_scope_of S1`1) in rng finSub by A8,A12;
  S1 = @S1 by SUBSTUT1:def 35;
  hence thesis by A2,A5,A7,A13,A11,SUBSTUT1:def 36;
end;

theorem Th40:
  [S,x] is quantifiable & not x in rng RestrictSub(x,All(x,S`1),
xSQ) implies not S_Bound(@CQCSub_All([S,x],xSQ)) in rng RestrictSub(x,All(x,S`1
  ),xSQ)
proof
  set S1 = CQCSub_All([S,x],xSQ);
  assume that
A1: [S,x] is quantifiable and
A2: not x in rng RestrictSub(x,All(x,S`1),xSQ);
A3: S1 = Sub_All([S,x],xSQ) by A1,Def6;
  then
A4: S1`1 = All([S,x]`2,([S,x]`1)`1) by A1,Th27;
  then
A5: S1`1 = All(x,([S,x]`1)`1) by MCART_1:7;
  set finSub = RestrictSub(bound_in S1`1,S1`1,S1`2);
A6: S1 = @S1 by SUBSTUT1:def 35;
  S1`1 = All(x,([S,x]`1)`1) by A4,MCART_1:7;
  then
A7: bound_in(S1`1) = x by QC_LANG2:8;
  S1`2 = xSQ by A1,A3,Th27;
  then not bound_in(S1`1) in rng finSub by A2,A7,A5,MCART_1:7;
  hence thesis by A2,A7,A6,SUBSTUT1:def 36;
end;

theorem Th41:
  [S,x] is quantifiable implies not S_Bound(@CQCSub_All([S,x],xSQ)
  ) in rng RestrictSub(x,All(x,S`1),xSQ)
proof
  assume
A1: [S,x] is quantifiable;
  then x in rng RestrictSub(x,All(x,S`1),xSQ) implies thesis by Th39;
  hence thesis by A1,Th40;
end;

theorem Th42:
  [S,x] is quantifiable implies S`2 = ExpandSub(x,S`1,RestrictSub(
  x,All(x,S`1),xSQ))
proof
  set Z = [All(x,S`1),xSQ];
  set q = All(x,S`1);
  assume [S,x] is quantifiable;
  then
A1: ([S,x]`1)`2 = QSub.[All([S,x]`2,([S,x]`1)`1),xSQ] by
  SUBSTUT1:def 23;
  [All([S,x]`2,([S,x]`1)`1),xSQ] = [All(x,([S,x]`1)`1),xSQ] by MCART_1:7;
  then
A2: [All([S,x]`2,([S,x]`1)`1),xSQ] = [All(x,S`1),xSQ] by MCART_1:7;
  Z in [:QC-WFF,vSUB:] by ZFMISC_1:def 2;
  then [Z,([S,x]`1)`2] in QSub by A1,A2,Th35,FUNCT_1:8;
  then [Z,S`2] in QSub by MCART_1:7;
  then consider p being QC-formula,Sub1,b such that
A3: [Z,S`2] = [[p,Sub1],b] and
A4: p,Sub1 PQSub b by SUBSTUT1:def 15;
  Z = [p,Sub1] by A3,ZFMISC_1:33;
  then
A5: All(x,S`1) = p & xSQ = Sub1 by ZFMISC_1:33;
A6: q is universal by QC_LANG1:def 20;
  then
A7: bound_in q = x by QC_LANG1:def 26;
  S`2 = b by A3,ZFMISC_1:33;
  then
  S`2 = ExpandSub(bound_in q,the_scope_of q,RestrictSub(bound_in q,q, xSQ
  )) by A4,A5,A6,SUBSTUT1:def 14;
  hence thesis by A6,A7,QC_LANG1:def 27;
end;

theorem
  still_not-bound_in VERUM c= Bound_Vars(VERUM) by QC_LANG3:7,SUBSTUT1:2;

theorem Th44:
  still_not-bound_in (P!ll) c= Bound_Vars(P!ll)
proof
  set l1 = the_arguments_of (P!ll);
A1: P!ll is atomic by QC_LANG1:def 17;
  then consider
  n being Element of NAT, P9 being (QC-pred_symbol of n), ll9 being
  QC-variable_list of n such that
A2: l1 = ll9 and
A3: P!ll = P9!ll9 by QC_LANG1:def 22;
  Bound_Vars(P!ll) = Bound_Vars(l1) by A1,SUBSTUT1:3;
  then
A4: Bound_Vars(P!ll) = { l1.i : 1 <= i & i <= len l1 & l1.i in
  bound_QC-variables} by SUBSTUT1:def 7;
  still_not-bound_in (P!ll) = still_not-bound_in ll by QC_LANG3:9;
  then
A5: still_not-bound_in (P!ll) = variables_in(ll,bound_QC-variables) by
  QC_LANG3:6;
A6: (<*P9*>^ll9).1 = P9 & (<*P*>^ll).1 = P by FINSEQ_1:58;
  P!ll = <*P*>^ll & P9!ll9 = <*P9*>^ll9 by QC_LANG1:23;
  then ll9 = ll by A3,A6,FINSEQ_1:46;
  hence thesis by A4,A5,A2,QC_LANG3:def 2;
end;

theorem Th45:
  still_not-bound_in (p) c= Bound_Vars(p) implies
  still_not-bound_in ('not' p) c= Bound_Vars('not' p)
proof
  'not' p is negative by QC_LANG1:def 18;
  then Bound_Vars('not' p) = Bound_Vars(the_argument_of ('not' p)) by
  SUBSTUT1:4;
  then
A1: Bound_Vars('not' p) = Bound_Vars(p) by QC_LANG2:2;
  assume still_not-bound_in (p) c= Bound_Vars(p);
  hence thesis by A1,QC_LANG3:11;
end;

theorem Th46:
  still_not-bound_in p c= Bound_Vars(p) & still_not-bound_in q c=
  Bound_Vars(q) implies still_not-bound_in (p '&' q) c= Bound_Vars(p '&' q)
proof
A1: still_not-bound_in (p '&' q) = still_not-bound_in p \/
  still_not-bound_in q by QC_LANG3:14;
  p '&' q is conjunctive by QC_LANG1:def 19;
  then Bound_Vars(p '&' q) = Bound_Vars(the_left_argument_of (p '&' q)) \/
  Bound_Vars(the_right_argument_of (p '&' q)) by SUBSTUT1:5;
  then
  Bound_Vars(p '&' q) = Bound_Vars(p) \/ Bound_Vars(the_right_argument_of
  (p '&' q)) by QC_LANG2:5;
  then
A2: Bound_Vars(p '&' q) = Bound_Vars(p) \/ Bound_Vars(q) by QC_LANG2:5;
  assume still_not-bound_in p c= Bound_Vars(p) & still_not-bound_in q c=
  Bound_Vars(q );
  hence thesis by A2,A1,XBOOLE_1:13;
end;

theorem Th47:
  still_not-bound_in p c= Bound_Vars(p) implies still_not-bound_in
  All(x,p) c= Bound_Vars(All(x,p))
proof
A1: still_not-bound_in All(x,p) = (still_not-bound_in p) \ {x} by QC_LANG3:
  16;
  All(x,p) is universal by QC_LANG1:def 20;
  then Bound_Vars(All(x,p)) = Bound_Vars(the_scope_of All(x,p)) \/ {bound_in
  All(x,p)} by SUBSTUT1:6;
  then Bound_Vars(All(x,p)) = Bound_Vars(p) \/ {bound_in All(x,p)} by
  QC_LANG2:8;
  then
  Bound_Vars(p) \ {x} c= Bound_Vars(p) & Bound_Vars(p) c= Bound_Vars(All(x
  ,p)) by XBOOLE_1:7,36;
  then
A2: Bound_Vars(p) \ {x} c= Bound_Vars(All(x,p)) by XBOOLE_1:1;
  assume still_not-bound_in p c= Bound_Vars(p);
  then still_not-bound_in All(x,p) c= Bound_Vars(p) \ {x} by
  A1,XBOOLE_1:33;
  hence thesis by A2,XBOOLE_1:1;
end;

theorem Th48:
  for p holds still_not-bound_in p c= Bound_Vars(p)
proof
  defpred P[Element of QC-WFF] means still_not-bound_in $1 c= Bound_Vars($1);
A1: for p,q,x,k for l being CQC-variable_list of k for P being
QC-pred_symbol of k holds P[VERUM] & P[P!l] & (P[p] implies P['not' p]) & (P[p]
& P[q] implies P[p '&' q]) & (P[p] implies P[All(x,p)]) by Th44,Th45,Th46,Th47,
  QC_LANG3:7,SUBSTUT1:2;
  thus for p holds P[p] from CQC_LANG:sch 1(A1);
end;

notation
  let A;
  let x;
  let a be Element of A;
  synonym x|a for x .--> a;
end;

definition
  let A;
  let x;
  let a be Element of A;
  redefine func x|a -> Val_Sub of A;
  coherence
  proof
    dom(x .--> a) = {x} & rng(x .--> a) = {a} by FUNCOP_1:14,19;
    hence thesis by RELSET_1:11;
  end;
end;

reserve a for Element of A;

theorem Th49:
  x <> b implies v.(x|a).b = v.b
proof
  assume x <> b;
  then not b in dom ({x} --> a) by TARSKI:def 1;
  hence thesis by FUNCT_4:12;
end;

theorem Th50:
  x = y implies v.(x|a).y = a
proof
  assume
A1: x = y;
  then y in {x} by TARSKI:def 1;
  then
A2: y in dom (x .--> a) by FUNCOP_1:19;
  (x .--> a).y = a by A1,FUNCOP_1:87;
  hence thesis by A2,FUNCT_4:14;
end;

theorem Th51:
  J,v |= All(x,p) iff for a holds J,v.(x|a) |= p
proof
  thus J,v |= All(x,p) implies for a holds J,v.(x|a) |= p
  proof
    assume
A1: J,v |= All(x,p);
    let a;
    for y st x <> y holds v.(x|a).y = v.y by Th49;
    hence thesis by A1,VALUAT_1:41;
  end;
  thus (for a holds J,v.(x|a) |= p) implies J,v |= All(x,p)
  proof
    assume
A2: for a holds J,v.(x|a) |= p;
    for w st for y st x <> y holds w.y = v.y holds J,w |= p
    proof
      let w such that
A3:   for y st x <> y holds w.y = v.y;
      set c = w.x;
A4:   for b st b in dom w holds w.b = v.(x|c).b
      proof
        let b;
        assume b in dom w;
        then reconsider y = b as bound_QC-variable;
        now
          assume
A5:       x <> y;
          then w.y = v.y by A3;
          hence thesis by A5,Th49;
        end;
        hence thesis by Th50;
      end;
      v.(x|c) is Element of Funcs(bound_QC-variables,A) by VALUAT_1:def 1;
      then
A6:   ex f st v.(x|c) = f & dom f = bound_QC-variables & rng f c= A by
      FUNCT_2:def 2;
      w is Element of Funcs(bound_QC-variables,A) by VALUAT_1:def 1;
      then ex f st w = f & dom f = bound_QC-variables & rng f c= A by
      FUNCT_2:def 2;
      then v.(x|c) = w by A4,A6,FUNCT_1:9;
      hence thesis by A2;
    end;
    hence thesis by VALUAT_1:41;
  end;
end;

definition
  let S, x, xSQ, A, v;
  func NEx_Val(v,S,x,xSQ) -> Val_Sub of A equals
  (@RestrictSub(x,All(x,S`1),
  xSQ))*v;
  coherence;
end;

definition
  let A;
  let v,w be Val_Sub of A;
  redefine func v+*w -> Val_Sub of A;
  coherence
  proof
    dom (v+*w) = dom v \/ dom w & rng (v+*w) c= A by FUNCT_4:def 1 ;
    hence thesis;
  end;
end;

theorem Th52:
  [S,x] is quantifiable & x in rng RestrictSub(x,All(x,S`1),xSQ)
implies S_Bound(@CQCSub_All([S,x],xSQ)) = x.upVar(RestrictSub(x,All(x,S`1),xSQ)
  ,S`1)
proof
  set S1 = CQCSub_All([S,x],xSQ);
  assume that
A1: [S,x] is quantifiable and
A2: x in rng RestrictSub(x,All(x,S`1),xSQ);
A3: S1 = Sub_All([S,x],xSQ) by A1,Def6;
  then
A4: S1`2 = xSQ by A1,Th27;
A5: S1`1 = All([S,x]`2,([S,x]`1)`1) by A1,A3,Th27;
  then
A6: S1`1 = All(x,([S,x]`1)`1) by MCART_1:7;
  then
A7: S1`1 = All(x,S`1) & x = bound_in S1`1 by MCART_1:7,QC_LANG2:8;
  set finSub = RestrictSub(bound_in S1`1,S1`1,S1`2);
A8: @S1 = S1 by SUBSTUT1:def 35;
  S1`1 = All(x,([S,x]`1)`1) by A5,MCART_1:7;
  then bound_in(S1`1) = x by QC_LANG2:8;
  then bound_in(S1`1) in rng finSub by A2,A4,A6,MCART_1:7;
  then S_Bound(@S1) = x.upVar(finSub,the_scope_of S1`1) by A8, SUBSTUT1:
  def 36;
  hence thesis by A4,A7,QC_LANG2:8;
end;

theorem Th53:
  [S,x] is quantifiable & not x in rng RestrictSub(x,All(x,S`1),
  xSQ) implies S_Bound(@CQCSub_All([S,x],xSQ)) = x
proof
  set S1 = CQCSub_All([S,x],xSQ);
  assume that
A1: [S,x] is quantifiable and
A2: not x in rng RestrictSub(x,All(x,S`1),xSQ);
A3: S1 = Sub_All([S,x],xSQ) by A1,Def6;
  then
A4: S1`1 = All([S,x]`2,([S,x]`1)`1) by A1,Th27;
  then
A5: S1`1 = All(x,([S,x]`1)`1) by MCART_1:7;
  set finSub = RestrictSub(bound_in S1`1,S1`1,S1`2);
A6: @S1 = S1 by SUBSTUT1:def 35;
  S1`1 = All(x,([S,x]`1)`1) by A4,MCART_1:7;
  then
A7: bound_in(S1`1) = x by QC_LANG2:8;
  S1`2 = xSQ by A1,A3,Th27;
  then not bound_in(S1`1) in rng finSub by A2,A7,A5,MCART_1:7;
  hence thesis by A7,A6,SUBSTUT1:def 36;
end;

theorem Th54:
  [S,x] is quantifiable implies for a holds Val_S(v.((S_Bound(@
CQCSub_All([S,x],xSQ)))|a),S) = NEx_Val(v.((S_Bound(@CQCSub_All([S,x],xSQ)))|a)
  ,S,x,xSQ)+*(x|a) & dom RestrictSub(x,All(x,S`1),xSQ) misses {x}
proof
  assume
A1: [S,x] is quantifiable;
  set finSub = RestrictSub(x,All(x,S`1),xSQ);
  let a;
  set S1 = CQCSub_All([S,x],xSQ);
  set z = S_Bound(@S1);
A2: S`2 = ExpandSub(x,S`1,RestrictSub(x,All(x,S`1),xSQ)) by A1,Th42;
A3: now
    reconsider F = {[x,x]} as Function;
A4: dom (x|a) = {x} by FUNCOP_1:19;
    assume
A5: not x in rng finSub;
    then S`2 = finSub \/ F by A2,SUBSTUT1:def 13;
    then
A6: @S`2 = finSub \/ F by SUBSTUT1:def 2;
A7: now
      set q = All(x,S`1);
      set X = {y1 : y1 in still_not-bound_in q & y1 is Element of dom xSQ & y1
      <> x & y1 <> xSQ.y1};
      assume not dom finSub misses {x};
      then dom finSub /\ {x} <> {} by XBOOLE_0:def 7;
      then consider b such that
A8:   b in dom finSub /\ {x} by XBOOLE_0:def 1;
      finSub = xSQ|X by SUBSTUT1:def 6;
      then finSub = (@xSQ)|X by SUBSTUT1:def 2;
      then @finSub = (@xSQ)|X by SUBSTUT1:def 2;
      then dom @finSub = dom @xSQ /\ X by RELAT_1:90;
      then
A9:   dom @finSub c= X by XBOOLE_1:17;
      b in dom finSub by A8,XBOOLE_0:def 4;
      then b in dom @finSub by SUBSTUT1:def 2;
      then b in X by A9;
      then
A10:  ex y st y = b & y in still_not-bound_in q & y is Element of dom xSQ
      & y <> x & y <> xSQ.y;
      b in {x} by A8,XBOOLE_0:def 4;
      hence contradiction by A10,TARSKI:def 1;
    end;
    hence dom finSub misses {x};
    dom {[x,x]} = {x} by RELAT_1:23;
    then dom @finSub misses dom F by A7,SUBSTUT1:def 2;
    then
A11: @finSub \/ F = (@finSub +* F) by FUNCT_4:32;
    v.(z|a) is Element of Funcs(bound_QC-variables,A) by VALUAT_1:def 1;
    then
A12: ex f st v.(z|a) = f & dom f = bound_QC-variables & rng f c= A by
    FUNCT_2:def 2;
A13: rng F = {x} by RELAT_1:23;
    then dom (F*v.(z|a)) = dom F by A12,RELAT_1:46;
    then
A14: dom (F*v.(z|a)) = {x} by RELAT_1:23;
A15: {[x,x]} = x .--> x by FUNCT_4:87;
    for b st b in dom (x|a) holds (x|a).b = (F*v.(z|a)).b
    proof
      let b such that
A16:  b in dom (x|a);
      b in dom (F*v.(z|a)) by A14,A16,FUNCOP_1:19;
      then
A17:  (F*v.(z|a)).b = v.(z|a).(F.b) by FUNCT_1:22;
      b = x by A4,A16,TARSKI:def 1;
      then (x|a).b = a & F.b = x by A15,FUNCOP_1:87;
      hence thesis by A1,A5,A17,Th50,Th53;
    end;
    then
A18: (x|a) = (F*v.(z|a)) by A14,FUNCOP_1:19,FUNCT_1:9;
    (@finSub +* F)* v.(z|a) = (@finSub*v.(z|a)) +* (F*v.(z|a)) by A12,A13
    ,FUNCT_7:10;
    hence Val_S(v.(z|a),S) = NEx_Val(v.(z|a),S,x,xSQ) +* (x|a) by A11,A6,
    A18,SUBSTUT1:def 2;
  end;
  now
    reconsider F = {[x,x.upVar(finSub,S`1)]} as Function;
    assume
A19: x in rng finSub;
A20: now
      set q = All(x,S`1);
      set X = {y1 : y1 in still_not-bound_in q & y1 is Element of dom xSQ & y1
      <> x & y1 <> xSQ.y1};
      assume dom finSub meets {x};
      then consider b such that
A21:  b in dom finSub and
A22:  b in {x} by XBOOLE_0:3;
      finSub = xSQ|X by SUBSTUT1:def 6;
      then finSub = (@xSQ)|X by SUBSTUT1:def 2;
      then @finSub = (@xSQ)|X by SUBSTUT1:def 2;
      then dom @finSub = dom @xSQ /\ X by RELAT_1:90;
      then
A23:  dom @finSub c= X by XBOOLE_1:17;
      b in dom @finSub by A21,SUBSTUT1:def 2;
      then b in X by A23;
      then ex y st y = b & y in still_not-bound_in q & y is Element of dom xSQ
      & y <> x & y <> xSQ.y;
      hence contradiction by A22,TARSKI:def 1;
    end;
    hence dom finSub misses {x};
    dom {[x,x.upVar(finSub,S`1)]} = {x} by RELAT_1:23;
    then dom @finSub misses dom F by A20,SUBSTUT1:def 2;
    then
A24: @finSub \/ F = (@finSub +* F) by FUNCT_4:32;
A25: dom (x|a) = {x} by FUNCOP_1:19;
    v.(z|a) is Element of Funcs(bound_QC-variables,A) by VALUAT_1:def 1;
    then
A26: ex f st v.(z|a) = f & dom f = bound_QC-variables & rng f c= A by
    FUNCT_2:def 2;
    rng F = {x.upVar(finSub,S`1)} by RELAT_1:23;
    then dom (F*v.(z|a)) = dom F by A26,RELAT_1:46;
    then
A27: dom (F*v.(z|a)) = {x} by RELAT_1:23;
A28: {[x,x.upVar(finSub,S`1)]} = x .--> x.upVar(finSub,S`1) by FUNCT_4:87;
    for b st b in dom (x|a) holds (x|a).b = (F*v.(z|a)).b
    proof
      let b such that
A29:  b in dom (x|a);
      b in dom (F*v.(z|a)) by A27,A29,FUNCOP_1:19;
      then
A30:  (F*v.(z|a)).b = (v.(z|a)).(F.b) by FUNCT_1:22;
      b = x by A25,A29,TARSKI:def 1;
      then (x|a).b = a & F.b = x.upVar(finSub,S`1) by A28,FUNCOP_1:87;
      hence thesis by A1,A19,A30,Th50,Th52;
    end;
    then
A31: (x|a) = (F*v.(z|a)) by A27,FUNCOP_1:19,FUNCT_1:9;
    rng F = {x.upVar(finSub,S`1)} by RELAT_1:23;
    then
A32: (@finSub +* F)*v.(z|a) = (@finSub*v.(z|a)) +* (F*v.(z|a)) by A26,
    FUNCT_7:10;
    S`2 = finSub \/ F by A2,A19,SUBSTUT1:def 13;
    then @S`2 = finSub \/ F by SUBSTUT1:def 2;
    hence Val_S(v.(z|a),S) = NEx_Val(v.(z|a),S,x,xSQ) +* (x|a) by A24,A32,
    A31,SUBSTUT1:def 2;
  end;
  hence thesis by A3;
end;

theorem Th55:
  [S,x] is quantifiable implies ((for a holds J,(v.((S_Bound(@
  CQCSub_All([S,x],xSQ)))|a)). Val_S(v.((S_Bound(@CQCSub_All([S,x],xSQ)))|a),S)
|= S) iff for a holds J,(v.((S_Bound(@CQCSub_All([S,x],xSQ)))|a)). (NEx_Val(v.(
  (S_Bound(@CQCSub_All([S,x],xSQ)))|a),S,x,xSQ)+*(x|a)) |= S)
proof
  set S1 = CQCSub_All([S,x],xSQ);
  set z = S_Bound(@S1);
  assume
A1: [S,x] is quantifiable;
  thus (for a holds J,(v.(z|a)).Val_S(v.(z|a),S) |= S) implies for a holds J,(
  v.(z|a)).(NEx_Val(v.(z|a),S,x,xSQ)+*(x|a)) |= S
  proof
    assume
A2: for a holds J,(v.(z|a)).Val_S(v.(z|a),S) |= S;
    let a;
    Val_S(v.(z|a),S) = NEx_Val(v.(z|a),S,x,xSQ)+*(x|a) by A1,Th54;
    hence thesis by A2;
  end;
  thus (for a holds J,(v.(z|a)).(NEx_Val(v.(z|a),S,x,xSQ)+*(x|a)) |= S)
  implies for a holds J,(v.(z|a)).Val_S(v.(z|a),S) |= S
  proof
    assume
A3: for a holds J,(v.(z|a)).(NEx_Val(v.(z|a),S,x,xSQ)+*(x|a)) |= S;
    let a;
    Val_S(v.(z|a),S) = NEx_Val(v.(z|a),S,x,xSQ)+*(x|a) by A1,Th54;
    hence thesis by A3;
  end;
end;

theorem Th56:
  [S,x] is quantifiable implies for a holds NEx_Val(v.((S_Bound(@
  CQCSub_All([S,x],xSQ)))|a),S,x,xSQ) = NEx_Val(v,S,x,xSQ)
proof
  assume
A1: [S,x] is quantifiable;
  set finSub = RestrictSub(x,All(x,S`1),xSQ);
  set NF1 = NEx_Val(v,S,x,xSQ);
  set S1 = CQCSub_All([S,x],xSQ);
  let a;
  set z = S_Bound(@S1);
  set NF = NEx_Val(v.(z|a),S,x,xSQ);
  v is Element of Funcs(bound_QC-variables,A) by VALUAT_1:def 1;
  then ex f st v = f & dom f = bound_QC-variables & rng f c= A by
  FUNCT_2:def
  2;
  then rng @finSub c= dom v;
  then
A2: dom NF1 = dom @finSub by RELAT_1:46;
  v.(z|a) is Element of Funcs(bound_QC-variables,A) by VALUAT_1:def 1;
  then ex f st v.(z|a) = f & dom f = bound_QC-variables & rng f c= A by
  FUNCT_2:def 2;
  then
A3: rng @finSub c= dom (v.(z|a));
  then
A4: dom NF = dom @finSub by RELAT_1:46;
  for b st b in dom NF holds NF.b = NF1.b
  proof
    let b such that
A5: b in dom NF;
A6: @finSub.b in rng @finSub by A4,A5,FUNCT_1:12;
    then reconsider x = @finSub.b as bound_QC-variable;
    not z in rng finSub by A1,Th41;
    then z <> x by A6,SUBSTUT1:def 2;
    then
A7: v.(z|a).x = v.x by Th49;
    NF.b = (v.(z|a)).x by A5,FUNCT_1:22;
    hence thesis by A4,A2,A5,A7,FUNCT_1:22;
  end;
  hence thesis by A3,A2,FUNCT_1:9,RELAT_1:46;
end;

theorem Th57:
  [S,x] is quantifiable implies ((for a holds J,(v.((S_Bound(@
CQCSub_All([S,x],xSQ)))|a)). (NEx_Val(v.((S_Bound(@CQCSub_All([S,x],xSQ)))|a),S
,x,xSQ)+*(x|a)) |= S) iff for a holds J,(v.((S_Bound(@CQCSub_All([S,x],xSQ)))|a
  )). (NEx_Val(v,S,x,xSQ)+*(x|a)) |= S )
proof
  set S1 = CQCSub_All([S,x],xSQ);
  set z = S_Bound(@S1);
  assume
A1: [S,x] is quantifiable;
  thus (for a holds J,(v.(z|a)).(NEx_Val(v.(z|a),S,x,xSQ)+*(x|a)) |= S)
  implies for a holds J,(v.(z|a)).(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S
  proof
    assume
A2: for a holds J,(v.(z|a)).(NEx_Val(v.(z|a),S,x,xSQ)+*(x|a)) |= S;
    let a;
    NEx_Val(v.(z|a),S,x,xSQ) = NEx_Val(v,S,x,xSQ) by A1,Th56;
    hence thesis by A2;
  end;
  thus (for a holds J,(v.(z|a)).(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S) implies for
  a holds J,(v.(z|a)).(NEx_Val(v.(z|a),S,x,xSQ)+*(x|a)) |= S
  proof
    assume
A3: for a holds J,(v.(z|a)).(NEx_Val(v,S,x,xSQ)+*(x|a)) |= S;
    let a;
    NEx_Val(v.(z|a),S,x,xSQ) = NEx_Val(v,S,x,xSQ) by A1,Th56;
    hence thesis by A3;
  end;
end;

begin :: The Coincidence Lemma

theorem Th58:
  rng l1 c= bound_QC-variables implies still_not-bound_in l1 = rng l1
proof
A1: variables_in(l1,bound_QC-variables) = { l1.k : 1 <= k & k <= len l1 & l1
  .k in bound_QC-variables} by QC_LANG3:def 2;
  assume
A2: rng l1 c= bound_QC-variables;
A3: rng l1 c= variables_in(l1,bound_QC-variables)
  proof
    let b;
    assume
A4: b in rng l1;
    then consider k being Nat such that
A5: k in dom l1 and
A6: l1.k = b by FINSEQ_2:11;
    k in Seg len l1 by A5,FINSEQ_1:def 3;
    then 1 <= k & k <= len l1 by FINSEQ_1:3;
    hence thesis by A2,A1,A4,A5,A6;
  end;
  variables_in(l1,bound_QC-variables) c= rng l1
  proof
    let b;
    assume b in variables_in(l1,bound_QC-variables);
    then consider k such that
A7: b = l1.k and
A8: 1 <= k & k <= len l1 and
    l1.k in bound_QC-variables by A1;
    k in Seg len l1 by A8,FINSEQ_1:3;
    then k in dom l1 by FINSEQ_1:def 3;
    hence thesis by A7,FUNCT_1:12;
  end;
  then variables_in(l1,bound_QC-variables) = rng l1 by
  A3,XBOOLE_0:def 10;
  hence thesis by QC_LANG3:6;
end;

theorem Th59:
  dom v = bound_QC-variables & dom (x|a) = {x}
proof
  v is Element of Funcs(bound_QC-variables,A) by VALUAT_1:def 1;
  then ex f st v = f & dom f = bound_QC-variables & rng f c= A by
  FUNCT_2:def
  2;
  hence dom v = bound_QC-variables;
  thus thesis by FUNCOP_1:19;
end;

theorem Th60:
  v*'ll = ll*(v|still_not-bound_in ll)
proof
  rng ll c= bound_QC-variables by RELAT_1:def 19;
  then
A1: rng ll = still_not-bound_in ll by Th58;
  dom (v|still_not-bound_in ll) = dom v /\ still_not-bound_in ll by
  RELAT_1:90;
  then
  dom (v|still_not-bound_in ll) = bound_QC-variables /\ still_not-bound_in
  ll by Th59;
  then rng ll = dom (v|still_not-bound_in ll) by A1,XBOOLE_1:28;
  then
A2: dom (ll*(v|still_not-bound_in ll)) = dom ll by RELAT_1:46;
  then
A3: dom (ll*(v|still_not-bound_in ll)) = Seg len ll by FINSEQ_1:def 3;
  then reconsider f = ll*(v|still_not-bound_in ll) as FinSequence by FINSEQ_1:
  def 2;
  len f = len ll by A3,FINSEQ_1:def 3;
  then
A4: len f = k by SUBSTUT1:34;
  then
A5: dom f = Seg k by FINSEQ_1:def 3;
A6: for j be Nat st j in dom f holds f.j = (v*'ll).j
  proof
A7: rng ll c= bound_QC-variables by RELAT_1:def 19;
    let j be Nat such that
A8: j in dom f;
    reconsider j as Element of NAT by ORDINAL1:def 13;
    ll.j in rng ll by A2,A8,FUNCT_1:12;
    then ll.j in bound_QC-variables by A7;
    then
A9: ll.j in dom v by Th59;
    ll.j in still_not-bound_in ll by A1,A2,A8,FUNCT_1:12;
    then ll.j in dom v /\ still_not-bound_in ll by A9,XBOOLE_0:def 4;
    then
A10: (v|still_not-bound_in ll).(ll.j) = v.(ll.j) by FUNCT_1:71;
    1 <= j & j <= k by A5,A8,FINSEQ_1:3;
    then (v|still_not-bound_in ll).(ll.j) = (v*'ll).j by A10,VALUAT_1:def
    8;
    hence thesis by A2,A8,FUNCT_1:23;
  end;
  len (v*'ll) = k by VALUAT_1:def 8;
  hence thesis by A4,A6,FINSEQ_2:10;
end;

theorem Th61:
  for v,w holds (v|still_not-bound_in (P!ll) = w|
  still_not-bound_in (P!ll) implies (J,v |= P!ll iff J,w |= P!ll))
proof
  let v,w;
  assume
A1: v|still_not-bound_in (P!ll) = w|still_not-bound_in (P!ll);
A2: still_not-bound_in (P!ll) = still_not-bound_in ll by QC_LANG3:9;
A3: w*'ll in J.P iff Valid(P!ll,J).w = TRUE by VALUAT_1:16;
A4: Valid(P!ll,J).v = TRUE iff v*'ll in J.P by VALUAT_1:16;
  ll*(w|still_not-bound_in ll) in J.P iff w*'ll in J.P by Th60;
  hence thesis by A1,A2,A4,A3,Th60,VALUAT_1:def 12;
end;

theorem Th62:
  (for v,w holds v|still_not-bound_in p = w|still_not-bound_in p
  implies (J,v |= p iff J,w |= p)) implies for v,w holds v|still_not-bound_in
'not' p = w|still_not-bound_in 'not' p implies (J,v |= 'not' p iff J,w |= 'not'
  p)
proof
  assume
A1: for v,w holds v|still_not-bound_in p = w|still_not-bound_in p
  implies (J,v |= p iff J,w |= p);
  let v,w;
A2: still_not-bound_in 'not' p = still_not-bound_in p by QC_LANG3:11;
  assume v|still_not-bound_in 'not' p = w|still_not-bound_in 'not' p;
  then not J,v |= p iff not J,w |= p by A1,A2;
  hence thesis by VALUAT_1:28;
end;

theorem Th63:
  (for v,w holds v|still_not-bound_in p = w|still_not-bound_in p
  implies (J,v |= p iff J,w |= p)) & (for v,w holds v|still_not-bound_in q = w|
still_not-bound_in q implies (J,v |= q iff J,w |= q)) implies for v,w holds v|
still_not-bound_in p '&' q = w|still_not-bound_in p '&' q implies (J,v |= p '&'
  q iff J,w |= p '&' q)
proof
  assume
A1: ( for v,w holds v|still_not-bound_in p = w|still_not-bound_in p
  implies ( J,v |= p iff J,w |= p))& for v,w holds v|still_not-bound_in q = w|
  still_not-bound_in q implies (J,v |= q iff J,w |= q);
  set X = (still_not-bound_in p) \/ (still_not-bound_in q);
  let v,w;
A2: still_not-bound_in p '&' q = X by QC_LANG3:14;
  assume v|still_not-bound_in p '&' q = w|still_not-bound_in p '&' q;
  then
  v|still_not-bound_in p = w|still_not-bound_in p & v|still_not-bound_in q
  = w |still_not-bound_in q by A2,RELAT_1:188,XBOOLE_1:7;
  then J,v |= p & J,v |= q iff J,w |= p & J,w |= q by A1;
  hence thesis by VALUAT_1:29;
end;

theorem Th64:
  for X being set st X c= bound_QC-variables holds dom (v|X) = dom
  (v.(x|a)|X) & dom (v|X) = X
proof
  let X be set;
A1: dom (v.(x|a)|X) = dom (v.(x|a)) /\ X by RELAT_1:90;
  dom (v|X) = dom v /\ X by RELAT_1:90;
  then
A2: dom (v|X) = bound_QC-variables /\ X by Th59;
  assume X c= bound_QC-variables;
  hence thesis by A2,A1,Th59,XBOOLE_1:28;
end;

theorem
  v|still_not-bound_in p = w|still_not-bound_in p implies v.(x|a)|
  still_not-bound_in p = w.(x|a)|still_not-bound_in p
proof
  assume
A1: v|still_not-bound_in p = w|still_not-bound_in p;
  dom (v|still_not-bound_in p) = dom (v.(x|a)|still_not-bound_in p) by
  Th64;
  then
A2: dom (v.(x|a)|still_not-bound_in p) = dom (w.(x|a)|still_not-bound_in p)
  by A1,Th64;
  for b st b in dom (v.(x|a)|still_not-bound_in p) holds (v.(x|a)|
  still_not-bound_in p).b = (w.(x|a)|still_not-bound_in p).b
  proof
    let b such that
A3: b in dom (v.(x|a)|still_not-bound_in p);
A4: (v.(x|a)|still_not-bound_in p).b = v.(x|a).b & (w.(x|a)|
    still_not-bound_in p ).b = w.(x|a).b by A2,A3,FUNCT_1:70;
    b in dom (v|still_not-bound_in p) by A3,Th64;
    then
A5: (v|still_not-bound_in p).b = v.b by FUNCT_1:70;
    b in dom (w|still_not-bound_in p) by A1,A3,Th64;
    then
A6: v.b = w.b by A1,A5,FUNCT_1:70;
A7: now
      assume
A8:   b <> x;
      then v.(x|a).b = v.b by Th49;
      hence thesis by A4,A6,A8,Th49;
    end;
    now
      assume
A9:   b = x;
      then v.(x|a).b = a by Th50;
      hence thesis by A4,A9,Th50;
    end;
    hence thesis by A7;
  end;
  hence thesis by A2,FUNCT_1:9;
end;

theorem
  still_not-bound_in p c= still_not-bound_in (All(x,p)) \/ {x}
proof
  set X = (still_not-bound_in p) \ {x};
  still_not-bound_in All(x,p) = X & {x} \/ X = {x} \/ (still_not-bound_in
  p) by QC_LANG3:16,XBOOLE_1:39;
  hence thesis by XBOOLE_1:7;
end;

theorem Th67:
  v|(still_not-bound_in p \ {x}) = w|(still_not-bound_in p \ {x})
  implies v.(x|a)|still_not-bound_in p = w.(x|a)|still_not-bound_in p
proof
A1: dom (w.(x|a)|still_not-bound_in p) = still_not-bound_in p by Th64;
  then
A2: dom (v.(x|a)|still_not-bound_in p) = dom (w.(x|a)|still_not-bound_in p)
  by Th64;
  assume
A3: v|(still_not-bound_in p \ {x}) = w|(still_not-bound_in p \ {x});
  for b st b in dom (v.(x|a)|still_not-bound_in p) holds (v.(x|a)|
  still_not-bound_in p).b = (w.(x|a)|still_not-bound_in p).b
  proof
    let b such that
A4: b in dom (v.(x|a)|still_not-bound_in p);
A5: (v.(x|a)|still_not-bound_in p).b = v.(x|a).b & (w.(x|a)|
    still_not-bound_in p ).b = w.(x|a).b by A2,A4,FUNCT_1:70;
A6: now
      assume
A7:   b <> x;
      then
A8:   not b in {x} by TARSKI:def 1;
      b in still_not-bound_in p by A4,Th64;
      then
A9:   b in still_not-bound_in p \ {x} by A8,XBOOLE_0:def 5;
      then b in dom (w|(still_not-bound_in p \ {x})) by Th64;
      then
A10:  w|(still_not-bound_in p \ {x}).b = w.b by FUNCT_1:70;
A11:  v.(x|a).b = v.b & w.(x|a).b = w.b by A7,Th49;
      b in dom (v|(still_not-bound_in p \ {x})) by A9,Th64;
      hence thesis by A3,A5,A10,A11,FUNCT_1:70;
    end;
    now
A12:  w.(x|a)|(still_not-bound_in p).b = w.(x|a).b by A2,A4,FUNCT_1:70;
      assume
A13:  b = x;
      v.(x|a)|(still_not-bound_in p).b = v.(x|a).b by A4,FUNCT_1:70;
      then v.(x|a)|(still_not-bound_in p).b = a by A13,Th50;
      hence thesis by A13,A12,Th50;
    end;
    hence thesis by A6;
  end;
  hence thesis by A1,Th64,FUNCT_1:9;
end;

theorem Th68:
  (for v,w holds v|still_not-bound_in p = w|still_not-bound_in p
implies (J,v |= p iff J,w |= p)) implies for v,w holds v|still_not-bound_in All
(x,p) = w|still_not-bound_in All(x,p) implies (J,v |= All(x,p) iff J,w |= All(x
  ,p))
proof
  assume
A1: for v,w holds (v|still_not-bound_in p = w|still_not-bound_in p
  implies (J,v |= p iff J,w |= p));
  set X = (still_not-bound_in p) \ {x};
  let v,w;
A2: v|still_not-bound_in All(x,p) = v|X by QC_LANG3:16;
  assume v|still_not-bound_in All(x,p) = w|still_not-bound_in All(x,p);
  then
A3: v|X = w|X by A2,QC_LANG3:16;
A4: (for a holds J,w.(x|a) |= p) implies for a holds J,v.(x|a) |= p
  proof
    assume
A5: for a holds J,w.(x|a) |= p;
    let a;
    v.(x|a)|still_not-bound_in p = w.(x|a)|still_not-bound_in p by A3,
    Th67;
    then J,v.(x|a) |= p iff J,w.(x|a) |= p by A1;
    hence thesis by A5;
  end;
  (for a holds J,v.(x|a) |= p) implies for a holds J,w.(x|a) |= p
  proof
    assume
A6: for a holds J,v.(x|a) |= p;
    let a;
    v.(x|a)|still_not-bound_in p = w.(x|a)|still_not-bound_in p by A3,
    Th67;
    then J,v.(x|a) |= p iff J,w.(x|a) |= p by A1;
    hence thesis by A6;
  end;
  hence thesis by A4,Th51;
end;
 
