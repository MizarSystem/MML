  end;
  cluster AddTo(a,b) -> parahalting;
  coherence
  proof
    set Ma = Macro (AddTo(a,b));
    let s be 0-started State of SCM+FSA;
A11: Start-At(0,SCM+FSA) c= s by MEMSTR_0:29;
  let P be Instruction-Sequence of SCM+FSA
  such that
A12: Ma c= P;
    take 1;
A13: dom P = NAT by PARTFUN1:def 2;
    thus IC Comput(P,s,1) in dom P by A13;
    dom SA0 = {IC SCM+FSA} by FUNCOP_1:13;
    then
A14: IC SCM+FSA in dom SA0 by TARSKI:def 1;
::    then
A15: IC s = SA0.IC SCM+FSA by A14,A11,GRFUNC_1:2
