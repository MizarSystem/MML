            then {p00} <> LSeg(p1,p2) /\ L1 by ZFMISC_1:37;
            then LSeg(p1,p2) /\ L1 = {} by A145,TOPREAL1:23,ZFMISC_1:39;
            hence P1 /\ P2 = {p1} \/ (LSeg(p1,p2) /\ L4) \/ {p2} by A140;
