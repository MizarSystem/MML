      end;
      suppose
A197:   p2 = p10;
        then LSeg(p10,p2) /\ L1 = {p10} /\ L1 by RLTOPSP1:71
          .= {} by Lm1,Lm16;
        hence P1 /\ P2 = {p1} \/ ({p2} \/ {p2}) by A187,A197,TOPREAL1:22,
        XBOOLE_1:4
          .= {p1,p2} by ENUMSET1:41;
      end;
      suppose
A198:   p2 <> p10 & p2 <> p00;
        now
          assume p00 in LSeg(p10,p2) /\ L1;
          then
A199:     p00 in LSeg(p2,p10) by XBOOLE_0:def 4;
          p2`1 <= p10`1 by A184,EUCLID:56;
          then p2`1 = 0 by A184,A199,Lm4,TOPREAL1:9;
          hence contradiction by A184,A198,EUCLID:57;
        end;
        then
A200:   {p00} <> LSeg(p10,p2) /\ L1 by ZFMISC_1:37;
        LSeg(p10,p2) /\ L1 c= L3 /\ L1 by A171,Lm24,TOPREAL1:12,XBOOLE_1
        :26;
        then
A201:   LSeg(p10,p2) /\ L1 = {} by A200,TOPREAL1:23,ZFMISC_1:39;
        now
          assume p10 in L4 /\ LSeg(p00,p2);
          then
A202:     p10 in LSeg(p00,p2) by XBOOLE_0:def 4;
          p00`1 <= p2`1 by A184,EUCLID:56;
          then 1 <= p2`1 by A202,Lm8,TOPREAL1:9;
          then p2`1 = 1 by A184,XXREAL_0:1;
