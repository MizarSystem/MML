A193:   L4 /\ LSeg(p1,p01) = {} by A192,TOPREAL1:24,ZFMISC_1:39;
        now
          assume p01 in LSeg(p1,p11) /\ L1;
          then
A194:     p01 in LSeg(p1,p11) by XBOOLE_0:def 4;
          p1`1 <= p11`1 by A13,A14,EUCLID:56;
          then p1`1 = 0 by A13,A15,A194,Lm6,TOPREAL1:9;
          hence contradiction by A13,A16,A190,EUCLID:57;
        end;
        then
A195:   {p01} <> LSeg(p1,p11) /\ L1 by ZFMISC_1:37;
        LSeg(p1,p11) /\ L1 c= L2 /\ L1 by A3,Lm26,TOPREAL1:12,XBOOLE_1:
        26;
        then LSeg(p1,p11) /\ L1 = {} by A195,TOPREAL1:21,ZFMISC_1:39;
        hence P1 /\ P2 = {p1} \/ (L4 /\ LSeg(p00,p2)) \/ ((LSeg(p10,p2) /\ L1)
