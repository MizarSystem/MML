      \/ ((LSeg(p10,p2) /\ L2) \/ {p2})) by A164;
A166:   now per cases;
      suppose A167: p1 = p00;
 then A168:   LSeg(p10,p2) /\ LSeg(p1,p00) = LSeg(p10,p2) /\
 {p00} by TOPREAL1:7;
          not p00 in LSeg(p10,p2) by A152,Lm4,TOPREAL1:9;
