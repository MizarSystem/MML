  g1" is continuous by A41,TOPS_2:def 5;
  then reconsider
  h = k1+*k2 as continuous Function of T,(TOP-REAL 2)|P by A8,A9,
  A39,A29,A18,A16,A15,A65,A63,A47,A64,A74,A81,A19,A17, COMPTS_1:30;
A82: 1 = (g1").p11 by A43,A48,A51,A58,FUNCT_1:54;
A83: rng(g2") = dom g2 by A60,A62,FUNCT_1:55;
  then
A84: rng k2 = rng f2 by A67,A27,RELAT_1:47
    .= [#] ((TOP-REAL 2)|P2) by A23,TOPS_2:def 5
    .= P2 by PRE_TOPC:def 10;
A85: 0 = (g2").p00 by A45,A60,A62,A68,FUNCT_1:54;
  now
    let x1,x2 be set;
    assume that
A86: x1 in dom k2 and
A87: x2 in dom k1 \ dom k2;
A88: x1 in dom(g2") by A86,FUNCT_1:21;
A89: k2.x1 in P2 by A84,A86,FUNCT_1:def 5;
A90: x2 in dom k1 by A87,XBOOLE_0:def 5;
    then
