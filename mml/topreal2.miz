A53: p1 = f.q1 by A6,A9,A10,A14,FUNCT_1:57;
then A54: p1 = F2.q1 by A52,FUNCT_1:71;
      q2 in {q1,q2} & {q1,q2} c= Q2 by A21,TARSKI:def 2,XBOOLE_1:17;
  then A55: q2 in dom f /\ Q2 by A12,A15,XBOOLE_0:def 3;
A56: p2 = f.q2 by A6,A9,A10,A14,FUNCT_1:57;
   then p2 = F2.q2 by A55,FUNCT_1:71;
  hence P2 is_an_arc_of p1,p2 by A20,A51,A54,Th3;
  thus P = f.:(Q1 \/ Q2) by A8,A13,A19,A21,RELAT_1:146
        .= P1 \/ P2 by RELAT_1:153;
