    hence thesis;
  end;
  suppose
A204: p2 in L4;
    then
A205: LSeg(p11,p2) c= L4 by Lm27,TOPREAL1:12;
    LSeg(p1,p11) c= L2 by A3,Lm26,TOPREAL1:12;
    then
A206: LSeg(p1,p11) /\ LSeg(p11,p2) c= L2 /\ L4 by A205,XBOOLE_1:27;
    take P1 = LSeg(p1,p11) \/ LSeg(p11,p2),P2 = LSeg(p1,p01) \/ (L1 \/ L3 \/
    LSeg(p10,p2));
A207: p11 in LSeg(p11,p2) by RLTOPSP1:69;
    p10 in LSeg(p10,p2) by RLTOPSP1:69;
    then
A208: L3 /\ LSeg(p10,p2) <> {} by Lm24,XBOOLE_0:def 4;
    p11 in LSeg(p1,p11) by RLTOPSP1:69;
    then LSeg(p1,p11) /\ LSeg(p11,p2) <> {} by A207,XBOOLE_0:def 4;
    then
A209: LSeg(p1,p11) /\ LSeg(p11,p2) = {p11} by A206,TOPREAL1:24 ,
    ZFMISC_1:39;
    p1 <> p11 or p11 <> p2 by A1;
    hence P1 is_an_arc_of p1,p2 by A209,TOPREAL1:18;
A210: L2 = LSeg(p1,p11) \/ LSeg(p1,p01) by A3,TOPREAL1:11;
A211: L3 is_an_arc_of p00,p10 by Lm4,Lm8,TOPREAL1:15;
    L1 is_an_arc_of p01,p00 by Lm5,Lm7,TOPREAL1:15;
    then
A212: L1 \/ L3 is_an_arc_of p01,p10 by A211,TOPREAL1:5,23;
A213: LSeg(p11,p2) /\ LSeg(p10,p2) = {p2} by A204,TOPREAL1:14;
A214: L4 = LSeg(p10,p2) \/ LSeg(p11,p2) by A204,TOPREAL1:11;
A215: LSeg(p10,p2) c= L4 by A204,Lm25,TOPREAL1:12;
    then
A216: L3 /\ LSeg(p10,p2) c= {p10} by TOPREAL1:22,XBOOLE_1:27;
A217: ex q st q = p2 & q`1 = 1 & q`2 <= 1 & q`2 >= 0 by A204,TOPREAL1:19;
    now
A218: p10`2 <= p2`2 by A217,EUCLID:56;
      assume
A219: p11 in LSeg(p1,p01) /\ LSeg(p10,p2);
      then
A220: p11 in LSeg(p01,p1) by XBOOLE_0:def 4;
      p11 in LSeg(p10,p2) by A219,XBOOLE_0:def 4;
      then p11`2 <= p2`2 by A218,TOPREAL1:10;
      then
A221: p11`2 = p2`2 by A217,Lm11,XXREAL_0:1;
      p01`1 <= p1`1 by A13,A15,EUCLID:56;
      then p11`1 <= p1`1 by A220,TOPREAL1:9;
      then p11`1 = p1`1 by A13,A14,Lm10,XXREAL_0:1;
      then p1 = |[p11`1,p11`2]| by A13,A16,Lm11,EUCLID:57
        .= p2 by A217,A221,Lm10,EUCLID:57;
      hence contradiction by A1;
    end;
    then
A222: {p11} <> LSeg(p1,p01) /\ LSeg(p10,p2) by ZFMISC_1:37;
A223: L1 /\ LSeg(p10,p2) = {} by A215,Lm3,XBOOLE_1:3,26;
    (L1 \/ L3) /\ LSeg(p10,p2) = (L1 /\ LSeg(p10,p2)) \/ (L3 /\ LSeg(p10
    ,p2)) by XBOOLE_1:23
      .= {p10} by A223,A216,A208,ZFMISC_1:39;
    then
A224: L1 \/ L3 \/ LSeg(p10,p2) is_an_arc_of p01,p2 by A212,TOPREAL1:
    16;
A225: LSeg(p2,p11) c= L4 by A204,Lm27,TOPREAL1:12;
    then
A226: LSeg(p11,p2) /\ L1 = {} by Lm3,XBOOLE_1:3,27;
    LSeg(p1,p01) /\ LSeg(p10,p2) c= {p11} by A11,A215,TOPREAL1:24,
    XBOOLE_1:27;
    then
A227: LSeg(p1,p01) /\ LSeg(p10,p2) = {} by A222,ZFMISC_1:39;
    LSeg(p1,p01) /\ (L1 \/ L3 \/ LSeg(p10,p2)) = (LSeg(p1,p01) /\ (L1 \/
    L3)) \/ (LSeg(p1,p01) /\ LSeg(p10,p2)) by XBOOLE_1:23
      .= (LSeg(p1,p01) /\ L1) \/ (LSeg(p1,p01) /\ L3) by A227,XBOOLE_1:23
      .= {p01} by A12,A7,A5,TOPREAL1:21,ZFMISC_1:39;
    hence P2 is_an_arc_of p1,p2 by A224,TOPREAL1:17;
    thus P1 \/ P2 = LSeg(p11,p2) \/ (LSeg(p1,p11) \/ (LSeg(p1,p01) \/ (L1 \/
    L3 \/ LSeg(p10,p2)))) by XBOOLE_1:4
      .= L2 \/ (L1 \/ L3 \/ LSeg(p10,p2)) \/ LSeg(p11,p2) by A210,XBOOLE_1:4
      .= L2 \/ ((L1 \/ L3 \/ LSeg(p10,p2)) \/ LSeg(p11,p2)) by XBOOLE_1:4
      .= L2 \/ (L1 \/ L3 \/ (LSeg(p10,p2) \/ LSeg(p11,p2))) by XBOOLE_1:4
      .= L2 \/ (L1 \/ (L3 \/ L4)) by A214,XBOOLE_1:4
      .= R^2-unit_square by TOPREAL1:def 4,XBOOLE_1:4;
A228: {p1} = LSeg(p1,p11) /\ LSeg(p1,p01) by A3,TOPREAL1:14;
A229: P1 /\ P2 = (LSeg(p1,p11) /\ (LSeg(p1,p01) \/ (L1 \/ L3 \/ LSeg(p10,
    p2)))) \/ (LSeg(p11,p2) /\ (LSeg(p1,p01) \/ (L1 \/ L3 \/ LSeg(p10,p2)))) by
