        hence P1 /\ P2 = {p1} \/ ({p2} \/ {p2}) by A166,A178,XBOOLE_1:4
          .= {p1,p2} by ENUMSET1:41;
      end;
      suppose
A179:   p2 = p11;
        then
A180:   LSeg(p1,p10) /\ LSeg(p11,p2) = LSeg(p1,p10) /\ {p11} by TOPREAL1:7;
        not p11 in LSeg(p1,p10) by A8,Lm4,TOPREAL1:10;
        then LSeg(p1,p10) /\ LSeg(p11,p2) = {} by A180,Lm1;
        hence P1 /\ P2 = {p1,p2} by A166,A179,ENUMSET1:41,TOPREAL1:24;
      end;
      suppose
A181:   p2 <> p11 & p2 <> p10;
A182:   LSeg(p1,p10) /\ LSeg(p11,p2) c= {p10} by A8,A153,TOPREAL1:22
        ,XBOOLE_1:27;
        now
          assume p10 in LSeg(p1,p10) /\ LSeg(p11,p2);
          then p10 in LSeg(p2,p11) & p2`2 <= p11`2 by A151,EUCLID:56
          ,XBOOLE_0:def 3;
          then p2`2 = 0 by A151,Lm4,TOPREAL1:10;
          hence contradiction by A151,A181,EUCLID:57;
        end;
        then {p10} <> LSeg(p1,p10) /\ LSeg(p11,p2) by ZFMISC_1:37;
        then
A183:   LSeg(p1,p10) /\ LSeg(p11,p2) = {} by A182,ZFMISC_1:39;
A184:   LSeg (p10,p2) /\ L2 c= L4 /\ L2 by A150,Lm18,TOPREAL1:12,XBOOLE_1:26;
        now
          assume p11 in LSeg(p10,p2) /\ L2;
          then p11 in LSeg(p10,p2) & p10`2 <= p2`2 by A151,EUCLID:56
          ,XBOOLE_0:def 3;
