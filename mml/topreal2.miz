    then
A93: k2.x1 in P1 /\ P2 by A89,XBOOLE_0:def 4;
    per cases by A4,A93,TARSKI:def 2;
    suppose
A94:  k2.x1 = p1;
A95:  (g1").x2 in dom ff1 by A90,FUNCT_1:21;
      p1 = ff1.((g1").x2) by A92,A90,A94,FUNCT_1:22;
      then
A96:  (g1").x2 = 0 by A34,A72,A28,A40,A95,FUNCT_1:def 8;
A97:  p00 in dom (g2") by A60,A62,A69,FUNCT_1:54;
A98:  (g2").x1 in dom ff2 by A86,FUNCT_1:21;
      p1 = ff2.((g2").x1) by A86,A94,FUNCT_1:22;
      then (g2").x1 = 0 by A24,A28,A66,A98,FUNCT_1:def 8;
      then
A99:  x1 = p00 by A60,A62,A85,A88,A97,FUNCT_1:def 8;
      p00 in dom(g1") by A42,A53,A28,A56,FUNCT_1:def 5;
      then x2 in dom k2 by A48,A51,A55,A86,A91,A99,A96,FUNCT_1:def 8;
      hence contradiction by A87,XBOOLE_0:def 5;
    end;
    suppose
A100: k2.x1 = p2;
A101: (g1").x2 in dom ff1 by A90,FUNCT_1:21;
      p2 = ff1.((g1").x2) by A92,A90,A100,FUNCT_1:22;
      then
A102: (g1").x2 = 1 by A35,A72,A30,A40,A101,FUNCT_1:def 8;
A103: p11 in dom (g2") by A46,A67,A77,A30,FUNCT_1:def 5;
A104: (g2").x1 in dom ff2 by A86,FUNCT_1:21;
      p2 = ff2.((g2").x1) by A86,A100,FUNCT_1:22;
      then (g2").x1 = 1 by A25,A30,A66,A104,FUNCT_1:def 8;
      then
A105: x1 = p11 by A60,A62,A76,A88,A103,FUNCT_1:def 8;
      p11 in dom(g1") by A43,A53,A56,A30,FUNCT_1:def 5;
      then x2 in dom k2 by A48,A51,A82,A86,A91,A105,A102,FUNCT_1:def
      8;
      hence contradiction by A87,XBOOLE_0:def 5;
    end;
  end;
  then
A106: h is one-to-one by A48,A60,A62,A51,A40,A66,TOPMETR2:2;
A107: (TOP-REAL 2)|P9 is T_2 by TOPMETR:3;
A108: dom k2 = dom(g2") by A27,A83,RELAT_1:46;
  k1.:(dom k1 /\ dom k2) c= rng k2
  proof
    let a be set;
A109: dom k2 = the carrier of T2 by FUNCT_2:def 1;
    assume a in k1.:(dom k1 /\ dom k2);
    then
A110: ex x being set st x in dom k1 & x in dom k1 /\ dom k2 & a = k1.x by
    FUNCT_1:def 12;
    dom k1 = the carrier of T1 by FUNCT_2:def 1;
    then a = p1 or a = p2 by A9,A18,A16,A73,A80,A110,A109,TARSKI:def 2;
    hence thesis by A70,A73,A74,A78,A80,A81,A108,FUNCT_1:def 5;
  end;
  then
A111: rng h = [#]((TOP-REAL 2)|P9) by A3,A31,A52,A84,TOPMETR2:3;
A112: dom h = [#]T by FUNCT_2:def 1;
  reconsider h as Function of ((TOP-REAL 2)|R^2-unit_square),(TOP-REAL 2)|P;
  take h;
  T is compact by Th2,COMPTS_1:12;
  hence thesis by A107,A111,A112,A106,COMPTS_1:26;
end;

theorem Th5:
  for P being non empty Subset of TOP-REAL 2 holds P is
being_simple_closed_curve iff (ex p1,p2 st p1 <> p2 & p1 in P & p2 in P) & for
  p1,p2 st p1 <> p2 & p1 in P & p2 in P ex P1,P2 being non empty Subset of
TOP-REAL 2 st P1 is_an_arc_of p1,p2 & P2 is_an_arc_of p1,p2 & P = P1 \/ P2 & P1
  /\ P2 = {p1,p2}
proof
  let P be non empty Subset of TOP-REAL 2;
  thus P is being_simple_closed_curve implies (ex p1,p2 st p1 <> p2 & p1 in P
& p2 in P) & for p1,p2 st p1 <> p2 & p1 in P & p2 in P ex P1,P2 being non empty
Subset of TOP-REAL 2 st P1 is_an_arc_of p1,p2 & P2 is_an_arc_of p1,p2 & P = P1
  \/ P2 & P1 /\ P2 = {p1,p2}
  proof
    assume
A1: P is being_simple_closed_curve;
    then consider
    f being Function of (TOP-REAL 2)|R^2-unit_square, (TOP-REAL 2)|P
    such that
A2: f is being_homeomorphism by Def1;
A3: dom f = [#]((TOP-REAL 2)|R^2-unit_square) by A2,TOPS_2:def 5;
A4: [#]((TOP-REAL 2)|P) c= [#](TOP-REAL 2) by PRE_TOPC:def 9;
A5: f is continuous by A2,TOPS_2:def 5;
    thus ex p1,p2 st p1 <> p2 & p1 in P & p2 in P by A1,Th4;
    set RS = R^2-unit_square;
    let p1,p2;
    assume that
A6: p1 <> p2 and
A7: p1 in P and
A8: p2 in P;
A9: [#]((TOP-REAL 2)|R^2-unit_square) = R^2-unit_square by PRE_TOPC:def
    10;
    set q1 = (f").p1, q2 = (f").p2;
A10: [#]((TOP-REAL 2)|RS) c= [#](TOP-REAL 2) by PRE_TOPC:def 9;
A11: I[01] is compact by HEINE:11,TOPMETR:27;
A12: f is one-to-one by A2,TOPS_2:def 5;
A13: rng f = [#]((TOP-REAL 2)|P) by A2,TOPS_2:def 5;
    then
A14: f" = (f qua Function)" by A12,TOPS_2:def 4;
    then
A15: rng(f") = dom f by A12,FUNCT_1:55;
A16: dom(f") = rng f by A12,A14,FUNCT_1:54;
    then
A17: p1 in dom(f") by A7,A13,PRE_TOPC:def 10;
A18: p2 in dom(f") by A8,A13,A16,PRE_TOPC:def 10;
    reconsider f as Function of (TOP-REAL 2)|RS, (TOP-REAL 2)|P;
A19: q1 in rng(f") by A17,FUNCT_1:def 5;
A20: q2 in rng(f") by A18,FUNCT_1:def 5;
    rng(f") = [#]((TOP-REAL 2)|RS) by A15,FUNCT_2:def 1;
    then reconsider q1, q2 as Point of TOP-REAL 2 by A10,A19,A20;
A21: q1 <> q2 by A6,A12,A14,A17,A18,FUNCT_1:def 8;
A22: dom f = the carrier of (TOP-REAL 2)|R^2-unit_square by FUNCT_2:def 1;
    then
A23: q2 in R^2-unit_square by A15,A18,A9,FUNCT_1:def 5;
A24: p1 = f.q1 by A12,A14,A16,A17,FUNCT_1:57;
    q1 in R^2-unit_square by A15,A17,A22,A9,FUNCT_1:def 5;
    then consider Q1,Q2 being non empty Subset of TOP-REAL 2 such that
A25: Q1 is_an_arc_of q1,q2 and
A26: Q2 is_an_arc_of q1,q2 and
A27: R^2-unit_square = Q1 \/ Q2 and
A28: Q1 /\ Q2 = {q1,q2} by A21,A23,Th1;
A29: Q2 c= dom f by A22,A9,A27,XBOOLE_1:7;
    set P1 = f.:Q1, P2 = f.:Q2;
    Q1 c= dom f by A22,A9,A27,XBOOLE_1:7;
    then reconsider P1, P2 as non empty Subset of TOP-REAL 2 by A29,A4,
    XBOOLE_1:1;
A30: rng(f|Q1) = P1 by RELAT_1:148
      .= [#]((TOP-REAL 2)|P1) by PRE_TOPC:def 10
      .= the carrier of (TOP-REAL 2)|P1;
A31: dom(f|Q1) = R^2-unit_square /\ Q1 by A22,A9,RELAT_1:90
      .= Q1 by A27,XBOOLE_1:21
      .= [#]((TOP-REAL 2)|Q1) by PRE_TOPC:def 10;
    then reconsider
    F1 = f|Q1 as Function of (TOP-REAL 2)|Q1, (TOP-REAL 2)|P1 by
    A30,FUNCT_2:def 1,RELSET_1:11;
A32: f"P1 c= Q1 by A12,FUNCT_1:152;
    [#]((TOP-REAL 2)|Q1) = Q1 by PRE_TOPC:def 10;
    then
A33: (TOP-REAL 2)|Q1 is SubSpace of (TOP-REAL 2)|R^2-unit_square by A9,
    A27,TOPMETR:4,XBOOLE_1:7;
    Q1 c= f"P1 by A22,A9,A27,FUNCT_1:146,XBOOLE_1:7;
    then
A34: f"P1 = Q1 by A32,XBOOLE_0:def 10;
    for R being Subset of (TOP-REAL 2)|P1 st R is closed holds F1"R is closed
    proof
      let R be Subset of (TOP-REAL 2)|P1;
      assume R is closed;
      then consider S1 being Subset of TOP-REAL 2 such that
A35:  S1 is closed and
A36:  R = S1 /\ [#]((TOP-REAL 2)|P1) by PRE_TOPC:43;
      S1 /\ rng f is Subset of (TOP-REAL 2)|P;
      then reconsider S2 = rng f /\ S1 as Subset of (TOP-REAL 2)|P;
      S2 is closed by A13,A35,PRE_TOPC:43;
      then
A37:  f"S2 is closed by A5,PRE_TOPC:def 12;
      F1"R = Q1 /\ (f"R) by FUNCT_1:139
        .= Q1 /\ ((f"S1) /\ f"([#]((TOP-REAL 2)|P1))) by A36,FUNCT_1:137
        .= (f"S1) /\ Q1 /\ Q1 by A34,PRE_TOPC:def 10
        .= (f"S1) /\ (Q1 /\ Q1) by XBOOLE_1:16
        .= (f"S1) /\ [#]((TOP-REAL 2)|Q1) by PRE_TOPC:def 10
        .= (f"(rng f /\ S1)) /\ [#]((TOP-REAL 2)|Q1) by RELAT_1:168;
      hence thesis by A33,A37,PRE_TOPC:43;
    end;
    then
A38: F1 is continuous by PRE_TOPC:def 12;
    reconsider Q19=Q1, Q29=Q2 as non empty Subset of TOP-REAL 2;
    consider ff being Function of I[01], (TOP-REAL 2)|Q1 such that
A39: ff is being_homeomorphism and
    ff.0 = q1 and
    ff.1 = q2 by A25,TOPREAL1:def 2;
A40: rng ff = [#]((TOP-REAL 2)|Q1) by A39,TOPS_2:def 5;
A41: rng(f|Q2) = P2 by RELAT_1:148
      .= [#]((TOP-REAL 2)|P2) by PRE_TOPC:def 10
      .= the carrier of (TOP-REAL 2)|P2;
A42: p2 = f.q2 by A12,A14,A16,A18,FUNCT_1:57;
A43: dom(f|Q2) = R^2-unit_square /\ Q2 by A22,A9,RELAT_1:90
      .= Q2 by A27,XBOOLE_1:21
      .= [#]((TOP-REAL 2)|Q2) by PRE_TOPC:def 10;
    then reconsider
    F2 = f|Q2 as Function of (TOP-REAL 2)|Q2, (TOP-REAL 2)|P2 by
    A41,FUNCT_2:def 1,RELSET_1:11;
A44: f"P2 c= Q2 by A12,FUNCT_1:152;
    [#]((TOP-REAL 2)|Q2) = Q2 by PRE_TOPC:def 10;
    then
A45: (TOP-REAL 2)|Q2 is SubSpace of (TOP-REAL 2)|R^2-unit_square by A9,
    A27,TOPMETR:4,XBOOLE_1:7;
    Q2 c= f"P2 by A22,A9,A27,FUNCT_1:146,XBOOLE_1:7;
    then
A46: f"P2 = Q2 by A44,XBOOLE_0:def 10;
    for R being Subset of (TOP-REAL 2)|P2 st R is closed holds F2"R is closed
    proof
      let R be Subset of (TOP-REAL 2)|P2;
      assume R is closed;
      then consider S1 being Subset of TOP-REAL 2 such that
A47:  S1 is closed and
A48:  R = S1 /\ [#]((TOP-REAL 2)|P2) by PRE_TOPC:43;
      S1 /\ rng f is Subset of (TOP-REAL 2)|P;
      then reconsider S2 = rng f /\ S1 as Subset of (TOP-REAL 2)|P;
      S2 is closed by A13,A47,PRE_TOPC:43;
      then
A49:  f"S2 is closed by A5,PRE_TOPC:def 12;
      F2"R = Q2 /\ (f"R) by FUNCT_1:139
        .= Q2 /\ ((f"S1) /\ f"([#]((TOP-REAL 2)|P2))) by A48,FUNCT_1:137
        .= (f"S1) /\ Q2 /\ Q2 by A46,PRE_TOPC:def 10
        .= (f"S1) /\ (Q2 /\ Q2) by XBOOLE_1:16
        .= (f"S1) /\ [#]((TOP-REAL 2)|Q2) by PRE_TOPC:def 10
        .= (f"(rng f /\ S1)) /\ [#]((TOP-REAL 2)|Q2) by RELAT_1:168;
      hence thesis by A45,A49,PRE_TOPC:43;
    end;
    then
A50: F2 is continuous by PRE_TOPC:def 12;
A51: q2 in {q1,q2} by TARSKI:def 2;
A52: q1 in {q1,q2} by TARSKI:def 2;
A53: q1 in {q1,q2} by TARSKI:def 2;
    {q1,q2} c= Q1 by A28,XBOOLE_1:17;
    then
A54: q1 in dom f /\ Q1 by A15,A19,A53,XBOOLE_0:def 4;
    take P1,P2;
A55: (TOP-REAL 2)|P1 is T_2 by TOPMETR:3;
A56: q2 in {q1,q2} by TARSKI:def 2;
    {q1,q2} c= Q1 by A28,XBOOLE_1:17;
    then
A57: q2 in dom f /\ Q1 by A15,A20,A56,XBOOLE_0:def 4;
A58: p2 = f.q2 by A12,A14,A16,A18,FUNCT_1:57
      .= F1.q2 by A57,FUNCT_1:71;
A59: rng F1 = [#]((TOP-REAL 2)|P1) by A30;
    ff is continuous by A39,TOPS_2:def 5;
    then
A60: (TOP-REAL 2)|Q19 is compact by A11,A40,COMPTS_1:23;
A61: F1 is one-to-one by A12,FUNCT_1:84;
    p1 = f.q1 by A12,A14,A16,A17,FUNCT_1:57
      .= F1.q1 by A54,FUNCT_1:71;
    hence P1 is_an_arc_of p1,p2 by A25,A31,A59,A61,A38,A60,A55,A58,Th3,
    COMPTS_1:26;
A62: (TOP-REAL 2)|P2 is T_2 by TOPMETR:3;
    consider ff being Function of I[01], (TOP-REAL 2)|Q2 such that
A63: ff is being_homeomorphism and
    ff.0 = q1 and
    ff.1 = q2 by A26,TOPREAL1:def 2;
A64: rng ff = [#]((TOP-REAL 2)|Q2) by A63,TOPS_2:def 5;
    {q1,q2} c= Q2 by A28,XBOOLE_1:17;
    then q1 in dom f /\ Q2 by A15,A19,A52,XBOOLE_0:def 4;
    then
A65: p1 = F2.q1 by A24,FUNCT_1:71;
A66: F2 is one-to-one by A12,FUNCT_1:84;
    {q1,q2} c= Q2 by A28,XBOOLE_1:17;
    then q2 in dom f /\ Q2 by A15,A20,A51,XBOOLE_0:def 4;
    then
A67: p2 = F2.q2 by A42,FUNCT_1:71;
    ff is continuous by A63,TOPS_2:def 5;
    then
A68: (TOP-REAL 2)|Q29 is compact by A11,A64,COMPTS_1:23;
    rng F2 = [#]((TOP-REAL 2)|P2) by A41;
    hence P2 is_an_arc_of p1,p2 by A26,A43,A66,A50,A68,A62,A65,A67,Th3,
    COMPTS_1:26;
    [#]((TOP-REAL 2)|P) = P by PRE_TOPC:def 10;
    hence P = f.:(Q1 \/ Q2) by A13,A3,A9,A27,RELAT_1:146
      .= P1 \/ P2 by RELAT_1:153;
    thus P1 /\ P2 = f.:(Q1 /\ Q2) by A12,FUNCT_1:121
      .= f.:({q1} \/ {q2}) by A28,ENUMSET1:41
      .= Im(f,q1) \/ Im(f,q2) by RELAT_1:153
      .= {p1} \/ Im(f,q2) by A15,A19,A24,FUNCT_1:117
      .= {p1} \/ {p2} by A15,A20,A42,FUNCT_1:117
      .= {p1,p2} by ENUMSET1:41;
  end;
  given p1,p2 such that
A69: p1 <> p2 and
A70: p1 in P and
A71: p2 in P;
  assume for p1,p2 st p1 <> p2 & p1 in P & p2 in P ex P1,P2 being non empty
Subset of TOP-REAL 2 st P1 is_an_arc_of p1,p2 & P2 is_an_arc_of p1,p2 & P = P1
  \/ P2 & P1 /\ P2 = {p1,p2};
  then ex P1,P2 being non empty Subset of TOP-REAL 2 st P1 is_an_arc_of p1,p2
  & P2 is_an_arc_of p1,p2 & P = P1 \/ P2 & P1 /\ P2 = {p1,p2} by A69,A70,A71;
  hence thesis by Lm34;
end;

theorem
  for P being non empty Subset of TOP-REAL 2 holds P is
  being_simple_closed_curve iff ex p1,p2 being Point of TOP-REAL 2, P1,P2 being
  non empty Subset of TOP-REAL 2 st p1 <> p2 & p1 in P & p2 in P & P1
is_an_arc_of p1,p2 & P2 is_an_arc_of p1,p2 & P = P1 \/ P2 & P1 /\ P2 = {p1,p2}
proof
  let P be non empty Subset of TOP-REAL 2;
  hereby
    assume
A1: P is being_simple_closed_curve;
    then consider p1,p2 such that
A2: p1 <> p2 and
A3: p1 in P and
A4: p2 in P by Th5;
    consider P1,P2 being non empty Subset of TOP-REAL 2 such that
A5: P1 is_an_arc_of p1,p2 and
A6: P2 is_an_arc_of p1,p2 and
A7: P = P1 \/ P2 and
A8: P1 /\ P2 = {p1,p2} by A1,A2,A3,A4,Th5;
    take p1,p2,P1,P2;
    thus p1 <> p2 & p1 in P & p2 in P & P1 is_an_arc_of p1,p2 & P2
is_an_arc_of p1,p2 & P = P1 \/ P2 & P1 /\ P2 = {p1,p2} by A2,A3,A4,A5,A6,A7,A8;
  end;
  thus thesis by Lm34;
end;

Lm35: for S being 1-sorted, T being 1-sorted, f being Function of S, T st S is
empty & rng f = [#]T holds T is empty
proof
  let S be 1-sorted, T be 1-sorted, f be Function of S, T such that
A1: S is empty and
A2: rng f = [#]T;
  assume T is non empty;
  then reconsider T as non empty 1-sorted;
  consider y being set such that
A3: y in the carrier of T by XBOOLE_0:def 1;
  ex x being set st x in dom f & f.x = y by A2,A3,FUNCT_1:def 5;
  hence contradiction by A1;
end;

Lm36: for S being 1-sorted, T being 1-sorted, f being Function of S, T st T is
empty & dom f = [#]S holds S is empty
proof
  let S be 1-sorted, T be 1-sorted, f be Function of S, T such that
A1: T is empty and
A2: dom f = [#]S;
  assume S is non empty;
  then reconsider S as non empty 1-sorted;
  consider x being set such that
A3: x in the carrier of S by XBOOLE_0:def 1;
  f.x in rng f by A2,A3,FUNCT_1:def 5;
  hence thesis by A1;
end;

Lm37: for S, T being TopStruct st ex f being Function of S,T st f is
being_homeomorphism holds S is empty iff T is empty
proof
  let S, T be TopStruct;
  given f being Function of S, T such that
A1: f is being_homeomorphism;
A2: dom f = [#]S by A1,TOPS_2:def 5;
  rng f = [#]T by A1,TOPS_2:def 5;
  hence thesis by A2,Lm35,Lm36;
end;

registration
  cluster being_simple_closed_curve -> non empty compact Subset of TOP-REAL 2;
  coherence
  proof
    let P be Subset of TOP-REAL 2;
    given f being Function of (TOP-REAL 2)|R^2-unit_square, (TOP-REAL 2)|P
    such that
A1: f is being_homeomorphism;
    thus P is non empty by A1,Lm37;
A2: rng f = [#]((TOP-REAL 2)|P) by A1,TOPS_2:def 5;
    reconsider R = P as non empty Subset of TOP-REAL 2 by A1,Lm37;
A3: f is continuous by A1,TOPS_2:def 5;
    (TOP-REAL 2)|R^2-unit_square is compact by Th2,COMPTS_1:12;
    then (TOP-REAL 2)|R is compact by A3,A2,COMPTS_1:23;
    hence thesis by COMPTS_1:12;
  end;
end;
