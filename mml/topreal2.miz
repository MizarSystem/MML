        LSeg(p1,p10) /\ LSeg(p11,p2) c= {p10} by A12,A223,TOPREAL1:22 ,
        XBOOLE_1:27;
        then LSeg(p1,p10) /\ LSeg(p11,p2) = {} by A254,ZFMISC_1:39;
        hence thesis by A231,A252,ENUMSET1:41;
      end;
    end;
    hence thesis;
  end;
end;

Lm33: p1 <> p2 & p2 in R^2-unit_square & p1 in LSeg(p10, p11) implies ex P1,P2
being non empty Subset of TOP-REAL 2 st P1 is_an_arc_of p1,p2 & P2 is_an_arc_of
p1,p2 & R^2-unit_square = P1 \/ P2 & P1 /\ P2 = {p1,p2}
proof
  assume that
A1: p1 <> p2 and
A2: p2 in R^2-unit_square and
A3: p1 in LSeg(p10, p11);
A4: p2 in L1 \/ L2 or p2 in L3 \/ L4 by A2,TOPREAL1:def 4,XBOOLE_0:def 3;
A5: LSeg(p1,p11) c= L4 by A3,Lm27,TOPREAL1:12;
  p11 in LSeg(p1,p11) by RLTOPSP1:69;
  then
A6: {} <> LSeg(p1,p11) /\ L2 by Lm26,XBOOLE_0:def 4;
  p10 in LSeg(p1,p10) by RLTOPSP1:69;
  then
A7: {} <> LSeg(p1,p10) /\ L3 by Lm24,XBOOLE_0:def 4;
A8: LSeg(p1,p11) /\ L2 c= L4 /\ L2 by A3,Lm27,TOPREAL1:12,XBOOLE_1:26;
A9: LSeg(p1,p10) /\ L3 c= L4 /\ L3 by A3,Lm25,TOPREAL1:12,XBOOLE_1:26;
  L1 /\ L4 = {} by TOPREAL1:26,XBOOLE_0:def 7;
  then
A10: LSeg(p1,p11) /\ L1 = {} by A5,XBOOLE_1:3,26;
A11: LSeg(p1,p10) c= L4 by A3,Lm25,TOPREAL1:12;
  L1 /\ L4 = {} by TOPREAL1:26,XBOOLE_0:def 7;
  then
A12: LSeg(p10,p1) /\ L1 = {} by A11,XBOOLE_1:3,26;
  consider p such that
A13: p = p1 and
A14: p`1 = 1 and
A15: p`2 <= 1 and
A16: p`2 >= 0 by A3,TOPREAL1:19;
  per cases by A4,XBOOLE_0:def 3;
  suppose
A17: p2 in L1;
    p00 in LSeg(p00,p2) by RLTOPSP1:69;
    then
A18: L3 /\ LSeg(p00,p2) <> {} by Lm21,XBOOLE_0:def 4;
    L3 /\ LSeg(p00,p2) c= L3 /\ L1 by A17,Lm20,TOPREAL1:12,XBOOLE_1:26;
    then L3 /\ LSeg(p00,p2) = {p00} by A18,TOPREAL1:23,ZFMISC_1:39;
    then
A19: L3 \/ LSeg(p00,p2) is_an_arc_of p10,p2 by
    Lm4,Lm8,TOPREAL1:15,16;
A20: LSeg(p2,p00) c= L1 by A17,Lm20,TOPREAL1:12;
    then
A21: LSeg(p1,p10) /\ LSeg(p00,p2) = {} by A11,Lm3,XBOOLE_1:3,27;
A22: LSeg(p2,p01) c= L1 by A17,Lm22,TOPREAL1:12;
    then
A23: LSeg(p1,p11) /\ LSeg(p01,p2) = {} by A5,Lm3,XBOOLE_1:3,27;
    L1 /\ L4 = {} by TOPREAL1:26,XBOOLE_0:def 7;
    then
A24: LSeg(p1,p11) /\ LSeg(p00,p2) = {} by A5,A20,XBOOLE_1:3,27;
    L1 /\ L4 = {} by TOPREAL1:26,XBOOLE_0:def 7;
    then
A25: LSeg(p01,p2) /\ LSeg(p1,p10) = {} by A11,A22,XBOOLE_1:3,27;
A26: LSeg(p01,p2) /\ LSeg(p00,p2) = {p2} by A17,TOPREAL1:14;
    p01 in LSeg(p01,p2) by RLTOPSP1:69;
    then
A27: p01 in L2 /\ LSeg(p01,p2) by Lm23,XBOOLE_0:def 4;
    L2 /\ LSeg(p01,p2) c= L2 /\ L1 by A17,Lm22,TOPREAL1:12,XBOOLE_1:26;
    then L2 /\ LSeg(p01,p2) = {p01} by A27,TOPREAL1:21,ZFMISC_1:39;
    then
A28: L2 \/ LSeg(p01,p2) is_an_arc_of p11,p2 by
    Lm6,Lm10,TOPREAL1:15,16;
    take P1 = LSeg(p1,p11) \/ L2 \/ LSeg(p01,p2),P2 = LSeg(p1,p10) \/ L3 \/
    LSeg(p00,p2);
A29: LSeg(p1,p11) \/ LSeg(p1,p10) = L4 by A3,TOPREAL1:11;
    LSeg(p1,p11) /\ (L2 \/ LSeg(p01,p2)) = (LSeg(p1,p11) /\ L2) \/ (LSeg(
    p1,p11) /\ LSeg(p01,p2)) by XBOOLE_1:23
      .= {p11} by A8,A6,A23,TOPREAL1:24,ZFMISC_1:39;
    then LSeg(p1,p11) \/ (L2 \/ LSeg(p01,p2)) is_an_arc_of p1,p2 by A28,
    TOPREAL1:17;
    hence P1 is_an_arc_of p1,p2 by XBOOLE_1:4;
A30: ex q st q = p2 & q`1 = 0 & q`2 <= 1 & q`2 >= 0 by A17,TOPREAL1:19;
    LSeg(p1,p10) /\ (L3 \/ LSeg(p00,p2)) = (LSeg(p1,p10) /\ L3) \/ (LSeg(
    p1,p10) /\ LSeg(p00,p2)) by XBOOLE_1:23
      .= {p10} by A9,A7,A21,TOPREAL1:22,ZFMISC_1:39;
    then LSeg(p1,p10) \/ (L3 \/ LSeg(p00,p2)) is_an_arc_of p1,p2 by A19,
    TOPREAL1:17;
    hence P2 is_an_arc_of p1,p2 by XBOOLE_1:4;
    thus R^2-unit_square = LSeg(p00,p2) \/ LSeg(p01,p2) \/ L2 \/ (L3 \/ L4) by
    A17,TOPREAL1:11,def 4
      .= LSeg(p00,p2) \/ (LSeg(p01,p2) \/ L2) \/ (L3 \/ L4) by XBOOLE_1:4
      .= L2 \/ LSeg(p01,p2) \/ (L4 \/ L3 \/ LSeg(p00,p2)) by XBOOLE_1:4
      .= L2 \/ LSeg(p01,p2) \/ (L4 \/ (L3 \/ LSeg(p00,p2))) by XBOOLE_1:4
      .= L2 \/ LSeg(p01,p2) \/ (LSeg(p1,p11) \/ (LSeg(p1,p10) \/ (L3 \/ LSeg
    (p00,p2)))) by A29,XBOOLE_1:4
      .= L2 \/ LSeg(p01,p2) \/ (LSeg(p1,p11) \/ (LSeg(p1,p10) \/ L3 \/ LSeg(
    p00,p2))) by XBOOLE_1:4
      .= (LSeg(p1,p11) \/ (L2 \/ LSeg(p01,p2))) \/ (LSeg(p1,p10) \/ L3 \/
    LSeg(p00,p2)) by XBOOLE_1:4
      .= P1 \/ P2 by XBOOLE_1:4;
A31: LSeg(p1,p11) /\ LSeg(p1,p10) = {p1} by A3,TOPREAL1:14;
A32: P1 /\ P2 = ((LSeg(p1,p11) \/ L2) /\ (LSeg(p1,p10) \/ L3 \/ LSeg(p00,
p2))) \/ (LSeg(p01,p2) /\ (LSeg(p1,p10) \/ L3 \/ LSeg(p00,p2))) by XBOOLE_1:23
      .= ((LSeg(p1,p11) \/ L2) /\ (LSeg(p1,p10) \/ L3 \/ LSeg(p00,p2))) \/ (
    (LSeg(p01,p2) /\ (LSeg(p1,p10) \/ L3)) \/ {p2}) by A26,XBOOLE_1:23
      .= ((LSeg(p1,p11) \/ L2) /\ (LSeg(p1,p10) \/ L3 \/ LSeg(p00,p2))) \/ (
(LSeg(p01,p2) /\ LSeg(p1,p10)) \/ (LSeg(p01,p2) /\ L3) \/ {p2}) by XBOOLE_1:23
      .= ((LSeg(p1,p11) /\ (LSeg(p1,p10) \/ L3 \/ LSeg(p00,p2))) \/ (L2 /\ (
LSeg(p1,p10) \/ L3 \/ LSeg(p00,p2)))) \/ ((LSeg(p01,p2) /\ L3) \/ {p2}) by A25,
    XBOOLE_1:23
      .= (LSeg(p1,p11) /\ (LSeg(p1,p10) \/ L3 \/ LSeg(p00,p2))) \/ ((L2 /\ (
LSeg(p1,p10) \/ L3)) \/ (L2 /\ LSeg(p00,p2))) \/ ((LSeg(p01,p2) /\ L3) \/ {p2})
    by XBOOLE_1:23
      .= (LSeg(p1,p11) /\ (LSeg(p1,p10) \/ L3 \/ LSeg(p00,p2))) \/ ((L2 /\
LSeg(p1,p10)) \/ (L3 /\ L2) \/ (L2 /\ LSeg(p00,p2))) \/ ((LSeg(p01,p2) /\ L3)
    \/ {p2}) by XBOOLE_1:23
      .= ((LSeg(p1,p11) /\ (LSeg(p1,p10) \/ L3)) \/ (LSeg(p1,p11) /\ LSeg(
p00,p2))) \/ ((L2 /\ LSeg(p1,p10)) \/ (L2 /\ LSeg(p00,p2))) \/ ((LSeg(p01,p2)
    /\ L3) \/ {p2}) by Lm2,XBOOLE_1:23
      .= {p1} \/ (LSeg(p1,p11) /\ L3) \/ ((L2 /\ LSeg(p1,p10)) \/ (L2 /\
    LSeg(p00,p2))) \/ ((LSeg(p01,p2) /\ L3) \/ {p2}) by A24,A31,XBOOLE_1:23;
A33: now
      per cases;
      suppose
A34:    p1 = p10;
        then L2 /\ LSeg(p1,p10) = L2 /\ {p10} by RLTOPSP1:71;
        then L2 /\ LSeg(p1,p10) = {} by Lm1,Lm17;
        hence P1 /\ P2 = {p1} \/ (L2 /\ LSeg(p00,p2)) \/ ((LSeg(p01,p2) /\ L3)
        \/ {p2}) by A32,A34,TOPREAL1:22;
      end;
      suppose
A35:    p1 = p11;
        then LSeg(p1,p11) /\ L3 = {p11} /\ L3 by RLTOPSP1:71;
        then LSeg(p1,p11) /\ L3 = {} by Lm1,Lm19;
        hence
        P1 /\ P2 = {p1} \/ {p1} \/ (L2 /\ LSeg(p00,p2)) \/ ((LSeg(p01,p2)
        /\ L3) \/ {p2}) by A32,A35,TOPREAL1:24,XBOOLE_1:4
          .= {p1} \/ (L2 /\ LSeg(p00,p2)) \/ ((LSeg(p01,p2) /\ L3) \/ {p2});
      end;
      suppose
A36:    p1 <> p11 & p1 <> p10;
        now
          assume p11 in L2 /\ LSeg(p1,p10);
          then
A37:      p11 in LSeg(p10,p1) by XBOOLE_0:def 4;
          p10`2 <= p1`2 by A13,A16,EUCLID:56;
          then p11`2 <= p1`2 by A37,TOPREAL1:10;
          then 1 = p1`2 by A13,A15,Lm11,XXREAL_0:1;
          hence contradiction by A13,A14,A36,EUCLID:57;
        end;
        then
A38:    {p11} <> L2 /\ LSeg(p1,p10) by ZFMISC_1:37;
        L2 /\ LSeg(p1,p10) c= {p11} by A3,Lm25,TOPREAL1:12,24,XBOOLE_1:26;
        then
A39:    L2 /\ LSeg(p1,p10) = {} by A38,ZFMISC_1:39;
        now
          assume p10 in LSeg(p1,p11) /\ L3;
          then
A40:      p10 in LSeg(p1,p11) by XBOOLE_0:def 4;
          p1`2 <= p11`2 by A13,A15,EUCLID:56;
          then p1`2 = 0 by A13,A16,A40,Lm9,TOPREAL1:10;
          hence contradiction by A13,A14,A36,EUCLID:57;
        end;
        then
A41:    {p10} <> LSeg(p1,p11) /\ L3 by ZFMISC_1:37;
        LSeg (p1,p11) /\ L3 c= L4 /\ L3 by A3,Lm27,TOPREAL1:12,XBOOLE_1:
        26;
        then LSeg(p1,p11) /\ L3 = {} by A41,TOPREAL1:22,ZFMISC_1:39;
        hence P1 /\ P2 = {p1} \/ (L2 /\ LSeg(p00,p2)) \/ ((LSeg(p01,p2) /\ L3)
        \/ {p2}) by A32,A39;
      end;
    end;
    now
      per cases;
      suppose
A42:    p2 = p00;
        then L2 /\ LSeg(p00,p2) = L2 /\ {p00} by RLTOPSP1:71;
        then L2 /\ LSeg(p00,p2) = {} by Lm1,Lm13;
        hence thesis by A33,A42,ENUMSET1:41,TOPREAL1:23;
      end;
      suppose
A43:    p2 = p01;
        then LSeg(p01,p2) /\ L3 = {p01} /\ L3 by RLTOPSP1:71;
        then LSeg(p01,p2) /\ L3 = {} by Lm1,Lm14;
        hence P1 /\ P2 = {p1} \/ ({p2} \/ {p2}) by A33,A43,TOPREAL1:21 ,
        XBOOLE_1:4
          .= {p1,p2} by ENUMSET1:41;
      end;
      suppose
A44:    p2 <> p01 & p2 <> p00;
        now
          assume p00 in LSeg(p01,p2) /\ L3;
          then
A45:      p00 in LSeg(p2,p01) by XBOOLE_0:def 4;
          p2`2 <= p01`2 by A30,EUCLID:56;
          then 0 = p2`2 by A30,A45,Lm5,TOPREAL1:10;
          hence contradiction by A30,A44,EUCLID:57;
        end;
        then
A46:    {p00} <> LSeg(p01,p2) /\ L3 by ZFMISC_1:37;
        LSeg(p01,p2) /\ L3 c= {p00} by A17,Lm22,TOPREAL1:12,23,XBOOLE_1:
        26;
        then
A47:    LSeg(p01,p2) /\ L3 = {} by A46,ZFMISC_1:39;
        now
          assume p01 in L2 /\ LSeg(p00,p2);
          then
A48:      p01 in LSeg(p00,p2) by XBOOLE_0:def 4;
          p00`2 <= p2`2 by A30,EUCLID:56;
          then p01`2 <= p2`2 by A48,TOPREAL1:10;
          then p2`2 = 1 by A30,Lm7,XXREAL_0:1;
          hence contradiction by A30,A44,EUCLID:57;
        end;
        then
A49:    {p01} <> L2 /\ LSeg(p00,p2) by ZFMISC_1:37;
        L2 /\ LSeg(p00,p2) c= L2 /\ L1 by A17,Lm20,TOPREAL1:12,XBOOLE_1:
        26;
        then L2 /\ LSeg(p00,p2) = {} by A49,TOPREAL1:21,ZFMISC_1:39;
        hence thesis by A33,A47,ENUMSET1:41;
      end;
    end;
    hence thesis;
  end;
  suppose
A50: p2 in L2;
    then
A51: ex q st q = p2 & q`1 <= 1 & q`1 >= 0 & q`2 = 1 by TOPREAL1:19;
    now
A52:  p01`1 <= p2`1 by A51,EUCLID:56;
      assume
A53:  p11 in LSeg(p1,p10) /\ LSeg(p01,p2);
      then
A54:  p11 in LSeg(p10,p1) by XBOOLE_0:def 4;
      p11 in LSeg(p01,p2) by A53,XBOOLE_0:def 4;
      then p11`1 <= p2`1 by A52,TOPREAL1:9;
      then
A55:  1 = p2`1 by A51,Lm10,XXREAL_0:1;
      p10`2 <= p1`2 by A13,A16,EUCLID:56;
      then p11`2 <= p1`2 by A54,TOPREAL1:10;
      then 1 = p1`2 by A13,A15,Lm11,XXREAL_0:1;
      then p1 = p11 by A13,A14,EUCLID:57
        .= p2 by A51,A55,EUCLID:57;
      hence contradiction by A1;
    end;
    then
A56: {p11} <> LSeg(p1,p10) /\ LSeg(p01,p2) by ZFMISC_1:37;
A57: L1 is_an_arc_of p00,p01 by Lm5,Lm7,TOPREAL1:15;
    L3 is_an_arc_of p10,p00 by Lm4,Lm8,TOPREAL1:15;
    then
A58: L3 \/ L1 is_an_arc_of p10,p01 by A57,TOPREAL1:5,23;
    L1 /\ L4 = {} by TOPREAL1:26,XBOOLE_0:def 7;
    then
A59: LSeg(p1,p11) /\ L1 = {} by A5,XBOOLE_1:3,26;
    take P1 = LSeg(p1,p11) \/ LSeg(p11,p2),P2 = LSeg(p1,p10) \/ (L3 \/ L1 \/
    LSeg(p01,p2));
A60: LSeg(p1,p11) \/ LSeg(p1,p10) = L4 by A3,TOPREAL1:11;
    p01 in LSeg(p01,p2) by RLTOPSP1:69;
    then
A61: L1 /\ LSeg(p01,p2) <> {} by Lm22,XBOOLE_0:def 4;
A62: p11 in LSeg(p11,p2) by RLTOPSP1:69;
    p11 in LSeg(p1,p11) by RLTOPSP1:69;
    then
A63: p11 in LSeg(p1,p11) /\ LSeg(p11,p2) by A62,XBOOLE_0:def 4;
A64: LSeg(p11,p2) c= L2 by A50,Lm26,TOPREAL1:12;
    then LSeg(p1,p11) /\ LSeg(p11,p2) c= L4 /\ L2 by A5,XBOOLE_1:27;
    then
A65: LSeg(p1,p11) /\ LSeg(p11,p2) = {p11} by A63,TOPREAL1:24 ,
    ZFMISC_1:39;
    p1 <> p11 or p11 <> p2 by A1;
    hence P1 is_an_arc_of p1,p2 by A65,TOPREAL1:18;
A66: {p1} = LSeg(p1,p11) /\ LSeg(p1,p10) by A3,TOPREAL1:14;
    L3 /\ L2 = {} by TOPREAL1:25,XBOOLE_0:def 7;
    then
A67: LSeg(p11,p2) /\ L3 = {} by A64,XBOOLE_1:3,26;
A68: LSeg(p2,p01) c= L2 by A50,Lm23,TOPREAL1:12;
    then
A69: L1 /\ LSeg(p01,p2) c= {p01} by TOPREAL1:21,XBOOLE_1:27;
A70: L3 /\ LSeg(p01,p2) = {} by A68,Lm2,XBOOLE_1:3,26;
    (L3 \/ L1) /\ LSeg(p01,p2) = (L3 /\ LSeg(p01,p2)) \/ (L1 /\ LSeg(p01
    ,p2)) by XBOOLE_1:23
      .= {p01} by A70,A69,A61,ZFMISC_1:39;
    then
A71: L3 \/ L1 \/ LSeg(p01,p2) is_an_arc_of p10,p2 by A58,TOPREAL1:
    16;
A72: {p2} = LSeg(p11,p2) /\ LSeg(p01,p2) by A50,TOPREAL1:14;
A73: LSeg(p01,p2) \/ LSeg(p11,p2) = L2 by A50,TOPREAL1:11;
    LSeg(p1,p10) /\ LSeg(p01,p2) c= L4 /\ L2 by A11,A68,XBOOLE_1:27;
    then
A74: LSeg(p1,p10) /\ LSeg(p01,p2) = {} by A56,TOPREAL1:24 ,ZFMISC_1
    :39;
    LSeg(p1,p10) /\ (L3 \/ L1 \/ LSeg(p01,p2)) = (LSeg(p1,p10) /\ (L3 \/
    L1)) \/ (LSeg(p1,p10) /\ LSeg(p01,p2)) by XBOOLE_1:23
      .= (LSeg(p1,p10) /\ L3) \/ (LSeg(p10,p1) /\ L1) by A74,XBOOLE_1:23
      .= {p10} by A9,A7,A12,TOPREAL1:22,ZFMISC_1:39;
    hence P2 is_an_arc_of p1,p2 by A71,TOPREAL1:17;
    thus P1 \/ P2 = LSeg(p11,p2) \/ (LSeg(p1,p11) \/ (LSeg(p1,p10) \/ (L3 \/
    L1 \/ LSeg(p01,p2)))) by XBOOLE_1:4
      .= LSeg(p11,p2) \/ (L4 \/ (L3 \/ L1 \/ LSeg(p01,p2))) by A60,XBOOLE_1
    :4
      .= LSeg(p11,p2) \/ (L4 \/ (L3 \/ L1) \/ LSeg(p01,p2)) by XBOOLE_1:4
      .= LSeg(p11,p2) \/ (L3 \/ L4 \/ L1 \/ LSeg(p01,p2)) by XBOOLE_1:4
      .= LSeg(p11,p2) \/ (L3 \/ L4 \/ (L1 \/ LSeg(p01,p2))) by XBOOLE_1:4
      .= (L1 \/ LSeg(p01,p2) \/ LSeg(p11,p2)) \/ (L3 \/ L4) by XBOOLE_1:4
      .= R^2-unit_square by A73,TOPREAL1:def 4,XBOOLE_1:4;
A75: P1 /\ P2 = (LSeg(p1,p11) /\ (LSeg(p1,p10) \/ (L3 \/ L1 \/ LSeg(p01,
    p2)))) \/ (LSeg(p11,p2) /\ (LSeg(p1,p10) \/ (L3 \/ L1 \/ LSeg(p01,p2)))) by
    XBOOLE_1:23
      .= (LSeg(p1,p11) /\ LSeg(p1,p10)) \/ (LSeg(p1,p11) /\ (L3 \/ L1 \/
LSeg(p01,p2))) \/ (LSeg(p11,p2) /\ (LSeg(p1,p10) \/ (L3 \/ L1 \/ LSeg(p01,p2)))
    ) by XBOOLE_1:23
      .= {p1} \/ ((LSeg(p1,p11) /\ (L3 \/ L1)) \/ (LSeg(p1,p11) /\ LSeg(p01,
p2))) \/ (LSeg(p11,p2) /\ (LSeg(p1,p10) \/ (L3 \/ L1 \/ LSeg(p01,p2)))) by A66
    ,XBOOLE_1:23
      .= {p1} \/ ((LSeg(p1,p11) /\ L3) \/ (LSeg(p1,p11) /\ L1) \/ (LSeg(p1,
p11) /\ LSeg(p01,p2))) \/ (LSeg(p11,p2) /\ (LSeg(p1,p10) \/ (L3 \/ L1 \/ LSeg(
    p01,p2)))) by XBOOLE_1:23
      .= {p1} \/ ((LSeg(p1,p11) /\ L3) \/ (LSeg(p1,p11) /\ LSeg(p01,p2))) \/
((LSeg(p11,p2) /\ LSeg(p1,p10)) \/ (LSeg(p11,p2) /\ (L3 \/ L1 \/ LSeg(p01,p2)))
    ) by A59,XBOOLE_1:23
      .= {p1} \/ ((LSeg(p1,p11) /\ L3) \/ (LSeg(p1,p11) /\ LSeg(p01,p2))) \/
