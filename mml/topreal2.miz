:: The Topological Space ${\calE}^2_{\rm T}$. Simple Closed Curves
::  by Agata Darmochwa{\l} and Yatsuka Nakamura
::
:: Received December 30, 1991
:: Copyright (c) 1991 Association of Mizar Users

environ

 vocabularies PRE_TOPC, EUCLID, FINSEQ_1, TOPREAL1, MCART_1, BOOLE, COMPTS_1,
      BORSUK_1, RELAT_1, TOPS_2, ORDINAL2, SUBSET_1, FUNCT_1, RCOMP_1, FUNCT_4,
      TOPREAL2, FINSEQ_4, COMPLEX1, ARYTM;
 notations TARSKI, XBOOLE_0, SUBSET_1, RELAT_1, FUNCT_1, RELSET_1, PARTFUN1,
      FUNCT_4, NUMBERS, FINSEQ_1, STRUCT_0, PRE_TOPC, TOPS_2, COMPTS_1,
      RCOMP_1, EUCLID, TOPMETR, TOPREAL1, XXREAL_0;
 constructors FUNCT_4, RCOMP_1, TOPS_2, COMPTS_1, TOPMETR, TOPREAL1;
 registrations XBOOLE_0, FUNCT_1, RELSET_1, FUNCT_2, XXREAL_0, MEMBERED,
      STRUCT_0, PRE_TOPC, BORSUK_1, EUCLID, TOPREAL1;
 requirements NUMERALS, REAL, BOOLE, SUBSET;
 definitions TARSKI, PRE_TOPC, TOPS_2, STRUCT_0, RELAT_1;
 theorems BORSUK_1, COMPTS_1, ENUMSET1, EUCLID, FUNCT_1, FUNCT_2, HEINE,
      PRE_TOPC, RCOMP_1, REAL_1, TARSKI, TOPMETR, TOPMETR2, TOPS_2, ZFMISC_1,
      TOPREAL1, RELAT_1, RELSET_1, XBOOLE_0, XBOOLE_1, STRUCT_0, XXREAL_0;

begin

reserve a for set;
reserve p,p1,p2,q,q1,q2 for Point of TOP-REAL 2;
reserve h1,h2 for FinSequence of TOP-REAL 2;

Lm1: now
  let x,X be set;
  assume not x in X;
  then {x} misses X by ZFMISC_1:56;
  hence {x} /\ X = {} by XBOOLE_0:def 7;
end;

Lm2: LSeg(|[0,0]|,|[1,0]|) /\ LSeg(|[0,1]|,|[1,1]|) = {}
by TOPREAL1:25,XBOOLE_0:def 7;

Lm3: LSeg(|[0,0]|,|[0,1]|) /\ LSeg(|[1,0]|,|[1,1]|) = {}
by TOPREAL1:26,XBOOLE_0:def 7;
set p00 = |[ 0,0 ]|, p01 = |[ 0,1 ]|, p10 = |[ 1,0 ]|, p11 = |[ 1,1 ]|,
L1 = LSeg(p00,p01),L2 = LSeg(p01,p11),L3 = LSeg(p00,p10),L4 = LSeg(p10,p11);

Lm4: p00`1 = 0 & p00`2 = 0 & p01`1 = 0 & p01`2 = 1 & p10`1 = 1 & p10`2 = 0 &
p11`1 = 1 & p11`2 = 1 by EUCLID:56;
then

Lm5: not p00 in L4 by TOPREAL1:9;

Lm6: not p00 in L2 by Lm4,TOPREAL1:10;

Lm7: not p01 in L3 by Lm4,TOPREAL1:10;

Lm8: not p01 in L4 by Lm4,TOPREAL1:9;

Lm9: not p10 in L1 by Lm4,TOPREAL1:9;

Lm10: not p10 in L2 by Lm4,TOPREAL1:10;

Lm11: not p11 in L1 by Lm4,TOPREAL1:9;

Lm12: not p11 in L3 by Lm4,TOPREAL1:10;

Lm13: p00 in L1 by TOPREAL1:6;

Lm14: p00 in L3 by TOPREAL1:6;

Lm15: p01 in L1 by TOPREAL1:6;

Lm16: p01 in L2 by TOPREAL1:6;

Lm17: p10 in L3 by TOPREAL1:6;

Lm18: p10 in L4 by TOPREAL1:6;

Lm19: p11 in L2 by TOPREAL1:6;

Lm20: p11 in L4 by TOPREAL1:6;
set L = { p : p`1 = 0 & p`2 <= 1 & p`2 >= 0 or
p`1 <= 1 & p`1 >= 0 & p`2 = 1 or p`1 <= 1 & p`1 >= 0 & p`2 = 0 or
p`1 = 1 & p`2 <= 1 & p`2 >= 0 };

Lm21: p00 in L by Lm4;

Lm22: p11 in L by Lm4;

Lm23: p1 <> p2 & p2 in R^2-unit_square & p1 in LSeg(p00, p01) implies
ex P1,P2 being non empty Subset of TOP-REAL 2
st P1 is_an_arc_of p1,p2 & P2 is_an_arc_of p1,p2 &
R^2-unit_square = P1 \/ P2 & P1 /\ P2 = {p1,p2}
proof
  assume that
A1: p1 <> p2 and
A2: p2 in R^2-unit_square and
A3: p1 in LSeg(p00, p01);
A4: p2 in L1 \/ L2 or p2 in L3 \/ L4 by A2,TOPREAL1:def 4,XBOOLE_0:def 2;
  consider p such that
A5: p = p1 and
A6: p`1 = 0 & p`2 <= 1 & p`2 >= 0 by A3,TOPREAL1:19;
A7: LSeg(p00,p1) c= L1 by A3,Lm13,TOPREAL1:12;
A8: LSeg(p1,p01) c= L1 by A3,Lm15,TOPREAL1:12;
A9: L1 /\ L4 = {} by TOPREAL1:26,XBOOLE_0:def 7;
  then
A10: LSeg(p1,p00) /\ L4 = {} by A7,XBOOLE_1:3,26;
  p00 in LSeg(p1,p00) by TOPREAL1:6;
  then p00 in LSeg(p1,p00) /\ L3 by Lm14,XBOOLE_0:def 3;
  then
A11: {p00} c= LSeg(p1,p00) /\ L3 by ZFMISC_1:37;
A12: LSeg(p1,p00) /\ L3 c= L1 /\ L3 & L1 /\ L3 = {p00}
  by A3,Lm13,TOPREAL1:12,23,XBOOLE_1:26;
  then
A13: LSeg(p1,p00) /\ L3 = {p00} by A11,XBOOLE_0:def 10;
A14: LSeg(p01,p1) /\ L4 = {} by A8,A9,XBOOLE_1:3,26;
  p01 in LSeg(p01,p1) by TOPREAL1:6;
  then p01 in LSeg(p01,p1) /\ L2 by Lm16,XBOOLE_0:def 3;
  then
A15: {p01} c= LSeg(p01,p1) /\ L2 by ZFMISC_1:37;
A16: LSeg(p01,p1) /\ L2 c= {p01} by A3,Lm15,TOPREAL1:12,21,XBOOLE_1:26;
  per cases by A4,XBOOLE_0:def 2;
  suppose
A17: p2 in L1;
    then consider q such that
A18: q = p2 and
A19: q`1 = 0 & q`2 <= 1 & q`2 >= 0 by TOPREAL1:19;
A20: LSeg(p2,p1) c= L1 by A3,A17,TOPREAL1:12;
A21: p = |[p`1,p`2]| & q = |[q`1,q`2]| by EUCLID:57;
    now per cases by A1,A5,A6,A18,A19,A21,REAL_1:def 5;
      case
A22:    p`2 < q`2;
A23:    LSeg(p01,p2) c= L1 by A17,Lm15,TOPREAL1:12;
A24:    LSeg(p01,p2) /\ L3 c= {p00} by A17,Lm15,TOPREAL1:12,23,XBOOLE_1:26;
        set P1 = LSeg(p1,p2),
        P2 = LSeg(p1,p00) \/ (L3 \/ L4 \/ LSeg(p11,p01) \/ LSeg(p01,p2));
        now
          assume p00 in LSeg(p01,p2) /\ L3;
          then p00 in
          LSeg(p2,p01) & p2`2 <= p01`2 by A18,A19,EUCLID:56,XBOOLE_0:def 3;
          hence contradiction by A6,A18,A22,Lm4,TOPREAL1:10;
        end;
        then {p00} <> LSeg(p01,p2) /\ L3 by ZFMISC_1:37;
        then
A25:    LSeg(p01,p2) /\ L3 = {} by A24,ZFMISC_1:39;
A26:    now
          assume
A27:      LSeg(p1,p00) /\ LSeg(p01,p2) <> {};
          consider a being Element of LSeg(p1,p00) /\ LSeg(p01,p2);
          reconsider p = a as Point of TOP-REAL 2 by A27,TARSKI:def 3;
          p in LSeg(p00,p1) & p in LSeg(p2,p01) & p00`2 <= p1`2 &
          p2`2 <= p01`2 by A5,A6,A18,A19,A27,EUCLID:56,XBOOLE_0:def 3;
          then p`2 <= p1`2 & p2`2 <= p`2 by TOPREAL1:10;
          hence contradiction by A5,A18,A22,XXREAL_0:2;
        end;
        p01 in LSeg(p01,p2) by TOPREAL1:6;
        then p01 in LSeg(p01,p2) /\ L2 by Lm16,XBOOLE_0:def 3;
        then
A28:    {p01} c= LSeg(p01,p2) /\ L2 by ZFMISC_1:37;
A29:    LSeg(p01,p2) /\ L2 c= L1 /\ L2 & L1 /\ L2 = {p01}
        by A17,Lm15,TOPREAL1:12,21,XBOOLE_1:26;
A30:    LSeg(p1,p00) /\ L2 c= {p01} by A3,Lm13,TOPREAL1:12,21,XBOOLE_1:26;
        now
          assume p01 in LSeg(p1,p00) /\ L2;
          then p01 in LSeg(p00,p1) & p00`2 <=
          p1`2 by A5,A6,EUCLID:56,XBOOLE_0:def 3;
          then p01`2 <= p1`2 by TOPREAL1:10;
          hence contradiction by A5,A6,A19,A22,Lm4,XXREAL_0:1;
        end;
        then
A31:    {p01} <> LSeg(p1,p00) /\ L2 by ZFMISC_1:37;
A32:    LSeg(p01,p2) \/ LSeg(p2,p1) \/
        LSeg(p1,p00) = L1 by A3,A17,TOPREAL1:13;
A33:    LSeg(p1,p2) c= L1 by A3,A17,TOPREAL1:12;
        p1 in LSeg(p1,p2) & p1 in LSeg(p1,p00) by TOPREAL1:6;
        then p1 in LSeg(p1,p2) /\ LSeg(p1,p00) by XBOOLE_0:def 3;
        then
A34:    {p1} c= LSeg(p1,p2) /\ LSeg(p1,p00) by ZFMISC_1:37;
        LSeg(p1,p2) /\ LSeg(p1,p00) c= {p1}
        proof
          let a;
          assume
A35:      a in LSeg(p1,p2) /\ LSeg(p1,p00);
          then reconsider p = a as Point of TOP-REAL 2;
          p in LSeg(p1,p2) & p in LSeg(p00,p1) & p1`2 <= p2`2 & p1`1 <= p2 `1
          & p00`2 <= p1`2 by A5,A6,A18,A19,A22,A35,EUCLID:56,XBOOLE_0:def 3;
          then p1`1 <= p`1 & p`1 <= p2`1 & p1`2 <= p`2 & p`2 <= p1`2
          by TOPREAL1:9,10;
          then p1`2 = p`2 & p`1 = 0 by A5,A6,A18,A19,XXREAL_0:1;
          then p = |[ 0, p1`2]| by EUCLID:57
            .= p1 by A5,A6,EUCLID:57;
          hence a in {p1} by TARSKI:def 1;
        end;
        then
A36:    LSeg(p1,p2) /\ LSeg(p1,p00) = {p1} by A34,XBOOLE_0:def 10;
        p2 in LSeg(p1,p2) & p2 in LSeg(p01,p2) by TOPREAL1:6;
        then p2 in LSeg(p1,p2) /\ LSeg(p01,p2) by XBOOLE_0:def 3;
        then
A37:    {p2} c= LSeg(p1,p2) /\ LSeg(p01,p2) by ZFMISC_1:37;
        LSeg(p1,p2) /\ LSeg(p01,p2) c= {p2}
        proof
          let a;
          assume
A38:      a in LSeg(p1,p2) /\ LSeg(p01,p2);
          then reconsider p = a as Point of TOP-REAL 2;
          p in LSeg(p1,p2) & p in LSeg(p2,p01) & p1`2 <= p2`2 & p1`1 <= p2 `1
          & p2`2 <= p01`2 by A5,A6,A18,A19,A22,A38,EUCLID:56,XBOOLE_0:def 3;
          then p1`1 <= p`1 & p`1 <= p2`1 & p2`2 <= p`2 & p`2 <= p2`2
          by TOPREAL1:9,10;
          then p2`2 = p`2 & p`1 = 0 by A5,A6,A18,A19,XXREAL_0:1;
          then p = |[0,p2`2]| by EUCLID:57
            .= p2 by A18,A19,EUCLID:57;
          hence a in {p2} by TARSKI:def 1;
        end;
        then
A39:    LSeg(p1,p2) /\ LSeg(p01,p2) = {p2} by A37,XBOOLE_0:def 10;
        thus P1 is_an_arc_of p1,p2 by A1,TOPREAL1:15;
A40:    L3 \/ L4 is_an_arc_of p00,p11 by Lm4,TOPREAL1:18,22;
        (L3 \/ L4) /\ LSeg(p11,p01)
        = {} \/ {p11} by Lm2,TOPREAL1:24,XBOOLE_1:23
          .= {p11};
        then
A41:    L3 \/ L4 \/ LSeg(p11,p01) is_an_arc_of p00,p01 by A40,TOPREAL1:16;
        (L3 \/ L4 \/ LSeg(p11,p01)) /\ LSeg(p01,p2)
        = LSeg(p01,p2) /\ (L3 \/ L4) \/ (LSeg(p01,p2) /\
        LSeg(p11,p01)) by XBOOLE_1:23
          .= {}

\/ (LSeg(p01,p2) /\ L4) \/ (LSeg(p01,p2) /\ LSeg(p11,p01)) by A25,XBOOLE_1:23
          .= {} \/ (LSeg(p01,p2) /\ LSeg(p11,p01)) by A23,Lm3,XBOOLE_1:3,26
          .= {p01} by A28,A29,XBOOLE_0:def 10;
        then
A42:    L3 \/ L4 \/ LSeg(p11,p01) \/ LSeg(p01,p2) is_an_arc_of p00,p2
        by A41,TOPREAL1:16;
        LSeg(p1,p00) /\ (L3 \/ L4 \/ LSeg(p11,p01) \/ LSeg(p01,p2))
        = LSeg(p1,p00) /\ (L3 \/ L4 \/ LSeg(p11,p01))
        \/ LSeg(p1,p00) /\ LSeg(p01,p2) by XBOOLE_1:23
          .= LSeg(p1,p00) /\ (L3 \/ L4) \/ LSeg(p1,p00) /\ LSeg(p11,p01)
        \/ LSeg(p1,p00) /\ LSeg(p01,p2) by XBOOLE_1:23
          .= (LSeg(p1,p00) /\ L3) \/ (LSeg(p1,p00) /\ L4)
        \/ (LSeg(p1,p00) /\ LSeg(p11,p01))
        \/ (LSeg(p1,p00) /\ LSeg(p01,p2)) by XBOOLE_1:23
          .= {p00} by A10,A13,A26,A30,A31,ZFMISC_1:39;
        hence LSeg(p1,p00) \/ (L3 \/ L4 \/ LSeg(p11,p01) \/ LSeg(p01,p2))
        is_an_arc_of p1,p2 by A42,TOPREAL1:17;
        thus R^2-unit_square =
        LSeg(p1,p2) \/ LSeg(p01,p2) \/ LSeg(p1,p00) \/ (L3 \/ L4 \/
        LSeg(p11,p01)) by A32,TOPREAL1:def 4,XBOOLE_1:4
          .= LSeg(p1,p2) \/ (LSeg(p1,p00) \/ LSeg(p01,p2))
        \/ (L3 \/ L4 \/ LSeg(p11,p01)) by XBOOLE_1:4
          .= LSeg(p1,p2) \/ ((LSeg(p1,p00) \/ LSeg(p01,p2))
        \/ (L3 \/ L4 \/ LSeg(p11,p01))) by XBOOLE_1:4
          .= P1 \/ P2 by XBOOLE_1:4;
A43:    P1 /\ P2 = (LSeg(p1,p2) /\ LSeg(p1,p00))
        \/ LSeg(p1,p2) /\ (L3 \/ L4 \/ LSeg(p11,p01) \/
        LSeg(p01,p2)) by XBOOLE_1:23
          .= (LSeg(p1,p2) /\ LSeg(p1,p00))
        \/ ((LSeg(p1,p2) /\ (L3 \/ L4 \/ LSeg(p11,p01)))
        \/ (LSeg(p1,p2) /\ LSeg(p01,p2))) by XBOOLE_1:23
          .= (LSeg(p1,p2) /\ LSeg(p1,p00)) \/ ((LSeg(p1,p2) /\ (L3 \/ L4))
        \/ (LSeg(p1,p2) /\ LSeg(p11,p01))
        \/ (LSeg(p1,p2) /\ LSeg(p01,p2))) by XBOOLE_1:23
          .= {p1} \/ ((LSeg(p1,p2) /\ L3) \/ (LSeg(p1,p2) /\ L4)
        \/ (LSeg(p1,p2) /\ LSeg(p11,p01))
        \/ (LSeg(p1,p2) /\ LSeg(p01,p2))) by A36,XBOOLE_1:23

.= {p1} \/ ((LSeg(p1,p2) /\ L3) \/ {} \/ (LSeg(p1,p2) /\ LSeg(p11,p01))
        \/ (LSeg(p1,p2) /\ LSeg(p01,p2))) by A33,Lm3,XBOOLE_1:3,26

.= {p1} \/ ((LSeg(p1,p2) /\ L3) \/ ((LSeg(p1,p2) /\ L2) \/ {p2})) by A39,
        XBOOLE_1:4
          .= {p1} \/ (LSeg(p1,p2) /\ L3) \/ ((LSeg(p1,p2) /\ L2) \/
        {p2}) by XBOOLE_1:4;
A44:    LSeg(p1,p2) /\ L3 c= {p00} by A3,A17,TOPREAL1:12,23,XBOOLE_1:26;
A45:    now per cases;
          suppose
A46:        p1 = p00;
            then p00 in LSeg(p1,p2) by TOPREAL1:6;
            then LSeg(p1,p2) /\ L3 <> {} by Lm14,XBOOLE_0:def 3;
            then LSeg(p1,p2) /\ L3 = {p1} by A44,A46,ZFMISC_1:39;
            hence P1 /\ P2 = {p1} \/ ((LSeg(p1,p2) /\ L2) \/ {p2}) by A43;
          end;
          suppose
A47:        p1 <> p00;
            now
              assume p00 in LSeg(p1,p2) /\ L3;
              then p00 in LSeg(p1,p2) & p1`2 <= p2`2 & p00`2 <= p01`2
              by A5,A18,A22,Lm4,XBOOLE_0:def 3;
              then p00`2 <= p1`2 & p1`2 <= p00`2 by A3,TOPREAL1:10;
              then p00`2 = p1`2 by XXREAL_0:1;
              hence contradiction by A5,A6,A47,Lm4,EUCLID:57;
            end;
            then LSeg(p1,p2) /\ L3 <> {p00} by ZFMISC_1:37;
            then LSeg(p1,p2) /\ L3 = {} by A44,ZFMISC_1:39;
            hence P1 /\ P2 = {p1} \/ ((LSeg(p1,p2) /\ L2) \/ {p2}) by A43;
          end;
        end;
A48:    LSeg(p1,p2) /\ L2 c= {p01} by A3,A17,TOPREAL1:12,21,XBOOLE_1:26;
        now per cases;
          suppose
A49:        p2 <> p01;
            now
              assume p01 in LSeg(p1,p2) /\ L2;
              then p01 in LSeg(p1,p2) & p1`2 <= p2`2 & p00`2 <= p01`2
              by A5,A18,A22,Lm4,XBOOLE_0:def 3;
              then p01`2 <= p2`2 & p2`2 <= p01`2 by A17,TOPREAL1:10;
              then
A50:          p01`2 = p2`2 by XXREAL_0:1;
              p2 = |[p2`1,p2`2]| by EUCLID:57
                .= |[0,1]| by A18,A19,A50,EUCLID:56;
              hence contradiction by A49;
            end;
            then LSeg(p1,p2) /\ L2 <> {p01} by ZFMISC_1:37;
            then LSeg(p1,p2) /\ L2 = {} by A48,ZFMISC_1:39;
            hence P1 /\ P2 = {p1,p2} by A45,ENUMSET1:41;
          end;
          suppose
A51:        p2 = p01;
            then p01 in LSeg(p1,p2) by TOPREAL1:6;
            then LSeg(p1,p2) /\ L2 <> {} by Lm16,XBOOLE_0:def 3;
            then LSeg(p1,p2) /\ L2 = {p2} by A48,A51,ZFMISC_1:39;
            hence P1 /\ P2 = {p1,p2} by A45,ENUMSET1:41;
          end;
        end;
        hence P1 /\ P2 = {p1,p2};
      end;
      case
A52:    p`2 > q`2;
        take P1 = LSeg(p2,p1),P2
        = LSeg(p2,p00) \/ (L3 \/ L4 \/ LSeg(p11,p01) \/ LSeg(p01,p1));
A53:    LSeg(p01,p1) /\ L3 c= {p00} by A3,Lm15,TOPREAL1:12,23,XBOOLE_1:26;
        now
          assume p00 in LSeg(p01,p1) /\ L3;
          then p00 in LSeg(p1,p01) & p1`2 <=
          p01`2 by A5,A6,EUCLID:56,XBOOLE_0:def 3;
          hence contradiction by A5,A19,A52,Lm4,TOPREAL1:10;
        end;
        then
A54:    {p00} <> LSeg(p01,p1) /\ L3 by ZFMISC_1:37;
A55:    LSeg(p2,p00) c= L1 by A17,Lm13,TOPREAL1:12;
A56:    now
          assume
A57:      LSeg(p2,p00) /\ LSeg(p01,p1) <> {};
          consider a being Element of LSeg(p2,p00) /\ LSeg(p01,p1);
          reconsider p = a as Point of TOP-REAL 2 by A57,TARSKI:def 3;
          p in LSeg(p00,p2) & p in LSeg(p1,p01) & p00`2 <= p2`2 &
          p1`2 <= p01`2 by A5,A6,A18,A19,A57,EUCLID:56,XBOOLE_0:def 3;
          then p`2 <= p2`2 & p1`2 <= p`2 by TOPREAL1:10;
          hence contradiction by A5,A18,A52,XXREAL_0:2;
        end;
        p00 in LSeg(p2,p00) by TOPREAL1:6;
        then p00 in LSeg(p2,p00) /\ L3 by Lm14,XBOOLE_0:def 3;
        then
A58:    {p00} c= LSeg(p2,p00) /\ L3 by ZFMISC_1:37;
A59:    LSeg(p2,p00) /\ L3 c= {p00} by A17,Lm13,TOPREAL1:12,23,XBOOLE_1:26;
A60:    LSeg(p2,p00) /\ L2 c= {p01} by A17,Lm13,TOPREAL1:12,21,XBOOLE_1:26;
        now
          assume p01 in LSeg(p2,p00) /\ L2;
          then p01 in
          LSeg(p00,p2) & p00`2 <= p2`2 by A18,A19,EUCLID:56,XBOOLE_0:def 3;
          then p01`2 <= p2`2 by TOPREAL1:10;
          hence contradiction by A6,A18,A19,A52,Lm4,XXREAL_0:1;
        end;
        then
A61:    {p01} <> LSeg(p2,p00) /\ L2 by ZFMISC_1:37;
A62:    LSeg(p01,p1) \/ LSeg(p1,p2) \/
        LSeg(p2,p00) = L1 by A3,A17,TOPREAL1:13;
        p2 in LSeg(p2,p1) & p2 in LSeg(p2,p00) by TOPREAL1:6;
