    now per cases;
      suppose
A176:   p2 = p10;
        then
A177:   LSeg(p1,p10) /\ LSeg(p11,p2) c= {p2} by A8,TOPREAL1:22,XBOOLE_1:27;
        p10 in LSeg(p1,p10) by TOPREAL1:6;
