    then A167: LSeg(p1,p00) /\ LSeg(p1,p01) = {p1} by A164,XBOOLE_0:def 10;
            LSeg(p1,p00) /\ LSeg(p11,p2) c= L1 /\ L4 by A7,A161,XBOOLE_1:27;
