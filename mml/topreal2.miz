A75: 1 in dom g2 by A67,BORSUK_1:83,XXREAL_1:1;
  then
A76: 1 = (g2").p11 by A46,A60,A62,FUNCT_1:54;
A77: dom(g2") = rng g2 by A60,A62,FUNCT_1:54;
  then
A78: p11 in dom (g2") by A46,A75,FUNCT_1:def 5;
  (g2").p11 in dom ff2 by A46,A60,A62,A53,A67,A27,A58,FUNCT_1:54;
  then
A79: p11 in dom(ff2*(g2")) by A78,FUNCT_1:21;
  (g1").p11 in dom ff1 by A43,A48,A51,A53,A72,A58,FUNCT_1:54;
