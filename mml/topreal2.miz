          then 1 = p2`2 by A151,Lm4,XXREAL_0:1;
          hence contradiction by A151,A181,EUCLID:57;
        end;
        then {p11} <> LSeg(p10,p2) /\ L2 by ZFMISC_1:37;
        then LSeg(p10,p2) /\ L2 = {} by A184,TOPREAL1:24,ZFMISC_1:39;
        hence P1 /\ P2 = {p1,p2} by A166,A183,ENUMSET1:41;
      end;
    end;
    hence P1 /\ P2 = {p1,p2};
  end;
end;

Lm26: p1 <> p2 & p2 in R^2-unit_square & p1 in LSeg(p10, p11) implies
ex P1,P2 being non empty Subset of TOP-REAL 2
st P1 is_an_arc_of p1,p2 & P2 is_an_arc_of p1,p2 &
R^2-unit_square = P1 \/ P2 & P1 /\ P2 = {p1,p2}
proof
  assume that
A1: p1 <> p2 and
A2: p2 in R^2-unit_square and
A3: p1 in LSeg(p10, p11);
A4: p2 in L1 \/ L2 or p2 in L3 \/ L4 by A2,TOPREAL1:def 4,XBOOLE_0:def 2;
  consider p such that
A5: p = p1 and
A6: p`1 = 1 & p`2 <= 1 & p`2 >= 0 by A3,TOPREAL1:19;
A7: LSeg(p1,p11) c= L4 by A3,Lm20,TOPREAL1:12;
A8: LSeg(p1,p10) c= L4 by A3,Lm18,TOPREAL1:12;
A9: LSeg(p1,p10) /\ L3 c= L4 /\ L3 by A3,Lm18,TOPREAL1:12,XBOOLE_1:26;
  p10 in LSeg(p1,p10) by TOPREAL1:6;
  then
A10: {} <> LSeg(p1,p10) /\ L3 by Lm17,XBOOLE_0:def 3;
A11: LSeg(p1,p11) /\ L2 c= L4 /\ L2 by A3,Lm20,TOPREAL1:12,XBOOLE_1:26;
  p11 in LSeg(p1,p11) by TOPREAL1:6;
  then
A12: {} <> LSeg(p1,p11) /\ L2 by Lm19,XBOOLE_0:def 3;
  LSeg(p10,p1) /\ L1 c= L4 /\ L1 & L1 /\ L4 = {}
  by A8,TOPREAL1:26,XBOOLE_0:def 7,XBOOLE_1:26;
  then
A13: LSeg(p10,p1) /\ L1 = {} by XBOOLE_1:3;
  LSeg(p1,p11) /\ L1 c= L4 /\ L1 & L1 /\ L4 = {}
  by A7,TOPREAL1:26,XBOOLE_0:def 7,XBOOLE_1:26;
  then
A14: LSeg(p1,p11) /\ L1 = {} by XBOOLE_1:3;
  per cases by A4,XBOOLE_0:def 2;
  suppose
A15: p2 in L1;
    then
A16: ex q st q = p2 & q`1 = 0 & q`2 <= 1 & q`2 >= 0 by TOPREAL1:19;
    take P1 = LSeg(p1,p11) \/ L2 \/ LSeg(p01,p2),P2
    = LSeg(p1,p10) \/ L3 \/ LSeg(p00,p2);
A17: LSeg(p2,p01) c= L1 by A15,Lm15,TOPREAL1:12;
A18: LSeg(p2,p00) c= L1 by A15,Lm13,TOPREAL1:12;
    p01 in LSeg(p01,p2) by TOPREAL1:6;
    then L2 /\ LSeg(p01,p2) c= L2 /\ L1 &
    p01 in L2 /\ LSeg(p01,p2) by A17,Lm16,XBOOLE_0:def 3,XBOOLE_1:26;
    then L2 /\ LSeg(p01,p2) = {p01} by TOPREAL1:21,ZFMISC_1:39;
    then
A19: L2 \/ LSeg(p01,p2) is_an_arc_of p11,p2 by Lm4,TOPREAL1:15,16;
A20: LSeg(p1,p11) /\ LSeg(p01,p2) = {} by A7,A17,Lm3,XBOOLE_1:3,27;
    LSeg(p1,p11) /\ (L2 \/ LSeg(p01,p2))
    = (LSeg(p1,p11) /\ L2) \/ (LSeg(p1,p11) /\ LSeg(p01,p2)) by XBOOLE_1:23
      .= {p11} by A11,A12,A20,TOPREAL1:24,ZFMISC_1:39;
    then LSeg(p1,p11) \/ (L2 \/ LSeg(p01,p2)) is_an_arc_of p1,p2
    by A19,TOPREAL1:17;
    hence P1 is_an_arc_of p1,p2 by XBOOLE_1:4;
    L3 /\ LSeg(p00,p2) c= L3 /\ L1 &
    p00 in LSeg(p00,p2) by A18,TOPREAL1:6,XBOOLE_1:26;
    then L3 /\ LSeg(p00,p2) c= {p00} & L3 /\ LSeg(p00,p2) <> {}
    by Lm14,TOPREAL1:23,XBOOLE_0:def 3;
    then L3 /\ LSeg(p00,p2) = {p00} by ZFMISC_1:39;
    then
A21: L3 \/ LSeg(p00,p2) is_an_arc_of p10,p2 by Lm4,TOPREAL1:15,16;
A22: LSeg(p1,p10) /\ LSeg(p00,p2) = {} by A8,A18,Lm3,XBOOLE_1:3,27;
    LSeg(p1,p10) /\ (L3 \/ LSeg(p00,p2))
    = (LSeg(p1,p10) /\ L3) \/ (LSeg(p1,p10) /\ LSeg(p00,p2)) by XBOOLE_1:23
      .= {p10} by A9,A10,A22,TOPREAL1:22,ZFMISC_1:39;
    then LSeg(p1,p10) \/ (L3 \/ LSeg(p00,p2)) is_an_arc_of p1,p2
    by A21,TOPREAL1:17;
    hence P2 is_an_arc_of p1,p2 by XBOOLE_1:4;
A23: LSeg(p1,p11) \/ LSeg(p1,p10) = L4 by A3,TOPREAL1:11;
    thus R^2-unit_square = LSeg(p00,p2) \/ LSeg(p01,p2) \/ L2 \/ (L3 \/
    L4) by A15,TOPREAL1:11,def 4
      .= LSeg(p00,p2) \/ (LSeg(p01,p2) \/ L2) \/ (L3 \/ L4) by XBOOLE_1:4
      .= L2 \/ LSeg(p01,p2) \/ (L4 \/ L3 \/ LSeg(p00,p2)) by XBOOLE_1:4
      .= L2 \/ LSeg(p01,p2) \/ (L4 \/ (L3 \/ LSeg(p00,p2))) by XBOOLE_1:4
      .= L2 \/ LSeg(p01,p2) \/ (LSeg(p1,p11) \/ (LSeg(p1,p10)
    \/ (L3 \/ LSeg(p00,p2)))) by A23,XBOOLE_1:4
