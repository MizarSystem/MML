 then A157: L1 \/ L2 \/ LSeg(p11,p2) is_an_arc_of p00,p2 by A154,TOPREAL1:16;
     A158: LSeg(p1,p00) /\
 LSeg(p11,p2) c= {p10} by A7,A153,TOPREAL1:22,XBOOLE_1:27
;
         now assume p10 in LSeg(p1,p00) /\ LSeg(p11,p2);
      then p10 in LSeg(p00,p1) & p10 in LSeg(p2,p11) & p00`1 <= p1`1 &
       p2`2 <= p11`2 by A5,A6,A151,EUCLID:56,XBOOLE_0:def 3;
      then p10`1 <= p1`1 & p2`2 <= p10`2 by TOPREAL1:9,10;
      then 0 = p1`2 & 0 = p2`2 & 1 = p1`1 & 1 = p2`1
