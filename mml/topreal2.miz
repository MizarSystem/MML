        .= p2 by EUCLID:57;
      hence a in {p2} by TARSKI:def 1;
    end;
    then
A228: LSeg(p10,p2) /\ LSeg(p11,p2) c= {p2} by TARSKI:def 3;
    LSeg(p10,p2) c= L4 by A214,Lm25,TOPREAL1:12;
    then
A229: L3 /\ LSeg(p10,p2) c= {p10} by TOPREAL1:22,XBOOLE_1:27;
    take P1 = LSeg(p1,p00) \/ L3 \/ LSeg(p10,p2),P2 = LSeg(p1,p01) \/ L2 \/
    LSeg(p11,p2);
A230: p10 in LSeg(p10,p2) by RLTOPSP1:69;
    p10 in L3 by RLTOPSP1:69;
    then L3 /\ LSeg(p10,p2) <> {} by A230,XBOOLE_0:def 4;
    then L3 /\ LSeg(p10,p2) = {p10} by A229,ZFMISC_1:39;
    then
A231: L3 \/ LSeg(p10,p2) is_an_arc_of p00,p2 by Lm4,Lm8,TOPREAL1:18;
    LSeg(p10,p2) c= L4 by A214,Lm25,TOPREAL1:12;
    then
A232: LSeg(p1,p00) /\ LSeg(p10,p2) = {} by A4,Lm3,XBOOLE_1:3,27;
    LSeg(p1,p00) /\ (L3 \/ LSeg(p10,p2)) = (LSeg(p1,p00) /\ L3) \/ (LSeg
    (p1,p00) /\ LSeg(p10,p2)) by XBOOLE_1:23
      .= {p00} by A5,A6,A232,TOPREAL1:23,XBOOLE_0:def 10;
    then
LSeg(p1,p00) \/ (L3 \/ LSeg(p10,p2)) is_an_arc_of p1,p2 by A231,TOPREAL1:17;
    hence P1 is_an_arc_of p1,p2 by XBOOLE_1:4;
    p11 in LSeg(p11,p2) by RLTOPSP1:69;
    then
A233: L2 /\ LSeg(p11,p2) <> {} by Lm26,XBOOLE_0:def 4;
A234: LSeg(p11,p2) c= L4 by A214,Lm27,TOPREAL1:12;
    then
A235: LSeg(p1,p01) /\ LSeg(p11,p2) = {} by A13,Lm3,XBOOLE_1:3,27;
    L2 /\ LSeg(p11,p2) c= {p11} by A234,TOPREAL1:24,XBOOLE_1:27;
    then L2 /\ LSeg(p11,p2) = {p11} by A233,ZFMISC_1:39;
    then
A236: L2 \/ LSeg(p11,p2) is_an_arc_of p01,p2 by Lm6,Lm10,TOPREAL1:18;
    LSeg(p1,p01) /\ (L2 \/ LSeg(p11,p2)) = (LSeg(p1,p01) /\ L2) \/ (LSeg
    (p1,p01) /\ LSeg(p11,p2)) by XBOOLE_1:23
      .= {p01} by A10,A12,A235,XBOOLE_0:def 10;
    then
LSeg(p1,p01) \/ (L2 \/ LSeg(p11,p2)) is_an_arc_of p1,p2 by A236,TOPREAL1:17;
    hence P2 is_an_arc_of p1,p2 by XBOOLE_1:4;
    thus R^2-unit_square = LSeg(p1,p00) \/ LSeg(p1,p01) \/ L2 \/ (L3 \/ L4) by
    A3,TOPREAL1:11,def 4
      .= LSeg(p1,p00) \/ (LSeg(p1,p01) \/ L2) \/ (L3 \/ L4) by XBOOLE_1:4
      .= LSeg(p1,p00) \/ ((LSeg(p1,p01) \/ L2) \/ (L3 \/ L4)) by XBOOLE_1:4
      .= LSeg(p1,p00) \/ (L3 \/ (LSeg(p1,p01) \/ L2 \/ L4)) by XBOOLE_1:4
      .= LSeg(p1,p00) \/ L3 \/ (LSeg(p1,p01) \/ L2 \/ L4) by XBOOLE_1:4
      .= LSeg(p1,p00) \/ L3 \/ (LSeg(p1,p01) \/ L2 \/ (LSeg(p11,p2) \/ LSeg(
    p10,p2))) by A214,TOPREAL1:11
      .= LSeg(p1,p00) \/ L3 \/ (LSeg(p10,p2) \/ (LSeg(p1,p01) \/ L2 \/ LSeg(
    p11,p2))) by XBOOLE_1:4
      .= P1 \/ P2 by XBOOLE_1:4;
A237: p1 in LSeg(p1,p01) by RLTOPSP1:69;
    p1 in LSeg(p1,p00) by RLTOPSP1:69;
    then p1 in LSeg(p1,p00) /\ LSeg(p1,p01) by A237,XBOOLE_0:def 4;
    then {p1} c= LSeg(p1,p00) /\ LSeg(p1,p01) by ZFMISC_1:37;
    then
A238: LSeg(p1,p00) /\ LSeg(p1,p01) = {p1} by A219,XBOOLE_0:def 10;
A239: LSeg(p1,p00) /\ LSeg(p11,p2) = {} by A4,A234,Lm3,XBOOLE_1:3,27;
A240: LSeg(p10,p2) c= L4 by A214,Lm25,TOPREAL1:12;
    then
A241: LSeg(p10,p2) /\ LSeg(p1,p01) = {} by A13,Lm3,XBOOLE_1:3,27;
A242: P1 /\ P2 = (LSeg(p1,p00) \/ L3) /\ (LSeg(p1,p01) \/ L2 \/ LSeg(p11,
p2)) \/ (LSeg(p10,p2) /\ (LSeg(p1,p01) \/ L2 \/ LSeg(p11,p2))) by XBOOLE_1:23
      .= (LSeg(p1,p00) /\ (LSeg(p1,p01) \/ L2 \/ LSeg(p11,p2))) \/ (L3 /\ (
LSeg(p1,p01) \/ L2 \/ LSeg(p11,p2))) \/ (LSeg(p10,p2) /\ (LSeg(p1,p01) \/ L2 \/
    LSeg(p11,p2))) by XBOOLE_1:23
      .= (LSeg(p1,p00) /\ (LSeg(p1,p01) \/ L2)) \/ (LSeg(p1,p00) /\ LSeg(p11
,p2)) \/ (L3 /\ (LSeg(p1,p01) \/ L2 \/ LSeg(p11,p2))) \/ (LSeg(p10,p2) /\ (LSeg
    (p1,p01) \/ L2 \/ LSeg(p11,p2))) by XBOOLE_1:23
      .= (LSeg(p1,p00) /\ LSeg(p1,p01)) \/ (LSeg(p1,p00) /\ L2) \/ (L3 /\ (
LSeg(p1,p01) \/ L2 \/ LSeg(p11,p2))) \/ (LSeg(p10,p2) /\ (LSeg(p1,p01) \/ L2 \/
    LSeg(p11,p2))) by A239,XBOOLE_1:23
      .= {p1} \/ (LSeg(p1,p00) /\ L2) \/ ((L3 /\ (LSeg(p1,p01) \/ L2)) \/ (
L3 /\ LSeg(p11,p2))) \/ (LSeg(p10,p2) /\ (LSeg(p1,p01) \/ L2 \/ LSeg(p11,p2)))
    by A238,XBOOLE_1:23
      .= {p1} \/ (LSeg(p1,p00) /\ L2) \/ ((L3 /\ LSeg(p1,p01)) \/ (L3 /\ L2)
\/ (L3 /\ LSeg(p11,p2))) \/ (LSeg(p10,p2) /\ (LSeg(p1,p01) \/ L2 \/ LSeg(p11,p2
    ))) by XBOOLE_1:23
      .= {p1} \/ (LSeg(p1,p00) /\ L2) \/ ((L3 /\ LSeg(p1,p01)) \/ (L3 /\
LSeg(p11,p2))) \/ ((LSeg(p10,p2) /\ (LSeg(p1,p01) \/ L2)) \/ (LSeg(p10,p2) /\
    LSeg(p11,p2))) by Lm2,XBOOLE_1:23
      .= {p1} \/ (LSeg(p1,p00) /\ L2) \/ ((L3 /\ LSeg(p1,p01)) \/ (L3 /\
LSeg(p11,p2))) \/ ((LSeg(p10,p2) /\ (LSeg(p1,p01) \/ L2)) \/ {p2}) by A221,A228
    ,XBOOLE_0:def 10
      .= {p1} \/ (LSeg(p1,p00) /\ L2) \/ ((L3 /\ LSeg(p1,p01)) \/ (L3 /\
LSeg(p11,p2))) \/ ((LSeg(p10,p2) /\ LSeg(p1,p01)) \/ (LSeg(p10,p2) /\ L2) \/ {
    p2}) by XBOOLE_1:23
      .= {p1} \/ (LSeg(p1,p00) /\ L2) \/ ((L3 /\ LSeg(p1,p01)) \/ (L3 /\
    LSeg(p11,p2))) \/ ((LSeg(p10,p2) /\ L2) \/ {p2}) by A241;
A243: now
      per cases;
      suppose
A244:   p2 = p11;
        then L3 /\ LSeg(p11,p2) = L3 /\ {p11} by RLTOPSP1:71
          .= {} by Lm1,Lm19;
        hence P1 /\ P2 = {p1} \/ (LSeg(p1,p00) /\ L2) \/ (L3 /\ LSeg(p1,p01))
        \/ {p2} by A242,A244,TOPREAL1:24;
      end;
      suppose
A245:   p2 = p10;
        then LSeg(p10,p2) /\ L2 = {p10} /\ L2 by RLTOPSP1:71
          .= {} by Lm1,Lm17;
        hence P1 /\ P2 = {p1} \/ (LSeg(p1,p00) /\ L2) \/ ((L3 /\ LSeg(p1,p01))
        \/ {p2} \/ {p2}) by A242,A245,TOPREAL1:22,XBOOLE_1:4
          .= {p1} \/ (LSeg(p1,p00) /\ L2) \/ ((L3 /\ LSeg(p1,p01)) \/ ({p2}
        \/ {p2})) by XBOOLE_1:4
          .= {p1} \/ (LSeg(p1,p00) /\ L2) \/ (L3 /\ LSeg(p1,p01)) \/ {p2} by
        XBOOLE_1:4;
      end;
      suppose
A246:   p2 <> p10 & p2 <> p11;
        now
          assume p11 in LSeg(p10,p2) /\ L2;
          then
A247:     p11 in LSeg(p10,p2) by XBOOLE_0:def 4;
          p10`2 <= p2`2 by A222,EUCLID:56;
          then p11`2 <= p2`2 by A247,TOPREAL1:10;
          then p11`2 = p2`2 by A222,Lm11,XXREAL_0:1;
          then p2 = |[p11`1,p11`2]| by A222,Lm10,EUCLID:57
            .= p11 by EUCLID:57;
          hence contradiction by A246;
        end;
        then
A248:   {p11} <> LSeg(p10,p2) /\ L2 by ZFMISC_1:37;
        LSeg(p10,p2) /\ L2 c= L4 /\ L2 by A240,XBOOLE_1:27;
        then
A249:   LSeg(p10,p2) /\ L2 = {} by A248,TOPREAL1:24,ZFMISC_1:39;
        now
          assume p10 in L3 /\ LSeg(p11,p2);
          then
A250:     p10 in LSeg(p2,p11) by XBOOLE_0:def 4;
          p2`2 <= p11`2 by A222,EUCLID:56;
          then p2`2 = p10`2 by A222,A250,Lm9,TOPREAL1:10;
          then p2 = |[p10`1,p10`2]| by A222,Lm8,EUCLID:57
            .= p10 by EUCLID:57;
          hence contradiction by A246;
        end;
        then
A251:   L3 /\ LSeg(p11,p2) <> {p10} by ZFMISC_1:37;
        L3 /\ LSeg(p11,p2) c= {p10} by A234,TOPREAL1:22,XBOOLE_1:27;
        then L3 /\ LSeg(p11,p2) = {} by A251,ZFMISC_1:39;
        hence P1 /\ P2 = {p1} \/ (LSeg(p1,p00) /\ L2) \/ (L3 /\ LSeg(p1,p01))
        \/ {p2} by A242,A249;
      end;
    end;
    now
      per cases;
      suppose
A252:   p1 = p01;
        then L3 /\ LSeg(p1,p01) = L3 /\ {p01} by RLTOPSP1:71
          .= {} by Lm1,Lm14;
        hence thesis by A243,A252,ENUMSET1:41,TOPREAL1:21;
      end;
      suppose
A253:   p1 <> p01 & p1 <> p00;
        now
          assume p01 in LSeg(p1,p00) /\ L2;
          then
A254:     p01 in LSeg(p00,p1) by XBOOLE_0:def 4;
          p00`2 <= p1`2 by A15,A18,EUCLID:56;
          then p01`2 <= p1`2 by A254,TOPREAL1:10;
          then p1`2 = p01`2 by A15,A17,Lm7,XXREAL_0:1;
          then p1 = |[p01`1,p01`2]| by A15,A16,Lm6,EUCLID:57
            .= p01 by EUCLID:57;
          hence contradiction by A253;
        end;
        then
A255:   {p01} <> LSeg(p1,p00) /\ L2 by ZFMISC_1:37;
        LSeg(p1,p00) /\ L2 c= {p01} by A4,TOPREAL1:21,XBOOLE_1:27;
        then
A256:   LSeg(p1,p00) /\ L2 = {} by A255,ZFMISC_1:39;
        now
          assume p00 in L3 /\ LSeg(p1,p01);
          then p00 in LSeg(p1,p01) by XBOOLE_0:def 4;
          then p1`2 = p00`2 by A15,A17,A18,Lm5,Lm7,TOPREAL1:10;
          then p1 = |[p00`1,p00`2]| by A15,A16,Lm4,EUCLID:57
            .= p00 by EUCLID:57;
          hence contradiction by A253;
        end;
        then
A257:   {p00} <> L3 /\ LSeg(p1,p01) by ZFMISC_1:37;
        L3 /\ LSeg(p1,p01) c= L3 /\ L1 by A13,XBOOLE_1:27;
        then L3 /\ LSeg(p1,p01) = {} by A257,TOPREAL1:23,ZFMISC_1:39;
        hence thesis by A243,A256,ENUMSET1:41;
      end;
      suppose
A258:   p1 = p00;
        then LSeg(p1,p00) /\ L2 = {p00} /\ L2 by RLTOPSP1:71
          .= {} by Lm1,Lm13;
        hence thesis by A243,A258,ENUMSET1:41,TOPREAL1:23;
      end;
    end;
    hence thesis;
  end;
end;

Lm31: p1 <> p2 & p2 in R^2-unit_square & p1 in LSeg(p01, p11) implies ex P1,P2
being non empty Subset of TOP-REAL 2 st P1 is_an_arc_of p1,p2 & P2 is_an_arc_of
p1,p2 & R^2-unit_square = P1 \/ P2 & P1 /\ P2 = {p1,p2}
proof
  assume that
A1: p1 <> p2 and
A2: p2 in R^2-unit_square and
A3: p1 in LSeg(p01, p11);
A4: p2 in L1 \/ L2 or p2 in L3 \/ L4 by A2,TOPREAL1:def 4,XBOOLE_0:def 3;
A5: LSeg(p01,p1) /\ L1 c= L2 /\ L1 by A3,Lm23,TOPREAL1:12,XBOOLE_1:26;
  p11 in LSeg(p1,p11) by RLTOPSP1:69;
  then
A6: LSeg(p1,p11) /\ L4 <> {} by Lm27,XBOOLE_0:def 4;
  p01 in LSeg(p01,p1) by RLTOPSP1:69;
  then
A7: LSeg(p01,p1) /\ L1 <> {} by Lm22,XBOOLE_0:def 4;
A8: LSeg(p1,p11) c= L2 by A3,Lm26,TOPREAL1:12;
  then
A9: LSeg(p1,p11) /\ L3 = {} by Lm2,XBOOLE_1:3,26;
A10: LSeg(p1,p11) /\ L4 c= {p11} by A3,Lm26,TOPREAL1:12,24,XBOOLE_1:26;
A11: LSeg(p01,p1) c= L2 by A3,Lm23,TOPREAL1:12;
  then
A12: LSeg(p1,p01) /\ L3 = {} by Lm2,XBOOLE_1:3,26;
  consider q1 such that
A13: q1 = p1 and
A14: q1`1 <= 1 and
A15: q1`1 >= 0 and
A16: q1`2 = 1 by A3,TOPREAL1:19;
  per cases by A4,XBOOLE_0:def 3;
  suppose
A17: p2 in L1;
    then
A18: LSeg(p01,p2) c= L1 by Lm22,TOPREAL1:12;
    LSeg(p1,p01) c= L2 by A3,Lm23,TOPREAL1:12;
    then
A19: LSeg(p1,p01) /\ LSeg(p01,p2) c= L2 /\ L1 by A18,XBOOLE_1:27;
    take P1 = LSeg(p1,p01) \/ LSeg(p01,p2),P2 = LSeg(p1,p11) \/ (L3 \/ L4 \/
    LSeg(p00,p2));
A20: p01 in LSeg(p01,p2) by RLTOPSP1:69;
    p00 in LSeg(p00,p2) by RLTOPSP1:69;
    then
A21: {} <> L3 /\ LSeg(p00,p2) by Lm21,XBOOLE_0:def 4;
    p01 in LSeg(p1,p01) by RLTOPSP1:69;
    then LSeg(p1,p01) /\ LSeg(p01,p2) <> {} by A20,XBOOLE_0:def 4;
    then
A22: LSeg(p1,p01) /\ LSeg(p01,p2) = {p01} by A19,
    TOPREAL1:21 ,ZFMISC_1
    :39;
    p1 <> p01 or p2 <> p01 by A1;
    hence P1 is_an_arc_of p1,p2 by A22,TOPREAL1:18;
A23: LSeg(p1,p01) \/ LSeg(p1,p11) = L2 by A3,TOPREAL1:11;
A24: L4 is_an_arc_of p11,p10 by Lm9,Lm11,TOPREAL1:15;
    L3 is_an_arc_of p10,p00 by Lm4,Lm8,TOPREAL1:15;
    then
A25: L3 \/ L4 is_an_arc_of p11,p00 by A24,TOPREAL1:5,22;
A26: L3 /\ LSeg(p00,p2) c= {p00} by A17,Lm20,TOPREAL1:12,23,XBOOLE_1:26;
A27: LSeg(p00,p2) \/ LSeg(p01,p2) = L1 by A17,TOPREAL1:11;
A28: LSeg(p01,p2) /\ LSeg(p00,p2) = {p2} by A17,TOPREAL1:14;
A29: LSeg(p00,p2) c= L1 by A17,Lm20,TOPREAL1:12;
    then
A30: L4 /\ LSeg(p00,p2) = {} by Lm3,XBOOLE_1:3,27;
A31: ex q2 st q2 = p2 & q2`1 = 0 & q2`2 <= 1 & q2`2 >= 0 by A17,TOPREAL1:
    19;
A32: now
A33:  p00`2 <= p2`2 by A31,EUCLID:56;
      assume
A34:  p01 in LSeg(p1,p11) /\ LSeg(p00,p2);
      then
A35:  p01 in LSeg(p1,p11) by XBOOLE_0:def 4;
      p01 in LSeg(p00,p2) by A34,XBOOLE_0:def 4;
      then p01`2 <= p2`2 by A33,TOPREAL1:10;
      then
A36:  p01`2 = p2`2 by A31,Lm7,XXREAL_0:1;
      p1`1 <= p11`1 by A13,A14,EUCLID:56;
      then p01`1 = p1`1 by A13,A15,A35,Lm6,TOPREAL1:9;
      then p1 = |[p01`1,p01`2]| by A13,A16,Lm7,EUCLID:57
        .= p2 by A31,A36,Lm6,EUCLID:57;
      hence contradiction by A1;
    end;
    (L3 \/ L4) /\ LSeg(p00,p2) = (L3 /\ LSeg(p00,p2)) \/ (L4 /\ LSeg(p00,
    p2)) by XBOOLE_1:23
      .= {p00} by A26,A21,A30,ZFMISC_1:39;
    then
A37: L3 \/ L4 \/ LSeg(p00,p2) is_an_arc_of p11,p2 by A25,TOPREAL1:16;
    LSeg(p1,p11) /\ LSeg(p00,p2) c= L2 /\ L1 by A8,A29,XBOOLE_1:27;
    then
A38: LSeg(p1,p11) /\ LSeg(p00,p2) = {p01} or LSeg(p1,p11) /\ LSeg(p00,p2)
    = {} by TOPREAL1:21,ZFMISC_1:39;
A39: LSeg(p2,p01) c= L1 by A17,Lm22,TOPREAL1:12;
    then
A40: LSeg(p01,p2) /\ L4 = {} by Lm3,XBOOLE_1:3,27;
    LSeg(p1,p11) /\ (L3 \/ L4 \/ LSeg(p00,p2)) = (LSeg(p1,p11) /\ (L3 \/
    L4)) \/ (LSeg(p1,p11) /\ LSeg(p00,p2)) by XBOOLE_1:23
      .= (LSeg(p1,p11) /\ L3) \/ (LSeg(p1,p11) /\ L4) by A38,A32,XBOOLE_1:23
    ,ZFMISC_1:37
      .= {p11} by A9,A6,A10,ZFMISC_1:39;
    hence P2 is_an_arc_of p1,p2 by A37,TOPREAL1:17;
    thus P1 \/ P2 = LSeg(p01,p2) \/ (LSeg(p1,p01) \/ (LSeg(p1,p11) \/ (L3 \/
    L4 \/ LSeg(p00,p2)))) by XBOOLE_1:4
      .= LSeg(p01,p2) \/ (L2 \/ (L3 \/ L4 \/ LSeg(p00,p2))) by A23,XBOOLE_1:
    4
      .= LSeg(p01,p2) \/ (L2 \/ (L3 \/ L4) \/ LSeg(p00,p2)) by XBOOLE_1:4
      .= LSeg(p00,p2) \/ LSeg(p01,p2) \/ (L2 \/ (L3 \/ L4)) by XBOOLE_1:4
      .= R^2-unit_square by A27,TOPREAL1:def 4,XBOOLE_1:4;
A41: {p1} = LSeg(p1,p01) /\ LSeg(p1,p11) by A3,TOPREAL1:14;
A42: P1 /\ P2 = (LSeg(p1,p01) /\ (LSeg(p1,p11) \/ (L3 \/ L4 \/ LSeg(p00,p2
    )))) \/ (LSeg(p01,p2) /\ (LSeg(p1,p11) \/ (L3 \/ L4 \/ LSeg(p00,p2)))) by
    XBOOLE_1:23
      .= (LSeg(p1,p01) /\ LSeg(p1,p11)) \/ (LSeg(p1,p01) /\ (L3 \/ L4 \/
LSeg(p00,p2))) \/ (LSeg(p01,p2) /\ (LSeg(p1,p11) \/ (L3 \/ L4 \/ LSeg(p00,p2)))
    ) by XBOOLE_1:23
      .= {p1} \/ ((LSeg(p1,p01) /\ (L3 \/ L4)) \/ (LSeg(p1,p01) /\ LSeg(p00,
p2))) \/ (LSeg(p01,p2) /\ (LSeg(p1,p11) \/ (L3 \/ L4 \/ LSeg(p00,p2)))) by A41,
    XBOOLE_1:23
      .= {p1} \/ ((LSeg(p1,p01) /\ L3) \/ (LSeg(p1,p01) /\ L4) \/ (LSeg(p1,
p01) /\ LSeg(p00,p2))) \/ (LSeg(p01,p2) /\ (LSeg(p1,p11) \/ (L3 \/ L4 \/ LSeg(
    p00,p2)))) by XBOOLE_1:23
      .= {p1} \/ ((LSeg(p1,p01) /\ L3) \/ (LSeg(p1,p01) /\ L4) \/ (LSeg(p1,
p01) /\ LSeg(p00,p2))) \/ ((LSeg(p01,p2) /\ LSeg(p1,p11)) \/ (LSeg(p01,p2) /\ (
