      \/ (LSeg(p10,p2) /\ (L1 \/ L2 \/ LSeg(p11,p2)))) by A9,XBOOLE_1:23
     .= {p1} \/ ((LSeg(p1,p10) /\ L1) \/ (LSeg(p1,p10) /\ LSeg(p11,p2)))
      \/ ((LSeg(p10,p2) /\ LSeg(p1,p00))
      \/ ((LSeg(p10,p2) /\ (L1 \/ L2)) \/ {p2})) by A163,XBOOLE_1:23
     .= {p1} \/ ((LSeg(p1,p10) /\ L1) \/ (LSeg(p1,p10) /\ LSeg(p11,p2)))
      \/ ((LSeg(p10,p2) /\ LSeg(p1,p00))
      \/ ((LSeg(p10,p2) /\ L1) \/ (LSeg(p10,p2) /\ L2) \/ {p2})) by XBOOLE_1:23
     .= {p1} \/ ((LSeg(p1,p10) /\ L1) \/ (LSeg(p1,p10) /\ LSeg(p11,p2)))
