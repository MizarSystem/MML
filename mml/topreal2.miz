          .= (L1 \/ L2 \/ L3) \/ L4 by A3,A148,TOPREAL1:13
          .= R^2-unit_square by TOPREAL1:def 4,XBOOLE_1:4;
A242:   p2 in LSeg(p10,p2) by RLTOPSP1:69;
        p2 in LSeg(p1,p2) by RLTOPSP1:69;
        then p2 in LSeg(p1,p2) /\ LSeg(p10,p2) by A242,XBOOLE_0:def 4;
        then {p2} c= LSeg(p1,p2) /\ LSeg(p10,p2) by ZFMISC_1:37;
        then
A243:   LSeg(p1,p2) /\ LSeg(p10,p2) = {p2} by A205,XBOOLE_0:def 10;
        p1 in LSeg(p1,p2) by RLTOPSP1:69;
        then p1 in LSeg(p1,p2) /\ LSeg(p1,p11) by A238,XBOOLE_0:def 4;
        then {p1} c= LSeg(p1,p2) /\ LSeg(p1,p11) by ZFMISC_1:37;
        then LSeg(p1,p2) /\ LSeg(p1,p11) = {p1} by A228,XBOOLE_0:def 10;
        then
A244:   P1 /\ P2 = {p1} \/ LSeg(p1,p2) /\ (L2 \/ L1 \/ L3 \/ LSeg(p10,p2
        )) by XBOOLE_1:23
          .= {p1} \/ ((LSeg(p1,p2) /\ (L2 \/ L1 \/ L3)) \/ {p2}) by A243,
        XBOOLE_1:23
          .= {p1} \/ ((LSeg(p1,p2) /\ (L2 \/ L1)) \/ (LSeg(p1,p2) /\ L3) \/
        {p2}) by XBOOLE_1:23
          .= {p1} \/ ((LSeg(p1,p2) /\ L2) \/ (LSeg(p1,p2) /\ L1) \/ (LSeg(p1
        ,p2) /\ L3) \/ {p2}) by XBOOLE_1:23
          .= {p1} \/ ((LSeg(p1,p2) /\ L2) \/ ((LSeg(p1,p2) /\ L3) \/ {p2}))
        by A227,XBOOLE_1:4
          .= {p1} \/ (LSeg(p1,p2) /\ L2) \/ ((LSeg(p1,p2) /\ L3) \/ {p2}) by
        XBOOLE_1:4;
A245:   LSeg(p1,p2) /\ L2 c= L4 /\ L2 by A3,A148,TOPREAL1:12,XBOOLE_1:26;
A246:   now
          per cases;
          suppose
A247:       p1 = p11;
            then p11 in LSeg(p1,p2) by RLTOPSP1:69;
            then LSeg(p1,p2) /\ L2 <> {} by Lm26,XBOOLE_0:def 4;
            then LSeg(p1,p2) /\ L2 = {p1} by A245,A247,TOPREAL1:24 ,
            ZFMISC_1:39;
            hence P1 /\ P2 = {p1} \/ ((LSeg(p1,p2) /\ L3) \/ {p2}) by A244;
          end;
          suppose
A248:       p1 <> p11;
            now
              assume p11 in LSeg(p1,p2) /\ L2;
              then p11 in LSeg(p2,p1) by XBOOLE_0:def 4;
              then p11`2 <= p1`2 by A13,A151,A204,TOPREAL1:10;
              then p1`2 = 1 by A13,A15,Lm11,XXREAL_0:1;
              hence contradiction by A13,A14,A248,EUCLID:57;
            end;
            then {p11} <> LSeg(p1,p2) /\ L2 by ZFMISC_1:37;
            then LSeg(p1,p2) /\ L2 = {} by A245,TOPREAL1:24,ZFMISC_1:39;
            hence P1 /\ P2 = {p1} \/ ((LSeg(p1,p2) /\ L3) \/ {p2}) by A244;
          end;
        end;
A249:   LSeg(p1,p2) /\ L3 c= L4 /\ L3 by A3,A148,TOPREAL1:12,XBOOLE_1:26;
        now
          per cases;
          suppose
A250:       p2 = p10;
            then p10 in LSeg(p1,p2) by RLTOPSP1:69;
            then LSeg(p1,p2) /\ L3 <> {} by Lm24,XBOOLE_0:def 4;
            then LSeg(p1,p2) /\ L3 = {p2} by A249,A250,TOPREAL1:22 ,
            ZFMISC_1:39;
            hence P1 /\ P2 = {p1,p2} by A246,ENUMSET1:41;
          end;
          suppose
A251:       p2 <> p10;
            now
              assume p10 in LSeg(p1,p2) /\ L3;
              then p10 in LSeg(p2,p1) by XBOOLE_0:def 4;
