A162:  LSeg(p1,p10) /\ LSeg(p1,p00)
       = {p1} by A3,TOPREAL1:14;
A163:  LSeg(p10,p2) /\ LSeg(p11,p2) = {p2} by A150,TOPREAL1:14;
