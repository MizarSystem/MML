  thus R1 = R1 + (i|->(0 qua Real)) by BINOP_2:2,FINSEQOP:57
    .= R1 + (-R + R) by Th22,Th23,BINOP_2:2,FINSEQOP:77
    .= R1 - R + R by FINSEQOP:29;
