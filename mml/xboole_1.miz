theorem
  (X /\ Z) \+\ (Y /\ Z) = (X \+\ Y) /\ Z
proof
  thus (X /\ Z) \+\ (Y /\ Z) = ((X \ Y) /\ Z) \/ ((Y /\ Z) \ (X /\ Z)) by Th111
    .= ((X \ Y) /\ Z) \/ ((Y \ X) /\ Z) by Th111
    .= (X \+\ Y) /\ Z by Th23;
end;

theorem
  X \/ Y \/ Z \/ V = X \/ (Y \/ Z \/ V)
proof
  X \/ Y \/ Z \/ V = X \/ Y \/ (Z \/ V) by Th4
    .= X \/ (Y \/ (Z \/ V)) by Th4
    .= X \/ (Y \/ Z \/ V) by Th4;
  hence thesis;
