       .= (]. 1,+infty.[ /\ ].-infty, 2 .]) \/
         (]. 1,+infty.[ /\ (IRRAT(2,4) \/ {4} \/ {5})) by XBOOLE_1:23
       .= (]. 1,+infty.[ /\ ].-infty, 2 .]) \/
         (IRRAT(2,4) \/ {4} \/ {5}) by Lm8,XBOOLE_1:28
       .= ]. 1,2 .] \/ (IRRAT(2,4) \/ {4} \/ {5}) by Th91
       .= ]. 1,2 .] \/ IRRAT(2,4) \/ {4} \/ {5} by XBOOLE_1:113;
