    ].-infty, 1 .[ /\ (].-infty, 2 .] \/ IRRAT(2,4) \/ {4} \/ {5}) =
    ].-infty, 1 .[ /\ (].-infty, 2 .] \/ (IRRAT(2,4) \/ {4} \/ {5}))
