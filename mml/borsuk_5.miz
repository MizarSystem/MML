     ]. 1,+infty.[ /\ (].-infty, 2 .] \/ IRRAT(2,4) \/ {4} \/ {5}) =
       ]. 1,+infty.[ /\ (].-infty, 2 .] \/ (IRRAT(2,4) \/ {4} \/ {5}))
