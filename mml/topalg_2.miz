begin :: Convex subspaces of TOP-REAL n

reserve
  n for Nat,
  a, b for real number;

registration
  let n;
  cluster non empty convex Subset of TOP-REAL n;
existence
proof
  consider a, b being Point of TOP-REAL n;
  take LSeg(a,b);
  thus thesis by BORSUK_4:8;
end;
end;

definition
  let n be Nat, T be SubSpace of TOP-REAL n;
  attr T is convex means :dconv:
  [#]T is convex Subset of TOP-REAL n;
end;

registration
  let n be Nat;
  cluster convex -> arcwise_connected (non empty SubSpace of TOP-REAL n);
coherence
proof
  let T be non empty SubSpace of TOP-REAL n;
  assume
a0: [#]T is convex Subset of TOP-REAL n;
  let a, b be Point of T;
a3: a in the carrier of T & b in the carrier of T;
  the carrier of T is Subset of TOP-REAL n by TSEP_1:1;
  then reconsider a1 = a, b1 = b as Point of TOP-REAL n by a3;
  per cases;
  suppose a1 <> b1;
  then LSeg(a1,b1) is_an_arc_of a1,b1 by TOPREAL1:15;
  then consider h being map of I[01], (TOP-REAL n)|LSeg(a1,b1) such that
A1: h is_homeomorphism and
A2: h.0 = a1 & h.1 = b1 by TOPREAL1:def 2;
A3: h is continuous by A1,TOPS_2:def 5;
a7: [#]T = the carrier of T by PRE_TOPC:12;
  [#]((TOP-REAL n)|LSeg(a1,b1)) = LSeg(a1,b1) by PRE_TOPC:def 10;
  then the carrier of (TOP-REAL n)|LSeg(a1,b1) = LSeg(a1,b1) by PRE_TOPC:12;
  then
A6:the carrier of (TOP-REAL n)|LSeg(a1,b1) c= the carrier of T
    by a7,a0,JORDAN1:def 1;
  then reconsider f = h as map of I[01], T by FUNCT_2:9;
  take f;
  (TOP-REAL n)|LSeg(a1,b1) is SubSpace of T by A6,TSEP_1:4;
  hence f is continuous by A3,TOPMETR:7;
  thus f.0 = a & f.1 = b by A2;
  end;
  suppose a1 = b1;
  hence thesis by BORSUK_2:4;
  end;
end;
end;

registration
  let n be Nat;
  cluster strict non empty convex SubSpace of TOP-REAL n;
existence
proof
  reconsider T = TOP-REAL n as SubSpace of TOP-REAL n by TSEP_1:2;
  take T;
  thus T is strict non empty;
  thus [#]T is convex Subset of TOP-REAL n by JORDAN2C:21;
end;
end;

theorem Tlo:
  for X being non empty TopSpace, Y being non empty SubSpace of X,
      x1, x2 being Point of X, y1, y2 being Point of Y,
      f being Path of y1,y2 st x1 = y1 & x2 = y2 & y1,y2 are_connected holds
   f is Path of x1,x2
proof
  let X be non empty TopSpace, Y be non empty SubSpace of X,
      x1, x2 be Point of X, y1, y2 be Point of Y,
      f be Path of y1,y2 such that
a1: x1 = y1 & x2 = y2;
  given m being map of I[01], Y such that
a2:  m is continuous & m.0 = y1 & m.1 = y2;
  the carrier of Y is Subset of the carrier of X by TSEP_1:1;
  then reconsider g = f as map of I[01], X by FUNCT_2:9;
  g is Path of x1,x2
  proof
a3: f is continuous by a2,BORSUK_2:def 1;
    thus ex f being map of I[01], X st f is continuous & f.0 = x1 & f.1 = x2
    proof
      take g;
      thus g is continuous by a3,TOPMETR:7;
      thus thesis by a1,a2,BORSUK_2:def 1;
    end;
    thus g is continuous by a3,TOPMETR:7;
    thus g.0 = x1 & g.1 = x2 by a1,a2,BORSUK_2:def 1;
  end;
  hence f is Path of x1,x2;
end;

set I = the carrier of I[01];
Lm0: the carrier of [:I[01],I[01]:] = [:I,I:] by BORSUK_1:def 5;
Lm1: the carrier of [:TOP-REAL 1,I[01]:] = [:the carrier of TOP-REAL 1,I:]
     by BORSUK_1:def 5;
Lm2: the carrier of [:R^1,I[01]:] = [:the carrier of R^1,I:] by BORSUK_1:def 5;
Lm3: dom id I[01] = I by FUNCT_2:def 1;

definition
  let n be Nat,
      T being non empty convex SubSpace of TOP-REAL n,
      a, b be Point of T,
      P, Q be Path of a,b;
  func ConvexHomotopy(P,Q) -> map of [:I[01],I[01]:], T means :def12:
  for s, t being Element of I[01],
      a1, b1 being Point of TOP-REAL n st a1 = P.s & b1 = Q.s holds
   it.(s,t) = (1-t) * a1 + t * b1;
existence
proof
  defpred P[Element of I,Element of I,set] means
    ex a1, b1 being Point of TOP-REAL n st a1 = P.$1 & b1 = Q.$1 &
      $3 = (1-$2) * a1 + $2 * b1;
a1: for x, y being Element of I ex z being Element of the carrier of T st
   P[x,y,z]
  proof
    let x, y be Element of I;
a2: P.x in the carrier of T & Q.x in the carrier of T;
    the carrier of T is Subset of TOP-REAL n by TSEP_1:1;
    then reconsider a1 = P.x, b1 = Q.x as Point of TOP-REAL n by a2;
a3: [#]T = the carrier of T by PRE_TOPC:12;
    set z = (1-y) * a1 + y * b1;
a5: y is Real by XREAL_0:def 1;
a6: 0 <= y & y <= 1 by JORDAN5A:2;
    LSeg(a1,b1) = { (1-l)*a1 + l*b1 where l is Real: 0 <= l & l <= 1 }
      by TOPREAL1:def 4;
    then
a4: z in LSeg(a1,b1) by a5,a6;
    [#]T is convex Subset of TOP-REAL n by dconv;
    then LSeg(a1,b1) c= [#]T by a3,JORDAN1:def 1;
    then z is Point of T by a4,PRE_TOPC:12;
    hence thesis;
  end;
  consider F being Function of [:I,I:],the carrier of T such that
A1: for x, y being Element of I holds P[x,y,F. [x,y]] from FUNCT_2:sch 7(a1);
  reconsider F as map of [:I[01],I[01]:], T by Lm0;
  take F;
  let x, y be Element of I;
  ex a1, b1 being Point of TOP-REAL n st
    a1 = P.x & b1 = Q.x & F. [x,y] = (1-y) * a1 + y * b1 by A1;
  hence thesis by BINOP_1:def 1;
end;
uniqueness
proof
  let f, g be map of [:I[01],I[01]:], T such that
a1: for s, t being Element of I[01], a1, b1 being Point of TOP-REAL n st
     a1 = P.s & b1 = Q.s holds f.(s,t) = (1-t) * a1 + t * b1 and
a2: for s, t being Element of I[01], a1, b1 being Point of TOP-REAL n st
     a1 = P.s & b1 = Q.s holds g.(s,t) = (1-t) * a1 + t * b1;
  for s, t being Element of I holds f.(s,t) = g.(s,t)
  proof
    let s, t be Element of I;
    reconsider a1 = P.s, b1 = Q.s as Point of TOP-REAL n by TOPMETR:2;
    f.(s,t) = (1-t) * a1 + t * b1 by a1;
    hence f.(s,t) = g.(s,t) by a2;
  end;
  hence f = g by Lm0,BINOP_1:2;
end;
end;

T90c:
  for T being non empty convex SubSpace of TOP-REAL n, a, b being Point of T,
      P, Q being Path of a,b holds ConvexHomotopy(P,Q) is continuous
proof
  let T be non empty convex SubSpace of TOP-REAL n,
      a, b be Point of T,
      P, Q being Path of a,b;
  set F = ConvexHomotopy(P,Q);
  set X = the carrier of T;
  set E = the carrier of TOP-REAL n;
  set PI = [:P,id I[01]:];
  set QI = [:Q,id I[01]:];
a2: the carrier of T is Subset of the carrier of TOP-REAL n by TSEP_1:1;
  then reconsider G = F as map of [:I[01],I[01]:],TOP-REAL n by FUNCT_2:9;
  a in the carrier of T & b in the carrier of T;
  then reconsider a1 = a, b1 = b as Point of TOP-REAL n by a2;
  a,b are_connected by BORSUK_6:47;
  then reconsider P1 = P, Q1 = Q as Path of a1,b1 by Tlo;
  set P1I = [:P1,id I[01]:];
  set Q1I = [:Q1,id I[01]:];
xA5: P1I is continuous & Q1I is continuous by BORSUK_2:12;
  deffunc Fa(Element of E,Element of I) = (1-$2)*$1;
  deffunc Fb(Element of E,Element of I) = $2*$1;
  consider Pa being Function of [:E,I:],E such that
xA1: for x being Element of E, i being Element of I holds
    Pa. [x,i] = Fa(x,i) from FUNCT_2:sch 8;
  consider Pb being Function of [:E,I:],E such that
xA2: for x being Element of E, i being Element of I holds
    Pb. [x,i] = Fb(x,i) from FUNCT_2:sch 8;
  the carrier of [:TOP-REAL n,I[01]:] = [:E,I:] by BORSUK_1:def 5;
  then reconsider Pa, Pb as map of [:TOP-REAL n,I[01]:],TOP-REAL n;
yA1: for x being Element of E, i being Element of I holds
    Pa.(x,i) = Fa(x,i)
  proof
    let x be Element of E, i be Element of I;
    thus Pa.(x,i) = Pa. [x,i] by BINOP_1:def 1
      .= Fa(x,i) by xA1;
  end;
  for x being Element of E, i being Element of I holds
    Pb.(x,i) = Fb(x,i)
  proof
    let x be Element of E, i be Element of I;
    thus Pb.(x,i) = Pb. [x,i] by BINOP_1:def 1
      .= Fb(x,i) by xA2;
  end;
  then Pa is continuous & Pb is continuous by yA1,TOPALG_1:18,19;
  then
xA3: Pa*P1I is continuous & Pb*Q1I is continuous by xA5,TOPS_2:58;
jj:  for p being Point of [:I[01],I[01]:] holds G.p = (Pa*P1I).p + (Pb*Q1I).p
  proof
    let p be Point of [:I[01],I[01]:];
    consider s, t being Point of I[01] such that
a1:   p = [s,t] by BORSUK_1:50;
a4: dom P = I by FUNCT_2:def 1;
a7: dom Q = I by FUNCT_2:def 1;
a6: (id I[01]).t = t by GRCAT_1:11;
a2: (Pa*PI).p = Pa.(PI.p) by FUNCT_2:21
      .= Pa. [P.s,t] by a1,a4,a6,Lm3,FUNCT_3:def 9
      .= (1-t) * P1.s by xA1;
k1: (Pb*QI).p = Pb.(QI.p) by FUNCT_2:21
      .= Pb. [Q.s,t] by a1,a7,a6,Lm3,FUNCT_3:def 9
      .= t * Q1.s by xA2;
    reconsider a1 = P.s, b1 = Q.s as Point of TOP-REAL n by TOPMETR:2;
    F.(s,t) = (1-t) * a1 + t * b1 by def12;
    hence G.p = (Pa*P1I).p + (Pb*Q1I).p by a2,k1,a1,BINOP_1:def 1;
  end;
  consider g being map of [:I[01],I[01]:],TOP-REAL n such that
Z1:  for r being Point of [:I[01],I[01]:] holds g.r = (Pa*P1I).r + (Pb*Q1I).r
  and
Z2:  g is continuous by xA3,JGRAPH_6:20;
  for p being Point of [:I[01],I[01]:] holds G.p = g.p
  proof
    let p be Point of [:I[01],I[01]:];
    thus G.p = (Pa*P1I).p + (Pb*Q1I).p by jj
      .= g.p by Z1;
  end;
  then G = g by FUNCT_2:113;
  hence F is continuous by Z2,TOPMETR:8;
end;
  
T90k:
  for T being non empty convex SubSpace of TOP-REAL n,
      a, b being Point of T,
      P, Q being Path of a,b holds
  for s being Point of I[01] holds ConvexHomotopy(P,Q).(s,0) = P.s &
                                   ConvexHomotopy(P,Q).(s,1) = Q.s &
  for t being Point of I[01] holds ConvexHomotopy(P,Q).(0,t) = a &
                                   ConvexHomotopy(P,Q).(1,t) = b
proof
  let T be non empty convex SubSpace of TOP-REAL n,
      a, b be Point of T,
      P, Q be Path of a,b;
  set F = ConvexHomotopy(P,Q);
  let s be Point of I[01];
  reconsider x0 = 0, x1 = 1 as Point of I[01] by BORSUK_1:def 17,def 18;
  reconsider a1 = P.s, b1 = Q.s as Point of TOP-REAL n by TOPMETR:2;
a3: F.(s,x0) = (1-x0) * a1 + x0 * b1 by def12;
  thus F.(s,0) = a1 + 0 * b1 by a3,EUCLID:33
    .= a1 + 0.REAL n by EUCLID:33
    .= P.s by EUCLID:31;
a3: F.(s,x1) = (1-x1) * a1 + x1 * b1 by def12;
  thus F.(s,1) = 0.REAL n + 1 * b1 by a3,EUCLID:33
    .= 0.REAL n + b1 by EUCLID:33
    .= Q.s by EUCLID:31;
  let t be Point of I[01];
  reconsider a1 = P.x0, b1 = Q.x0 as Point of TOP-REAL n by TOPMETR:2;
a3: F.(0,t) = (1-t) * a1 + t * b1 by def12;
A2: P.x0 = a by BORSUK_2:def 3;
A3: Q.x0 = a by BORSUK_2:def 3;
A4: P.x1 = b by BORSUK_2:def 3;
A5: Q.x1 = b by BORSUK_2:def 3;
  thus F.(0,t) = 1*a1 - t*a1 + t*a1 by A2,A3,a3,EUCLID:54
    .= 1*a1 by EUCLID:52
    .= a by A2,EUCLID:33;
  reconsider a1 = P.x1, b1 = Q.x1 as Point of TOP-REAL n by TOPMETR:2;
a3: F.(1,t) = (1-t) * a1 + t * b1 by def12;
  thus F.(1,t) = 1*a1 - t*b1 + t*a1 by A4,A5,a3,EUCLID:54
    .= 1*b1 by A4,A5,EUCLID:52
    .= b by A5,EUCLID:33;
end;

theorem T90:
  for T being non empty convex SubSpace of TOP-REAL n,
      a, b being Point of T,
      P, Q being Path of a,b holds
  P, Q are_homotopic
proof
  let T be non empty convex SubSpace of TOP-REAL n,
      a, b be Point of T,
      P, Q be Path of a,b;
  take F = ConvexHomotopy(P,Q);
  thus F is continuous by T90c;
  thus thesis by T90k;
end;

definition
  let n be Nat,
      T be non empty convex SubSpace of TOP-REAL n,
      a, b be Point of T,
      P, Q be Path of a,b;
  redefine func ConvexHomotopy(P,Q) -> Homotopy of P,Q;
coherence
proof
  thus P, Q are_homotopic by T90;
  thus ConvexHomotopy(P,Q) is continuous by T90c;
  thus thesis by T90k;
end;
end;

registration
  let n be Nat,
      T be non empty convex SubSpace of TOP-REAL n,
      a, b be Point of T,
      P, Q be Path of a,b;
  cluster -> continuous Homotopy of P,Q;
coherence
proof
  let F be Homotopy of P,Q;
  P, Q are_homotopic by T90;
  hence F is continuous by BORSUK_6:def 13;
end;
end;

theorem T91:
  for T being non empty convex SubSpace of TOP-REAL n,
      a being Point of T,
      C being Loop of a holds
  the carrier of pi_1(T,a) = { Class(EqRel(T,a),C) }
proof
  let T be non empty convex SubSpace of TOP-REAL n,
      a be Point of T,
      C be Loop of a;
  set E = EqRel(T,a);
  hereby
    let x be set;
    assume x in the carrier of pi_1(T,a);
    then consider P being Loop of a such that
a3:   x = Class(E,P) by TOPALG_1:48;
    P,C are_homotopic by T90;
    then x = Class(E,C) by a3,TOPALG_1:47;
    hence x in { Class(E,C) } by TARSKI:def 1;
  end;
  let x be set;
  assume x in { Class(E,C) };
  then
a2: x = Class(E,C) by TARSKI:def 1;
  C in Loops a by TOPALG_1:def 1;
  then x in Class E by a2,EQREL_1:def 5;
  hence thesis by TOPALG_1:def 3;
end;
  
registration
  let n be Nat,
      T be non empty convex SubSpace of TOP-REAL n,
      a be Point of T;
  cluster pi_1(T,a) -> trivial;
coherence
proof
  set E = EqRel(T,a);
  consider C being constant Loop of a;
a1: the carrier of pi_1(T,a) = Class E by TOPALG_1:def 3;
  then { Class(E,C) } = Class E by T91;
  hence thesis by a1,REALSET1:def 13;
end;
end;

begin :: Convex subspaces of R^1

theorem Taa:
  Proj(|[a]|,1) = a
proof
  reconsider y = a as Real by XREAL_0:def 1;
  |[a]| = <*y*> by JORDAN2B:def 2;
  hence Proj(|[a]|,1) = <*y*>/.1 by JORDAN2B:def 1
    .= a by FINSEQ_4:25;
end;
  
registration
  cluster -> real-membered SubSpace of R^1;
coherence
proof
  let T be SubSpace of R^1;
  let r be set such that
a0: r in the carrier of T;
  the carrier of T is Subset of R^1 by TSEP_1:1;
  hence thesis by a0,TOPMETR:24,XREAL_0:def 1;
end;
end;

theorem
  a <= b implies
  [.a,b.] = { (1-l)*a + l*b where l is Real: 0 <= l & l <= 1 }
proof
  assume
a1:  a <= b;
  set X = {(1-l)*a+l*b where l is Real: 0 <= l & l <= 1};
  hereby
    let x be set;
    assume
b1:   x in [.a,b.];
    then reconsider y = x as Real;
w1: a <= y & y <= b by b1,TOPREAL5:1;
    per cases by a1,REAL_1:def 5;
    suppose a < b;
    then
b3: b-a > b-b by REAL_1:92;
    reconsider l = (y-a)/(b-a) as Real by XREAL_0:def 1;
    l in the carrier of Closed-Interval-TSpace (0,1) by w1,BORSUK_6:5;
    then l in [.0,1.] by TOPMETR:25;
    then
b2: 0 <= l & l <= 1 by TOPREAL5:1;
    (1-l)*a+l*b = a+l*(b-a)
    .= a+(y-a) by b3,XCMPLX_1:88
    .= y;
    hence x in X by b2;
    end;
    suppose a = b;
    then (1-1)*a+1*b = y by w1,AXIOMS:21;
    hence x in X;
    end;
  end;
  let x be set;
  assume x in X;
  then consider l being Real such that
a2: x = (1-l)*a+l*b and
a3: 0 <= l and
a4: l <= 1;
  a <= (1-l)*a+l*b & (1-l)*a+l*b <= b by a1,a3,a4,JGRAPH_6:2;
  hence x in [.a,b.] by a2,TOPREAL5:1;
end;

theorem TT100:
  for F being map of [:R^1,I[01]:], R^1 st
   for x being Point of R^1, i being Point of I[01] holds
    F.(x,i) = (1-i) * x holds
  F is continuous
proof
  let F be map of [:R^1,I[01]:], R^1 such that
A3: for x being Point of R^1, i being Point of I[01] holds
   F.(x,i) = (1-i) * x;
   deffunc Fa(Element of the carrier of TOP-REAL 1,
              Element of I) = (1-$2)*$1;
   consider G being Function of [:the carrier of TOP-REAL 1,I:],
              the carrier of TOP-REAL 1 such that
A1: for x being Point of TOP-REAL 1, i being Point of I[01] holds
