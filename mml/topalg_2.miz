a1: a < x & x < b by JORDAN6:45;
    assume y in P;
    then
a2: a < y & y < b by JORDAN6:45;
    let m be set;
    assume
a3:   m in [. x,y .];
    then reconsider m as Real;
    x <= m & m <= y by a3,TOPREAL5:1;
    then a < m & m < b by a1,a2,AXIOMS:22;
    hence thesis by JORDAN6:45;
  end;
  hence thesis;
end;

