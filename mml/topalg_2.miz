theorem Tc1:
  for T being non empty convex SubSpace of R^1,
      a, b being Point of T holds
  [. a, b .] c= the carrier of T
proof
  let T be non empty convex SubSpace of R^1,
      a, b be Point of T;
  reconsider a1 = a, b1 = b as Point of R^1 by TOPMETR:2;
k1: [#]T is convex Subset of R^1 by dconv;
  [#]T = the carrier of T by PRE_TOPC:12;
  then [. a1, b1 .] c= the carrier of T by k1,dc1;
  hence [. a, b .] c= the carrier of T;
end;
      
registration
  cluster convex -> arcwise_connected (non empty SubSpace of R^1);
coherence
proof
  let T be non empty SubSpace of R^1 such that
a1: T is convex;
  let a, b be Point of T;
  per cases;
  suppose
s1: a <= b;
  set f = L[01]((#)(a,b),(a,b)(#));
a2: [.a,b.] c= the carrier of T by a1,Tc1;
  set X = Closed-Interval-TSpace(a,b);
  the carrier of X = [.a,b.] by s1,TOPMETR:25;
  then reconsider f as map of I[01], T by a2,FUNCT_2:9,TOPMETR:27;
  take f;
  the carrier of X is Subset of the carrier of R^1 by TSEP_1:1;
  then reconsider g = f as map of I[01], R^1 by FUNCT_2:9,TOPMETR:27;
  L[01]((#)(a,b),(a,b)(#)) is continuous by s1,TREAL_1:11;
  then g is continuous by TOPMETR:7,27;
  hence f is continuous by TOPMETR:8;
  thus f.0 = f.(#)(0,1) by TREAL_1:def 1
    .= (#)(a,b) by s1,TREAL_1:12
    .= a by s1,TREAL_1:def 1;
  thus f.1 = f.(0,1)(#) by TREAL_1:def 2
    .= (a,b)(#) by s1,TREAL_1:12
    .= b by s1,TREAL_1:def 2;
  end;
  suppose
s1: b <= a;
  set f = L[01]((b,a)(#),(#)(b,a));
a2: [.b,a.] c= the carrier of T by a1,Tc1;
  set X = Closed-Interval-TSpace(b,a);
  the carrier of X = [.b,a.] by s1,TOPMETR:25;
  then reconsider f as map of I[01], T by a2,FUNCT_2:9,TOPMETR:27;
  take f;
  the carrier of X is Subset of the carrier of R^1 by TSEP_1:1;
  then reconsider g = f as map of I[01], R^1 by FUNCT_2:9,TOPMETR:27;
  L[01]((b,a)(#),(#)(b,a)) is continuous by s1,TREAL_1:11;
  then g is continuous by TOPMETR:7,27;
  hence f is continuous by TOPMETR:8;
  thus f.0 = f.(#)(0,1) by TREAL_1:def 1
    .= (b,a)(#) by s1,TREAL_1:12
    .= a by s1,TREAL_1:def 2;
  thus f.1 = f.(0,1)(#) by TREAL_1:def 2
    .= (#)(b,a) by s1,TREAL_1:12
    .= b by s1,TREAL_1:def 1;
  end;
end;
end;

theorem To:
  a <= b implies Closed-Interval-TSpace(a,b) is convex
proof
  set X = Closed-Interval-TSpace(a,b);
  assume a <= b;
  then [.a,b.] = the carrier of X by TOPMETR:25
    .= [#]X by PRE_TOPC:12;
  hence [#]X is convex Subset of R^1 by T20;
end;
  
theorem Tcc:
  I[01] is convex by To,TOPMETR:27;
  
theorem
  a <= b implies Closed-Interval-TSpace(a,b) is arcwise_connected
proof
  assume a <= b;
  then reconsider X = Closed-Interval-TSpace(a,b) as
    non empty convex SubSpace of R^1 by To;
  X is arcwise_connected;
  hence thesis;  
end;
  
definition
  let T be non empty convex SubSpace of R^1,
      a, b be Point of T,
      P, Q be Path of a,b;
  func R1Homotopy(P,Q) -> map of [:I[01],I[01]:], T means :def12:
  for s, t being Element of I[01] holds it.(s,t) = (1-t) * P.s + t * Q.s;
existence
proof
  defpred P[Element of I, Element of I, set] means
   $3 = (1-$2) * P.$1 + $2 * Q.$1;
a1: for m, n being Element of I
  ex z being Element of the carrier of T st P[m,n,z]
  proof
    let m, n be Element of I;
r1: 0 <= n & n <= 1 by JORDAN5A:2;
    set z = (1-n) * P.m + n * Q.m;
    per cases;
    suppose P.m <= Q.m;
    then P.m <= z & z <= Q.m by r1,JGRAPH_6:2;
    then
a0: z in [. P.m,Q.m .] by TOPREAL5:1;
    [. P.m,Q.m .] c= the carrier of T by Tc1;
    hence thesis by a0;
    end;
    suppose
d2:   P.m > Q.m;
    then Q.m - P.m < Q.m - Q.m by REAL_1:92;
    then n*(Q.m-P.m) <= n*0 by r1,SQUARE_1:23;
    then
d1: P.m+n*(Q.m-P.m) <= P.m+0 by AXIOMS:24;
    1-1 <= 1-n by r1,REAL_1:92;
    then (1-n)*Q.m <= (1-n)*P.m by d2,AXIOMS:25;
    then (1-n)*Q.m+n*Q.m <= (1-n)*P.m+n*Q.m by AXIOMS:24;
    then
a0: z in [. Q.m, P.m .] by d1,TOPREAL5:1;
    [. Q.m, P.m .] c= the carrier of T by Tc1;
    hence thesis by a0;
    end;
  end;
  consider F being Function of [:I,I:], the carrier of T such that
A1: for m, n being Element of I holds P[m,n,F. [m,n]] from FUNCT_2:sch 7(a1);
  reconsider F as map of [:I[01],I[01]:], T by Lm0;
  take F;
  let s, t be Element of I;
  thus F.(s,t) = F. [s,t] by BINOP_1:def 1
    .= (1-t) * P.s + t * Q.s by A1;
end;
uniqueness
proof
  let f, g be map of [:I[01],I[01]:], T such that
a1: for s, t being Element of I[01] holds f.(s,t) = (1-t) * P.s + t * Q.s
  and
a2: for s, t being Element of I[01] holds g.(s,t) = (1-t) * P.s + t * Q.s;
  for s, t being Element of I holds f.(s,t) = g.(s,t)
  proof
    let s, t be Element of I;
    thus f.(s,t) = (1-t) * P.s + t * Q.s by a1
      .= g.(s,t) by a2;
  end;
  hence f = g by Lm0,BINOP_1:2;
end;
end;

T90c:
