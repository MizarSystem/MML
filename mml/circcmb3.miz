   let A be one-gate Circuit of S;
   let s be State of A;
A1: s is stabilizing by Def2;
   Following s is stable by Th20;
   then Following(s,1) is stable by FACIRC_1:14;
   hence stabilization-time s <= 1 by A1,Def5;
  end;

