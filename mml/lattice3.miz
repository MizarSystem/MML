      RELAT_2, BHSP_3, FILTER_0, TARSKI, ZF_LANG, LATTICE3, ORDERS_1, SETFAM_1;
 notations TARSKI, XBOOLE_0, ZFMISC_1, SUBSET_1, SETFAM_1,
 DOMAIN_1, STRUCT_0, FUNCT_1,
      FUNCT_2, BINOP_1, LATTICES, FILTER_0, LATTICE2, FILTER_1, RELAT_1,
      RELAT_2, RELSET_1, ORDERS_1, ORDERS_2;
 constructors DOMAIN_1, BINOP_1, LATTICE2, FILTER_1, ORDERS_2;
 registrations LATTICE2, ORDERS_2, RELSET_1, STRUCT_0, SUBSET_1, LATTICES,
      XBOOLE_0;
 requirements SUBSET, BOOLE;
 definitions XBOOLE_0, TARSKI, LATTICES, RELAT_1, RELAT_2, ORDERS_2, STRUCT_0;
 theorems TARSKI, ENUMSET1, ZFMISC_1, FUNCT_1, FUNCT_2, BINOP_1, LATTICES,
