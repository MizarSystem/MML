    N3 >= N0 by A19,A28,XXREAL_0:2;
    then
A32: abs(q.N3-0) < 1/2 by A12;
    q.N3 = p.N3 / g.N3 by Lm4
      .= log(2,N3) / g.N3 by A28,Def2
      .= log(2,N3) / (N3 to_power 1) by A28,Def3
      .= log(2,N3) / N3 by POWER:30;
