  end;
A55: now
    let n;
    assume
A56: n >= 1;
A57: n + 1 > n + 0 by XREAL_1:10;
    then
A58: n to_power n < (n+1) to_power n by A56,POWER:42;
    n+1 >= 1+1 by A56,XREAL_1:8;
