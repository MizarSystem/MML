           .= n+1 by POWER:30;
       n+0 <= n+1 by XREAL_1:8;
  hence g.n <= g.(n+1) by A24,A25;
 end;
then A26:  g is eventually-nondecreasing by ASYMPT_0:def 8;
A27: h is Element of Funcs(NAT, REAL) by FUNCT_2:11;
    now let n; assume A28: n >= 1;
 then A29: 2*n > 2*0 by XREAL_1:70;
 A30: h.n = g.(2*n) by ASYMPT_0:def 18
        .= (2*n) to_power 1 by A29,Def3
        .= 2*n by POWER:30;
        g.n = n to_power 1 by A28,Def3
        .= n by POWER:30;
  hence h.n <= 2*g.n by A30;
