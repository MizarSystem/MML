theorem
  for a,b being positive Real holds seq_prob28(a,b) is eventually-nondecreasing
proof
 let a,b be positive Real;
 set f = seq_prob28(a,b);
   defpred R[Element of NAT] means f.$1 <= f.($1+1);
   defpred P[Element of NAT] means (for n st n >= 1 & n < $1 holds R[n])
    implies R[$1];
A1: for k st k >= 1 & (for n st n >= 1 & n < k holds R[n]) holds R[k] proof
     for k st k >= 1 holds P[k] proof
    A2: P[1] proof
         assume (for n st n >= 1 & n < 1 holds f.n <= f.(n+1));
         A3: f.1 = Prob28(1,a,b) by Def7
                .= a by Lm54;
         A4: [/2/2\] = 1 by INT_1:54;
             consider n1 such that
         A5: n1 = [/2/2\] and
         A6: Prob28(2,a,b) = 4*Prob28(n1,a,b) + b*2 by Lm54;
         A7: f.(1+1) = 4*Prob28(1,a,b) + b*2 by A4,A5,A6,Def7
                    .= 4*a + b*2 by Lm54;
           4*a > 1*a by XREAL_1:70;
             then 4*a + 2*b > a + 0 by XREAL_1:10;
          hence thesis by A3,A7;
        end;
    A8: for m st m >= 1 & P[m] holds P[m+1] proof
         let m such that
        A9: m >= 1 and P[m];
         assume
        A10: (for n st n >= 1 & n < m+1 holds f.n <= f.(n+1));
        A11: (m+1)+0 < (m+1)+1 by XREAL_1:10;
          set t1 = [/(m+1)/2\];
          set t2 = [/((m+1)+1)/2\];
              (m+1)*2" > 0*2" by XREAL_1:70;
        then A12: (m+1)/2 > 0;
        A13: [/(m+1)/2\] >= (m+1)/2 by INT_1:def 5;
        A14: t1 > 0 by A12,INT_1:def 5;
            reconsider t1 as Element of NAT by A13,INT_1:16;
               [/((m+1)+1)/2\] >= ((m+1)+1)/2 by INT_1:def 5;
            then reconsider t2 as Element of NAT by INT_1:16;
               f.t1 <= f.t2 proof
             per cases by NAT_D:12;
             suppose (m+1) mod 2 = 0;
             then ((m qua Integer)+1) mod 2 = 0 by NEWTON:101;
                 then 0 = (m+1) - (((m qua Integer)+1) div 2)*2 by INT_1:def 8
                  .= (m+1) + -(((m qua Integer)+1) div 2)*2;
                 then 2 divides (m qua Integer)+1 by INT_1:def 9;
                 then 2 divides ((m qua Integer)+1)+2 by WSIERP_1:9;
                 then reconsider t4 = (m+3)/2 as Integer by WSIERP_1:22;
             A15: t4 - 1 is Integer;
             A16: (m+3)/2 - 1 = (m+1)/2;
                   A17: t2 >= ((m+1)+1)/2 by INT_1:def 5;
                   A18: t2 < ((m+1)+1)/2 + 2/2 by INT_1:def 5;
             A19: now assume
              A20: t2 <> t4;
                  thus contradiction proof
                   per cases by A20,REAL_1:def 5;
                   suppose t2 > t4;
                       then A21: t2 >= (m+3)/2 + 2/2 by INT_1:20;
                          m+5 > m+4 by XREAL_1:10;
                       then (m+5)*2" > (m+4)*2" by XREAL_1:70;
                       then (m+5)/2 > (m+4)*2";
                       then (m+5)/2 > (m+4)/2;
                    hence thesis by A18,A21,XXREAL_0:2;
                   end;
                   suppose t2 < t4;
                       then t2 + 1 <= t4 by INT_1:20;
                   then A22: t2 <= (m+1)/2 by A16,XREAL_1:21;
                          m+1 < m+2 by XREAL_1:10;
                       then (m+1)*2" < (m+2)*2" by XREAL_1:70;
                       then (m+1)/2 < (m+2)*2";
                       then (m+1)/2 < (m+2)/2;
                    hence thesis by A17,A22,XXREAL_0:2;
                  end;
                  end;
              end;
                    (m+1)/2 + 1 = t4;
             then A23: t1 + 1 = t2 by A15,A19,INT_1:54;
             A24: t1 >= 1+0 by A14,NAT_1:13;
                    m+1 >= 1+1 by A9,XREAL_1:8;
                 then t1 < m+1 by Lm53;
              hence thesis by A10,A23,A24;
             end;
             suppose (m+1) mod 2 = 1;
             then A25: ((m qua Integer)+1) mod 2 = 1 by NEWTON:101;
                   A26: t1 < (m+1)/2 + 2/2 by INT_1:def 5;
             A27: 2 divides
((m qua Integer)+2) iff (m+2)/2 is Integer by WSIERP_1:22;
                    1 = (m+1) - (((m qua Integer)+1) div 2) * 2 by A25,
INT_1:def 8
                  .= (m+1) + -(((m qua Integer)+1) div 2) * 2;
                 then 2 divides (m qua Integer) by INT_1:def 9;
                 then reconsider t3 = (m+2)/2 as Integer by A27,WSIERP_1:9;
              now assume
              A28: t1 <> t3;
                  thus contradiction proof
                   per cases by A28,REAL_1:def 5;
                   suppose t1 > t3;
                       then A29: t1 >= t3 + 2/2 by INT_1:20;
                          m+4 > m+3 by XREAL_1:10;
                       then (m+4)*2" > (m+3)*2" by XREAL_1:70;
                       then (m+4)/2 > (m+3)*2";
                       then (m+4)/2 > (m+3)/2;
                    hence thesis by A26,A29,XXREAL_0:2;
                   end;
                   suppose t1 < t3;
                       then t1 + 1 <= t3 by INT_1:20;
                       then A30: t1 <= t3 - 2/2 by XREAL_1:21;
                          m+0 < m+1 by XREAL_1:10;
                       then m*2" < (m+1)*2" by XREAL_1:70;
                       then m/2 < (m+1)*2";
                       then m/2 < (m+1)/2;
                    hence thesis by A13,A30,XXREAL_0:2;
                  end;
                  end;
              end;
              hence thesis by INT_1:54;
            end;
            end;
        then A31: 4*(f.t1) <= 4*(f.t2) by XREAL_1:66;
        A32: b*(m+1) < b*((m+1)+1) by A11,XREAL_1:70;
        A33: m+1 >= 1+1 by A9,XREAL_1:8;
            then consider n1 such that
        A34: n1 = [/(m+1)/2\] and
        A35: Prob28(m+1,a,b) = 4*Prob28(n1,a,b) + b*(m+1) by Lm54;
        A36: f.(m+1) = 4*Prob28(n1,a,b) + b*(m+1) by A35,Def7
                   .= 4*(f.t1) + b*(m+1) by A34,Def7;
               (m+1)+1 >= 2 by A11,A33,XXREAL_0:2;
            then consider n2 such that
        A37: n2 = [/((m+1)+1)/2\] and
        A38: Prob28((m+1)+1,a,b) = 4*Prob28(n2,a,b) + b*((m+1)+1) by Lm54;
               f.((m+1)+1) = 4*Prob28(n2,a,b) + b*((m+1)+1) by A38,Def7
                       .= 4*(f.t2) + b*((m+1)+1) by A37,Def7;
         hence f.(m+1) <= f.((m+1)+1) by A31,A32,A36,XREAL_1:10;
        end;
     thus thesis from NAT_1:sch 8(A2, A8);
     end;
     hence thesis;
    end;
    for n st n >= 1 holds R[n] from NAT_1:sch 9(A1);
 hence thesis by ASYMPT_0:def 8;
end;

:: Problem 3.29 -- Proven in theorems ASYMPT_0:39

begin :: Problem 3.30

definition
 func POWEROF2SET -> non empty Subset of NAT equals
  { 2 to_power n where n is Element of NAT : not contradiction };
 coherence proof
  set IT = { 2 to_power n where n is Element of NAT : not contradiction };
 A1: 2 to_power 1 in IT;
       now let x; assume x in IT;
     then consider n being Element of NAT such that
    A2: 2 to_power n = x and not contradiction;
     thus x in NAT by A2;
    end;
  hence thesis by A1,TARSKI:def 3;
 end;
end;

Lm55:
 for n st n >= 2 holds n^2 > n+1
proof
  defpred P[Element of NAT] means $1^2 > $1+1;
A1: P[2];
A2: for k st k >= 2 & P[k] holds P[k+1] proof let k such that
    A3: k >= 2 and
    A4: k^2 > k+1;
         A5: k^2 + (2*k + 1) > (k+1) + (2*k + 1) by A4,XREAL_1:8;
         2*k > 2*0 by A3,XREAL_1:70;
       then 2*k + 1 > 0 + 1 by XREAL_1:8;
       then (k+1) + (2*k + 1) > (k+1) + 1 by XREAL_1:8;
     hence (k+1)^2 > (k+1) + 1 by A5,XXREAL_0:2;
    end;
      for n st n >= 2 holds P[n] from NAT_1:sch 8(A1, A2);
   hence thesis;
end;

Lm56:
 for n st n >= 1 holds (2 to_power (n+1)) - (2 to_power n) > 1
proof
 let n such that
A1: n >= 1;
      2 to_power n >= 2 to_power 1 by A1,PRE_FF:10;
   then (2 to_power n)*1 >= 2 by POWER:30;
   then (2 to_power n)*2 - (2 to_power n)*1 > 1 by XXREAL_0:2;
   then (2 to_power n)*(2 to_power 1) - (2 to_power n) > 1 by POWER:30;
 hence (2 to_power (n+1)) - (2 to_power n) > 1 by POWER:32;
end;

Lm57:
 for n st n >= 2 holds not ((2 to_power n) - 1) in POWEROF2SET
proof
 let n such that
A1: n >= 2;
A2:1 = 2-1;
   assume (2 to_power n) - 1 in POWEROF2SET;
   then consider m such that
A3: 2 to_power m = (2 to_power n) - 1;
A4: now assume m >= n;
   then A5: 2 to_power m >= 2 to_power n by PRE_FF:10;
         (2 to_power n) + 1 > (2 to_power n) + 0 by XREAL_1:8;
    hence contradiction by A3,A5,XREAL_1:21;
   end;
A6: n-1 >= 1 by A1,A2,XREAL_1:11;
   then n-1 is Element of NAT by INT_1:16;
   then (2 to_power ((n+-1)+1)) - (2 to_power (n-1)) > 1 by A6,Lm56;
   then (2 to_power n) > 1 + (2 to_power (n-1)) by XREAL_1:22;
then A7: (2 to_power n) - 1 > (2 to_power (n-1)) by XREAL_1:22;
then A8: m >= n-1 by A3,POWER:44;
      m+1 <= n by A4,INT_1:20;
   then m <= n-1 by XREAL_1:21;
 hence contradiction by A3,A7,A8,XXREAL_0:1;
end;

theorem :: Part a)
