  then
A13: h is convergent by SEQ_2:def 6;
  then
A14: lim h = 0 by A3,SEQ_2:def 7;
  then not g in Big_Oh(f) by A13,ASYMPT_0:16;
  then
A15: not f in Big_Omega(g) by ASYMPT_0:19;
