    end;
  end;
  then
A13: h = (1/e)(#)q by SEQ_1:13;
A14: lim q = 0 by A4,Lm10;
  lim h = lim((1/e)(#)q) by A7,SEQ_1:13
    .= (1/e)*0 by A5,A14,SEQ_2:22;
  hence thesis by A5,A13,SEQ_2:21;
