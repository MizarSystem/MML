    end;
  end;
  suppose
A20: not LE a,b,C & LE b,c,C & not LE c,d,C;
    then
A21: LE b,a,C & LE d,c,C by A1,A2,A3,A4,JORDAN16:11;
    thus thesis
    proof
      per cases by A1,A4,JORDAN16:11;
      suppose
        LE a,d,C;
        hence thesis by A21,Def1;
      end;
      suppose
A22:    LE d,a,C;
