        LE a,c,C or LE c,a,C by A1,A3,JORDAN16:11;
        hence thesis by A20,A21,A22,A23,Def1;
      end;
    end;
  end;
  suppose
A24: not LE a,b,C & not LE b,c,C & LE c,d,C;
    then
A25: LE b,a,C & LE c,b,C by A1,A2,A3,JORDAN16:11;
