      suppose
A26:    LE d,a,C;
        LE b,d,C or LE d,b,C by A2,A4,JORDAN16:11;
        hence thesis by A24,A25,A26,Def1;
      end;
    end;
  end;
  suppose
A27: not LE a,b,C & not LE b,c,C & not LE c,d,C;
    then
A28: LE d,c,C by A3,A4,JORDAN16:11;
    LE b,a,C & LE c,b,C by A1,A2,A3,A27,JORDAN16:11;
    hence thesis by A28,Def1;
  end;
end;
