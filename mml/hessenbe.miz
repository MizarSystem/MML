environ

 vocabularies ANPROJ_2, SUBSET_1, PARSP_2, AFF_2, INCSP_1;
 notations STRUCT_0, COLLSP, ANPROJ_2;
 constructors ANPROJ_2;
 registrations ANPROJ_2, PROJDES1;
 theorems ANPROJ_2, COLLSP;

begin

reserve PCPP for CollProjectiveSpace,
  a,a9,a1,a2,a3,b,b9,b1,b2,c,c1,c3,d,d9,e,
  o,p,p1,p2,p3,r,q, q1,q2,q3,x,y for Element of PCPP;

theorem Th3:
  a,b,c is_collinear implies b,c,a is_collinear & c,a,b
  is_collinear & b,a,c is_collinear & a,c,b is_collinear & c,b,a is_collinear
proof
  assume
A1: a,b,c is_collinear;
  then b,a,c is_collinear by COLLSP:4;
  then
A2: b,c,a is_collinear by COLLSP:4;
  a,c,b is_collinear by A1,COLLSP:4;
  hence thesis by A2,COLLSP:4;
end;

theorem Th4:
  a<>b & a,b,c is_collinear & a,b,d is_collinear implies a,c,d is_collinear
proof
A1: a,b,a is_collinear by ANPROJ_2:def 7;
  assume a<>b & a,b,c is_collinear & a,b,d is_collinear;
  hence thesis by A1,ANPROJ_2:def 8;
end;

theorem
  p<>q & a,b,p is_collinear & a,b,q is_collinear & p,q,r is_collinear
  implies a,b,r is_collinear
proof
  assume that
A1: p<>q and
A2: a,b,p is_collinear & a,b,q is_collinear and
A3: p,q,r is_collinear;
  now
    assume
A4: a<>b;
    b,a,p is_collinear & b,a,q is_collinear by A2,Th3;
    then b,p,q is_collinear by A4,Th4;
    then
A5: p,q,b is_collinear by Th3;
    a,p,q is_collinear by A2,A4,Th4;
    then p,q,a is_collinear by Th3;
    hence thesis by A1,A3,A5,ANPROJ_2:def 8;
  end;
  hence thesis by ANPROJ_2:def 7;
end;

theorem Th6:
  p<>q implies ex r st not p,q,r is_collinear
proof
  consider a,b,c such that
A1: not a,b,c is_collinear by COLLSP:def 6;
  assume p<>q;
  then p,q,a is_collinear & p,q,b is_collinear & p,q,c is_collinear implies
  contradiction by A1,ANPROJ_2:def 8;
  hence thesis;
end;

theorem
  ex q,r st not p,q,r is_collinear
proof
  consider q such that
A1: p<>q and
  p<>q and
  p,p,q is_collinear by ANPROJ_2:def 10;
  ex r st not p,q,r is_collinear by A1,Th6;
  hence thesis;
end;

theorem Th8:
  not a,b,c is_collinear & a,b,b9 is_collinear & a<>b9 implies not
  a,b9,c is_collinear
proof
  assume that
A1: not a,b,c is_collinear and
A2: a,b,b9 is_collinear and
A3: a<>b9;
  assume
A4: not thesis;
  a,b9,b is_collinear by A2,Th3;
  hence contradiction by A1,A3,A4,Th4;
end;

theorem Th9:
  not a,b,c is_collinear & a,b,d is_collinear & a,c,d is_collinear implies a=d
proof
  assume that
A1: not a,b,c is_collinear and
A2: a,b,d is_collinear & a,c,d is_collinear;
  assume
A3: not thesis;
A4: a,d,a is_collinear by ANPROJ_2:def 7;
  a,d,b is_collinear & a,d,c is_collinear by A2,Th3;
  hence contradiction by A1,A3,A4,ANPROJ_2:def 8;
end;

theorem Th10:
  not o,a,d is_collinear & o,d,d9 is_collinear & d<>d9 & a9,d9,x
  is_collinear & o,a,a9 is_collinear & o<>a9 implies x<>d
proof
  assume that
A1: not o,a,d is_collinear and
A2: o,d,d9 is_collinear and
A3: d<>d9 and
A4: a9,d9,x is_collinear and
A5: o,a,a9 is_collinear and
A6: o<>a9;
  assume not thesis;
  then
A7: d,d9,a9 is_collinear by A4,Th3;
  d,d9,o is_collinear by A2,Th3;
  then d,o,a9 is_collinear by A3,A7,Th4;
  then
A8: o,a9,d is_collinear by Th3;
  o,a9,a is_collinear by A5,Th3;
  hence contradiction by A1,A6,A8,Th4;
end;

theorem Th12:
  not a1,a2,a3 is_collinear & a1,a2,c3 is_collinear & a2,a3,c1
is_collinear & c1,c3,x is_collinear & c3<>a1 & c3<>a2 & c1<>a2 & c1<>a3 implies
  a1<>x & a3<>x
proof
  assume that
A1: not a1,a2,a3 is_collinear and
A2: a1,a2,c3 is_collinear and
A3: a2,a3,c1 is_collinear and
A4: c1,c3,x is_collinear and
A5: c3<>a1 and
A6: c3<>a2 and
A7: c1<>a2 and
A8: c1<>a3;
A9: a3<>x
  proof
    assume not thesis;
    then
A10: a3,c1,c3 is_collinear by A4,Th3;
    a3,c1,a2 is_collinear by A3,Th3;
    then a3,a2,c3 is_collinear by A8,A10,Th4;
    then
A11: a2,c3,a3 is_collinear by Th3;
    a2,c3,a1 is_collinear by A2,Th3;
    then a2,a1,a3 is_collinear by A6,A11,Th4;
    hence contradiction by A1,Th3;
  end;
  a1<>x
  proof
    assume not thesis;
    then
A12: a1,c3,c1 is_collinear by A4,Th3;
    a1,c3,a2 is_collinear by A2,Th3;
    then a1,c1,a2 is_collinear by A5,A12,Th4;
    then
A13: a2,c1,a1 is_collinear by Th3;
    a2,c1,a3 is_collinear by A3,Th3;
    then a2,a1,a3 is_collinear by A7,A13,Th4;
    hence contradiction by A1,Th3;
  end;
  hence thesis by A9;
end;

theorem Th13:
  not a,b,c is_collinear & a,b,d is_collinear & c,e,d is_collinear
  & e<>c & d<>a implies not e,a,c is_collinear
proof
  assume that
A1: not a,b,c is_collinear and
A2: a,b,d is_collinear and
A3: c,e,d is_collinear & e<>c and
A4: d<>a;
  assume not thesis;
  then c,e,a is_collinear by Th3;
  then c,a,d is_collinear by A3,Th4;
  then
A5: a,d,c is_collinear by Th3;
  a,d,b is_collinear by A2,Th3;
  hence contradiction by A1,A4,A5,Th4;
end;

theorem Th14:
  not p1,p2,q1 is_collinear & p1,p2,q2 is_collinear & q1,q2,q3
  is_collinear & q2<>q3 implies not p2,p1,q3 is_collinear
proof
  assume that
A1: not p1,p2,q1 is_collinear and
A2: p1,p2,q2 is_collinear and
A3: q1,q2,q3 is_collinear and
A4: q2<>q3;
A5: p1<>p2 by A1,ANPROJ_2:def 7;
  assume
A6: not thesis;
  then p1,p2,q3 is_collinear by Th3;
  then p1,q2,q3 is_collinear by A2,A5,Th4;
  then
A7: q2,q3,p1 is_collinear by Th3;
  p2,p1,q2 is_collinear by A2,Th3;
  then p2,q2,q3 is_collinear by A6,A5,Th4;
  then
A8: q2,q3,p2 is_collinear by Th3;
  q2,q3,q1 is_collinear by A3,Th3;
  hence contradiction by A1,A4,A7,A8,ANPROJ_2:def 8;
end;

theorem
  not p1,p2,q1 is_collinear & p1,p2,p3 is_collinear & q1,q2,p3
  is_collinear & p3<>q2 & p2<>p3 implies not p3,p2,q2 is_collinear
proof
  assume that
A1: not p1,p2,q1 is_collinear and
A2: p1,p2,p3 is_collinear and
A3: q1,q2,p3 is_collinear and
A4: p3<>q2 and
A5: p2<>p3;
  assume
A6: not thesis;
  p3,p2,p1 is_collinear by A2,Th3;
  then
A7: p3,q2,p1 is_collinear by A5,A6,Th4;
A8: p3,q2,q1 is_collinear by A3,Th3;
  p3,q2,p2 is_collinear by A6,Th3;
  hence contradiction by A1,A4,A7,A8,ANPROJ_2:def 8;
end;

theorem Th16:
  not p1,p2,q1 is_collinear & p1,p2,p3 is_collinear & q1,q2,p1
  is_collinear & p1<>p3 & p1<>q2 implies not p3,p1,q2 is_collinear
proof
  assume that
A1: not p1,p2,q1 is_collinear and
A2: p1,p2,p3 is_collinear and
A3: q1,q2,p1 is_collinear and
A4: p1<>p3 and
A5: p1<>q2;
A6: p1,q2,q1 is_collinear by A3,Th3;
  assume not thesis;
  then
A7: p1,p3,q2 is_collinear by Th3;
  p1,p3,p2 is_collinear by A2,Th3;
  then p1,q2,p2 is_collinear by A4,A7,Th4;
  hence contradiction by A1,A5,A6,Th4;
end;

theorem Th17:
  a1<>a2 & b1<>b2 & b1,b2,x is_collinear & b1,b2,y is_collinear &
a1,a2,x is_collinear & a1,a2,y is_collinear & not a1,a2,b1 is_collinear implies
  x=y
proof
  assume that
A1: a1<>a2 and
A2: b1<>b2 & b1,b2,x is_collinear & b1,b2,y is_collinear and
A3: a1,a2,x is_collinear & a1,a2,y is_collinear and
A4: not a1,a2,b1 is_collinear;
  a1,a2,a2 is_collinear by ANPROJ_2:def 7;
  then
A5: x,y,a2 is_collinear by A1,A3,ANPROJ_2:def 8;
  b1,b2,b1 is_collinear by ANPROJ_2:def 7;
  then
A6: x,y,b1 is_collinear by A2,ANPROJ_2:def 8;
  assume
A7: not thesis;
  a1,a2,a1 is_collinear by ANPROJ_2:def 7;
  then x,y,a1 is_collinear by A1,A3,ANPROJ_2:def 8;
  hence contradiction by A4,A7,A6,A5,ANPROJ_2:def 8;
end;

theorem Th19:
  not o,a1,a2 is_collinear & o,a1,b1 is_collinear & o,a2,b2
  is_collinear & o<>b1 & o<>b2 implies not o,b1,b2 is_collinear
proof
  assume that
A1: ( not o,a1,a2 is_collinear)& o,a1,b1 is_collinear and
A2: o,a2,b2 is_collinear and
A3: o<>b1 and
A4: o<>b2;
  not o,b1,a2 is_collinear by A1,A3,Th8;
  then not o,a2,b1 is_collinear by Th3;
  then not o,b2,b1 is_collinear by A2,A4,Th8;
  hence thesis by Th3;
end;

reserve PCPP for Pappian CollProjectivePlane,
  a,a1,a2,a3,b1,b2,b3,c1,c2,c3,o,p
  ,p1,p2,p3,q,q9, q1,q2,q3,r,r1,r2,r3,x,y,z for Element of PCPP;

theorem Th20:
  p2<>p3 & p1<>p3 & q2<>q3 & q1<>q2 & q1<>q3 & not p1,p2,q1
  is_collinear & p1,p2,p3 is_collinear & q1,q2,q3 is_collinear & p1,q2,r3
  is_collinear & q1,p2,r3 is_collinear & p1,q3,r2 is_collinear & p3,q1,r2
  is_collinear & p2,q3,r1 is_collinear & p3,q2,r1 is_collinear implies r1,r2,r3
  is_collinear
proof
  assume that
A1: p2<>p3 and
A2: p1<>p3 and
A3: q2<>q3 and
A4: q1<>q2 and
A5: q1<>q3 and
A6: not p1,p2,q1 is_collinear and
A7: p1,p2,p3 is_collinear and
A8: q1,q2,q3 is_collinear and
A9: p1,q2,r3 is_collinear and
A10: q1,p2,r3 is_collinear and
A11: p1,q3,r2 is_collinear and
A12: p3,q1,r2 is_collinear and
A13: p2,q3,r1 is_collinear and
A14: p3,q2,r1 is_collinear;
A15: p1<>p2 by A6,ANPROJ_2:def 7;
A16: now
    assume
A17: not p1,p2,q2 is_collinear;
A18: now
      assume
A19:  not p1,p2,q3 is_collinear;
A20:  now
A21:    now
          ( not p2,p1,q2 is_collinear)& p2,p1,p3 is_collinear by A7,A17,Th3;
          then not p2,p3,q2 is_collinear by A1,Th8;
          then
A22:      not q2,p2,p3 is_collinear by Th3;
          assume
A23:      q1,q2,p2 is_collinear;
          then
A24:      p2,q1,q2 is_collinear by Th3;
          p2<>q1 & p2,q1,r3 is_collinear by A6,A10,Th3,ANPROJ_2:def 7;
          then p2,r3,q2 is_collinear by A24,Th4;
          then
A25:      q2,p2,r3 is_collinear by Th3;
          q2,p1,r3 is_collinear & not q2,p1,p2 is_collinear by A9,A17,Th3;
          then
A26:      q2=r3 by A25,Th9;
A27:      q2,q1,q3 is_collinear by A8,Th3;
          q2,q1,p2 is_collinear by A23,Th3;
          then q2,p2,q3 is_collinear by A4,A27,Th4;
          then
A28:      p2,q3,q2 is_collinear by Th3;
          p2<>q3 by A19,ANPROJ_2:def 7;
          then p2,r1,q2 is_collinear by A13,A28,Th4;
          then
A29:      q2,p2,r1 is_collinear by Th3;
          q2,p3,r1 is_collinear by A14,Th3;
          then q2=r1 by A29,A22,Th9;
          hence thesis by A26,ANPROJ_2:def 7;
        end;
A30:    now
          ( not p2,p1,q3 is_collinear)& p2,p1,p3 is_collinear by A7,A19,Th3;
          then not p2,p3,q3 is_collinear by A1,Th8;
          then
A31:      not q3,p2,p3 is_collinear by Th3;
          assume
A32:      q1,q2,p3 is_collinear;
          then
A33:      q2,q1,p3 is_collinear by Th3;
          q2,q1,q3 is_collinear by A8,Th3;
          then q2,q3, p3 is_collinear by A4,A33,Th4;
          then p3,q2,q3 is_collinear by Th3;
          then p3,r1,q3 is_collinear by A7,A14,A17,Th4;
          then
A34:      q3,p3,r1 is_collinear by Th3;
          not p1,p3,q3 is_collinear by A2,A7,A19,Th8;
          then
A35:      not q3,p1,p3 is_collinear by Th3;
          q1,q3,p3 is_collinear by A4,A8,A32,Th4;
          then p3,q1,q3 is_collinear by Th3;
          then p3,r2,q3 is_collinear by A6,A7,A12,Th4;
          then
A36:      q3,p3,r2 is_collinear by Th3;
          q3,p2,r1 is_collinear by A13,Th3;
          then
A37:      q3=r1 by A34,A31,Th9;
          q3,p1,r2 is_collinear by A11,Th3;
          then q3=r2 by A36,A35,Th9;
          hence thesis by A37,ANPROJ_2:def 7;
        end;
A38:    now
          not p1,p3,q1 is_collinear by A2,A6,A7,Th8;
          then
A39:      not q1,p1,p3 is_collinear by Th3;
          assume
A40:      q1,q2,p1 is_collinear;
          then q1,p1,q3 is_collinear by A4,A8,Th4;
          then
A41:      p1,q3,q1 is_collinear by Th3;
          p1<>q3 by A19,ANPROJ_2:def 7;
          then p1,r2,q1 is_collinear by A11,A41,Th4;
          then
A42:      q1,p1,r2 is_collinear by Th3;
A43:      p1<>q2 by A17,ANPROJ_2:def 7;
          p1,q2,q1 is_collinear by A40,Th3;
          then p1,r3,q1 is_collinear by A9,A43,Th4;
          then
A44:      q1,p1,r3 is_collinear by Th3;
          not q1,p2,p1 is_collinear by A6,Th3;
          then
A45:      q1=r3 by A10,A44,Th9;
          q1,p3,r2 is_collinear by A12,Th3;
          then q1=r2 by A42,A39,Th9;
          hence thesis by A45,ANPROJ_2:def 7;
        end;
        assume q1,q2,p1 is_collinear or q1,q2,p2 is_collinear or q1,q2,p3
        is_collinear;
        hence thesis by A38,A21,A30;
      end;
      now
        assume that
A46:    not q1,q2,p1 is_collinear and
A47:    ( not q1,q2,p2 is_collinear)& not q1,q2,p3 is_collinear;
        consider o such that
A48:    p1,p2,o is_collinear and
A49:    q1,q2,o is_collinear by ANPROJ_2:def 14;
        not p1,o,q1 is_collinear by A6,A46,A48,A49,Th8;
        then
A50:    not o,p1,q1 is_collinear by Th3;
        q1,q3,o is_collinear by A4,A8,A49,Th4;
        then
A51:    o,q1,q3 is_collinear by Th3;
        p1,p3,o is_collinear by A7,A15,A48,Th4;
        then
A52:    o,p1,p3 is_collinear by Th3;
        o,p1,p2 is_collinear & o,q1,q2 is_collinear by A48,A49,Th3;
        hence
        thesis by A1,A2,A3,A4,A5,A9,A10,A11,A12,A13,A14,A15,A17,A19,A47,A48,A49
,A52,A51,A50,ANPROJ_2:def 13;
      end;
      hence thesis by A20;
    end;
    now
      assume
A53:  p1,p2,q3 is_collinear;
A54:  now
        assume that
A55:    p2<>q3 and
A56:    p1<>q3;
        p1,q3,p2 is_collinear by A53,Th3;
        then p1,p2,r2 is_collinear by A11,A56,Th4;
        then p1,r2,p3 is_collinear by A7,A15,Th4;
        then
A57:    p3,p1,r2 is_collinear by Th3;
        not p1,p3,q1 is_collinear by A2,A6,A7,Th8;
        then not p3,q1,p1 is_collinear by Th3;
        then
A58:    p3=r2 by A12,A57,Th9;
A59:    p2,p1,p3 is_collinear by A7,Th3;
        p2,q3,p1 is_collinear by A53,Th3;
        then p2,p1,r1 is_collinear by A13,A55,Th4;
        then p2,r1,p3 is_collinear by A15,A59,Th4;
        then
A60:    p3,p2,r1 is_collinear by Th3;
        ( not p2,p1,q2 is_collinear)& p2,p1,p3 is_collinear by A7,A17,Th3;
        then not p2,p3,q2 is_collinear by A1,Th8;
        then not p3,q2,p2 is_collinear by Th3;
        then p3=r1 by A14,A60,Th9;
        hence thesis by A58,ANPROJ_2:def 7;
      end;
A61:  now
        not p1,p3,q1 is_collinear by A2,A6,A7,Th8;
        then
A62:    not p3,p1,q1 is_collinear by Th3;
A63:    not p2,q1,p1 is_collinear by A6,Th3;
A64:    p2,q1,r3 is_collinear by A10,Th3;
        assume
A65:    p2=q3;
        then p2,q1,q2 is_collinear by A8,Th3;
        then p2,q2,r3 is_collinear by A5,A65,A64,Th4;
        then
A66:    q2,p2,r3 is_collinear by Th3;
        p2,q1,q2 is_collinear by A8,A65,Th3;
        then not p2,q2,p1 is_collinear by A3,A65,A63,Th8;
        then
A67:    not q2,p1,p2 is_collinear by Th3;
        p1,p3,r2 is_collinear by A7,A11,A15,A65,Th4;
        then p3,p1,r2 is_collinear by Th3;
        then
A68:    p3=r2 by A12,A62,Th9;
        q2,p1,r3 is_collinear by A9,Th3;
        then q2=r3 by A66,A67,Th9;
        hence thesis by A14,A68,Th3;
      end;
      now
        assume
A69:    p1=q3;
        then p1,p2,r1 is_collinear by A13,Th3;
        then p1,p3,r1 is_collinear by A7,A15,Th4;
        then
A70:    p3,p1,r1 is_collinear by Th3;
        p1,q2,q1 is_collinear by A8,A69,Th3;
        then p1,q1,r3 is_collinear by A3,A9,A69,Th4;
        then
A71:    q1,p1,r3 is_collinear by Th3;
        not p3,p1,q2 is_collinear by A2,A3,A6,A7,A8,A69,Th16;
        then
A72:    p3=r1 by A14,A70,Th9;
        not q1,p1,p2 is_collinear by A6,Th3;
        then q1=r3 by A10,A71,Th9;
        hence thesis by A12,A72,Th3;
      end;
      hence thesis by A61,A54;
    end;
    hence thesis by A18;
  end;
  now
A73: now
