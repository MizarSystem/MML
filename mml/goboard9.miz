 notations TARSKI, XBOOLE_0, SUBSET_1, ORDINAL1, CARD_1, NUMBERS, COMPLEX1,
      REAL_1, NAT_1, NAT_D, FUNCT_1, PARTFUN1, FINSEQ_1, MATRIX_1, STRUCT_0,
