      TREES_1, TOPS_1, ARYTM_3, GOBOARD9, FINSEQ_4, ORDINAL2;
 notations TARSKI, XBOOLE_0, SUBSET_1, ORDINAL1, CARD_1, NUMBERS, COMPLEX1,
      REAL_1, NAT_1, NAT_D, BINARITH, FUNCT_1, PARTFUN1, FINSEQ_1, MATRIX_1,
      STRUCT_0, PRE_TOPC, TOPS_1, CONNSP_1, EUCLID, TOPREAL1, GOBOARD1,
      GOBOARD2, JORDAN1, FINSEQ_5, FINSEQ_6, GOBOARD5, XXREAL_0;
 constructors PARTFUN1, XXREAL_0, REAL_1, NAT_1, COMPLEX1, BINARITH, FINSEQ_5,
      TOPS_1, CONNSP_1, GOBOARD2, JORDAN1, GOBOARD5, SEQ_1, GOBOARD1, NAT_D;
 registrations XBOOLE_0, RELSET_1, NUMBERS, XXREAL_0, MEMBERED, FINSEQ_1,
      FINSEQ_6, STRUCT_0, PRE_TOPC, EUCLID, TOPREAL1, GOBOARD2, GOBOARD5,
      ORDINAL1, FINSET_1;
 requirements REAL, NUMERALS, BOOLE, SUBSET, ARITHM;
 definitions TARSKI, FINSEQ_6, GOBOARD5, GOBOARD1, JORDAN1, XBOOLE_0, STRUCT_0;
 theorems CONNSP_1, GOBOARD5, TOPS_1, SPPOL_2, PRE_TOPC, GOBOARD1, TARSKI,
      TOPREAL1, GOBOARD2, FINSEQ_3, NAT_1, SUBSET_1, FINSEQ_6, GOBOARD6,
      GOBOARD7, GOBOARD8, FINSEQ_1, FINSEQ_5, JORDAN1, TSEP_1, INT_1, XBOOLE_0,
      XBOOLE_1, XREAL_1, COMPLEX1, XXREAL_0, PARTFUN1, MATRIX_1, NAT_D;
 schemes NAT_1;

begin

reserve f for non constant standard special_circular_sequence,
  i,j,k,i1,i2,j1,j2 for Element of NAT,
  r,s,r1,s1,r2,s2 for Real,
  a,b for natural number,
  p,q for Point of TOP-REAL 2,
  G for Go-board;

canceled 2;

theorem Th3:
  for GX being TopSpace, A1,A2,B being Subset of GX
  st A1 is_a_component_of B & A2 is_a_component_of B holds
  A1 = A2 or A1 misses A2
proof
  let GX be TopSpace, A1,A2,B be Subset of GX;
  assume A1 is_a_component_of B;
  then consider B1 being Subset of GX|B such that
A1: B1 = A1 and
A2: B1 is_a_component_of GX|B by CONNSP_1:def 6;
  assume A2 is_a_component_of B;
  then ex B2 being Subset of GX|B st B2 = A2 & B2 is_a_component_of GX|B
  by CONNSP_1:def 6;
  hence A1 = A2 or A1 misses A2 by A1,A2,CONNSP_1:37;
end;

theorem Th4:
  for GX being TopSpace, A,B being Subset of GX,
  AA being Subset of GX|B st A = AA holds GX|A = GX|B|AA
proof
  let GX be TopSpace, A,B be Subset of GX;
  let AA be Subset of GX|B;
  assume
A1: A = AA;
  the carrier of GX|A = [#](GX|A)
    .= A by PRE_TOPC:def 10;
  then reconsider GY = GX|A as strict SubSpace of GX|B by A1,TSEP_1:4;
  [#] GY = AA by A1,PRE_TOPC:def 10;
  hence GX|A = GX|B|AA by PRE_TOPC:def 10;
end;

theorem Th5:
  for GX being non empty TopSpace, A, B being non empty Subset of GX
  st A c= B & A is connected
  ex C being Subset of GX st C is_a_component_of B & A c= C
proof
  let GX be non empty TopSpace, A, B be non empty Subset of GX such that
A1: A c= B and
A2: A is connected;
  consider p being set such that
A3: p in A by XBOOLE_0:def 1;
A4: B = [#](GX|B) by PRE_TOPC:def 10
    .= the carrier of GX|B;
  then reconsider p as Point of GX|B by A1,A3;
  reconsider C = Component_of p as Subset of GX by PRE_TOPC:39;
  take C;
A5: Component_of p is_a_component_of GX|B by CONNSP_1:43;
  hence C is_a_component_of B by CONNSP_1:def 6;
  reconsider AA = A as Subset of GX|B by A1,A4;
  GX|A is connected by A2,CONNSP_1:def 3;
  then GX|B|AA is connected by Th4;
  then
A6: AA is connected by CONNSP_1:def 3;
  p in Component_of p by CONNSP_1:40;
  then AA /\ Component_of p <> {}(GX|B) by A3,XBOOLE_0:def 4;
  then AA meets Component_of p by XBOOLE_0:def 7;
  hence A c= C by A5,A6,CONNSP_1:38;
end;

theorem Th6:
  for GX being non empty TopSpace, A,B,C,D being Subset of GX
  st B is connected & C is_a_component_of D & A c= C & A meets B & B c= D
  holds B c= C
proof
  let GX be non empty TopSpace, A,B,C,D be Subset of GX such that
A1: B is connected and
A2: C is_a_component_of D and
A3: A c= C and
A4: A meets B and
A5: B c= D;
A6: A <> {} & B <> {} by A4,XBOOLE_1:65;
  reconsider A,B as non empty Subset of GX by A4,XBOOLE_1:65;
  reconsider C,D as non empty Subset of GX by A3,A5,A6,XBOOLE_1:3;
  consider CC being Subset of GX such that
A7: CC is_a_component_of D and
A8: B c= CC by A1,A5,Th5;
A9: A meets CC by A4,A8,XBOOLE_1:63;
  consider C1 being Subset of GX|D such that
A10: C1 = C and
A11: C1 is_a_component_of GX|D by A2,CONNSP_1:def 6;
  consider CC1 being Subset of GX|D such that
A12: CC1 = CC and
A13: CC1 is_a_component_of GX|D by A7,CONNSP_1:def 6;
  thus thesis by A3,A8,A9,A10,A11,A12,A13,CONNSP_1:37,XBOOLE_1:63;
end;

theorem Th7:
  LSeg(p,q) is convex
proof
  for w1,w2 be Point of TOP-REAL 2 st w1 in LSeg(p,q) & w2 in LSeg(p,q)
  holds LSeg(w1,w2) c= LSeg(p,q) by TOPREAL1:12;
  hence LSeg(p,q) is convex by JORDAN1:def 1;
end;

theorem Th8:
  LSeg(p,q) is connected by Th7,JORDAN1:9;

registration
  cluster convex non empty Subset of TOP-REAL 2;
  existence
  proof consider p being Point of TOP-REAL 2;
    take LSeg(p,p);
    thus thesis by Th7;
  end;
end;

theorem Th9:
  for P,Q being convex Subset of TOP-REAL 2 holds P /\ Q is convex
proof
  let P,Q be convex Subset of TOP-REAL 2;
  let p,q;
  assume
A1: p in P /\ Q & q in P /\ Q;
  then p in P & q in P by XBOOLE_0:def 4;
  then
A2: LSeg(p,q) c= P by JORDAN1:def 1;
  p in Q & q in Q by A1,XBOOLE_0:def 4;
  then LSeg(p,q) c= Q by JORDAN1:def 1;
  hence LSeg(p,q) c= P /\ Q by A2,XBOOLE_1:19;
end;

theorem Th10:
  for f being FinSequence of TOP-REAL 2 holds Rev X_axis f = X_axis Rev f
proof
  let f be FinSequence of TOP-REAL 2;
A1: len Rev X_axis f = len X_axis f by FINSEQ_5:def 3
    .= len f by GOBOARD1:def 3
    .= len Rev f by FINSEQ_5:def 3;
  len X_axis f = len f by GOBOARD1:def 3;
  then
A2: dom X_axis f = dom f by FINSEQ_3:31;
A3: len f = len Rev f by FINSEQ_5:def 3;
  now
    let k such that
A4: k in dom Rev X_axis f;
    set l = len f + 1 -' k;
    k <= len f & len f < len f + 1 by A1,A3,A4,FINSEQ_3:27,NAT_1:13;
    then
A5: l + k = len f + 1 by XREAL_1:237,XXREAL_0:2;
A6: Rev Rev X_axis f = X_axis f by FINSEQ_6:29;
    then
A7: l in dom X_axis f by A1,A3,A4,A5,FINSEQ_5:62;
    thus (Rev X_axis f).k = (Rev X_axis f)/.k by A4,PARTFUN1:def 8
      .= (X_axis f)/.l by A1,A3,A4,A5,A6,FINSEQ_5:69
      .= (X_axis f).l by A7,PARTFUN1:def 8
      .= (f/.l)`1 by A7,GOBOARD1:def 3
      .= ((Rev f)/.k)`1 by A1,A2,A3,A4,A5,A6,FINSEQ_5:62,69;
  end;
  hence Rev X_axis f = X_axis Rev f by A1,GOBOARD1:def 3;
end;

theorem Th11:
  for f being FinSequence of TOP-REAL 2 holds Rev Y_axis f = Y_axis Rev f
proof
  let f be FinSequence of TOP-REAL 2;
A1: len Rev Y_axis f = len Y_axis f by FINSEQ_5:def 3
    .= len f by GOBOARD1:def 4
    .= len Rev f by FINSEQ_5:def 3;
  len Y_axis f = len f by GOBOARD1:def 4;
  then
A2: dom Y_axis f = dom f by FINSEQ_3:31;
A3: len f = len Rev f by FINSEQ_5:def 3;
  now
    let k such that
A4: k in dom Rev Y_axis f;
    set l = len f + 1 -' k;
    k <= len f & len f < len f + 1 by A1,A3,A4,FINSEQ_3:27,NAT_1:13;
    then
A5: l + k = len f + 1 by XREAL_1:237,XXREAL_0:2;
A6: Rev Rev Y_axis f = Y_axis f by FINSEQ_6:29;
    then
A7: l in dom Y_axis f by A1,A3,A4,A5,FINSEQ_5:62;
    thus (Rev Y_axis f).k = (Rev Y_axis f)/.k by A4,PARTFUN1:def 8
      .= (Y_axis f)/.l by A1,A3,A4,A5,A6,FINSEQ_5:69
      .= (Y_axis f).l by A7,PARTFUN1:def 8
      .= (f/.l)`2 by A7,GOBOARD1:def 4
      .= ((Rev f)/.k)`2 by A1,A2,A3,A4,A5,A6,FINSEQ_5:62,69;
  end;
  hence Rev Y_axis f = Y_axis Rev f by A1,GOBOARD1:def 4;
end;

Lm1: for f,ff being non empty FinSequence of TOP-REAL 2 st ff = Rev f
holds GoB ff = GoB f
proof
  let f,ff be non empty FinSequence of TOP-REAL 2;
  assume
A1: ff = Rev f;
  then
A2: Rev X_axis f = X_axis ff by Th10;
A3: rng Incr X_axis ff = rng X_axis ff by GOBOARD2:def 2
    .= rng X_axis f by A2,FINSEQ_5:60;
  len Incr X_axis ff = card rng X_axis ff by GOBOARD2:def 2
    .= card rng X_axis f by A2,FINSEQ_5:60;
  then
A4: Incr X_axis f = Incr X_axis ff by A3,GOBOARD2:def 2;
A5: Rev Y_axis f = Y_axis ff by A1,Th11;
A6: rng Incr Y_axis ff = rng Y_axis ff by GOBOARD2:def 2
    .= rng Y_axis f by A5,FINSEQ_5:60;
  len Incr Y_axis ff = card rng Y_axis ff by GOBOARD2:def 2
    .= card rng Y_axis f by A5,FINSEQ_5:60;
  then Incr Y_axis f = Incr Y_axis ff by A6,GOBOARD2:def 2;
  hence GoB ff = GoB(Incr X_axis f,Incr Y_axis f) by A4,GOBOARD2:def 3
    .= GoB f by GOBOARD2:def 3;
end;

registration
  cluster non constant FinSequence;
  existence
  proof
    take <*1,2*>,1,2;
    1 in {1,2} & 2 in {1,2} by TARSKI:def 2;
    hence 1 in dom <*1,2*> & 2 in dom <*1,2*> by FINSEQ_1:4,FINSEQ_3:29;
    <*1,2*>.1 = 1 & <*1,2*>.2 = 2 by FINSEQ_1:61;
    hence thesis;
  end;
end;

registration
  let f be non constant FinSequence;
  cluster Rev f -> non constant;
  coherence
  proof
    consider i,j such that
A1: i in dom f & j in dom f and
A2: f.i <> f.j by GOBOARD1:def 2;
    i <= len f & j <= len f by A1,FINSEQ_3:27;
    then reconsider k1 = len f -i, l1 = len f -j as Element of NAT by INT_1:18;
    take k = k1+1, l = l1+1;
    k + i = len f +1;
    hence k in dom Rev f by A1,FINSEQ_5:62;
    then
A3: (Rev f).k = f.(len f -k +1) by FINSEQ_5:def 3
      .= f.(0 + i);
    l + j = len f +1;
    hence l in dom Rev f by A1,FINSEQ_5:62;
    then (Rev f).l = f.(len f -l +1) by FINSEQ_5:def 3
      .= f.(0 + j);
    hence thesis by A2,A3;
  end;
end;

definition
  let f be standard special_circular_sequence;
  redefine func Rev f -> standard special_circular_sequence;
  coherence
  proof
    reconsider ff = Rev f as non empty FinSequence of TOP-REAL 2;
A1: Rev ff = f by FINSEQ_6:29;
A2: GoB ff = GoB f by Lm1;
A3: f is_sequence_on GoB f by GOBOARD5:def 5;
A4: len f = len ff by FINSEQ_5:def 3;
A5: ff is standard
    proof
      hereby
        let k such that
A6:     k in dom ff;
        set l = len f + 1 -' k;
        k <= len f & len f < len f + 1 by A4,A6,FINSEQ_3:27,NAT_1:13;
        then
A7:     l + k = len f + 1 by XREAL_1:237,XXREAL_0:2;
        then l in dom f by A1,A4,A6,FINSEQ_5:62;
        then consider i,j such that
A8:     [i,j] in Indices GoB f and
A9:     f/.l = (GoB f)*(i,j) by A3,GOBOARD1:def 11;
        take i,j;
        thus [i,j] in Indices GoB ff by A8,Lm1;
        thus ff/.k = (GoB ff)*(i,j) by A1,A2,A4,A6,A7,A9,FINSEQ_5:69;
      end;
      let k such that
A10:  k in dom ff and
A11:  k+1 in dom ff;
      set l = len f + 1 -' (k + 1);
      k <= len f by A4,A10,FINSEQ_3:27;
      then k+1 <= len f +1 by XREAL_1:8;
      then
A12:  l + (k+1) = len f + 1 by XREAL_1:237;
      then
A13:  l in dom f by A1,A4,A11,FINSEQ_5:62;
A14:  l+1 + k = len f + 1 by A12;
      then
A15:  l+1 in dom f by A1,A4,A10,FINSEQ_5:62;
      let i1,j1,i2,j2 such that
A16:  [i1,j1] in Indices GoB ff and
A17:  [i2,j2] in Indices GoB ff and
A18:  ff/.k = (GoB ff)*(i1,j1) and
A19:  ff/.(k+1) = (GoB ff)*(i2,j2);
A20:  abs(i2-i1) = abs(-(i1-i2))
        .= abs(i1-i2) by COMPLEX1:138;
A21:  abs(j2-j1) = abs(-(j1-j2))
        .= abs(j1-j2) by COMPLEX1:138;
A22:  f/.l = (GoB f)*(i2,j2) by A1,A2,A4,A11,A12,A19,FINSEQ_5:69;
      f/.(l+1) = (GoB f)*(i1,j1) by A1,A2,A4,A10,A14,A18,FINSEQ_5:69;
      hence abs(i1-i2)+abs(j1-j2) = 1
      by A2,A3,A13,A15,A16,A17,A20,A21,A22,GOBOARD1:def 11;
    end;
A23: ff is circular
    proof
      thus ff/.1 = f/.len f by FINSEQ_5:68
        .= f/.1 by FINSEQ_6:def 1
        .= ff/.len ff by A4,FINSEQ_5:68;
    end;
    ff is s.c.c.
    proof
      let i,j such that
A24:  i+1 < j and
A25:  i > 1 & j < len ff or j+1 < len ff;
      set k = len f -' i, l = len f -' j;
A26:  j <= len f by A4,A25,NAT_1:13;
      then
A27:  l + j = len f by XREAL_1:237;
      i < j by A24,NAT_1:13;
      then
A28:  k + i = len f by A26,XREAL_1:237,XXREAL_0:2;
      then i+1+k = l+1+j by A27;
      then l+1+j < j+k by A24,XREAL_1:8;
      then
A29:  l+1 < k by XREAL_1:8;
A30:  j+1 <= len f by A4,A25,NAT_1:13;
      per cases by A30,NAT_1:14,XXREAL_0:1;
      suppose j+1 = len f;
        then k+1 < len f by A25,A28,FINSEQ_5:def 3,XREAL_1:8;
        then LSeg(f,k) misses LSeg(f,l) by A29,GOBOARD5:def 4;
        then
A31:    LSeg(f,k) /\ LSeg(f,l) = {} by XBOOLE_0:def 7;
        LSeg(f,k) = LSeg(ff,i) by A28,SPPOL_2:2;
        hence LSeg(ff,i) /\ LSeg(ff,j) = {} by A27,A31,SPPOL_2:2;
      end;
      suppose that
A32:    i >= 1 and
A33:    j+1 < len f;
A34:    l > 1 by A27,A33,XREAL_1:8;
        k+1 <= len f by A28,A32,XREAL_1:8;
        then k < len f by NAT_1:13;
        then LSeg(f,k) misses LSeg(f,l) by A29,A34,GOBOARD5:def 4;
        then
A35:    LSeg(f,k) /\ LSeg(f,l) = {} by XBOOLE_0:def 7;
        LSeg(f,k) = LSeg(ff,i) by A28,SPPOL_2:2;
        hence LSeg(ff,i) /\ LSeg(ff,j) = {} by A27,A35,SPPOL_2:2;
      end;
      suppose i = 0;
        then LSeg(ff,i) = {} by TOPREAL1:def 5;
        hence LSeg(ff,i) /\ LSeg(ff,j) = {};
      end;
    end;
    hence thesis by A5,A23,SPPOL_2:29,42;
  end;
end;

theorem Th12:
  i >= 1 & j >= 1 & i+j = len f implies left_cell(f,i) = right_cell(Rev f,j)
proof
  assume that
A1: i >= 1 and
A2: j >= 1 and
A3: i+j = len f;
A4: i+1 <= len f by A2,A3,XREAL_1:8;
  len f = len Rev f by FINSEQ_5:def 3;
  then
A5: j+1 <= len Rev f by A1,A3,XREAL_1:8;
A6: GoB Rev f = GoB f by Lm1;
  now
    let i1,j1,i2,j2 be Element of NAT such that
A7: [i1,j1] in Indices GoB f and
A8: [i2,j2] in Indices GoB f and
A9: f/.i = (GoB f)*(i1,j1) and
A10: f/.(i+1) = (GoB f)*(i2,j2);
    1 <= i+1 by NAT_1:11;
    then
A11: i+1 in dom f by A4,FINSEQ_3:27;
    i+1+j = len f + 1 by A3;
    then
A12: (Rev f)/.j = (GoB f)*(i2,j2) by A10,A11,FINSEQ_5:69;
    i <= len f by A4,NAT_1:13;
    then
A13: i in dom f by A1,FINSEQ_3:27;
    j+1+i = len f + 1 by A3;
    then
A14: (Rev f)/.(j+1) = (GoB f)*(i1,j1) by A9,A13,FINSEQ_5:69;
    1 <= i1 by A7,MATRIX_1:39;
    then
A15: i1-'1+1 = i1 by XREAL_1:237;
    1 <= j1 by A7,MATRIX_1:39;
    then
A16: j1-'1+1 = j1 by XREAL_1:237;
    reconsider i1'=i1, i2'=i2, j1'=j1, j2'=j2 as Element of REAL;
    f is_sequence_on GoB f by GOBOARD5:def 5;
    then abs(i1-i2)+abs(j1-j2) = 1 by A7,A8,A9,A10,A11,A13,GOBOARD1:def 11;
    then
A17: abs(i1'-i2')=1 & j1=j2 or abs(j1'-j2')=1 & i1=i2 by GOBOARD1:2;
    per cases by A17,GOBOARD1:1;
    case i1 = i2 & j1+1 = j2;
      hence right_cell(Rev f,j) = cell(GoB f,i1-'1,j1)
      by A2,A5,A6,A7,A8,A12,A14,A15,GOBOARD5:31;
    end;
    case i1+1 = i2 & j1 = j2;
      hence right_cell(Rev f,j) = cell(GoB f,i1,j1)
      by A2,A5,A6,A7,A8,A12,A14,A16,GOBOARD5:30;
    end;
    case i1 = i2+1 & j1 = j2;
      hence right_cell(Rev f,j) = cell(GoB f,i2,j2-'1)
      by A2,A5,A6,A7,A8,A12,A14,A16,GOBOARD5:29;
    end;
    case i1 = i2 & j1 = j2+1;
      hence right_cell(Rev f,j) = cell(GoB f,i1,j2)
      by A2,A5,A6,A7,A8,A12,A14,A15,GOBOARD5:28;
    end;
  end;
  hence left_cell(f,i) = right_cell(Rev f,j) by A1,A4,GOBOARD5:def 7;
end;

theorem Th13:
  i >= 1 & j >= 1 & i+j = len f implies left_cell(Rev f,i) = right_cell(f,j)
proof
  len Rev f = len f & Rev Rev f = f by FINSEQ_5:def 3,FINSEQ_6:29;
  hence thesis by Th12;
end;

theorem Th14:
  1 <= k & k+1 <= len f implies ex i,j st i <= len GoB f & j <= width GoB f &
  cell(GoB f,i,j) = left_cell(f,k)
proof
  assume that
A1: 1 <= k and
A2: k+1 <= len f;
A3: f is_sequence_on GoB f by GOBOARD5:def 5;
  k <= len f by A2,NAT_1:13;
  then
A4: k in dom f by A1,FINSEQ_3:27;
  then consider i1,j1 being Element of NAT such that
A5: [i1,j1] in Indices GoB f and
A6: f/.k = (GoB f)*(i1,j1) by A3,GOBOARD1:def 11;
  1 <= k+1 by NAT_1:11;
  then
A7: k+1 in dom f by A2,FINSEQ_3:27;
  then consider i2,j2 being Element of NAT such that
A8: [i2,j2] in Indices GoB f and
A9: f/.(k+1) = (GoB f)*(i2,j2) by A3,GOBOARD1:def 11;
  1 <= i1 by A5,MATRIX_1:39;
  then
A10: i1-'1+1 = i1 by XREAL_1:237;
  1 <= j1 by A5,MATRIX_1:39;
  then
A11: j1-'1+1 = j1 by XREAL_1:237;
  reconsider i1'=i1, i2'=i2, j1'=j1, j2'=j2 as Element of REAL;
  abs(i1-i2)+abs(j1-j2) = 1 by A3,A4,A5,A6,A7,A8,A9,GOBOARD1:def 11;
  then
A12: abs(i1'-i2')=1 & j1=j2 or abs(j1'-j2')=1 & i1=i2 by GOBOARD1:2;
A13: i1 <= len GoB f by A5,MATRIX_1:39;
A14: j1 <= width GoB f by A5,MATRIX_1:39;
  per cases by A12,GOBOARD1:1;
  suppose
A15: i1 = i2 & j1+1 = j2;
    take i1-'1,j1;
    i1-'1 <= i1 by NAT_D:35;
    hence i1-'1 <= len GoB f by A13,XXREAL_0:2;
    thus j1 <= width GoB f by A5,MATRIX_1:39;
    thus left_cell(f,k) = cell(GoB f,i1-'1,j1)
    by A1,A2,A5,A6,A8,A9,A10,A15,GOBOARD5:28;
  end;
  suppose
A16: i1+1 = i2 & j1 = j2;
    take i1,j1;
    thus i1 <= len GoB f by A5,MATRIX_1:39;
    thus j1 <= width GoB f by A5,MATRIX_1:39;
    thus left_cell(f,k) = cell(GoB f,i1,j1)
    by A1,A2,A5,A6,A8,A9,A11,A16,GOBOARD5:29;
  end;
  suppose
A17: i1 = i2+1 & j1 = j2;
    take i2,j1-'1;
    thus i2 <= len GoB f by A8,MATRIX_1:39;
    j1-'1 <= j1 by NAT_D:35;
    hence j1-'1 <= width GoB f by A14,XXREAL_0:2;
    thus left_cell(f,k) = cell(GoB f,i2,j1-'1)
    by A1,A2,A5,A6,A8,A9,A11,A17,GOBOARD5:30;
  end;
  suppose
A18: i1 = i2 & j1 = j2+1;
    take i1,j2;
    thus i1 <= len GoB f by A5,MATRIX_1:39;
    thus j2 <= width GoB f by A8,MATRIX_1:39;
    thus left_cell(f,k) = cell(GoB f,i1,j2)
    by A1,A2,A5,A6,A8,A9,A10,A18,GOBOARD5:31;
  end;
end;

theorem Th15:
  j <= width G implies Int h_strip(G,j) is convex
proof
  assume
A1: j <= width G;
  per cases by A1,NAT_1:14,XXREAL_0:1;
  suppose j = 0;
    then Int h_strip(G,j) = { |[r,s]| : s < G*(1,1)`2 } by GOBOARD6:18;
    hence Int h_strip(G,j) is convex by JORDAN1:22;
  end;
  suppose j = width G;
    then Int h_strip(G,j) = { |[r,s]| : G*(1,width G)`2 < s } by GOBOARD6:19;
    hence Int h_strip(G,j) is convex by JORDAN1:20;
  end;
  suppose 1 <= j & j < width G;
    then
A2: Int h_strip(G,j) = { |[r,s]| : G*(1,j)`2 < s & s < G*(1,j+1)`2 }
    by GOBOARD6:20;
A3: { |[r,s]| : G*(1,j)`2 < s} c= the carrier of TOP-REAL 2
    proof
      let x be set;
      assume x in { |[r,s]| : G*(1,j)`2 < s};
      then ex r,s st x = |[r,s]| & G*(1,j)`2 < s;
      hence thesis;
    end;
    { |[r,s]| : s < G*(1,j+1)`2 } c= the carrier of TOP-REAL 2
    proof
      let x be set;
      assume x in { |[r,s]| : s < G*(1,j+1)`2 };
      then ex r,s st x = |[r,s]| & s < G*(1,j+1)`2;
      hence thesis;
    end;
    then reconsider P = { |[r,s]| : G*(1,j)`2 < s},
    Q = { |[r,s]| : s < G*(1,j+1)`2 } as Subset of TOP-REAL 2 by A3;
A4: Int h_strip(G,j) = P /\ Q
    proof
      hereby
        let x be set;
        assume x in Int h_strip(G,j);
        then ex r,s st x = |[r,s]| & G*(1,j)`2 < s & s < G*(1,j+1)`2 by A2;
        then x in P & x in Q;
        hence x in P /\ Q by XBOOLE_0:def 4;
      end;
      let x be set;
      assume
A5:   x in P /\ Q;
      then x in P by XBOOLE_0:def 4;
      then consider r1,s1 such that
A6:   x = |[r1,s1]| and
A7:   G*(1,j)`2 < s1;
      x in Q by A5,XBOOLE_0:def 4;
      then consider r2,s2 such that
A8:   x = |[r2,s2]| and
A9:   s2 < G*(1,j+1)`2;
      r1 = r2 & s1 = s2 by A6,A8,SPPOL_2:1;
      hence x in Int h_strip(G,j) by A2,A6,A7,A9;
    end;
A10: P is convex by JORDAN1:20;
    Q is convex by JORDAN1:22;
    hence Int h_strip(G,j) is convex by A4,A10,Th9;
  end;
end;

theorem Th16:
  i <= len G implies Int v_strip(G,i) is convex
proof
  assume
A1: i <= len G;
  per cases by A1,NAT_1:14,XXREAL_0:1;
  suppose i = 0;
    then Int v_strip(G,i) = { |[r,s]| : r < G*(1,1)`1 } by GOBOARD6:15;
    hence Int v_strip(G,i) is convex by JORDAN1:18;
  end;
  suppose i = len G;
    then Int v_strip(G,i) = { |[r,s]| : G*(len G,1)`1 < r } by GOBOARD6:16;
    hence Int v_strip(G,i) is convex by JORDAN1:16;
  end;
  suppose 1 <= i & i < len G;
    then
A2: Int v_strip(G,i) = { |[r,s]| : G*(i,1)`1 < r & r < G*(i+1,1)`1 }
    by GOBOARD6:17;
A3: { |[r,s]| : G*(i,1)`1 < r} c= the carrier of TOP-REAL 2
    proof
      let x be set;
      assume x in { |[r,s]| : G*(i,1)`1 < r};
      then ex r,s st x = |[r,s]| & G*(i,1)`1 < r;
      hence thesis;
    end;
    { |[r,s]| : r < G*(i+1,1)`1 } c= the carrier of TOP-REAL 2
    proof
      let x be set;
      assume x in { |[r,s]| : r < G*(i+1,1)`1 };
      then ex r,s st x = |[r,s]| & r < G*(i+1,1)`1;
      hence thesis;
    end;
    then reconsider P = { |[r,s]| : G*(i,1)`1 < r},
    Q = { |[r,s]| : r < G*(i+1,1)`1 } as Subset of TOP-REAL 2 by A3;
A4: Int v_strip(G,i) = P /\ Q
    proof
      hereby
        let x be set;
        assume x in Int v_strip(G,i);
        then ex r,s st x = |[r,s]| & G*(i,1)`1 < r & r < G*(i+1,1)`1 by A2;
        then x in P & x in Q;
        hence x in P /\ Q by XBOOLE_0:def 4;
      end;
      let x be set;
      assume
A5:   x in P /\ Q;
      then x in P by XBOOLE_0:def 4;
      then consider r1,s1 such that
A6:   x = |[r1,s1]| and
A7:   G*(i,1)`1 < r1;
      x in Q by A5,XBOOLE_0:def 4;
      then consider r2,s2 such that
A8:   x = |[r2,s2]| and
A9:   r2 < G*(i+1,1)`1;
      r1 = r2 & s1 = s2 by A6,A8,SPPOL_2:1;
      hence x in Int v_strip(G,i) by A2,A6,A7,A9;
    end;
A10: P is convex by JORDAN1:16;
    Q is convex by JORDAN1:18;
    hence Int v_strip(G,i) is convex by A4,A10,Th9;
  end;
end;

theorem Th17:
  i <= len G & j <= width G implies Int cell(G,i,j) <> {}
proof
  assume
A1: i <= len G & j <= width G;
  then
A2: j = width G or j < width G by XXREAL_0:1;
A3: i = len G or i < len G by A1,XXREAL_0:1;
  consider p being Point of TOP-REAL 2;
  per cases by A2,A3,NAT_1:13,14;
  suppose 1 <= i & i+1 <= len G & 1 <= j & j+1 <= width G;
    then LSeg(1/2*(G*(i,j)+G*
    (i+1,j+1)),p) meets Int cell(G,i,j) by GOBOARD6:85;
    hence Int cell(G,i,j) <> {} by XBOOLE_1:65;
  end;
  suppose 1 <= i & i+1 <= len G & j = width G;
    then LSeg(p,1/2*(G*(i,j)+G*(i+1,j))+|[0,1]|) meets Int cell(G,i,j)
    by GOBOARD6:86;
    hence Int cell(G,i,j) <> {} by XBOOLE_1:65;
  end;
  suppose 1 <= i & i+1 <= len G & j = 0;
    then LSeg(1/2*(G*(i,j+1)+G*(i+1,j+1))- |[0,1]|,p) meets Int cell(G,i,j)
    by GOBOARD6:87;
    hence Int cell(G,i,j) <> {} by XBOOLE_1:65;
  end;
  suppose 1 <= j & j+1 <= width G & i = 0;
    then LSeg(1/2*(G*(i+1,j)+G*(i+1,j+1))- |[1,0]|,p) meets Int cell(G,i,j)
    by GOBOARD6:88;
    hence Int cell(G,i,j) <> {} by XBOOLE_1:65;
  end;
  suppose 1 <= j & j+1 <= width G & i = len G;
    then LSeg(p,1/2*(G*(i,j)+G*(i,j+1))+|[1,0]|) meets Int cell(G,i,j)
    by GOBOARD6:89;
    hence Int cell(G,i,j) <> {} by XBOOLE_1:65;
  end;
  suppose i = 0 & j = 0;
    then LSeg(p,G*(i+1,j+1)- |[1,1]|) meets Int cell(G,i,j) by GOBOARD6:90;
    hence Int cell(G,i,j) <> {} by XBOOLE_1:65;
  end;
  suppose i = len G & j = width G;
    then LSeg(p,G*(i,j)+|[1,1]|) meets Int cell(G,i,j) by GOBOARD6:91;
    hence Int cell(G,i,j) <> {} by XBOOLE_1:65;
  end;
  suppose i = 0 & j = width G;
    then LSeg(p,G*(i+1,j)+|[-1,1]|) meets Int cell(G,i,j) by GOBOARD6:92;
    hence Int cell(G,i,j) <> {} by XBOOLE_1:65;
  end;
  suppose i = len G & j = 0;
    then LSeg(p,G*(i,j+1)+|[1,-1]|) meets Int cell(G,i,j) by GOBOARD6:93;
    hence Int cell(G,i,j) <> {} by XBOOLE_1:65;
  end;
end;

theorem Th18:
  1 <= k & k+1 <= len f implies Int left_cell(f,k) <> {}
proof
  assume 1 <= k & k+1 <= len f;
  then ex i,j st i <= len GoB f & j <=
  width GoB f & cell(GoB f,i,j) = left_cell(f,k) by Th14;
  hence thesis by Th17;
end;

theorem Th19:
  1 <= k & k+1 <= len f implies Int right_cell(f,k) <> {}
proof
  assume that
A1: 1 <= k and
A2: k+1 <= len f;
A3: len f = len Rev f by FINSEQ_5:def 3;
  k <= len f by A2,NAT_1:13;
  then
A4: len f-'k+k = len f by XREAL_1:237;
  then
A5: len f-'k+1 <= len f by A1,XREAL_1:8;
A6: len f-'k >= 1 by A2,A4,XREAL_1:8;
  then right_cell(f,k) = left_cell(Rev f,len f-'k) by A1,A4,Th13;
  hence Int right_cell(f,k) <> {} by A3,A5,A6,Th18;
end;

theorem Th20:
  i <= len G & j <= width G implies Int cell(G,i,j) is convex
proof
  assume
A1: i <= len G & j <= width G;
  set P = Int cell(G,i,j);
A2: P = Int v_strip(G,i) /\ Int h_strip(G,j) by TOPS_1:46;
  Int v_strip(G,i) is convex & Int h_strip(G,j) is convex by A1,Th15,Th16;
  hence P is convex by A2,Th9;
end;

theorem Th21:
  i <= len G & j <= width G implies Int cell(G,i,j) is connected
  by Th20,JORDAN1:9;

theorem Th22:
  1 <= k & k+1 <= len f implies Int left_cell(f,k) is connected
proof
  assume 1 <= k & k+1 <= len f;
  then ex i,j st i <= len GoB f & j <=
  width GoB f & cell(GoB f,i,j) = left_cell(f,k) by Th14;
  hence thesis by Th21;
end;

theorem Th23:
  1 <= k & k+1 <= len f implies Int right_cell(f,k) is connected
proof
  assume that
A1: 1 <= k and
A2: k+1 <= len f;
A3: len f = len Rev f by FINSEQ_5:def 3;
  k <= len f by A2,NAT_1:13;
  then
A4: len f-'k+k = len f by XREAL_1:237;
  then
A5: len f-'k+1 <= len f by A1,XREAL_1:8;
A6: len f-'k >= 1 by A2,A4,XREAL_1:8;
  then right_cell(f,k) = left_cell(Rev f,len f-'k) by A1,A4,Th13;
  hence Int right_cell(f,k) is connected by A3,A5,A6,Th22;
end;

definition
  let f;
A1: 1+1 < len f by GOBOARD7:36,XXREAL_0:2;
  then
A2: Int left_cell(f,1) <> {} by Th18;
A3: Int right_cell(f,1) <> {} by A1,Th19;
  func LeftComp f -> Subset of TOP-REAL 2 means
  :Def1:
  it is_a_component_of (L~f)` & Int left_cell(f,1) c= it;
  existence
  proof
A4: Int left_cell(f,1) is connected by A1,Th22;
    Int left_cell(f,1) misses L~f by A1,GOBOARD8:37;
    then
A5: Int left_cell(f,1) c= (L~f)` by SUBSET_1:43;
    then (L~f)` <> {} by A1,Th18,XBOOLE_1:3;
    hence thesis by A2,A4,A5,Th5;
  end;
  uniqueness by A2,Th3,XBOOLE_1:67;
  func RightComp f -> Subset of TOP-REAL 2 means
  :Def2:
  it is_a_component_of (L~f)` & Int right_cell(f,1) c= it;
  existence
  proof
A6: Int right_cell(f,1) is connected by A1,Th23;
    Int right_cell(f,1) misses L~f by A1,GOBOARD8:38;
    then
A7: Int right_cell(f,1) c= (L~f)` by SUBSET_1:43;
    then (L~f)` <> {} by A1,Th19,XBOOLE_1:3;
    hence thesis by A3,A6,A7,Th5;
  end;
  uniqueness by A3,Th3,XBOOLE_1:67;
end;

theorem Th24:
  for k st 1 <= k & k+1 <= len f holds Int left_cell(f,k) c= LeftComp f
proof
  defpred P[Element of NAT] means
  1 <= $1 & $1+1 <= len f implies Int left_cell(f,$1) c= LeftComp f;
A1: P[0];
A2: now
    let k such that
A3: P[k];
    thus P[k+1]
    proof
      assume that
A4:   1 <= k+1 and
A5:   k+1+1 <= len f;
      per cases by NAT_1:14;
      suppose k = 0;
        hence Int left_cell(f,k+1) c= LeftComp f by Def1;
      end;
      suppose
A6:     k >= 1;
A7:     k+1 <= len f by A5,NAT_1:13;
A8:     k <= k+1 by NAT_1:11;
        then k <= len f by A7,XXREAL_0:2;
        then
A9:     k in dom f by A6,FINSEQ_3:27;
        then consider i0,j0 being Element of NAT such that
A10:    [i0,j0] in Indices GoB f and
A11:    f/.k = (GoB f)*(i0,j0) by GOBOARD2:25;
A12:    k+1 in dom f by A4,A7,FINSEQ_3:27;
        then consider i1,j1 being Element of NAT such that
A13:    [i1,j1] in Indices GoB f and
A14:    f/.(k+1) = (GoB f)*(i1,j1) by GOBOARD2:25;
        k+1+1 >= 1 by NAT_1:11;
        then
A15:    k+1+1 in dom f by A5,FINSEQ_3:27;
        then consider i2,j2 being Element of NAT such that
A16:    [i2,j2] in Indices GoB f and
A17:    f/.(k+1+1) = (GoB f)*(i2,j2) by GOBOARD2:25;
A18:    1 <= i0 & i0 <= len GoB f by A10,MATRIX_1:39;
A19:    1 <= i1 & i1 <= len GoB f by A13,MATRIX_1:39;
A20:    1 <= i2 & i2 <= len GoB f by A16,MATRIX_1:39;
A21:    1 <= j0 & j0 <= width GoB f by A10,MATRIX_1:39;
A22:    1 <= j1 & j1 <= width GoB f by A13,MATRIX_1:39;
A23:    1 <= j2 & j2 <= width GoB f by A16,MATRIX_1:39;
A24:    i0 = i1 or j0 = j1 by A9,A10,A11,A12,A13,A14,GOBOARD2:16;
A25:    i1 = i2 or j1 = j2 by A12,A13,A14,A15,A16,A17,GOBOARD2:16;
        reconsider i1'=i1, i0'=i0, i2'=i2, j1'=j1, j0'=j0, j2'=j2
        as Element of REAL;
A26:    f is_sequence_on GoB f by GOBOARD5:def 5;

then abs(i0-i1)+abs(j0-j1) = 1 by A9,A10,A11,A12,A13,A14,GOBOARD1:def 11;
        then
A27:    abs(i0'-i1')=1 & j0 = j1 or abs(j0'-j1')=1 & i0=i1 by GOBOARD1:2;
        then
A28:    i0 = i1 or i0 = i1+1 or i0+1 = i1 by GOBOARD1:1;

abs(i1-i2)+abs(j1-j2) = 1 by A12,A13,A14,A15,A16,A17,A26,GOBOARD1:def 11;
        then
A29:    abs(i1'-i2')=1 & j1 = j2 or abs(j1'-j2')=1 & i1=i2 by GOBOARD1:2;
        then
A30:    i1 = i2 or i1 = i2+1 or i1+1 = i2 by GOBOARD1:1;
A31:    j0 = j1 or j0 = j1+1 or j0+1 = j1 by A27,GOBOARD1:1;
A32:    j1 = j2 or j1 = j2+1 or j1+1 = j2 by A29,GOBOARD1:1;
A33:    now
          assume
A34:      i0 = i2 & j0 = j2;
A35:      now
            assume k <= 1;
            then
A36:        k = 1 by A6,XXREAL_0:1;
            assume k+1+1 >= len f;
            then k+1+1 = len f by A5,XXREAL_0:1;
            hence contradiction by A36,GOBOARD7:36;
          end;
A37:      k < k+1+1 by A8,NAT_1:13;
          per cases by A35;
          suppose k > 1;
            hence contradiction by A5,A11,A17,A34,A37,GOBOARD7:39;
          end;
          suppose k+1+1 < len f;
            hence contradiction by A6,A11,A17,A34,A37,GOBOARD7:38;
          end;
        end;
        i1 >= 1 by A13,MATRIX_1:39;
        then
A38:    i1 = i1 -' 1 + 1 by XREAL_1:237;
        j1 >= 1 by A13,MATRIX_1:39;
        then
A39:    j1 = j1 -' 1 + 1 by XREAL_1:237;
        then j1 -' 1 <= j1 by NAT_1:11;
        then
A40:    j1 -' 1 <= width GoB f by A22,XXREAL_0:2;
        len GoB f > 1 by GOBOARD7:34;
        then
A41:    len GoB f -'1 + 1 = len GoB f by XREAL_1:237;
        width GoB f >= 1 by GOBOARD7:35;
        then
A42:    width GoB f -'1 + 1 = width GoB f by XREAL_1:237;
A43:    k+1+1 = k+(1+1);
A44:    i0+1+1 = i0+(1+1);
A45:    i2+1+1 = i2+(1+1);
A46:    j0+1+1 = j0+(1+1);
A47:    j2+1+1 = j2+(1+1);
A48:    LeftComp f is_a_component_of (L~f)` by Def1;
A49:    0+1 = 1;

now per cases by A9,A11,A12,A14,A15,A17,A28,A30,A31,A32,A33,GOBOARD7:31;
          suppose that
A50:        i0 = i1+1 and
A51:        i1 = i2+1 and
A52:        j0 = 1;
A53:        left_cell(f,k) = cell(GoB f,i1,0)
            by A6,A7,A10,A11,A13,A14,A24,A49,A50,A52,GOBOARD5:30;
            0+1 = j2 by A24,A25,A50,A51,A52;
            then
A54:        left_cell(f,k+1) = cell(GoB f,i2,0)
            by A4,A5,A13,A14,A16,A17,A24,A50,A51,A52,GOBOARD5:30;
A55:        LSeg(1/2*((GoB f)*(i1,1)+(GoB f)*(i0,1))- |[0,1]|,
            1/2*((GoB f)*(i2,1)+(GoB f)*(i1,1))- |[0,1]|) is connected by Th7,
JORDAN1:9;
A56:        LSeg(1/2*((GoB f)*(i1,1)+(GoB f)*(i0,1))- |[0,1]|,
            1/2*((GoB f)*(i2,1)+(GoB f)*(i1,1))- |[0,1]|)
            meets Int cell(GoB f,i1,0) by A18,A19,A50,GOBOARD6:87;
            LSeg(1/2*((GoB f)*(i1,1)+(GoB f)*(i0,1))- |[0,1]|,
            1/2*((GoB f)*(i2,1)+(GoB f)*(i1,1))- |[0,1]|) misses L~f
            by A18,A20,A45,A50,A51,GOBOARD8:25;
            then LSeg(1/2*((GoB f)*(i1,1)+(GoB f)*(i0,1))- |[0,1]|,
            1/2*((GoB f)*(i2,1)+(GoB f)*(i1,1))- |[0,1]|) c= (L~f)`
            by SUBSET_1:43;
            then
A57:        LSeg(1/2*((GoB f)*(i1,1)+(GoB f)*(i0,1))- |[0,1]|,
