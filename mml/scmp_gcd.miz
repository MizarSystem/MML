Lm9: for s1,s2 being State of SCMPDS,a be Int_position,k be Element of NAT
st Initialize GCD-Algorithm c= s1 & Initialize GCD-Algorithm c= s2
& s1.a=s2.a & k <= 4 holds IC Comput(ProgramPart(s1),s1,k) = IC Comput(
ProgramPart(s2),s2,k) &
Comput(ProgramPart(s1),s1,k).a = Comput(ProgramPart(s2),s2,k).a
proof
  let s1,s2 be State of SCMPDS,a be Int_position,k be Element of NAT;
  set GA=GCD-Algorithm;
  assume that
A1: Initialize GA c= s1 and
A2: Initialize GA c= s2;
  assume that
A3: s1.a=s2.a and
A4: k <= 4;
A5: GA c= s1 by A1,COMPOS_1:132;
A6: GA c= s2 by A2,COMPOS_1:132;
A7: IC s1= 0 by A1,SCMPDS_5:18;
Y:  (ProgramPart s1)/.IC s1
 = s1.IC s1 by COMPOS_1:38;
A8: Comput(ProgramPart(s1),s1,0+1) =
Following(ProgramPart s1,Comput(ProgramPart(s1),s1,0)
)
 by EXTPRO_1:4
    .= Following(ProgramPart s1,s1) by EXTPRO_1:3
    .= Exec(i00,s1) by A5,A7,Lm1,Y;
A9: IC s2= 0 by A2,SCMPDS_5:18;
Y:  (ProgramPart s2)/.IC s2
 = s2.IC s2 by COMPOS_1:38;
A10: Comput(ProgramPart(s2),s2,0+1) =
Following(ProgramPart s2,Comput(ProgramPart(s2),s2,0)
)
 by EXTPRO_1:4
    .= Following(ProgramPart s2,s2) by EXTPRO_1:3
    .= Exec(i00,s2) by A6,A9,Lm1,Y;
Y:  (ProgramPart Comput(ProgramPart(s1),s1,1))/.IC Comput(ProgramPart(s1),s1,1)
 = Comput(ProgramPart(s1),s1,1).IC Comput(ProgramPart(s1),s1,1) by COMPOS_1:38;
A11: IC Comput(ProgramPart(s1),s1,1)= succ IC s1 by A8,SCMPDS_2:57
    .=  (0+1) by A7;
  then
A12: CurInstr(ProgramPart Comput(ProgramPart(s1),s1,1),Comput(ProgramPart(s1),
s1,1))
=s1. 1 by Y,AMI_1:54
    .=i01 by A5,Lm1;
T: ProgramPart s1 = ProgramPart Comput(ProgramPart(s1),s1,1)
by AMI_1:123;
A13: Comput(ProgramPart(s1),s1,1+1) =
Following(ProgramPart s1,Comput(ProgramPart(s1),s1,1)
)
 by EXTPRO_1:4
    .= Exec(i01,Comput(ProgramPart(s1),s1,1)) by A12,T;
Y:  (ProgramPart Comput(ProgramPart(s2),s2,1))/.IC Comput(ProgramPart(s2),s2,1)
 = Comput(ProgramPart(s2),s2,1).IC Comput(ProgramPart(s2),s2,1) by COMPOS_1:38;
A14: IC Comput(ProgramPart(s2),s2,1)= succ IC s2 by A10,SCMPDS_2:57
    .=  (0+1) by A9;
  then
A15: CurInstr(ProgramPart Comput(ProgramPart(s2),s2,1),Comput(ProgramPart(s2),
s2,1))
=s2. 1 by Y,AMI_1:54
    .=i01 by A6,Lm1;
T: ProgramPart s2 = ProgramPart Comput(ProgramPart(s2),s2,1)
by AMI_1:123;
A16: Comput(ProgramPart(s2),s2,1+1) =
Following(ProgramPart s2,Comput(ProgramPart(s2),s2,1)
)
 by EXTPRO_1:4
    .= Exec(i01,Comput(ProgramPart(s2),s2,1)) by A15,T;
Y:  (ProgramPart Comput(ProgramPart(s1),s1,2))/.IC Comput(ProgramPart(s1),s1,2)
 = Comput(ProgramPart(s1),s1,2).IC Comput(ProgramPart(s1),s1,2) by COMPOS_1:38;
A17: IC Comput(ProgramPart(s1),s1,2)= succ IC Comput(ProgramPart(s1),s1,1) by
A13,SCMPDS_2:57
    .=  (1+1) by A11;
  then
A18: CurInstr(ProgramPart Comput(ProgramPart(s1),s1,2),Comput(ProgramPart(s1),
s1,2))
=s1. 2 by Y,AMI_1:54
    .=i02 by A5,Lm1;
T: ProgramPart s1 = ProgramPart Comput(ProgramPart(s1),s1,2)
by AMI_1:123;
A19: Comput(ProgramPart(s1),s1,2+1) =
Following(ProgramPart s1,Comput(ProgramPart(s1),s1,2)
)
 by EXTPRO_1:4
    .= Exec(i02,Comput(ProgramPart(s1),s1,2)) by A18,T;
A20: Comput(ProgramPart(s1),s1,2).SBP=7 by A13,SCMPDS_2:57;
Y:  (ProgramPart Comput(ProgramPart(s2),s2,2))/.IC Comput(ProgramPart(s2),s2,2)
 = Comput(ProgramPart(s2),s2,2).IC Comput(ProgramPart(s2),s2,2) by COMPOS_1:38;
A21: IC Comput(ProgramPart(s2),s2,2)= succ IC Comput(ProgramPart(s2),s2,1) by
A16,SCMPDS_2:57
    .=  (1+1) by A14;
  then
A22:CurInstr(ProgramPart Comput(ProgramPart(s2),s2,2),Comput(ProgramPart(s2),s2
,2))
=s2. 2 by Y,AMI_1:54
    .=i02 by A6,Lm1;
T: ProgramPart s2 = ProgramPart Comput(ProgramPart(s2),s2,2)
by AMI_1:123;
A23: Comput(ProgramPart(s2),s2,2+1) =
Following(ProgramPart s2,Comput(ProgramPart(s2),s2,2)
)
 by EXTPRO_1:4
    .= Exec(i02,Comput(ProgramPart(s2),s2,2)) by A22,T;
A24: Comput(ProgramPart(s2),s2,2).SBP=7 by A16,SCMPDS_2:57;
Y:  (ProgramPart Comput(ProgramPart(s1),s1,3))/.IC Comput(ProgramPart(s1),s1,3)
 = Comput(ProgramPart(s1),s1,3).IC Comput(ProgramPart(s1),s1,3) by COMPOS_1:38;
A25: IC Comput(ProgramPart(s1),s1,3)= succ IC Comput(ProgramPart(s1),s1,2) by
A19,SCMPDS_2:71
    .=  (2+1) by A17;
  then
A26: CurInstr(ProgramPart Comput(ProgramPart(s1),s1,3),Comput(ProgramPart(s1),
s1,3))
=s1. 3 by Y,AMI_1:54
    .=i03 by A5,Lm1;
T: ProgramPart s1 = ProgramPart Comput(ProgramPart(s1),s1,3)
by AMI_1:123;
A27: Comput(ProgramPart(s1),s1,3+1) =
Following(ProgramPart s1,Comput(ProgramPart(s1),s1,3)
)
 by EXTPRO_1:4
    .= Exec(i03,Comput(ProgramPart(s1),s1,3)) by A26,T;
Y:  (ProgramPart Comput(ProgramPart(s2),s2,3))/.IC Comput(ProgramPart(s2),s2,3)
 = Comput(ProgramPart(s2),s2,3).IC Comput(ProgramPart(s2),s2,3) by COMPOS_1:38;
A28: IC Comput(ProgramPart(s2),s2,3)= succ IC Comput(ProgramPart(s2),s2,2) by
A23,SCMPDS_2:71
    .=  (2+1) by A21;
  then
A29: CurInstr(ProgramPart Comput(ProgramPart(s2),s2,3),Comput(ProgramPart(s2),
s2,3))
=s2. 3 by Y,AMI_1:54
    .=i03 by A6,Lm1;
T: ProgramPart s2 = ProgramPart Comput(ProgramPart(s2),s2,3)
by AMI_1:123;
A30: Comput(ProgramPart(s2),s2,3+1) =
Following(ProgramPart s2,Comput(ProgramPart(s2),s2,3)
)
 by EXTPRO_1:4
    .= Exec(i03,Comput(ProgramPart(s2),s2,3)) by A29,T;
A31: now
    let b;
    assume
A32: s1.b=s2.b;
    per cases;
    suppose
A33:  b=GBP;
      hence Comput(ProgramPart(s1),s1,1).b=0 by A8,SCMPDS_2:57
        .=Comput(ProgramPart(s2),s2,1).b by A10,A33,SCMPDS_2:57;
    end;
    suppose
A34:  b<>GBP;
      hence Comput(ProgramPart(s1),s1,1).b=s1.b by A8,SCMPDS_2:57
        .=Comput(ProgramPart(s2),s2,1).b by A10,A32,A34,SCMPDS_2:57;
    end;
  end;
A35: now
    let b;
    assume
A36: s1.b=s2.b;
    per cases;
    suppose
A37:  b=SBP;
      hence Comput(ProgramPart(s1),s1,2).b=7 by A13,SCMPDS_2:57
        .=Comput(ProgramPart(s2),s2,2).b by A16,A37,SCMPDS_2:57;
    end;
    suppose
A38:  b<>SBP;
      hence Comput(ProgramPart(s1),s1,2).b=Comput(ProgramPart(s1),s1,1).b by
A13,SCMPDS_2:57
        .=Comput(ProgramPart(s2),s2,1).b by A31,A36
        .=Comput(ProgramPart(s2),s2,2).b by A16,A38,SCMPDS_2:57;
    end;
  end;
A39: now
    let b;
    assume
A40: s1.b=s2.b;
    per cases;
    suppose
A41:  b=DataLoc(Comput(ProgramPart(s1),s1,2).SBP,RetIC);
      hence Comput(ProgramPart(s1),s1,3).b=IC Comput(ProgramPart(s1),s1,2) by
A19,SCMPDS_2:71
        .=Comput(ProgramPart(s2),s2,3).b by A17,A20,A21,A23,A24,A41,SCMPDS_2:71
;
    end;
    suppose
A42:  b<>DataLoc(Comput(ProgramPart(s1),s1,2).SBP,RetIC);
      hence Comput(ProgramPart(s1),s1,3).b=Comput(ProgramPart(s1),s1,2).b by
A19,SCMPDS_2:71
        .=Comput(ProgramPart(s2),s2,2).b by A35,A40
        .=Comput(ProgramPart(s2),s2,3).b by A20,A23,A24,A42,SCMPDS_2:71;
    end;
  end;
  per cases by A4,NAT_1:29;
  suppose
A43: k=0;
    hence IC Comput(ProgramPart(s1),s1,k) = IC s1 by EXTPRO_1:3
      .=IC Comput(ProgramPart(s2),s2,k) by A7,A9,A43,EXTPRO_1:3;
    thus Comput(ProgramPart(s1),s1,k).a = s1.a by A43,EXTPRO_1:3
      .=Comput(ProgramPart(s2),s2,k).a by A3,A43,EXTPRO_1:3;
  end;
  suppose
A44: k=1;
    hence IC Comput(ProgramPart(s1),s1,k)=IC Comput(ProgramPart(s2),s2,k) by
A11,A14;
    thus thesis by A3,A31,A44;
  end;
  suppose
A45: k=2;
    hence IC Comput(ProgramPart(s1),s1,k)=IC Comput(ProgramPart(s2),s2,k) by
A17,A21;
    thus thesis by A3,A35,A45;
  end;
  suppose
A46: k=3;
    hence IC Comput(ProgramPart(s1),s1,k)=IC Comput(ProgramPart(s2),s2,k) by
A25,A28;
    thus thesis by A3,A39,A46;
  end;
  suppose
A47: k=4;
    hence IC Comput(ProgramPart(s1),s1,k)= ICplusConst(Comput(ProgramPart(s1),
s1,3),2)
    by A27,SCMPDS_2:66
      .=  (3+2) by A25,SCMPDS_6:23
      .= ICplusConst(Comput(ProgramPart(s2),s2,3),2) by A28,SCMPDS_6:23
      .= IC Comput(ProgramPart(s2),s2,k) by A30,A47,SCMPDS_2:66;
    thus Comput(ProgramPart(s1),s1,k).a = Comput(ProgramPart(s1),s1,3).a by A27
,A47,SCMPDS_2:66
      .=Comput(ProgramPart(s2),s2,3).a by A3,A39
      .=Comput(ProgramPart(s2),s2,k).a by A30,A47,SCMPDS_2:66;
  end;
end;

begin :: The Autonomy of Recursive Euclide Algorithm

theorem
  for p being FinPartState of SCMPDS,x,y being Integer st y >= 0 & x >= y
  & p=(intpos 9,intpos 10) --> (x,y)
  holds Initialize GCD-Algorithm +* p is autonomic
proof
  let p be FinPartState of SCMPDS,x,y be Integer;
  set GA=GCD-Algorithm, IA=Initialize GA, a=intpos 9, b=intpos 10;
  assume that
A1: y >= 0 and
A2: x >= y and
A3: p=(a,b) --> (x,y);
A4: dom p = { a,b } by A3,FUNCT_4:65;
A5: dom IA= {IC SCMPDS} \/ dom GA by COMPOS_1:76;
  now
    assume dom IA meets dom p;
    then consider z being set such that
A6: z in dom IA and
A7: z in dom p by XBOOLE_0:3;
    z = a or z = b by A4,A7,TARSKI:def 2;
    hence contradiction by A6,SCMPDS_4:31;
  end;
  then
A8: IA c= IA +* p by FUNCT_4:33;
A9: a in dom p by A4,TARSKI:def 2;
A10: b in dom p by A4,TARSKI:def 2;
A11: for t being State of SCMPDS st IA +* p c= t holds t.a = x & t.b = y
  proof
    let t be State of SCMPDS such that
A12: IA +* p c= t;
    p c= IA +* p by FUNCT_4:26;
    then
A13: p c= t by A12,XBOOLE_1:1;
    hence t.a = p.a by A9,GRFUNC_1:8
      .= x by A3,AMI_3:52,FUNCT_4:66;
    thus t.b = p.b by A10,A13,GRFUNC_1:8
      .= y by A3,FUNCT_4:66;
  end;
  thus IA +* p is autonomic
  proof
    let s1,s2 be State of SCMPDS such that
A14: IA +* p c= s1 and
A15: IA +* p c= s2;
A16: IA c= s1 by A8,A14,XBOOLE_1:1;
    then
A17: GA c= s1 by COMPOS_1:132;
A18: IA c= s2 by A8,A15,XBOOLE_1:1;
    then
A19: GA c= s2 by COMPOS_1:132;
A20: s1.a=x by A11,A14;
A21: s1.b=y by A11,A14;
A22: s2.a=x by A11,A15;
A23: s2.b=y by A11,A15;
    set s4=Comput(ProgramPart(s1),s1,4), t4=Comput(ProgramPart(s2),s2,4);
A24: GA c= s4 by A16,AMI_1:81,COMPOS_1:132;
A25: IC s4 =  5 by A16,Th15;
A26: s4.GBP = 0 by A16,Th15;
A27: s4.SBP = 7 by A16,Th15;
A28: s4.intpos(7+RetIC) =  2 by A16,Th15;
A29: s4.intpos 9 = s1.intpos 9 by A16,Th15;
A30: s4.intpos 10 = s1.intpos 10 by A16,Th15;
A31: s4.DataLoc(s4.SBP,3)=s4.intpos (7+3) by A27,Th5
      .=y by A11,A14,A30;
A32: DataLoc(s4.SBP,2)=intpos(7+2) by A27,Th5;
A33: GA c= t4 by A18,AMI_1:81,COMPOS_1:132;
A34: IC t4 =  5 by A18,Th15;
A35: t4.GBP = 0 by A18,Th15;
A36: t4.SBP = 7 by A18,Th15;
A37: t4.intpos(7+RetIC) =  2 by A18,Th15;
A38: t4.intpos 9 = s2.intpos 9 by A18,Th15;
A39: t4.intpos 10 = s2.intpos 10 by A18,Th15;
    t4.DataLoc(t4.SBP,3)=t4.intpos (7+3) by A36,Th5
      .=s4.DataLoc(s4.SBP,3) by A11,A15,A31,A39;
    then consider n such that
A40: CurInstr(ProgramPart Comput(ProgramPart(s4),s4,n),Comput(ProgramPart(s4),
s4,n))
 = return SBP and
A41: s4.SBP=Comput(ProgramPart(s4),s4,n).SBP and
A42: CurInstr(ProgramPart Comput(ProgramPart(t4),t4,n),Comput(ProgramPart(t4),
t4,n))
 = return SBP and
A43: t4.SBP=Comput(ProgramPart(t4),t4,n).SBP and
A44: for j be Element of NAT st 1<j & j <= s4.SBP+1 holds s4.intpos j=
    Comput(ProgramPart(s4),s4,n).intpos j & t4.intpos j=Comput(ProgramPart(t4),
t4,n).intpos j and
A45: for k be Element of NAT,c be Int_position st k <= n & s4.c = t4.c
    holds IC Comput(ProgramPart(s4),s4,k) = IC Comput(ProgramPart(t4),t4,k) &
Comput(ProgramPart(s4),s4,k).c =
    Comput(ProgramPart(t4),t4,k).c
    by A1,A2,A11,A15,A20,A24,A25,A26,A27,A29,A31,A32,A33,A34,A35,A36,A38,Lm8;
A46: Comput(ProgramPart(s4),s4,n).DataLoc(Comput(ProgramPart(s4),s4,n).SBP,
RetIC)
    =Comput(ProgramPart(s4),s4,n).intpos(7+1) by A27,A41,Th5,SCMPDS_1:def 23
      .= 2 by A27,A28,A44,SCMPDS_1:def 23;
A47: Comput(ProgramPart(t4),t4,n).DataLoc(Comput(ProgramPart(t4),t4,n).SBP,
RetIC)
    =Comput(ProgramPart(t4),t4,n).intpos(7+1) by A36,A43,Th5,SCMPDS_1:def 23
      .= 2 by A27,A37,A44,SCMPDS_1:def 23;
Y:  (ProgramPart Comput(ProgramPart(s4),s4,n+1))/.IC Comput(ProgramPart(s4),s4,
n+1)
 = Comput(ProgramPart(s4),s4,n+1).IC Comput(ProgramPart(s4),s4,n+1) by
COMPOS_1:38;
T: ProgramPart s4 = ProgramPart Comput(ProgramPart(s4),s4,n)
by AMI_1:123;
A48: Comput(ProgramPart(s4),s4,n+1)=
Following(ProgramPart s4,Comput(ProgramPart(s4),s4,n)
)
 by EXTPRO_1:4
      .= Exec(i14,Comput(ProgramPart(s4),s4,n)) by A40,T;
    then
A49: IC Comput(ProgramPart(s4),s4,n+1)=
    (abs(Comput(ProgramPart(s4),s4,n).DataLoc(Comput(ProgramPart(s4),s4,n).SBP,
RetIC)))+2
    by SCMPDS_2:70
      .=  (2+2) by A46,ABSVALUE:46;
    then
A50: CurInstr(ProgramPart Comput(ProgramPart(s4),s4,n+1),Comput(ProgramPart(s4)
,s4,n+1))
 =s4. 4 by Y,AMI_1:54
      .=s1. 4 by AMI_1:54
      .=i04 by A17,Lm1;
TX1: ProgramPart s4 = ProgramPart Comput(ProgramPart(s4),s4,n+1)
by AMI_1:123;
Y:  (ProgramPart Comput(ProgramPart(t4),t4,n+1))/.IC Comput(ProgramPart(t4),t4,
n+1)
 = Comput(ProgramPart(t4),t4,n+1).IC Comput(ProgramPart(t4),t4,n+1) by
COMPOS_1:38;
T: ProgramPart t4 = ProgramPart Comput(ProgramPart(t4),t4,n)
by AMI_1:123;
A51: Comput(ProgramPart(t4),t4,n+1)=
Following(ProgramPart t4,Comput(ProgramPart(t4),t4,n)
)
 by EXTPRO_1:4
      .= Exec(i14,Comput(ProgramPart(t4),t4,n)) by A42,T;
    then
A52: IC Comput(ProgramPart(t4),t4,n+1)= (abs(Comput(ProgramPart(t4),t4,n).
    DataLoc(Comput(ProgramPart(t4),t4,n).SBP,RetIC)))+2 by SCMPDS_2:70
      .= 2+2 by A47,ABSVALUE:46;
    then
A53: CurInstr(ProgramPart Comput(ProgramPart(t4),t4,n+1),Comput(ProgramPart(t4)
,t4,n+1))
 =t4. 4 by Y,AMI_1:54
      .=s2. 4 by AMI_1:54
      .=i04 by A19,Lm1;
TX: ProgramPart t4 = ProgramPart Comput(ProgramPart(t4),t4,n+1)
by AMI_1:123;
Tx: ProgramPart t4 = ProgramPart Comput(ProgramPart(t4),t4,n+1)
by AMI_1:123;
A54: s4.a=t4.a by A16,A18,A20,A22,Lm9;
A55: s4.b=t4.b by A16,A18,A21,A23,Lm9;
A56: Comput(ProgramPart(s4),s4,n+1).a=Comput(ProgramPart(s4),s4,n).a by A48,
AMI_3:52,SCMPDS_2:70
      .=Comput(ProgramPart(t4),t4,n).a by A45,A54
      .=Comput(ProgramPart(t4),t4,n+1).a by A51,AMI_3:52,SCMPDS_2:70;
A57: Comput(ProgramPart(s4),s4,n+1).b=Comput(ProgramPart(s4),s4,n).b by A48,
AMI_3:52,SCMPDS_2:70
      .=Comput(ProgramPart(t4),t4,n).b by A45,A55
      .=Comput(ProgramPart(t4),t4,n+1).b by A51,AMI_3:52,SCMPDS_2:70;
A58: now
      let j be Element of NAT;
A59:  j<n+4+1 or j>=n+5;
A60:  now
        assume
A61:    j <= n+4;
A62:    j<3+1 or j>= 4;
        per cases by A62,NAT_1:13;
        case j<=3;
          hence j<=3;
        end;
        case j>=4;
          hence j>=4 & j <= n+4 by A61;
        end;
      end;
      per cases by A59,A60,NAT_1:13;
      suppose j<=3;
        then
A63:    j<=4 by XXREAL_0:2;
        hence IC Comput(ProgramPart(s1),s1,j)=IC Comput(ProgramPart(s2),s2,j)
by A16,A18,A20,A22,Lm9;
        thus Comput(ProgramPart(s1),s1,j).a=Comput(ProgramPart(s2),s2,j).a
        by A16,A18,A20,A22,A63,Lm9;
        thus Comput(ProgramPart(s1),s1,j).b=Comput(ProgramPart(s2),s2,j).b
        by A16,A18,A21,A23,A63,Lm9;
      end;
      suppose
A64:    j>=4 & j<=n+4;
        then consider j1 be Nat such that
A65:    j=4+j1 by NAT_1:10;
        reconsider j1 as Element of NAT by ORDINAL1:def 13;
T: ProgramPart s2 = ProgramPart t4
by AMI_1:123;
S: ProgramPart s1 = ProgramPart s4
by AMI_1:123;
A66:    j1 <= n by A64,A65,XREAL_1:8;
        thus IC Comput(ProgramPart(s1),s1,j) = IC Comput(ProgramPart(s1),s4,j1)
by A65,EXTPRO_1:5
          .=IC Comput(ProgramPart(s2),t4,j1) by A45,A54,A66,T,S
          .=IC Comput(ProgramPart(s2),s2,j) by A65,EXTPRO_1:5;
        thus Comput(ProgramPart(s1),s1,j).a = Comput(ProgramPart(s1),s4,j1).a
by A65,EXTPRO_1:5
          .=Comput(ProgramPart(s2),t4,j1).a by A45,A54,A66,T,S
          .=Comput(ProgramPart(s2),s2,j).a by A65,EXTPRO_1:5;
        thus Comput(ProgramPart(s1),s1,j).b = Comput(ProgramPart(s1),s4,j1).b
by A65,EXTPRO_1:5
          .=Comput(ProgramPart(s2),t4,j1).b by A45,A55,A66,T,S
          .=Comput(ProgramPart(s2),s2,j).b by A65,EXTPRO_1:5;
      end;
      suppose j>=n+5;
        then consider j1 be Nat such that
A67:    j=n+(1+4)+j1 by NAT_1:10;
        reconsider j1 as Element of NAT by ORDINAL1:def 13;
T: ProgramPart s2 = ProgramPart t4
by AMI_1:123;
S: ProgramPart s1 = ProgramPart s4
by AMI_1:123;
A68:    j=n+1+j1+4 by A67;
        hence IC Comput(ProgramPart(s1),s1,j)=IC Comput(ProgramPart(s1),s4,n+1+
j1) by EXTPRO_1:5
          .=IC Comput(ProgramPart(t4),t4,n+1)
          by A49,A50,A52,S,TX1,EXTPRO_1:6,NAT_1:11
          .=IC Comput(ProgramPart(s2),t4,n+1+j1) by A53,T,TX,EXTPRO_1:6
,NAT_1:11
          .=IC Comput(ProgramPart(s2),s2,j) by A68,EXTPRO_1:5;
        thus Comput(ProgramPart(s1),s1,j).a = Comput(ProgramPart(s1),s4,n+1+j1)
.a by A68,EXTPRO_1:5
          .=Comput(ProgramPart(t4),t4,n+1).a
          by A50,A56,S,TX1,EXTPRO_1:6,NAT_1:11
          .=Comput(ProgramPart(s2),t4,n+1+j1).a by A53,T,Tx,EXTPRO_1:6,NAT_1:11
          .=Comput(ProgramPart(s2),s2,j).a by A68,EXTPRO_1:5;
        thus Comput(ProgramPart(s1),s1,j).b = Comput(ProgramPart(s1),s4,n+1+j1)
.b by A68,EXTPRO_1:5
          .=Comput(ProgramPart(t4),t4,n+1).b
          by A50,A57,S,TX1,EXTPRO_1:6,NAT_1:11
          .=Comput(ProgramPart(s2),t4,n+1+j1).b by A53,T,TX,EXTPRO_1:6,NAT_1:11
          .=Comput(ProgramPart(s2),s2,j).b by A68,EXTPRO_1:5;
      end;
    end;
    set A = { IC SCMPDS, a,b };
A69: dom(IA +* p) = { IC SCMPDS } \/ dom GA \/ { a, b } by A4,A5,FUNCT_4:def 1
      .= { IC SCMPDS } \/ { a,b } \/ dom GA by XBOOLE_1:4
      .= A \/ dom GA by ENUMSET1:42;
    let k be Element of NAT;
A70: GA c= Comput(ProgramPart(s2),s2,k) by A19,AMI_1:86;
    GA c= Comput(ProgramPart(s1),s1,k) by A17,AMI_1:86;
    then
A71: Comput(ProgramPart(s1),s1,k) | dom GA = GA by GRFUNC_1:64
      .= (Comput(ProgramPart(s2),s2,k))| dom GA by A70,GRFUNC_1:64;
A72: (Comput(ProgramPart(s1),s1,k)).IC SCMPDS = IC Comput(ProgramPart(s1),s1,k)
      .=IC Comput(ProgramPart(s2),s2,k) by A58
      .=(Comput(ProgramPart(s2),s2,k)).IC SCMPDS;
A73: Comput(ProgramPart(s1),s1,k).a = Comput(ProgramPart(s2),s2,k).a by A58;
A74: Comput(ProgramPart(s1),s1,k).b = Comput(ProgramPart(s2),s2,k).b by A58;
    dom(Comput(ProgramPart(s1),s1,k)) = the carrier of SCMPDS by PARTFUN1:def 4
      .= dom(Comput(ProgramPart(s2),s2,k)) by PARTFUN1:def 4;
    then (Comput(ProgramPart(s1),s1,k))|A = (Comput(ProgramPart(s2),s2,k))|A
by A72,A73,A74,GRFUNC_1:92;
    hence thesis
    by A69,A71,RELAT_1:185;
  end;
end;

