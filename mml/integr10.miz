A4: dom Intf = [.a,b.[ and
A5: for x be Real st x in dom Intf holds Intf.x =integral(f,a,x) and
A6: Intf is_left_convergent_in b and
A7: ext_right_integral(f,a,b) = lim_left(Intf,b) by A3,Def3;
