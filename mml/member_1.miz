theorem Th155:
  r--F c= r--G implies F c= G
proof
  assume
A1: r--F c= r--G;
  let i;
  assume i in F;
  then r-i in r--F by Th152;
  then ex w st r-i = r-w & w in G by A1,Th154;
  hence thesis by XXREAL_3:12;
end;

theorem
  r--F = r--G implies F = G
proof
  assume r--F = r--G;
  then F c= G & G c= F by Th155;
  hence thesis by XBOOLE_0:def 10;
end;

theorem Th157:
  r -- (F/\G) = (r--F) /\ (r--G)
proof
