      c1 = 1 by A2,TARSKI:def 1;
      hence a in A by A1,A3;
    end;
    assume
A4: a in A;
    a in COMPLEX by XCMPLX_0:def 2;
    then 1*a in {1*c: c in A} by A4;
    hence thesis by Th200;
  end;

theorem
  (a*b)**A = a**(b**A)
  proof
    thus (a*b)**A = {a}**{b}**A by Th104
    .= a**(b**A) by Th96;
  end;

theorem
  a**(A**B) = (a**A)**B by Th96;

theorem
  (a+b)**A c= a**A ++ b**A
  proof
    {a}++{b} = {a+b} by Th57;
    hence thesis by Th101;
  end;

theorem
  (a-b)**A c= a**A -- b**A
  proof
    {a}--{b} = {a-b} by Th81;
    hence thesis by Th102;
