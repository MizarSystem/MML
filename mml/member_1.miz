theorem
  F c= G & H c= I implies F--H c= G--I
  proof
A1: F--H = {w1-w2: w1 in F & w2 in H} by Th60;
    assume
A2: F c= G & H c= I;
    let i;
    assume i in F--H;
    then ex w,w1 st i = w-w1 & w in F & w1 in H by A1;
    hence thesis by A2,Th61;
  end;

theorem
  F -- (G \/ H) = (F--G) \/ (F--H)
  proof
    thus F -- (G \/ H) = F++(--G\/--H) by Th11
    .= (F--G) \/ (F--H) by Th47;
end;

theorem
  F -- (G /\ H) c= (F--G) /\ (F--H)
  proof
    F -- (G /\ H) = F++((--G)/\--H) by Th12;
    hence thesis by Th48;
  end;

Lm5:
--(F++G) = (--F) ++ --G
proof
  let j;
  hereby
    assume j in --(F++G);
    then consider w such that
A1: j = -w and
A2: w in F++G;
    consider w1,w2 such that
A3: w = w1+w2 and
A4: w1 in F & w2 in G by A2;
A5: -(w1+w2) = -w1-w2 by XXREAL_3:9;
    -w1 in --F & -w2 in --G by A4;
    hence j in (--F) ++ --G by A1,A3,A5;
  end;
  assume j in (--F) ++ --G;
  then consider w1,w2 such that
A6: j = w1+w2 and
A7: w1 in --F & w2 in --G;
A8: -(w1+w2) = -w1-w2 by XXREAL_3:9;
  -w1 in F & -w2 in G by A7,Th8;
  then -w1+-w2 in F++G;
  hence thesis by A6,A8,Th8;
end;

theorem
  --(F++G) = (--F) -- G by Lm5;

theorem Th66:
  --(F--G) = (--F) ++ G
  proof
    thus --(F--G) = (--F)----G by Lm5
    .= (--F) ++ G;
