      then consider c1,c2 such that
A1:   e = c1*c2 and
A2:   c1 in {a} and
A3:   c2 in A"";
A4:   c2" in A by A3,Th35;
A5:   c1*c2 = c1/(c2");
      c1 = a by A2,TARSKI:def 1;
      hence thesis by A1,A4,A5;
    end;
    let e;
    assume e in {a/c: c in A};
    then ex c st e = a/c & c in A;
    hence thesis by Th219;
