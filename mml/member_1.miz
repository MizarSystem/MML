theorem Th222:
  a <> 0 & a///A c= a///B implies A c= B
  proof
    assume that
A1: a <> 0 and
A2: a///A c= a///B;
    let z;
    assume z in A;
    then a/z in a///A by Th219;
    then consider c such that
A3: a/z = a/c and
A4: c in B by A2,Th221;
    z" = c" by A1,A3,XCMPLX_1:5;
    hence thesis by A4,XCMPLX_1:202;
  end;

theorem
  a <> 0 & a///A = a///B implies A = B
  proof
    assume a <> 0 & a///A = a///B;
    then A c= B & B c= A by Th222;
