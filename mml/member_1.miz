    let e;
    assume e in a///A;
    then consider c1,c2 such that
A1: e = c1*c2 and
A2: c1 in {a} & c2 in A"";
A3: c1*c2 = c1/(c2");
    c2" in A & c1 = a by A2,Th29,TARSKI:def 1;
    hence thesis by A1,A3;
  end;
  let e;
  assume e in {a/c: c in A};
  then ex c st e = a/c & c in A;
