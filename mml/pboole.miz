definition
  let F,G be Function-yielding Function;
  func G**F -> Function-yielding Function means
  :Def24:
  dom it = (dom F) /\ (
  dom G) & for i be set st i in dom it holds it.i = (G.i)*(F.i);
  existence
  proof
    defpred P[set,set] means $2 = (G.$1)*(F.$1);
A1: for i be set st i in dom F /\ dom G ex u be set st P[i,u];
    consider H being Function such that
A2: dom H = dom F /\ dom G & for i be set st i in dom F /\ dom G holds
    P[i,H.i] from CLASSES1:sch 1(A1);
    for x be set st x in dom H holds H.x is Function
    proof
      let x be set;
      reconsider f = F.x as Function;
      reconsider g = G.x as Function;
      assume x in dom H;
      then H.x = g*f by A2;
      hence thesis;
    end;
    then reconsider H as Function-yielding Function by FUNCOP_1:def 6;
    take H;
    thus thesis by A2;
  end;
  uniqueness
  proof
    let H1,H2 be Function-yielding Function;
    assume that
A3: dom H1 = dom F /\ dom G and
A4: for i be set st i in dom H1 holds H1.i = (G.i)*(F.i) and
A5: dom H2 = dom F /\ dom G and
A6: for i be set st i in dom H2 holds H2.i = (G.i)*(F.i);
    now
      let i be set;
      reconsider f = F.i as Function;
      reconsider g = G.i as Function;
      assume
A7:   i in dom F /\ dom G;
      then H1.i = g*f by A3,A4;
      hence H1.i = H2.i by A5,A6,A7;
    end;
    hence thesis by A3,A5,FUNCT_1:9;
  end;
end;

definition
  let I be set, A be ManySortedSet of I, F be ManySortedFunction of I;
  func F.:.:A -> ManySortedSet of I means
  for i be set st i in I holds it.i =
  (F.i).:(A.i);
  existence
  proof
    defpred P[set,set] means $2 = (F.$1).:(A.$1);
A1: for i be set st i in I ex u be set st P[i,u];
    consider g being Function such that
A2: dom g = I & for i be set st i in I holds P[i,g.i] from CLASSES1:
    sch 1( A1);
    reconsider g as ManySortedSet of I by A2,PARTFUN1:def 4,RELAT_1:def 18;
    take g;
    thus thesis by A2;
  end;
  uniqueness
  proof
    let B,B1 be ManySortedSet of I;
    assume that
A3: for i be set st i in I holds B.i = (F.i).:(A.i) and
A4: for i be set st i in I holds B1.i = (F.i).:(A.i);
    now
      let i be set;
      reconsider f = F.i as Function;
      assume
A5:   i in I;
      then B.i = f.:(A.i) by A3;
      hence B.i = B1.i by A4,A5;
    end;
    hence thesis by Th3;
  end;
end;

