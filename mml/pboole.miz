:: from AUTALG_1, 2008.07.06, A.T

theorem
  for F, G, H be Function-yielding Function holds (H ** G) ** F = H ** (
  G ** F)
proof
  let F, G, H be Function-yielding Function;
  set f = (H ** G) ** F, g = H ** (G ** F);
  now
A1: dom f = (dom (H ** G)) /\ dom F by Def24
      .= (dom H) /\ (dom G) /\ (dom F) by Def24;
    then
A2: dom f = (dom H) /\ ((dom G) /\ dom F) by XBOOLE_1:16;
    hence
A3: dom f = (dom H) /\ (dom (G ** F)) by Def24
      .= dom g by Def24;
    let x be set;
    assume
A4: x in dom f;
    then x in (dom H) /\ (dom G) by A1,XBOOLE_0:def 4;
    then
A5: x in dom (H ** G) by Def24;
    x in (dom G) /\ (dom F) by A2,A4,XBOOLE_0:def 4;
    then
A6: x in dom (G ** F) by Def24;
    thus f.x = ((H**G).x) * (F.x) by A4,Def24
      .= (H.x)*(G.x)*(F.x) by A5,Def24
      .= (H.x)*((G.x)*(F.x)) by RELAT_1:55
      .= (H.x)*((G**F).x) by A6,Def24
      .= g.x by A3,A4,Def24;
  end;
  hence thesis by FUNCT_1:9;
end;

