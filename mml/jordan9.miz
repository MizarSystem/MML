A583:      i1 = i2 & j1 = j2+1 & i1' = i2' & j1' = j2'+1;
then A584:      i1 = i1' by A541,A546,A548,A550,A551,A552,A554,A556,A558,A559
,A560,GOBOARD7:21;
              j2 = j2' or j2 = j2'+1 or j2+1 = j2'
           by A541,A546,A548,A550,A551,A552,A554,A556,A558,A559,A560,A583,
GOBOARD7:24;
