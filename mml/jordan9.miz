  then A656: len rg -'1+1 = len rg by BINARITH:53;

    set p = rg/.1, q = rg/.2;
    set a = f/.(m-'1);
    set l = (m-'1)+(len g-'1);

  A657: p = f/.m by A421,A431,FINSEQ_5:68;
  A658: m-'1+1 = m by A424,BINARITH:53;
  then A659: 1 <= m-'1 by A589,NAT_1:13;

         1+1-'1 <= len g-'1 by A585,BINARITH:60;
  then A660: 1 <= len g-'1 by BINARITH:39;
        A661: 1+1 - 1 <= len g - 1 by A585,XREAL_1:11;
A662: 1 - 1 < m - 1 by A589,XREAL_1:11;
         l = m+(len g -'1)-'1 by A424,BINARITH:56
     .= (len g -'1)+m - 1 by A660,BINARITH:55
     .= (len g - 1)+m - 1 by A661,BINARITH:def 3
     .= ((k - (m - 1)) - 1)+m - 1 by A422,A429,A662,BINARITH:def 3
     .= k - 1;
  then A663: k = l+1;
  then A664: l < k by XREAL_1:31;
         len g-'1 <= l by NAT_1:11;
  then A665: 1 <= l by A660,XXREAL_0:2;
         len g-'1 <= len g by BINARITH:62;
  then A666: len g-'1 in dom g by A660,FINSEQ_3:27;
         1 <= len g by A586,SPPOL_1:5;
  then A667: len g-'1+2 = len g+1 by Lm1;
  then A668: q = g/.(len g-'1) by A666,FINSEQ_5:69
       .= f/.l by A666,FINSEQ_5:30;
  A669: right_cell(f,m-'1,G) meets C by A293,A424,A658,A659;
  A670: right_cell(f,l,G) meets C by A293,A422,A663,A665;

         not a in L~g by A606,A659;
  then A671: not a in L~rg by SPPOL_2:22;

  A672: m-'1+2 = m+1 by A424,Lm1;

  A673: m-'1 <= l by NAT_1:11;
    then m-'1 <= len(F.l) by A95;
  then A674: m-'1 in dom(F.l) by A659,FINSEQ_3:27;
