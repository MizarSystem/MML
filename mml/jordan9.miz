    then Int right_cell(g,1,G) c= RightComp g by A802,XBOOLE_1:1;
    then Int right_cell(f,m-'1+1,G) c= RightComp g by A423,A428,A586,GOBRD13:33
;
    then A803:  Int right_cell(f,m,G) c= RightComp g by A424,BINARITH:53;
