then A440: len f >= 1+1 by NAT_1:38;
  then reconsider f as non constant special unfolded non empty
    FinSequence of TOP-REAL 2 by A414,A437,JORDAN8:7,8;
  set g = f/^(m-'1);
A441: m+1 <= len f by A439,NAT_1:38;
