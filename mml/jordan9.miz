        then cell(G,1-'1,j2) meets C by A98,A101,A102,A104,A105,A124,A125,
GOBRD13:35;
        then cell(G,0,j2) meets C by BINARITH:51;
        hence contradiction by A2,A108,JORDAN8:21;
       end;
