A601: for j,k st 1 <= j & j <= k holds (F.k)/.j = (F.j)/.j
  proof
    let j,k;
    assume that
  A602: 1 <= j and
  A603: j <= k;
  A604: (F.k)|j = F.j by A351,A603;
           j <= len(F.k) by A95,A603;
  then len(F.k|j) = j by FINSEQ_1:80;
      then j in dom((F.k)|j) by A602,FINSEQ_3:27;
      hence thesis by A604,FINSEQ_4:85;
  end;

A605:  L~g c= L~f by JORDAN3:75;

A606: for n st 1 <= n & n <= m-'1 holds not f/.n in L~g
  proof
    let n such that
  A607: 1 <= n and
  A608: n <= m-'1;
  A609: n <= len f by A428,A608,XXREAL_0:2;

    set p = f/.n;
    assume p in L~g;
    then consider j such that
  A610: m-'1+1 <= j and
  A611: j+1 <= len f and
  A612: p in LSeg(f,j) by A428,Th9;

  A613: n < m-'1+1 by A608,NAT_1:13;
  then A614: n < j by A610,XXREAL_0:2;

  A615: m-'1+1=m by A424,BINARITH:53;
  then A616: 1 < j by A589,A610,XXREAL_0:2;
  A617: j < k by A422,A611,NAT_1:13;
 A618: 2 <= len G & 2 <= width G by A2,NAT_1:12;
A619: p in Values G by A423,A607,A609,Th8;
    per cases by A423,A611,A612,A616,A618,A619,Th25;
    suppose
    A620: p = f/.j;
           n <= len(F.j) by A95,A614;
    then A621: n in dom(F.j) by A607,FINSEQ_3:27;
    A622: n <> len(F.j) by A95,A610,A613;
           (F.j)/.n = (F.n)/.n by A601,A607,A614
              .= p by A422,A601,A607,A609
              .= (F.j)/.j by A601,A616,A617,A620
              .= (F.j)/.len(F.j) by A95;
      hence contradiction by A418,A616,A617,A621,A622;

    end;
    suppose
    A623: p = f/.(j+1);
           now
        per cases by A422,A611,REAL_1:def 5;
        suppose
        A624: j+1 = k;
               n <= len(F.m) by A95,A613,A615;
        then A625: n in dom(F.m) by A607,FINSEQ_3:27;
        A626: n <> len(F.m) by A95,A613,A615;
               (F.m)/.n = (F.n)/.n by A601,A607,A613,A615
                  .= (F.k)/.k by A422,A601,A607,A609,A623,A624
                  .= (F.m)/.m by A421,A422,A424,A601
                  .= (F.m)/.len(F.m) by A95;
          hence contradiction by A418,A422,A424,A425,A625,A626;
        end;
        suppose
        A627: j+1 < k;
          set l = j+1;
        A628: 1 <= l by NAT_1:11;
          A629: n < n+1 & n+1 < l by A614,XREAL_1:8,31;
        then A630: n < l by XXREAL_0:2;
          then n <= len(F.l) by A95;
        then A631: n in dom(F.l) by A607,FINSEQ_3:27;
        A632: n <> len(F.l) by A95,A629;
               (F.l)/.n = (F.n)/.n by A601,A607,A630
                  .= p by A422,A601,A607,A609
                  .= (F.l)/.l by A601,A623,A627,A628
                  .= (F.l)/.len(F.l) by A95;
          hence contradiction by A418,A627,A628,A631,A632;
      end;
      end;
      hence contradiction;
  end;
  end;

A633: for h being standard non constant special_circular_sequence st L~h c= L~f
    for Comp being Subset of TOP-REAL 2 st Comp is_a_component_of (L~h)`
    for n st 1 <= n & n+1 <= len f & f/.n in Comp & not f/.n in L~h
      holds
     C meets Comp
  proof
    let h be standard non constant special_circular_sequence such that
  A634: L~h c= L~f;
    let Comp be Subset of TOP-REAL 2 such that
  A635: Comp is_a_component_of (L~h)`;
    let n such that
  A636: 1 <= n & n+1 <= len f and
  A637: f/.n in Comp and
  A638: not f/.n in L~h;

    reconsider rc = right_cell(f,n,G)\L~h as Subset of TOP-REAL 2;
  A639: rc c= right_cell(f,n,G) by XBOOLE_1:36;
  A640: rc = right_cell(f,n,G) /\ (L~h)` by SUBSET_1:32;

  A641: rc meets C
  proof
         right_cell(f,n,G) meets C by A293,A636;
    then consider p being set such that
  A642: p in right_cell(f,n,G) & p in C by XBOOLE_0:3;
    reconsider p as Element of TOP-REAL 2 by A642;
         now take a = p;
         now
      assume p in L~h;
      then consider j such that
    A643: 1 <= j and
    A644: j+1 <= len f and
    A645: p in LSeg(f,j) by A634,SPPOL_2:13;
           p in
 left_cell(f,j,G) /\ right_cell(f,j,G) by A423,A643,A644,A645,GOBRD13:30;
    then A646: p in left_cell(f,j,G) by XBOOLE_0:def 3;
           left_cell(f,j,G) misses C by A293,A643,A644;
      hence contradiction by A642,A646,XBOOLE_0:3;
    end;
    hence a in rc by A642,XBOOLE_0:def 4;
    thus a in C by A642;
  end; hence thesis by XBOOLE_0:3; end;

  A647: Int right_cell(f,n,G) is connected by A423,A636,Th12;
         Int right_cell(f,n,G) misses L~f by A423,A636,Th17;
    then Int right_cell(f,n,G) misses L~h by A634,XBOOLE_1:63;
  then A648: Int right_cell(f,n,G) c= (L~h)` by SUBSET_1:43;
         Int right_cell(f,n,G) c= right_cell(f,n,G) by TOPS_1:44;
  then A649: Int right_cell(f,n,G) c= rc by A640,A648,XBOOLE_1:19;
         rc c= Cl Int right_cell(f,n,G) by A423,A636,A639,Th13;
  then A650: rc is connected by A647,A649,CONNSP_1:19;
         f/.n in right_cell(f,n,G) by A423,A636,Th10;
    then f/.n in rc by A638,XBOOLE_0:def 4;
  then A651: rc meets Comp by A637,XBOOLE_0:3;
         rc c= (L~h)` by A640,XBOOLE_1:17;
    then rc c= Comp by A635,A650,A651,GOBOARD9:6;
    hence C meets Comp by A641,XBOOLE_1:63;
  end;

A652:
 C meets RightComp Rev g
  proof
    set rg=Rev g;

  A653: rg is_sequence_on G by A432,Th7;

  A654: 1+1 <= len rg by A585,FINSEQ_5:def 3;
