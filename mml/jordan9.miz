  then A675: m-'1 <> len(F.l) by A95;

    consider p1,p2,q1,q2 being Element of NAT such that
  A676: [p1,p2] in Indices G and
  A677: p = G*(p1,p2) and
  A678: [q1,q2] in Indices G and
  A679: q = G*(q1,q2) and
  A680: p1 = q1 & p2+1 = q2 or p1+1 = q1 & p2 = q2 or
        p1 = q1+1 & p2 = q2 or p1 = q1 & p2 = q2+1 by A653,A654,JORDAN8:6;

  A681: 1 <= p1 & p1 <= len G & 1 <= p2 & p2 <= width G by A676,GOBOARD5:1;

    per cases by A680;
    suppose
    A682: p1 = q1 & p2+1 = q2;
      consider a1,a2,p'1,p'2 being Element of NAT such that
    A683: [a1,a2] in Indices G and
    A684: a = G*(a1,a2) and
    A685: [p'1,p'2] in Indices G and
    A686: p = G*(p'1,p'2) and
    A687: a1 = p'1 & a2+1 = p'2 or a1+1 = p'1 & a2 = p'2 or
          a1 = p'1+1 & a2 = p'2 or a1 = p'1 & a2 = p'2+1
            by A423,A424,A657,A658,A659,JORDAN8:6;
    A688: 1 <= a1 & a1 <= len G & 1 <= a2 & a2 <= width G by A683,GOBOARD5:1;

      thus thesis
      proof
        per cases by A687;
        suppose
               a1 = p'1 & a2+1 = p'2;
        then A689: a1 = p1 & a2+1 = p2 by A676,A677,A685,A686,GOBOARD1:21;
        A690: F.m is_sequence_on G by A293;
        A691: m-'1+1 <= len (F.m) by A95,A658;
        A692: m-'1 <= m by A658,NAT_1:11;
        A693: F.k|(m+1)=F.(m+1) by A351,A422,A427;
A694:      p2+1 > a2+1 & a2+1 > a2 by A689,NAT_1:13;
        A695: f/.(m-'1) = (F.(m-'1))/.(m-'1) by A422,A428,A601,A659
                     .= (F.m)/.(m-'1) by A601,A659,A692;
        A696: f/.(m-'1+1) = (F.m)/.m by A422,A424,A601,A658;
               right_cell(f,l,G) = cell(G,p1-'1,p2)
                 by A421,A422,A423,A657,A663,A665,A668,A676,A677,A678,A679,A682
,GOBRD13:29
            .= front_left_cell(F.m,m-'1,G) by A657,A658,A659,A676,A677,A683
,A684,A689,A690,A691,A695,A696,GOBRD13:35;
          then F.(m+1) turns_left m-'1,G by A352,A589,A670;
          then f turns_left m-'1,G by A427,A659,A672,A693,GOBRD13:45;
        then A697: f/.(m+1) = G*(p1-'1,p2) & [p1-'1,p2] in Indices G
            by A657,A658,A672,A676,A677,A683,A684,A694,GOBRD13:def 7;
        then A698: 1 <= p1-'1 & p1-'1 <= len G by GOBOARD5:1;

          set rc = right_cell(rg,len rg-'1,G)\L~rg;
        A699: rc c= RightComp rg by A653,A655,A656,Th29;
        A700: p2-'1 = a2 & p1-'1+1 = p1 by A681,A689,BINARITH:39,53;
        A701: 2 in dom g by A585,FINSEQ_3:27;
                len rg-'1+2 = len g +1 by A667,FINSEQ_5:def 3;
        then A702: rg/.(len rg-'1) = g/.2 by A701,FINSEQ_5:69
                           .= f/.(m+1) by A672,A701,FINSEQ_5:30;
             p = g/.1 by A421,A430,A431,FINSEQ_5:68
              .= rg/.len g by FINSEQ_5:68
              .= rg/.len rg by FINSEQ_5:def 3;
          then right_cell(rg,len rg-'1,G) = cell(G,p1-'1,a2)
            by A653,A655,A656,A676,A677,A697,A700,A702,GOBRD13:25;
          then a in right_cell(rg,len rg-'1,G)
            by A681,A684,A688,A689,A698,A700,Th22;
then A703:       a in rc by A671,XBOOLE_0:def 4;
        A704: L~rg c= L~f by A605,SPPOL_2:22;
               RightComp rg is_a_component_of (L~rg)` by GOBOARD9:def 2;
          hence C meets RightComp Rev g by A424,A633,A658,A659,A671,A699,A703
,A704;

        end;
        suppose
          A705: a1+1 = p'1 & a2 = p'2;
        then A706: a1+1 = p1 & a2 = p2 by A676,A677,A685,A686,GOBOARD1:21;
        then q1-'1 = a1 by A682,BINARITH:39;
          then right_cell(f,l,G) = cell(G,a1,a2)
                 by A421,A422,A423,A657,A663,A665,A668,A676,A677,A678,A679,A682
,A706,GOBRD13:29
            .= left_cell(f,m-'1,G)
                 by A423,A424,A657,A658,A659,A683,A684,A685,A686,A705,
GOBRD13:24;
          hence C meets RightComp Rev g by A293,A424,A658,A659,A670;

        end;
        suppose
               a1 = p'1+1 & a2 = p'2;
        then a1 = p1+1 & a2 = p2 by A676,A677,A685,A686,GOBOARD1:21;
          then right_cell(f,m-'1,G) = cell(G,p1,p2)
                 by A423,A424,A657,A658,A659,A676,A677,A683,A684,GOBRD13:27
            .= left_cell(f,l,G)
                 by A421,A422,A423,A657,A663,A665,A668,A676,A677,A678,A679,A682
,GOBRD13:28;
          hence C meets RightComp Rev g by A293,A422,A663,A665,A669;

        end;
        suppose
               a1 = p'1 & a2 = p'2+1;
        then A707: a1 = q1 & a2 = q2 by A676,A677,A682,A685,A686,GOBOARD1:21;
               (F.l)/.(m-'1) = (F.(m-'1))/.(m-'1) by A601,A659,A673
                     .= q by A422,A428,A601,A659,A679,A684,A707
                     .= (F.l)/.l by A601,A664,A665,A668
                     .= (F.l)/.len(F.l) by A95;
          hence C meets RightComp Rev g by A418,A664,A665,A674,A675;
      end;
      end;

    end;
    suppose
    A708: p1+1 = q1 & p2 = q2;
      consider a1,a2,p'1,p'2 being Element of NAT such that
    A709: [a1,a2] in Indices G and
    A710: a = G*(a1,a2) and
    A711: [p'1,p'2] in Indices G and
    A712: p = G*(p'1,p'2) and
    A713: a1 = p'1 & a2+1 = p'2 or a1+1 = p'1 & a2 = p'2 or
          a1 = p'1+1 & a2 = p'2 or a1 = p'1 & a2 = p'2+1
            by A423,A424,A657,A658,A659,JORDAN8:6;
    A714: 1 <= a1 & a1 <= len G & 1 <= a2 & a2 <= width G by A709,GOBOARD5:1;

      thus thesis
      proof
        per cases by A713;
        suppose
          A715: a1 = p'1 & a2+1 = p'2;
        then A716: a1 = p1 & a2+1 = p2 by A676,A677,A711,A712,GOBOARD1:21;
        then A717: q2-'1 = a2 by A708,BINARITH:39;
               right_cell(f,m-'1,G) = cell(G,a1,a2)
                 by A423,A424,A657,A658,A659,A709,A710,A711,A712,A715,
GOBRD13:23
            .= left_cell(f,l,G)
                by A421,A422,A423,A657,A663,A665,A668,A676,A677,A678,A679,A708
,A716,A717,GOBRD13:26;
          hence C meets RightComp Rev g by A293,A422,A663,A665,A669;

        end;
        suppose
               a1+1 = p'1 & a2 = p'2;
        then A718: a1+1 = p1 & a2 = p2 by A676,A677,A711,A712,GOBOARD1:21;
        A719: F.m is_sequence_on G by A293;
        A720: m-'1+1 <= len (F.m) by A95,A658;
        A721: m-'1 <= m by A658,NAT_1:11;
        A722: F.k|(m+1)=F.(m+1) by A351,A422,A427;
