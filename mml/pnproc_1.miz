 notations TARSKI, XBOOLE_0, DOMAIN_1, RELAT_1, FUNCT_1, SUBSET_1, FINSET_1,
      CARD_1, FINSEQ_1, FUNCT_2, FUNCOP_1, NUMBERS, ORDINAL1, ORDINAL2,
      XCMPLX_0, XREAL_0, NAT_1, BINARITH, MCART_1, FINSEQ_2, FINSEQ_4, LANG1,
      FUNCT_7, INT_1, GRAPH_2;
 constructors WELLORD2, DOMAIN_1, INT_1, FINSEQ_4, FUNCT_7, BINARITH, GRAPH_2,
      MEMBERED, PRALG_1, NAT_1, LANG1;
 registrations RELAT_1, RELSET_1, FUNCT_1, FUNCT_7, FUNCOP_1, SUBSET_1,
      FINSEQ_1, HEYTING2, TREES_2, INT_1, ORDINAL2, ARYTM_3;
 requirements BOOLE, SUBSET, REAL, NUMERALS, ARITHM;
 definitions TARSKI, XBOOLE_0, FUNCOP_1, RELAT_1;
