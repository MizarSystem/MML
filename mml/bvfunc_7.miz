A6: (b 'imp' a).z = 'not' (b).z 'or' (a).z by BVFUNC_1:def 11;
A7: (a).z=TRUE or (a).z=FALSE by XBOOLEAN:def 3;
  then
A8: 'not' (b).z=TRUE or (a).z=TRUE by A3,A6,BINARITH:7;
A9: ((a 'eqv' c) 'eqv' (b 'eqv' c)).z
  =((((a).z '&' ('not' c).z) 'or' ((c).z '&' 'not' (a).z)) 'or'
  (('not' (b).z 'or' (c).z) '&' (('not' c).z 'or' (b).z))) '&'
  ((((b).z '&' ('not' c).z) 'or' ((c).z '&' 'not' (b).z)) 'or'
  (('not' (a).z 'or' (c).z) '&' (('not' c).z 'or' (a).z)))
  proof
    ((a 'eqv' c) 'eqv' (b 'eqv' c)).z
    =(((a 'eqv' c) 'imp' (b 'eqv' c)) '&' ((b 'eqv' c) 'imp' (a 'eqv' c))).z
    by BVFUNC_4:7
      .=((((a 'imp' c) '&' (c 'imp' a)) 'imp' (b 'eqv' c)) '&'
    ((b 'eqv' c) 'imp' (a 'eqv' c))).z by BVFUNC_4:7
      .=((((a 'imp' c) '&' (c 'imp' a)) 'imp' (b 'imp' c) '&' (c 'imp' b)) '&'
    ((b 'eqv' c) 'imp' (a 'eqv' c))).z by BVFUNC_4:7
      .=((((a 'imp' c) '&' (c 'imp' a)) 'imp' (b 'imp' c) '&' (c 'imp' b)) '&'
    (((b 'imp' c) '&' (c 'imp' b)) 'imp' (a 'eqv' c))).z by BVFUNC_4:7

.=((((a 'imp' c) '&' (c 'imp' a)) 'imp' ((b 'imp' c) '&' (c 'imp' b))) '&'
    (((b 'imp' c) '&' (c 'imp' b)) 'imp' ((a 'imp' c) '&' (c 'imp' a)))).z
    by BVFUNC_4:7
      .=(((('not'
    a 'or' c) '&' (c 'imp' a)) 'imp' ((b 'imp' c) '&' (c 'imp' b))) '&'
    (((b 'imp' c) '&' (c 'imp' b)) 'imp' ((a 'imp' c) '&' (c 'imp' a)))).z
    by BVFUNC_4:8
      .=(((('not' a 'or' c) '&' ('not'
    c 'or' a)) 'imp' ((b 'imp' c) '&' (c 'imp' b))) '&'
    (((b 'imp' c) '&' (c 'imp' b)) 'imp' ((a 'imp' c) '&' (c 'imp' a)))).z
    by BVFUNC_4:8
      .=(((('not' a 'or' c) '&' ('not' c 'or' a)) 'imp' (('not'
    b 'or' c) '&' (c 'imp' b))) '&'
    (((b 'imp' c) '&' (c 'imp' b)) 'imp' ((a 'imp' c) '&' (c 'imp' a)))).z
    by BVFUNC_4:8
      .=(((('not' a 'or' c) '&' ('not' c 'or' a)) 'imp'
    (('not' b 'or' c) '&' ('not' c 'or' b))) '&'
    (((b 'imp' c) '&' (c 'imp' b)) 'imp' ((a 'imp' c) '&' (c 'imp' a)))).z
    by BVFUNC_4:8
      .=(((('not' a 'or' c) '&' ('not' c 'or' a)) 'imp'
    (('not' b 'or' c) '&' ('not' c 'or' b))) '&' ((('not'
    b 'or' c) '&' (c 'imp' b)) 'imp' ((a 'imp' c) '&' (c 'imp' a)))).z
    by BVFUNC_4:8
      .=(((('not' a 'or' c) '&' ('not' c 'or' a)) 'imp'
    (('not' b 'or' c) '&' ('not' c 'or' b))) '&' ((('not' b 'or' c) '&' ('not'
    c 'or' b)) 'imp' ((a 'imp' c) '&' (c 'imp' a)))).z by BVFUNC_4:8
      .=(((('not' a 'or' c) '&' ('not' c 'or' a)) 'imp'
    (('not' b 'or' c) '&' ('not' c 'or' b))) '&'
    ((('not' b 'or' c) '&' ('not' c 'or' b)) 'imp' (('not'
    a 'or' c) '&' (c 'imp' a)))).z by BVFUNC_4:8
