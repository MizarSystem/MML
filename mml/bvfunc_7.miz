    case
A10:  'not' (a).z=TRUE;
      then
A11:  (a).z=FALSE by MARGREL1:41;
      then 'not' (b).z=TRUE by A7,BINARITH:7;
      then ((a '&' c) 'eqv' (b '&' c)).z
