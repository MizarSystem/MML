  proof
    let x be Element of Y;
       Pj(a 'imp' I_el(Y),x)
   ='not' Pj(a,x) 'or' Pj(I_el(Y),x) by BVFUNC_1:def 11
  .='not' Pj(a,x) 'or' TRUE by BVFUNC_1:def 14;
    hence thesis by BINARITH:19;
  end;
  hence thesis by BVFUNC_1:def 14;
