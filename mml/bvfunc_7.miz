FALSE '&' (('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x)) by MARGREL1:
        50
          .=(('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x)) 'or'
        FALSE by MARGREL1:49
          .=((a 'imp' b) '&' (b 'imp' c)).x by A6,BINARITH:7;
