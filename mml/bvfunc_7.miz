    (Pj('not'( b 'eqv' a),z) 'or' Pj((a 'imp' b) '&' (b 'imp' a),z))
    by BVFUNC_4:7
  .=(Pj('not'( (a 'imp' b) '&' (b 'imp' a)),z) 'or'
     Pj((b 'imp' a) '&' (a 'imp' b),z)) '&'
    (Pj('not'( b 'eqv' a),z) 'or' Pj((a 'imp' b) '&' (b 'imp' a),z))
    by BVFUNC_4:7
  .=(Pj('not'( (a 'imp' b) '&' (b 'imp' a)),z) 'or'
     Pj((b 'imp' a) '&' (a 'imp' b),z)) '&'
    (Pj('not'( (b 'imp' a) '&' (a 'imp' b)),z) 'or'
     Pj((a 'imp' b) '&' (b 'imp' a),z))
    by BVFUNC_4:7
  .=('not' Pj((a 'imp' b) '&' (b 'imp' a),z) 'or'
      Pj((b 'imp' a) '&' (a 'imp' b),z)) '&'
    (Pj('not'( (b 'imp' a) '&' (a 'imp' b)),z) 'or'
     Pj((a 'imp' b) '&' (b 'imp' a),z))
    by VALUAT_1:def 5
  .=('not' Pj((a 'imp' b) '&' (b 'imp' a),z) 'or'
