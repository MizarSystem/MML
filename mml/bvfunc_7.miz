A7: (b 'imp' a).z = 'not' (b).z 'or' (a).z by BVFUNC_1:def 11;
A8: (b).z=TRUE or (b).z=FALSE by XBOOLEAN:def 3;
  now
    per cases by A3,A8,BINARITH:7;
    case
A9:   'not' (a).z=TRUE;
      then
A10:  (a).z=FALSE by MARGREL1:41;
      then 'not' (b).z=TRUE by A4,BINARITH:7;
      then
      ((a 'or' c) 'eqv' (b 'or' c)).z =('not' (c).z 'or' (FALSE 'or' (c).
      z)) '&' ('not' (c).z 'or' (FALSE 'or' (c).z)) by A5,A9,A10,MARGREL1:50
        .=('not' (c).z 'or' (c).z) '&' ('not' (c).z 'or' (c).z) by BINARITH:
