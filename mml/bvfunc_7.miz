    case
A12:  (b).z=TRUE;
      then 'not' (b).z=FALSE by MARGREL1:41;
      then ((a 'imp' c) 'eqv' (b 'imp' c)).z
      =('not' (c).z 'or' (FALSE 'or' (c).z)) '&'
