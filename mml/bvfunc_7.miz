      ('not' (c).z 'or' (FALSE 'or' (c).z)) by A9,A10,A11,MARGREL1:50
        .=('not' (c).z 'or' (c).z) '&' ('not' (c).z 'or' (c).z) by BINARITH:7
        .=TRUE by XBOOLEAN:102;
      hence thesis;
    end;
    case
A12:  (b).z=TRUE;
