environ

 vocabularies XBOOLE_0, SUBSET_1, FUNCT_2, MARGREL1, BVFUNC_1, XBOOLEAN,
      FUNCT_1, RELAT_1, TARSKI, PARTIT1;
 notations TARSKI, XBOOLE_0, SUBSET_1, XBOOLEAN, MARGREL1, RELAT_1, FUNCT_1,
      FUNCT_2, BVFUNC_1;
 constructors BVFUNC_1;
 registrations XBOOLE_0, XBOOLEAN, MARGREL1;
 requirements SUBSET, BOOLE;
 definitions BVFUNC_1, XBOOLEAN;
 theorems FUNCT_1, FUNCT_2, MARGREL1, BINARITH, BVFUNC_1, BVFUNC_4, XBOOLEAN;

begin
 
::Chap. 1  Propositional Calculus

reserve Y for non empty set;

theorem
  for a,b being Element of Funcs(Y,BOOLEAN) holds (a 'imp' b) '&' ('not'
  a 'imp' b) = b
proof
  let a,b be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: ((a 'imp' b) '&' ('not' a 'imp' b))=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: b=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds ((a 'imp' b) '&' ('not' a 'imp' b)).x = ( b).x
  proof
    let x be Element of Y;
A5: ((a 'imp' b) '&' ('not' a 'imp' b)).x =(a 'imp' b).x '&' ('not' a
    'imp' b).x by MARGREL1:def 21
      .=('not' (a).x 'or' (b).x) '&' ('not' a 'imp' b).x by BVFUNC_1:def 11
      .=('not' (a).x 'or' (b).x) '&' ('not' ('not' a).x 'or' (b).x) by
    BVFUNC_1:def 11
      .=('not' (a).x 'or' (b).x) '&' ((a).x 'or' (b).x) by MARGREL1:def 20;
    now
      per cases by XBOOLEAN:def 3;
      case
        (a).x=TRUE;
        then ((a 'imp' b) '&' ('not' a 'imp' b)).x =(FALSE 'or' (b).x) '&' (
        TRUE 'or' (b).x) by A5,MARGREL1:41
          .=(FALSE 'or' (b).x) '&' TRUE by BINARITH:19
          .=TRUE '&' (b).x by BINARITH:7
          .=(b).x by MARGREL1:50;
        hence thesis;
      end;
      case
        (a).x=FALSE;
        then ((a 'imp' b) '&' ('not' a 'imp' b)).x =(TRUE 'or' (b).x) '&' (
        FALSE 'or' (b).x) by A5,MARGREL1:41
          .=TRUE '&' (FALSE 'or' (b).x) by BINARITH:19
          .=TRUE '&' (b).x by BINARITH:7
          .=(b).x by MARGREL1:50;
        hence thesis;
      end;
    end;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b being Element of Funcs(Y,BOOLEAN) holds (a 'imp' b) '&' (a
  'imp' 'not' b) = 'not' a
proof
  let a,b be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: ((a 'imp' b) '&' (a 'imp' 'not' b))=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: 'not' a=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds ((a 'imp' b) '&' (a 'imp' 'not' b)).x = (
  'not' a).x
  proof
    let x be Element of Y;
A5: ((a 'imp' b) '&' (a 'imp' 'not' b)).x =(a 'imp' b).x '&' (a 'imp'
    'not' b).x by MARGREL1:def 21
      .=('not' (a).x 'or' (b).x) '&' (a 'imp' 'not' b).x by BVFUNC_1:def 11
      .=('not' (a).x 'or' (b).x) '&' ('not' (a).x 'or' ('not' b).x) by
    BVFUNC_1:def 11
      .=('not' (a).x 'or' (b).x) '&' ('not' (a).x 'or' 'not' (b).x) by
    MARGREL1:def 20;
    now
      per cases by XBOOLEAN:def 3;
      case
        (b).x=TRUE;
        then
        ((a 'imp' b) '&' (a 'imp' 'not' b)).x =('not' (a).x 'or' TRUE) '&'
        ('not' (a).x 'or' FALSE) by A5, MARGREL1:41
          .=('not' (a).x 'or' TRUE) '&' 'not' (a).x by BINARITH:7
          .=TRUE '&' 'not' (a).x by BINARITH:19
          .='not' (a).x by MARGREL1:50
          .=('not' a).x by MARGREL1:def 20;
        hence thesis;
      end;
      case
        (b).x=FALSE;
        then ((a 'imp' b) '&' (a 'imp' 'not' b)).x =('not' (a).x 'or' FALSE)
        '&' ('not' (a).x 'or' TRUE) by A5, MARGREL1:41
          .='not' (a).x '&' ('not' (a).x 'or' TRUE) by BINARITH:7
          .=TRUE '&' 'not' (a).x by BINARITH:19
          .='not' (a).x by MARGREL1:50
          .=('not' a).x by MARGREL1:def 20;
        hence thesis;
      end;
    end;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b,c being Element of Funcs(Y,BOOLEAN) holds a 'imp' (b 'or' c) =
  (a 'imp' b) 'or' (a 'imp' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: (a 'imp' (b 'or' c))=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: (a 'imp' b) 'or' (a 'imp' c)=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds (a 'imp' (b 'or' c)).x = ((a 'imp' b)
  'or' (a 'imp' c)).x
  proof
    let x be Element of Y;
    ((a 'imp' b) 'or' (a 'imp' c)).x =(a 'imp' b).x 'or' (a 'imp' c).x by
    BVFUNC_1:def 7
      .=('not' (a).x 'or' (b).x) 'or' (a 'imp' c).x by BVFUNC_1:def 11
      .=('not' (a).x 'or' (b).x) 'or' ('not' (a).x 'or' (c).x) by BVFUNC_1:
    def 11
      .=('not' (a).x 'or' ('not' (a).x 'or' (b).x)) 'or' (c).x by BINARITH:
    20
      .=(('not' (a).x 'or' 'not' (a).x) 'or' (b).x) 'or' (c).x by BINARITH:
    20
      .='not' (a).x 'or' ((b).x 'or' (c).x) by BINARITH:20
      .='not' (a).x 'or' (b 'or' c).x by BVFUNC_1:def 7
      .=(a 'imp' (b 'or' c)).x by BVFUNC_1:def 11;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b,c being Element of Funcs(Y,BOOLEAN) holds a 'imp' (b '&' c) =
  (a 'imp' b) '&' (a 'imp' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: (a 'imp' (b '&' c))=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: (a 'imp' b) '&' (a 'imp' c)=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds (a 'imp' (b '&' c)).x = ((a 'imp' b) '&'
  (a 'imp' c)).x
  proof
    let x be Element of Y;
    ((a 'imp' b) '&' (a 'imp' c)).x =(a 'imp' b).x '&' (a 'imp' c).x by
    MARGREL1:def 21
      .=('not' (a).x 'or' (b).x) '&' (a 'imp' c).x by BVFUNC_1:def 11
      .=('not' (a).x 'or' (b).x) '&' ('not' (a).x 'or' (c).x) by BVFUNC_1:
    def 11
      .='not' (a).x 'or' ((b).x '&' (c).x) by XBOOLEAN:9
      .='not' (a).x 'or' ((b '&' c).x) by MARGREL1:def 21
      .=(a 'imp' (b '&' c)).x by BVFUNC_1:def 11;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b,c being Element of Funcs(Y,BOOLEAN) holds (a 'or' b) 'imp' c =
  (a 'imp' c) '&' (b 'imp' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: ((a 'or' b) 'imp' c)=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: (a 'imp' c) '&' (b 'imp' c)=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds ((a 'or' b) 'imp' c).x = ((a 'imp' c) '&'
  (b 'imp' c)).x
  proof
    let x be Element of Y;
    ((a 'imp' c) '&' (b 'imp' c)).x =(a 'imp' c).x '&' (b 'imp' c).x by
    MARGREL1:def 21
      .=('not' (a).x 'or' (c).x) '&' (b 'imp' c).x by BVFUNC_1:def 11
      .=((c).x 'or' 'not' (a).x) '&' ('not' (b).x 'or' (c).x) by BVFUNC_1:
    def 11
      .='not'( (a).x 'or' (b).x) 'or' (c).x by XBOOLEAN:9
      .='not' (a 'or' b).x 'or' (c).x by BVFUNC_1:def 7
      .=((a 'or' b) 'imp' c).x by BVFUNC_1:def 11;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b,c being Element of Funcs(Y,BOOLEAN) holds (a '&' b) 'imp' c =
  (a 'imp' c) 'or' (b 'imp' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: ((a '&' b) 'imp' c)=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: (a 'imp' c) 'or' (b 'imp' c)=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds ((a '&' b) 'imp' c).x = ((a 'imp' c) 'or'
  (b 'imp' c)).x
  proof
    let x be Element of Y;
    ((a 'imp' c) 'or' (b 'imp' c)).x =(a 'imp' c).x 'or' (b 'imp' c).x by
    BVFUNC_1:def 7
      .=('not' (a).x 'or' (c).x) 'or' (b 'imp' c).x by BVFUNC_1:def 11
      .=('not' (a).x 'or' (c).x) 'or' ('not' (b).x 'or' (c).x) by BVFUNC_1:
    def 11
      .=('not' (a).x 'or' ((c).x 'or' 'not' (b).x)) 'or' (c).x by BINARITH:
    20
      .=(('not' (a).x 'or' 'not' (b).x) 'or' (c).x) 'or' (c).x by BINARITH:
    20
      .=('not' (a).x 'or' 'not' (b).x) 'or' ((c).x 'or' (c).x) by BINARITH:
    20
      .='not' (a '&' b).x 'or' (c).x by MARGREL1:def 21
      .=((a '&' b) 'imp' c).x by BVFUNC_1:def 11;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem Th7:
  for a,b,c being Element of Funcs(Y,BOOLEAN) holds (a '&' b) 'imp'
  c = a 'imp' (b 'imp' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: ((a '&' b) 'imp' c)=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: a 'imp' (b 'imp' c)=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds ((a '&' b) 'imp' c).x = (a 'imp' (b 'imp'
  c)).x
  proof
    let x be Element of Y;
    (a 'imp' (b 'imp' c)).x ='not' (a).x 'or' (b 'imp' c).x by BVFUNC_1:
    def 11
      .='not' (a).x 'or' ('not' (b).x 'or' (c).x) by BVFUNC_1:def 11
      .=('not' (a).x 'or' 'not' (b).x) 'or' (c).x by BINARITH:20
      .='not' (a '&' b).x 'or' (c).x by MARGREL1:def 21
      .=((a '&' b) 'imp' c).x by BVFUNC_1:def 11;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b,c being Element of Funcs(Y,BOOLEAN) holds (a '&' b) 'imp' c =
  a 'imp' ('not' b 'or' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: ((a '&' b) 'imp' c)=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: a 'imp' ('not' b 'or' c)=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds ((a '&' b) 'imp' c).x = (a 'imp' ('not' b
  'or' c)).x
  proof
    let x be Element of Y;
    (a 'imp' ('not' b 'or' c)).x =(a 'imp' (b 'imp' c)).x by BVFUNC_4:8;
    hence thesis by Th7;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b,c being Element of Funcs(Y,BOOLEAN) holds a 'imp' (b 'or' c) =
  (a '&' 'not' b) 'imp' c
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: (a 'imp' (b 'or' c))=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: (a '&' 'not' b) 'imp' c =k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds (a 'imp' (b 'or' c)).x = ((a '&' 'not' b)
  'imp' c).x
  proof
    let x be Element of Y;
    ((a '&' 'not' b) 'imp' c).x ='not' (a '&' 'not' b).x 'or' (c).x by
    BVFUNC_1:def 11
      .=('not' (a).x 'or' 'not' ('not' b).x) 'or' (c).x by MARGREL1:def 21
      .=('not' (a).x 'or' (b).x) 'or' (c).x by MARGREL1:def 20
      .='not' (a).x 'or' ((b).x 'or' (c).x) by BINARITH:20
      .='not' (a).x 'or' (b 'or' c).x by BVFUNC_1:def 7
      .=(a 'imp' (b 'or' c)).x by BVFUNC_1:def 11;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b being Element of Funcs(Y,BOOLEAN) holds a '&' (a 'imp' b) = a '&' b
proof
  let a,b be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: (a '&' (a 'imp' b))=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: a '&' b=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds (a '&' (a 'imp' b)).x = (a '&' b).x
  proof
    let x be Element of Y;
    (a '&' (a 'imp' b)).x =(a).x '&' (a 'imp' b).x by MARGREL1:def 21
      .=(a).x '&' ('not' (a).x 'or' (b).x) by BVFUNC_1:def 11
      .=((a).x '&' 'not' (a).x) 'or' ((a).x '&' (b).x) by XBOOLEAN:8
      .=FALSE 'or' ((a).x '&' (b).x) by XBOOLEAN:138
      .=(a).x '&' (b).x by BINARITH:7
      .=(a '&' b).x by MARGREL1:def 21;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b being Element of Funcs(Y,BOOLEAN) holds (a 'imp' b) '&' 'not'
  b = 'not' a '&' 'not' b
proof
  let a,b be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: ((a 'imp' b) '&' 'not' b)=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: 'not' a '&' 'not' b=k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds ((a 'imp' b) '&' 'not' b).x = ('not' a
  '&' 'not' b).x
  proof
    let x be Element of Y;
    ((a 'imp' b) '&' 'not' b).x =(a 'imp' b).x '&' ('not' b).x by MARGREL1
    :def 21
      .=('not' b).x '&' ('not' (a).x 'or' (b).x) by BVFUNC_1:def 11
      .=(('not' b).x '&' 'not' (a).x) 'or' (('not' b).x '&' (b).x) by
    XBOOLEAN:8
      .=(('not' b).x '&' 'not' (a).x) 'or' ((b).x '&' 'not' (b).x) by
    MARGREL1:def 20
      .=(('not' b).x '&' 'not' (a).x) 'or' FALSE by XBOOLEAN:138
      .=('not' b).x '&' 'not' (a).x by BINARITH:7
      .=('not' b).x '&' ('not' a).x by MARGREL1:def 20
      .=('not' a '&' 'not' b).x by MARGREL1:def 21;
    hence thesis;
  end;
  then for u being set st u in Y holds k3.u=k4.u by A1,A3;
  hence thesis by A1,A2,A3,A4,FUNCT_1:9;
end;

theorem
  for a,b,c being Element of Funcs(Y,BOOLEAN) holds (a 'imp' b) '&' (b
  'imp' c) = (a 'imp' b) '&' (b 'imp' c) '&' (a 'imp' c)
proof
  let a,b,c be Element of Funcs(Y,BOOLEAN);
  consider k3 being Function such that
A1: ((a 'imp' b) '&' (b 'imp' c))=k3 and
A2: dom k3=Y and
  rng k3 c= BOOLEAN by FUNCT_2:def 2;
  consider k4 being Function such that
A3: (a 'imp' b) '&' (b 'imp' c) '&' (a 'imp' c)= k4 and
A4: dom k4=Y and
  rng k4 c= BOOLEAN by FUNCT_2:def 2;
  for x being Element of Y holds ((a 'imp' b) '&' (b 'imp' c)).x = ((a
  'imp' b) '&' (b 'imp' c) '&' (a 'imp' c)).x
  proof
    let x be Element of Y;
A5: ((a 'imp' b) '&' (b 'imp' c) '&' (a 'imp' c)).x =((a 'imp' b) '&' (b
    'imp' c)).x '&' ((a 'imp' c)).x by MARGREL1:def 21
      .=((a 'imp' b).x '&' (b 'imp' c).x) '&' ((a 'imp' c)).x by MARGREL1:
    def 21
      .=(('not' (a).x 'or' (b).x) '&' (b 'imp' c).x) '&' ((a 'imp' c)).x by
    BVFUNC_1:def 11
      .=(('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x)) '&' ((a
    'imp' c)).x by BVFUNC_1:def 11
      .=(('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x)) '&' ('not' (
    a).x 'or' (c).x) by BVFUNC_1:def 11
      .=(('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x)) '&' 'not' (a
).x 'or' (('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x)) '&' (c).x by
    XBOOLEAN:8;
A6: (('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x)) =((a 'imp' b).
    x '&' ('not' (b).x 'or' (c).x)) by BVFUNC_1:def 11
      .=(a 'imp' b).x '&' (b 'imp' c).x by BVFUNC_1:def 11
      .=((a 'imp' b) '&' (b 'imp' c)).x by MARGREL1:def 21;
A7: ((a 'imp' b) '&' (b 'imp' c)).x =(a 'imp' b).x '&' (b 'imp' c).x by
    MARGREL1:def 21
      .=('not' (a).x 'or' (b).x) '&' (b 'imp' c).x by BVFUNC_1:def 11
      .=('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x) by BVFUNC_1:
    def 11;
    now
      per cases by XBOOLEAN:def 3;
      case
        (a).x=TRUE & (c).x=TRUE;
        then ((a 'imp' b) '&' (b 'imp' c) '&' (a 'imp' c)).x =(('not' (a).x
'or' (b).x) '&' ('not' (b).x 'or' (c).x)) '&' FALSE 'or' (('not' (a).x 'or' (b)
        .x) '&' ('not' (b).x 'or' (c).x)) '&' TRUE by A5,MARGREL1:41
          .=FALSE 'or' (('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x
        )) '&' TRUE by MARGREL1:49
          .=FALSE 'or' (('not' (a).x 'or' (b).x) '&' ('not' (b).x 'or' (c).x
        )) by MARGREL1:50
          .=((a 'imp' b) '&' (b 'imp' c)).x by A6,BINARITH:7;
