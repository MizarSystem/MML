    ('not'( Pj(b,z) '&' Pj(c,z)) 'or' (Pj(a,z) '&' Pj(c,z)))
   by VALUAT_1:def 6
  .=(('not' Pj(a,z) 'or' 'not' Pj(c,z)) 'or' (Pj(b,z) '&' Pj(c,z))) '&'
    ('not'( Pj(b,z) '&' Pj(c,z)) 'or' (Pj(a,z) '&' Pj(c,z)))
