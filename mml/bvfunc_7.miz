      then ((a '&' c) 'eqv' (b '&' c)).z
      =((FALSE 'or' 'not' (c).z) 'or' (c).z) '&'
      ((FALSE 'or' 'not' (c).z) 'or' (c).z) by A1,A2,A6,A8,A9,A12,BINARITH:7
