      .= (b-a)*c/b + (a/b) * d by Lm8
      .= ((b - a)/b)*c + (a/b) * d by Lm8
      .= (b/b - a/b)*c + (a/b) * d by Th121
      .= (1 - a/b)*c + (a/b) * d by A3,Lm5;
    hence thesis;
  end;
end;
 
