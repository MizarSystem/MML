theorem Th189: :: REAL_1'39_2
  a / (- b) = -a / b
proof
    a/(-b)=(a*(-1))/((-b)*(-1)) by Lm27;
  then a/(-b)=(-a*1)/((-b)*(-1)) by Lm20
   .= (-a)/((-(-b))*1) by Lm21
   .= -a/b by Lm35;
  hence thesis;
end;

theorem  :: REAL_2'58_1
    - a / (- b) = a / b
