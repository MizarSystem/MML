   then A5:e<>0 by A1,A4,Th6;
   thus a=a*1
        .=a*(e*e") by A5,XCMPLX_0:def 7
        .=a*e*e"
        .=a*(a"*b)*e" by XCMPLX_0:def 9
        .=a*a"*b*e"
        .=1*b*e" by A3,XCMPLX_0:def 7
        .=b/e by XCMPLX_0:def 9;
   end;
  end;

:: from IRRAT_1, 2005.02.05, A.T.

theorem
  a/(b*c*(d/e))=(e/c)*(a/(b*d))
proof
 thus a/(b*c*(d/e)) = a/(b*c*(d*e")) by XCMPLX_0:def 9
  .= a/(c*(b*d*e"))
  .= a/(c*((b*d)/e)) by XCMPLX_0:def 9
  .= a/((b*d)/(e/c)) by Th82
  .= (e/c)*(a/(b*d)) by Th82;
end;

:: from BORSUK_6, 2005.02.05, A.T.

theorem
    ((d - c)/b) * a + c = (1 - a/b)*c + (a/b) * d
  proof
    per cases;
    suppose
A1:    b = 0;
A2:    a/b = a*b" by XCMPLX_0:def 9 .= a*0 by A1,XCMPLX_0:def 7;
      thus ((d - c)/b) * a + c = ((d - c)*b") * a + c by XCMPLX_0:def 9
                              .= ((d - c)*0) * a + c by A1,XCMPLX_0:def 7
                              .= (1 - (a/b))*c + (a/b) * d by A2;
    end;
    suppose
A3:   b <> 0;
      ((d - c)/b) * a + c = ((d - c)/b) * a + c*1
                         .= ((d - c)/b) * a + c*(b/b) by A3,Lm5
                         .= ((d - c)/b) * a + c*b/b by Lm9
                         .= (d - c) * a / b + c*b/b by Lm9
                         .= ((d-c)*a + c*b)/ b by Th63
                         .= ((b-a)*c + a * d) / b
                         .= (b-a)*c/b + a*d/b by Th63
                         .= (b-a)*c/b + (a/b) * d by Lm9
                         .= ((b - a)/b)*c + (a/b) * d by Lm9
                         .= (b/b - a/b)*c + (a/b) * d by Th121
                         .= (1 - a/b)*c + (a/b) * d by A3,Lm5;
      hence thesis;
    end;
  end;

