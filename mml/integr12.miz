::f.x=1/(1+x^2)

X: -1 is Real by XREAL_0:def 1;

theorem Th1:
 Z c= dom ((f1+f2)^) & (for x st x in Z holds f1.x=1) & f2=#Z 2
 implies (f1+f2)^ is_differentiable_on Z & for x st x in Z holds
 (((f1+f2)^)`|Z).x = -2*x/(1+x |^2)^2
 proof
  assume
A1:Z c= dom ((f1+f2)^) & (for x st x in Z holds f1.x=1) & f2=#Z 2;
dom ((f1+f2)^) c= dom (f1+f2) by RFUNCT_1:11;
then A3:Z c= dom (f1+f2) by A1,XBOOLE_1:1;
then A4:f1+f2 is_differentiable_on Z &
   for x st x in Z holds ((f1+f2)`|Z).x = 2*x by A1,SIN_COS9:101;
A5:for x st x in Z holds (f1+f2).x<>0 by RFUNCT_1:13,A1;
then A6:(f1+f2)^ is_differentiable_on Z by A4,FDIFF_2:22;
  for x st x in Z holds (((f1+f2)^)`|Z).x = -2*x/(1+x |^2)^2
  proof
    let x;
    assume
A7: x in Z; then
A8: (f1+f2).x<>0 by RFUNCT_1:13,A1;
A9: f1+f2 is_differentiable_in x by A4,A7,FDIFF_1:16;
A10:f2.x=x #Z 2 by A1,TAYLOR_1:def 1
      .=x |^2 by PREPOWER:46;
A11:(f1+f2).x=f1.x+f2.x by A3,A7,VALUED_1:def 1
      .=1+x |^2 by A1,A7,A10;
    (((f1+f2)^)`|Z).x = diff((f1+f2)^,x) by A6,A7,FDIFF_1:def 8
                   .= - diff((f1+f2),x)/((f1+f2).x)^2 by A8,A9,FDIFF_2:15
                   .= -((f1+f2)`|Z).x/((f1+f2).x)^2 by A4,A7,FDIFF_1:def 8
                   .= -2*x/(1+x |^2)^2 by A1,A3,SIN_COS9:101,A7,A11;
    hence thesis;
  end;
  hence thesis by A4,A5,FDIFF_2:22;
end;
 
::f.x=1/((1+x^2)*arccot.x)

theorem
 A c= Z & f = ((g1+g2)^)/f2 & f2 = arccot & Z c= ]. -1,1 .[ & g2=#Z 2
 & (for x st x in Z holds g1.x=1 & f2.x>0) & Z = dom f implies
 integral(f,A)=(-ln*arccot).(upper_bound A)-(-ln*arccot).(lower_bound A)
proof
   assume
A1:A c= Z & f = ((g1+g2)^)/f2 & f2 = arccot & Z c= ]. -1,1 .[ & g2=#Z 2
 & (for x st x in Z holds g1.x=1 & f2.x>0) & Z = dom f;
then  Z = dom ((g1+g2)^) /\ (dom f2 \ f2"{0}) by RFUNCT_1:def 4;
then A4: Z c= dom ((g1+g2)^) & Z c= dom f2 \ f2"{0} by XBOOLE_1:18;
    dom ((g1+g2)^) c= dom (g1+g2) by RFUNCT_1:11;then
AB:Z c= dom (g1+g2) by A4,XBOOLE_1:1;
 for x st x in Z holds g1.x=1 by A1;
then A6: (g1+g2)^ is_differentiable_on Z by A1,A4,Th1;
A7: f2 is_differentiable_on Z by A1,SIN_COS9:82;
 for x st x in Z holds f2.x<>0 by A1;
then  f is_differentiable_on Z by A1,A6,A7,FDIFF_2:21;
then f|Z is continuous by FDIFF_1:33;
then A11:f|A is continuous by FCONT_1:17,A1;
A12:Z c= dom (f2^) by A4,RFUNCT_1:def 8;
    dom (f2^) c= dom f2 by RFUNCT_1:11;then
A13:Z c= dom f2 by A12,XBOOLE_1:1;
A14:for x st x in Z holds f2.x>0 by A1;
rng (f2|Z) c= right_open_halfline(0)
    proof
      let x be set;
      assume x in rng (f2|Z); then
      consider y be set such that
A18:  y in dom (f2|Z) & x = (f2|Z).y by FUNCT_1:def 5;
      y in Z by A18,RELAT_1:86; then
      f2.y > 0 by A1; then
      (f2|Z).y > 0 by A18,FUNCT_1:70;
      hence thesis by A18,XXREAL_1:235;
    end;
then f2.:Z c= dom ln by RELAT_1:148,TAYLOR_1:18;
then A22:Z c= dom (ln*arccot) by A1,A13,FUNCT_1:171;
A23:f is_integrable_on A & f|A is bounded by A1,A11,INTEGRA5:10,11;
A24:ln*arccot is_differentiable_on Z by A1,A22,A14,SIN_COS9:90;
Z c= dom (-ln*arccot) by A22,VALUED_1:8;
then A26:-ln*arccot is_differentiable_on Z by A24,FDIFF_1:28,X;
A27:for x st x in Z holds ((-ln*arccot)`|Z).x = 1/((1+x^2)*arccot.x)
    proof
      let x;
      assume
A28:x in Z;then
A29:-1 < x & x < 1 by A1,XXREAL_1:4;
    arccot is_differentiable_on Z by SIN_COS9:82,A1;then
A30:arccot is_differentiable_in x by A28,FDIFF_1:16;
A31:arccot.x >0 by A1,A28;
A32:ln*arccot is_differentiable_in x by A24,A28,FDIFF_1:16;
 ((-ln*arccot)`|Z).x=diff((-ln*arccot),x) by A26,A28,FDIFF_1:def 8
                   .=(-1)*diff(ln*arccot,x) by A32,FDIFF_1:23,X
                   .=(-1)*(diff(arccot,x)/arccot.x) by A30,A31,TAYLOR_1:20
                   .=(-1)*((-1/(1+x^2))/arccot.x) by A29,SIN_COS9:76
                   .= (1/(1+x^2))/arccot.x
                   .= 1/((1+x^2)*arccot.x) by XCMPLX_1:79;
                 hence thesis;
     end;
B1:for x st x in Z holds f.x=1/((1+x^2)*arccot.x)
   proof
   let x;
   assume
B2:x in Z;
   then (((g1+g2)^)/f2).x =((g1+g2)^).x/f2.x by RFUNCT_1:def 4,A1
  .=((g1+g2).x)"/f2.x by RFUNCT_1:def 8,A4,B2
  .=(g1.x+g2.x)"/f2.x by VALUED_1:def 1,AB,B2
  .=1/((g1.x+g2.x)*f2.x) by XCMPLX_1:223
  .=1/((1+( #Z 2).x)*f2.x) by A1,B2
  .=1/((1+(x #Z 2))*f2.x) by TAYLOR_1:def 1
  .=1/((1+x^2)*arccot.x) by A1, FDIFF_7:1;
    hence thesis by A1;
    end;
A33:for x st x in dom ((-ln*arccot)`|Z) holds ((-ln*arccot)`|Z).x=f.x
   proof
     let x;
     assume x in dom ((-ln*arccot)`|Z);then
A34:x in Z by A26,FDIFF_1:def 8;then
  ((-ln*arccot)`|Z).x=1/((1+x^2)*arccot.x) by A27
                      .=f.x by B1,A34;
  hence thesis;
  end;
  dom ((-ln*arccot)`|Z)=dom f by A1,A26,FDIFF_1:def 8;
  then ((-ln*arccot)`|Z)= f by A33,PARTFUN1:34;
  hence thesis by A1,A23,A26,INTEGRA5:13;
end;
 
::f.x=exp_R.x/(1+(exp_R.x)^2)

theorem
 A c= Z & (for x st x in Z holds exp_R.x < 1 & f1.x=1)
 & Z c= dom (arctan*exp_R)
 & Z = dom f & f=exp_R/(f1+(exp_R)^2)
 implies integral(f,A)=(arctan*exp_R).(upper_bound A)
                      -(arctan*exp_R).(lower_bound A)
proof
   assume
A1: A c= Z & (for x st x in Z holds exp_R.x < 1 & f1.x=1)
    & Z c= dom (arctan*exp_R)
    & Z = dom f & f=exp_R/(f1+(exp_R)^2);
then Z c= dom exp_R /\ (dom (f1+(exp_R)^2) \ (f1+(exp_R)^2)"{0})
   by RFUNCT_1:def 4;
then Z c= dom exp_R & Z c= dom (f1+(exp_R)^2) \ (f1+(exp_R)^2)"{0}
   by XBOOLE_1:18;
then A5:Z c= dom ((f1+(exp_R)^2)^) by RFUNCT_1:def 8;
   dom ((f1+(exp_R)^2)^) c= dom (f1+(exp_R)^2) by RFUNCT_1:11;then
A6:Z c= dom (f1+(exp_R)^2) by XBOOLE_1:1,A5;
then B1:Z c= dom f1 /\ dom ((exp_R)^2) by VALUED_1:def 1;
then B2:Z c= dom f1 & Z c= dom ((exp_R)^2) by XBOOLE_1:18;
A7:Z c= dom ((exp_R)(#)(exp_R)) by XBOOLE_1:18,B1;
A8:exp_R is_differentiable_on Z by FDIFF_1:34,TAYLOR_1:16;
then A9:(exp_R)(#)(exp_R) is_differentiable_on Z by A7, FDIFF_1:29;
for x st x in Z holds f1.x=0*x+1 by A1;
then f1 is_differentiable_on Z by FDIFF_1:31,B2;
then A13:f1+(exp_R)^2 is_differentiable_on Z by A6,A9,FDIFF_1:26;
for x st x in Z holds (f1+(exp_R)^2).x<>0
   proof
   let x;
   assume x in Z;then
   x in dom exp_R /\ (dom (f1+(exp_R)^2) \ (f1+(exp_R)^2)"{0})
     by A1,RFUNCT_1:def 4;then
   x in dom (f1+(exp_R)^2) \ (f1+(exp_R)^2)"{0} by XBOOLE_0:def 4; then
   x in dom ((f1+(exp_R)^2)^) by RFUNCT_1:def 8;
      hence thesis by RFUNCT_1:13;
   end;
then f is_differentiable_on Z by A1,A8,A13,FDIFF_2:21;
then f|Z is continuous by FDIFF_1:33;
then f|A is continuous by A1,FCONT_1:17;
then A18:f is_integrable_on A & f|A is bounded by A1,INTEGRA5:10,11;
A19:for x st x in Z holds exp_R.x < 1 by A1;then
A20:arctan*exp_R is_differentiable_on Z by A1,SIN_COS9:115;
B3:for x st x in Z holds f.x=exp_R.x/(1+(exp_R.x)^2)
   proof
   let x;
   assume
B4:x in Z;
    then (exp_R/(f1+(exp_R)^2)).x=(exp_R.x)*((f1+(exp_R)^2).x)"
                                 by RFUNCT_1:def 4,A1
                           .=(exp_R.x)*(f1.x+((exp_R)^2).x)"
                                by VALUED_1:def 1,B4,A6
                           .=(exp_R.x)*(f1.x+(exp_R.x)^2)"
                                by VALUED_1:11
                           .=exp_R.x/(1+(exp_R.x)^2) by A1,B4;
        hence thesis by A1;
      end;
A21:for x st x in dom ((arctan*exp_R)`|Z) holds ((arctan*exp_R)`|Z).x=f.x
    proof
       let x;
       assume x in dom ((arctan*exp_R)`|Z);then
A22:x in Z by A20, FDIFF_1:def 8;then
   ((arctan*exp_R)`|Z).x=exp_R.x/(1+(exp_R.x)^2) by A1,A19,SIN_COS9:115
                       .=f.x by A22,B3;
   hence thesis;
   end;
   dom ((arctan*exp_R)`|Z)=dom f by A1,A20,FDIFF_1:def 8;
   then ((arctan*exp_R)`|Z)= f by A21,PARTFUN1:34;
   hence thesis by A1,A18,A20,INTEGRA5:13;
end;
 
::f.x=-exp_R.x/(1+(exp_R.x)^2)

theorem
A c= Z & (for x st x in Z holds exp_R.x < 1 & f1.x=1)
& Z c= dom (arccot*exp_R) & Z = dom f & f=(-exp_R)/(f1+(exp_R)^2)
 implies integral(f,A)=(arccot*exp_R).(upper_bound A)
                      -(arccot*exp_R).(lower_bound A)
proof
   assume
A1:A c= Z & (for x st x in Z holds exp_R.x < 1 & f1.x=1)
   & Z c= dom (arccot*exp_R) & Z = dom f
   & f=(-exp_R)/(f1+(exp_R)^2);
then Z c= dom (-exp_R) /\ (dom (f1+(exp_R)^2) \ (f1+(exp_R)^2)"{0})
   by RFUNCT_1:def 4;
then A4:Z c= dom (-exp_R) & Z c= dom (f1+(exp_R)^2) \ (f1+(exp_R)^2)"{0}
   by XBOOLE_1:18;
then A5:Z c= dom ((f1+(exp_R)^2)^) by RFUNCT_1:def 8;
   dom ((f1+(exp_R)^2)^) c= dom (f1+(exp_R)^2) by RFUNCT_1:11;then
A6:Z c= dom (f1+(exp_R)^2) by XBOOLE_1:1,A5;
then A7:Z c= dom f1 /\ dom ((exp_R)^2) by VALUED_1:def 1;
then A8:Z c= dom f1 & Z c= dom ((exp_R)^2) by XBOOLE_1:18;
A9:Z c= dom ((exp_R)(#)(exp_R)) by XBOOLE_1:18,A7;
A10:exp_R is_differentiable_on Z by FDIFF_1:34,TAYLOR_1:16;
then A11:(-1)(#)(exp_R) is_differentiable_on Z by A4,FDIFF_1:28,X;
A12:(exp_R)(#)(exp_R) is_differentiable_on Z by A9,A10,FDIFF_1:29;
for x st x in Z holds f1.x=0*x+1 by A1;
then f1 is_differentiable_on Z by FDIFF_1:31,A8;
then A16:f1+(exp_R)^2 is_differentiable_on Z by A6,A12,FDIFF_1:26;
for x st x in Z holds (f1+(exp_R)^2).x<>0
   proof
   let x;
   assume x in Z;then
   x in dom (-exp_R) /\ (dom (f1+(exp_R)^2) \ (f1+(exp_R)^2)"{0})
     by RFUNCT_1:def 4,A1;then
   x in dom (f1+(exp_R)^2) \ (f1+(exp_R)^2)"{0} by XBOOLE_0:def 4; then
   x in dom ((f1+(exp_R)^2)^) by RFUNCT_1:def 8;
   hence thesis by RFUNCT_1:13;
   end;
then f is_differentiable_on Z by A1,A11,A16,FDIFF_2:21;
then f|Z is continuous by FDIFF_1:33;
then f|A is continuous by A1,FCONT_1:17;
then A21:f is_integrable_on A & f|A is bounded by A1,INTEGRA5:10,11;
A22:for x st x in Z holds exp_R.x < 1 by A1;then
A23:arccot*exp_R is_differentiable_on Z by A1,SIN_COS9:116;
B1:for x st x in Z holds f.x=-exp_R.x/(1+(exp_R.x)^2)
   proof
   let x;
   assume
B2:x in Z;
    then ((-exp_R)/(f1+(exp_R)^2)).x=((-exp_R).x)*((f1+(exp_R)^2).x)"
                                 by RFUNCT_1:def 4,A1
                              .=(-exp_R.x)*((f1+(exp_R)^2).x)"
                                by RFUNCT_1:74
                              .=(-exp_R.x)*(f1.x+((exp_R)^2).x)"
                                by VALUED_1:def 1,B2,A6
                              .=(-exp_R.x)*(f1.x+(exp_R.x)^2)"
                                by VALUED_1:11
                              .=(-exp_R.x)/(1+(exp_R.x)^2) by A1,B2
                              .=-exp_R.x/(1+(exp_R.x)^2);
        hence thesis by A1;
      end;
A24:for x st x in dom ((arccot*exp_R)`|Z) holds ((arccot*exp_R)`|Z).x=f.x
    proof
       let x;
       assume x in dom ((arccot*exp_R)`|Z);then
A25:x in Z by A23, FDIFF_1:def 8;
   then
((arccot*exp_R)`|Z).x=-exp_R.x/(1+(exp_R.x)^2) by A1,A22,SIN_COS9:116
                       .=f.x by A25,B1;
   hence thesis;
   end;
   dom ((arccot*exp_R)`|Z)=dom f by A1,A23,FDIFF_1:def 8;
   then ((arccot*exp_R)`|Z)= f by A24,PARTFUN1:34;
   hence thesis by A1,A21,A23,INTEGRA5:13;
end;
 
::f.x=exp_R.x*sin.x/cos.x+exp_R.x/(cos.x)^2

theorem
 A c= Z & Z = dom f & f=exp_R(#)(sin/cos)+exp_R/cos^2 implies
 integral(f,A)=(exp_R(#)tan).(upper_bound A)-(exp_R(#)tan).(lower_bound A)
 proof
 assume
A1:A c= Z & Z = dom f & f=exp_R(#)(sin/cos)+exp_R/cos^2;
then Z = dom (exp_R(#)(sin/cos)) /\ dom (exp_R/cos^2) by VALUED_1:def 1;
then
A4:Z c= dom (exp_R(#)(sin/cos)) & Z c= dom (exp_R/cos^2) by XBOOLE_1:18;
A5:dom (exp_R(#)(sin/cos)) c= dom exp_R /\ dom (sin/cos) by VALUED_1:def 4;
dom (exp_R/cos^2) c= dom exp_R /\ (dom cos^2 \ (cos^2)"{0})
   by RFUNCT_1:def 4;
   then dom (exp_R(#)(sin/cos)) c= dom exp_R &
   dom (exp_R(#)(sin/cos)) c= dom (sin/cos) &
   dom (exp_R/cos^2) c= dom cos^2 \ (cos^2)"{0} by XBOOLE_1:18,A5; then
A7:Z c= dom exp_R & Z c= dom (sin/cos) & Z c= dom cos^2 \ (cos^2)"{0}
   by XBOOLE_1:1,A4;
A9:exp_R is_differentiable_on Z by FDIFF_1:34,TAYLOR_1:16;
for x st x in Z holds sin/cos is_differentiable_in x
    proof
    let x;
    assume x in Z;then
    cos.x<>0 by A7,FDIFF_8:1;
    hence thesis by FDIFF_7:46;
    end;
then sin/cos is_differentiable_on Z by A7,FDIFF_1:16;
then A12:exp_R(#)(sin/cos) is_differentiable_on Z by A4,A9,FDIFF_1:29;
cos is_differentiable_on Z by FDIFF_1:34,SIN_COS:72;
then A14:cos^2 is_differentiable_on Z by FDIFF_2:20;
x in Z implies (cos^2).x<>0
    proof
    assume x in Z;then
    x in dom (exp_R/cos^2) by A4;
    then x in dom exp_R /\ (dom cos^2 \ (cos^2)"{0}) by RFUNCT_1:def 4;
    then x in dom cos^2 \ (cos^2)"{0} by XBOOLE_0:def 4;
    then x in dom ((cos^2)^) by RFUNCT_1:def 8;
      hence thesis by RFUNCT_1:13;
    end;
then exp_R/cos^2 is_differentiable_on Z by A9,A14,FDIFF_2:21;
    then f|Z is continuous by FDIFF_1:33,FDIFF_1:26,A1,A12;then
f|A is continuous by A1,FCONT_1:17;
then A19:f is_integrable_on A & f|A is bounded by A1,INTEGRA5:10,11;
A20:(exp_R(#)tan) is_differentiable_on Z by A4,FDIFF_8:30;
B1:for x st x in Z holds f.x=exp_R.x*sin.x/cos.x+exp_R.x/(cos.x)^2
   proof
   let x;
   assume
B2:x in Z;
  then (exp_R(#)(sin/cos)+exp_R/cos^2).x
  =((exp_R(#)(sin/cos)).x)+(exp_R/cos^2).x by VALUED_1:def 1,A1
 .=((exp_R.x)*((sin/cos).x))+(exp_R/cos^2).x by VALUED_1:5
 .=((exp_R.x)*(sin.x*(cos.x)"))+(exp_R/cos^2).x by RFUNCT_1:def 4,A7,B2
 .=(exp_R.x)*sin.x/cos.x+exp_R.x/(cos^2).x by RFUNCT_1:def 4,A4,B2
 .=exp_R.x*sin.x/cos.x+exp_R.x/(cos.x)^2 by VALUED_1:11;
   hence thesis by A1;
   end;
A21:for x st x in dom((exp_R(#)tan)`|Z) holds ((exp_R(#)tan)`|Z).x=f.x
   proof
     let x;
     assume x in dom((exp_R(#)tan)`|Z);then
A22: x in Z by A20,FDIFF_1:def 8; then
