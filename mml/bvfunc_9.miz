   ((a1 'imp' a2) '&' (b1 'imp' b2) '&' 'not'( a2 '&' b2)) '&'
   ((a1 'imp' a2) '&' (c1 'imp' c2) '&' 'not'( a2 '&' c2)) '&'
   ((b1 'imp' b2) '&' (c1 'imp' c2) '&' 'not'( b2 '&' c2)) 'imp'
   'not'( a1 '&' b1) '&' 'not'( a1 '&' c1) =I_el(Y) by A11,A12,BVFUNC_6:18;
