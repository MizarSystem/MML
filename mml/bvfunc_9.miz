   ((a1 'imp' a2) '&' (c1 'imp' c2) '&' 'not'( a2 '&' c2)) =I_el(Y)
  by A2,A4,BVFUNC_5:10;
A7:((a1 'imp' a2) '&' (b1 'imp' b2) '&' 'not'( a2 '&' b2)) '&'
   ((a1 'imp' a2) '&' (c1 'imp' c2) '&' 'not'( a2 '&' c2)) '&'
   ((b1 'imp' b2) '&' (c1 'imp' c2) '&' 'not'( b2 '&' c2)) 'imp'
   ((b1 'imp' b2) '&' (c1 'imp' c2) '&' 'not'( b2 '&' c2)) =I_el(Y)
