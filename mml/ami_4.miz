    end;
    suppose
A73:  i1 = i2;
A74:  i1 mod i2 = i19 mod i29
        .= 0 by A73,NAT_D:25;
A75:  t9 = Comput(ProgramPart(t),t,4*(0+1));
      t = Comput(ProgramPart(t),t,4*0) by AMI_1:13;
      then t9.b = t.a mod t.b by A4,A5,A70,A72,A71,A75,Lm5;
