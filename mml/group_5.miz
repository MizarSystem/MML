theorem
  ([.a,b",c.] |^ b) * ([.b,c",a.] |^ c) * ([.c,a",b.] |^ a) = 1_G
proof
  set d = a" * [.b,c".] * a;
  set e = c" * [.a,b".] * c;
  set f = b" * [.c,a".] * b;
  set x = [.a,b",c.] |^ b;
  set y = [.b,c",a.] |^ c;
  set z = [.c,a",b.] |^ a;
A1: [.c",b.] = c"" * (b" * c" * b) by Th19;
A2: [.a",c.] = a"" * (c" * a" * c) by Th19;
A3: [.b",a.] = b"" * (a" * b" * a) by Th19;
  [.a,b",c.] = [.a,b".]" * e by Th19
    .= [.b",a.] * e by Th25;
  then
A4: [.a,b",c.] |^ b = b" * (b"" * ((a" * b" * a) * e)) * b by
  A3, GROUP_1
  :def 4
    .= (a" * b" * a) * e * b by GROUP_3:1;
  [.c,a",b.] = [.c,a".]" * f by Th19
    .= [.a",c.] * f by Th25;
  then
A5: z = a" * (a"" * ((c" * a" * c) * f)) * a by A2,GROUP_1:def 4
    .= (c" * a" * c) * f * a by GROUP_3:1;
  [.b,c",a.] = [.b,c".]" * (a" * [.b,c".] * a) by Th19
    .= [.c",b.] * (a" * [.b,c".] * a) by Th25;
  then [.b,c",a.] |^ c = c" * (c"" * ((b" * c" * b) * d)) * c by
  A1, GROUP_1
  :def 4
    .= (b" * c" * b) * d * c by GROUP_3:1
    .= (b" * c" * b) * (d * c) by GROUP_1:def 4
    .= b" * (c" * b) * (d * c) by GROUP_1:def 4
    .= b" * ((c" * b) * (d * c)) by GROUP_1:def 4;
  then x * y = (a" * b" * a) * e * (b * (b" * ((c" * b) * (d * c)))) by
  A4,
  GROUP_1:def 4
    .= (a" * b" * a) * e * ((c" * b) * (d * c)) by GROUP_3:1
    .= (a" * b" * a) * (c" * ([.a,b".] * c)) * ((c" * b) * (d * c)) by
  GROUP_1:def 4
    .= (a" * b" * a) * c" * ([.a,b".] * c) * ((c" * b) * (d * c)) by GROUP_1
  :def 4
    .= (a" * b" * a) * c" * (([.a,b".] * c) * ((c" * b) * (d * c))) by
  GROUP_1:def 4
    .= (a" * b" * a) * c" * (([.a,b".] * c) * (c" * b) * (d * c)) by GROUP_1
  :def 4
    .= (a" * b" * a) * c" * (([.a,b".] * c) * c" * b * (d * c)) by GROUP_1:
  def 4
