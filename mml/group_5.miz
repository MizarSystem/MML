    .= a" * c" * (c * a) * (b" * ([.a,c.] * b)) * [.b,c.] by GROUP_1:def 4
    .= (c * a)" * (c * a) * (b" * ([.a,c.] * b)) * [.b,c.] by GROUP_1:25
    .= 1_G * (b" * ([.a,c.] * b)) * [.b,c.] by GROUP_1:def 6
    .= b" * ([.a,c.] * b) * [.b,c.] by GROUP_1:def 5
    .= b" * ((a" * c" * a * c) * b) * ((b" * c") * (b * c)) by Th19
    .= b" * ((a" * c" * a) * c) * b * ((b" * c") * (b * c)) by GROUP_1:def 4
    .= b" * (a" * c" * (a * c)) * b * ((b" * c") * (b * c)) by GROUP_1:def 4
    .= b" * (a" * (c" * (a * c))) * b * ((b" * c") * (b * c)) by GROUP_1:def
  4
    .= b" * a" * (c" * (a * c)) * b * ((b" * c") * (b * c)) by GROUP_1:def 4
    .= b" * a" * c" * (a * c) * b * ((b" * c") * (b * c)) by GROUP_1:def 4
    .= b" * a" * c" * a * c * b * ((b" * c") * (b * c)) by GROUP_1:def 4
