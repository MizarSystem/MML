    .= (a" * (b" * (a * b))) * a" * [.b,a.] * a by Th19
    .= (a" * ((b" * (a"" * b))) * a") * [.b,a.] * a
    .= (a" * (((b" * (a"" * b))) * a")) * [.b,a.] * a by GROUP_1:def 4
    .= a" * [.b,a".] * [.b,a.] * a by Th19
    .= ([.b,a".] * [.b,a.]) |^ a by GROUP_1:def 4;
