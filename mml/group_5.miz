theorem
    ([.a,b",c.] |^ b) * ([.b,c",a.] |^ c) * ([.c,a",b.] |^ a) = 1.G
  proof set d = a" * [.b,c".] * a; set e = c" * [.a,b".] * c;
        set f = b" * [.c,a".] * b;
        set x = [.a,b",c.] |^ b; set y = [.b,c",a.] |^ c;
        set z = [.c,a",b.] |^ a;
    A1: [.b,c",a.] = [.[.b,c".],a.] by Def3
                 .= [.b,c".]" * (a" * [.b,c".] * a) by Th19
                 .= [.c",b.] * (a" * [.b,c".] * a) by Th25;
    A2: [.c",b.] = c"" * (b" * c" * b) by Th19;
    A3: [.b,c",a.] |^ c = c" * [.b,c",a.] * c by GROUP_3:20
                     .= c" * (c"" * ((b" * c" * b) * d)) * c by A1,A2,
GROUP_1:def 4
                     .= (b" * c" * b) * d * c by GROUP_3:1
                     .= (b" * c" * b) * (d * c) by GROUP_1:def 4
                     .= b" * (c" * b) * (d * c) by GROUP_1:def 4
                     .= b" * ((c" * b) * (d * c)) by GROUP_1:def 4;
    A4: [.c,a",b.] = [.[.c,a".],b.] by Def3
                 .= [.c,a".]" * f by Th19
                 .= [.a",c.] * f by Th25;
    A5: [.a",c.] = a"" * (c" * a" * c) by Th19;
    A6: [.a,b",c.] = [.[.a,b".],c.] by Def3
                 .= [.a,b".]" * e by Th19
                 .= [.b",a.] * e by Th25;
    A7: [.b",a.] = b"" * (a" * b" * a) by Th19;
    A8: z = a" * [.c,a",b.] * a by GROUP_3:20
        .= a" * (a"" * ((c" * a" * c) * f)) * a by A4,A5,GROUP_1:def 4
        .= (c" * a" * c) * f * a by GROUP_3:1;
       [.a,b",c.] |^ b = b" * [.a,b",c.] * b by GROUP_3:20
                  .= b" * (b"" * ((a" * b" * a) * e)) * b by A6,A7,
GROUP_1:def 4
                  .= (a" * b" * a) * e * b by GROUP_3:1;
    then x * y = (a" * b" * a) * e * (b * (b" * ((c" * b) * (d * c))))
                                                    by A3,GROUP_1:def 4
      .= (a" * b" * a) * e * ((c" * b) * (d * c)) by GROUP_3:1
      .= (a" * b" * a) * (c" * ([.a,b".] * c)) * ((c" * b) * (d * c))
                                                      by GROUP_1:def 4
      .= (a" * b" * a) * c" * ([.a,b".] * c) * ((c" * b) * (d * c))
                                                      by GROUP_1:def 4
      .= (a" * b" * a) * c" * (([.a,b".] * c) * ((c" * b) * (d * c)))
                                                      by GROUP_1:def 4
      .= (a" * b" * a) * c" * (([.a,b".] * c) * (c" * b) * (d * c))
                                                      by GROUP_1:def 4
      .= (a" * b" * a) * c" * (([.a,b".] * c) * c" * b * (d * c)) by
GROUP_1:def 4
      .= (a" * b" * a) * c" * ([.a,b".] * b * (d * c)) by GROUP_3:1
      .= (a" * b" * a) * c" * ((a" * b"" * a) * b" * b * (d * c)) by Th19
      .= (a" * b" * a) * c" * ((a" * b"" * a) * (d * c)) by GROUP_3:1
      .= (a" * b" * a) * c" * ((a" * b"" * a) * (a" * [.b,c".] * (a * c)))
                                                      by GROUP_1:def 4
      .= (a" * b" * a) * c" * ((a" * b"" * a) * (a" * ([.b,c".] * (a * c))))
                                                       by GROUP_1:def 4
      .= (a" * b" * a) * c" * ((a" * b"" * a) * a" * ([.b,c".] * (a * c)))
                                                       by GROUP_1:def 4
      .= (a" * b" * a) * c" * ((a" * b"") * ([.b,c".] * (a * c))) by GROUP_3:1
      .= (a" * b" * a) * c" * ((a" * b"") * [.b,c".] * (a * c)) by
GROUP_1:def 4
      .= (a" * b" * a) * c" * ((a" * b"") * (b" * (c"" * b * c")) * (a * c))
                                                                       by Th19
      .= (a" * b" * a) * c" * ((a" * b"") * ((b" * (c"" * b * c")) * (a * c)))
                                                                by
GROUP_1:def 4
      .= (a" * b" * a) * c" * ((a" * b"") * (b" * ((c"" * b * c") * (a * c))))
                                                                  by
GROUP_1:def 4
      .= (a" * b" * a) * c" * ((a" * b"") * b" * ((c"" * b * c") * (a * c)))
                                                                  by
GROUP_1:def 4
      .= (a" * b" * a) * c" * (a" * ((c"" * b * c") * (a * c))) by GROUP_3:1;
    then x * y * z =
        ((a" * b" * a) * c" * (a" * (c"" * b * c") * (a * c))) *
        ((c" * a" * c) * f * a) by A8,GROUP_1:def 4
     .= ((a" * b" * a) * (c" * (a" * (c"" * b * c") * (a * c)))) *
        ((c" * a" * c) * f * a) by GROUP_1:def 4
     .= ((a" * b" * a) * (c" * (a" * ((c"" * b * c") * (a * c))))) *
        ((c" * a" * c) * f * a) by GROUP_1:def 4
     .= ((a" * b" * a) * c" * (a" * ((c"" * b * c") * (a * c)))) *
        ((c" * a" * c) * f * a) by GROUP_1:def 4
     .= ((a" * b" * a) * c" * (a" * (c"" * (b * c") * (a * c)))) *
        ((c" * a" * c) * f * a) by GROUP_1:def 4
     .= ((a" * b" * a) * c" * (a" * (c"" * (b * c" * (a * c))))) *
        ((c" * a" * c) * f * a) by GROUP_1:def 4
     .= ((a" * b" * a) * c" * a" * (c"" * (b * c" * (a * c)))) *
        ((c" * a" * c) * f * a) by GROUP_1:def 4
     .= ((a" * b" * a) * c" * a" * c"" * (b * c" * (a * c))) *
        (c" * a" * c * f * a) by GROUP_1:def 4
     .= ((a" * b" * a) * c" * a" * c"" * (b * (c" * (a * c)))) *
        (c" * a" * c * f * a) by GROUP_1:def 4
     .= ((a" * b" * a) * c" * a" * c"" * b * (c" * (a * c))) *
        (c" * a" * c * f * a) by GROUP_1:def 4
     .= (((a" * b" * a) * c" * a" * c"") * b * c" * (a * c)) *
        (c" * a" * c * f * a) by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"" * b * c") * ((a * c) *
        (c" * a" * c * f * a)) by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"" * b * c") * ((a * c) *
        (c" * a" * c * (f * a))) by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"" * b * c") * ((a * c) *
        (c" * a" * (c * (f * a)))) by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"" * b * c") * ((a * c) *
        (c" * a") * (c * (f * a))) by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"" * b * c") * ((a * c) *
        (a * c)" * (c * (f * a))) by GROUP_1:25
     .= (((a" * b") * a * c") * a" * c"" * b * c") * (1.G *
        (c * (f * a))) by GROUP_1:def 6
     .= (((a" * b") * a * c") * a" * c"" * b * c") * ((c * (f * a)))
     by GROUP_1:def 5
     .= (((a" * b") * a * c") * a" * c"" * b * c") * c * (f * a)
     by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"" * b) * (f * a) by GROUP_3:1
     .= (((a" * b") * a * c") * a" * c"" * b) * (b" * [.c,a".] * (b * a))
     by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"" * b) * (b" * ([.c,a".] * (b * a)))
     by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"" * b) * b" * ([.c,a".] * (b * a))
     by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"") * ([.c,a".] * (b * a)) by GROUP_3:1
     .= (((a" * b") * a * c") * a" * c"") * (c" * (a"" * c * a") * (b * a))
     by Th19
     .= (((a" * b") * a * c") * a" * c"") * (c" * ((a"" * c * a") * (b * a)))
     by GROUP_1:def 4
     .= (((a" * b") * a * c") * a" * c"") * c" * ((a"" * c * a") * (b * a))
     by GROUP_1:def 4
     .= (((a" * b") * a * c") * a") * ((a"" * c * a") * (b * a)) by GROUP_3:1
     .= (((a" * b") * a * c") * a") * (a"" * (c * a") * (b * a))
     by GROUP_1:def 4
     .= (((a" * b") * a * c") * a") * (a"" * ((c * a") * (b * a)))
     by GROUP_1:def 4
     .= (((a" * b") * a * c") * a") * a"" * ((c * a") * (b * a))
     by GROUP_1:def 4
     .= ((a" * b") * a * c") * ((c * a") * (b * a)) by GROUP_3:1
     .= ((a" * b") * a * c") * (c * a") * (b * a) by GROUP_1:def 4
     .= (a" * b") * (a * c") * (c * a") * (b * a) by GROUP_1:def 4
     .= (a" * b") * (a * c") * (c"" * a") * (b * a) by GROUP_1:19
     .= (a" * b") * (a * c") * (a * c")" * (b * a) by GROUP_1:25
     .= (a" * b") * ((a * c") * (a * c")") * (b * a) by GROUP_1:def 4
     .= (a" * b") * 1.G * (b * a) by GROUP_1:def 6
     .= (a" * b") * (b * a) by GROUP_1:def 5
     .= (b * a)" * (b * a) by GROUP_1:25;
   hence thesis by GROUP_1:def 6;
  end;

definition let G,A,B;
 func commutators(A,B) -> Subset of G equals
  :Def4:  {[.a,b.] : a in A & b in B};
 coherence
  proof
    deffunc F(Element of G,Element of G) =
      [.$1,$2.];
    defpred P[Element of G,Element of G] means
      $1 in A & $2 in B;
     {F(a,b) : P[a,b]} is Subset of G from DOMAIN_1:sch 9;
   hence thesis;
  end;
end;

