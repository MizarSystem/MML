      XXREAL_0, XREAL_0, RAT_1, INT_1, ORDINAL1;
 constructors ENUMSET1, SETFAM_1, XCMPLX_0, XXREAL_0, RAT_1;
 registrations XREAL_0, INT_1, RAT_1, ORDINAL1, XCMPLX_0, NUMBERS, NAT_1;
