definition
  let X be complex-membered set, Y be set;
  redefine pred X c= Y means
  :Def7:
  c in X implies c in Y;
  compatibility
  proof
    thus X c= Y implies for c st c in X holds c in Y;
    assume
A1: c in X implies c in Y;
    let x;
    assume x in X;
    hence thesis by A1;
  end;
end;

definition
  let X be ext-real-membered set, Y be set;
  redefine pred X c= Y means
  :Def8:
  e in X implies e in Y;
  compatibility
  proof
    thus X c= Y implies for e st e in X holds e in Y;
    assume
