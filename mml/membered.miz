theorem Th1:
  X is complex-membered implies X c= COMPLEX
proof
  assume
A1: X is complex-membered;
  let x;
  assume x in X;
  then x is complex by A1,Def1;
  hence thesis by XCMPLX_0:def 2;
end;

theorem Th2:
  X is ext-real-membered implies X c= ExtREAL
proof
  assume
A1: X is ext-real-membered;
  let x;
  assume x in X;
  then x is ext-real by A1,Def2;
  hence thesis by XXREAL_0:def 1;
end;

theorem Th3:
  X is real-membered implies X c= REAL
proof
