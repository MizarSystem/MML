theorem Th20:
 for Y being natural-membered set st X c= Y
  holds X is natural-membered
proof let Y be natural-membered set such that
A1: X c= Y;
 let x;
 thus thesis by A1,Def5;
end;

registration
 cluster {} -> natural-membered;
 coherence proof let x; thus thesis; end;
end;

