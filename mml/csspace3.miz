begin :: Complex_l1_Space:The Space of Absolute Summable Complex Sequences

definition
func the_set_of_l1ComplexSequences -> Subset of
 Linear_Space_of_ComplexSequences means :Def1:
  for x being set holds x in it
     iff
  (x in the_set_of_ComplexSequences & seq_id(x) is absolutely_summable);
existence
proof
   defpred P[set] means seq_id($1) is absolutely_summable;
   consider IT being set such that
A1:for x being set holds x in IT iff
    x in the_set_of_ComplexSequences & P[x] from XBOOLE_0:sch 1;
    IT is Subset of the_set_of_ComplexSequences
   proof
      for x be set st x in IT holds x in the_set_of_ComplexSequences by A1;
     hence thesis by TARSKI:def 3;
   end;
   hence thesis by A1,CSSPACE:def 7;
end;
uniqueness
proof
   let X1,X2 be Subset of Linear_Space_of_ComplexSequences;
   assume that
A2:for x being set holds x in X1 iff
    (x in the_set_of_ComplexSequences & seq_id(x) is absolutely_summable)
   and
A3:for x being set holds x in X2 iff
    (x in the_set_of_ComplexSequences & seq_id(x) is absolutely_summable);
    for x being set st x in X1 holds x in X2
   proof
     let x be set;
     assume x in X1;
     then x in the_set_of_ComplexSequences & seq_id(x) is absolutely_summable
by A2;
     hence thesis by A3;
   end;
then A4:X1 c= X2 by TARSKI:def 3;
    for x being set st x in X2 holds x in X1
   proof
     let x be set;
     assume x in X2;
     then x in the_set_of_ComplexSequences & seq_id(x) is absolutely_summable
by A3;
     hence thesis by A2;
   end;
   then X2 c= X1 by TARSKI:def 3;
   hence thesis by A4,XBOOLE_0:def 10;
end;
end;

theorem Th1:
for c be Complex, seq be Complex_Sequence, rseq be Real_Sequence st
 seq is convergent & ( for i be Nat holds rseq.i = |.(seq.i-c).| ) holds
  ( rseq is convergent & lim rseq = |.(lim seq-c).| )
proof
   let c be Complex;
   let seq be Complex_Sequence;
   let rseq be Real_Sequence;
   assume that
A1:seq is convergent and
A2:for i be Nat holds rseq.i = |.(seq.i-c).|;
   deffunc F(set) = c;
   consider cseq be Complex_Sequence such that
A3:for n be Nat holds cseq.n=F(n) from COMSEQ_1:sch 1;
    now let i be Nat;
     thus rseq.i=|.(seq.i-c).| by A2
        .=|.(seq.i-(cseq.i)).|by A3
        .=|.(seq.i+-(cseq.i)).|
        .=|.(seq.i+(-cseq).i).| by COMSEQ_1:def 9
        .=|.((seq+(-cseq)).i).| by COMSEQ_1:def 4
        .=|.(seq -cseq).i .| by COMSEQ_1:def 10
        .=|. seq-cseq .| .i by COMSEQ_1:def 14;
   end;
then A4:for x be set st x in NAT holds rseq.x = |.(seq -cseq).| .x;
A5:cseq is convergent by A3,COMSEQ_2:9;
then A6:seq -cseq is convergent by A1,COMSEQ_2:25;
   reconsider seq' = seq-cseq as convergent Complex_Sequence
     by A1,A5,COMSEQ_2:25;
A7:|.seq'.| is convergent;
    lim |.(seq -cseq).| = |.(lim(seq-cseq)).| by A6,COMSEQ_2:11
    .=|.(lim seq-lim cseq).| by A1,A5,COMSEQ_2:26
    .=|.(lim seq-c).| by A3,COMSEQ_2:10;
   hence thesis by A4,A7,SEQ_1:8;
end;

registration
cluster the_set_of_l1ComplexSequences -> non empty;
coherence
proof
    seq_id CZeroseq is absolutely_summable
   proof
     reconsider cseq=seq_id CZeroseq as Complex_Sequence;
      for n being Nat holds cseq.n = 0c by CSSPACE:def 6;
     hence thesis by COMSEQ_3:52;
   end;
   hence thesis by Def1;
end;
end;

registration
cluster the_set_of_l1ComplexSequences -> lineary-closed;
coherence
proof
   set W = the_set_of_l1ComplexSequences;
A1:for v,u be VECTOR of Linear_Space_of_ComplexSequences st
    v in the_set_of_l1ComplexSequences &
    u in the_set_of_l1ComplexSequences holds
     v + u in the_set_of_l1ComplexSequences
   proof
     let v,u be VECTOR of Linear_Space_of_ComplexSequences such that
A2:  v in W & u in W;
      seq_id(v+u) is absolutely_summable
     proof
A3:    seq_id v is absolutely_summable by A2,Def1;
A4:    seq_id u is absolutely_summable by A2,Def1;
       set p = |.seq_id v.|;
       set q = |.seq_id u.|;
       set r = |.seq_id(v+u).|;
A5:    p is summable by A3,COMSEQ_3:def 11;
        q is summable by A4,COMSEQ_3:def 11;
then A6:    p+q is summable by A5,SERIES_1:10;
A7:    for n be Nat holds 0<=r.n
       proof
         let n be Nat;
          r.n=|.((seq_id(v+u)).n).| by COMSEQ_1:def 14;
         hence thesis by COMPLEX1:132;
       end;
        for n be Nat holds r.n <=(p+q).n
       proof
         let n be Nat;
A8:      r.n=|.((seq_id(v+u)).n).| by COMSEQ_1:def 14
           .=|.((seq_id(seq_id(v) + seq_id(u))).n).| by CSSPACE:4,def 7
           .=|.((seq_id v + seq_id u).n).| by CSSPACE:3
           .=|.((seq_id v).n + (seq_id u).n).| by COMSEQ_1:def 4;
          |.((seq_id v).n).|+|.((seq_id u).n).|
            = |.(seq_id v).| .n + |.((seq_id u).n).| by COMSEQ_1:def 14
           .= |.(seq_id v).| .n+ |.(seq_id u).| .n by COMSEQ_1:def 14
           .= (p + q).n by SEQ_1:11;
         hence r.n <= (p+q).n by A8,COMPLEX1:142;
       end;
       then r is summable by A6,A7,SERIES_1:24;
       hence thesis by COMSEQ_3:def 11;
     end;
     hence v+u in W by Def1,CSSPACE:def 7;
   end;

    for a be Complex, v be VECTOR of Linear_Space_of_ComplexSequences
    st v in W holds a * v in W
   proof
     let a be Complex;
     let v be VECTOR of Linear_Space_of_ComplexSequences such that
A9:  v in W;
      seq_id(a*v) is absolutely_summable
     proof
        seq_id v is absolutely_summable by A9,Def1;
then A10:   |. seq_id v .| is summable by COMSEQ_3:def 11;
       set q1 = |.seq_id(a*v).|;
       set r1 = ((|.a.|)(#)|.(seq_id v).|);
A11:   r1 is summable by A10,SERIES_1:13;
A12:   for n be Nat holds 0<=q1.n
       proof
         let n be Nat;
          q1.n=|.((seq_id(a*v)).n).| by COMSEQ_1:def 14;
         hence thesis by COMPLEX1:132;
       end;
        for n be Nat holds q1.n <=r1.n
       proof
         let n be Nat;
          q1.n=|.((seq_id(a*v)).n).| by COMSEQ_1:def 14
            .=|.((seq_id(a(#)seq_id v)).n).| by CSSPACE:5,def 7
            .=|.((a(#)seq_id v).n).| by CSSPACE:3
            .=|.(a(#)seq_id v).| .n by COMSEQ_1:def 14;
         hence thesis by COMSEQ_1:53;
       end;
       then q1 is summable by A11,A12,SERIES_1:24;
       hence thesis by COMSEQ_3:def 11;
     end;
     hence a*v in W by Def1,CSSPACE:def 7;
   end;
   hence thesis by A1,CLVECT_1:def 3;
end;
end;

canceled;
 Lm1:
CLSStruct (# the_set_of_l1ComplexSequences,
      Zero_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
      Add_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
      Mult_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences) #)
is Subspace of Linear_Space_of_ComplexSequences by CSSPACE:13;

registration
 cluster CLSStruct (# the_set_of_l1ComplexSequences,
      Zero_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
      Add_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
      Mult_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences) #)
  -> Abelian add-associative right_zeroed right_complementable
        ComplexLinearSpace-like;
coherence by CSSPACE:13;
end;

canceled;
 Lm2:
CLSStruct (# the_set_of_l1ComplexSequences,
      Zero_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
      Add_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
      Mult_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences) #)
is ComplexLinearSpace;

Lm3:
  ex NORM be Function of the_set_of_l1ComplexSequences,REAL st
   for x be set st x in the_set_of_l1ComplexSequences holds
    NORM.x = Sum |.seq_id x.|
proof
   deffunc F(set) = Sum |.seq_id $1.|;
A1:for z be set st z in the_set_of_l1ComplexSequences holds F(z) in REAL;
    ex f being Function of the_set_of_l1ComplexSequences,REAL st
     for x being set st x in the_set_of_l1ComplexSequences holds
       f.x = F(x) from FUNCT_2:sch 2(A1);
   hence thesis;
end;

definition
  func cl_norm -> Function of the_set_of_l1ComplexSequences, REAL means
  :Def2:
  for x be set st x in the_set_of_l1ComplexSequences holds
    it.x = Sum |.seq_id x.|;
existence by Lm3;
uniqueness
proof
   let NORM1,NORM2 be Function of the_set_of_l1ComplexSequences, REAL such that
A1:(for x be set st x in the_set_of_l1ComplexSequences holds
     NORM1.x = Sum |.seq_id x.|) and
A2:(for x be set st x in the_set_of_l1ComplexSequences holds
     NORM2.x = Sum |.seq_id x.|);
A3:dom NORM1 = the_set_of_l1ComplexSequences &
     dom NORM2 = the_set_of_l1ComplexSequences by FUNCT_2:def 1;
    for z be set st z in the_set_of_l1ComplexSequences holds NORM1.z = NORM2.z
   proof
     let z be set such that
A4:  z in the_set_of_l1ComplexSequences;
      NORM1.z = Sum |.seq_id z.| by A1,A4;
     hence thesis by A2,A4;
  end;
  hence thesis by A3,FUNCT_1:9;
end;
end;

registration let X be non empty set,
               Z be Element of X, A be BinOp of X,
               M be Function of [:COMPLEX, X:], X,
               N be Function of X, REAL;
  cluster CNORMSTR (# X, Z, A, M, N #) -> non empty;
  coherence by STRUCT_0:def 1;
end;

theorem Th4:
for l be CNORMSTR st CLSStruct (# the carrier of l, the Zero of l,
 the add of l, the Mult of l #)
  is ComplexLinearSpace holds l is ComplexLinearSpace
proof
   let l be CNORMSTR such that
A1:CLSStruct(# the carrier of l, the Zero of l, the add  of l, the Mult of l#)
    is ComplexLinearSpace;
    the carrier of l is non empty by A1,STRUCT_0:def 1;
   then reconsider l as non empty CLSStruct by STRUCT_0:def 1;
   reconsider l0=CLSStruct (# the carrier of l, the Zero of l, the add  of l,
        the Mult of l#) as ComplexLinearSpace by A1;
A2:for v,w being VECTOR of l holds v + w = w + v
   proof
     let v,w be VECTOR of l;
     reconsider v1=v, w1=w as VECTOR of l0;
    thus v+w = (the add of l).[v1,w1] by RLVECT_1:def 3
       .=v1+w1 by RLVECT_1:def 3
       .= (the add of l).[w1,v1] by RLVECT_1:def 3
       .= w +v by RLVECT_1:def 3;
   end;
A3:for u,v,w being VECTOR of l holds (u + v) + w = u + (v + w)
   proof
     let u,v,w be VECTOR of l;
     reconsider u1=u, v1=v, w1=w as VECTOR of l0;
     thus (u + v) + w = (the add of l).[(u+v),w] by RLVECT_1:def 3
       .= (the add of l).[(the add of l).[u,v],w] by RLVECT_1:def 3
       .= (the add of l).[(u1+v1),w] by RLVECT_1:def 3
       .=(u1+v1)+w1 by RLVECT_1:def 3
       .=u1+(v1+w1) by RLVECT_1:def 6
       .= (the add of l).[u1,(v1+w1)] by RLVECT_1:def 3
       .= (the add of l).[u1,(the add of l).[v1,w1]] by RLVECT_1:def 3
       .= (the add of l).[u,v+w] by RLVECT_1:def 3
       .= u+(v+w) by RLVECT_1:def 3;
   end;
A4:for v being VECTOR of l holds v + 0.l = v
   proof
     let v be VECTOR of l;
     reconsider v1=v as VECTOR of l0;
A5:  0.l=the Zero of l by RLVECT_1:def 2
       .=0.l0 by RLVECT_1:def 2;
      v+0.l= (the add of l).[v,0.l] by RLVECT_1:def 3
         .= v1 + 0.l0 by A5,RLVECT_1:def 3
         .= v1 by RLVECT_1:def 7;
     hence thesis;
   end;

A6:for v being VECTOR of l ex w being VECTOR of l st v + w = 0.l
   proof
     let v be VECTOR of l;
     reconsider v1=v as VECTOR of l0;
A7:  0.l=the Zero of l by RLVECT_1:def 2
       .=0.l0 by RLVECT_1:def 2;
     consider w1 being VECTOR of l0 such that
A8:  v1 + w1 = 0.l0 by RLVECT_1:def 8;
     reconsider w = w1 as VECTOR of l;
A9:  v+w = (the add of l).[v,w] by RLVECT_1:def 3
        .=0.l by A7,A8,RLVECT_1:def 3;
     take w;
     thus thesis by A9;
   end;

A10:for a be Complex for v,w being VECTOR of l holds
         a * (v + w) = a * v + a * w
   proof
     let a be Complex;
     let v,w be VECTOR of l;
     reconsider v1=v, w1=w as VECTOR of l0;
     thus a*(v+w)
        = (the Mult of l).[a,v+w] by CLVECT_1:def 1
       .= (the Mult of l).[a,(the add of l).[v1,w1]] by RLVECT_1:def 3
       .= (the Mult of l).[a,(v1+w1)] by RLVECT_1:def 3
       .=a*(v1+w1) by CLVECT_1:def 1
       .=a*v1+a*w1 by CLVECT_1:def 2
       .=(the add of l).[a*v1,a*w1] by RLVECT_1:def 3
       .=(the add of l).[(the Mult of l).[a,v1],a*w1] by CLVECT_1:def 1
       .=(the add of l).[(the Mult of l).[a,v1],
                (the Mult of l).[a,w1]] by CLVECT_1:def 1
       .=(the add of l).[a*v, (the Mult of l).[a,w]] by CLVECT_1:def 1
       .=(the add of l).[a*v, a*w] by CLVECT_1:def 1
       .= a*v +a*w by RLVECT_1:def 3;
   end;

A11:for a,b be Complex for v being VECTOR of l
     holds (a + b) * v = a * v + b * v
   proof
     let a,b be Complex;
     let v be VECTOR of l;
     reconsider v1=v as VECTOR of l0;
     thus (a+b)*v = (the Mult of l).[(a+b),v] by CLVECT_1:def 1
       .=(a+b)*v1 by CLVECT_1:def 1
       .=a*v1+b*v1 by CLVECT_1:def 2
       .=(the add of l).[a*v1,b*v1] by RLVECT_1:def 3
       .=(the add of l).[(the Mult of l).[a,v1],b*v1] by CLVECT_1:def 1
       .=(the add of l).[(the Mult of l).[a,v1],
                (the Mult of l).[b,v1]] by CLVECT_1:def 1
       .=(the add of l).[a*v, (the Mult of l).[b,v]] by CLVECT_1:def 1
       .=(the add of l).[a*v, b*v] by CLVECT_1:def 1
       .= a*v +b*v by RLVECT_1:def 3;
   end;

A12:for a,b be Complex for v being VECTOR of l holds (a * b) * v = a * (b * v)
   proof
     let a,b be Complex;
     let v be VECTOR of l;
     reconsider v1=v as VECTOR of l0;
     thus (a*b)*v = (the Mult of l).[a*b,v] by CLVECT_1:def 1
       .=(a*b)*v1 by CLVECT_1:def 1
       .=a*(b*v1) by CLVECT_1:def 2
       .=(the Mult of l).[a,b*v1] by CLVECT_1:def 1
       .=(the Mult of l).[a,(the Mult of l).[b,v1]] by CLVECT_1:def 1
       .=(the Mult of l).[a,b*v] by CLVECT_1:def 1
       .= a*(b*v) by CLVECT_1:def 1;
   end;
    for v being VECTOR of l holds 1r * v = v
   proof
     let v be VECTOR of l;
     reconsider v1=v as VECTOR of l0;
     thus 1r*v= (the Mult of l).[1r,v] by CLVECT_1:def 1
            .= 1r*v1 by CLVECT_1:def 1
            .= v by CLVECT_1:def 2;
   end;
   hence thesis by A2,A3,A4,A6,A10,A11,A12,CLVECT_1:1;
end;

theorem Th5:
for cseq be Complex_Sequence st (for n be Nat holds cseq.n=0c) holds
 cseq is absolutely_summable & Sum |.cseq.| = 0
proof
   let cseq be Complex_Sequence such that
A1:for n be Nat holds cseq.n=0c;
A2:for n be Nat holds (|.cseq.|).n=0
   proof
     let n be Nat;
      cseq.n=0c by A1;
     hence thesis by COMPLEX1:130,COMSEQ_1:def 14;
   end;
A3:for m be Nat holds Partial_Sums (|.cseq.|).m = 0
   proof
     let m be Nat;
     defpred P[Nat] means (|.cseq.|).$1 = (Partial_Sums |.cseq.|).$1;
A4:  P[0] by SERIES_1:def 1;
A5:  for k be Nat st P[k] holds P[k+1]
     proof
       let k be Nat such that
A6:    (|.cseq.|).k = (Partial_Sums (|.cseq.|)).k;
       thus (|.cseq.|).(k+1) = 0 + (|.cseq.|).(k+1)
         .= (|.cseq.|).k + (|.cseq.|).(k+1) by A2
         .= (Partial_Sums |.cseq.|).(k+1) by A6,SERIES_1:def 1;
     end;
      for n be Nat holds P[n] from NAT_1:sch 1(A4,A5);
     hence (Partial_Sums |.cseq.|).m = (|.cseq.|).m .= 0 by A2;
   end;

    Sum(|.cseq.|) =0 & cseq is absolutely_summable
   proof
A7:  for p be real number st 0<p
      ex n be Nat st for m be Nat st n<=m holds
       abs((Partial_Sums |.cseq.|).m-0)<p
     proof
       let p be real number such that
A8:    0<p;
       take 0;
       let m be Nat such that 0<=m;
        abs((Partial_Sums |.cseq.|).m-0)
          = abs(0-0) by A3
         .= 0 by ABSVALUE:def 1;
       hence abs((Partial_Sums |.cseq.|).m-0)<p by A8;
     end;
then A9: Partial_Sums |.cseq.| is convergent by SEQ_2:def 6;
then A10: |.cseq.| is summable by SERIES_1:def 2;
      lim Partial_Sums |.cseq.| =0 by A7,A9,SEQ_2:def 7;
     hence thesis by A10,COMSEQ_3:def 11,SERIES_1:def 3;
   end;
   hence thesis;
end;

theorem Th6:
for cseq be Complex_Sequence st
 ( cseq is absolutely_summable & Sum |.cseq.| = 0 ) holds
  for n be Nat holds cseq.n = 0c
proof
   let cseq being Complex_Sequence such that
A1:cseq is absolutely_summable and
A2:Sum |.cseq.| = 0;
   reconsider rseq = |.cseq.| as Real_Sequence;
A3:rseq is summable by A1,COMSEQ_3:def 11;
A4:for n be Nat holds 0 <= (|.cseq.|).n
   proof
     let n be Nat;
      0 <= |.(cseq.n).| by COMPLEX1:132;
     hence thesis by COMSEQ_1:def 14;
   end;
    for n be Nat holds cseq.n = 0c
   proof
     let n be Nat;
      (|.cseq.|).n =0 by A2,A3,A4,RSSPACE:21;
     then |.(cseq.n).| = 0 by COMSEQ_1:def 14;
     hence thesis by COMPLEX1:131;
   end;
   hence thesis;
end;

theorem Th7:
CNORMSTR (# the_set_of_l1ComplexSequences,
    Zero_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
    Add_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
    Mult_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
    cl_norm #) is ComplexLinearSpace by Lm2,Th4;

definition
func Complex_l1_Space -> non empty CNORMSTR equals :Def3:
  CNORMSTR (# the_set_of_l1ComplexSequences,
      Zero_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
      Add_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
      Mult_(the_set_of_l1ComplexSequences,Linear_Space_of_ComplexSequences),
      cl_norm #);
coherence;
end;

begin :: Complex_l1_Space is Banach

theorem Th8:
the carrier of Complex_l1_Space = the_set_of_l1ComplexSequences &
( for x be set holds
   x is VECTOR of Complex_l1_Space
   iff x is Complex_Sequence & seq_id x is absolutely_summable ) &
0.Complex_l1_Space = CZeroseq &
( for u be VECTOR of Complex_l1_Space holds u =seq_id u ) &
( for u,v be VECTOR of Complex_l1_Space holds u+v =seq_id(u)+seq_id(v) ) &
( for p be Complex for u be VECTOR of Complex_l1_Space holds
   p*u =p(#)seq_id(u) ) &
( for u be VECTOR of Complex_l1_Space holds
   -u = -seq_id u & seq_id(-u) = -seq_id(u) ) &
( for u,v be VECTOR of Complex_l1_Space holds u-v =seq_id(u)-seq_id v ) &
( for v be VECTOR of Complex_l1_Space holds seq_id v is absolutely_summable ) &
( for v be VECTOR of Complex_l1_Space holds ||.v.|| = Sum |.seq_id v.| )
proof
   set l1 =Complex_l1_Space;
A1:for x be set holds x is Element of l1
     iff x is Complex_Sequence & seq_id x is absolutely_summable
   proof
     let x be set;
      x in the_set_of_ComplexSequences
      iff x is Complex_Sequence by CSSPACE:def 1;
     hence thesis by Def1,Def3;
   end;

A2:for u be VECTOR of l1 holds u =seq_id u
   proof
     let u be VECTOR of l1;
      u is VECTOR of Linear_Space_of_ComplexSequences
       by Def3,Lm1,CLVECT_1:30;
     hence thesis by CSSPACE:def 2,def 7;
   end;

A3:for u,v be VECTOR of l1 holds u+v =seq_id(u)+seq_id(v)
   proof
     let u,v be VECTOR of l1;
     reconsider u1=u, v1=v as VECTOR of Linear_Space_of_ComplexSequences
       by Def3,Lm1,CLVECT_1:30;
     set L1=Linear_Space_of_ComplexSequences;
     set W = the_set_of_l1ComplexSequences;
A4:  [:W,W:] c= [:the carrier of L1,the carrier of L1:] by ZFMISC_1:119;
      dom (the add of L1)
       = [:the carrier of L1,the carrier of L1:] by FUNCT_2:def 1;
then A5:  dom ((the add of Linear_Space_of_ComplexSequences) | [:W,W:])
       =[:W,W:] by A4,RELAT_1:91;
      u+v =(the add of l1).[u,v] by RLVECT_1:def 3
        .=((the add of Linear_Space_of_ComplexSequences)|[:W,W:]).[u,v]
          by Def3,CSSPACE:def 8
        .=(the add of Linear_Space_of_ComplexSequences).[u,v]
          by A5,Def3,FUNCT_1:70
        .=u1+v1 by RLVECT_1:def 3;
     hence thesis by CSSPACE:4,def 7;
   end;

A6:for p be Complex for u be VECTOR of l1 holds p*u =p(#)seq_id(u)
   proof
     let p be Complex;
     let u be VECTOR of l1;
     reconsider u1=u as VECTOR of Linear_Space_of_ComplexSequences
       by Def3,Lm1,CLVECT_1:30;
     set L1=Linear_Space_of_ComplexSequences;
     set W = the_set_of_l1ComplexSequences;
A7: [:COMPLEX,W:] c= [:COMPLEX,the carrier of L1:] by ZFMISC_1:119;
      dom (the Mult of L1) = [:COMPLEX,the carrier of L1:]
       by FUNCT_2:def 1;
then A8:  dom ((the Mult of Linear_Space_of_ComplexSequences) | [:COMPLEX,W:])
       =[:COMPLEX,W:] by A7,RELAT_1:91;
      p*u =(the Mult of l1).[p,u] by CLVECT_1:def 1
        .=((the Mult of Linear_Space_of_ComplexSequences)|[:COMPLEX,W:]).[p,u]
          by Def3,CSSPACE:def 9
        .=(the Mult of Linear_Space_of_ComplexSequences).[p,u]
          by A8,Def3,FUNCT_1:70
        .=p*u1 by CLVECT_1:def 1;
     hence thesis by CSSPACE:5,def 7;
   end;

A9:for u be VECTOR of l1 holds -u =-seq_id u & seq_id(-u)=-seq_id u
   proof
     let u be VECTOR of l1;
       -u = (-1r)*u by Def3,Th7,CLVECT_1:4
        .= (-1r)(#)seq_id(u) by A6
        .= -seq_id(u) by COMSEQ_1:14;
     hence thesis by A2;
   end;
A10:for u,v be VECTOR of l1 holds u-v =seq_id(u)-seq_id(v)
   proof
     let u,v be VECTOR of l1;
     thus u-v = u+ -v by RLVECT_1:def 11
             .= seq_id(u)+seq_id(-v) by A3
             .= seq_id(u)+(-seq_id(v)) by A9
             .= seq_id(u)-seq_id(v) by COMSEQ_1:def 10;
   end;

A11:0.l1 = CZeroseq
   proof
     set W = the_set_of_l1ComplexSequences;
     thus 0.l1 = Zero_(W,Linear_Space_of_ComplexSequences)
                   by Def3,RLVECT_1:def 2
              .= 0.Linear_Space_of_ComplexSequences by CSSPACE:def 10
              .= CZeroseq by CSSPACE:def 7,RLVECT_1:def 2;
   end;
    for v be VECTOR of l1 holds ||.v.|| = Sum |.seq_id v.|
   proof
     let v be VECTOR of l1;
     thus ||.v.|| = (the norm of l1).v by CLVECT_1:def 9
                 .= Sum |.seq_id v.| by Def2,Def3;
   end;
   hence thesis by A1,A2,A3,A6,A9,A10,A11,Def3;
end;

theorem Th9:
for x, y being Point of Complex_l1_Space, p be Complex holds
  ( ||.x.|| = 0 iff x = 0.Complex_l1_Space ) &
  0 <= ||.x.|| &
  ||.x+y.|| <= ||.x.|| + ||.y.|| &
  ||. p*x .|| = |.p.| * ||.x.||
proof
   let x, y be Point of Complex_l1_Space;
   let p be Complex;
A1:now assume
A2:  ||.x.|| = 0;
A3:  ||.x.|| = Sum |.seq_id x.| by Th8;
      seq_id(x) is absolutely_summable by Th8;
then A4:  for n be Nat holds 0 = (seq_id x).n by A2,A3,Th6;
      x in the_set_of_ComplexSequences by Def1,Def3;
     hence x=0.Complex_l1_Space by A4,Th8,CSSPACE:def 6;
   end;

A5:now assume
      x=0.Complex_l1_Space;
then A6:  for n be Nat holds (seq_id x).n=0 by Th8,CSSPACE:def 6;
     thus ||.x.|| = (the norm of Complex_l1_Space). x by CLVECT_1:def 9
            .= Sum |.seq_id x.| by Def2,Def3
            .= 0 by A6,Th5;
   end;
A7:for n be Nat holds 0 <= (|.seq_id x.|).n
   proof
     let n be Nat;
      0 <= |.((seq_id x).n).| by COMPLEX1:132;
     hence thesis by COMSEQ_1:def 14;
   end;
    seq_id x is absolutely_summable by Def1,Def3;
then A8:|.seq_id x.| is summable by COMSEQ_3:def 11;
A9:||.x.|| = Sum |.seq_id x.| by Th8;
A10:||.y.|| = Sum |.seq_id y.| by Th8;
A11:Sum |.seq_id(x+y).| = ||.x + y.|| by Th8;
A12:for n be Nat holds (|.seq_id(x+y).|).n
       = |.(((seq_id x).n) + ((seq_id y).n)).|
   proof
     let n be Nat;
      (|.seq_id(x+y).|).n
         = (|.(seq_id(seq_id(x)+seq_id y)).|).n by Th8
        .= (|.(seq_id x+seq_id y).|).n by CSSPACE:3
        .= |.((seq_id x+seq_id y).n).| by COMSEQ_1:def 14
        .= |.(((seq_id x).n)+((seq_id y).n)).| by COMSEQ_1:def 4;
     hence thesis;
   end;

A13:for n be Nat holds (|.seq_id(x+y).|).n
        <= (|.seq_id x.|).n + (|.seq_id y.|).n
   proof
     let n be Nat;
      (|.(((seq_id x).n)+ ((seq_id y).n)).|)
       <= |.((seq_id x).n).| + |.((seq_id y).n).| by COMPLEX1:142;
     then (|.(seq_id(x+y)).|) .n
       <= |.((seq_id x).n).| + |.((seq_id y).n).| by A12;
     then (|.(seq_id(x+y)).|) .n
        <= (|.(seq_id x).|).n + |.((seq_id y).n).| by COMSEQ_1:def 14;
     hence thesis by COMSEQ_1:def 14;
   end;

A14:for n being Nat holds
     (|.seq_id(x+y).|).n <= (|.seq_id x.| + |.seq_id y.|).n
   proof
     let n be Nat;
      (|.seq_id x.|).n + (|.seq_id y.|).n
        =((|.seq_id x.|) + (|.seq_id y.|)).n by SEQ_1:11;
     hence (|.(seq_id(x+y)).|).n
          <= ((|.seq_id x.|) + (|.seq_id y.|)).n by A13;
    end;
     seq_id x is absolutely_summable by Def1,Def3;
then A15:|.seq_id x.| is summable by COMSEQ_3:def 11;
     seq_id y is absolutely_summable by Def1,Def3;
then A16:|.seq_id y.| is summable by COMSEQ_3:def 11;
then A17:|.seq_id x.| + |.seq_id y.| is summable by A15,SERIES_1:10;
    now
     let n be Nat;
      (|.seq_id(x+y).|).n = |.((seq_id(x+y)).n).| by COMSEQ_1:def 14;
     hence 0 <= (|.seq_id(x+y).|).n by COMPLEX1:132;
   end;
then A18:Sum(|.seq_id(x+y).|) <= Sum(|.seq_id x.| + |.seq_id y.|)
     by A14,A17,SERIES_1:24;
A19:for n be Nat holds
     (|.p(#)seq_id(x).|).n =|.p.|*(|.seq_id x.| .n)
   proof
     let n be Nat;
      (|.(p(#)seq_id(x)).|).n
               =|.((p(#)seq_id(x)).n).| by COMSEQ_1:def 14
              .=|.(p*((seq_id x).n)).| by COMSEQ_1:def 7
              .=(|.p.|)*(|.((seq_id x).n).|) by COMPLEX1:151
              .=(|.p.|)*((|.seq_id x.|).n) by COMSEQ_1:def 14;
     hence thesis;
   end;
    seq_id x is absolutely_summable by Def1,Def3;
then A20:|.seq_id x.| is summable by COMSEQ_3:def 11;
    ||.(p*x).|| =Sum(|.seq_id(p*x).|) by Th8
            .=Sum(|.seq_id(p(#)seq_id(x)).|) by Th8
            .=Sum(|.p(#)seq_id(x).|) by CSSPACE:3
            .=Sum(|.p.|(#)|.seq_id x.|) by A19,SEQ_1:13
            .=|.p.|*Sum(|.seq_id x.|) by A20,SERIES_1:13
            .=|.p.|*||.x.|| by Th8;
   hence thesis by A1,A5,A7,A8,A9,A10,A11,A16,A18,SERIES_1:10,21;
end;

registration
 cluster Complex_l1_Space -> ComplexNormSpace-like ComplexLinearSpace-like
       Abelian add-associative right_zeroed right_complementable;
  coherence by Def3,Lm2,Th4,Th9,CLVECT_1:def 10;
end;

Lm4:
for c be Complex, seq be Complex_Sequence, seq1 be Real_Sequence st
 seq is convergent & seq1 is convergent
  for rseq be Real_Sequence st
  (for i be Nat holds rseq .i = |.(seq.i-c).|+seq1.i) holds
   (rseq is convergent & lim rseq =|.(lim seq-c).|+lim seq1)
proof
   let c be Complex;
   let seq be Complex_Sequence;
   let seq1 be Real_Sequence;
   assume that
A1:seq is convergent and
A2:seq1 is convergent;
   let rseq be Real_Sequence such that
A3:for i be Nat holds rseq .i = |.(seq.i-c).|+seq1.i;
   deffunc F(set) = c;
   consider cseq be Complex_Sequence such that
A4:for n be Nat holds cseq.n=F(n) from COMSEQ_1:sch 1;

    now
     let i be Nat;
     thus rseq.i=|.(seq.i-c).|+seq1.i by A3
            .=|.(seq.i-cseq.i).|+seq1.i by A4
            .=|.(seq.i+(-cseq.i)).|+seq1.i
            .=|.(seq.i+(-cseq).i).|+seq1.i by COMSEQ_1:def 9
            .=|.((seq+ -cseq).i).| + seq1.i by COMSEQ_1:def 4
            .=|.((seq-cseq).i).| + seq1.i by COMSEQ_1:def 10
            .=|.(seq-cseq).| .i + seq1.i by COMSEQ_1:def 14
            .=(|.(seq-cseq).| + seq1).i by SEQ_1:11;
   end;
   then for x be set st x in NAT holds
    rseq.x =(|.(seq-(cseq)).| + seq1).x;
then A5:rseq = (|.(seq-(cseq)).| + seq1) by SEQ_1:8;
A6:lim cseq = c by A4,COMSEQ_2:10 .= cseq.0 by A4;
A7:cseq is convergent by A4,COMSEQ_2:9;
then A8:seq -cseq is convergent by A1,COMSEQ_2:25;
   reconsider seq1=seq-cseq as convergent Complex_Sequence
     by A1,A7,COMSEQ_2:25;
A9:|.seq1.| is convergent;
    lim |.(seq-cseq).|
     = |.(lim(seq-cseq)).| by A8,COMSEQ_2:11
    .= |.(lim seq - (cseq.0)).| by A1,A6,A7,COMSEQ_2:26
    .= |.(lim seq - c).| by A4;
   hence thesis by A2,A5,A9,SEQ_2:19,20;
end;

definition let X be non empty CNORMSTR, x, y be Point of X;
  func dist(x,y) -> Real equals :Def4:
  ||.x - y.||;
  coherence;
end;

definition
 let CNRM be non empty CNORMSTR;
 let seqt be sequence of CNRM;
 attr seqt is CCauchy means :Def5:
 for r1 be Real st r1 > 0 ex k1 be Nat st
  for n1, m1 be Nat st n1 >= k1 & m1 >= k1 holds
   dist(seqt.n1, seqt.m1) < r1;
  end;

notation
 let CNRM be non empty CNORMSTR;
 let seq be sequence of CNRM;
 synonym seq is Cauchy_sequence_by_Norm for seq is CCauchy;
end;

 reserve NRM for non empty ComplexNormSpace;
 reserve seq for sequence of NRM;

theorem Th10:
seq is Cauchy_sequence_by_Norm iff for r be Real st r > 0
 ex k be Nat st for n, m be Nat st
  n >= k & m >= k holds ||.(seq.n) - (seq.m).|| < r
proof
   thus
    seq is Cauchy_sequence_by_Norm implies for r be Real
   st r > 0 ex k be Nat st for n, m be Nat st
   n >= k & m >= k holds ||.(seq.n) - (seq.m).|| < r
   proof
     assume
A1:  seq is Cauchy_sequence_by_Norm;
     let r be Real such that
A2:  r > 0;
     consider k be Nat such that
A3:  for n, m be Nat st n >= k & m >= k holds
     dist(seq.n, seq.m) < r by A1,A2,Def5;
      for n, m be Nat st n >= k & m >= k holds ||.(seq.n) - (seq.m).|| < r
     proof
       let n,m be Nat such that
A4:    n >= k & m >= k;
        dist(seq.n, seq.m) < r by A3,A4;
       hence thesis by Def4;
     end;
     hence thesis;
   end;
   thus (for r be Real st r > 0 ex k be Nat st for n, m be Nat st
     n >= k & m >= k holds ||.(seq.n) - (seq.m).|| < r) implies
     seq is Cauchy_sequence_by_Norm
   proof
     assume
A5:  for r be Real
     st r > 0 ex k be Nat st for n, m be Nat st
     n >= k & m >= k holds ||.(seq.n) - (seq.m).|| < r;
      now
       let r be Real;
       assume
A6:   r > 0;
        now
         consider k be Nat such that
A7:    for n, m be Nat st
         n >= k & m >= k holds ||.(seq.n) - (seq.m).|| < r by A5,A6;
          now
           let n,m be Nat;
           assume n >= k & m >= k;
           then ||.(seq.n) - (seq.m).|| < r by A7;
           hence dist(seq.n, seq.m) < r by Def4;
         end;
         hence
          ex k be Nat st for n, m be Nat st
          n >= k & m >= k holds dist(seq.n, seq.m) < r;
       end;
       hence ex k be Nat st for n, m be Nat st
         n >= k & m >= k holds dist(seq.n, seq.m) < r;
     end;
     hence seq is Cauchy_sequence_by_Norm by Def5;
   end;
end;

theorem
 for vseq be sequence of Complex_l1_Space st
 vseq is Cauchy_sequence_by_Norm holds vseq is convergent
proof
   let vseq be sequence of Complex_l1_Space such that
A1:vseq is Cauchy_sequence_by_Norm;
   defpred P[set,set] means
        ex i be Nat st $1=i &
     ex rseqi be Complex_Sequence st
      (for n be Nat holds rseqi.n=(seq_id(vseq.n)).i) &
     rseqi is convergent & $2 = lim rseqi;
A2:for x be set st x in NAT
    ex y be set st y in COMPLEX & P[x,y]
   proof
     let x be set such that
A3:  x in NAT;
     reconsider i=x as Nat by A3;
     deffunc F(Nat) = (seq_id(vseq.$1)).i;
     consider rseqi be Complex_Sequence such that
A4:  for n be Nat holds rseqi.n= F(n) from  COMSEQ_1:sch 1;
A5:  rseqi is convergent
     proof
        now
         let e be Real such that
A6:      e > 0;
         thus ex k be Nat st
          for m be Nat st k <= m holds |.(rseqi.m -rseqi.k).| < e
         proof
           consider k be Nat such that
A7:        for n, m be Nat st n >= k & m >= k holds
           ||.(vseq.n) - (vseq.m).|| < e by A1,A6,Th10;
            for m being Nat st k <= m holds |.(rseqi.m-rseqi.k).| < e
           proof
             let m be Nat such that
A8:          k<=m;
              ||.(vseq.m) - (vseq.k).||
               =Sum(|.(seq_id((vseq.m) - (vseq.k))).|) by Th8;
then A9:          Sum(|.(seq_id((vseq.m) - (vseq.k))).|) < e by A7,A8;
              seq_id((vseq.m)-(vseq.k)) is absolutely_summable
               by Def1,Def3;
then A10:         |.(seq_id((vseq.m)-(vseq.k))).| is summable by
COMSEQ_3:def 11;
              for i be Nat holds 0 <= |.(seq_id((vseq.m) - (vseq.k))).| .i
             proof
               let i be Nat;
                0 <= |.((seq_id((vseq.m) - (vseq.k))).i).| by COMPLEX1:132;
               hence thesis by COMSEQ_1:def 14;
             end;
then A11:         |.(seq_id((vseq.m) - (vseq.k))).| .i
               <= Sum(|.(seq_id((vseq.m) - (vseq.k))).|) by A10,RSSPACE2:4;
              seq_id((vseq.m) - (vseq.k))
               = seq_id(seq_id((vseq.m))-seq_id((vseq.k))) by Th8
              .= seq_id((vseq.m))-seq_id((vseq.k)) by CSSPACE:3
              .= seq_id((vseq.m))+-seq_id((vseq.k)) by COMSEQ_1:def 10;
             then (seq_id((vseq.m) - (vseq.k))).i
               =(seq_id((vseq.m))).i+(-seq_id((vseq.k))).i by COMSEQ_1:def 4
              .=(seq_id((vseq.m))).i+(-(seq_id((vseq.k))).i) by COMSEQ_1:def 9
              .=(seq_id((vseq.m))).i-(seq_id((vseq.k))).i
              .=rseqi.m -(seq_id((vseq.k))).i by A4
              .=rseqi.m - rseqi.k by A4;
             then |.(rseqi.m-rseqi.k).| = |.(seq_id((vseq.m) - (vseq.k))).| .i
               by COMSEQ_1:def 14;
             hence thesis by A9,A11,AXIOMS:22;
           end;
           hence thesis;
         end;
       end;
       hence thesis by COMSEQ_3:46;
     end;
     take y = lim rseqi;
     thus thesis by A4,A5;
   end;
   consider f be Function of NAT,COMPLEX such that
A12:for x be set st x in NAT holds P[x,f.x] from FUNCT_2:sch 1(A2);
   reconsider tseq=f as Complex_Sequence;
A13:now
     let i be Nat;
     reconsider x=i as set;
      ex i0 be Nat st
      x=i0 &
      ex rseqi be Complex_Sequence st
       ( for n be Nat holds rseqi.n=(seq_id(vseq.n)).i0 ) &
      rseqi is convergent & f.x=lim rseqi by A12;
      hence ex rseqi be Complex_Sequence st
       ( for n be Nat holds rseqi.n=(seq_id(vseq.n)).i ) &
       rseqi is convergent & tseq.i=lim rseqi;
   end;
A14:for e be Real st e >0
   ex k be Nat st
    for n be Nat st n >= k holds
     ( |.(seq_id tseq-seq_id(vseq.n)).| is summable &
       Sum(|.(seq_id tseq-seq_id(vseq.n)).|) < e )
   proof
     let e1 be Real such that
A15: e1 >0;
     set e=e1/2;
A16: e > 0 by A15,SEQ_2:3;
A17: e < e1 by A15,SEQ_2:4;
     consider k be Nat such that
A18: for n, m be Nat st n >= k & m >= k holds
      ||.(vseq.n) - (vseq.m).|| < e by A1,A16,Th10;
A19: for m,n be Nat st n >= k & m >= k holds
      ( |.(seq_id((vseq.n) - (vseq.m))).| is summable &
        Sum(|.(seq_id((vseq.n) - (vseq.m))).|) < e &
        for i be Nat holds 0 <= |.(seq_id(vseq.n-vseq.m)).| .i )
     proof
       let m,n be Nat such that
A20:    n >= k & m >= k;
        ||.(vseq.n) - (vseq.m).|| < e by A18,A20;
then A21:   (the norm of Complex_l1_Space).(vseq.n-vseq.m) < e by
CLVECT_1:def 9;
A22:   seq_id(vseq.n-vseq.m) is absolutely_summable
         by Def1,Def3;
        for i be Nat holds 0 <= |.(seq_id(vseq.n-vseq.m)).| .i
       proof
         let i be Nat;
          0 <= |.((seq_id(vseq.n-vseq.m)).i).| by COMPLEX1:132;
         hence thesis by COMSEQ_1:def 14;
       end;
       hence thesis by A21,A22,Def2,Def3,COMSEQ_3:def 11;
     end;
A23: for n be Nat for i be Nat holds
     for rseq be Real_Sequence st
      ( for m be Nat holds
        rseq.m=Partial_Sums(|.seq_id(vseq.m-vseq.n).|).i )
       holds
      ( rseq is convergent &
        lim rseq = Partial_Sums(|.(seq_id tseq-seq_id(vseq.n)).|).i )
     proof
       let n be Nat;
A24:   for m,k be Nat holds
        seq_id((vseq.m) - (vseq.k)) = seq_id((vseq.m))-seq_id((vseq.k))
       proof
         let m,k be Nat;
          seq_id((vseq.m) - (vseq.k))
          = seq_id(seq_id((vseq.m))-seq_id((vseq.k))) by Th8;
         hence thesis by CSSPACE:3;
       end;
       defpred P[Nat] means
        for rseq be Real_Sequence st
         for m be Nat holds
          rseq.m=Partial_Sums(|.seq_id(vseq.m-vseq.n).|).$1 holds
           rseq is convergent &
          lim rseq = Partial_Sums(|.seq_id tseq-seq_id(vseq.n).|).$1;
A25:   P[0]
       proof
          now
           let rseq be Real_Sequence such that
A26:       for m be Nat holds
            rseq.m=Partial_Sums(|.seq_id(vseq.m-vseq.n).|).0;
           thus rseq is convergent &
            lim rseq = Partial_Sums(|.seq_id tseq-seq_id(vseq.n).|).0
           proof
             consider rseq0 be Complex_Sequence such that
A27:          ( for m be Nat holds rseq0.m=(seq_id(vseq.m)).0 ) &
                rseq0 is convergent & tseq.0=lim rseq0 by A13;
A28:         for m being Nat holds rseq.m = |.(rseq0.m-(seq_id(vseq.n)).0).|
             proof
               let m be Nat;
                rseq.m=Partial_Sums(|.seq_id(vseq.m-vseq.n).|).0 by A26
