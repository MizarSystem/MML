 per cases;
 suppose S-min L~z = W-min L~z;
  hence thesis by A1,Lm4;
 end;
 suppose S-min L~z <> W-min L~z;
  then (S-min L~z)..z < (W-min L~z)..z by A1,SPRECT_2:78;
