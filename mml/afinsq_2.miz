theorem :: NUMERAL1:2
  for S being Real_Sequence st rF=S|(n+1) holds Sum rF = Partial_Sums(S).n
proof
  let S be Real_Sequence;
  A1:rF is REAL -valued by Lm7;
  n+1 c= NAT by ORDINAL1:def 2;
  then
A2: n+1 c= dom S by FUNCT_2:def 1;
  assume
A3: rF=S|(n+1);
  then dom rF = dom S /\ (n+1) by RELAT_1:90;
  then
A4: dom rF = n+1 by A2,XBOOLE_1:28;
  then consider f be Function of NAT,REAL such that
A5: f.0 = rF.0 and
A6: for m be Nat st m+1 < len rF holds f.(m + 1) = addreal.(f.m,rF.(m + 1)) and
A7: addreal "**" rF = f.(len rF-1) by Def9,A1;
  defpred P[Nat] means $1 in dom rF implies f.$1=Partial_Sums(S).$1;
A9: now
    let k;
    assume
A10: P[k];
    thus P[k+1]
    proof
      assume
A11:  k+1 in dom rF;
      then
A12:  k+1 < len rF by NAT_1:45;
      then
A13:  k<len rF by NAT_1:13;
A14:      k in NAT by ORDINAL1:def 13;
      thus f.(k+1)= addreal.(f.k,rF.(k + 1)) by A6,A12
        .= (f.k)+rF.(k + 1) by BINOP_2:def 9
        .= (f.k)+S.(k+1) by A3,A11,FUNCT_1:70
        .= Partial_Sums(S).(k+1) by A10,A13,NAT_1:45,SERIES_1:def 1,A14;
    end;
  end;
  Partial_Sums(S).0=S.0 by SERIES_1:def 1;
  then
A15: P[0] by A3,A5,FUNCT_1:70;
  for m holds P[m] from NAT_1:sch 2(A15,A9);
  hence Partial_Sums(S).n=f.n by A4,NAT_1:46
     .= Sum rF by Th61,A7,A4;
end;

theorem Th67: :: NUMERAL1:4
  len rF1 = len rF2 &
  (for i st i in dom rF1 holds rF1.i<=rF2.i) implies
  Sum rF1 <= Sum rF2
proof
  set d=rF1,e=rF2;
assume that
A1: len d = len e and
A2: for i st i in dom d holds d.i<=e.i;
reconsider d,e as  XFinSequence of REAL by Lm7;
A3:  Sum d = addreal "**" d & Sum e = addreal "**" e by Th61;
per cases by NAT_1:14;
  suppose A4:len d >=1;
  consider f being Function of NAT,REAL such that
A5: f.0 = d.0 and
A6: for n st n+1 < len d holds f.(n + 1) = addreal.
  (f.n,d.(n + 1)) and
A7: Sum d = f.(len d-1) by A4,Def9,A3;
  consider g being Function of NAT,REAL such that
A8: g.0 = e.0 and
A9: for n st n+1 < len e holds g.(n + 1) = addreal.
  (g.n,e.(n + 1)) and
A10: Sum e = g.(len e-1) by A4,A1,Def9,A3;
  defpred P[Nat] means $1 in dom d implies f.$1 <= g.$1;
A11: now
    let i;
    assume
A12: P[i];
    thus P[i+1]
    proof
      assume
A13:  i+1 in dom d;
      then
A14:  i+1 < len d by NAT_1:45;
      then
A15:  i < len d by NAT_1:13;
A16:  d.(i+1) <= e.(i+1) by A2,A13;
A17:  f.(i+1) = addreal.(f.i,d.(i + 1)) by A6,A14
        .= f.i + d.(i+1) by BINOP_2:def 9;
      g.(i+1) = addreal.(g.i,e.(i + 1)) by A1,A9,A14
        .= g.i + e.(i+1) by BINOP_2:def 9;
      hence thesis by A12,A15,A17,A16,NAT_1:45,XREAL_1:9;
    end;
  end;
  reconsider ld=len d-1 as Element of NAT by A4,NAT_1:21;
  len d-1 < len d - 0 by XREAL_1:12;
  then
A18: ld in len d by NAT_1:45;
A19: P[0] by A2,A5,A8;
  for i holds P[i] from NAT_1:sch 2(A19,A11);
  hence thesis by A1,A7,A10,A18;
end;
suppose len d=0;
  then Sum d = the_unity_wrt addreal & Sum e = the_unity_wrt addreal
     by Def9,A3,A1;
  hence thesis;
end;
end;

theorem Th68:
  Sum (n-->c) = n*c
proof
  set Fn= n-->c;
  reconsider Fn as XFinSequence of COMPLEX by Lm6;
A1:dom Fn = n by FUNCOP_1:19;
  now
    per cases;
    suppose
      dom Fn=0;
      hence thesis by A1;
    end;
    suppose
A4:   dom Fn>0;
      then consider f be Function of NAT,COMPLEX such that
A5:   f.0 = Fn.0 and
A6:   for k st k+1 < len Fn holds
         f.(k + 1) = addcomplex.(f.k,Fn.(k + 1)) and
A7:   Sum Fn= f.(len Fn-1) by Def9;
      defpred P[Nat] means $1 < len Fn implies f.$1 =($1+1)*c;
A8:   for m st P[m] holds P[m+1]
      proof
        let m such that
A9:     P[m];
        assume
A10:     m + 1 < len Fn;
        then f.(m+1)=addcomplex.(f.m,Fn.(m+1)) by A6;
        then
A11:     f.(m + 1) = f.m + Fn.(m+1) by BINOP_2:def 3;
        m+1 in len Fn by A10,NAT_1:45;
        then Fn.(m+1) = c by A1,FUNCOP_1:13;
        hence thesis by A9,A10,A11,NAT_1:13;
      end;
      reconsider lenFn1=len Fn -1 as Element of NAT by A4,NAT_1:20;
A12:  lenFn1<lenFn1+1 by NAT_1:13;
A13:  P[0]
      proof
        assume 0<len Fn;
        then 0 in dom Fn by NAT_1:45;
        hence thesis by A5,FUNCOP_1:13,A1;
      end;
      for m holds P[m] from NAT_1:sch 2(A13,A8);

      hence thesis by A7,A12,A1;
    end;
  end;
  hence thesis;
end;

theorem :: STIRL2_1:50
  (for n st n in dom rF holds rF.n <= r) implies
   Sum rF <= len rF * r
proof
  set L=len rF-->r;
  assume A1:n in dom rF implies rF.n <= r;
  A3:len L=len rF by FUNCOP_1:19;
  now let n;assume n in dom rF;
     then rF.n <= r & L.n = r by A1,FUNCOP_1:13;
     hence rF.n <= L.n;
  end;
  then Sum rF <= Sum L by Th67,A3;
  hence thesis by Th68;
end;

theorem :: STIRL2_1:51
  (for n st n in dom rF holds rF.n >= r) implies
  Sum rF >= len rF *r
proof
  set L=len rF-->r;
  assume A1:n in dom rF implies rF.n >= r;
  A3:len L=len rF by FUNCOP_1:19;
  now let n;assume n in dom rF;
     then rF.n >= r & L.n = r by A1,FUNCOP_1:13;
     hence rF.n >= L.n;
  end;
  then Sum rF >= Sum L by Th67,A3;
  hence thesis by Th68;
end;

theorem Th71: :: STIRL2_1:52
  rF is nonnegative-yielding & len rF > 0 &
  (ex x st x in dom rF & rF.x = r) implies Sum rF >= r
proof
  assume that
A1:rF is nonnegative-yielding and
A2: len rF > 0 and
A3: ex x st x in dom rF & rF.x = r;
  consider x such that
A5: x in dom rF and
A6: rF.x = r by A3;
  reconsider lenrF1=len rF-1 as Element of NAT by A2,NAT_1:20;
A7: dom rF=lenrF1+1;
  reconsider x as Element of NAT by A5;
A8: lenrF1 < lenrF1+1 by NAT_1:13;
A9:  x < dom rF by A5,NAT_1:45;
  then
A10: x<=lenrF1 by A7,NAT_1:13;
  rF is REAL-valued by Lm7;then
  consider f be Function of NAT,REAL such that
A11: f.0 = rF.0 and
A12: for n st n+1 < len rF holds f.(n + 1) = addreal.(f.n,rF.(n + 1)) and
A13: addreal "**" rF= f.(len rF-1) by Def9,A2;
defpred P[Nat] means $1 < x implies f.$1 >= 0;
0 in len rF by A2,NAT_1:45;
then rF.0 in rng rF by FUNCT_1:def 5;
then
A14:P[0] by A1,PARTFUN3:def 4,A11;
A15:P[n] implies P[n+1]
proof
  assume A16:P[n];
  assume K1:n+1 < x;
  then n < x & n+1 < len rF by NAT_1:13,A9,XXREAL_0:2;
  then A18:f.(n + 1) = addreal.(f.n,rF.(n + 1)) & f.n >=0 & n+1 in dom rF
       by A12,A16,NAT_1:45;
  then rF.(n+1) in rng rF by FUNCT_1:def 5;
  then rF.(n+1) >=0 by PARTFUN3:def 4,A1;
  then f.n+rF.(n + 1) >=0+0 by K1,NAT_1:13,A16;
  hence thesis by A18,BINOP_2:def 9;

end;
A19:P[n] from NAT_1:sch 2(A14,A15);
  defpred P[Nat] means x <= $1 & $1 < len rF implies f.$1 >= r;
  now
    per cases;
    suppose
A20:  x=0;
      assume that
      x <= x and
      x < len rF;
      thus f.x>=r by A6,A11,A20;
    end;
    suppose
      x>0;
      then reconsider x1=x-1 as Element of NAT by NAT_1:20;
      assume that
      x <= x and
A21:  x < len rF;
A22:       x1 <x1+1 by NAT_1:13;
      x1+1 < len rF by A21;
      then f.x = addreal.(f.x1,rF.x) by A12;
      then f.x=f.x1+rF.x & f.x1 >=0
       by BINOP_2:def 9,A22,A19;
      then f.x>=r+0 by A6,XREAL_1:9;
      hence f.x>=r;
    end;
  end;
  then
A25: P[x];
A26: for m be Nat st m>=x & P[m] holds P[m+1]
  proof
    let m be Nat such that
A27: m>=x and
A28: P[m];
    reconsider m1 = m as Element of NAT by ORDINAL1:def 13;
    assume that
    x <= m+1 and
A29: m+1 < len rF;
       m+1 in dom rF by NAT_1:45,A29;
then A31:rF.(m+1) in rng rF by FUNCT_1:def 5;
    f.(m1 + 1) = addreal.(f.m1,rF.(m1 + 1)) by A12,A29;
    then f.(m1+1)=f.m1+rF.(m1+1) & rF.(m1+1) >=0
   by BINOP_2:def 9,A31,PARTFUN3:def 4,A1;
    then f.(m+1) >= r+0 by A27,A28,A29,NAT_1:13,XREAL_1:9;
    hence thesis;
  end;
  for m be Nat st m>=x holds P[m] from NAT_1:sch 8(A25,A26);
  then addreal "**" rF >= r by A13,A10,A8;
  hence thesis by Th61;
end;

theorem Th72: :: STIRL2_1:53
  rF is nonnegative-yielding implies
  (Sum rF=0 iff (len rF=0 or rF = len rF --> 0))
proof
assume A1:
  rF is nonnegative-yielding;
  hereby
    assume
A2: Sum rF=0;
    assume len rF <>0;
  set L=len rF -->0;
    assume rF <> len rF -->0;
    then consider k  such that
A5: k in dom L & L.k <> rF.k by FUNCOP_1:19,AFINSQ_1:10;
    rF.k in rng rF by A5,FUNCT_1:def 5;
    then
    L.k = 0 & rF.k >=0 by A5,FUNCOP_1:13,A1,PARTFUN3:def 4;
    hence contradiction by A2,Th71,A1,A5;
 end;
A7:rF is COMPLEX-valued by Lm6;
    assume len rF=0 or rF= len rF -->0 ;
    then Sum rF = 0 or Sum rF = len rF *0 by A7,Th68,Def9,BINOP_2:1;
    hence thesis;
end;

theorem TTT1:
  c(#)cF|n = (c(#)cF)|n
proof
  set ccF=c(#)cF;
  set cFn = cF|n;
A2:len ccF = len cF & len (c(#)cFn) = len cFn by VALUED_1:def 5;
  per cases;
    suppose A3:n <= len cF;
        then A4:len(cFn) = n & len (ccF|n)=n by A2,AFINSQ_1:58;
        now let i;
          assume i < len (c(#)cFn);
          then A6: i in dom (c(#)cFn) by NAT_1:45;
          thus (c(#)cFn).i = c* (cFn.i) by VALUED_1:6
                           .= c* (cF.i) by A6,A2,A3,A4,AFINSQ_1:57
                           .=ccF.i by  VALUED_1:6
             .=(ccF|n).i by A6,A2,A3,A4,AFINSQ_1:57;
        end;
        hence thesis by AFINSQ_1:11,A2,A4;
    end;
    suppose n > len cF;
       then cF|n= cF & ccF|n=ccF by AFINSQ_1:56,A2;
       hence thesis;
    end;
end;

theorem
  c * Sum cF = Sum (c(#)cF)
proof
    defpred P[Nat] means for cF st dom cF=$1 holds
  c * Sum cF = Sum (c(#)cF);
A2: for k st P[k] holds P[k+1]
  proof
    let k such that
A3: P[k];
A4: k<k+1 by NAT_1:13;
    let cF such that
A5: dom cF=k+1;
    set cF1 = c(#)cF;
   A6:dom cF=dom cF1 by VALUED_1:def 5;
    reconsider cF,cF1 as XFinSequence of COMPLEX by Lm6;
A8:cF|(k+1)=cF by A5,RELAT_1:98;
    len cF =k+1 by A5;
    then
A9: len (cF|k)=k by A4,AFINSQ_1:14;
    k<k+1 by NAT_1:13;
    then
A10: k in dom cF by A5,NAT_1:45;
    then addcomplex.
      (addcomplex "**" cF|k, cF.k) = addcomplex "**" cF|(k+1) by Th56;
    then
A11: Sum cF=Sum (cF|k)+cF.k by A8,BINOP_2:def 3;
A15: c * Sum (cF|k)= Sum (c(#)(cF|k)) by A3,A9
        .= Sum(cF1|k) by TTT1;
A16: c*cF.k=cF1.k by VALUED_1:6;
A17: cF1|(k+1)=cF1 by A5,A6,RELAT_1:98;
    addcomplex.(addcomplex "**" cF1|k,cF1.k)
     =addcomplex "**" cF1|(k+1) by A6,A10,Th56;
    then Sum cF1=Sum (cF1|k)+cF1.k by A17,BINOP_2:def 3;
    hence thesis by A11,A16,A15;
  end;
A18: P[0]
  proof
    let cF such that
A19: dom cF=0;
    set cF1 = c(#)cF;
      reconsider cF,cF1 as XFinSequence of COMPLEX by Lm6;
    len cF=0 by A19;
    then
A21: addcomplex "**" cF=0 by BINOP_2:1,Def9;
    len cF1=0 by A19,VALUED_1:def 5;
    hence thesis by A21,BINOP_2:1,Def9;
  end;
 for k holds P[k] from NAT_1:sch 2(A18,A2);
  then P[len cF];
  hence thesis;
end;

theorem Th74: :: CARD_FIN:44
   n in dom cF implies Sum (cF|n) + cF.n = Sum (cF|(n+1))
proof
  assume
A1:  n in dom cF;
    reconsider cF as XFinSequence of COMPLEX by Lm6;
 addcomplex.(addcomplex "**" cF|n, cF.n) = addcomplex "**" cF|(n+1)
      by Th56,A1;
  hence thesis by BINOP_2:def 3;
end;

theorem Th75: ::CARD_FIN:13
for f be Function st
  f.y=x & y in dom f holds {y}\/(f|(dom f\{y}))"{x}=f"{x}
proof
  let f be Function;
  assume that
A1: f.y=x and
A2: y in dom f;
  set d=dom f\{y};
A3: (f|d)"{x} c= f"{x}
  proof
    let x1 be set such that
A4: x1 in (f|d)"{x};
A5: (f|d).x1 in {x} by A4,FUNCT_1:def 13;
A6: x1 in dom (f|d) by A4,FUNCT_1:def 13;
    then dom (f|d)=dom f/\d & f.x1=(f|d).x1 by FUNCT_1:70,RELAT_1:90;
    hence thesis by A6,A5,FUNCT_1:def 13;
  end;
A7: f"{x} c= {y}\/(f|d)"{x}
  proof
    let x1 be set such that
A8: x1 in f"{x};
    x1 in dom f & not x1 in {y} or x1=y by A8,FUNCT_1:def 13,TARSKI:def 1;
    then x1 in dom f & x1 in d & dom (f|d)=dom f/\d or x1=y by RELAT_1:90
,XBOOLE_0:def 5;
    then x1 in dom (f|d) or x1=y by XBOOLE_0:def 4;
    then x1 in dom (f|d) & f.x1=(f|d).x1 & f.x1 in {x} or x1 in {y} by A8,
FUNCT_1:70,def 13,TARSKI:def 1;
    then x1 in (f|d)"{x} or x1 in {y} by FUNCT_1:def 13;
    hence thesis by XBOOLE_0:def 3;
  end;
  {y} c= f"{x}
  proof
    let z;
    assume z in {y};
    then
A9: z=y by TARSKI:def 1;
    f.y in {x} by A1,TARSKI:def 1;
    hence thesis by A2,A9,FUNCT_1:def 13;
  end;
  then {y}\/(f|d)"{x} c= f"{x} by A3,XBOOLE_1:8;
  hence thesis by A7,XBOOLE_0:def 10;
end;

theorem Th76: :: CARD_FIN:15
 for f be Function st f.y<>x holds (f|(dom f\{y}))"{x}=f"{x}
proof
  let f be Function;
  set d=dom f\{y};
  assume
A1: f.y<>x;
A2: f"{x} c= (f|d)"{x}
  proof
A3: dom (f|d)=dom f/\d by RELAT_1:90;
    let x1 be set such that
A4: x1 in f"{x};
A5: f.x1 in {x} by A4,FUNCT_1:def 13;
    f.x1 in {x} by A4,FUNCT_1:def 13;
    then f.x1=x by TARSKI:def 1;
    then
A6: not x1 in {y} by A1,TARSKI:def 1;
    x1 in dom f by A4,FUNCT_1:def 13;
    then x1 in d by A6,XBOOLE_0:def 5;
    then
A7: x1 in dom (f|d) by A3,XBOOLE_0:def 4;
    then f.x1=(f|d).x1 by FUNCT_1:70;
    hence thesis by A7,A5,FUNCT_1:def 13;
  end;
  (f|d)"{x} c= f"{x}
  proof
    let x1 be set such that
A8: x1 in (f|d)"{x};
A9: (f|d).x1 in {x} by A8,FUNCT_1:def 13;
A10: x1 in dom (f|d) by A8,FUNCT_1:def 13;
    then dom (f|d)=dom f/\d & f.x1=(f|d).x1 by FUNCT_1:70,RELAT_1:90;
    hence thesis by A10,A9,FUNCT_1:def 13;
  end;
  hence thesis by A2,XBOOLE_0:def 10;
end;

theorem :: CATALAN2:45
  rng cF c= {0,c} implies Sum cF = c * card (cF"{c})
proof
  defpred P[Nat] means for cF,c st dom cF=$1 &
  rng cF c= {0,c} holds Sum cF = c* card (cF"{c});
assume
A1: rng cF c= {0,c};
A2: for k st P[k] holds P[k+1]
  proof
    let k such that
A3: P[k];
    let F be complex-valued XFinSequence,
     c be complex number such that
A4: dom F=k+1 and
A5: rng F c= {0,c};
    per cases;
    suppose
A6:   c <>0;
      ( not k in k)& k\/{k}=k+1 by AFINSQ_1:4;
      then
A7:   dom F\{k}=k by A4,ZFMISC_1:141;
      k <k+1 by NAT_1:13;
      then k in dom F by A4,NAT_1:45;
      then
A8:   F.k in rng F by FUNCT_1:def 5;
      per cases by A5,A8,TARSKI:def 2;
      suppose
A9:     F.k=0;
A10:    F|(k+1)=F by A4,RELAT_1:98;
A11:    k <k+1 by NAT_1:13;
        then k in dom F by A4,NAT_1:45;
        then
A12:    Sum (F|k) + 0= Sum F by A9,A10,Th74;
        dom F=len F;
        then
        A13:    len (F|k)=k by A4,A11,AFINSQ_1:58;
        rng (F|k) c= rng F & (F|k)"{c}=F"{c} by A6,A7,A9,Th76,RELAT_1:99;
        hence thesis by A3,A5,A13,A12,XBOOLE_1:1;
      end;
      suppose
A14:    F.k=c;
        set Fk=(F|k)"{c};
        not k in k;
        then not k in dom F/\k by XBOOLE_0:def 4;
        then not k in dom (F|k) by RELAT_1:90;
        then
A15:    not k in Fk by FUNCT_1:def 13;
A16:    k <k+1 by NAT_1:13;
        then
A17:    k in dom F by A4,NAT_1:45;
        dom F=len F;
        then rng (F| k) c= rng F & len (F|k)= k by A4,A16,AFINSQ_1:58,
RELAT_1:99;
        then
A18:    Sum (F|k)=c* card ((F|k)"{c}) by A3,A5,XBOOLE_1:1;
        F|(k+1)=F by A4,RELAT_1:98;
        then
A19:    Sum (F|k)+ c = Sum F by A14,A17,Th74;
        {k}\/Fk=F"{c} by A7,A14,A17,Th75;
        then (card Fk)+1=card (F"{c}) by A15,CARD_2:54;
        hence thesis by A18,A19;
      end;
    end;
    suppose
A20:  c = 0;
      for x st x in dom F holds F.x = 0
      proof
        let x;
        assume x in dom F;
        then F.x in rng F by FUNCT_1:def 5;
        hence thesis by A5,A20,TARSKI:def 2;
      end;
      then F = dom F --> 0 by FUNCOP_1:17;
      then Sum F = len F*0 by Th72;
hence thesis by A20;
    end;
  end;
A21: P[0]
  proof
    let F be complex-valued XFinSequence,
c be complex number such that
A22: dom F=0 and
    rng F c= {0,c};
    F"{c} c= 0 & F={} by A22,RELAT_1:167;
then card (F"{c})=0 & Sum F =0;
    hence thesis;
  end;
 for k holds P[k] from NAT_1:sch 2(A21,A2);
then P[len cF];
  hence thesis by A1;
end;

theorem :: CATALAN2:48
  Sum cF = Sum Rev cF
proof
  cF is COMPLEX-valued by Lm6;then
  reconsider Fr2 = cF,Fr1 = Rev cF as XFinSequence of COMPLEX;
A1: len Fr1=len Fr2 by Def1;
  defpred P[set,set] means for i st i=$1 holds $2=len Fr1-(1+i);
A3: card len Fr1 =card len Fr1;
A4: for x st x in len Fr1 ex y st y in len Fr1 & P[x,y]
  proof
    let x such that
A5: x in len Fr1;
     reconsider k=x as Element of NAT by Th1,A5;
    k< len Fr1 by A5,NAT_1:45;
    then k+1 <= len Fr1 by NAT_1:13;
    then
A6: len Fr1-'(1+k)=len Fr1-(1+k) by XREAL_1:235;
    take len Fr1-'(1+k);
    len Fr1 +0< len Fr1 +(1+k) by XREAL_1:10;
    then len Fr1-(1+k) < len Fr1+(1+k)-(1+k) by XREAL_1:11;
    hence thesis by A6,NAT_1:45;
  end;
  consider P be Function of len Fr1,len Fr1 such that
A7: for x st x in len Fr1 holds P[x,P.x] from FUNCT_2:sch 1(A4);
A8: for x1,x2 be set
    st x1 in len Fr1 & x2 in len Fr1 & P.x1 = P.x2 holds x1 = x2
  proof
    let x1,x2 be set such that
A9: x1 in len Fr1 and
A10: x2 in len Fr1 and
A11: P.x1 = P.x2;
     reconsider i=x1,j=x2 as Element of NAT by A9,A10,Th1;
A12: P.x2=len Fr1-(1+j) by A7,A10;
    P.x1=len Fr1-(1+i) by A7,A9;
    hence thesis by A11,A12;
  end;
  then
A13: P is one-to-one by FUNCT_2:77;
  P is one-to-one by A8,FUNCT_2:77;
  then P is onto by A3,Lm2;
  then reconsider P as Permutation of dom Fr1 by A13;
A14: now
    let x such that
A15: x in dom Fr1;
    reconsider k=x as Element of NAT by A15;
    P.k=len Fr1-(1+k) by A7,A15;
    hence Fr1.x=Fr2.(P.x) by A1,Def1,A15;
  end;
A16: now
    let x such that
A17: x in dom Fr1;
    x in dom P by A17,FUNCT_2:67;
    then P.x in rng P by FUNCT_1:12;
    hence x in dom P & P.x in dom Fr2 by A1,A17,FUNCT_2:67;
  end;
  for x st x in dom P & P.x in dom Fr2 holds x in dom Fr1;
  then Fr1 = Fr2 * P by A16,A14,FUNCT_1:20;
  hence thesis by A1,Th58;
end;

theorem Th79:
  for f be Function,p,q,fp,fq be XFinSequence st
       rng p c= dom f & rng q c= dom f & fp = f*p & fq = f*q
    holds fp ^ fq = f*(p^q)
proof
  let f be Function,p,q,fp,fq be XFinSequence such that
A1:       rng p c= dom f & rng q c= dom f & fp = f*p & fq = f*q;
set pq=p^q;
C2:rng pq = rng p \/rng q by AFINSQ_1:29;
then A2:dom (f*pq)=dom pq by RELAT_1:46,A1,XBOOLE_1:8;
reconsider fpq = f*pq as XFinSequence by C2,A1,XBOOLE_1:8,AFINSQ_1:13;
A3:dom fp=dom p & dom fq = dom q by A1,RELAT_1:46;
A4:dom pq=len p+len q & dom (fp^fq) = len fp+len fq by AFINSQ_1:def 4;
A5:len fpq = len (fp^fq) by C2,RELAT_1:46,A1,XBOOLE_1:8,A3,A4;
k < len fpq implies (fp^fq).k = fpq.k
proof
  assume G1:k< len fpq;
  then G2:k in dom fpq by NAT_1:45;
  per cases;
   suppose k < len p;
      then k in dom p by NAT_1:45;
      then pq.k = p.k & fp.k = f.(p.k) & (fp^fq).k =fp.k
        by A1,A3,AFINSQ_1:def 4,FUNCT_1:23;
      hence thesis by G2,FUNCT_1:22;
   end;
   suppose G3:k >= len p;
      then reconsider kp=k-len p as Element of NAT by NAT_1:21;
      len p + kp < len p+len q by A4,C2,RELAT_1:46,A1,XBOOLE_1:8,G1;
      then
      kp < len q by XREAL_1:9;
      then kp in dom q by NAT_1:45;
      then  pq.k = q.kp & (fp^fq).k = fq.kp & fq.kp = f.(q.kp)
          by AFINSQ_1:21,G1,A1,A2,A3,A4,G3,FUNCT_1:23;
      hence thesis by G2,FUNCT_1:22;
   end;
end;
hence thesis by AFINSQ_1:11,A5;
end;

theorem
  for B1,B2 being finite natural-membered set st
     B1 <N< B2 holds
Sum (SubXFinS(cF,B1\/B2))=Sum (SubXFinS(cF,B1))+Sum(SubXFinS(cF,B2))
proof
  let B1,B2 be finite natural-membered set such that A1: B1 <N< B2;
  set B12=B1\/B2;
  set B12L=B12/\len cF;
  set B1L=B1/\len cF;
  set B2L=B2/\len cF;
  A2:B1L\/B2L=B12L by XBOOLE_1:23;
  B1L <N< B2L by A1,Th36;
  then A3:Sgm0(B12L) = Sgm0(B1L) ^ Sgm0(B2L) by Th46,A2;
rng Sgm0(B1L) = B1L  & rng Sgm0(B2L) = B2L
        by Def5;
  then rng Sgm0(B1L) c= dom cF & rng Sgm0(B2L) c= dom cF by XBOOLE_1:17;
  then SubXFinS (cF,B1) ^ SubXFinS (cF,B2) = SubXFinS (cF,B12) by A3,Th79;
hence thesis by Th65;
end;

