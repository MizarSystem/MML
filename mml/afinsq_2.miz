A22:      l in dom p by NAT_1:45;
A23:      m<=len q-'1 by A21,XREAL_0:def 2;
A24:      now
A25:        k1 = p.l by A15,A19,A22,AFINSQ_1:def 4;
            assume m <> len q -'1;
