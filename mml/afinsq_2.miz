              .=(p^<% k %>).(len p) by NAT_D:34
              .= k by AFINSQ_1:40;
            k1 = p.l by A15,A19,A22,AFINSQ_1:def 4;
            then k1 in Y by A13,A22,FUNCT_1:def 5;
            hence thesis by A9,A20,A27,NAT_1:45;
