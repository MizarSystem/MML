A97: now
      let g be FinSeq-Location;
A98:  (ex m being Element of NAT st m = abs(s.da) & Exec(INS, s ).f = m
|-> 0 )& ex m9 being Element of NAT st m9 =
 abs((s +* Start-At(IC s+k,SCM+FSA)).da)
      & Exec (INS,s +* Start-At(IC s+k,SCM+FSA)).f = m9 |-> 0
       by A94,SCMFSA_2:101;
