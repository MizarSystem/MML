begin :: Incrementing addresses

reserve L, j, k, l, m, n, p, q for Element of NAT,
  A for Data-Location,
  I for Instruction of SCM;

registration
 let a,b be Int-Location;
 cluster a:=b -> ins-loc-free;
 coherence
  proof
     a := b = [1,{},<*a,b*>] by SCMFSA10:9;
   hence JumpPart(a:=b) is empty;
  end;
 cluster AddTo(a,b) -> ins-loc-free;
 coherence
  proof
    AddTo(a,b) = [2,{},<*a,b*>] by SCMFSA10:10;
   hence JumpPart AddTo(a,b) is empty;
  end;
 cluster SubFrom(a,b) -> ins-loc-free;
 coherence
  proof
    SubFrom(a,b) = [3,{},<*a,b*>] by SCMFSA10:11;
   hence JumpPart SubFrom(a,b) is empty;
  end;
 cluster MultBy(a,b) -> ins-loc-free;
 coherence
  proof
    MultBy(a,b) = [4,{},<*a,b*>] by SCMFSA10:12;
   hence JumpPart MultBy(a,b) is empty;
  end;
 cluster Divide(a,b) -> ins-loc-free;
 coherence
  proof
    Divide(a,b) = [5,{},<*a,b*>] by SCMFSA10:13;
   hence JumpPart Divide(a,b) is empty;
  end;
end;

canceled 7;

theorem Th8:
  for k being Element of NAT holds IncAddr(halt SCM+FSA,k) = halt
  SCM+FSA by COMPOS_1:93;

theorem Th9:
  for k being Element of NAT, a,b being Int-Location
   holds IncAddr(a:=b,k) = a:=b by COMPOS_1:92;

theorem Th10:
  for k being Element of NAT, a,b being Int-Location holds IncAddr
  (AddTo(a,b),k) = AddTo(a,b) by COMPOS_1:92;

theorem Th11:
