        .= Exec(IncAddr(INS, k), s).d by A90,SCMFSA_2:101
        .= (Exec(IncAddr(INS,k), s) +* Start-At (IC Exec(IncAddr(INS,k),s)
      -'k,SCM+FSA)).d by SCMFSA_3:11;
    end;
A92: now
      let d be FinSeq-Location;
      per cases;
      suppose
A93:    f = d;
A94:    (s +* Start-At(IC s -'k,SCM+FSA)).da = s.da by SCMFSA_3:11;
        (ex m being Element of NAT st m = abs(s.da) & Exec( IncAddr(INS,
k), s).f = m |-> 0 )& ex m9 being Element of NAT st m9 = abs((s +* Start-At (IC
s -'k,SCM+FSA)). da) & Exec(INS,s +* Start-At(IC s -'k,SCM+FSA)).f = m9 |-> 0
 by A89,A90,SCMFSA_2:101;
        hence
        Exec(INS, s +* Start-At(IC s -'k,SCM+FSA)).d = (Exec(IncAddr(INS,k), s)
+* Start-At (IC Exec(IncAddr(INS,k),s) -'k,SCM+FSA)).d by A93,A94,SCMFSA_3:12;
      end;
      suppose
A95:    f <> d;
        hence Exec(INS, s +* Start-At(IC s -'k,SCM+FSA)).d =
         (s +* Start-At (IC s -'
        k,SCM+FSA)).d by A89,SCMFSA_2:101
          .= s.d by SCMFSA_3:12
          .= Exec(IncAddr(INS, k), s).d by A90,A95,SCMFSA_2:101
          .= (Exec(IncAddr(INS,k), s) +* Start-At (IC Exec(IncAddr(INS,k),s)
        -'k,SCM+FSA)).d by SCMFSA_3:12;
      end;
    end;
    IC Exec(INS, s +* Start-At(IC s -'k,SCM+FSA)) = succ IC (s +* Start-At (IC
    s -'k,SCM+FSA)) by A89,SCMFSA_2:101
      .= ((succ IC s) ) -' k by A6,FUNCT_4:121
      .= IC Exec(IncAddr(INS,k), s) -' k by A90,SCMFSA_2:101
      .= IC (Exec(IncAddr(INS,k),s) +* Start-At (IC Exec(IncAddr(INS,k),s)
    -'k,SCM+FSA)) by FUNCT_4:121;
    hence thesis by A1,A91,A92,SCMFSA_2:86;
  end;
end;

theorem
 not InsCode i in {6,7,8} implies IncAddr(i,k) = i
proof
 assume not InsCode i in {6,7,8};
  then
Z: InsCode i <> 6 & InsCode i <> 7 & InsCode i <> 8 by ENUMSET1:def 1;
A4: InsCode i <= 12 by SCMFSA_2:35;
  per cases by Z,A4,NAT_1:37;
  suppose InsCode i = 0;
    then i = halt SCM+FSA by SCMFSA_2:122;
   hence thesis by COMPOS_1:93;
  end;
  suppose
    InsCode i = 1;
    then consider da,db being Int-Location such that
A8: i = da := db by SCMFSA_2:54;
   thus thesis by A8,COMPOS_1:92;
  end;
  suppose
    InsCode i = 2;
    then consider da,db being Int-Location such that
A14: i = AddTo(da, db) by SCMFSA_2:55;
    thus thesis by A14,COMPOS_1:92;
  end;
  suppose
    InsCode i = 3;
    then consider da,db being Int-Location such that
A20: i = SubFrom(da, db) by SCMFSA_2:56;
    thus thesis by A20,COMPOS_1:92;
  end;
  suppose
    InsCode i = 4;
    then consider da,db being Int-Location such that
A26: i = MultBy(da, db) by SCMFSA_2:57;
    thus thesis by A26,COMPOS_1:92;
  end;
  suppose
    InsCode i = 5;
    then consider da,db being Int-Location such that
A32: i = Divide(da, db) by SCMFSA_2:58;
    thus thesis by A32,COMPOS_1:92;
  end;
  suppose
    InsCode i = 9;
    then consider db,da being Int-Location, f being FinSeq-Location such that
A70: i = da :=(f, db) by SCMFSA_2:62;
    thus thesis by A70,COMPOS_1:92;
  end;
  suppose
    InsCode i = 10;
    then consider db,da being Int-Location, f being FinSeq-Location such that
A79: i = (f, db):=da by SCMFSA_2:63;
    thus thesis by A79,COMPOS_1:92;
  end;
  suppose
    InsCode i = 11;
    then consider da being Int-Location, f being FinSeq-Location such that
A88: i = da :=len f by SCMFSA_2:64;
    thus thesis by A88,COMPOS_1:92;
  end;
  suppose
    InsCode i = 12;
    then consider da being Int-Location, f being FinSeq-Location such that
A94: i = f:=<0,...,0>da by SCMFSA_2:65;
   thus thesis by A94,COMPOS_1:92;
  end;
 end;
