::
::       Bounded above, bounded below and bounded sets of r_eal numbers
::

definition let X be ext-real-membered set;
  attr X is bounded_above means :Def11:
    ex UB being majorant of X st UB in REAL;
end;

canceled;

theorem Th44:
  for X,Y being ext-real-membered set st X c= Y & Y is bounded_above holds
    X is bounded_above
proof
   let X,Y be ext-real-membered set;
   assume
A1:X c= Y;
   assume Y is bounded_above;
   then ex UB being majorant of Y st UB in REAL by Def11;
   then consider UB being ext-real number such that
A2:UB is majorant of Y & UB in REAL;
   UB is majorant of X & UB in REAL by A1,A2,Th34;
   hence thesis by Def11;
end;

theorem
  not REAL is bounded_above
proof
   for X being non empty ext-real-membered set holds
   X = REAL implies not X is bounded_above
   proof
      let X be non empty ext-real-membered set;
      assume
   A1:X = REAL;
      assume X is bounded_above;
      then consider UB being majorant of X such that
   A2:UB in REAL by Def11;
      reconsider UB as Real by A2;
      consider x being real number such that
   A3:UB < x by XREAL_1:3;
      reconsider x as Real by XREAL_0:def 1;
      reconsider x as ext-real number by XXREAL_0:def 1;
      reconsider UB as ext-real number;
   A4:x <= UB by A1,Def9;
      consider a,b being ext-real number;
      thus thesis by A3,A4;
   end;
   hence thesis;
end;

definition let X be ext-real-membered set;
  attr X is bounded_below means :Def12:
    ex LB being minorant of X st LB in REAL;
end;

canceled;

theorem Th47:
  for X,Y being ext-real-membered set st
   X c= Y & Y is bounded_below holds X is bounded_below
proof
   let X,Y be ext-real-membered set;
   assume
A1:X c= Y;
   assume Y is bounded_below;
then ex LB being minorant of Y st LB in REAL by Def12;
   then consider LB being ext-real number such that
A2:LB is minorant of Y & LB in REAL;
   LB is minorant of X & LB in REAL by A1,A2,Th39;
   hence thesis by Def12;
end;

theorem
  not REAL is bounded_below
proof
   for X being non empty ext-real-membered set holds
   X = REAL implies not X is bounded_below
   proof
      let X be non empty ext-real-membered set;
      assume
   A1:X = REAL;
      assume X is bounded_below;
      then consider LB being minorant of X such that
   A2:LB in REAL by Def12;
      reconsider LB as Real by A2;
      consider x being real number such that
   A3:x < LB by XREAL_1:4;
      reconsider x as Real by XREAL_0:def 1;
      reconsider x as ext-real number by XXREAL_0:def 1;
      reconsider LB as ext-real number;
   A4:LB <= x by A1,Def10;
      consider a,b being ext-real number;
      thus thesis by A3,A4;
   end;
   hence thesis;
end;

definition let X be ext-real-membered set;
  attr X is bounded means :Def13:
    X is bounded_above bounded_below;
end;

canceled;

theorem
  for X,Y being ext-real-membered set st
   X c= Y & Y is bounded holds X is bounded
proof
   let X,Y be ext-real-membered set;
   assume
A1:X c= Y;
   assume Y is bounded;
   then Y is bounded_above bounded_below by Def13;
   then X is bounded_above bounded_below by A1,Th44,Th47;
   hence thesis by Def13;
end;

theorem Th51:
   for X being ext-real-membered set holds
   ex Y being non empty ext-real-membered set st
   for x being ext-real number holds x in Y iff x is majorant of X
proof
   let X be ext-real-membered set;
   defpred P[ext-real number] means $1 is majorant of X;
   consider Y being ext-real-membered set such that
A1:for x being ext-real number holds x in Y iff P[x] from MEMBERED:sch 2;
    +infty is majorant of X by Th33;
   then reconsider Y as non empty set by A1;
   reconsider Y as non empty ext-real-membered set;
   take Y;
   thus thesis by A1;
end;

::
::       Set of majorant and set of minorant of X being a subset of ExtREAL
::

definition let X be ext-real-membered set;
  func SetMajorant(X) -> ext-real-membered set means :Def14:
    for x being ext-real number holds x in it iff x is majorant of X;
existence
  proof
   consider Y being non empty ext-real-membered set such that
A1:   for x being ext-real number holds x in Y iff x is majorant of X by Th51;
  take Y;
  thus thesis by A1;
  end;
uniqueness
proof
   let Y1,Y2 be ext-real-membered set such that
A2:for x being ext-real number holds
   x in Y1 iff x is majorant of X and
A3:for x being ext-real number holds
   x in Y2 iff x is majorant of X;
      let x be ext-real number;
      x in Y1 iff x is majorant of X by A2;
      hence thesis by A3;
end;
end;

registration let X be ext-real-membered set;
  cluster SetMajorant(X) -> non empty;
  coherence
  proof
    consider x being majorant of X;
    x in SetMajorant(X) by Def14;
    hence thesis;
  end;
end;

canceled 2;

theorem
  for X,Y being ext-real-membered set st X c= Y holds
    for x being ext-real number
     holds x in SetMajorant Y implies x in SetMajorant(X)
proof
   let X,Y be ext-real-membered set;
   assume
A1:X c= Y;
   let x be ext-real number;
   assume x in SetMajorant(Y);
   then x is majorant of Y by Def14;
   then x is majorant of X by A1,Th34;
   hence thesis by Def14;
end;

theorem Th55:
  for X being ext-real-membered set holds
   ex Y being non empty ext-real-membered set st
   for x being ext-real number holds x in Y iff x is minorant of X
proof
   let X be ext-real-membered set;
   defpred P[ext-real number] means $1 is minorant of X;
   consider Y being ext-real-membered set such that
A1:for x being ext-real number holds x in Y iff P[x] from MEMBERED:sch 2;
   -infty is minorant of X by Th36;
   then reconsider Y as non empty set by A1;
   reconsider Y as non empty ext-real-membered set;
   take Y;
   thus thesis by A1;
end;

definition let X be ext-real-membered set;
  func SetMinorant(X) -> ext-real-membered set means :Def15:
    for x being ext-real number holds x in it iff x is minorant of X;
existence
 proof
  consider Y being non empty ext-real-membered set such that
A1:  for x being ext-real number holds x in Y iff x is minorant of X by Th55;
