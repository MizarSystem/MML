canceled 2;

theorem
  for X,Y being ext-real-membered set st X c= Y holds
   for x being ext-real number
    holds x in SetMinorant(Y) implies x in SetMinorant(X)
proof
   let X,Y be ext-real-membered set;
   assume
A1:X c= Y;
   let x be ext-real number;
   assume x in SetMinorant(Y);
   then x is minorant of Y by Def15;
   then x is minorant of X by A1,Th39;
   hence thesis by Def15;
end;

theorem Th59:
   for X being non empty ext-real-membered set st
   X is bounded_above & X <> {-infty} holds
   ex x being Real st x in X & x <> -infty
proof
   let X be non empty ext-real-membered set;
   assume
A1:X is bounded_above & X <> {-infty};
   ex x being set st x in X & x <> -infty
   proof
      assume
A2:   not ex x being set st x in X & x <> -infty;
      for x being set holds x in X implies x in {-infty}
      proof
        let x be set;
        assume x in X;
        then x = -infty by A2;
        hence thesis by TARSKI:def 1;
      end;
      then X c= {-infty} by TARSKI:def 3;
      hence thesis by A1,ZFMISC_1:39;
   end;
   then consider x being set such that
A3:x in X & x <> -infty;
   x in REAL
   proof
A4:  x <> +infty
     proof
       assume
    A5:x = +infty;
       then reconsider x as ext-real number;
       consider UB0 being majorant of X such that
    A6:UB0 in REAL by A1,Def11;
       reconsider UB0 as Real by A6;
       reconsider UB0 as ext-real number;
       x <= UB0 by A3,Def9;
       hence thesis by A5,XXREAL_0:9;
     end;
     X c= ExtREAL by MEMBERED:2;
     then x in REAL or x in {-infty,+infty} by A3,XBOOLE_0:def 2;
     hence thesis by A3,A4,TARSKI:def 2;
   end;
   then reconsider x as Real;
   take x;
   thus thesis by A3;
end;

theorem Th60:
   for X being non empty ext-real-membered set st
   X is bounded_below & not X = {+infty} holds
   ex x being Real st x in X & x <> +infty
proof
   let X be non empty ext-real-membered set;
   assume
A1:X is bounded_below & X <> {+infty};
   ex x being set st x in X & x <> +infty
   proof
      assume
A2:   not ex x being set st x in X & x <> +infty;
      for x being set holds x in X implies x in {+infty}
      proof
        let x be set;
        assume x in X;
        then x = +infty by A2;
        hence thesis by TARSKI:def 1;
      end;
      then X c= {+infty} by TARSKI:def 3;
      hence thesis by A1,ZFMISC_1:39;
   end;
   then consider x being set such that
A3:x in X & x <> +infty;
   x in REAL
   proof
   A4:x <> -infty
      proof
        assume
     A5:x = -infty;
        then reconsider x as ext-real number;
        consider LB0 being minorant of X such that
     A6:LB0 in REAL by A1,Def12;
        reconsider LB0 as Real by A6;
        reconsider LB0 as ext-real number;
        LB0 <= x by A3,Def10;
        hence thesis by A5,XXREAL_0:12;
      end;
       X c= ExtREAL by MEMBERED:2;
       then x in REAL or x in {-infty,+infty} by A3,XBOOLE_0:def 2;
      hence thesis by A3,A4,TARSKI:def 2;
   end;
   then reconsider x as Real;
   take x;
   thus thesis by A3;
end;

canceled;

theorem Th62:
   for X being non empty ext-real-membered set st
   X is bounded_above & X <> {-infty} holds
   ex UB being ext-real number st UB is majorant of X &
   for y being ext-real number st y is majorant of X holds UB <= y
proof
   let X be non empty ext-real-membered set;
   assume
A1:X is bounded_above & X <> {-infty};
   then consider UB0 being majorant of X such that
A2:UB0 in REAL by Def11;
A3:UB0 in SetMajorant(X) by Def14;
   consider x being Real such that
A4:x in X & x <> -infty by A1,Th59;
   reconsider S = X /\ REAL as Subset of REAL by XBOOLE_1:17;
   reconsider Y = REAL /\ SetMajorant(X) as Subset of REAL by XBOOLE_1:17;
   reconsider UB0 as Real by A2;
A5:x in S by A4,XBOOLE_0:def 3;
      now let a,b be real number;
       assume
   A6:a in S & b in Y;
   then A7:a in X by XBOOLE_0:def 3;
   A8:b in SetMajorant(X) by A6,XBOOLE_0:def 3;
A9:  a is Real & b is Real by XREAL_0:def 1;
    reconsider d = b as ext-real number by XXREAL_0:def 1;
A10:d is majorant of X by A8,Def14;
    reconsider c = a as ext-real number by XXREAL_0:def 1;
    c <= d by A7,A10,Def9;
    then ex p,q being Real st p = c & q = d & p <= q by A9;
    hence a <= b;
   end;
   then consider UB being real number such that
A11:for a,b being real number st a in S & b in Y holds a <= UB & UB <= b
       by AXIOMS:26;
    set d = UB;
    reconsider UB as Real by XREAL_0:def 1;
    reconsider UB as ext-real number by XXREAL_0:def 1;
A12:now let a,b be ext-real number;
       assume
   A13:a in X & b in SetMajorant(X);
       thus a <= UB & UB <= b
       proof
       A14:(not a = -infty & b = +infty) implies a <= UB & UB <= b
           proof
             assume
         A15:a <> -infty & b = +infty;
             a in REAL
             proof
           A16:a <> +infty
               proof
                 assume
             A17:a = +infty;
                 reconsider UB0 as ext-real number;
                 a <= UB0 by A13,Def9;
                 hence thesis by A17,XXREAL_0:9;
               end;
               a in ExtREAL by XXREAL_0:def 1;
               then a in REAL or a in {-infty,+infty} by XBOOLE_0:def 2;
               hence thesis by A15,A16,TARSKI:def 2;
             end;
             then reconsider a as Real;
         A18:a in S by A13,XBOOLE_0:def 3;
             a <= d & d <= UB0
             proof
               UB0 in Y by A3,XBOOLE_0:def 3;
               hence thesis by A11,A18;
             end;
             hence thesis by A15,XXREAL_0:4;
           end;
      A19:(a = -infty & not b = +infty) implies a <= UB & UB <= b
           proof
             assume
         A20:a = -infty & b <> +infty;
A21:         b is majorant of X by A13,Def14;
             reconsider x as ext-real number by XXREAL_0:def 1;
             x <= b by A4,A21,Def9;then
         A22:b <> -infty by XXREAL_0:12;
             b in ExtREAL by XXREAL_0:def 1;
             then b in REAL or b in {-infty,+infty} by XBOOLE_0:def 2;
             then reconsider b as Real by A20,A22,TARSKI:def 2;
         A23:b in Y by A13,XBOOLE_0:def 3;
             reconsider x as Real;
             x <= d & d <= b by A5,A11,A23;
             hence thesis by A20,XXREAL_0:5;
           end;
           (a <> -infty & b <> +infty) implies a <= UB & UB <= b
           proof
             assume
         A24:a <> -infty & b <> +infty;
             a in REAL
             proof
           A25:a <> +infty
               proof
                 assume
             A26:a = +infty;
                 reconsider UB0 as ext-real number;
                 a <= UB0 by A13,Def9;
                 hence thesis by A26,XXREAL_0:9;
               end;
             a in ExtREAL by XXREAL_0:def 1;
               then a in REAL or a in {-infty,+infty} by XBOOLE_0:def 2;
               hence thesis by A24,A25,TARSKI:def 2;
             end;
             then reconsider a as Real;
A27:         b is majorant of X by A13,Def14;
             reconsider x as ext-real number by XXREAL_0:def 1;
             x <= b by A4,A27,Def9;
             then A28:not b = -infty by XXREAL_0:12;
             b in ExtREAL by XXREAL_0:def 1;
             then b in REAL or b in {-infty,+infty} by XBOOLE_0:def 2;
             then reconsider b as Real by A24,A28,TARSKI:def 2;
         A29:b in Y by A13,XBOOLE_0:def 3;
             a in S by A13,XBOOLE_0:def 3;
             then a <= d & d <= b by A11,A29;
             hence thesis;
           end;
           hence thesis by A14,A19,XXREAL_0:3,5;
       end;
   end;
A30:UB is majorant of X
   proof
      for a being ext-real number st a in X holds a <= UB by A3,A12;
      hence thesis by Def9;
   end;
A31:for y being ext-real number st y is majorant of X holds UB <= y
   proof
     let y be ext-real number;
     assume y is majorant of X;
     then y in SetMajorant(X) by Def14;
     hence thesis by A4,A12;
   end;
   take UB;
   thus thesis by A30,A31;
end;

theorem Th63:
   for X being non empty ext-real-membered set st
   X is bounded_below & X <> {+infty} holds
   ex LB being ext-real number st LB is minorant of X &
   (for y being ext-real number st y is minorant of X holds y <= LB)
proof
   let X be non empty ext-real-membered set;
   assume
A1:X is bounded_below & X <> {+infty};
   then consider LB0 being minorant of X such that
A2:LB0 in REAL by Def12;
A3:LB0 in SetMinorant(X) by Def15;
   consider x being Real such that
A4:x in X & not x = +infty by A1,Th60;
   reconsider S = X /\ REAL as Subset of REAL by XBOOLE_1:17;
