definition let x,y be ext-real number;
 redefine
 canceled 2;

  pred x <= y means
    ex p,q being Real st p = x & q = y & p <= q if x in REAL & y in REAL
    otherwise x = -infty or y = +infty;
  consistency;
  compatibility
   proof
F:   x in ExtREAL by XXREAL_0:def 1;
G:   y in ExtREAL by XXREAL_0:def 1;
    thus x in REAL & y in REAL implies
     (x <= y iff ex p,q being Real st p = x & q = y & p <= q);
    thus not(x in REAL & y in REAL) implies
     (x <= y iff x = -infty or y = +infty)
     proof assume
A1:     not(x in REAL & y in REAL);
      per cases by A1;
      suppose not x in REAL;
        then
A2:       x in {-infty,+infty} by XBOOLE_0:def 2,F;
       thus x <= y implies x = -infty or y = +infty
        proof assume
A3:        x <= y;
         assume x <> -infty;
          then x = +infty by A2,TARSKI:def 2;
         hence thesis by A3,XXREAL_0:4;
        end;
       thus thesis by XXREAL_0:3,5;
      end;
      suppose not y in REAL;
        then
A4:       y in {-infty,+infty} by XBOOLE_0:def 2,G;
       thus x <= y implies x = -infty or y = +infty
        proof assume
A5:        x <= y;
         assume
A6:        x <> -infty;
         assume y <> +infty;
          then y = -infty by A4,TARSKI:def 2;
         hence thesis by A5,XXREAL_0:6,A6;
        end;
       thus thesis by XXREAL_0:3,5;
      end;
     end;
   end;
  end;

scheme SepReal {P[R_eal]}:
   ex X being Subset of ExtREAL st
   for x being R_eal holds x in X iff P[x]
proof defpred Q[set] means ex x being R_eal st x=$1 & P[x];
   consider X being set such that
A1:for r being set holds r in X iff r in REAL \/ {-infty,+infty} & Q[r]
     from XBOOLE_0:sch 1;
   X c= REAL \/ {-infty,+infty}
   proof
     let r be set;
     assume r in X;
     hence thesis by A1;
   end;
   then reconsider X as Subset of ExtREAL by MEMBERED:20;
   take X;
   let x be R_eal;
   hereby assume x in X;
     then ex y being R_eal st y = x & P[y] by A1;
     hence P[x];
   end;
   assume A2: P[x];
   thus thesis by A1,A2;
end;

