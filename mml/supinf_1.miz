  thus thesis by A1;
 end;
uniqueness
proof
   let Y1,Y2 be ext-real-membered set such that
A2:for x being ext-real number holds
   x in Y1 iff x is minorant of X and
A3:for x being ext-real number holds
   x in Y2 iff x is minorant of X;
     let x be ext-real number;
     x in Y1 iff x is minorant of X by A2;
     hence thesis by A3;
end;
end;

registration let X be ext-real-membered set;
