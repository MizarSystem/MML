::
::       sup X, inf X least upper bound and greatest lower bound of set X
::

definition let X be non empty ext-real-membered set;
  func sup X -> ext-real number means :Def16:
    it is majorant of X &
    for y being ext-real number st y is majorant of X holds it <= y;
existence by Th72;
uniqueness
proof
   let S1,S2 be ext-real number such that
A1:S1 is majorant of X &
   for y being ext-real number st
   y is majorant of X holds S1 <= y and
A2:S2 is majorant of X &
   for y being ext-real number st y is majorant of X holds S2 <= y;
   S1 <= S2 & S2 <= S1 by A1,A2;
   hence thesis by XXREAL_0:1;
end;
end;

