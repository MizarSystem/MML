begin :: arctan,arccot

reserve x, r, s, h for Real,


  n for Element of NAT,
  rr, y for set,
  Z for open Subset of REAL,

  f, f1, f2 for PartFunc of REAL,REAL;

theorem Th1:
  ].-PI/2,PI/2.[ c= dom tan
proof
  ].-PI/2,PI/2.[ /\ cos"{0}={}
  proof
    assume ].-PI/2,PI/2.[ /\ cos"{0}<>{};
    then consider rr such that
A1: rr in ].-PI/2,PI/2.[ /\ cos"{0} by XBOOLE_0:7;
    rr in cos"{0} by A1,XBOOLE_0:def 4;
    then
A2: cos.rr in {0} by FUNCT_1:def 13;
    rr in ].-PI/2,PI/2.[ by A1,XBOOLE_0:def 4;
    then cos.rr <>0 by COMPTRIG:27;
    hence contradiction by A2,TARSKI:def 1;
  end;
  then
A3: ].-PI/2,PI/2.[ misses cos"{0} by XBOOLE_0:def 7;
  ].-PI/2,PI/2.[ \ cos"{0} c= dom cos \ cos"{0} by SIN_COS:27,XBOOLE_1:33;
  then ].-PI/2,PI/2.[ c= dom cos \ cos"{0} by A3,XBOOLE_1:83;
  then ].-PI/2,PI/2.[ c= dom sin /\ (dom cos \ cos"{0}) by SIN_COS:27,
  XBOOLE_1:19;
  hence thesis by RFUNCT_1:def 4;
end;

theorem Th2:
  ].0,PI.[ c= dom cot
proof
  ].0,PI.[ /\ sin"{0}={}
  proof
    assume ].0,PI.[ /\ sin"{0}<>{};
    then consider rr such that
A1: rr in ].0,PI.[ /\ sin"{0} by XBOOLE_0:7;
    rr in sin"{0} by A1,XBOOLE_0:def 4;
    then
A2: sin.rr in {0} by FUNCT_1:def 13;
    rr in ].0,PI.[ by A1,XBOOLE_0:def 4;
    then sin.rr <>0 by COMPTRIG:23;
    hence contradiction by A2,TARSKI:def 1;
  end;
  then
A3: ].0,PI.[ misses sin"{0} by XBOOLE_0:def 7;
  ].0,PI.[ \ sin"{0} c= dom sin \ sin"{0} by SIN_COS:27,XBOOLE_1:33;
  then ].0,PI.[ c= dom sin \ sin"{0} by A3,XBOOLE_1:83;
  then ].0,PI.[ c= dom cos /\ (dom sin \ sin"{0}) by SIN_COS:27,XBOOLE_1:
  19;
  hence thesis by RFUNCT_1:def 4;
end;

Lm1: tan is_differentiable_on ].-PI/2,PI/2.[
proof
  for x st x in ].-PI/2,PI/2.[ holds tan is_differentiable_in x
  proof
    let x;
    assume x in ].-PI/2,PI/2.[;
    then cos.x <> 0 by COMPTRIG:27;
    hence thesis by FDIFF_7:46;
  end;
  hence thesis by Th1,FDIFF_1:16;
end;

Lm2: cot is_differentiable_on ].0,PI.[
proof
  for x st x in ].0,PI.[ holds cot is_differentiable_in x
  proof
    let x;
    assume x in ].0,PI.[;
    then sin.x <> 0 by COMPTRIG:23;
    hence thesis by FDIFF_7:47;
  end;
  hence thesis by Th2,FDIFF_1:16;
end;

Lm3: for x st x in ].-PI/2,PI/2.[ holds diff(tan,x) = 1/(cos.x)^2
proof
  let x;
  assume x in ].-PI/2,PI/2.[;
  then cos.x <>0 by COMPTRIG:27;
  hence thesis by FDIFF_7:46;
end;

Lm4: for x st x in ].0,PI.[ holds diff(cot,x) = -1/(sin.x)^2
proof
  let x;
  assume x in ].0,PI.[;
  then sin.x <>0 by COMPTRIG:23;
  hence thesis by FDIFF_7:47;
end;

theorem
  tan is_differentiable_on ].-PI/2,PI/2.[ & for x st x in ].-PI/2,PI/2.[
  holds diff(tan,x) = 1/(cos.x)^2 by Lm1,Lm3;

theorem
  cot is_differentiable_on ].0,PI.[ & for x st x in ].0,PI.[ holds diff(
  cot,x) = -1/(sin.x)^2 by Lm2,Lm4;

theorem
  tan|].-PI/2,PI/2.[ is continuous by Lm1,FDIFF_1:33;

theorem
  cot|].0,PI.[ is continuous by Lm2,FDIFF_1:33;

theorem Th7:
  tan|].-PI/2,PI/2.[ is increasing
proof
A1: for x be Real st x in ].-PI/2,PI/2.[ holds diff(tan,x) > 0
  proof
    let x be Real;
    assume
A2: x in ].-PI/2,PI/2.[;
    then 0 < cos.x by COMPTRIG:27;
    then (cos.x)^2 > 0 by SQUARE_1:74;
    then 1/(cos.x)^2 > 0 /(cos.x)^2 by XREAL_1:76;
    hence thesis by A2,Lm3;
  end;
  ].-PI/2,PI/2.[ c= dom tan by Lm1,FDIFF_1:def 7;
  hence thesis by A1,Lm1,ROLLE:9;
end;

theorem Th8:
  cot|].0,PI.[ is decreasing
proof
A1: for x be Real st x in ].0,PI.[ holds diff(cot,x) < 0
  proof
    let x be Real;
    assume
A2: x in ].0,PI.[;
    then 0 < sin.x by COMPTRIG:23;
    then (sin.x)^2 > 0 by SQUARE_1:74;
    then 1/(sin.x)^2 > 0 /(sin.x)^2 by XREAL_1:76;
    then -1/(sin.x)^2 < -0 by XREAL_1:26;
    hence thesis by A2,Lm4;
  end;
  ].0,PI.[ c= dom cot by Lm2,FDIFF_1:def 7;
  hence thesis by A1,Lm2,ROLLE:10;
end;

theorem
  tan | ].-PI/2,PI/2.[ is one-to-one by Th7,FCONT_3:16;

theorem
  cot | ].0,PI.[ is one-to-one by Th8,FCONT_3:16;

registration
  cluster tan | ].-PI/2,PI/2.[ -> one-to-one;
  coherence by Th7,FCONT_3:16;
  cluster cot | ].0,PI.[ -> one-to-one;
  coherence by Th8,FCONT_3:16;
end;

definition
  func arctan -> PartFunc of REAL,REAL equals
  (tan | ].-PI/2,PI/2.[)";
  coherence;
  func arccot -> PartFunc of REAL, REAL equals
  (cot | ].0,PI.[)";
  coherence;
end;

definition
  let r be Real;
  func arctan r equals
  arctan.r;
  coherence;
  func arccot r equals
  arccot.r;
  coherence;
end;

definition
  let r be Real;
  redefine func arctan r -> Real;
  coherence;
  redefine func arccot r -> Real;
  coherence;
end;

Lm5: (arctan qua Function)" = tan | ].-PI/2,PI/2.[ by FUNCT_1:65;

Lm6: (arccot qua Function)" = cot | ].0,PI.[ by FUNCT_1:65;

theorem Th11:
  rng arctan = ].-PI/2,PI/2.[
proof
  dom (tan|].-PI/2,PI/2.[) = ].-PI/2,PI/2.[ by Th1,RELAT_1:91;
  hence thesis by FUNCT_1:55;
end;

theorem Th12:
  rng arccot = ].0,PI.[
proof
  dom (cot|].0,PI.[) = ].0,PI.[ by Th2,RELAT_1:91;
  hence thesis by FUNCT_1:55;
end;

registration
  cluster arctan -> one-to-one;
  coherence;
  cluster arccot -> one-to-one;
  coherence;
end;

Lm7: -PI/4 in ].-PI/2,PI/2.[
proof
  PI in ].0,4.[ by SIN_COS:def 32;
  then PI > 0 by XXREAL_1:4;
  then PI/(-4) > PI/(-2) by XREAL_1:101;
  then -PI/4 in { s where s is Real: -PI/2 < s & s < PI/2 };
  hence thesis by RCOMP_1:def 2;
end;

Lm8: PI/4 in ].-PI/2,PI/2.[
proof
  PI in ].0,4.[ by SIN_COS:def 32;
  then PI > 0 by XXREAL_1:4;
  then PI/4 < PI/2 by XREAL_1:78;
  then PI/4 in { s where s is Real: -PI/2 < s & s < PI/2 };
  hence thesis by RCOMP_1:def 2;
end;

Lm9: PI/4 in ].0,PI.[
proof
  PI in ].0,4.[ by SIN_COS:def 32;
  then
A1: PI > 0 by XXREAL_1:4;
  then
A2: PI/4 < PI/1 by XREAL_1:78;
  PI/4 > 0/4 by A1,XREAL_1:76;
  hence thesis by A2,XXREAL_1:4;
end;

Lm10: 3/4*PI in ].0,PI.[
proof
  PI in ].0,4.[ by SIN_COS:def 32;
  then
A1: PI > 0 by XXREAL_1:4;
  then
A2: 3/4*PI < PI by XREAL_1:159;
  (3/4)*PI > (3/4)*0 by A1,XREAL_1:70;
  hence thesis by A2,XXREAL_1:4;
end;

Lm11: dom (tan | [.-PI/4,PI/4.]) = [.-PI/4,PI/4.]
proof
  [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  hence thesis by Th1,RELAT_1:91,XBOOLE_1:1;
end;

Lm12: dom (cot | [.PI/4,3/4*PI.]) = [.PI/4,3/4*PI.]
proof
  [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
  hence thesis by Th2,RELAT_1:91,XBOOLE_1:1;
end;

theorem Th13:
  for x be real number st x in ].-PI/2,PI/2.[ holds tan.x = tan x
proof
  let x be real number;
  assume x in ].-PI/2,PI/2.[;
  then tan.x = (sin x)/(cos x) by Th1,RFUNCT_1:def 4
    .= tan x by SIN_COS4:def 1;
  hence thesis;
end;

theorem Th14:
  for x be real number st x in ].0,PI.[ holds cot.x = cot x
proof
  let x be real number;
  assume x in ].0,PI.[;
  then cot.x = (cos x)/(sin x) by Th2,RFUNCT_1:def 4
    .= cot x by SIN_COS4:def 2;
  hence thesis;
end;

theorem
  for x be Real st cos.x <> 0 holds tan.x = tan x
proof
  let x be Real;
  assume
A1: cos.x <> 0;
  not x in cos"{0}
  proof
    assume x in cos"{0};
    then cos.x in {0} by FUNCT_1:def 13;
    hence contradiction by A1,TARSKI:def 1;
  end;
  then x in dom cos \ cos"{0} by SIN_COS:27,XBOOLE_0:def 5;
  then x in dom sin /\ (dom cos \ cos"{0}) by SIN_COS:27,XBOOLE_0:def 4;
  then x in dom (sin/cos) by RFUNCT_1:def 4;
  then tan.x = (sin x)/(cos x) by RFUNCT_1:def 4
    .= tan x by SIN_COS4:def 1;
  hence thesis;
end;

theorem
  for x be Real st sin.x <> 0 holds cot.x = cot x
proof
  let x be Real;
  assume
A1: sin.x <> 0;
  not x in sin"{0}
  proof
    assume x in sin"{0};
    then sin.x in {0} by FUNCT_1:def 13;
    hence contradiction by A1,TARSKI:def 1;
  end;
  then x in dom sin \ sin"{0} by SIN_COS:27,XBOOLE_0:def 5;
  then x in dom cos /\ (dom sin \ sin"{0}) by SIN_COS:27,XBOOLE_0:def 4;
  then x in dom (cos/sin) by RFUNCT_1:def 4;
  then cot.x = (cos x)/(sin x) by RFUNCT_1:def 4
    .= cot x by SIN_COS4:def 2;
  hence thesis;
end;

theorem Th17:
  tan.(-PI/4) = -1 & tan(-PI/4) = -1
proof
  cos.(PI/4) <> 0 by Lm8,COMPTRIG:27;
  then
A1: sin.(PI/4)/cos.(PI/4) = 1 by SIN_COS:78,XCMPLX_1:60;
  tan.(-PI/4) = sin.(-PI/4)/(cos.(-PI/4)) by Lm7,Th1,RFUNCT_1:def 4
    .= (-sin.(PI/4))/cos.(-PI/4) by SIN_COS:33
    .= (-sin.(PI/4))/cos.(PI/4) by SIN_COS:33
    .= -1 by A1;
  hence thesis by Lm7,Th13;
end;

theorem Th18:
  cot.(PI/4) = 1 & cot(PI/4) = 1 & cot.(3/4*PI) = -1 & cot(3/4*PI) = -1
proof
A1: sin.(PI/4) <> 0 by Lm9,COMPTRIG:23;
A2: cot.(3/4*PI) = cos.(3/4*PI)*(sin.(3/4*PI))" by Lm10,Th2,RFUNCT_1:def 4
    .= (-sin.(PI/4))/sin.(PI/2 + PI/4) by SIN_COS:83
    .= (-sin.(PI/4))/cos.(PI/4) by SIN_COS:83
    .= -sin.(PI/4)/cos.(PI/4)
    .= -1 by A1,SIN_COS:78,XCMPLX_1:60;
  cot.(PI/4) = cos.(PI/4)/(sin.(PI/4)) by Lm9,Th2,RFUNCT_1:def 4
    .= 1 by A1,SIN_COS:78,XCMPLX_1:60;
  hence thesis by A2,Lm9,Lm10,Th14;
end;

theorem Th19:
  for x be set st x in [.-PI/4,PI/4.] holds tan.x in [.-1,1.]
proof
  let x be set;
  assume x in [.-PI/4,PI/4.];
  then x in ].-PI/4,PI/4.[ \/ {-PI/4,PI/4} by XXREAL_1:128;
  then
A1: x in ].-PI/4,PI/4.[ or x in {-PI/4,PI/4} by XBOOLE_0:def 3;
  per cases by A1,TARSKI:def 2;
  suppose
A2: x in ].-PI/4,PI/4.[;
    then x in { s where s is Real: -PI/4 < s & s < PI/4 } by
    RCOMP_1:def 2;
    then
A3: ex s be Real st s=x & -PI/4 < s & s < PI/4;
A4: ].-PI/4,PI/4.[ c= [.-PI/4,PI/4.] by XXREAL_1:25;
    -PI/4 in {s where s is Real: -PI/4 <= s & s <= PI/4};
    then
A5: -PI/4 in [.-PI/4,PI/4.] by RCOMP_1:def 1;
A6: [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
    then
A7: tan|[.-PI/4,PI/4.] is increasing by Th7,RFUNCT_2:60;
A8: [.-PI/4,PI/4.] /\ dom tan = [.-PI/4,PI/4.] by A6,Th1,XBOOLE_1:1,28;
    PI/4 in {s where s is Real: -PI/4 <= s & s <= PI/4};
    then PI/4 in [.-PI/4,PI/4.] /\ dom tan by A8,RCOMP_1:def 1;
    then tan.x < tan.(PI/4) by A2,A7,A8,A4,A3,RFUNCT_2:43;
    then
A9: tan.x < 1 by SIN_COS:def 32;
    x in { s where s is Real: -PI/4 < s & s < PI/4 } by A2,RCOMP_1:def 2;
    then ex s be Real st s=x & -PI/4 < s & s < PI/4;
    then -1 < tan.x by A2,A7,A5,A8,A4,Th17,RFUNCT_2:43;
    then tan.x in { s where s is Real: -1 < s & s < 1 } by A9;
    then
A10: tan.x in ].-1,1.[ by RCOMP_1:def 2;
    ].-1,1.[ c= [.-1,1.] by XXREAL_1:25;
    hence thesis by A10;
  end;
  suppose
    x = -PI/4;
    then tan.x in { s where s is Real: -1 <= s & s <= 1 } by Th17;
    hence thesis by RCOMP_1:def 1;
  end;
  suppose
    x = PI/4;
    then tan.x = 1 by SIN_COS:def 32;
    then tan.x in { s where s is Real: -1 <= s & s <= 1 };
    hence thesis by RCOMP_1:def 1;
  end;
end;

theorem Th20:
  for x be set st x in [.PI/4,3/4*PI.] holds cot.x in [.-1,1.]
proof
  let x be set;
  PI in ].0,4.[ by SIN_COS:def 32;
  then PI > 0 by XXREAL_1:4;
  then PI/4 > 0/4 by XREAL_1:76;
  then
A1: (PI/4)*3 > PI/4 by XREAL_1:157;
  assume x in [.PI/4,3/4*PI.];
  then x in ].PI/4,3/4*PI.[ \/ {PI/4,3/4*PI} by A1,XXREAL_1:128;
  then
A2: x in ].PI/4,3/4*PI.[ or x in {PI/4,3/4*PI} by XBOOLE_0:def 3;
  per cases by A2,TARSKI:def 2;
  suppose
A3: x in ].PI/4,3/4*PI.[;
    then x in { s where s is Real: PI/4 < s & s < 3/4*PI } by
    RCOMP_1:def 2;
    then
A4: ex s be Real st s=x & PI/4 < s & s < 3/4*PI;
A5: [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
    then
A6: cot|[.PI/4,3/4*PI.] is decreasing by Th8,RFUNCT_2:61;
    x in { s where s is Real: PI/4 < s & s < 3/4*PI } by A3,RCOMP_1:def 2;
    then
A7: ex s be Real st s=x & PI/4 < s & s < 3/4*PI;
A8: ].PI/4,3/4*PI.[ c= [.PI/4,3/4*PI.] by XXREAL_1:25;
A9: [.PI/4,3/4*PI.] /\ dom cot = [.PI/4,3/4*PI.] by A5,Th2,XBOOLE_1:1,28;
    3/4*PI in {s where s is Real: PI/4 <= s & s <= 3/4*PI} by A1;
    then 3/4*PI in [.PI/4,3/4*PI.] /\ dom cot by A9,RCOMP_1:def 1;
    then
A10: -1 < cot.x by A3,A6,A9,A8,A7,Th18,RFUNCT_2:44;
    PI/4 in {s where s is Real: PI/4 <= s & s <= 3/4*PI} by A1;
    then PI/4 in [.PI/4,3/4*PI.] /\ dom cot by A9,RCOMP_1:def 1;
    then cot.x < 1 by A3,A6,A9,A8,A4,Th18,RFUNCT_2:44;
    then cot.x in { s where s is Real: -1 < s & s < 1 } by A10;
    then
A11: cot.x in ].-1,1.[ by RCOMP_1:def 2;
    ].-1,1.[ c= [.-1,1.] by XXREAL_1:25;
    hence thesis by A11;
  end;
  suppose
    x = PI/4;
    then cot.x in { s where s is Real: -1 <= s & s <= 1 } by Th18;
    hence thesis by RCOMP_1:def 1;
  end;
  suppose
    x = 3/4*PI;
    then cot.x in { s where s is Real: -1 <= s & s <= 1 } by Th18;
    hence thesis by RCOMP_1:def 1;
  end;
end;

theorem Th21:
  rng (tan | [.-PI/4,PI/4.]) = [.-1,1.]
proof
  now
    let y be set;
    thus y in [.-1,1.] implies ex x be set st x in dom (tan | [.-PI/4,PI/4.])
    & y = (tan | [.-PI/4,PI/4.]).x
    proof
      assume
A1:   y in [.-1,1.];
      then reconsider y1=y as Real;
      y1 in [.tan.(-PI/4),tan.(PI/4).] by A1,Th17,SIN_COS:def 32;
      then
A2:   y1 in [.tan.(-PI/4),tan.(PI/4).] \/ [.tan.(PI/4),tan.(-PI/4).] by
      XBOOLE_0:def 3;
A3:   [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
      tan|].-PI/2,PI/2.[ is continuous by Lm1,FDIFF_1:33;
      then tan|[.-PI/4,PI/4.] is continuous by A3,FCONT_1:17;
      then consider x be Real such that
A4:   x in [.-PI/4,PI/4.] and
A5:   y1 = tan.x by A3,A2,Th1,FCONT_2:16,XBOOLE_1:1;
      take x;
      thus thesis by A4,A5,Lm11,FUNCT_1:72;
    end;
    thus (ex x be set st x in dom (tan | [.-PI/4,PI/4.]) & y = (tan | [.-PI/4,
    PI/4.]).x) implies y in [.-1,1.]
    proof
      given x be set such that
A6:   x in dom (tan | [.-PI/4,PI/4.]) and
A7:   y = (tan | [.-PI/4,PI/4.]).x;
      reconsider x1=x as Real by A6;
      y = tan.x1 by A6,A7,Lm11,FUNCT_1:72;
      hence thesis by A6,Lm11,Th19;
    end;
  end;
  hence thesis by FUNCT_1:def 5;
end;

theorem Th22:
  rng (cot | [.PI/4,3/4*PI.]) = [.-1,1.]
proof
  now
    let y be set;
    thus y in [.-1,1.] implies ex x be set st x in dom (cot | [.PI/4,3/4*PI.])
    & y = (cot | [.PI/4,3/4*PI.]).x
    proof
      PI in ].0,4.[ by SIN_COS:def 32;
      then PI > 0 by XXREAL_1:4;
      then PI/4 > 0/4 by XREAL_1:76;
      then
A1:   (PI/4)*3 > PI/4 by XREAL_1:157;
      assume
A2:   y in [.-1,1.];
      then reconsider y1=y as Real;
A3:   y1 in [.cot.(3/4*PI),cot.(PI/4).] \/ [.cot.(PI/4),cot.(3/4*PI).] by
      A2,Th18,XBOOLE_0:def 3;
A4:   [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
      cot|].0,PI.[ is continuous by Lm2,FDIFF_1:33;
      then cot|[.PI/4,3/4*PI.] is continuous by A4,FCONT_1:17;
      then consider x be Real such that
A5:   x in [.PI/4,3/4*PI.] and
A6:   y1 = cot.x by A1,A4,A3,Th2,FCONT_2:16,XBOOLE_1:1;
      take x;
      thus thesis by A5,A6,Lm12,FUNCT_1:72;
    end;
    thus (ex x be set st x in dom (cot | [.PI/4,3/4*PI.]) & y = (cot | [.PI/4,
    3/4*PI.]).x) implies y in [.-1,1.]
    proof
      given x be set such that
A7:   x in dom (cot | [.PI/4,3/4*PI.]) and
A8:   y = (cot | [.PI/4,3/4*PI.]).x;
      reconsider x1=x as Real by A7;
      y = cot.x1 by A7,A8,Lm12,FUNCT_1:72;
      hence thesis by A7,Lm12,Th20;
    end;
  end;
  hence thesis by FUNCT_1:def 5;
end;

theorem Th23:
  [.-1,1.] c= dom arctan
proof
A1: [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  rng (tan | [.-PI/4,PI/4.]) c= rng (tan | ].-PI/2,PI/2.[)
  proof
    let y be set;
    assume y in rng (tan | [.-PI/4,PI/4.]);
    then y in tan.:[.-PI/4,PI/4.] by RELAT_1:148;
    then ex x be set st x in dom tan & x in [.-PI/4,PI/4.] & y = tan.x by
    FUNCT_1:def 12;
    then y in tan.:].-PI/2,PI/2.[ by A1,FUNCT_1:def 12;
    hence thesis by RELAT_1:148;
  end;
  hence thesis by Th21,FUNCT_1:55;
end;

theorem Th24:
  [.-1,1.] c= dom arccot
proof
A1: [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
  rng (cot | [.PI/4,3/4*PI.]) c= rng (cot | ].0,PI.[)
  proof
    let y be set;
    assume y in rng (cot | [.PI/4,3/4*PI.]);
    then y in cot.:[.PI/4,3/4*PI.] by RELAT_1:148;
    then
    ex x be set st x in dom cot & x in [.PI/4,3/4*PI.] & y = cot.x by
    FUNCT_1:def 12;
    then y in cot.:].0,PI.[ by A1,FUNCT_1:def 12;
    hence thesis by RELAT_1:148;
  end;
  hence thesis by Th22,FUNCT_1:55;
end;

Lm13: tan | [.-PI/4,PI/4.]|[.-PI/4,PI/4.] is increasing
proof
  [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  then tan|[.-PI/4,PI/4.] is increasing by Th7,RFUNCT_2:60;
  hence thesis;
end;

Lm14: cot | [.PI/4,3/4*PI.] |[.PI/4,3/4*PI.] is decreasing
proof
  [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
  then cot|[.PI/4,3/4*PI.] is decreasing by Th8,RFUNCT_2:61;
  hence thesis;
end;

Lm15: tan | [.-PI/4,PI/4.] is one-to-one
proof
  [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  then (tan | ].-PI/2,PI/2.[) | [.-PI/4,PI/4.] = tan | [.-PI/4,PI/4.] by
  RELAT_1:103;
  hence thesis;
end;

Lm16: cot | [.PI/4,3/4*PI.] is one-to-one
proof
  [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
  then (cot | ].0,PI.[) | [.PI/4,3/4*PI.] = cot | [.PI/4,3/4*PI.] by
  RELAT_1
  :103;
  hence thesis;
end;

registration
  cluster tan | [.-PI/4,PI/4.] -> one-to-one;
  coherence by Lm15;
  cluster cot | [.PI/4,3/4*PI.] -> one-to-one;
  coherence by Lm16;
end;

theorem Th25:
  arctan | [.-1,1.] = (tan | [.-PI/4,PI/4.])"
proof
  set h = tan | ].-PI/2,PI/2.[;
A1: [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  then (tan | [.-PI/4,PI/4.])" = (h | [.-PI/4,PI/4.])" by RELAT_1:103
    .= h" | (h.:[.-PI/4,PI/4.]) by RFUNCT_2:40
    .= h" | rng (h | [.-PI/4,PI/4.]) by RELAT_1:148
    .= h" | ([.-1,1.]) by A1,Th21,RELAT_1:103;
  hence thesis;
end;

theorem Th26:
  arccot | [.-1,1.] = (cot | [.PI/4,3/4*PI.])"
proof
  set h = cot | ].0,PI.[;
A1: [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
  then (cot | [.PI/4,3/4*PI.])" = (h | [.PI/4,3/4*PI.])" by RELAT_1:103
    .= h" | (h.:[.PI/4,3/4*PI.]) by RFUNCT_2:40
    .= h" | rng (h | [.PI/4,3/4*PI.]) by RELAT_1:148
    .= h" | ([.-1,1.]) by A1,Th22,RELAT_1:103;
  hence thesis;
end;

theorem
  (tan | [.-PI/4,PI/4.]) qua Function * (arctan | [.-1,1.]) = id [.-1,1
  .] by Th21,Th25,FUNCT_1:61;

theorem
  (cot | [.PI/4,3/4*PI.]) qua Function * (arccot | [.-1,1.]) = id [.-1,1
  .] by Th22,Th26,FUNCT_1:61;

theorem
  (tan | [.-PI/4,PI/4.]) * (arctan | [.-1,1.]) = id [.-1,1.] by Th21,
  Th25,FUNCT_1:61;

theorem
  (cot | [.PI/4,3/4*PI.]) * (arccot | [.-1,1.]) = id [.-1,1.] by Th22,
  Th26,FUNCT_1:61;

theorem Th31:
  (arctan qua Function) * (tan | ].-PI/2,PI/2.[) = id ].-PI/2,PI/2
  .[ by Lm5,Th11,FUNCT_1:61;

theorem Th32:
  arccot * (cot | ].0,PI.[) = id ].0,PI.[ by Lm6,Th12,FUNCT_1:61;

theorem
  arctan qua Function * (tan | ].-PI/2,PI/2.[) = id ].-PI/2,PI/2.[ by
  Lm5,Th11,FUNCT_1:61;

theorem
  arccot qua Function * (cot | ].0,PI.[) = id ].0,PI.[ by Lm6,Th12,
  FUNCT_1:61;

theorem Th35:
  -PI/2 < r & r < PI/2 implies arctan tan.r = r & arctan tan r = r
proof
  assume that
A1: -PI/2 < r and
A2: r < PI/2;
A3: dom (tan|].-PI/2,PI/2.[) = ].-PI/2,PI/2.[ by Th1,RELAT_1:91;
A4: r in ].-PI/2,PI/2.[ by A1,A2,XXREAL_1:4;
  then arctan tan.r = arctan.((tan|].-PI/2,PI/2.[).r) by FUNCT_1:72
    .= (id ].-PI/2,PI/2.[).r by A4,A3,Th31,FUNCT_1:23
    .= r by A4,FUNCT_1:35;
  hence thesis by A4,Th13;
end;

theorem Th36:
  0 < r & r < PI implies arccot cot.r = r & arccot cot r = r
proof
  assume that
A1: 0 < r and
A2: r < PI;
A3: dom (cot|].0,PI.[) = ].0,PI.[ by Th2,RELAT_1:91;
A4: r in ].0,PI.[ by A1,A2,XXREAL_1:4;
  then arccot cot.r = arccot.((cot|].0,PI.[).r) by FUNCT_1:72
    .= (id ].0,PI.[).r by A4,A3,Th32,FUNCT_1:23
    .= r by A4,FUNCT_1:35;
  hence thesis by A4,Th14;
end;

theorem Th37:
  arctan (-1) = -PI/4 & arctan.(-1) = -PI/4
proof
  -PI/2 < -PI/4 by Lm7,XXREAL_1:4;
  then arctan (-1) = -PI/4 by Th17,Th35;
  hence thesis;
end;

theorem Th38:
  arccot (-1) = 3/4*PI & arccot.(-1) = 3/4*PI
proof
A1: 3/4*PI < PI by Lm10,XXREAL_1:4;
  0 < 3/4*PI by Lm10,XXREAL_1:4;
  then arccot (-1) = 3/4*PI by A1,Th18,Th36;
  hence thesis;
end;

theorem Th39:
  arctan 1 = PI/4 & arctan.1 = PI/4
proof
A1: arctan 1 = arctan tan.(PI/4) by SIN_COS:def 32;
  PI/4 < PI/2 by Lm8,XXREAL_1:4;
  hence thesis by A1,Th35;
end;

theorem Th40:
  arccot 1 = PI/4 & arccot.1 = PI/4
proof
A1: PI/4 < PI by Lm9,XXREAL_1:4;
  0 < PI/4 by Lm9,XXREAL_1:4;
  then arccot 1 = PI/4 by A1,Th18,Th36;
  hence thesis;
end;

theorem Th41:
  tan.0 = 0 & tan 0 =0
proof
  PI in ].0,4.[ by SIN_COS:def 32;
  then PI > 0 by XXREAL_1:4;
  then
A1: PI/2 > 0/2 by XREAL_1:76;
  0-PI/2 < 0 by XREAL_1:51;
  then
A2: 0 in ].-PI/2,PI/2.[ by A1,XXREAL_1:4;
  then tan.0 = 0/cos.0 by Th1,RFUNCT_1:def 4,SIN_COS:33
    .= 0;
  hence thesis by A2,Th13;
end;

theorem Th42:
  cot.(PI/2) = 0 & cot (PI/2) = 0
proof
  PI in ].0,4.[ by SIN_COS:def 32;
  then
A1: PI > 0 by XXREAL_1:4;
  then
A2: PI/2 < PI/1 by XREAL_1:78;
  PI/2 > 0/2 by A1,XREAL_1:76;
  then
A3: PI/2 in ].0,PI.[ by A2,XXREAL_1:4;
  then cot.(PI/2) = 0/sin.(PI/2) by Th2,RFUNCT_1:def 4,SIN_COS:81
    .= 0;
  hence thesis by A3,Th14;
end;

theorem
  arctan 0 = 0 & arctan.0 = 0
proof
  PI in ].0,4.[ by SIN_COS:def 32;
  then PI > 0 by XXREAL_1:4;
  then
A1: PI/2 > 0/2 by XREAL_1:76;
  0 - PI/2 < 0 by XREAL_1:51;
  then arctan 0 = 0 by A1,Th35,Th41;
  hence thesis;
end;

theorem
  arccot 0 = PI/2 & arccot.0 = PI/2
proof
  PI in ].0,4.[ by SIN_COS:def 32;
  then
A1: PI > 0 by XXREAL_1:4;
  then
A2: PI/2 < PI/1 by XREAL_1:78;
  PI/2 > 0/2 by A1,XREAL_1:76;
  then arccot 0 = PI/2 by A2,Th36,Th42;
  hence thesis;
end;

theorem Th45:
  arctan|(tan.:].-PI/2,PI/2.[) is increasing
proof
  set f = tan | ].-PI/2,PI/2.[;
A1: f.:].-PI/2,PI/2.[ = rng(f|].-PI/2,PI/2.[) by RELAT_1:148
    .= rng(tan|].-PI/2,PI/2.[) by RELAT_1:102
    .= tan.:].-PI/2,PI/2.[ by RELAT_1:148;
  f|].-PI/2,PI/2.[ = f by RELAT_1:102;
  hence thesis by A1,Th7,FCONT_3:17;
end;

theorem Th46:
  arccot|(cot.:].0,PI.[) is decreasing
proof
  set f = cot | ].0,PI.[;
A1: f.:].0,PI.[ = rng(f|].0,PI.[) by RELAT_1:148
    .= rng(cot|].0,PI.[) by RELAT_1:102
    .= cot.:].0,PI.[ by RELAT_1:148;
  f|].0,PI.[ = f by RELAT_1:102;
  hence thesis by A1,Th8,FCONT_3:18;
end;

theorem Th47:
  arctan|[.-1,1.] is increasing
proof
A1: [.-1,1.] = tan.:[.-PI/4,PI/4.] by Th21,RELAT_1:148;
  [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  hence thesis by A1,Th45,RELAT_1:156,RFUNCT_2:60;
end;

theorem Th48:
  arccot|[.-1,1.] is decreasing
proof
A1: [.-1,1.] = cot.:[.PI/4,3/4*PI.] by Th22,RELAT_1:148;
  [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
  hence thesis by A1,Th46,RELAT_1:156,RFUNCT_2:61;
end;

theorem Th49:
  for x be set st x in [.-1,1.] holds arctan.x in [.-PI/4,PI/4.]
proof
  let x be set;
  assume x in [.-1,1.];
  then x in ].-1,1.[ \/ {-1,1} by XXREAL_1:128;
  then
A1: x in ].-1,1.[ or x in {-1,1} by XBOOLE_0:def 3;
  per cases by A1,TARSKI:def 2;
  suppose
A2: x in ].-1,1.[;
    then x in { s where s is Real: -1 < s & s < 1 } by RCOMP_1:def 2;
    then
A3: ex s be Real st s=x & -1 < s & s < 1;
A4: ].-1,1.[ c= [.-1,1.] by XXREAL_1:25;
A5: [.-1,1.] /\ dom arctan = [.-1,1.] by Th23,XBOOLE_1:28;
    then 1 in [.-1,1.] /\ dom arctan by XXREAL_1:1;
    then
A6: arctan.x < PI/4 by A2,A5,A4,A3,Th39,Th47,RFUNCT_2:43;
    -1 in [.-1,1.] by XXREAL_1:1;
    then -PI/4 < arctan.x by A2,A5,A4,A3,Th37,Th47,RFUNCT_2:43;
    hence thesis by A6,XXREAL_1:1;
  end;
  suppose
    x = -1;
    hence thesis by Th37,XXREAL_1:1;
  end;
  suppose
    x = 1;
    hence thesis by Th39,XXREAL_1:1;
  end;
end;

theorem Th50:
  for x be set st x in [.-1,1.] holds arccot.x in [.PI/4,3/4*PI.]
proof
  let x be set;
  assume x in [.-1,1.];
  then x in ].-1,1.[ \/ {-1,1} by XXREAL_1:128;
  then
A1: x in ].-1,1.[ or x in {-1,1} by XBOOLE_0:def 3;
  per cases by A1,TARSKI:def 2;
  suppose
A2: x in ].-1,1.[;
    then x in { s where s is Real: -1 < s & s < 1 } by RCOMP_1:def 2;
    then
A3: ex s be Real st s=x & -1 < s & s < 1;
A4: ].-1,1.[ c= [.-1,1.] by XXREAL_1:25;
A5: [.-1,1.] /\ dom arccot = [.-1,1.] by Th24,XBOOLE_1:28;
    then 1 in [.-1,1.] /\ dom arccot by XXREAL_1:1;
    then
A6: arccot.x > PI/4 by A2,A5,A4,A3,Th40,Th48,RFUNCT_2:44;
    -1 in [.-1,1.] by XXREAL_1:1;
    then 3/4*PI > arccot.x by A2,A5,A4,A3,Th38,Th48,RFUNCT_2:44;
    hence thesis by A6,XXREAL_1:1;
  end;
  suppose
A7: x = -1;
    PI in ].0,4.[ by SIN_COS:def 32;
    then PI > 0 by XXREAL_1:4;
    then PI/4 > 0/4 by XREAL_1:76;
    then PI/4 < (PI/4)*3 by XREAL_1:157;
    hence thesis by A7,Th38,XXREAL_1:1;
  end;
  suppose
A8: x = 1;
    PI in ].0,4.[ by SIN_COS:def 32;
    then PI > 0 by XXREAL_1:4;
    then PI/4 > 0/4 by XREAL_1:76;
    then PI/4 < (PI/4)*3 by XREAL_1:157;
    hence thesis by A8,Th40,XXREAL_1:1;
  end;
end;

theorem Th51:
  -1 <= r & r <= 1 implies tan arctan r = r
proof
A1: [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  assume that
A2: -1 <= r and
A3: r <= 1;
A4: r in [.-1,1.] by A2,A3,XXREAL_1:1;
  then
A5: r in dom (arctan | [.-1,1.]) by Th23,RELAT_1:91;
  arctan.r in [.-PI/4,PI/4.] by A4,Th49;
  hence tan (arctan r) = tan.(arctan.r) by A1,Th13
    .= ((tan|[.-PI/4,PI/4.]) qua Function).(arctan.r) by A4,Th49,FUNCT_1:72
    .= ((tan|[.-PI/4,PI/4.]) qua Function).((arctan | [.-1,1.]).r) by A4,
  FUNCT_1:72
    .= ((tan | [.-PI/4,PI/4.]) qua Function * (arctan | [.-1,1.])).r by A5,
  FUNCT_1:23
    .= (id [.-1,1.]).r by Th21,Th25,FUNCT_1:61
    .= r by A4,FUNCT_1:35;
end;

theorem Th52:
  -1 <= r & r <= 1 implies cot arccot r = r
proof
A1: [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
  assume that
A2: -1 <= r and
A3: r <= 1;
A4: r in [.-1,1.] by A2,A3,XXREAL_1:1;
  then
A5: r in dom (arccot | [.-1,1.]) by Th24,RELAT_1:91;
  arccot.r in [.PI/4,3/4*PI.] by A4,Th50;
  hence cot (arccot r) = cot.(arccot.r) by A1,Th14
    .= ((cot|[.PI/4,3/4*PI.]) qua Function).(arccot.r) by A4,Th50,FUNCT_1:72
    .= ((cot|[.PI/4,3/4*PI.]) qua Function).((arccot | [.-1,1.]).r) by A4,
  FUNCT_1:72
    .= ((cot|[.PI/4,3/4*PI.]) qua Function * (arccot | [.-1,1.])).r by A5,
  FUNCT_1:23
    .= (id [.-1,1.]).r by Th22,Th26,FUNCT_1:61
    .= r by A4,FUNCT_1:35;
end;

theorem Th53:
  arctan|[.-1,1.] is continuous
proof
  set f = tan | [.-PI/4,PI/4.];
A1: f|[.-PI/4,PI/4.] = f by RELAT_1:101;
  (f|[.-PI/4,PI/4.])"|(f.:[.-PI/4,PI/4.]) is continuous by Lm11,Lm13,
  FCONT_1:54;
  then arctan | [.-1,1.]|[.-1,1.] is continuous by A1,
  Th21,Th25,RELAT_1:148;
  hence thesis by FCONT_1:16;
end;

theorem Th54:
  arccot|[.-1,1.] is continuous
proof
  set f = cot | [.PI/4,3/4*PI.];
  PI in ].0,4.[ by SIN_COS:def 32;
  then PI > 0 by XXREAL_1:4;
  then PI/4 > 0/4 by XREAL_1:76;
  then PI/4 < (PI/4)*3 by XREAL_1:157;
  then
A1: (f|[.PI/4,3/4*PI.])"|(f.:[.PI/4,3/4*PI.]) is continuous by
  Lm12,Lm14,
  FCONT_1:54;
  f|[.PI/4,3/4*PI.] = f by RELAT_1:101;
  then arccot | [.-1,1.]|[.-1,1.] is continuous by
  A1,Th22,Th26,RELAT_1:148;
  hence thesis by FCONT_1:16;
end;

theorem Th55:
  rng(arctan | [.-1,1.]) = [.-PI/4,PI/4.]
proof
  now
    let y be set;
    thus y in [.-PI/4,PI/4.] implies ex x be set st x in dom (arctan | [.-1,1
    .]) & y = (arctan | [.-1,1.]).x
    proof
      assume
A1:   y in [.-PI/4,PI/4.];
      then reconsider y1=y as Real;
      y1 in [.arctan.(-1),arctan.1.] \/ [.arctan.1,arctan.(-1).] by A1,
      Th37,Th39,XBOOLE_0:def 3;
      then consider x be Real such that
A2:   x in [.-1,1.] and
A3:   y1 = arctan.x by Th23,Th53,FCONT_2:16;
      take x;
      thus thesis by A2,A3,Th23,FUNCT_1:72,RELAT_1:91;
    end;
    thus (ex x be set st x in dom (arctan | [.-1,1.]) & y = (arctan | [.-1,1.]
    ).x) implies y in [.-PI/4,PI/4.]
    proof
      given x be set such that
A4:   x in dom (arctan | [.-1,1.]) and
A5:   y = (arctan | [.-1,1.]).x;
      reconsider x1=x as Real by A4;
A6:   dom (arctan | [.-1,1.]) = [.-1,1.] by Th23,RELAT_1:91;
      then y = arctan.x by A4,A5,FUNCT_1:72;
      hence thesis by A4,A6,Th49;
    end;
  end;
  hence thesis by FUNCT_1:def 5;
end;

theorem Th56:
  rng(arccot | [.-1,1.]) = [.PI/4,3/4*PI.]
proof
  now
    let y be set;
    thus y in [.PI/4,3/4*PI.] implies ex x be set st x in dom (arccot | [.-1,1
    .]) & y = (arccot | [.-1,1.]).x
    proof
      assume
A1:   y in [.PI/4,3/4*PI.];
      then reconsider y1=y as Real;
      y1 in [.arccot.1,arccot.(-1).] \/ [.arccot.(-1),arccot.1.] by A1,
      Th38,Th40,XBOOLE_0:def 3;
      then consider x be Real such that
A2:   x in [.-1,1.] and
A3:   y1 = arccot.x by Th24,Th54,FCONT_2:16;
      take x;
      thus thesis by A2,A3,Th24,FUNCT_1:72,RELAT_1:91;
    end;
    thus (ex x be set st x in dom (arccot | [.-1,1.]) & y = (arccot | [.-1,1.]
    ).x) implies y in [.PI/4,3/4*PI.]
    proof
      given x be set such that
A4:   x in dom (arccot | [.-1,1.]) and
A5:   y = (arccot | [.-1,1.]).x;
      reconsider x1=x as Real by A4;
A6:   dom (arccot | [.-1,1.]) = [.-1,1.] by Th24,RELAT_1:91;
      then y = arccot.x by A4,A5,FUNCT_1:72;
      hence thesis by A4,A6,Th50;
    end;
  end;
  hence thesis by FUNCT_1:def 5;
end;

theorem
  -1 <= r & r <= 1 & arctan r = -PI/4 implies r = -1 by Th17,Th51;

theorem
  -1 <= r & r <= 1 & arccot r = 3/4*PI implies r = -1 by Th18,Th52;

theorem
  -1 <= r & r <= 1 & arctan r = 0 implies r = 0 by Th41,Th51;

theorem
  -1 <= r & r <= 1 & arccot r = PI/2 implies r = 0 by Th42,Th52;

theorem
  -1 <= r & r <= 1 & arctan r = PI/4 implies r = 1
proof
  assume that
A1: -1 <= r and
A2: r <= 1 and
A3: arctan r = PI/4;
  thus r = tan(PI/4) by A1,A2,A3,Th51
    .= tan.(PI/4) by Lm8,Th13
    .= 1 by SIN_COS:def 32;
end;

theorem
  -1 <= r & r <= 1 & arccot r = PI/4 implies r = 1 by Th18,Th52;

theorem Th63:
  -1 <= r & r <= 1 implies -PI/4 <= arctan r & arctan r <= PI/4
proof
  assume that
A1: -1 <= r and
A2: r <= 1;
A3: r in [.-1,1.] by A1,A2,XXREAL_1:1;
  then r in dom (arctan | [.-1,1.]) by Th23,RELAT_1:91;
  then (arctan | [.-1,1.]).r in rng (arctan | [.-1,1.]) by FUNCT_1:def 5;
  then arctan r in rng (arctan | [.-1,1.]) by A3,FUNCT_1:72;
  hence thesis by Th55,XXREAL_1:1;
end;

theorem Th64:
  -1 <= r & r <= 1 implies PI/4 <= arccot r & arccot r <= 3/4*PI
proof
  assume that
A1: -1 <= r and
A2: r <= 1;
A3: r in [.-1,1.] by A1,A2,XXREAL_1:1;
  then r in dom (arccot | [.-1,1.]) by Th24,RELAT_1:91;
  then (arccot | [.-1,1.]).r in rng (arccot | [.-1,1.]) by FUNCT_1:def 5;
  then arccot r in rng (arccot | [.-1,1.]) by A3,FUNCT_1:72;
  hence thesis by Th56,XXREAL_1:1;
end;

theorem
  -1 < r & r < 1 implies -PI/4 < arctan r & arctan r < PI/4
proof
A1: tan (PI/4) = tan.(PI/4) by Lm8,Th13
    .= 1 by SIN_COS:def 32;
  assume that
A2: -1 < r and
A3: r < 1;
A4: arctan r <= PI/4 by A2,A3,Th63;
  -PI/4 <= arctan r by A2,A3,Th63;
  then
  -PI/4 < arctan r & arctan r < PI/4 or -PI/4 = arctan r or arctan r = PI/
  4 by A4,XXREAL_0:1;
  hence thesis by A2,A3,A1,Th17,Th51;
end;

theorem
  -1 < r & r < 1 implies PI/4 < arccot r & arccot r < 3/4*PI
proof
  assume that
A1: -1 < r and
A2: r < 1;
A3: arccot r <= 3/4*PI by A1,A2,Th64;
  PI/4 <= arccot r by A1,A2,Th64;
  then
  PI/4 < arccot r & arccot r < 3/4*PI or PI/4 = arccot r or arccot r = 3/4
  *PI by A3,XXREAL_0:1;
  hence thesis by A1,A2,Th18,Th52;
end;

theorem
  -1 <= r & r <= 1 implies arctan r = -arctan(-r)
proof
A1: [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  assume that
A2: -1 <= r and
A3: r <= 1;
A4: -r >= -1 by A3,XREAL_1:26;
A5: --1 >= -r by A2,XREAL_1:26;
  then
A6: arctan(-r) <= PI/4 by A4,Th63;
  -PI/4 <= arctan(-r) by A5,A4,Th63;
  then
A7: -arctan(-r) <= --PI/4 by XREAL_1:26;
  arctan(-r) <= PI/4 by A5,A4,Th63;
  then -PI/4 <= -arctan(-r) by XREAL_1:26;
  then
A8: -arctan(-r) in [.-PI/4,PI/4.] by A7,XXREAL_1:1;
  -PI/4 <= arctan(-r) by A5,A4,Th63;
  then arctan(-r) in [.-PI/4,PI/4.] by A6,XXREAL_1:1;
  then
A9: cos arctan(-r) <> 0 by A1,COMPTRIG:27;
  tan arctan(-r) = -r by A5,A4,Th51;
  then
A10: r = (tan 0 - tan arctan(-r))/(1 + (tan 0) * tan arctan(-r)) by
  Th41
    .= tan(0 - arctan(-r)) by A9,SIN_COS:34,SIN_COS4:12;
A11: [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  then
A12: -arctan(-r) < PI/2 by A8,XXREAL_1:4;
  -PI/2 < -arctan(-r) by A8,A11,XXREAL_1:4;
  hence thesis by A10,A12,Th35;
end;

theorem
  -1 <= r & r <= 1 implies arccot r = PI - arccot(-r)
proof
  set x = arccot(-r);
  assume that
A1: -1 <= r and
A2: r <= 1;
A3: -r >= -1 by A2,XREAL_1:26;
A4: --1 >= -r by A1,XREAL_1:26;
  then -r = cot x by A3,Th52;
  then
A5: r = -cot x .= -(cos x/sin x) by SIN_COS4:def 2
    .= cos x/(-(sin x)) by XCMPLX_1:189
    .= cos x/sin(-x) by SIN_COS:34
    .= cos(-x)/sin(-x) by SIN_COS:34
    .= cot(-x) by SIN_COS4:def 2;
  -r in [.-1,1.] by A4,A3,XXREAL_1:1;
  then
A6: x in [.PI/4,3/4*PI.] by Th50;
  then x <= 3/4*PI by XXREAL_1:1;
  then -x >= -3/4*PI by XREAL_1:26;
  then
A7: PI+(-x) >= PI+(-3/4*PI) by XREAL_1:8;
  PI/4 <= x by A6,XXREAL_1:1;
  then -PI/4 >= -x by XREAL_1:26;
  then PI+(-PI/4) >= PI+(-x) by XREAL_1:8;
  then
A8: PI+(-x) in [.PI/4,3/4*PI.] by A7,XXREAL_1:1;
A9: [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
  then
A10: PI+(-x) < PI by A8,XXREAL_1:4;
A11: cot(PI+(-x)) = cos(PI+(-x))/sin(PI+(-x)) by SIN_COS4:def 2
    .= (-cos(-x))/sin(PI+(-x)) by SIN_COS:84
    .= (-cos(-x))/(-sin(-x)) by SIN_COS:84
    .= cos(-x)/sin(-x) by XCMPLX_1:192
    .= cot(-x)by SIN_COS4:def 2;
  0 < PI+(-x) by A8,A9,XXREAL_1:4;
  hence thesis by A5,A10,A11,Th36;
end;

theorem
  -1 <= r & r <= 1 implies cot arctan r = 1/r
proof
  set x = arctan r;
  assume that
A1: -1 <= r and
A2: r <= 1;
A3: sin x /cos x = tan x by SIN_COS4:def 1
    .= r by A1,A2,Th51;
  cot x = cos x/sin x by SIN_COS4:def 2
    .= 1/r by A3,XCMPLX_1:57;
  hence thesis;
end;

theorem
  -1 <= r & r <= 1 implies tan arccot r = 1/r
proof
  set x = arccot r;
  assume that
A1: -1 <= r and
A2: r <= 1;
A3: cos x /sin x = cot x by SIN_COS4:def 2
    .= r by A1,A2,Th52;
  tan x = sin x/cos x by SIN_COS4:def 1
    .= 1/r by A3,XCMPLX_1:57;
  hence thesis;
end;

theorem Th71:
  arctan is_differentiable_on tan.:].-PI/2,PI/2.[
proof
  set g = arctan;
  set f = tan|].-PI/2,PI/2.[;
A1: dom(f") = rng (tan|].-PI/2,PI/2.[) by FUNCT_1:55
    .= tan.:].-PI/2,PI/2.[ by RELAT_1:148;
  dom f = dom tan /\ ].-PI/2,PI/2.[ by RELAT_1:90;
  then
A2: ].-PI/2,PI/2.[ c= dom f by Th1,XBOOLE_1:19;
A3: f is_differentiable_on ].-PI/2,PI/2.[ by Lm1,FDIFF_2:16;
A4: now
A5: for x0 be Real st x0 in ].-PI/2,PI/2.[ holds 1/(cos.x0)^2 > 0
    proof
      let x0 be Real;
      assume x0 in ].-PI/2,PI/2.[;
      then 0 < cos.x0 by COMPTRIG:27;
      then (cos.x0)^2 > 0 by SQUARE_1:74;
      then 1/(cos.x0)^2 > 0 /(cos.x0)^2 by XREAL_1:76;
      hence thesis;
    end;
    let x0 be Real such that
A6: x0 in ].-PI/2,PI/2.[;
    diff(f,x0) = (f`|].-PI/2,PI/2.[).x0 by A3,A6,FDIFF_1:def 8
      .= (tan`|].-PI/2,PI/2.[).x0 by Lm1,FDIFF_2:16
      .= diff(tan,x0) by A6,Lm1,FDIFF_1:def 8
      .= 1/(cos.x0)^2 by A6,Lm3;
    hence 0 < diff(f,x0) by A6,A5;
  end;
  f|].-PI/2,PI/2.[ = f by RELAT_1:101;
  hence thesis by A1,A3,A2,A4,FDIFF_2:48;
end;

theorem Th72:
  arccot is_differentiable_on cot.:].0,PI.[
proof
  set g = arccot;
  set f = cot|].0,PI.[;
A1: dom(f") = rng (cot|].0,PI.[) by FUNCT_1:55
    .= cot.:].0,PI.[ by RELAT_1:148;
  dom f = dom cot /\ ].0,PI.[ by RELAT_1:90;
  then
A2: ].0,PI.[ c= dom f by Th2,XBOOLE_1:19;
A3: f is_differentiable_on ].0,PI.[ by Lm2,FDIFF_2:16;
A4: now
A5: for x0 be Real st x0 in ].0,PI.[ holds -1/(sin.x0)^2 < 0
    proof
      let x0 be Real;
      assume x0 in ].0,PI.[;
      then 0 < sin.x0 by COMPTRIG:23;
      then (sin.x0)^2 > 0 by SQUARE_1:74;
      then 1/(sin.x0)^2 > 0 /(sin.x0)^2 by XREAL_1:76;
      then -1/(sin.x0)^2 < -0 by XREAL_1:26;
      hence thesis;
    end;
    let x0 be Real such that
A6: x0 in ].0,PI.[;
    diff(f,x0) = (f`|].0,PI.[).x0 by A3,A6,FDIFF_1:def 8
      .= (cot`|].0,PI.[).x0 by Lm2,FDIFF_2:16
      .= diff(cot,x0) by A6,Lm2,FDIFF_1:def 8
      .= -1/(sin.x0)^2 by A6,Lm4;
    hence diff(f,x0) < 0 by A6,A5;
  end;
  f|].0,PI.[ = f by RELAT_1:101;
  hence thesis by A1,A2,A3,A4,FDIFF_2:48;
end;

theorem Th73:
  arctan is_differentiable_on ].-1,1.[
proof
  ].-1,1.[ c= [.-1,1.] by XXREAL_1:25;
  then
A1: ].-1,1.[ c= dom arctan by Th23,XBOOLE_1:1;
  for x be Real st x in ].-1,1.[ holds arctan is_differentiable_in x
  proof
    let x be Real;
A2: dom arctan = rng (tan | ].-PI/2,PI/2.[) by FUNCT_1:55
      .= tan.:].-PI/2,PI/2.[ by RELAT_1:148;
    assume x in ].-1,1.[;
    then arctan|dom arctan is_differentiable_in x by A1,
    A2,Th71,FDIFF_1:def
    7;
    hence thesis by RELAT_1:98;
  end;
  hence thesis by A1,FDIFF_1:16;
end;

theorem Th74:
  arccot is_differentiable_on ].-1,1.[
proof
  ].-1,1.[ c= [.-1,1.] by XXREAL_1:25;
  then
A1: ].-1,1.[ c= dom arccot by Th24,XBOOLE_1:1;
  for x be Real st x in ].-1,1.[ holds arccot is_differentiable_in x
  proof
    let x be Real;
A2: dom arccot = rng (cot | ].0,PI.[) by FUNCT_1:55
      .= cot.:].0,PI.[ by RELAT_1:148;
    assume x in ].-1,1.[;
    then arccot|dom arccot is_differentiable_in x by A1,
    A2,Th72,FDIFF_1:def
    7;
    hence thesis by RELAT_1:98;
  end;
  hence thesis by A1,FDIFF_1:16;
end;

theorem Th75:
  -1 <= r & r <= 1 implies diff(arctan,r) = 1/(1+r^2)
proof
  set X = tan.:].-PI/2,PI/2.[;
  set g = arctan;
  set f = tan|].-PI/2,PI/2.[;
  set x = arctan.r;
  assume that
A1: -1 <= r and
A2: r <= 1;
A3: (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
A4: f is_differentiable_on ].-PI/2,PI/2.[ by Lm1,FDIFF_2:16;
A5: now
A6: for x0 be Real st x0 in ].-PI/2,PI/2.[ holds 1/(cos.x0)^2 > 0
    proof
      let x0 be Real;
      assume x0 in ].-PI/2,PI/2.[;
      then 0 < cos.x0 by COMPTRIG:27;
      then (cos.x0)^2 > 0 by SQUARE_1:74;
      then 1/(cos.x0)^2 > 0 /(cos.x0)^2 by XREAL_1:76;
      hence thesis;
    end;
    let x0 be Real such that
A7: x0 in ].-PI/2,PI/2.[;
    diff(f,x0) = (f`|].-PI/2,PI/2.[).x0 by A4,A7,FDIFF_1:def 8
      .= (tan`|].-PI/2,PI/2.[).x0 by Lm1,FDIFF_2:16
      .= diff(tan,x0) by A7,Lm1,FDIFF_1:def 8
      .= 1/(cos.x0)^2 by A7,Lm3;
    hence 0 < diff(f,x0) by A7,A6;
  end;
A8: r in [.-1,1.] by A1,A2,XXREAL_1:1;
  then
A9: x in [.-PI/4,PI/4.] by Th49;
  x = arctan r;
  then
A10: r = tan x by A1,A2,Th51
    .= sin x/cos x by SIN_COS4:def 1;
  dom f = dom tan /\ ].-PI/2,PI/2.[ by RELAT_1:90;
  then
A11: ].-PI/2,PI/2.[ c= dom f by Th1,XBOOLE_1:19;
A12: f|].-PI/2,PI/2.[ = f by RELAT_1:101;
A13: [.-PI/4,PI/4.] c= ].-PI/2,PI/2.[ by Lm7,Lm8,XXREAL_2:def 12;
  then cos x <> 0 by A9,COMPTRIG:27;
  then r * cos x = sin x by A10,XCMPLX_1:88;
  then
A14: 1 = (cos x)^2 * ( r^2 + 1 ) by A3;
  f is_differentiable_on ].-PI/2,PI/2.[ by Lm1,FDIFF_2:16;
  then diff(f,x) = (f`|].-PI/2,PI/2.[).x by A9,A13,FDIFF_1:def 8
    .= (tan`|].-PI/2,PI/2.[).x by Lm1,FDIFF_2:16
    .= diff(tan,x) by A9,A13,Lm1,FDIFF_1:def 8
    .= 1/(cos x)^2 by A9,A13,Lm3;
  then diff(g,r) = 1/(1/(cos x)^2) by A8,A4,A5,A12,A11,Th23,FDIFF_2:48
    .= 1/(r^2+1) by A14,XCMPLX_1:74;
  hence thesis;
end;

theorem Th76:
  -1 <= r & r <= 1 implies diff(arccot,r) = -1/(1+r^2)
proof
  set X = cot.:].0,PI.[;
  set g = arccot;
  set f = cot|].0,PI.[;
  set x = arccot.r;
  assume that
A1: -1 <= r and
A2: r <= 1;
A3: (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
A4: f is_differentiable_on ].0,PI.[ by Lm2,FDIFF_2:16;
A5: now
A6: for x0 be Real st x0 in ].0,PI.[ holds -1/(sin.x0)^2 < 0
    proof
      let x0 be Real;
      assume x0 in ].0,PI.[;
      then 0 < sin.x0 by COMPTRIG:23;
      then (sin.x0)^2 > 0 by SQUARE_1:74;
      then 1/(sin.x0)^2 > 0 /(sin.x0)^2 by XREAL_1:76;
      then -1/(sin.x0)^2 < -0 by XREAL_1:26;
      hence thesis;
    end;
    let x0 be Real such that
A7: x0 in ].0,PI.[;
    diff(f,x0) = (f`|].0,PI.[).x0 by A4,A7,FDIFF_1:def 8
      .= (cot`|].0,PI.[).x0 by Lm2,FDIFF_2:16
      .= diff(cot,x0) by A7,Lm2,FDIFF_1:def 8
      .= -1/(sin.x0)^2 by A7,Lm4;
    hence diff(f,x0) < 0 by A7,A6;
  end;
A8: r in [.-1,1.] by A1,A2,XXREAL_1:1;
  then
A9: x in [.PI/4,3/4*PI.] by Th50;
  x = arccot r;
  then
A10: r = cot x by A1,A2,Th52
    .= cos x/sin x by SIN_COS4:def 2;
  dom f = dom cot /\ ].0,PI.[ by RELAT_1:90;
  then
A11: ].0,PI.[ c= dom f by Th2,XBOOLE_1:19;
A12: f|].0,PI.[ = f by RELAT_1:101;
A13: [.PI/4,3/4*PI.] c= ].0,PI.[ by Lm9,Lm10,XXREAL_2:def 12;
  then sin x <> 0 by A9,COMPTRIG:23;
  then r * sin x = cos x by A10,XCMPLX_1:88;
  then
A14: 1 = (sin x)^2 * ( r^2 + 1 ) by A3;
  f is_differentiable_on ].0,PI.[ by Lm2,FDIFF_2:16;
  then diff(f,x) = (f`|].0,PI.[).x by A9,A13,FDIFF_1:def 8
    .= (cot`|].0,PI.[).x by Lm2,FDIFF_2:16
    .= diff(cot,x) by A9,A13,Lm2,FDIFF_1:def 8
    .= -1/(sin x)^2 by A9,A13,Lm4;
  then diff(g,r) = 1/(-(1/(sin x)^2)) by A8,
  A4,A5,A12,A11,Th24, FDIFF_2:
  48
    .= -1/(1/(sin x)^2) by XCMPLX_1:189
    .= -1/(r^2+1) by A14,XCMPLX_1:74;
  hence thesis;
end;

theorem
  arctan|(tan.:].-PI/2,PI/2.[) is continuous by Th71,FDIFF_1:33;

theorem
  arccot|(cot.:].0,PI.[) is continuous by Th72,FDIFF_1:33;

theorem
  dom arctan is open
proof
  for x0 be Real st x0 in ].-PI/2,PI/2.[ holds 0 < diff(tan,x0)
  proof
    let x0 be Real;
    assume
A1: x0 in ].-PI/2,PI/2.[;
    then 0 < cos.x0 by COMPTRIG:27;
    then (cos.x0)^2 > 0 by SQUARE_1:74;
    then 1/(cos.x0)^2 > 0 /(cos.x0)^2 by XREAL_1:76;
    hence thesis by A1,Lm3;
  end;
  then rng (tan|].-PI/2,PI/2.[) is open by Lm1,Th1,FDIFF_2:41;
  hence thesis by FUNCT_1:55;
end;

theorem
  dom arccot is open
proof
  for x0 be Real st x0 in ].0,PI.[ holds diff(cot,x0) < 0
  proof
    let x0 be Real;
    assume
A1: x0 in ].0,PI.[;
    then 0 < sin.x0 by COMPTRIG:23;
    then (sin.x0)^2 > 0 by SQUARE_1:74;
    then 1/(sin.x0)^2 > 0 /(sin.x0)^2 by XREAL_1:76;
    then -1/(sin.x0)^2 < -0 by XREAL_1:26;
    hence thesis by A1,Lm4;
  end;
  then rng (cot|].0,PI.[) is open by Lm2,Th2,FDIFF_2:41;
  hence thesis by FUNCT_1:55;
end;

begin ::  Differentiation Formulas of arctan,arccot
:: f.x=arctan.x

theorem Th81:
  Z c= ].-1,1.[ implies arctan is_differentiable_on Z & for x st x
  in Z holds ((arctan)`|Z).x = 1/(1+x^2)
proof
  assume
A1: Z c= ].-1,1.[;
  then
A2: arctan is_differentiable_on Z by Th73,FDIFF_1:34;
  for x st x in Z holds ((arctan)`|Z).x = 1/(1+x^2)
  proof
    let x be Real;
    assume
A3: x in Z;
    then
A4: -1 <= x by A1,XXREAL_1:4;
A5: x <= 1 by A1,A3,XXREAL_1:4;
    thus ((arctan)`|Z).x = diff(arctan,x) by A2,A3,FDIFF_1:def 8
      .= 1/(1+x^2) by A4,A5,Th75;
  end;
  hence thesis by A1,Th73,FDIFF_1:34;
end;
 
:: f.x=arccot.x

theorem Th82:
  Z c= ].-1,1.[ implies arccot is_differentiable_on Z & for x st x
  in Z holds ((arccot)`|Z).x = -1/(1+x^2)
proof
  assume
A1: Z c= ].-1,1.[;
  then
A2: arccot is_differentiable_on Z by Th74,FDIFF_1:34;
  for x st x in Z holds ((arccot)`|Z).x = -1/(1+x^2)
  proof
    let x be Real;
    assume
A3: x in Z;
    then
A4: -1 <= x by A1,XXREAL_1:4;
A5: x <= 1 by A1,A3,XXREAL_1:4;
    thus ((arccot)`|Z).x = diff(arccot,x) by A2,A3,FDIFF_1:def 8
      .= -1/(1+x^2) by A4,A5,Th76;
  end;
  hence thesis by A1,Th74,FDIFF_1:34;
end;
 
::f.x=r*arctanx

