      hence IExec(B1,s).a4 >= 7+IExec(B1,s).a6 by A1,A4,A38,A80,XXREAL_0:2;

    end;
    suppose
A81:  t2.DataLoc(t2.a,5) > 0;
      s.a4-1 >= 7+s.a6-1 by A1,XREAL_1:11;
      hence IExec(B1,s).a4 >= 7+IExec(B1,s).a6 by A78,A81;
