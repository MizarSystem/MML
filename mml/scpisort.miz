  end;
  hereby
    per cases;
    suppose
A80:  t2.DataLoc(t2.GBP,5) <= 0;
      then IExec(IF,t2).a4=IExec(FA,t2).a4 by SCMPDS_6:88
        .=Exec(Fi,t02).a4 by SCMPDS_5:45
        .=s.a4 by A24,A37,AMI_3:52,SCMPDS_2:58;
