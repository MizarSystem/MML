end;

Lm3: now
  let A;
  set FR = RRing(A);
  let h, a be Element of FR;
  reconsider g = h as Element of Funcs(A,REAL);
  assume
A1: a = RealFuncUnit(A);
  hence h * a = (RealFuncMult(A)).(RealFuncUnit(A),g) by Th18
    .= h by Th20;
  thus a * h = h by A1,Th20;
end;

registration
  let A;
  cluster RRing(A) -> unital;
  coherence
  proof
    reconsider e = RealFuncUnit(A) as Element of RRing(A);
    take e;
    thus thesis by Lm3;
  end;
