  let x, e be Element of F;
  reconsider f = x as Element of Funcs(A,REAL);
  assume
A1: e = RealFuncUnit(A);
  hence x * e = (RealFuncMult(A)).(RealFuncUnit(A),f) by Th18
    .= x by Th20;
  thus e * x = x by A1,Th20;
