A2: p in c and
  p in c and
A3: lower_bound (p2W.:c) = p2W.p by FUNCT_2:115;
A4: c = N-most X by PRE_TOPC:29;
  then reconsider p as Point of TOP-REAL 2 by A2;
  p in LSeg(NW-corner X, NE-corner X) by A4,A2,XBOOLE_0:def 4;
