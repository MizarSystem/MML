theorem Th2:
  for a, b being real number st b > 0
  ex r being real number st r = b*-[\ a/b /]+a & 0 <= r & r < b
proof
  let a, b be real number such that
A1: b > 0;
  set ab = [\ a/b /];
  set i = -ab;
  take r = b*i+a; thus r = b*i+a;
A2: ab <= a/b & a/b-1 < ab by INT_1:def 4;
  then ab*b <= a/b*b by A1,XREAL_1:66;
  then ab*b <= a by A1,XCMPLX_1:88;
  then -(ab*b) >= -a by XREAL_1:26;
  then b*i+a >= a+-a by XREAL_1:8;
  hence 0 <= r;
  -(a/b-1) > i by A2,XREAL_1:26;
  then (-(a/b)+1)*b > i*b by A1,XREAL_1:70;
  then -(a/b*b)+b > i*b;
  then -a+b > i*b by A1,XCMPLX_1:88;
  then -a+b+a > r by XREAL_1:10;
  hence r < b;
end;

theorem Th3:
  for a, b, c being real number st a > 0 & b >= 0 & c >= 0 & b < a & c < a
  for i being Integer st b = c + a*i holds b = c
proof
  let a, b, c be real number such that
A1: a > 0 and
A2: b >= 0 and
A3: c >= 0 and
A4: b < a and
A5: c < a;
  let i be Integer such that
A6: b = c + a*i;
A7: 0+a <= c+a by A3,XREAL_1:9;
  per cases;
  suppose i < 0;
    then i <= -1 by INT_1:21;
    then a*i <= a*(-1) by A1,XREAL_1:66;
    then c+a*i <= c-a by XREAL_1:9;
    hence b = c by A2,A5,A6,XREAL_1:51;
  end;
  suppose i = 0;
    hence b = c by A6;
  end;
  suppose i > 0;
    then 0+1 <= i by INT_1:20;
    then a*i >= a by A1,XREAL_1:153;
    then c+a*i >= c+a by XREAL_1:9;
    hence b = c by A4,A6,A7,XXREAL_0:2;
  end;
end;

theorem Th4:
  for a, b being real number holds sin(a-b) = sin(a)*cos(b)-cos(a)*sin(b) &
  cos(a-b) = cos(a)*cos(b)+sin(a)*sin(b)
proof
  let th1, th2 be real number;
  thus sin(th1-th2)=sin.(th1+(-th2)) by SIN_COS:def 21
    .=(sin.(th1)) *(cos.(-th2))+(cos.(th1)) * (sin.(-th2)) by SIN_COS:79
    .=(sin.(th1)) *(cos.(th2))+(cos.(th1)) * (sin.(-th2)) by SIN_COS:33
    .=(sin.(th1)) *(cos.(th2))+(cos.(th1)) * -(sin.(th2)) by SIN_COS:33
    .=(sin(th1)) *(cos.(th2))-(cos.(th1)) * (sin.(th2)) by SIN_COS:def 21
    .=(sin(th1)) *(cos(th2))-(cos.(th1)) * (sin.(th2)) by SIN_COS:def 23
    .=(sin(th1)) *(cos(th2))-(cos(th1)) * (sin.(th2)) by SIN_COS:def 23
    .=(sin(th1)) *(cos(th2))-(cos(th1)) * (sin(th2)) by SIN_COS:def 21;
  thus cos(th1-th2)=cos.(th1+(-th2)) by SIN_COS:def 23
    .=(cos.(th1)) *(cos.(-th2))-(sin.(th1)) * (sin.(-th2)) by SIN_COS:79
    .=(cos.(th1)) *(cos.(th2))-(sin.(th1)) * (sin.(-th2)) by SIN_COS:33
    .=(cos.(th1)) *(cos.(th2))-(sin.(th1)) * -(sin.(th2)) by SIN_COS:33
    .=(cos(th1)) *(cos.(th2))+(sin.(th1)) * (sin.(th2)) by SIN_COS:def 23
    .=(cos(th1)) *(cos(th2))+(sin.(th1)) * (sin.(th2)) by SIN_COS:def 23
    .=(cos(th1)) *(cos(th2))+(sin(th1)) * (sin.(th2)) by SIN_COS:def 21
    .=(cos(th1)) *(cos(th2))+(sin(th1)) * (sin(th2)) by SIN_COS:def 21;
end;

theorem
  for a being real number holds sin.(a-PI) = -sin.a & cos.(a-PI) = -cos.a
proof
  let th be real number;
  thus sin.(th- PI)
     = (sin.(th))* (cos.(-PI)) + (cos.(th)) *( sin.(-PI)) by SIN_COS:79
    .= (sin.(th))* (cos.(PI)) + (cos.(th)) *(sin.(-PI)) by SIN_COS:33
    .= (sin.(th))* (cos.(PI)) + (cos.(th)) *(-(sin.(PI))) by SIN_COS:33
    .= -sin.th by SIN_COS:81;
  thus cos.(th- PI)
     = (cos.(th)) * (cos.(-PI)) - ( sin.(th)) *( sin.(-PI)) by SIN_COS:79
    .= (cos.(th)) * (cos.(PI)) - ( sin.(th)) *( sin.(-PI)) by SIN_COS:33
    .= (cos.(th)) * (cos.(PI)) - ( sin.(th)) *(-(sin.(PI))) by SIN_COS:33
    .= -cos.th by SIN_COS:81;
end;

theorem Th6:
  for a being real number holds sin(a-PI) = -sin a & cos(a-PI) = -cos a
proof
  let r be real number;
  thus sin (r-PI) = sin(r)*cos(PI)-cos(r)*sin(PI) by Th4
    .= -sin r by SIN_COS:82;
  thus cos (r-PI) = cos(r)*cos(PI)+sin(r)*sin(PI) by Th4
    .= -cos r by SIN_COS:82;
end;

theorem Th7:
  for a, b being real number st a in ].0,PI/2.[ & b in ].0,PI/2.[
  holds a < b iff sin a < sin b
proof
  let a, b be real number such that
A1: a in ].0,PI/2.[ & b in ].0,PI/2.[;
A2: sin a = sin.a & sin b = sin.b by SIN_COS:def 21;
A3: a in ].0,PI/2.[ /\ dom sin & b in ].0,PI/2.[ /\ dom sin
  by A1,XBOOLE_0:def 4,SIN_COS:27;
  hence a < b implies sin a < sin b by A2,RFUNCT_2:43,SIN_COS2:2;
  assume
A4: sin a < sin b;
  assume a >= b;
  then a > b by A4,XXREAL_0:1;
  hence contradiction by A2,A3,A4,RFUNCT_2:43,SIN_COS2:2;
end;

theorem Th8:
  for a, b being real number st a in ].PI/2,PI.[ & b in ].PI/2,PI.[
  holds a < b iff sin a > sin b
proof
  let a, b be real number such that
A1: a in ].PI/2,PI.[ & b in ].PI/2,PI.[;
A2: sin a = sin.a & sin b = sin.b by SIN_COS:def 21;
A3: a in ].PI/2,PI.[ /\ dom sin & b in ].PI/2,PI.[ /\ dom sin
  by A1,XBOOLE_0:def 4,SIN_COS:27;
  hence a < b implies sin a > sin b by A2,RFUNCT_2:44,SIN_COS2:3;
  assume
A4: sin a > sin b;
  assume a >= b;
  then a > b by A4,XXREAL_0:1;
  hence contradiction by A2,A3,A4,RFUNCT_2:44,SIN_COS2:3;
end;

theorem Th9:
  for a being real number, i being Integer holds sin a = sin (2*PI*i+a)
proof
  let r being real number, i being Integer;
A1: sin.r = sin r by SIN_COS:def 21;
A2: sin.(2*PI*i+r) = sin (2*PI*i+r) by SIN_COS:def 21;
A3: sin.(2*PI*(-i)+(2*PI*i+r)) = sin (2*PI*(-i)+(2*PI*i+r)) by SIN_COS:def 21;
  per cases;
  suppose i >= 0;
    then reconsider iN = i as Element of NAT by INT_1:16;
    sin r = sin (2*PI*iN+r) by A1,A2,SIN_COS2:10;
    hence sin r = sin (2*PI*i+r);
  end;
  suppose i < 0;
    then reconsider iN = -i as Element of NAT by INT_1:16;
    reconsider aa = 2*PI*i+r as Real;
    sin (aa) = sin (2*PI*iN+aa) by A2,A3,SIN_COS2:10;
    hence sin r = sin (2*PI*i+r);
  end;
end;

theorem Th10:
  for a being real number, i being Integer holds cos a = cos (2*PI*i+a)
proof
  let r be real number, i being Integer;
A1: cos.r = cos r by SIN_COS:def 23;
A2: cos.(2*PI*i+r) = cos (2*PI*i+r) by SIN_COS:def 23;
A3: cos.(2*PI*(-i)+(2*PI*i+r)) = cos (2*PI*(-i)+(2*PI*i+r)) by SIN_COS:def 23;
  per cases;
  suppose i >= 0;
    then reconsider iN = i as Element of NAT by INT_1:16;
    cos r = cos (2*PI*iN+r) by A1,A2,SIN_COS2:11;
    hence cos r = cos (2*PI*i+r);
  end;
  suppose i < 0;
    then reconsider iN = -i as Element of NAT by INT_1:16;
    reconsider aa = 2*PI*i+r as Real;
    cos (aa) = cos (2*PI*iN+aa) by A2,A3,SIN_COS2:11;
    hence cos r = cos (2*PI*i+r);
  end;
end;

theorem Th11:
  for a being real number st sin a = 0 holds cos a <> 0
proof
  let a be real number;
  consider r being real number such that
A1: r = 2*PI*-[\ a/(2*PI) /]+a and
A2: 0 <= r & r < 2*PI by Th2,COMPTRIG:21;
A3: sin a = sin r by A1,Th9;
A4: cos a = cos r by A1,Th10;
  assume sin a = 0 & cos a = 0;
  then (r = 0 or r = PI) & (r = PI/2 or r = 3/2*PI) by A2,A3,A4,COMPTRIG:33,34;
  hence thesis by COMPTRIG:21;
end;

theorem Th12:
  for a, b being real number
  st 0 <= a & a < 2*PI & 0 <= b & b < 2*PI & sin a = sin b & cos a = cos b
  holds a = b
proof
  let r, s be real number such that
A1: 0 <= r and
A2: r < 2*PI and
A3: 0 <= s and
A4: s < 2*PI and
A5: sin r = sin s and
A6: cos r = cos s;
A7: cos(r-s)=(cos r)*(cos s)+(sin r)*(sin s) by Th4
    .= 1 by A5,A6,SIN_COS:32;
A8: cos (s-r)=(cos r)*(cos s)+(sin r)*(sin s) by Th4
    .= 1 by A5,A6,SIN_COS:32;
A9: sin(r-s)=(sin r)*(cos s)-(cos r)*(sin s) by Th4
    .= 0 by A5,A6;
A10: sin (s-r)=(sin s)*(cos r)-(cos s)*(sin r) by Th4
    .= 0 by A5,A6;
  per cases by XXREAL_0:1;
  suppose r > s;
    then r > s+0;
    then
A11: 0 <= r-s by XREAL_1:22;
    r+0 < 2*PI+s by A2,A3,XREAL_1:10;
    then r-s < 2*PI by XREAL_1:21;
    then r-s = 0 or r-s = PI by A9,A11,COMPTRIG:33;
    hence r = s by A7,SIN_COS:82;
  end;
  suppose r < s;
    then s > r+0;
    then
A12: 0 <= s-r by XREAL_1:22;
    s+0 < 2*PI+r by A1,A4,XREAL_1:10;
    then s-r < 2*PI by XREAL_1:21;
    then s-r = 0 or s-r = PI by A10,A12,COMPTRIG:33;
    hence r = s by A8,SIN_COS:82;
  end;
  suppose r = s;
    hence thesis;
  end;
end;

begin :: More on the argument of a complex number

:: ALL these to be changed (mainly deleted) after the change in COMPTRIG

canceled 6;

theorem Th19:
  for z being complex number holds z = |.z.|*cos Arg z + |.z.|*sin Arg z *<i>
proof
  let a be complex number;
  per cases;
  suppose a <> 0;
    hence a = |.a.|*cos Arg a + |.a.|*sin Arg a *<i> by COMPTRIG:def 1;
  end;
  suppose a = 0c;
    hence thesis by COMPLEX1:130;
  end;
end;

Lm2: Arg 0 = 0 by COMPTRIG:53;

canceled 2;

theorem Th22:
  for z being complex number st z <> 0
  holds (Arg z < PI implies Arg -z = Arg z +PI) &
  (Arg z >= PI implies Arg -z = Arg z -PI)
proof
  let z be complex number;
  assume
A1: z <> 0;
  then
A2: |.z.| <> 0 by COMPLEX1:131;
A4: z = |.z.|*cos Arg z + (|.z.|*sin Arg z)*<i> by Th19;
A5: -z = |.-z.|*cos Arg -z + |.-z.|*(sin Arg -z)*<i> by Th19;
A6: -z= -|.z.|*cos Arg z +(-|.z.|*sin Arg z)*<i> by A4;
A7: |.z.| = |.-z.| by COMPLEX1:138;
  then |.z.|*cos Arg -z = |.z.|*-cos Arg z by A5,A6,COMPLEX1:163;
  then cos Arg -z = -cos Arg z by A2,XCMPLX_1:5;
  then
A8: cos Arg -z = cos (Arg z +PI) & cos Arg -z = cos (Arg z -PI)
  by Th6,SIN_COS:84;
  |.z.|*sin Arg -z = |.z.|*-sin Arg z by A5,A6,A7,COMPLEX1:163;
  then sin Arg -z = -sin Arg z by A2,XCMPLX_1:5;
  then
A9: sin Arg -z = sin (Arg z +PI) & sin Arg -z = sin (Arg z -PI)
  by Th6,SIN_COS:84;
  hereby
    assume Arg z<PI;
    then
A10: Arg z+PI<PI+PI by XREAL_1:10;
    0 <= Arg z by COMPTRIG:52;
    hence Arg -z = Arg z +PI by A1,A5,A8,A9,A10,COMPTRIG:def 1,COMPTRIG:21;
  end;
  assume Arg z >= PI;
  then
A11: Arg z -PI >= PI-PI by XREAL_1:11;
  Arg z < 2*PI by COMPTRIG:52;
  then Arg z +0 < 2*PI+PI by COMPTRIG:21,XREAL_1:10;
  then Arg z -PI < 2*PI by XREAL_1:21;
  hence Arg -z = Arg z -PI by A1,A5,A8,A9,A11,COMPTRIG:def 1;
end;

theorem Th23:
  for r being Real st r >= 0 holds Arg r = 0
proof
  let r be Real such that
A1: r >= 0;
B2: r = r+0*<i>;
A2: |.r.| = r by A1,ABSVALUE:def 1;
B3:  r = |.r.|*cos Arg r + |.r.|*sin Arg r *<i> by Th19;
  then
A3: r =|.r.|*cos Arg r & 0=|.r.|*sin Arg r by B2,COMPLEX1:163;
  per cases by A1;
  suppose r = 0;
    hence Arg r = 0 by COMPTRIG:53;
  end;
  suppose
A4: r > 0;
    then
A5: cos Arg r = 1 by A2,A3,XCMPLX_1:7;
A6: sin Arg r = 0 by A2,B3,A4,B2,COMPLEX1:163;
    0 <= Arg r & Arg r < 2*PI by COMPTRIG:52;
    hence Arg r = 0 by A5,A6,Th12,SIN_COS:34;
  end;
end;

theorem Th24:
  for z being complex number holds Arg z = 0 iff z = |.z.|
proof
  let z be complex number;
  hereby
    assume Arg z=0;
    then z= |.z.|*cos 0 + |.z.|*sin 0 *<i> by Th19;
    hence z= |.z.| by SIN_COS:34;
  end;
  assume z= |.z.|;
  hence Arg z = 0 by Th23,COMPLEX1:132;
end;

theorem Th25:
  for z being complex number st z <> 0 holds Arg(z) < PI iff Arg -z >= PI
proof
  let z be complex number;
  assume
A1: z<>0;
  thus Arg(z)<PI implies Arg(-z)>=PI
  proof
    assume
A3: Arg(z)<PI;
    Arg(z)>=0 by COMPTRIG:52;
    then PI+0<=PI+Arg(z) by XREAL_1:9;
    hence Arg(-z)>=PI by A1,A3,Th22;
  end;
  thus Arg(-z)>=PI implies Arg(z)<PI
  proof
    assume Arg(-z)>=PI;
    then
A4: Arg(--z)=Arg(-z)-PI by A1,Th22;
    2*PI>Arg(-z) by COMPTRIG:52;
    then PI+PI-PI>Arg(-z)-PI by XREAL_1:11;
    hence Arg(z)<PI by A4;
  end;
end;

theorem Th26:
  for x, y being complex number st x <> y or x - y <> 0 holds
  Arg(x-y) < PI iff Arg(y-x) >= PI
proof
  let z1,z2 be complex number;
  assume z1<>z2 or z1 - z2 <> 0;
  then z1-z2<>0c;
  then Arg(z1-z2)<PI iff Arg(-(z1-z2))>=PI by Th25;
  hence Arg(z1-z2)<PI iff Arg(z2-z1)>=PI;
end;

theorem Th27:
  for z being complex number holds Arg z in ].0,PI.[ iff Im z > 0
proof
  let z be complex number;
  thus Arg z in ].0,PI.[ implies Im z > 0
  proof
    assume Arg z in ].0,PI.[;
    then
A1: 0<Arg(z) & Arg(z)<PI by XXREAL_1:4;
A2: Arg(z)<PI/2 or Arg(z)=PI/2 or Arg(z)>PI/2 by XXREAL_0:1;
    now per cases by A1,A2,XXREAL_1:4;
      case Arg(z) in ].0,PI/2.[;
        hence Im z>0 by COMPTRIG:59;
      end;
      case Arg(z)=PI/2;
        hence Im z>0 by COMPTRIG:66,SIN_COS:82;
      end;
      case Arg(z) in ].PI/2,PI.[;
        hence Im z>0 by COMPTRIG:60;
      end;
    end;
    hence Im z>0;
  end;
A3: ].0,PI/2.[ c= ].0,PI.[ by COMPTRIG:21,XXREAL_1:46;
A4: ].PI/2,PI.[ c= ].0,PI.[ by COMPTRIG:21,XXREAL_1:46;
  thus Im z > 0 implies Arg z in ].0,PI.[
  proof
    assume
A5: Im z >0;
    now per cases;
      case Re z>0;
        then Arg(z) in ].0,PI/2.[ by A5,COMPTRIG:59;
        hence Arg(z) in ].0,PI.[ by A3;
      end;
      case Re z=0;
        then z= 0 + (Im z)*<i> by COMPLEX1:29;
        then Arg(z)=PI/2 by A5,COMPTRIG:55;
        hence Arg(z) in ].0,PI.[ by COMPTRIG:21,XXREAL_1:4;
      end;
      case Re z<0;
        then Arg z in ].PI/2,PI.[ by A5,COMPTRIG:60;
        hence Arg(z) in ].0,PI.[ by A4;
      end;
    end;
    hence Arg z in ].0,PI.[;
  end;
end;

theorem
  for z being complex number st Arg z <> 0 holds Arg z < PI iff sin Arg z> 0
proof
  let z be complex number;
  assume
A1: Arg(z)<>0;
A2: Arg(z)>=0 by COMPTRIG:52;
  hereby
    assume Arg(z)<PI;
    then Arg(z) in ].0,PI.[ by A1,A2,XXREAL_1:4;
    then Im (z)>0 by Th27;
    hence sin (Arg(z))>0 by COMPTRIG:63;
  end;
A3: ].0,PI/2.[ c= ].0,PI.[ by COMPTRIG:21,XXREAL_1:46;
  thus sin (Arg(z))>0 implies Arg(z)<PI
  proof
    assume
A4: sin (Arg(z))>0;
    then
A5: Im (z)>0 by COMPTRIG:66;
    now per cases;
      suppose Re z>0;
        then Arg(z) in ].0,PI/2.[ by A5,COMPTRIG:59;
        hence Arg(z)<PI by A3,XXREAL_1:4;
      end;
      suppose Re z=0;
        then z= 0 + (Im z)*<i> by COMPLEX1:29;
        hence Arg(z)<PI by A4,COMPTRIG:21,55,66;
      end;
      suppose Re z<0;
        then Arg(z) in ].PI/2,PI.[ by A5,COMPTRIG:60;
        hence Arg(z)<PI by XXREAL_1:4;
      end;
    end;
    hence Arg(z)<PI;
  end;
end;

theorem
  for x, y being complex number st Arg x < PI & Arg y < PI holds Arg(x+y) < PI
proof
  let z1,z2 be complex number;
  assume
A1: Arg(z1)<PI & Arg(z2)<PI;
A2: |.z2.| = |.z2.|+0*<i>;
A3: |.z1.| = |.z1.|+0*<i>;
  per cases by COMPTRIG:52;
    suppose
B4:  Arg(z1)=0;
      then z1= |.z1.| by Th24;
      then
A5:   z1=|.z1.| & Im z1=0 by A3,COMPLEX1:28;
      per cases by COMPTRIG:52;
        suppose Arg(z2)=0;
          then
B7:         z2= |.z2.| by Th24;
A10:          z1+z2= |.z1.| + |.z2.| + 0*<i> by B4,B7,Th24;
A11:          0<= |.z1.| by COMPLEX1:132;
              0<= |.z2.| by COMPLEX1:132;
              hence Arg(z1+z2)<PI by A10,COMPTRIG:21,53,A11;
        end;
        suppose Arg(z2)>0;
          then Arg(z2) in ].0,PI.[ by A1,XXREAL_1:4;
          then
A12:      Im z2>0 by Th27;
          Im (z1+z2)=(Im z1)+(Im z2) by COMPLEX1:19;
          then Arg(z1+z2) in ].0,PI.[ by A5,A12,Th27;
          hence Arg(z1+z2)<PI by XXREAL_1:4;
        end;
    end;
    suppose Arg(z1)>0;
      then Arg(z1) in ].0,PI.[ by A1,XXREAL_1:4;
      then
A13:  Im z1>0 by Th27;
      per cases by COMPTRIG:52;
        suppose Arg(z2)=0;
          then z2= |.z2.| by Th24;
          then
A14:      z2= |.z2.| & Im z2=0 by A2,COMPLEX1:28;
          Im (z1+z2)=(Im z1)+(Im z2) by COMPLEX1:19;
          then Arg(z1+z2) in ].0,PI.[ by A13,A14,Th27;
          hence Arg(z1+z2)<PI by XXREAL_1:4;
        end;
        suppose Arg(z2)>0;
          then Arg(z2) in ].0,PI.[ by A1,XXREAL_1:4;
          then
A15:      Im z2>0 by Th27;
          Im (z1+z2)=(Im z1)+(Im z2) by COMPLEX1:19;
          then Arg(z1+z2) in ].0,PI.[ by Th27,A13,A15;
          hence Arg(z1+z2)<PI by XXREAL_1:4;
        end;
    end;
end;

canceled 4;

theorem Th34:
  for z being complex number holds Arg z = 0 iff Re z >= 0 & Im z = 0
proof
  let z being complex number;
A1: |.z.| = |.z.|+0*<i>;
  hereby
    assume Arg z = 0;
    then z = |.z.| by Th24;
    then Re z = |.z.| & Im z = 0 by A1,COMPLEX1:28;
    hence Re z >= 0 & Im z = 0 by COMPLEX1:132;
  end;
  assume
A2: Re z >= 0 & Im z = 0;
  then z = Re z +0*<i> by COMPLEX1:29;
  hence Arg z = 0 by A2,COMPTRIG:53;
end;

theorem Th35:
  for z being complex number holds Arg z = PI iff Re z < 0 & Im z=0
proof
  let z be complex number;
  hereby
    assume
A1: Arg z = PI;
    per cases;
    suppose
A2:   z<>0;
      then
B3:    z=|.z.|*cos PI+|.z.|*sin PI *<i> by A1,COMPTRIG:def 1;
      --|.z.| > 0 by A2,COMPLEX1:133;
      hence Re z < 0 & Im z = 0 by B3,COMPLEX1:def 2,def 3,SIN_COS:82;
    end;
    suppose z=0;
      hence Re z < 0 & Im z = 0 by A1,COMPTRIG:21,53;
    end;
  end;
  assume
A4: Re z < 0 & Im z=0;
  then z = Re z +0*<i> by COMPLEX1:29;
  hence Arg z = PI by A4,COMPTRIG:54;
end;

theorem Th36:
  for z being complex number holds Im z = 0 iff Arg z = 0 or Arg z = PI
proof
  let z be complex number;
  hereby
    assume
A1: Im z = 0;
    Arg(Re z+0*<i>) = 0 or Arg(Re z+0*<i>) = PI by COMPTRIG:53,54;
    hence Arg z = 0 or Arg z = PI by A1,COMPLEX1:29;
  end;
  assume Arg z = 0 or Arg z = PI;
  hence Im z = 0 by Th34,Th35;
end;

theorem Th37:
  for z being complex number st Arg z <= PI holds Im z >= 0
proof
  let z be complex number;
  assume
A1: Arg z <= PI;
  per cases by A1,COMPTRIG:52,XXREAL_0:1;
  suppose Arg z = PI or Arg z = 0;
    hence Im z >= 0 by Th36;
  end;
  suppose 0 < Arg z & Arg z < PI;
    then Arg z in ].0,PI.[ by XXREAL_1:4;
    hence Im z >= 0 by Th27;
  end;
end;

theorem Th38:
  for z being Element of COMPLEX st z <> 0
  holds cos Arg -z = -cos Arg z & sin Arg -z = - sin Arg z
proof
  let a be Element of COMPLEX;
  assume
A1: a <> 0;
A2: a = |.a.|*cos Arg a+(|.a.|*sin Arg a)*<i> by Th19;
A3: -a = |.-a.|*cos Arg -a+(|.-a.|*sin Arg -a)*<i> by Th19;
  |.-a.| = |.a.| by COMPLEX1:138;
  then
A4: 0+0*<i> = |.a.|*cos Arg a + (|.a.|*cos Arg -a)+
  (|.a.|*sin Arg a + (|.a.|*sin Arg -a))*<i> by A2,A3;
  then
A5: |.a.|*(cos Arg a + cos Arg -a) = 0 by COMPLEX1:12,28;
A6: |.a.|*(sin Arg a+sin Arg -a)= 0 by A4,COMPLEX1:12,28;
  |.a.|<>0 by A1,COMPLEX1:131;
  then cos Arg a +--cos Arg -a = 0 & sin Arg a +--sin Arg -a = 0 by A5,A6;
  hence cos Arg -a = - cos Arg a & sin Arg -a = - sin Arg a;
end;

theorem Th39:
  for a being complex number st a <> 0
  holds cos Arg a = Re a / |.a.| & sin Arg a = Im a / |.a.|
proof
  let a be complex number;
  assume
A1: a <> 0;
  a = |.a.|*cos Arg a+(|.a.|*sin Arg a)*<i> by Th19;
  then Re a = |.a.|*cos Arg a & Im a = |.a.|*sin Arg a by COMPLEX1:28;
  hence cos Arg a = Re a / |.a.| & sin Arg a = Im a / |.a.| by A1,COMPLEX1:131
,XCMPLX_1:90;
end;

theorem Th40:
  for a being complex number, r being Real st r > 0
  holds Arg(a*r) = Arg a
proof
  let a be complex number, r be Real such that
A1: r > 0;
  per cases;
  suppose a = 0;
    hence Arg(a*r) = Arg a;
  end;
  suppose
A2: a <> 0;
    set b = a*r;
    r=r+0*<i>;
    then
A4: Re r = r & Im r = 0 by COMPLEX1:28;
    then
A5: Re b = Re a * r - 0*Im a by COMPLEX1:24
      .= r*Re a;
A6: Im b = Re a * 0 + r * Im a by A4,COMPLEX1:24
      .= r*Im a;
A7: |.b.| = |.a.|*abs r by COMPLEX1:151
      .= |.a.|*r by A1,ABSVALUE:def 1;
A9: cos Arg a = Re a/|.a.| & sin Arg a = Im a/|.a.| by A2,Th39;
A10: cos Arg b = Re b/|.b.|by A2,Th39,A1
      .=cos Arg a by A1,A5,A7,A9,XCMPLX_1:92;
A11: sin Arg b = Im b /|.b.| by A2,Th39,A1
      .=sin Arg a by A1,A6,A7,A9,XCMPLX_1:92;
    0 <= Arg b & Arg b < 2*PI & 0 <= Arg a & Arg a < 2*PI by COMPTRIG:52;
    hence Arg(a*r) = Arg a by A10,A11,Th12;
  end;
end;

theorem Th41:
  for a being complex number, r being Real st r < 0
  holds Arg(a*r) = Arg -a
proof
  let a be complex number, r be Real such that
A1: r < 0;
  per cases;
  suppose a = 0;
    hence Arg(a*r) = Arg -a;
  end;
  suppose
A2: a <> 0;
    set b = a*r;
A3: a in COMPLEX & b in COMPLEX by XCMPLX_0:def 2;
    r =r+0*<i>;
    then
A5: Re r = r & Im r = 0 by COMPLEX1:28;
    then
A6: Re b = Re a * r - 0*Im a by COMPLEX1:24
      .= r*Re a;
A7: Im b = Re a * 0 + r * Im a by A5,COMPLEX1:24
      .= r*Im a;
A8: |.b.| = |.a.|*abs r by COMPLEX1:151
      .= |.a.|*(-r) by A1,ABSVALUE:def 1;
A11: cos Arg a = Re a/|.a.| & sin Arg a = Im a/|.a.| by A2,Th39;
A12: cos Arg b = r*Re a/(-|.a.|*r) by A6,A8,A2,Th39,A1
      .= -r*Re a/(
    |.a.|*r) by XCMPLX_1:189
      .= -cos Arg a by A1,A11,XCMPLX_1:92
      .= cos Arg -a by A2,A3,Th38;
A13: sin Arg b = r*Im a/(-|.a.|*r) by A7,A8,A2,Th39,A1
      .= -r*Im a/(
    |.a.|*r) by XCMPLX_1:189
      .= -sin Arg a by A1,A11,XCMPLX_1:92
      .= sin Arg -a by A2,A3,Th38;
    0 <= Arg b & Arg b < 2*PI & 0 <= Arg -a & Arg -a < 2*PI by COMPTRIG:52;
    hence Arg(a*r) = Arg -a by A12,A13,Th12;
  end;
end;

begin :: Inner product

definition :: Inner product of complex numbers
  let x, y be complex number;
  canceled 2;
  func x .|. y -> Element of COMPLEX equals

  x*(y*');
  correctness by XCMPLX_0:def 2;
end;

reserve a, b, c, d, x, y, z for Element of COMPLEX;

theorem Th42:
  x.|.y = (Re x)*(Re y)+(Im x)*(Im y) + (-(Re x)*(Im y)+(Im x)*(Re y))*<i>
proof
  x = Re x+(Im x)*<i> & y*' = Re y-(Im y)*<i> by COMPLEX1:29,def 15;
  hence x.|.y
   =(Re x)*(Re y)+(Im x)*(Im y)+(-((Re x)*(Im y))+(Im x)*(Re y)) *<i>;
end;

theorem Th43:
  z.|.z = (Re z)*(Re z)+(Im z)*(Im z) & z.|.z = (Re z)^2+(Im z)^2
proof thus z.|.z
  = (Re z)*(Re z)+(Im z)*(Im z)+(-((Im z)*(Re z))+(Im z)*(Re z))*<i> by Th42
    .= (Re z)*(Re z)+(Im z)*(Im z);
  hence z.|.z = (Re z)^2+(Im z)^2;
end;

theorem Th44:
  z.|.z = |.z.|^2 & |.z.|^2 = Re (z.|.z)
proof
A1: (Re z)^2>=0 by XREAL_1:65;
B2: (Im z)^2>=0 by XREAL_1:65;
A3: |.z.|=sqrt((Re z)^2 + (Im z)^2) by COMPLEX1:def 16;
A4: |.z.|^2 = |.z.|^2+0*<i>;
  thus z.|.z = (Re z)^2+(Im z)^2 by Th43
    .= |.z.|^2 by B2,A3,SQUARE_1:def 4,A1;
  hence thesis by A4,COMPLEX1:28;
end;

theorem Th45:
  |. x.|.y .| = |.x.|*|.y.|
proof thus |.(x.|.y).| =(|.x.|)*(|.y*'.|) by COMPLEX1:151
    .=|.x.|*|.y.| by COMPLEX1:139;
end;

theorem
  x.|.x = 0 implies x = 0
proof
  assume x.|.x = 0;
  then (Re x)^2+(Im x)^2 = 0 by Th43;
  hence x = 0 by COMPLEX1:13;
end;

theorem Th47:
  y.|.x = (x.|.y)*'
proof thus (y.|.x)=(y*x*')*'*'
    .=((x*')*'*(y*'))*' by COMPLEX1:121
    .=(x.|.y)*';
end;

theorem
  (x+y).|.z = x.|.z + y.|.z;

theorem Th49:
  x.|.(y+z) = x.|.y + x.|.z
proof thus x.|.(y+z) =x*(y*'+z*') by COMPLEX1:118
    .=(x.|.y)+(x.|.z);
end;

theorem
  (a*x).|.y = a * x.|.y;

theorem Th51:
  x.|.(a*y) = (a*') * x.|.y
proof thus x.|.(a*y) =x*((a*')*(y*')) by COMPLEX1:121
    .= (a*') * x.|.y;
end;

theorem
  (a*x).|.y = x.|.((a*')*y)
proof thus (a*x).|.y =x*((a*'*')*(y*'))
    .= x.|.((a*')*y) by COMPLEX1:121;
end;

theorem
  (a*x+b*y).|.z = a * x.|.z + b * y.|.z;

theorem
  x.|.(a*y + b*z) = (a*') * x.|.y + (b*') * x.|.z
proof thus x.|.(a*y + b*z) = x.|.(a*y) +x.|.(b*z) by Th49
    .=(a*')*x.|.y + x.|.(b*z) by Th51
    .=(a*') * x.|.y + (b*') * x.|.z by Th51;
end;

theorem Th55:
  (-x).|.y = x.|.(-y)
proof thus (-x).|.y = (-(1r*')) * x.|.y by COMPLEX1:115,def 7
    .=((-1r)*')* x.|.y by COMPLEX1:119
    .= x.|.((-1r)*y) by Th51
    .=x.|.(-y) by COMPLEX1:def 7;
end;

theorem
  (-x).|.y = - x.|.y;

theorem Th57:
  - x.|.y = x.|.(-y)
proof thus - x.|.y = (-x).|.y
    .=x.|.(-y) by Th55;
end;

theorem
  (-x).|.(-y) = x.|.y
proof (-x).|.(-y) = - x.|.(-y)
    .= - ( - x.|.y ) by Th57;
  hence thesis;
end;

theorem
  (x - y).|.z = x.|.z - y.|.z;

theorem Th60:
  x.|.(y - z) = x.|.y - x.|.z
proof thus x.|.(y-z) =x*(y*'-z*') by COMPLEX1:120
    .=(x.|.y)-(x.|.z);
end;

canceled;

theorem
  (x + y).|.(x + y) = x.|.x + x.|.y + y.|.x + y.|.y
proof (x + y).|.(x + y) = x.|.(x+y) + y.|.(x+y)
    .= x.|.x + x.|.y + y.|.(x+y) by Th49
    .= x.|.x + x.|.y + (y.|.x + y.|.y) by Th49
    .= x.|.x + x.|.y + y.|.x + y.|.y;
  hence thesis;
end;

theorem Th63:
  (x-y).|.(x-y) = x.|.x - x.|.y - y.|.x + y.|.y
proof (x - y).|.(x - y) =x.|.(x - y) - y.|.(x - y)
    .=x.|.x - x.|.y - y.|.(x - y) by Th60
    .=x.|.x - x.|.y - (y.|.x - y.|.y) by Th60
    .=x.|.x - x.|.y - y.|.x + y.|.y;
  hence thesis;
end;

Lm3: (|.z.|)^2 = (Re z)^2 + (Im z)^2
proof thus (|.z.|)^2 =|.z*z.| by COMPLEX1:151
    .= (Re z)^2 + (Im z)^2 by COMPLEX1:154;
end;

theorem Th64:
  Re (x.|.y) = 0 iff Im (x.|.y) = |.x.|*|.y.| or Im (x.|.y) = -|.x.|*|.y.|
proof
  hereby
    assume
A1: Re (x.|.y)=0;
    (|.x.|*|.y.|)^2=(|.(x.|.y).|)^2 by Th45
      .=0^2+ (Im (x.|.y))^2 by A1,Lm3
      .= (Im (x.|.y))^2;
    hence Im (x.|.y)=|.x.|*|.y.| or Im (x.|.y)= - (|.x.|*|.y.|)
    by SQUARE_1:109;
  end;
  hereby
    assume Im (x.|.y)=|.x.|*|.y.| or Im (x.|.y)= - (|.x.|*|.y.|);
    then (Im (x.|.y))^2= (|.x.|*|.y.|)^2
      .= (|.(x.|.y).|)^2 by Th45
      .=(Re (x.|.y))^2+(Im (x.|.y))^2 by Lm3;
    hence Re (x.|.y)=0;
  end;
end;

begin :: Rotation

definition
  let a be complex number, r be Real;
  func Rotate(a, r) -> Element of COMPLEX equals
   |.a.|*cos (r+Arg a) + |.a.|*sin (r+Arg a) *<i>;
  correctness by XCMPLX_0:def 2;
end;

reserve r for Real;

theorem
  Rotate(a, 0) = a by Th19;

theorem Th66:
  for a being complex number
  holds Rotate(a, r) = 0 iff a = 0
proof let a be complex number;
  hereby
    assume Rotate(a, r) = 0;
    then
B1:   Rotate(a, r) = 0+0*<i>;
    per cases by B1,COMPLEX1:163;
    suppose |.a.| = 0;
      hence a = 0 by COMPLEX1:131;
    end;
    suppose cos (r+Arg a) = 0 & sin (r+Arg a) = 0;
      hence a = 0 by Th11;
    end;
  end;
  assume a = 0;
  hence Rotate(a, r)= 0 by COMPLEX1:130;
end;

theorem Th67:
 for a being complex number holds |.Rotate(a,r).| = |.a.|
proof let a be complex number;
  Re Rotate(a,r) = |.a.|*cos (r+Arg a) & Im Rotate(a,r) = |.a.|*sin (r+
  Arg a) by COMPLEX1:28;
  hence |.Rotate(a,r).|
  = sqrt((|.a.|*cos (r+Arg a))^2 + (|.a.|*sin (r+Arg a))^2) by COMPLEX1:def 16
    .= sqrt(|.a.|^2*((cos (r+Arg a))^2 + (sin (r+Arg a))^2))
    .= sqrt(|.a.|^2*1) by SIN_COS:32
    .= |.a.| by COMPLEX1:132,SQUARE_1:89;
end;

theorem Th68:
 for a being complex number st a <> 0
  ex i being Integer st Arg(Rotate(a,r)) = 2*PI*i+(r+Arg(a))
proof let a be complex number;
  assume a <> 0;
  then
A1: Rotate(a, r) <> 0c by Th66;
A2: |.a.| = |.Rotate(a,r).| by Th67;
  consider AR being real number such that
A3: AR = 2*PI*-[\ (r+Arg a)/(2*PI) /]+(r+Arg a) and
A4: 0 <= AR and
A5: AR < 2*PI by Th2,COMPTRIG:21;
  reconsider ar = AR as Real by XREAL_0:def 1;
A6: cos (r+Arg a) = cos ar by A3,Th10;
A7: sin (r+Arg a) = sin ar by A3,Th9;
  take i = -[\ (r+Arg a)/(2*PI) /];
  thus thesis by A1,A2,A3,A4,A5,A6,A7,COMPTRIG:def 1;
end;

theorem
  Rotate(a,-Arg a) = |.a.| by SIN_COS:34;

theorem Th70:
  Re Rotate(a,r) = (Re a)*(cos r)-(Im a)*(sin r) &
  Im Rotate(a,r) = (Re a)*(sin r)+(Im a)*(cos r)
proof
  set ra = Rotate(a,r);
  a = |.a.|*cos Arg a+(|.a.|*sin Arg a)*<i> by Th19;
  then
A2: Re a = |.a.|*cos Arg a & Im a = |.a.|*sin Arg a by COMPLEX1:28;
  thus Re Rotate(a,r) = |.a.|*cos (r+Arg a) by COMPLEX1:28
    .= |.a.|*((cos r)*cos(Arg a)-(sin r)*sin(Arg a)) by SIN_COS:80
    .= (Re a)*(cos r)-(Im a)*(sin r) by A2;
  thus Im Rotate(a,r) = |.a.|*sin(r+Arg a) by COMPLEX1:28
    .= |.a.|*((sin r)*cos(Arg a)+(cos r)*sin(Arg a)) by SIN_COS:80
    .= (Re a)*(sin r)+(Im a)*(cos r) by A2;
end;

theorem Th71:
  Rotate(a+b,r) = Rotate(a,r)+Rotate(b,r)
proof
  set ab = a+b;
  set rab = Rotate(ab,r),ra = Rotate(a,r), rb = Rotate(b,r);
A1: Re rab = (Re ab)*(cos r)-(Im ab)*(sin r) &
  Im rab = (Re ab)*(sin r)+(Im ab)*(cos r) by Th70;
A2: Re ra = (Re a)*(cos r)-(Im a)*(sin r) &
  Im ra = (Re a)*(sin r)+(Im a)*(cos r) by Th70;
A3: Re rb = (Re b)*(cos r)-(Im b)*(sin r) &
  Im rb = (Re b)*(sin r)+(Im b)*(cos r) by Th70;
A4: Re ab = Re a + Re b & Im ab = Im a + Im b by COMPLEX1:19;
A5: Re (ra+rb) = (Re a)*(cos r)-(Im a)*(sin r)+((Re b)*(cos r)-(Im b)*(sin r))
  by A2,A3,COMPLEX1:19
    .= Re rab by A1,A4;
  Im (ra+rb) = (Re a)*(sin r)+(Im a)*(cos r)+((Re b)*(sin r)+(Im b)*(cos r))
  by A2,A3,COMPLEX1:19
    .= Im rab by A1,A4;
  hence Rotate(a+b,r) = Rotate(a,r)+Rotate(b,r) by A5,COMPLEX1:9;
end;

theorem Th72:
  Rotate(-a,r) = -Rotate(a,r)
proof
  per cases;
  suppose
A1: a <> 0c;
A3: |.a.| = |.-a.| by COMPLEX1:138;
    cos (r+Arg-a) = -cos(r+Arg a) & sin (r+Arg-a) = -sin(r+Arg a)
    proof
      per cases;
      suppose Arg a < PI;
        then Arg -a = PI+Arg a by A1,Th22;
        then r+Arg-a = PI+(r+Arg a);
        hence thesis by SIN_COS:84;
      end;
      suppose Arg a >= PI;
        then Arg -a = (Arg a)-PI by A1,Th22;
        then r+Arg-a = (Arg a)+r-PI;
        hence thesis by Th6;
      end;
    end;
    hence Rotate(-a,r) = -Rotate(a,r) by A3;
  end;
  suppose
A4: a = 0c;
    hence Rotate(-a,r) = -0 by Th66
      .= -Rotate(a,r) by A4,Th66;
  end;
end;

theorem Th73:
  Rotate(a-b,r) = Rotate(a,r)-Rotate(b,r)
proof thus Rotate(a-b,r) = Rotate(a,r)+Rotate(-b,r) by Th71
    .= Rotate(a,r)-Rotate(b,r) by Th72;
end;

theorem Th74:
  Rotate(a, PI) = -a
proof
A1: a = |.a.|*cos Arg a+(|.a.|*sin Arg a)*<i> by Th19;
A2: |.a.|*-cos Arg a = -(|.a.|*cos Arg a)
    .= - Re a by A1,COMPLEX1:28;
A3: |.a.|*-sin Arg a = -(|.a.|*sin Arg a)
    .= - Im a by A1,COMPLEX1:28;
  thus Rotate(a, PI)
     = |.a.|*-cos Arg a + |.a.|*sin (PI+Arg a) *<i> by SIN_COS:84
    .= |.a.|*-cos Arg a + (|.a.|*-sin Arg a) *<i> by SIN_COS:84
    .= -a by A2,A3,COMPLEX1:def 11;
end;

begin :: Angles

definition
  let a, b be complex number;
  func angle(a,b) -> Real equals
  :Def5:
  Arg(Rotate(b, -Arg a)) if Arg a = 0 or b <> 0 otherwise 2*PI - Arg a;
  correctness;
end;

theorem Th75:
 for a being complex number holds
  r >= 0 implies angle(r,a) = Arg a
proof let a be complex number;
  assume r >= 0;
  then Arg r = 0 by Th23;
  hence angle(r,a) = Arg(Rotate(a,-0)) by Def5
    .= Arg a by Th19;
end;

theorem Th76:
 for a,b being complex number holds
  Arg a = Arg b & a <> 0 & b <> 0 implies Arg Rotate(a,r) = Arg Rotate(b,r)
proof let a,b be complex number;
  assume that
A1: Arg a = Arg b and
A2: a <> 0 and
A3: b <> 0;
  consider i being Integer such that
A4: Arg(Rotate(a,r)) = 2*PI*i+(r+Arg(a)) by A2,Th68;
  consider j being Integer such that
A5: Arg(Rotate(b,r)) = 2*PI*j+(r+Arg(b)) by A3,Th68;
A6: Arg(Rotate(b,r)) = 2*PI*(j-i)+Arg(Rotate(a,r)) by A1,A4,A5;
  0 <= Arg Rotate(a,r) & 0 <= Arg Rotate(b,r) &
  Arg Rotate(a,r) < 2*PI & Arg Rotate(b,r) < 2*PI by COMPTRIG:52;
  hence Arg Rotate(a,r) = Arg Rotate(b,r) by A6,Th3;
end;

theorem Th77:
  r > 0 implies angle(a,b) = angle(a*r,b*r)
proof
  assume that
A1: r > 0;
A2: Arg(a*r) = Arg a by A1,Th40;
  per cases;
  suppose
A3: b <> 0;
    thus angle(a,b) = Arg(Rotate(b,-Arg a)) by A3,Def5
      .= Arg(Rotate(b*r,-Arg(a*r))) by A1,A2,A3,Th40,Th76
      .= angle(a*r,b*r) by Def5,A1,A3;
  end;
  suppose
A5: b = 0;
    thus thesis
    proof
      per cases;
      suppose
A6:     Arg a = 0;
        hence angle(a,b) = Arg(Rotate(b,-Arg a)) by Def5
          .= Arg 0c by A5,Th66
          .= Arg(Rotate(b*r,-Arg(a*r))) by A5,Th66
          .= angle(a*r,b*r) by A2,A6,Def5;
      end;
      suppose
A7:     Arg a <> 0;
        hence angle(a,b) = 2*PI-Arg a by A5,Def5
          .= angle(a*r,b*r) by A2,A5,A7,Def5;
      end;
    end;
  end;
end;

theorem Th78:
  a <> 0 & b <> 0 & Arg a = Arg b implies Arg -a = Arg -b
proof
  assume a <> 0 & b <> 0 & Arg a = Arg b;
  then Arg Rotate(a,PI) = Arg Rotate(b,PI) by Th76;
  then Arg -a = Arg Rotate(b,PI) by Th74;
  hence Arg -a = Arg -b by Th74;
end;

theorem Th79:
  a <> 0 & b <> 0 implies angle(a,b) = angle(Rotate(a,r),Rotate(b,r))
proof
  assume that
A1: a <> 0 and
A2: b <> 0;
  consider i being Integer such that
A3: Arg(Rotate(b, -Arg a)) = 2*PI*i+(-(Arg a) + Arg(b)) by A2,Th68;
A4: Rotate(b,r) <> 0 by A2,Th66;
  then consider j being Integer such that
A5: Arg(Rotate(Rotate(b,r), -Arg Rotate(a,r)))
  = 2*PI*j+(-(Arg Rotate(a, r)) + Arg(Rotate(b,r))) by Th68;
  consider k being Integer such that
A6: Arg Rotate(a,r) = 2*PI*k+(r+Arg(a)) by A1,Th68;
  consider l being Integer such that
A7: Arg Rotate(b,r) = 2*PI*l+(r+Arg(b)) by A2,Th68;
A8: Arg(Rotate(Rotate(b,r), -Arg Rotate(a,r)))
  = 2*PI*(j-k+l-i)+Arg(Rotate(b, -Arg a)) by A3,A5,A6,A7;
A9: 0 <= Arg(Rotate(b, -Arg a)) & Arg(Rotate(b, -Arg a)) < 2*PI &
  0 <= Arg(Rotate(Rotate(b,r), -Arg Rotate(a,r))) &
  Arg(Rotate(Rotate(b,r), -Arg Rotate(a,r))) < 2*PI by COMPTRIG:52;
  thus angle(a,b) = Arg(Rotate(b, -Arg a)) by A2,Def5
    .= Arg(Rotate(Rotate(b,r), -Arg Rotate(a,r))) by A8,A9,Th3
    .= angle(Rotate(a,r),Rotate(b,r)) by A4,Def5;
end;

theorem
  r < 0 & a <> 0 & b <> 0 implies angle(a,b) = angle(a*r,b*r)
proof
  assume that
A1: r < 0 and
A2: a <> 0 and
A3: b <> 0;
A4: Arg(b*r) = Arg -b by A1,Th41;
A5: Arg(a*r) = Arg -a by A1,Th41;
  consider i being Integer such that
A7: Arg(Rotate(-b, -Arg -a)) = 2*PI*i+(-Arg -a + Arg -b) by Th68,A3;
  set br = b*r, ar = a*r;
  consider j being Integer such that
A9: Arg(Rotate(br,-Arg ar)) = 2*PI*j+(-Arg -a + Arg -b) by A4,A5,Th68,A1,A3;
A10: Arg(Rotate(br,-Arg ar)) = 2*PI*(j-i)+Arg(Rotate(-b,-Arg -a)) by A7,A9;
A11: 0 <= Arg(Rotate(br,-Arg ar)) & Arg(Rotate(br,-Arg ar)) < 2*PI &
  0 <= Arg(Rotate(-b,-Arg -a)) & Arg(Rotate(-b,-Arg -a)) < 2*PI
  by COMPTRIG:52;
  thus angle(a,b) = angle(Rotate(a,PI),Rotate(b,PI)) by A2,A3,Th79
    .= angle(-a,Rotate(b,PI)) by Th74
    .= angle(-a,-b) by Th74
    .= Arg(Rotate(-b,-Arg -a)) by A3,Def5
    .= Arg(Rotate(br,-Arg ar)) by A10,A11,Th3
    .= angle(a*r,b*r) by Def5,A1,A3;
end;

theorem
  a <> 0 & b <> 0 implies angle(a,b) = angle(-a,-b)
proof
  assume a <> 0 & b <> 0;
  hence angle(a,b) = angle(Rotate(a,PI),Rotate(b,PI)) by Th79
    .= angle(-a,Rotate(b,PI)) by Th74
    .= angle(-a,-b) by Th74;
end;

theorem
  b <> 0 & angle(a,b) = 0 implies angle(a,-b) = PI
proof
  assume that
A1: b <> 0 and
A2: angle(a,b) = 0;
A4: Arg(Rotate(b, -Arg a)) = 0 by A1,A2,Def5;
A5: Rotate(b, -Arg a) <> 0 by A1,Th66;
A6: Arg(Rotate(-b, -Arg a)) = Arg(-Rotate(b, -Arg a)) by Th72;
  Arg -Rotate(b, -Arg a) = Arg Rotate(b, -Arg a)+PI by A4,A5,Th22,COMPTRIG:21
    .= PI by A4;
  hence angle(a,-b) = PI by A1,A6,Def5;
end;

theorem Th83:
  a <> 0 & b <> 0 implies cos angle(a,b) = Re (a.|.b)/(|.a.|*|.b.|) &
  sin angle(a,b) = - Im (a.|.b)/(|.a.|*|.b.|)
proof
  assume that
A1: a <> 0 and
A2: b <> 0;
A3: |.a.| <> 0 by A1,COMPLEX1:131;
A4: |.b.| <> 0 by A2,COMPLEX1:131;
  set mab = (|.a.|*|.b.|);
  set mabi = (|.a.|*|.b.|)";
A5: |.a.| >= 0 & |.b.| >= 0 by COMPLEX1:132;
  set ra = Rotate(a,-Arg a), rb = Rotate(b,-Arg a), r = -Arg a;
B9: angle(a,b) = angle(ra,rb) by A1,A2,Th79;
A10: Re ra = |.a.| & Im ra = 0 by SIN_COS:34,COMPLEX1:28;
  mabi = mabi+0*<i>;
  then
A11: Re mabi = mabi & Im mabi = 0 by COMPLEX1:28;
  then Re (ra*mabi) = |.a.|*mabi-0*0 &
  Im (ra*mabi) = |.a.|*0+0*mabi by A10,COMPLEX1:24;
  then ra*mabi = |.a.|*mabi+0*<i> by COMPLEX1:29;
  then
A12: angle(a,b) = angle(|.a.|*mabi,rb*mabi) by B9,A3,A4,A5,Th77
         .= Arg(rb*mabi) by A5,Th75;
  set IT = rb*mabi;
A13: Re rb = (Re b)*(cos r)-(Im b)*(sin r) &
  Im rb = (Re b)*(sin r)+(Im b)*(cos r) by Th70;
A14: IT = |.IT.|*cos Arg IT+(|.IT.|*sin Arg IT)*<i> by Th19;
A15: Re IT = Re rb * mabi - (Im rb)*0 by A11,COMPLEX1:24
    .= Re rb * mabi;
A16: Im IT = Re rb * 0 + Im rb * mabi by A11,COMPLEX1:24
    .= Im rb * mabi;
  a = |.a.|*cos Arg a+(|.a.|*sin Arg a)*<i> by Th19;
  then Re a = |.a.|*cos Arg a & Im a = |.a.|*sin Arg a by COMPLEX1:28;
  then
A17: cos Arg a = (Re a)/|.a.| & sin Arg a = (Im a)/|.a.| by A1,COMPLEX1:131
,XCMPLX_1:90;
A18: a.|.b
    = (Re a)*(Re b)+(Im a)*(Im b)+(-((Re a)*(Im b))+(Im a)*(Re b))*<i> by Th42;
  then
A19: Re (a.|.b) = (Re a)*(Re b)+(Im a)*(Im b) by COMPLEX1:28;
A20: Re rb = (Re b)*(cos r)-(Im b)*(-sin Arg a) by A13,SIN_COS:34
    .= (Re b)*((Re a)/|.a.|)+(Im b)*((Im a)/|.a.|) by A17,SIN_COS:34
    .= Re (a.|.b) / |.a.| by A19;
A21: Im rb = (Re b)*(-sin Arg a)+(Im b)*(cos r) by A13,SIN_COS:34
    .= (-(Re b))*((Im a)/|.a.|)+(Im b)*((Re a)/|.a.|) by A17,SIN_COS:34
    .= -(-((-(Re b))*(Im a)+(Im b)*(Re a)))/|.a.|
    .= - Im (a.|.b)/|.a.| by A18,COMPLEX1:28;
A22: mabi^2 >= 0 by XREAL_1:65;
B23: (Re rb)^2 >= 0 & (Im rb)^2 >= 0 & 0 = 0+0 by XREAL_1:65;
A24: |.IT.| = sqrt((Re rb * mabi)^2+(Im rb * mabi)^2) by A15,A16,
COMPLEX1:def 16
    .= sqrt(mabi^2*((Re rb)^2+(Im rb)^2))
    .= sqrt(mabi^2)*sqrt((Re rb)^2+(Im rb)^2) by A22,B23,SQUARE_1:97
    .= mabi*sqrt((Re rb)^2+(Im rb)^2) by A5,SQUARE_1:89
    .= mabi*|.rb.| by COMPLEX1:def 16
    .= mabi*|.b.| by Th67;
  |.IT.|*cos Arg IT = Re rb * mabi by A14,A15,COMPLEX1:28;
  hence cos angle(a,b) = Re rb * mabi/|.IT.| by A4,A3,A12,A24,XCMPLX_1:90
    .= Re rb *(mabi/|.IT.|)
    .= Re (a.|.b) / |.a.|*(mabi/mabi/|.b.|) by A20,A24,XCMPLX_1:79
    .= Re (a.|.b) / |.a.|*(1/|.b.|) by A3,A4,XCMPLX_1:60
    .= Re (a.|.b)*1 / (|.a.|*|.b.|) by XCMPLX_1:77
    .= Re (a.|.b)/mab;
  |.IT.|*sin Arg IT = Im rb * mabi by A14,A16,COMPLEX1:28;
  hence sin angle(a,b) = Im rb * mabi/|.IT.| by A4,A3,A12,A24,XCMPLX_1:90
    .= Im rb * (mabi/|.IT.|)
    .= (- Im (a.|.b) / |.a.|)*(mabi/mabi/|.b.|) by A21,A24,XCMPLX_1:79
    .= ((- Im (a.|.b)) / |.a.|)*(1/|.b.|) by A3,A4,XCMPLX_1:60
    .= (- Im (a.|.b))*1/(|.a.|*|.b.|) by XCMPLX_1:77
    .= - Im (a.|.b)/mab;
end;

definition
  let x, y, z be complex number;
  func angle(x,y,z) -> real number equals
  :Def6:
  Arg(z-y)-Arg(x-y) if Arg(z-y)-Arg(x-y) >= 0
  otherwise 2*PI+(Arg(z-y)-Arg(x-y));
  correctness;
end;

theorem Th84:
  0 <= angle(x,y,z) & angle(x,y,z) < 2*PI
proof now per cases;
    case
A1:   Arg(z-y)-Arg(x-y)>=0;
      then
A2:   angle(x,y,z)=Arg(z-y)-Arg(x-y) by Def6;
A3:   Arg(z-y)<2*PI by COMPTRIG:52;
      Arg(x-y)>=0 by COMPTRIG:52;
      then Arg(z-y)+0<=Arg(z-y)+Arg(x-y) by XREAL_1:9;
      then Arg(z-y)-Arg(x-y)<=Arg(z-y) by XREAL_1:22;
      hence 0<=angle(x,y,z) & angle(x,y,z)<2*PI by A1,A2,A3,XXREAL_0:2;
    end;
    case
A4:   Arg(z-y)-Arg(x-y)<0;
      then
A5:   Arg(z-y)-Arg(x-y) + 2*PI < 0 + 2*PI by XREAL_1:10;
A6:   Arg(x-y)<2*PI by COMPTRIG:52;
      Arg(z-y)>=0 by COMPTRIG:52;
      then Arg(x-y)+0<=Arg(x-y)+Arg(z-y) by XREAL_1:9;
      then Arg(x-y)-Arg(z-y)<=Arg(x-y) by XREAL_1:22;
      then (Arg(x-y)-Arg(z-y))<2*PI by A6,XXREAL_0:2;
      then 2*PI-(Arg(x-y)-Arg(z-y))>0 by XREAL_1:52;
      hence 0<=angle(x,y,z) & angle(x,y,z)<2*PI by A4,A5,Def6;
    end;
  end;
  hence 0<=angle(x,y,z) & angle(x,y,z)<2*PI;
end;

theorem Th85:
  angle(x,y,z)=angle(x-y,0,z-y)
proof now per cases;
    case
A1:   Arg((z-y)-0c)-Arg((x-y)-0c)>=0;
      then angle(x-y,0c,z-y)=Arg(z-y)-Arg(x-y) by Def6;
      hence angle(x-y,0c,z-y)=angle(x,y,z) by A1,Def6;
    end;
    case
A2:   Arg((z-y)-0c)-Arg((x-y)-0c)<0;
      then angle(x-y,0c,z-y)=2*PI+(Arg(z-y)-Arg(x-y)) by Def6;
      hence angle(x-y,0c,z-y)=angle(x,y,z) by A2,Def6;
    end;
  end;
  hence angle(x,y,z)=angle(x-y,0,z-y);
end;

theorem Th86:
  angle(a,b,c) = angle(a+d,b+d,c+d)
proof thus angle(a,b,c) = angle(a-b,0c,c-b) by Th85
    .= angle((a+d)-(b+d),0c,(c+d)-(b+d))
    .= angle(a+d,b+d,c+d) by Th85;
end;

theorem Th87:
  angle(a,b) = angle(a,0,b)
proof
  set ab2 = angle(a,b);
A1: 0 <= Arg(Rotate(b,-Arg a)) & Arg(Rotate(b,-Arg a)) < 2*PI &
  0 <= Arg b & Arg b < 2*PI by COMPTRIG:52;
  per cases;
  suppose
A2: b <> 0;
    then
A3: ab2 = Arg(Rotate(b, -Arg a)) by Def5;
    thus thesis
    proof
      per cases;
      suppose Arg(b-0c)-Arg(a-0c) >= 0;
        then
A4:     angle(a,0c,b) = -Arg(a)+Arg(b) by Def6;
        consider i being Integer such that
A5:     Arg(Rotate(b,-Arg a)) = 2*PI*i+(-Arg a + Arg(b)) by A2,Th68;
        0 <= angle(a,0c,b) & angle(a,0c,b) < 2*PI by Th84;
        hence ab2 = angle(a,0,b) by A1,A3,A4,A5,Th3;
      end;
      suppose Arg(b-0c)-Arg(a-0c) < 0;
        then
A6:     angle(a,0c,b) = 2*PI+(Arg(b)+-Arg(a)) by Def6;
        consider i being Integer such that
A7:     Arg(Rotate(b,-Arg a)) = 2*PI*i+(-Arg a + Arg(b)) by A2,Th68;
A8:     2*PI*i+(-Arg a + Arg(b)) = 2*PI*(i-1)+(2*PI+(-Arg a + Arg(b)));
        0 <= 2*PI+(-Arg a + Arg(b)) & 2*PI+(-Arg a + Arg(b)) < 2*PI by A6,Th84;
        hence thesis by A1,A3,A6,A7,A8,Th3;
      end;
    end;
  end;
  suppose
A9: b = 0;
    thus thesis
    proof
      per cases;
      suppose
A10:    Arg a = 0;
        then
A11:    ab2=Arg(Rotate(b, -Arg a)) by Def5
          .= 0 by A9,Lm2,Th66;
        Arg(b-0c)-Arg(a-0c) = 0 by A9,A10,COMPTRIG:53;
        hence thesis by A11,Def6;
      end;
      suppose
A12:    Arg a <> 0;
A13:    Arg(b-0c)-Arg(a-0c) = - Arg a by A9,Lm2;
        0 < --Arg a by A12,COMPTRIG:52;
        then angle(a,0c,b) = 2*PI-Arg a by A13,Def6;
        hence thesis by A9,A12,Def5;
      end;
    end;
  end;
end;

theorem Th88:
  angle(x,y,z) = 0 implies Arg(x-y) = Arg(z-y) & angle(z,y,x)=0
proof
  assume
A1: angle(x,y,z) =0;
  now per cases;
    case Arg(z-y)-Arg(x-y)>=0;
      then Arg(z-y)-Arg(x-y)=0 by A1,Def6;
      hence Arg(x-y)=Arg(z-y) & angle(z,y,x)=0 by Def6;
    end;
    case
A2:   Arg(z-y)-Arg(x-y)<0;
      then
A3:   angle(x,y,z)=2*PI+(Arg(z-y)-Arg(x-y)) by Def6;
      -(Arg(z-y)-Arg(x-y))>0 by A2;
      then angle(z,y,x)=Arg(x-y)-Arg(z-y) by Def6;
      hence contradiction by A1,A3,Th84;
    end;
  end;
  hence Arg(x-y)=Arg(z-y)& angle(z,y,x)=0;
end;

theorem Th89:
  a <> 0 & b <> 0 implies (Re (a.|.b) = 0
  iff angle(a,0,b) = PI/2 or angle(a,0,b) = 3/2*PI)
proof
  assume that
A1: a <> 0 and
A2: b <> 0;
A3: cos angle(a,b) = Re (a.|.b)/(|.a.|*|.b.|) &
  sin angle(a,b) = - Im (a.|.b)/(|.a.|*|.b.|) by A1,A2,Th83;
A4: angle(a,b) = angle(a,0c,b) by Th87;
A5: |.a.| <> 0 & |.b.| <> 0 by A1,A2,COMPLEX1:131;
A6: 0 <= angle(a,0c,b) & angle(a,0c,b) < 2*PI by Th84;
A7: PI/2 < 2*PI by COMPTRIG:21,XXREAL_0:2;
A8: -Im (a.|.b)/(|.a.|*|.b.|) = (-Im (a.|.b))/(|.a.|*|.b.|);
  hereby
    assume
A9: Re (a.|.b) = 0;
    then Im (a.|.b)=|.a.|*|.b.| or Im (a.|.b)= - (|.a.|*|.b.|) by Th64;
    then sin angle(a,b) = 1 or sin angle(a,b) = -1 by A3,A5,A8,XCMPLX_1:60;
    hence angle(a,0,b) = PI/2 or angle(a,0,b) = 3/2*PI
    by A3,A4,A6,A7,A9,Th12,COMPTRIG:21,SIN_COS:82;
  end;
  assume angle(a,0,b) = PI/2 or angle(a,0,b) = 3/2*PI;
  hence Re (a.|.b) = 0 by A3,Th87,A5,SIN_COS:82;
end;

theorem
  a <> 0 & b <> 0 implies (Im(a.|.b) = |.a.|*|.b.| or Im(a.|.b) = -|.a.|*|.b
  .| iff angle(a,0,b) = PI/2 or angle(a,0,b) = 3/2*PI)
proof
  assume
A1: a<>0 & b<>0;
  hereby
    assume Im(a.|.b)=|.a.|*|.b.| or Im(a.|.b)=-(|.a.|*|.b.|);
    then Re (a.|.b)=0 by Th64;
    hence angle(a,0,b)=PI/2 or angle(a,0,b)=3/2*PI by A1,Th89;
  end;
  hereby
    assume angle(a,0,b)=PI/2 or angle(a,0,b)=3/2*PI;
    then Re (a.|.b)=0 by A1,Th89;
    hence Im(a.|.b)=|.a.|*|.b.| or Im(a.|.b)=-(|.a.|*|.b.|) by Th64;
  end;
end;

Lm4: for a, b, c being Element of COMPLEX st a <> b & c <> b
holds Re ((a-b).|.(c-b)) = 0 iff angle(a,b,c) = PI/2 or angle(a,b,c) = 3/2*PI
proof
  let x, y,z be Element of COMPLEX;
  assume
A1: x<>y & z<>y;
  then
A2: x-y <> 0c;
A3: z-y <> 0c by A1;
  angle(x,y,z)=angle(x-y,0c,z-y) by Th85;
  hence (Re ((x-y).|.(z-y)) = 0 iff angle(x,y,z)=PI/2 or angle(x,y,z)=3/2*PI)
  by A2,A3,Th89;
end;

theorem
  x <> y & z <> y & (angle(x,y,z) = PI/2 or angle(x,y,z) = 3/2*PI)
  implies |.x-y.|^2+|.z-y.|^2 = |.x-z.|^2
proof
  assume
A1: x<>y & z<>y & (angle(x,y,z)=PI/2 or angle(x,y,z)=3/2*PI);
A2: |.x-y.|^2+|.z-y.|^2 =Re ((x-y).|.(x-y)) + |.z-y.|^2 by Th44
    .=Re ((x-y).|.(x-y)) + Re ((z-y).|.(z-y)) by Th44
    .=Re ((x-y).|.(x-y)+(z-y).|.(z-y)) by COMPLEX1:19;
  set x3=x-z;
  x3=(x-y)-(z-y);
  then (x-z).|.(x-z)
  =(x-y).|.(x-y)-(x-y).|.(z-y)-(z-y).|.(x-y) +(z-y).|.(z-y) by Th63;
  then Re((x-z).|.(x-z)) =Re((x-y).|.(x-y)+(z-y).|.(z-y)-((x-y).|.(z-y)+(z-y)
  .|.(x-y)))

.=Re((x-y).|.(x-y)+(z-y).|.(z-y))-Re((x-y).|.(z-y)+(z-y).|.(x-y))by COMPLEX1:48
    .=Re((x-y).|.(x-y)+(z-y).|.(z-y))-(Re((x-y).|.(z-y))+Re((z-y).|.(x-y)))
  by COMPLEX1:19
    .=Re((x-y).|.(x-y)+(z-y).|.(z-y))-(Re((x-y).|.(z-y))+Re(((x-y).|.(z-y))*'))
  by Th47
    .=Re((x-y).|.(x-y)+(z-y).|.(z-y))-(Re((x-y).|.(z-y))+Re((x-y).|.(z-y)))
  by COMPLEX1:112
    .=Re((x-y).|.(x-y)+(z-y).|.(z-y))-(0+Re((x-y).|.(z-y))) by A1,Lm4
    .=Re((x-y).|.(x-y)+(z-y).|.(z-y)) -0 by A1,Lm4
    .=Re((x-y).|.(x-y)+(z-y).|.(z- y));
  hence |.x-y.|^2+|.z-y.|^2=|.x-z.|^2 by A2,Th44;
end;

theorem Th92:
  a <> b & b <> c implies
  angle(a,b,c) = angle(Rotate(a,r), Rotate(b,r), Rotate(c,r))
proof
  set cb = c-b, ab = a-b;
  set rc = Rotate(c,r), rb = Rotate(b,r), ra = Rotate(a,r);
  set rcb = Rotate(c,r)-Rotate(b,r), rab = Rotate(a,r)-Rotate(b,r);
A1: rab = Rotate(ab,r) by Th73;
  assume a <> b & b <> c;
  then
A2: ab <> 0 & cb <> 0;
  then consider abi being Integer such that
A3: Arg(Rotate(ab,r)) = 2*PI*abi+(r+Arg(ab)) by Th68;
  consider cbi being Integer such that
A4: Arg(Rotate(cb,r)) = 2*PI*cbi+(r+Arg(cb)) by A2,Th68;
A5: 0 <= angle(a,b,c) & angle(a,b,c) < 2*PI by Th84;
A6: 0 <= angle(Rotate(a,r),Rotate(b,r), Rotate(c,r)) &
  angle(Rotate(a,r),Rotate(b,r), Rotate(c,r))< 2*PI by Th84;
A7: Arg(rcb)-Arg(rab)
  = 2*PI*cbi+r+Arg(cb) - (2*PI*abi+r+Arg(ab)) by A1,A3,A4,Th73
    .= Arg(cb)-Arg(ab)+2*PI*(cbi-abi);
  per cases;
  suppose Arg(c-b)-Arg(a-b) >= 0;
    then
A8: angle(a,b,c) = Arg(c-b)-Arg(a-b) by Def6;
    thus angle(a,b,c) = angle(Rotate(a,r),Rotate(b,r), Rotate(c,r))
    proof
      per cases;
      suppose Arg(rcb)-Arg(rab) >= 0;
        then angle(ra,rb,rc) = Arg(rcb)-Arg(rab) by Def6;
        hence thesis by A5,A6,A7,A8,Th3;
      end;
      suppose Arg(rcb)-Arg(rab) < 0;
        then angle(ra,rb,rc) = Arg(cb)-Arg(ab)+2*PI*(cbi-abi)+2*PI by A7,Def6
          .= Arg(cb)-Arg(ab)+2*PI*(cbi-abi+1);
        hence thesis by A5,A6,A8,Th3;
      end;
    end;
  end;
  suppose Arg(cb)-Arg(ab) < 0;
    then
A9: angle(a,b,c) = 2*PI+(Arg(cb)-Arg(ab)) by Def6;
    thus thesis
    proof
      per cases;
      suppose Arg(rcb)-Arg(rab) >= 0;
        then angle(ra,rb,rc)
        = 2*PI+(Arg(cb)-Arg(ab))+2*PI*(cbi-abi+-1) by A7,Def6;
        hence thesis by A5,A6,A9,Th3;
      end;
      suppose Arg(rcb)-Arg(rab) < 0;
        then angle(ra,rb,rc) = Arg(cb)-Arg(ab)+2*PI*(cbi-abi)+2*PI by A7,Def6
          .= 2*PI+(Arg(cb)-Arg(ab))+2*PI*(cbi-abi);
        hence thesis by A5,A6,A9,Th3;
      end;
    end;
  end;
end;

theorem Th93:
  angle(a,b,a) = 0
proof Arg(a-b)-Arg(a-b) = 0;
  hence angle(a,b,a) = 0 by Def6;
end;

Lm5: angle(x,y,z) <> 0 implies angle(z,y,x)=2*PI-angle(x,y,z)
proof
  assume
A1: angle(x,y,z) <>0;
  now per cases;
    case
A2:   Arg(x-y)-Arg(z-y)>0;
      then --(Arg(x-y)-Arg(z-y))>0;
      then angle(x,y,z)=2*PI+(Arg(z-y)-Arg(x-y)) by Def6
        .=2*PI -(Arg(x-y)-Arg(z-y));
      hence angle(z,y,x)=2*PI-angle(x,y,z) by A2,Def6;
    end;
    case
A3:   Arg(x-y)-Arg(z-y)<=0;
      Arg(x-y)-Arg(z-y)<>0 by A1,Def6;
      then
A4:   angle(z,y,x) =2*PI -(Arg(z-y)-Arg(x-y)) by A3,Def6;
      --(Arg(x-y)-Arg(z-y))<=0 by A3;
      then angle(x,y,z)+angle(z,y,x)
      =(2*PI -(Arg(z-y)-Arg(x-y)))+(Arg(z-y)-Arg(x-y)) by A4,Def6
        .=2*PI;
      hence angle(z,y,x)=2*PI-angle(x,y,z);
    end;
  end;
  hence angle(z,y,x)=2*PI-angle(x,y,z);
end;

theorem Th94: :: COMPLEX2:47, 48
  angle(a,b,c) <> 0 iff angle(a,b,c)+angle(c,b,a) = 2*PI
proof
  hereby
    assume angle(a,b,c) <> 0;
    then angle(c,b,a) = 2*PI-angle(a,b,c) by Lm5;
    hence angle(a,b,c)+angle(c,b,a) = 2*PI;
  end;
  assume angle(a,b,c)+angle(c,b,a) = 2*PI & angle(a,b,c) = 0;
  hence contradiction by Th84;
end;

theorem
  angle(a,b,c) <> 0 implies angle(c,b,a) <> 0
proof
  assume angle(a,b,c) <> 0;
  then
A1: angle(a,b,c)+angle(c,b,a) = 2*PI by Th94;
  assume not thesis;
  hence contradiction by A1,Th84;
end;

theorem
  angle(a,b,c) = PI implies angle(c,b,a) = PI
proof
  assume
A1: angle(a,b,c) = PI;
  then angle(a,b,c)+angle(c,b,a) = 0+2*PI by Th94,COMPTRIG:21;
  hence angle(c,b,a) = PI by A1;
end;

theorem Th97:
  a <> b & a <> c & b <> c
  implies angle(a,b,c) <> 0 or angle(b,c,a) <> 0 or angle(c,a,b) <> 0
proof
  assume that
A1: a <> b and
A2: a <> c and
A3: b <> c;
  assume
A4: not thesis;
  then
A5: Arg(a-c) = Arg (b-c) by Th88;
A6: Arg(b-a) = Arg (c-a) by A4,Th88;
A7: a-b <> 0 & b-a <> 0 & a-c <> 0 & c-a <> 0 & b-c <> 0 & c-b <> 0
  by A1,A2,A3;
A8: -(a-b) = b-a & -(b-a) = a-b;
  -(a-c) = c-a & -(c-a) = a-c;
  then
A9: Arg(a-b) = Arg (a-c) by A6,A7,A8,Th78;
  Arg(c-b) < PI iff Arg (-(c-b)) >= PI by A7,Th25;
  hence thesis by A4,A5,A9,Th88;
end;

Lm6: Im a = 0 & Re a > 0 & 0 < Arg(b) & Arg(b) < PI
implies angle(a,0c,b)+angle(0c,b,a)+angle(b,a,0c) = PI &
0 < angle(0c,b,a) & 0 < angle(b,a,0c)
proof
  assume that
A1: Im a = 0 and
A2: Re a > 0 and
A3: 0 < Arg(b) and
A4: Arg(b) < PI;
  a = Re a + 0*<i> by A1,COMPLEX1:29;
  then
A5: Arg(a) = 0 by A2,COMPTRIG:53;
  then
A6: Arg(-a) = Arg(a)+PI by A2,Th22,COMPLEX1:12,COMPTRIG:21
    .= PI by A5;
  Arg b in ].0,PI.[ by A3,A4,XXREAL_1:4;
  then
A7: Im b > 0 by Th27;
A8: Im (a-b) = Im a - Im b by COMPLEX1:48
    .= - Im b by A1;
A10: Arg(a-b) > PI by Th37,A7,A8;
  Arg(b-0c)-Arg(a-0c) >= 0 by A3,A5;
  then
A11: angle(a,0c,b) = Arg(b)-Arg(a) by Def6;
  now
    let a, b be Element of COMPLEX such that
A12: Im a = 0 and
A13: Re a > 0 and
A14: 0 < Arg(b) and
A15: Arg(b) < PI;
    Arg b in ].0,PI.[ by A14,A15,XXREAL_1:4;
    then
A16: Im b > 0 by Th27;
A17: Im (b-a) = Im b - Im a by COMPLEX1:48
      .= Im b by A12;
A18: Re (b-a) = Re b - Re a by COMPLEX1:48;
A19: Re b + -Re a < Re b + 0 by XREAL_1:10,A13;
    set ba = b-a;
    set Rb = Re b, Rba = Re ba, Ib = Im b, Iba = Im ba;
    set B = Arg b, BA = Arg ba, mb = |.b.|, mba = |.b-a.|;
A20: b = mb*cos B + mb*sin B *<i> by Th19;
    b = Re b+(Im b)*<i> by COMPLEX1:29;
    then
A21: Re b = mb*cos B & Im b = mb*sin B by A20,COMPLEX1:163;
A22: b-a = mba*cos BA + mba*sin BA *<i> by Th19;
    b-a = Re ba+(Im ba)*<i> by COMPLEX1:29;
    then
A23: Re ba = mba*cos BA & Im ba = mba*sin BA by A22,COMPLEX1:163;
A24: mb = sqrt(Rb^2+Ib^2) by COMPLEX1:def 16;
A25: mba = sqrt(Rba^2+Ib^2) by A17,COMPLEX1:def 16;
A26: mb >= 0 & mb <> 0 by A16,COMPLEX1:12,133;
A27: mba >= 0 & mba <> 0 by A16,A17,COMPLEX1:12,133;
B28: Rba^2 >= 0 & Ib^2 >= 0 by XREAL_1:65;
B29: Rb^2 >= 0 & Ib^2 >= 0 by XREAL_1:65;
A30: sin BA > 0 by A16,A17,COMPTRIG:63;
    then
A31: mba/mb = (sin B)/sin BA by A17,A21,A23,A26,XCMPLX_1:95;
    per cases;
    suppose
A32:  0 < Re ba;
      then
A33:  B in ].0,PI/2.[ by A16,A18,A19,COMPTRIG:59;
A34:  BA in ].0,PI/2.[ by A16,A17,A32,COMPTRIG:59;
      Rb^2 > Rba^2 by A18,A19,A32,SQUARE_1:78;
      then Rb^2 + Ib^2 > Rba^2 + Ib^2 by XREAL_1:10;
      then mb > mba by A24,A25,B28,SQUARE_1:95;
      then mba/mb < 1 by A27,XREAL_1:191;
      then ((sin B)/sin BA)*sin BA < 1*sin BA by A30,A31,XREAL_1:70;
      then sin B < 1*sin BA by A30,XCMPLX_1:88;
      then BA > B by A33,A34,Th7;
      then BA+-B > B+-B by XREAL_1:10;
      hence Arg ba - Arg b > 0;
    end;
    suppose
A35:  0 = Rba;
      then ba = 0+Iba*<i> by COMPLEX1:29;
      then
A36:  BA = PI/2 by A16,A17,COMPTRIG:55;
      B in ].0,PI/2.[ by A16,A18,A13,A35,COMPTRIG:59;
      then B < BA by A36,XXREAL_1:4;
      then B+-B < BA+-B by XREAL_1:10;
      hence Arg ba - Arg(b) > 0;
    end;
    suppose
A37:  0 > Re ba;
      thus Arg ba - Arg(b) > 0
      proof
        per cases;
        suppose 0 < Re b;
          then B in ].0,PI/2.[ by A16,COMPTRIG:59;
          then
A38:      B < PI/2 by XXREAL_1:4;
          BA in ].PI/2,PI.[ by A16,A17,A37,COMPTRIG:60;
          then BA > PI/2 by XXREAL_1:4;
          then BA > B by A38,XXREAL_0:2;
          then B+-B < BA+-B by XREAL_1:10;
          hence Arg ba - Arg(b) > 0;
        end;
        suppose 0 = Re b;
          then b = 0+Ib*<i> by COMPLEX1:29;
          then
A39:      B = PI/2 by A16,COMPTRIG:55;
          BA in ].PI/2,PI.[ by A16,A17,A37,COMPTRIG:60;
          then B < BA by A39,XXREAL_1:4;
          then B+-B < BA+-B by XREAL_1:10;
          hence Arg ba - Arg(b) > 0;
        end;
        suppose
A40:      0 > Re b;
          then
A41:      B in ].PI/2,PI.[ by A16,COMPTRIG:60;
A42:      BA in ].PI/2,PI.[ by A16,A17,A37,COMPTRIG:60;
          Rb^2 < Rba^2 by A18,A19,A40,SQUARE_1:114;
          then Rb^2 + Ib^2 < Rba^2 + Ib^2 by XREAL_1:10;
          then mb < mba by A24,A25,B29,SQUARE_1:95;
          then mba/mb > 1 by A26,XREAL_1:189;
          then ((sin B)/sin BA)*sin BA > 1*sin BA by A30,A31,XREAL_1:70;
          then sin B > 1*sin BA by A30,XCMPLX_1:88;
          then BA > B by A41,A42,Th8;
          then BA+-B > B+-B by XREAL_1:10;
          hence Arg ba - Arg b > 0;
        end;
      end;
    end;
  end;
  then Arg(b-a)-Arg(b) > 0 by A1,A2,A3,A4;
  then Arg(-(a-b))-Arg(b) > 0;
  then Arg(a-b)-PI-Arg(b) > 0 by A7,A8,A10,Th22,COMPLEX1:12;
  then
B43: Arg(a-b)-(Arg(b)+PI) > 0;
  then
A43: Arg(a-b)-Arg(-b) > 0 by A4,A7,Th22,COMPLEX1:12;
A44: Arg(a-b)-Arg(0c-b) > 0 by A4,A7,Th22,COMPLEX1:12,B43;
A45: angle(0c,b,a) = Arg(a-b)-Arg(0c-b) by A43,Def6;
  Arg(b-a) <= PI by A7,A8,A10,Th26,COMPLEX1:12;
  then -Arg(b-a) >= -PI by XREAL_1:26;
  then Arg(-a)+-Arg(b-a) >= 0-PI+PI by A6,XREAL_1:9;
  then
A46: angle(b,a,0c) = Arg(0c-a)-Arg(b-a) by Def6;
A47: Arg(b)-Arg(-b) = Arg(b)-(Arg(b)+PI) by A4,A7,Th22,COMPLEX1:12
    .= -PI;
A48: Arg(a)-Arg(-a) = Arg(a)-(Arg(a)+PI) by A2,A5,Th22,COMPLEX1:12,COMPTRIG:21
    .= -PI;
A49: Arg(a-b)-Arg(b-a) = Arg(a-b)-Arg(-(a-b))
    .= Arg(a-b)-(Arg(a-b)-PI) by A7,A8,A10,Th22,COMPLEX1:12
    .= PI;
  thus angle(a,0c,b)+angle(0c,b,a)+angle(b,a,0c)
  = Arg(b)-Arg(-b)-Arg(a)+Arg(a-b)+(Arg(-a)-Arg(b-a)) by A11,A45,A46
    .= PI by A47,A48,A49;
  thus 0 < angle(0c,b,a) by A44,Def6;
A50: Arg(-a)-(Arg(a-b)-PI) = Arg(a)+(2*PI-Arg(a-b))by A48;
  2*PI > 0+Arg(a-b) by COMPTRIG:52;
  then 2*PI-Arg(a-b) > 0 & Arg(a) >= 0 by COMPTRIG:52,XREAL_1:22;
  hence 0 < angle(b,a,0c) by A46,A49,A50;
end;

theorem Th98:
  a <> b & b <> c & 0 < angle(a,b,c) & angle(a,b,c) < PI
  implies angle(a,b,c)+angle(b,c,a)+angle(c,a,b) = PI &
  0 < angle(b,c,a) & 0 < angle(c,a,b)
proof
  assume that
A1: a <> b and
A2: b <> c and
A3: 0 < angle(a,b,c) and
A4: angle(a,b,c) < PI;
A5: angle(a+-b,0c,c+-b) = angle(a+-b,b+-b,c+-b)
    .= angle(a,b,c) by Th86;
  set r = -Arg (a+-b);
  set A = Rotate(a+-b,r), B = Rotate(c+-b, r);
A6: Rotate(0c,r) = 0c by Th66;
A7: a-b <> 0 by A1;
A8: c+-b <> a+-b by A3,Th93;
A9: c-b <> 0 by A2;
  then
A10: angle(a+-b,0c,c+-b) = angle(A,0c,B) by A6,A7,Th92;
A12: Im A = 0 by COMPLEX1:28,SIN_COS:34;
  a+-b <> 0c by A1;
  then |.a+-b.| > 0 by COMPLEX1:133;
  then
A13: Re A > 0 by SIN_COS:34,COMPLEX1:28;
  then
A14: Arg A = 0c by A12,Th34;
  then Arg(B-0c)-Arg(A-0c)>= 0 by COMPTRIG:52;
  then
A15: angle(a,b,c) = Arg B by A5,A10,A14,Def6;
A16: angle(b,c,a) = angle(b+-b,c+-b,a+-b) by Th86
    .=angle(0c,B,A) by A6,A8,A9,Th92;
A17: angle(c,a,b) = angle(c+-b,a+-b,b+-b) by Th86
    .= angle(B,A,0c) by A6,A7,A8,Th92;
  hence angle(a,b,c)+angle(b,c,a)+angle(c,a,b) = PI by A3,A4,A5,A10,A12,A13,A15
,A16,Lm6;
  thus 0 < angle(b,c,a) by A3,A4,A12,A13,A15,A16,Lm6;
  thus 0 < angle(c,a,b) by A3,A4,A12,A13,A15,A17,Lm6;
end;

theorem Th99:
  a <> b & b <> c & angle(a,b,c) > PI
  implies angle(a,b,c)+angle(b,c,a)+angle(c,a,b) = 5*PI &
  angle(b,c,a) > PI & angle(c,a,b) > PI
proof
  assume that
A1: a <> b and
A2: b <> c and
A3: angle(a,b,c) > PI;
A4: angle(a,b,c) + angle(c,b,a)= 2*PI+0 by A3,Th94,COMPTRIG:21;
A5: 0 <= angle(c,b,a) by Th84;
A6: angle(c,b,a) <> 0 by A4,Th84;
A7: angle(c,b,a) < PI
  proof
    assume not thesis;
    then angle(a,b,c) + angle(c,b,a) > PI+PI by A3,XREAL_1:10;
    hence contradiction by A3,Th94,COMPTRIG:21;
  end;
  then angle(a,c,b) > 0 by A1,A2,A5,A6,Th98;
  then
A8: angle(b,c,a) + angle(a,c,b)= 2*PI+0 by Th94;
  angle(b,a,c) > 0 by A1,A2,A5,A6,A7,Th98;
  then angle(c,a,b)+angle(b,a,c)=2*PI+0 by Th94;
  then angle(a,b,c)+angle(b,c,a)+angle(c,a,b)
  = 2*PI+2*PI+2*PI - (angle(c,b,a)+angle(b,a,c)+angle(a,c,b)) by A4,A8;
  hence
A9: angle(a,b,c)+angle(b,c,a)+angle(c,a,b)
  = 2*PI+2*PI+PI+PI-PI by A1,A2,A5,A6,A7,Th98
    .= 5*PI;
A10: 2*PI+PI+2*PI = 5*PI;
A11: 2*PI+2*PI+PI= 5*PI;
A12: angle(a,b,c) < 2*PI by Th84;
  hereby
    assume
A13: angle(b,c,a) <= PI;
A14: angle(c,a,b) < 2*PI by Th84;
    angle(a,b,c)+angle(b,c,a) < 2*PI+PI by A12,A13,XREAL_1:10;
    hence contradiction by A9,A10,A14,XREAL_1:10;
  end;
  assume
A15: angle(c,a,b) <= PI;
  angle(b,c,a) < 2*PI by Th84;
  then angle(a,b,c)+angle(b,c,a) < 2*PI+2*PI by A12,XREAL_1:10;
  hence contradiction by A9,A11,A15,XREAL_1:10;
end;

Lm7: Im a = 0 & Re a > 0 & Arg(b) = PI
implies angle(a,0,b)+angle(0,b,a)+angle(b,a,0) = PI &
0 = angle(0,b,a) & 0 = angle(b,a,0)
proof
  assume that
A1: Im a = 0 and
A2: Re a > 0 and
A3: Arg(b) = PI;
  a = Re a + 0*<i> by A1,COMPLEX1:29;
  then
A4: Arg(a) = 0 by A2,COMPTRIG:53;
  then
A5: Arg(-a) = Arg(a)+PI by A2,Th22,COMPLEX1:12,COMPTRIG:21
    .= PI by A4;
A6: Re b < 0 & Im b = 0 by A3,Th35;
A7: Im (a-b) = Im a - Im b by COMPLEX1:48
    .= - Im b by A1;
A8: Re b +0 < Re a by A2,A3,Th35;
  then Re a - Re b > 0 by XREAL_1:22;
  then
A9: Re (a-b) > 0 by COMPLEX1:48;
  then
A10: Arg(a-b) = 0 by A6,A7,Th34;
  --(Re a - Re b) > 0 by A8,XREAL_1:22;
  then Re b - Re a < 0;
  then
A11: Re (b-a) < 0 by COMPLEX1:48;
A12: Im (b-a) = Im b - Im a by COMPLEX1:48
    .= 0 by A1,A3,Th35;
  then
A13: Arg(b-a) = PI by A11,Th35;
A14: Arg(b-0c)-Arg(a-0c) > 0 by A3,A4,COMPTRIG:21;
  then
A15: angle(a,0c,b) = Arg(b)-Arg(a) by Def6;
A16: Arg -b = Arg b -PI by A3,Lm2,Th22,COMPTRIG:21;
A17: Arg(a-b)+PI-Arg(b) = 0 by A3,A6,A7,A9,Th34;
A18: Arg(a-b)-Arg(0c-b) = 0 by A3,A6,A7,A9,A16,Th34;
A19: angle(0c,b,a) = Arg(a-b)-Arg(0c-b) by A16,A17,Def6;
A20: angle(0c,b,a) = Arg(a-b)-Arg(-b) by A18,Def6;
  -Arg(b-a) = -PI by A11,A12,Th35;
  then
A21: angle(b,a,0c) = Arg(0c-a)-Arg(b-a) by A5,Def6;
A22: Arg -b = Arg b -PI by A3,Lm2,Th22,COMPTRIG:21;
A23: Arg(a)-Arg(-a) = Arg(a)-(Arg(a)+PI) by A2,A4,Th22,COMPLEX1:12,COMPTRIG:21
    .=-PI; thus
A24: angle(a,0,b)+angle(0,b,a)+angle(b,a,0)
  = Arg(b)-Arg(a)+(Arg(a-b)-Arg(-b))+(Arg(0-a)-Arg(b-a)) by A14,A20,A21,Def6
    .= Arg(b)-Arg(-b)-Arg(a)+Arg(a-b)+(Arg(-a)-Arg(b-a))
    .= PI by A10,A13,A22,A23;
  thus 0 = angle(0,b,a) by A18,Def6;
  thus 0 = angle(b,a,0) by A3,A4,A15,A18,A19,A24,XCMPLX_1:3;
end;

theorem Th100:

a <> b & b <> c & angle(a,b,c) = PI implies angle(b,c,a) = 0 & angle(c,a,b) = 0
proof
  assume that
A1: a <> b and
A2: b <> c and
A3: angle(a,b,c) = PI;
A4: angle(a+-b,0c,c+-b) = angle(a+-b,b+-b,c+-b)
    .= angle(a,b,c) by Th86;
  set r = -Arg (a+-b);
A5: Rotate(0c,r) = 0c by Th66;
  set A = Rotate(a+-b,r), B = Rotate(c+-b, r);
A6: a-b <> 0 by A1;
A7: c+-b <> a+-b by A3,Th93,COMPTRIG:21;
A8: c-b <> 0 by A2;
  then
A9: angle(a+-b,0c,c+-b) = angle(A,0c,B) by A5,A6,Th92;
A11: Im A = 0 by COMPLEX1:28,SIN_COS:34;
  a+-b <> 0c by A1;
  then |.a+-b.| > 0 by COMPLEX1:133;
  then
A12: Re A > 0 by SIN_COS:34,COMPLEX1:28;
  then
A13: Arg A = 0c by A11,Th34;
  then Arg(B-0c)-Arg(A-0c)>=0 by COMPTRIG:52;
  then
A14: angle(a,b,c) = Arg B by A4,A9,A13,Def6;
A15: angle(b,c,a) = angle(b+-b,c+-b,a+-b) by Th86
    .= angle(0c,B,A) by A5,A7,A8,Th92;
A16: angle(c,a,b) = angle(c+-b,a+-b,b+-b) by Th86
    .=angle(B,A,0c) by A5,A6,A7,Th92;
  thus angle(b,c,a) = 0 by A3,A11,A12,A14,A15,Lm7;
  thus angle(c,a,b) = 0 by A3,A11,A12,A14,A16,Lm7;
end;

theorem Th101:
  a <> b & a <> c & b <> c & angle(a,b,c) = 0
  implies angle(b,c,a) = 0 & angle(c,a,b) = PI or
  angle(b,c,a) = PI & angle(c,a,b) = 0
proof
  assume that
A1: a <> b & a <> c & b <> c & angle(a,b,c) = 0;
  per cases by A1,Th97;
  suppose
A2: angle(b,c,a) <> 0;
A3: 0 <= angle(b,c,a) by Th84;
    thus thesis
    proof
      per cases by XXREAL_0:1;
      suppose angle(b,c,a) < PI;
        hence thesis by A1,A2,A3,Th98;
      end;
      suppose angle(b,c,a) = PI;
        hence thesis by A1,Th100;
      end;
      suppose angle(b,c,a) > PI;
        hence thesis by A1,Th99,COMPTRIG:21;
      end;
    end;
  end;
  suppose
A4: angle(c,a,b) <> 0;
A5: 0 <= angle(c,a,b) by Th84;
    thus thesis
    proof
      per cases by XXREAL_0:1;
      suppose angle(c,a,b) < PI;
        hence thesis by A1,A4,A5,Th98;
      end;
      suppose angle(c,a,b) = PI;
        hence thesis by A1,Th100;
      end;
      suppose angle(c,a,b) > PI;
        hence thesis by A1,Th99,COMPTRIG:21;
      end;
    end;
  end;
end;


theorem
  angle(a,b,c)+angle(b,c,a)+angle(c,a,b) = PI or
  angle(a,b,c)+angle(b,c,a)+angle(c,a,b) = 5*PI iff a <> b & a <> c & b <> c
proof
  hereby
    assume
A1: angle(a,b,c)+angle(b,c,a)+angle(c,a,b) = PI
    or angle(a,b,c)+angle(b,c,a)+angle(c,a,b) = 5*PI;
    per cases by A1;
    suppose
A2:   angle(a,b,c)+angle(b,c,a)+angle(c,a,b) = PI;
      thus a <> b & a <> c & b <> c
      proof
        assume
A3:     not (a <> b & a <> c & b <> c);
        per cases by A3;
        suppose
A4:       a=b;
A5:       angle(a,c,a) = 0 by Th93;
          not (angle(a,a,c) = 0 & angle(c,a,a) = 0) by A2,A4,Th93,COMPTRIG:21;
          hence contradiction by A2,A4,A5,Th94,COMPTRIG:21;
        end;
        suppose
A6:       a=c;
A7:       angle(a,b,a) = 0 by Th93;
          not (angle(a,a,b) = 0 & angle(b,a,a) = 0) by A2,A6,Th93,COMPTRIG:21;
          hence contradiction by A2,A6,A7,Th94,COMPTRIG:21;
        end;
        suppose
A8:       b=c;
A9:       angle(b,a,b) = 0 by Th93;
          not (angle(a,b,b) = 0 & angle(b,b,a) = 0) by A2,A8,Th93,COMPTRIG:21;
          hence contradiction by A2,A8,A9,Th94,COMPTRIG:21;
        end;
      end;
    end;
    suppose
A10:  angle(a,b,c)+angle(b,c,a)+angle(c,a,b) = 5*PI;
A11:  (2+2)*PI < 5*PI by COMPTRIG:21,XREAL_1:70;
      then
A12:  2*PI+2*PI < 5*PI;
      thus a <> b & a <> c & b <> c
      proof
        assume
A13:    not (a <> b & a <> c & b <> c);
        per cases by A13;
        suppose a=b;
          then
A14:      angle(b,c,a) = 0 by Th93;
A15:      angle(c,a,b) < 2*PI by Th84;
          angle(a,b,c)+angle(b,c,a) < 2*PI by A14,Th84;
          hence contradiction by A10,A12,A15,XREAL_1:10;
        end;
        suppose b=c;
          then
A16:      angle(c,a,b) = 0 by Th93;
          angle(a,b,c) < 2*PI & angle(b,c,a) < 2*PI by Th84;
          then angle(a,b,c)+angle(b,c,a) < 2*PI+2*PI by XREAL_1:10;
          hence contradiction by A10,A11,A16;
        end;
        suppose a=c;
          then
A17:      angle(a,b,c) = 0 by Th93;
A18:      angle(c,a,b) < 2*PI by Th84;
          angle(a,b,c)+angle(b,c,a) < 2*PI by A17,Th84;
          hence contradiction by A10,A12,A18,XREAL_1:10;
        end;
      end;
    end;
  end;
  assume
A19: a <> b & a <> c & b <> c;
  per cases by XXREAL_0:1;
  suppose
S:  angle(a,b,c) = 0;
 angle(b,c,a)+angle(c,a,b) = PI
 proof
  per cases by A19,Th101,S;
  suppose angle(b,c,a) = 0 & angle(c,a,b) = PI;
    hence angle(b,c,a)+angle(c,a,b) = PI;
  end;
  suppose angle(b,c,a) = PI & angle(c,a,b) = 0;
    hence angle(b,c,a)+angle(c,a,b) = PI;
  end;
end;
    hence thesis by S;
  end;
  suppose
A20: 0 <> angle(a,b,c) & angle(a,b,c) < PI;
    0 <= angle(a,b,c) by Th84;
    hence thesis by A19,A20,Th98;
  end;
  suppose
A21: 0 <> angle(a,b,c) & angle(a,b,c) = PI;
    then angle(b,c,a) = 0 & angle(c,a,b) = 0 by A19,Th100;
    hence thesis by A21;
  end;
  suppose 0 <> angle(a,b,c) & angle(a,b,c) > PI;
    hence thesis by A19,Th99;
  end;
end;

