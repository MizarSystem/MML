    end;
A77: now
      let c be Int-Location;
      thus Exec(I,s1).c = s1.c by A73,SCMFSA_2:101
        .= s2.c by A1,Th30
        .= Exec(I,s2).c by A73,SCMFSA_2:101;
    end;
    IC Exec(I,s1) = succ IC s1 by A73,SCMFSA_2:101
      .= succ IC s2 by A1,COMPOS_1:24
      .= IC Exec(I,s2) by A73,SCMFSA_2:101;
