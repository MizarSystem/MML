theorem Th23:
  AddressPart goto i1 = <*i1*>
proof
  thus AddressPart goto i1 = [ 6, <*i1*>]`2 by Th14
    .= <*i1*> by MCART_1:def 2;
end;

theorem Th24:
  AddressPart (a=0_goto i1) = <*i1,a*>
proof
  thus AddressPart (a=0_goto i1) = [ 7, <*i1,a*>]`2 by Th15
    .= <*i1,a*> by MCART_1:def 2;
