theorem Th76:
  NIC(a=0_goto i1, il) = {i1, Next il}
proof
  hereby
    let x be set;
    assume x in NIC(a=0_goto i1, il);
    then consider s being State of SCM+FSA such that
A1: x = IC Following s & IC s = il & s.il = a=0_goto i1;
    per cases;
    suppose s.a = 0;
      then x = i1 by A1,SCMFSA_2:96;
      hence x in {i1, Next il} by TARSKI:def 2;
    end;
    suppose s.a <> 0;
      then x = Next il by A1,SCMFSA_2:96;
      hence x in {i1, Next il} by TARSKI:def 2;
    end;
  end;
  let x be set;
  assume
A2: x in {i1, Next il};
  consider t being State of SCM+FSA;
  il in NAT by AMI_1:def 4;
  then reconsider il1 = il as Element of ObjectKind IC SCM+FSA
  by AMI_1:def 11;
  reconsider I = a=0_goto i1 as Element of ObjectKind il by AMI_1:def 14;
  set u = t+*((IC SCM+FSA, il)-->(il1, I));
  il in NAT by AMI_1:def 4;
  then
A3: a <> il by Th3;
A4: IC SCM+FSA <> a by SCMFSA_2:81;
  per cases by A2,TARSKI:def 2;
  suppose
A5: x = i1;
    set v = u+*(a .--> 0);
A6: dom (a .--> 0) = {a} by FUNCOP_1:19;
    then not IC SCM+FSA in dom (a .--> 0) by A4,TARSKI:def 1;
    then
A7: IC v = IC u by FUNCT_4:12
      .= il by AMI_1:129;
    not il in dom (a .--> 0) by A3,A6,TARSKI:def 1;
    then
A8: v.il = u.il by FUNCT_4:12
      .= I by AMI_1:129;
    a in dom (a .--> 0) by A6,TARSKI:def 1;
    then v.a = (a .--> 0).a by FUNCT_4:14
      .= 0 by FUNCOP_1:87;
    then IC Following v = i1 by A7,A8,SCMFSA_2:96;
    hence thesis by A5,A7,A8;
  end;
  suppose
A9: x = Next il;
    set v = u+*(a .--> 1);
A10: dom (a .--> 1) = {a} by FUNCOP_1:19;
    then not IC SCM+FSA in dom (a .--> 1) by A4,TARSKI:def 1;
    then
A11: IC v = IC u by FUNCT_4:12
      .= il by AMI_1:129;
    not il in dom (a .--> 1) by A3,A10,TARSKI:def 1;
    then
A12: v.il = u.il by FUNCT_4:12
      .= I by AMI_1:129;
    a in dom (a .--> 1) by A10,TARSKI:def 1;
    then v.a = (a .--> 1).a by FUNCT_4:14
      .= 1 by FUNCOP_1:87;
    then IC Following v = Next il by A11,A12,SCMFSA_2:96;
    hence thesis by A9,A11,A12;
  end;
end;

theorem Th77:
