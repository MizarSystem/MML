reserve a, b, d1, d2, d3, d4 for Int-Location,
  A, B for Data-Location,
  f, f1, f2, f3 for FinSeq-Location,
  il, i1, i2 for Element of NAT,
  L for Element of NAT,
  I for Instruction of SCM+FSA,
  s,s1,s2 for State of SCM+FSA,
  T for InsType of SCM+FSA,
  k for natural number;

definition
  let la, lb be Int-Location, a, b be Integer;
  redefine func (la,lb) --> (a,b) -> PartState of SCM+FSA;
  coherence
  proof
A1: ObjectKind lb = INT by SCMFSA_2:11;
 b is Element of INT by INT_1:def 2;
     then reconsider b as Element of ObjectKind lb by A1;
A2: ObjectKind la = INT by SCMFSA_2:11;
    a is Element of INT by INT_1:def 2;
     then reconsider a as Element of ObjectKind la by A2;
      (la,lb) --> (a,b) is PartState of SCM+FSA;
    hence thesis;
  end;
end;

theorem Th7:
  for o being Object of SCM+FSA st o in Data-Locations SCM+FSA
   holds o is Int-Location or o is FinSeq-Location
proof
  let o be Object of SCM+FSA;
  assume o in Data-Locations SCM+FSA;
   then o in SCM+FSA-Data-Loc or o in SCM+FSA-Data*-Loc
    by SCMFSA_2:100,XBOOLE_0:def 3;
  hence thesis by SCMFSA_2:def 4,def 5;
end;

theorem Th9:
  a := b = [1,{},<*a,b*>]
proof
  ex A,B st a = A & b = B & a := b = A:=B by SCMFSA_2:def 8;
  hence thesis;
end;

theorem Th10:
  AddTo(a,b) = [2,{},<*a,b*>]
proof
  ex A,B st a = A & b = B & AddTo(a,b) = AddTo(A,B) by SCMFSA_2:def 9;
  hence thesis;
end;

theorem Th11:
  SubFrom(a,b) = [3,{},<* a,b *>]
proof
  ex A,B st a = A & b = B & SubFrom(a,b) = SubFrom(A,B) by SCMFSA_2:def 10;
  hence thesis;
end;

theorem Th12:
  MultBy(a,b) = [4,{},<* a,b *>]
proof
  ex A,B st a = A & b = B & MultBy(a,b) = MultBy(A,B) by SCMFSA_2:def 11;
  hence thesis;
end;

theorem Th13:
  Divide(a,b) = [5,{},<* a,b *>]
proof
  ex A,B st a = A & b = B & Divide(a,b) = Divide(A,B) by SCMFSA_2:def 12;
  hence thesis;
end;

theorem Th15:
  a=0_goto il = [7, <* il*>,<*a *>]
proof
  ex A st A = a & A=0_goto il = a=0_goto il by SCMFSA_2:def 14;
  hence thesis;
end;

theorem Th16:
  a>0_goto il = [8, <* il*>,<*a *>]
proof
  ex A st A = a & A>0_goto il = a>0_goto il by SCMFSA_2:def 15;
  hence thesis;
end;

reserve J,K for Element of Segm 13,
  b,b1,c,c1 for Element of SCM+FSA-Data-Loc,
  f,f1 for Element of SCM+FSA-Data*-Loc;

Lm1: T = 0 or T = 1 or T = 2 or T = 3 or T = 4 or T = 5 or T = 6 or T = 7 or T
= 8 or T = 9 or T = 10 or T = 11 or T = 12

proof
  consider y being set such that
A1: [T,y] in proj1 the Instructions of SCM+FSA by RELAT_1:def 4;
  consider x being set such that
A2: [[T,y],x] in the Instructions of SCM+FSA by A1,RELAT_1:def 4;
   reconsider I = [T,y,x] as Instruction of SCM+FSA by A2;
A3: I in SCM-Instr \/ { [J,{},<*c,f,b*>] : J in {9,10} }
 or I in { [K,{},<*c1,f1*>] : K in {11,12} }
  by XBOOLE_0:def 3;

  per cases by A3,XBOOLE_0:def 3;
  suppose
    I in SCM-Instr;
    then reconsider I as Instruction of SCM;
    T = InsCode I by RECDEF_2:def 1;
    hence thesis by AMI_6:1;
  end;
  suppose
    I in { [J,{},<*c,f,b*>] : J in {9,10} };
    then ex J,c,b,f st I = [J,{},<*c,f,b*>] & J in {9,10};
    then T in {9,10} by MCART_1:25;
    hence thesis by TARSKI:def 2;
  end;
  suppose
    I in { [K,{},<*c1,f1*>] : K in {11,12} };
    then ex K,c1,f1 st I = [K,{},<*c1,f1*>] & K in {11,12};
    then T in {11,12} by MCART_1:25;
    hence thesis by TARSKI:def 2;
  end;
end;

