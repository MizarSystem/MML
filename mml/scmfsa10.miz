    end;
A48: now
      let f be FinSeq-Location;
      thus Exec(I,s1).f = s1.f by A42,SCMFSA_2:97
        .= s2.f by A1,Th31
        .= Exec(I,s2).f by A42,SCMFSA_2:97;
