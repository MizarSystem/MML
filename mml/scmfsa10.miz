      NIC(a=0_goto i1, il1) = {i1, Next il1} &
      NIC(a=0_goto i1, il2) = {i1, Next il2} by Th76;

then (x = i1 or x = Next il1) & (x = i1 or x = Next il2) by A2,TARSKI:def 2;
      hence x in {i1} by TARSKI:def 1;
    end;
    assume x in {i1};
