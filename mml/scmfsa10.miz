theorem Th30:
  T = 0 implies JumpParts T = {0}
proof
  assume
A1: T = 0;
  hereby
    let a be set;
    assume a in JumpParts T;
    then consider I such that
A2: a = JumpPart I and
A3: InsCode I = T;
    I = halt SCM+FSA by A1,A3,SCMFSA_2:95;
    hence a in {0} by A2,Th17,TARSKI:def 1;
  end;
  let a be set;
  assume a in {0};
  then a = 0 by TARSKI:def 1;
  hence thesis by A1,Th17,SCMFSA_2:97;
end;

theorem Th31:
  T = 1 implies JumpParts T = {{}}
proof
  assume
A1: T = 1;
  hereby
    let x be set;
    assume x in JumpParts T;
     then consider I being Instruction of SCM+FSA such that
A2:   x = JumpPart I and
A3:   InsCode I = T;
     consider a,b such that
A4:   I = a:=b by A1,A3,SCMFSA_2:30;
     x = {} by A2,Th18,A4;
    hence x in {{}} by TARSKI:def 1;
  end;
  set a = the Int-Location;
  let x be set;
  assume x in {{}};
   then x = {} by TARSKI:def 1;
   then
A5:  x = JumpPart(a:= a) by Th18;
    InsCode(a:= a) = 1 by SCMFSA_2:18;
  hence thesis by A5,A1;
end;

