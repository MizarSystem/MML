Lm7: intloc 0 <> intloc 1 by AMI_3:52;

registration
  let a, b;
  cluster InsCode (a:=b) -> non jump-only InsType of SCM+FSA;
  coherence
  proof
    consider w being State of SCM+FSA;
    set t = w+*((intloc 0, intloc 1)-->(0,1));
A1: InsCode (a:=b) = 1 by SCMFSA_2:42
      .= InsCode (intloc 0:=intloc 1) by SCMFSA_2:42;
A2: intloc 0 <> IC SCM+FSA by SCMFSA_2:81;
    dom ((intloc 0, intloc 1)-->(0,1)) = {intloc 0, intloc 1} by FUNCT_4:65;
    then
A3: intloc 0 in dom ((intloc 0, intloc 1)-->(0,1)) &
    intloc 1 in dom ((intloc 0, intloc 1)-->(0,1)) by TARSKI:def 2;
    then
A4: t.intloc 0 = (intloc 0, intloc 1)-->(0,1).intloc 0 by FUNCT_4:14
      .= 0 by Lm7,FUNCT_4:66;
    Exec((intloc 0:=intloc 1), t).intloc 0 = t.intloc 1 by SCMFSA_2:89
      .= (intloc 0, intloc 1)-->(0,1).intloc 1 by A3,FUNCT_4:14
      .= 1 by FUNCT_4:66;
    hence thesis by A1,A2,A4,AMISTD_1:def 3;
  end;
  cluster InsCode AddTo(a,b) -> non jump-only InsType of SCM+FSA;
  coherence
  proof
    consider w being State of SCM+FSA;
    set t = w+*((intloc 0, intloc 1)-->(0,1));
A5: InsCode AddTo(a,b) = 2 by SCMFSA_2:43
      .= InsCode AddTo(intloc 0, intloc 1) by SCMFSA_2:43;
A6: intloc 0 <> IC SCM+FSA by SCMFSA_2:81;
    dom ((intloc 0, intloc 1)-->(0,1)) = {intloc 0, intloc 1} by FUNCT_4:65;
    then
A7: intloc 0 in dom ((intloc 0, intloc 1)-->(0,1)) &
    intloc 1 in dom ((intloc 0, intloc 1)-->(0,1)) by TARSKI:def 2;
    then
A8: t.intloc 0 = (intloc 0, intloc 1)-->(0,1).intloc 0 by FUNCT_4:14
      .= 0 by Lm7,FUNCT_4:66;
    t.intloc 1 = (intloc 0, intloc 1)-->(0,1).intloc 1 by A7,FUNCT_4:14
      .= 1 by FUNCT_4:66;
    then Exec(AddTo(intloc 0, intloc 1), t).intloc 0 = 0+1 by A8,SCMFSA_2:90;
    hence thesis by A5,A6,A8,AMISTD_1:def 3;
  end;
  cluster InsCode SubFrom(a,b) -> non jump-only InsType of SCM+FSA;
  coherence
  proof
    consider w being State of SCM+FSA;
    set t = w+*((intloc 0, intloc 1)-->(0,1));
A9: InsCode SubFrom(a,b) = 3 by SCMFSA_2:44
      .= InsCode SubFrom(intloc 0, intloc 1) by SCMFSA_2:44;
A10: intloc 0 <> IC SCM+FSA by SCMFSA_2:81;
    dom ((intloc 0, intloc 1)-->(0,1)) = {intloc 0, intloc 1} by FUNCT_4:65;
    then
A11: intloc 0 in dom ((intloc 0, intloc 1)-->(0,1)) &
    intloc 1 in dom ((intloc 0, intloc 1)-->(0,1)) by TARSKI:def 2;
    then
A12: t.intloc 0 = (intloc 0, intloc 1)-->(0,1).intloc 0 by FUNCT_4:14
      .= 0 by Lm7,FUNCT_4:66;
A13: t.intloc 1 = (intloc 0, intloc 1)-->(0,1).intloc 1 by A11,FUNCT_4:14
      .= 1 by FUNCT_4:66;
    Exec(SubFrom(intloc 0, intloc 1), t).intloc 0
    = t.intloc 0 - t.intloc 1 by SCMFSA_2:91
      .= -1 by A12,A13;
    hence thesis by A9,A10,A12,AMISTD_1:def 3;
  end;
  cluster InsCode MultBy(a,b) -> non jump-only InsType of SCM+FSA;
  coherence
  proof
    consider w being State of SCM+FSA;
    set t = w+*((intloc 0, intloc 1)-->(1,0));
A14: InsCode MultBy(a,b) = 4 by SCMFSA_2:45
      .= InsCode MultBy(intloc 0, intloc 1) by SCMFSA_2:45;
A15: intloc 0 <> IC SCM+FSA by SCMFSA_2:81;
    dom ((intloc 0, intloc 1)-->(1,0)) = {intloc 0, intloc 1} by FUNCT_4: 65;
    then
A16: intloc 0 in dom ((intloc 0, intloc 1)-->(1,0)) &
    intloc 1 in dom ((intloc 0, intloc 1)-->(1,0)) by TARSKI:def 2;
    then
A17: t.intloc 0 = (intloc 0, intloc 1)-->(1,0).intloc 0 by FUNCT_4:14
      .= 1 by Lm7,FUNCT_4:66;
A18: t.intloc 1 = (intloc 0, intloc 1)-->(1,0).intloc 1 by A16,FUNCT_4:14
      .= 0 by FUNCT_4:66;
