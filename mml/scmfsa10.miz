  dom JumpPart (a=0_goto (i1+k))
   = dom <*(i1+k)*> by Th24
    .= Seg 1 by FINSEQ_1:55
    .= dom <*i1*> by FINSEQ_1:55
    .= dom JumpPart (a=0_goto i1) by Th24;
   then JumpPart IncAddr(a=0_goto i1,k) = JumpPart (a=0_goto (i1+k))
     by A2,A3,FUNCT_1:9;
  hence thesis by A1,B1,COMPOS_1:7;
