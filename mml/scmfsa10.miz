      hence (AddressPart IncAddr(a=0_goto i1,k)).x
        = (AddressPart (a=0_goto i1)).x by A4,AMISTD_2:def 14
       .= <*i1,a*>.x by Th24
       .= a by A9,FINSEQ_1:61
       .= <*il.(SCM+FSA, locnum i1 + k),a*>.x by A9,FINSEQ_1:61
       .= (AddressPart (a=0_goto il.(SCM+FSA, locnum i1 + k))).x by Th24;
    end;
