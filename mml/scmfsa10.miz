  <*a,x*>.2 = x by FINSEQ_1:61;
  then x in pi(AddressParts InsCode (a:=len f),2) by A9,CARD_3:def 6;
  hence thesis by A2,CARD_3:93;
end;

theorem Th66:
  (product" AddressParts InsCode (f:=<0,...,0>a)).1 = SCM+FSA-Data-Loc
proof
A1: InsCode (f:=<0,...,0>a) = 12 by SCMFSA_2:53;
  dom product" AddressParts InsCode (f:=<0,...,0>a) = {1,2} by Th42,SCMFSA_2
  :53;
  then
A2: 1 in dom product" AddressParts InsCode (f:=<0,...,0>a) by TARSKI:def 2;
  hereby
    let x be set;
    assume x in (product" AddressParts InsCode (f:=<0,...,0>a)).1;
    then x in pi(AddressParts InsCode (f:=<0,...,0>a),1) by A2,CARD_3:93;
    then consider g being Function such that
A3: g in AddressParts InsCode (f:=<0,...,0>a) and
A4: x = g.1 by CARD_3:def 6;
    consider I being Instruction of SCM+FSA such that
A5: g = AddressPart I and
A6: InsCode I = InsCode (f:=<0,...,0>a) by A3;
    consider a, f such that
A7: I = f:=<0,...,0>a by A1,A6,SCMFSA_2:65;
    g = <*a,f*> by A5,A7,MCART_1:def 2;
    then x = a by A4,FINSEQ_1:61;
    hence x in SCM+FSA-Data-Loc by SCMFSA_2:def 4;
  end;
  let x be set;
  assume x in SCM+FSA-Data-Loc;
  then reconsider x as Int-Location by SCMFSA_2:28;
A8: AddressPart (f:=<0,...,0>x) = <*x,f*> by MCART_1:def 2;
  InsCode (f:=<0,...,0>x) = InsCode (f:=<0,...,0>a) by A1,SCMFSA_2:53;
  then
A9: <*x,f*> in AddressParts InsCode (f:=<0,...,0>a) by A8;
  <*x,f*>.1 = x by FINSEQ_1:61;
  then x in pi(AddressParts InsCode (f:=<0,...,0>a),1) by A9,CARD_3:def 6;
  hence thesis by A2,CARD_3:93;
end;

theorem Th67:
  (product" AddressParts InsCode (f:=<0,...,0>a)).2 = SCM+FSA-Data*-Loc
proof
A1: InsCode (f:=<0,...,0>a) = 12 by SCMFSA_2:53;
