A240:   K = (f3,d3):=d4 by A216,A239,SCMFSA_2:63;
A241: z = <*d4,f3,d3*> by A238,A240,Th27;
A242: dom <*b,f2,d3*> = {1,2,3} by FINSEQ_3:1,30;
