      then k = 1 by A52,FINSEQ_1:90;
      then
A64:  f = i1 by A63,A53,FINSEQ_1:def 8;
      JUMP I = {i1} by A62,Th77;
      hence thesis by A64,TARSKI:def 1;
    end;
    suppose
      ex i1,a st I = a>0_goto i1;
      then consider a, i1 such that
A65:  I = a>0_goto i1;
A66:  JumpPart I = <*i1*> by A65,Th25;
      then k = 1 by A52,FINSEQ_1:90;
      then
A67:  f = i1 by A66,A53,FINSEQ_1:def 8;
      JUMP I = {i1} by A65,Th79;
      hence thesis by A67,TARSKI:def 1;
    end;
    suppose
      ex a,b,f st I = b:=(f,a);
      then consider a, b, f such that
A68:  I = b:=(f,a);
      k in dom {} by A52,A68,RECDEF_2:def 2;
      hence thesis;
    end;
    suppose
      ex a,b,f st I = (f,a):=b;
      then consider a, b, f such that
A69:  I = (f,a):=b;
      k in dom {} by A52,A69,RECDEF_2:def 2;
      hence thesis;
    end;
    suppose
      ex a,f st I = a:=len f;
      then consider a, f such that
A70:  I = a:=len f;
      k in dom {} by A52,A70,RECDEF_2:def 2;
      hence thesis;
    end;
    suppose
      ex a,f st I = f:=<0,...,0>a;
      then consider a, f such that
A71:  I = f:=<0,...,0>a;
      k in dom {} by A52,A71,RECDEF_2:def 2;
      hence thesis;
    end;
  end;
  end;
end;

registration
  cluster SCM+FSA -> regular J/A-independent;
  coherence
  proof
   thus SCM+FSA is regular
   proof
    let I be Instruction of SCM+FSA;
     set T = InsCode I;
A1:   JumpPart I in JumpParts T;
    per cases by Lm1;
    suppose T = 0;
      then JumpParts T = {{}} by Th30;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
    suppose T = 1;
      then JumpParts T = {{}} by Th31;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
    suppose T = 2;
      then JumpParts T = {{}} by Th32;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
    suppose T = 3;
      then JumpParts T = {{}} by Th33;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
    suppose T = 4;
      then JumpParts T = {{}} by Th34;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
    suppose T = 5;
      then JumpParts T = {{}} by Th35;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
    suppose
A2: T = 6;
     then consider i2 such that
A3:    I = goto i2 by SCMFSA_2:35;
     let k be set;
     assume
A4:      k in dom JumpPart I;
      for f being Function st f in JumpParts T holds k in dom f
       proof let f be Function;
        assume f in JumpParts T;
         then consider J being Instruction of SCM+FSA such that
A5:       f = JumpPart J and
A6:       InsCode J = T;
        thus k in dom f by A4,A5,A6,COMPOS_1:def 12;
       end;
      then k in dom product" JumpParts T by CARD_3:def 12;
      then k in {1} by A2,Th36;
      then k = 1 by TARSKI:def 1;
     hence (product" JumpParts InsCode I).k = NAT by A3,Th53;
    end;
    suppose
A7: T = 7;
     then consider i1,a such that
A8:    I = a=0_goto i1 by SCMFSA_2:36;
     let k be set;
     assume
A9:      k in dom JumpPart I;
      for f being Function st f in JumpParts T holds k in dom f
       proof let f be Function;
        assume f in JumpParts T;
         then consider J being Instruction of SCM+FSA such that
A10:       f = JumpPart J and
A11:       InsCode J = T;
        thus k in dom f by A9,A10,A11,COMPOS_1:def 12;
       end;
      then k in dom product" JumpParts T by CARD_3:def 12;
      then k in {1} by A7,Th37;
      then k = 1 by TARSKI:def 1;
     hence (product" JumpParts InsCode I).k = NAT by A8,Th54;
    end;
    suppose
A12: T = 8;
     then consider i1,a such that
A13:    I = a>0_goto i1 by SCMFSA_2:37;
     let k be set;
     assume
A14:      k in dom JumpPart I;
      for f being Function st f in JumpParts T holds k in dom f
       proof let f be Function;
        assume f in JumpParts T;
         then consider J being Instruction of SCM+FSA such that
A15:       f = JumpPart J and
A16:       InsCode J = T;
        thus k in dom f by A14,A15,A16,COMPOS_1:def 12;
       end;
      then k in dom product" JumpParts T by CARD_3:def 12;
      then k in {1} by A12,Th38;
      then k = 1 by TARSKI:def 1;
     hence (product" JumpParts InsCode I).k = NAT by A13,Th56;
    end;
    suppose T = 9;
      then JumpParts T = {{}} by Th39;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
    suppose T = 10;
      then JumpParts T = {{}} by Th40;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
    suppose T = 11;
      then JumpParts T = {{}} by Th41;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
    suppose T = 12;
      then JumpParts T = {{}} by Th42;
      then JumpPart I = {} by A1,TARSKI:def 1;
     hence thesis;
    end;
   end;
   let T be InsType of SCM+FSA,
       f1,f2 be Function, p be set such that
A17: f1 in JumpParts T and
A18: f2 in product product" JumpParts T and
A19: [T,f1,p] in the Instructions of SCM+FSA;
    per cases by Lm1;
    suppose T = 0 or T = 1 or T = 2 or T = 3 or T = 4 or T = 5
       or T = 9 or T = 10 or T = 11 or T = 12;
     then
A20:  JumpParts T = {0} by Th30,Th31,Th32,Th33,Th34,Th35,Th39,Th40,Th41,Th42;
     then f1 = 0 by A17,TARSKI:def 1 .= f2 by A18,A20,CARD_3:10,106
,TARSKI:def 1;
    hence [T,f2,p] in the Instructions of SCM+FSA by A19;
    end;
    suppose
A21:    T = 6;
    reconsider J = [T,f1,p] as Instruction of SCM+FSA by A19;
A22: InsCode J = 6 by A21,RECDEF_2:def 1;
    then consider i1 being Element of NAT such that
A23:  J = goto i1 by SCMFSA_2:35;
A24:   p = {} by A23,MCART_1:25;
A25: dom f2 = dom product" JumpParts T  by A18,CARD_3:9;
A26: dom f2 = {1} by A21,A25,Th36;
    then 1 in dom f2 by TARSKI:def 1;
    then f2.1 in (product" JumpParts T).1 by A25,A18,CARD_3:9;
    then reconsider l = f2.1 as Element of NAT by A23,A21,A22,Th53;
    set I = [T,f2,{}];
    I = goto l by A21,A26,FINSEQ_1:2,def 8;
    then  reconsider I as Instruction of SCM+FSA;
    f2 = JumpPart I by RECDEF_2:def 2;
    hence [T,f2,p] in the Instructions of SCM+FSA by A24;
    end;
    suppose
A27:    T = 7;
    reconsider J = [T,f1,p] as Instruction of SCM+FSA by A19;
A28: InsCode J = T by RECDEF_2:def 1;
    then consider i1,a such that
A29:  J = a=0_goto i1 by A27,SCMFSA_2:36;
    consider A being Data-Location such that
A30:  a = A & a=0_goto i1 = A=0_goto i1 by SCMFSA_2:def 14;
A31:   p = <*a*> by A29,A30,MCART_1:25;
A32: dom f2 = dom product" JumpParts T  by A18,CARD_3:9;
A33: dom f2 = {1} by A27,A32,Th37;
    then 1 in dom f2 by TARSKI:def 1;
    then f2.1 in (product" JumpParts T).1 by A32,A18,CARD_3:9;
    then reconsider l = f2.1 as Element of NAT by A29,A28,Th54;
    set I = [T,f2,p];
    f2 = <*l*> by A33,FINSEQ_1:2,def 8;
    then I = a=0_goto l by A27,A31,Th15;
    then reconsider I as Instruction of SCM+FSA;
A34: InsCode I = T by RECDEF_2:def 1;
    consider i2,b such that
A35: I = b=0_goto i2 by A27,A34,SCMFSA_2:36;
    thus [T,f2,p] in the Instructions of SCM+FSA by A35;
    end;
    suppose
A36:    T = 8;
    reconsider J = [T,f1,p] as Instruction of SCM+FSA by A19;
A37: InsCode J = T by RECDEF_2:def 1;
    then consider i1,a such that
A38:  J = a>0_goto i1 by A36,SCMFSA_2:37;
    consider A being Data-Location such that
A39:  a = A & a>0_goto i1 = A>0_goto i1 by SCMFSA_2:def 15;
A40:   p = <*a*> by A38,A39,MCART_1:25;
A41: dom f2 = dom product" JumpParts T  by A18,CARD_3:9;
A42: dom f2 = {1} by A36,A41,Th38;
    then 1 in dom f2 by TARSKI:def 1;
    then f2.1 in (product" JumpParts T).1 by A41,A18,CARD_3:9;
    then reconsider l = f2.1 as Element of NAT by A38,A37,Th56;
    set I = [T,f2,p];
    f2 = <*l*> by A42,FINSEQ_1:2,def 8;
    then I = a>0_goto l by A36,A40,Th16;
    then reconsider I as Instruction of SCM+FSA;
A43: InsCode I = T by RECDEF_2:def 1;
    consider i2,b such that
A44: I = b>0_goto i2 by A36,A43,SCMFSA_2:37;
    thus [T,f2,p] in the Instructions of SCM+FSA by A44;
    end;
  end;
end;

theorem Th88:
  IncAddr(goto i1,k) = goto (i1+ k)
proof
A1: InsCode IncAddr(goto i1,k) = InsCode goto i1 by COMPOS_1:def 17
    .= 6 by SCMFSA_2:23
    .= InsCode goto (i1+k) by SCMFSA_2:23;
A2: AddressPart IncAddr(goto i1,k) = AddressPart goto i1 by COMPOS_1:def 17
    .= {} by RECDEF_2:def 3
    .= AddressPart goto (i1+k) by RECDEF_2:def 3;
A3: JumpPart IncAddr(goto i1,k) = k + JumpPart goto i1
 by COMPOS_1:def 17;
    then
A4: dom JumpPart IncAddr(goto i1,k) = dom JumpPart goto i1
   by VALUED_1:def 2;
A5: for x being set st x in dom JumpPart goto i1 holds (JumpPart
  IncAddr(goto i1,k)).x = (JumpPart goto (i1+k)).x
  proof
    let x be set;
    assume
A6: x in dom JumpPart goto i1;
    then x in dom <*i1*> by RECDEF_2:def 2;
    then
A7: x = 1 by FINSEQ_1:90;
    set f = (JumpPart goto i1).x;
A8: (JumpPart IncAddr(goto i1,k)).x = k+f by A4,A3,A6,VALUED_1:def 2;
    f = <*i1*>.x by RECDEF_2:def 2
      .= i1 by A7,FINSEQ_1:def 8;
    hence
    (JumpPart IncAddr(goto i1,k)).x =
     <*(i1+k)*>.
    x by A7,A8,FINSEQ_1:def 8
      .= (JumpPart goto (i1+k)).x by RECDEF_2:def 2;
  end;
  dom JumpPart goto (i1+k)
   = dom <*(i1+k)*> by RECDEF_2:def 2
    .= Seg 1 by FINSEQ_1:def 8
    .= dom <*i1*> by FINSEQ_1:def 8
    .= dom JumpPart goto i1 by RECDEF_2:def 2;
   then JumpPart IncAddr(goto i1,k) = JumpPart goto(i1+k) by A4,A5,FUNCT_1:2;
  hence thesis by A1,A2,COMPOS_1:1;
end;

theorem Th89:
  IncAddr(a=0_goto i1,k) = a=0_goto (i1+k)
proof
A1: InsCode IncAddr(a=0_goto i1,k) = InsCode (a=0_goto i1) by COMPOS_1:def 17
    .= 7 by SCMFSA_2:24
    .= InsCode (a=0_goto (i1+k)) by SCMFSA_2:24;
A2: a=0_goto i1 = [7, <* i1*>,<*a *>] by Th15;
A3: a=0_goto(i1+k) = [7, <* i1+k*>,<*a *>] by Th15;
A4: AddressPart IncAddr(a=0_goto i1,k)
     = AddressPart (a=0_goto i1) by COMPOS_1:def 17
    .= <*a*> by A2,RECDEF_2:def 3
    .= AddressPart (a=0_goto (i1+k)) by A3,RECDEF_2:def 3;
A5: JumpPart IncAddr(a=0_goto i1,k) = k + JumpPart (a=0_goto i1)
  by COMPOS_1:def 17;
  then
A6: dom JumpPart IncAddr(a=0_goto i1,k) = dom JumpPart (a=0_goto i1)
  by VALUED_1:def 2;
A7: for x being set st x in dom JumpPart (a=0_goto i1) holds (JumpPart
IncAddr(a=0_goto i1,k)).x = (JumpPart (a=0_goto (i1+k))).x
  proof
    let x be set;
    assume
A8: x in dom JumpPart (a=0_goto i1);
    then
    x in dom <*i1*> by Th24;
     then
A9:   x = 1 by FINSEQ_1:90;
      set f = (JumpPart (a=0_goto i1)).x;
A10:   (JumpPart IncAddr(a=0_goto i1,k)).x = k+f by A6,A5,A8,VALUED_1:def 2;
      f = <*i1*>.x by Th24
        .= i1 by A9,FINSEQ_1:40;
      hence
      (JumpPart IncAddr(a=0_goto i1,k)).x =
       <*(i1+k)*>.x by A9,A10,FINSEQ_1:40
        .= (JumpPart (a=0_goto (i1+k))).x
         by Th24;
  end;
  dom JumpPart (a=0_goto (i1+k))
   = dom <*(i1+k)*> by Th24
    .= Seg 1 by FINSEQ_1:38
    .= dom <*i1*> by FINSEQ_1:38
    .= dom JumpPart (a=0_goto i1) by Th24;
   then JumpPart IncAddr(a=0_goto i1,k) = JumpPart (a=0_goto (i1+k))
     by A6,A7,FUNCT_1:2;
  hence thesis by A1,A4,COMPOS_1:1;
end;

theorem Th90:
  IncAddr(a>0_goto i1,k) = a>0_goto (i1+k)
proof
A1: InsCode IncAddr(a>0_goto i1,k) = InsCode (a>0_goto i1) by COMPOS_1:def 17
    .= 8 by SCMFSA_2:25
    .= InsCode (a>0_goto (i1+k)) by SCMFSA_2:25;
A2: a>0_goto i1 = [8, <* i1*>,<*a *>] by Th16;
A3: a>0_goto(i1+k) = [8, <* i1+k*>,<*a *>] by Th16;
A4: AddressPart IncAddr(a>0_goto i1,k)
     = AddressPart (a>0_goto i1) by COMPOS_1:def 17
    .= <*a*> by A2,RECDEF_2:def 3
    .= AddressPart (a>0_goto (i1+k)) by A3,RECDEF_2:def 3;
A5:  JumpPart IncAddr(a>0_goto i1,k) = k + JumpPart (a>0_goto i1)
  by COMPOS_1:def 17;
  then
A6: dom JumpPart IncAddr(a>0_goto i1,k) = dom JumpPart (a>0_goto i1)
  by VALUED_1:def 2;
A7: for x being set st x in dom JumpPart (a>0_goto i1) holds (JumpPart
IncAddr(a>0_goto i1,k)).x = (JumpPart (a>0_goto (i1+k))).x
  proof
    let x be set;
    assume
A8: x in dom JumpPart (a>0_goto i1);
    then x in dom <*i1*> by Th25;
     then
A9:   x = 1 by FINSEQ_1:90;
    set f = (JumpPart (a>0_goto i1)).1;
A10:   (JumpPart IncAddr(a>0_goto i1,k)).1 = k+f by A9,A6,A5,A8,VALUED_1:def 2;
      f = <*i1*>.x by Th25,A9
        .= i1 by A9,FINSEQ_1:40;
      hence
      (JumpPart IncAddr(a>0_goto i1,k)).x
       = <*(i1+k)*>.x by A9,A10,FINSEQ_1:40
        .= (JumpPart (a>0_goto (i1+k))).x by Th25;
  end;
  dom JumpPart (a>0_goto (i1+k)) =
   dom <*(i1+k)*> by Th25
    .= Seg 1 by FINSEQ_1:38
    .= dom <*i1*> by FINSEQ_1:38
    .= dom JumpPart (a>0_goto i1) by Th25;
   then JumpPart IncAddr(a>0_goto i1,k) = JumpPart (a>0_goto (i1+k))
     by A6,A7,FUNCT_1:2;
  hence thesis by A1,A4,COMPOS_1:1;
end;

registration
  cluster SCM+FSA -> IC-relocable;
  coherence
  proof
   let I be Instruction of SCM+FSA;
      per cases by SCMFSA_2:93;
      suppose
        I = [0,{},{}];
        hence thesis by AMI_3:26,SCMFSA_2:96;
      end;
      suppose
        ex a,b st I = a:=b;
        hence thesis;
      end;
      suppose
        ex a,b st I = AddTo(a,b);
        hence thesis;
      end;
      suppose
        ex a,b st I = SubFrom(a,b);
        hence thesis;
      end;
      suppose
        ex a,b st I = MultBy(a,b);
        hence thesis;
      end;
      suppose
        ex a,b st I = Divide(a,b);
        hence thesis;
      end;
      suppose
A1:     ex i1 st I = goto i1;
        let j,k be Nat, s1 be State of SCM+FSA;
        set s2 = IncIC(s1,k);
        consider i1 such that
A2:     I = goto i1 by A1;
        thus IC Exec(IncAddr(I,j),s1) + k
         = IC Exec(goto(j+i1),s1) + k by A2,Th88
        .= j+i1+k by SCMFSA_2:69
        .= IC Exec(goto(j+k+i1),s2) by SCMFSA_2:69
        .= IC Exec(IncAddr(I,j+k), s2) by A2,Th88;
      end;
      suppose
        ex i1,a st I = a=0_goto i1;
        then consider a, i1 such that
A3:     I = a=0_goto i1;
        let j,k be natural number, s1 be State of SCM+FSA;
        set s2 = IncIC(s1,k);
        a <> IC SCM+FSA & dom (IC SCM+FSA .--> (IC s1 + k)) = {IC SCM+FSA}
        by FUNCOP_1:13,SCMFSA_2:56;
        then not a in dom (IC SCM+FSA .--> (IC s1 + k)) by TARSKI:def 1;
        then
A4:     s1.a = s2.a by FUNCT_4:11;
          per cases;
          suppose
A5:         s1.a = 0;
            thus IC Exec(IncAddr(I,j),s1) + k
         = IC Exec(a=0_goto(j+i1),s1) + k by A3,Th89
        .= j+i1+k by A5,SCMFSA_2:70
        .= IC Exec(a=0_goto(j+k+i1),s2) by A4,A5,SCMFSA_2:70
            .= IC Exec(IncAddr(I,j+k), s2) by A3,Th89;
          end;
          suppose
A6:         s1.a <> 0;
A7:         IncAddr(I,j) = a=0_goto(i1+j) by A3,Th89;
A8:         IncAddr(I,j+k) = a=0_goto(i1+(j+k)) by A3,Th89;
            dom (IC SCM+FSA .--> (IC s1 + k)) = {IC SCM+FSA} by FUNCOP_1:13;
            then IC SCM+FSA in dom (IC SCM+FSA .--> (IC s1 + k))
                 by TARSKI:def 1;
            then
A9:         IC s2 = (IC SCM+FSA .--> (IC s1 + k)).IC SCM+FSA by FUNCT_4:13
              .= (IC s1 + k) by FUNCOP_1:72;
            thus IC Exec(IncAddr(I,j),s1) + k
              = succ IC s1 + k by A7,A6,SCMFSA_2:70
             .= succ IC s2 by A9
             .= IC Exec(IncAddr(I,j+k), s2) by A8,A6,A4,SCMFSA_2:70;
          end;
      end;
      suppose
        ex i1,a st I = a>0_goto i1;
        then consider i1,a such that
A10:    I = a>0_goto i1;
        let j,k be natural number, s1 be State of SCM+FSA;
        set s2 = IncIC(s1,k);
        a <> IC SCM+FSA & dom (IC SCM+FSA .--> (IC s1 + k)) = {IC SCM+FSA}
        by FUNCOP_1:13,SCMFSA_2:56;
        then not a in dom (IC SCM+FSA .--> (IC s1 + k)) by TARSKI:def 1;
        then
A11:    s1.a = s2.a by FUNCT_4:11;
          per cases;
          suppose
A12:        s1.a > 0;
            thus IC Exec(IncAddr(I,j),s1) + k
         = IC Exec(a>0_goto(j+i1),s1) + k by A10,Th90
        .= j+i1+k by A12,SCMFSA_2:71
        .= IC Exec(a>0_goto(j+k+i1),s2) by A11,A12,SCMFSA_2:71
            .= IC Exec(IncAddr(I,j+k), s2) by A10,Th90;
          end;
          suppose
A13:         s1.a <= 0;
A14:         IncAddr(I,j) = a>0_goto(i1+j) by A10,Th90;
A15:         IncAddr(I,j+k) = a>0_goto(i1+(j+k)) by A10,Th90;
            dom (IC SCM+FSA .--> (IC s1 + k)) = {IC SCM+FSA} by FUNCOP_1:13;
            then IC SCM+FSA in dom (IC SCM+FSA .--> (IC s1 + k))
             by TARSKI:def 1;
            then
A16:         IC s2 = (IC SCM+FSA .--> (IC s1 + k)).IC SCM+FSA by FUNCT_4:13
              .= (IC s1 + k) by FUNCOP_1:72;
            thus IC Exec(IncAddr(I,j),s1) + k
              = succ IC s1 + k by A14,A13,SCMFSA_2:71
             .= succ IC s2 by A16
             .= IC Exec(IncAddr(I,j+k), s2) by A15,A13,A11,SCMFSA_2:71;
          end;
      end;
      suppose
        ex a,b,f st I = b:=(f,a);
        hence thesis;
      end;
      suppose
        ex a,b,f st I = (f,a):=b;
        hence thesis;
      end;
      suppose
        ex a,f st I = a:=len f;
        hence thesis;
      end;
      suppose
        ex a,f st I = f:=<0,...,0>a;
        hence thesis;
      end;
  end;
end;

