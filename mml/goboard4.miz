A220:             Lma/.k2 = Lma.k2 by A130,PARTFUN1:def 8;
A221:             x in rng gg by A216,XBOOLE_0:def 4;
A222:             dom Col(G1,k2) = Seg len G1 by A197,FINSEQ_1:def 3
