A312:             g/.(m+n) in rng Col(G1,i2) by A307,A310,A311,PARTFUN2:4;
                  now
                    1<=n by A291,A305,FINSEQ_1:3;
                    then
A313:               m+1<=m+n by XREAL_1:9;
                    assume
                    i2=1;
                    then m+n<=m by A286,A297,A305,A312;
                    then m+1<=m by A313,XXREAL_0:2;
                    then 1<=m-m by XREAL_1:21;
