A87: f/.(len f1+len f2)=f2/.len f2 by FINSEQ_4:84;
A88: y2.(len f2)=q2`2 by A11,A69,A86,GOBOARD1:def 4;
A89: now
    let m;
    set s = y.m;
    assume
A90: m in dom f;
    then
A91: m<=len f by FINSEQ_3:27;
A92: 1<=m by A90,FINSEQ_3:27;
