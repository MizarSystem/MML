A301:           Seg len t = dom t by FINSEQ_1:def 3;
                reconsider t as FinSequence of TOP-REAL 2;
A302:           width D = k by A3,A31,A92,GOBOARD1:26,NAT_1:3;
A303:           Indices D = [:dom D,Seg width D:] by MATRIX_1:def 5;
A304:           now
                  let n;
                  assume
A305:             n in dom t;
