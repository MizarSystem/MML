A189:           l2 in Seg l2 by FINSEQ_1:3;
A190:           len h2 in dom h2 by A132,A188,FINSEQ_3:27;
A191:           len Line(G1,len G1)=width G1 by MATRIX_1:def 8;
                then
A192:           k2 in dom Ll by A133,FINSEQ_1:def 3;
