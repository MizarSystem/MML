A21: x1.1=p1`1 by A12,A14,GOBOARD1:def 3;
A22: now
    let m;
    set s = x.m;
    assume
A23: m in dom f;
    then
A24: m<=len f by FINSEQ_3:27;
A25: 1<=m by A23,FINSEQ_3:27;
    now
