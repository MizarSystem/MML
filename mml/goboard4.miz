A13: 1<=len f1 by A1,XXREAL_0:2;
  then
A14: 1 in dom f1 by FINSEQ_3:27;
  then
A15: f/.1=f1/.1 by FINSEQ_4:83;
A16: Seg len f=dom f by FINSEQ_1:def 3;
