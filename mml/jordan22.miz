begin :: Properties of the Approximations

 reserve C for Simple_closed_curve,
         i for Nat;

Lm1: TOP-REAL 2 = TopSpaceMetr Euclid 2 by EUCLID:def 8;
Lm3:
  for r being real number, X being Subset of TOP-REAL 2 st r in proj2.:X
    ex x being Point of TOP-REAL 2 st x in X & proj2.x = r
    proof
      let r be real number,
          X be Subset of TOP-REAL 2;
      assume r in proj2.:X;
      then ex x being set st x in the carrier of TOP-REAL 2 &
       x in X & proj2.x = r by FUNCT_2:115;
      hence thesis;
    end;

Lm4:
  for n being real number holds n+1+1 <> n
  proof
    let n be real number;
    assume n+1+1 = n;
    then n+(1+1) = n+0 by XCMPLX_1:1;
    hence contradiction by REAL_1:53;
  end;

theorem Nega:
  (Upper_Appr C).i c= Cl RightComp Cage(C,0)
proof
XX: (Upper_Appr C).i = Upper_Arc L~Cage (C,i) by JORDAN19:def 1;
XY: Upper_Arc L~Cage (C,i) c= L~Cage (C,i) by JORDAN1A:16;
  0 <= i by NAT_1:18; then
  L~Cage (C,i) c= Cl RightComp Cage(C,0) by JORDAN1H:54;
  hence thesis by XX,XY,XBOOLE_1:1;
end;

theorem Nega2:
  (Lower_Appr C).i c= Cl RightComp Cage(C,0)
proof
XX: (Lower_Appr C).i = Lower_Arc L~Cage (C,i) by JORDAN19:def 2;
XY: Lower_Arc L~Cage (C,i) c= L~Cage (C,i) by JORDAN1A:16;
  0 <= i by NAT_1:18; then
  L~Cage (C,i) c= Cl RightComp Cage(C,0) by JORDAN1H:54;
  hence thesis by XX,XY,XBOOLE_1:1;
end;

registration
  let C be Simple_closed_curve;
  cluster Upper_Arc C -> connected;
coherence
proof
  Upper_Arc C is_an_arc_of W-min C,E-max C by JORDAN6:def 8;
  hence thesis by JORDAN6:11;
end;
  cluster Lower_Arc C -> connected;
coherence
proof
  Lower_Arc C is_an_arc_of E-max C,W-min C by JORDAN6:def 9;
  hence thesis by JORDAN6:11;
end;
end;

theorem
  (Upper_Appr C).i is compact connected
proof
  Upper_Arc L~Cage(C,i) is compact;
  hence (Upper_Appr C).i is compact by JORDAN19:def 1;
  Upper_Arc L~Cage(C,i) is connected;
  hence (Upper_Appr C).i is connected by JORDAN19:def 1;
end;

theorem
  (Lower_Appr C).i is compact connected
proof
  Lower_Arc L~Cage(C,i) is compact;
  hence (Lower_Appr C).i is compact by JORDAN19:def 2;
  Lower_Arc L~Cage(C,i) is connected;
  hence (Lower_Appr C).i is connected by JORDAN19:def 2;
end;

registration
  let C be Simple_closed_curve;
  cluster North_Arc C -> compact;
  coherence
  proof
AB: Cl RightComp Cage(C,0) is Bounded by JORDAN2C:73;
    for i being Nat holds (Upper_Appr C).i c= Cl RightComp Cage(C,0) by Nega;
    then Lim_inf Upper_Appr C is compact by AB,KURATO_2:63;
   hence thesis by JORDAN19:def 3;
  end;
  cluster South_Arc C -> compact;
  coherence
  proof
AB: Cl RightComp Cage(C,0) is Bounded by JORDAN2C:73;
    for i being Nat holds (Lower_Appr C).i c= Cl RightComp Cage(C,0) by Nega2;
    then Lim_inf Lower_Appr C is compact by AB,KURATO_2:63;
   hence thesis by JORDAN19:def 4;
  end;
end;

begin :: On Special Points of Approximations

theorem
  W-min C in North_Arc C
  proof
A1: North_Arc C = Lim_inf Upper_Appr C by JORDAN19:def 3;
    reconsider w = W-min C as Point of Euclid 2 by Lm1,TOPMETR:16;
    for r being real number st r > 0
      ex k being Nat st
     for m being Nat st m > k holds (Upper_Appr C).m meets Ball(w,r)
    proof
      let r be real number;
      assume r > 0;
      then r/2 > 0 by SEQ_2:3;
      then consider k being Nat such that
        1 < k and
A2:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < r/2 and
A3:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < r/2 by GOBRD14:21;
      take k;
      let m be Nat such that
A5:     m > k;
      reconsider p = W-min L~Cage(C,m) as Point of Euclid 2 by Lm1,TOPMETR:16;
A6:   (Upper_Appr C).m = Upper_Arc L~Cage(C,m) by JORDAN19:def 1;
A7:   W-min L~Cage(C,m) in Upper_Arc L~Cage(C,m) by JORDAN7:1;
A8:   W-min L~Cage(C,m) in rng Cage(C,m) by SPRECT_2:47;
      len Cage(C,m) > 4 by GOBOARD7:36;
      then
A9:   1+1 <= len Cage(C,m) by AXIOMS:22;
A10:   W-min L~Cage(C,m) = Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
        by A8,FINSEQ_6:98;
A11:   1+1 <= len Rotate(Cage(C,m),W-min L~Cage(C,m)) by A9,REVROT_1:14; then
A12:   Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
        in LSeg(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) by TOPREAL1:27;
      left_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) /\
        right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) =
        LSeg(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
          by A11,GOBOARD5:32;
      then
A13:   W-min L~Cage(C,m) in right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
        by A10,A12,XBOOLE_0:def 3;
A14:   W-min C in right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
        by JORDAN1I:8;
A15:   right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
       = right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,
                GoB Rotate(Cage(C,m),W-min L~Cage(C,m))) by A11,JORDAN1H:29
      .= right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,GoB Cage(C,m))
         by REVROT_1:28
      .= right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,Gauge(C,m))
         by JORDAN1H:52;
      Cage(C,m) is_sequence_on Gauge(C,m) by JORDAN9:def 1;
      then
A16:   Rotate(Cage(C,m),W-min L~Cage(C,m)) is_sequence_on Gauge(C,m)
       by REVROT_1:34;
      Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
       = W-min L~Rotate(Cage(C,m),W-min L~Cage(C,m)) by A10,REVROT_1:33;
      then
      consider i, j being Nat such that
A17:     [i,j] in Indices Gauge(C,m) &
        [i,j+1] in Indices Gauge(C,m) and
A18:     Rotate(Cage(C,m),W-min L~Cage(C,m))/.1 = Gauge(C,m)*(i,j) &
        Rotate(Cage(C,m),W-min L~Cage(C,m))/.(1+1) = Gauge(C,m)*(i,j+1)
          by A16,A11,JORDAN1I:23;
A19:   right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
       = cell(Gauge(C,m),i,j) by A16,A11,A15,A17,A18,GOBRD13:23;
A20:   1 <= i & 1 <= j & j+1 <= width Gauge(C,m) by A17,GOBOARD5:1;
A21:   1 <= i+1 by NAT_1:29;
A22:   j <= width Gauge(C,m) by A17,GOBOARD5:1;
      i < len Gauge(C,m) by A16,A17,A11,A18,JORDAN1I:16;
      then
A23:   i+1 <= len Gauge(C,m) by NAT_1:38;
      then
A24:   [i+1,j] in Indices Gauge(C,m) by A20,A22,A21,GOBOARD7:10;
      Gauge(C,m)*(i,j)`1 <= (W-min C)`1 &
        (W-min C)`1 <= Gauge(C,m)*(i+1,j)`1 &
        Gauge(C,m)*(i,j)`2 <= (W-min C)`2 &
        (W-min C)`2 <= Gauge(C,m)*(i,j+1)`2 &
        Gauge(C,m)*(i,j)`1 <= (W-min L~Cage(C,m))`1 &
        (W-min L~Cage(C,m))`1 <= Gauge(C,m)*(i+1,j)`1 &
        Gauge(C,m)*(i,j)`2 <= (W-min L~Cage(C,m))`2 &
        (W-min L~Cage(C,m))`2 <= Gauge(C,m)*(i,j+1)`2
          by A14,A13,A19,A23,A20,JORDAN9:19;
      then
A25:   dist(W-min C,W-min L~Cage(C,m)) <=
       (Gauge(C,m)*(i+1,j)`1-Gauge(C,m)*(i,j)`1) +
       (Gauge(C,m)*(i,j+1)`2-Gauge(C,m)*(i,j)`2) by GOBRD14:12;
A26:   [1,1] in Indices Gauge(C,m) by JORDAN21:14;
A27:   [1,1+1] in Indices Gauge(C,m) by JORDAN21:15;
A28:   [1+1,1] in Indices Gauge(C,m) by JORDAN21:16;
A29:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
          = (E-bound C - W-bound C)/2|^m by A26,A28,GOBRD14:20;
A30:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
          = (N-bound C - S-bound C)/2|^m by A26,A27,GOBRD14:19;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) by A5,JORDAN21:18;
      then
A31:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) < r/2 by A2,AXIOMS:22;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) by A5,JORDAN21:20;
      then
A32:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) < r/2 by A3,AXIOMS:22;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
         = dist(Gauge(C,m)*(i,j),Gauge(C,m)*(i,j+1)) by A17,A30,GOBRD14:19;
      then
A33:   Gauge(C,m)*(i,j+1)`2 - Gauge(C,m)*(i,j)`2 < r/2 by A17,A31,GOBRD14:16;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
         = dist(Gauge(C,m)*(i,j),Gauge(C,m)*(i+1,j)) by A17,A24,A29,GOBRD14:20;
      then
      Gauge(C,m)*(i+1,j)`1 - Gauge(C,m)*(i,j)`1 < r/2 by A17,A24,A32
,GOBRD14:15;
      then
      (Gauge(C,m)*(i+1,j)`1-Gauge(C,m)*(i,j)`1) +
       (Gauge(C,m)*(i,j+1)`2-Gauge(C,m)*(i,j)`2) < r/2 + r/2
        by A33,REAL_1:67;
      then dist(W-min C,W-min L~Cage(C,m)) < r by A25,AXIOMS:22;
      then dist(w,p) < r by GOBRD14:def 1;
      then p in Ball(w,r) by METRIC_1:12;
      hence (Upper_Appr C).m meets Ball(w,r) by A6,A7,XBOOLE_0:3;
    end;
    hence W-min C in North_Arc C by A1,KURATO_2:50;
  end;

theorem
  E-max C in North_Arc C
  proof
A1: North_Arc C = Lim_inf Upper_Appr C by JORDAN19:def 3;
    reconsider w = E-max C as Point of Euclid 2 by Lm1,TOPMETR:16;
    for r being real number st r > 0
      ex k being Nat st
     for m being Nat st m > k holds (Upper_Appr C).m meets Ball(w,r)
    proof
      let r be real number;
      assume r > 0;
      then r/2 > 0 by SEQ_2:3;
      then consider k being Nat such that
        1 < k and
A2:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < r/2 and
A3:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < r/2 by GOBRD14:21;
      take k;
      let m be Nat such that
A5:     m > k;
      reconsider p = E-max L~Cage(C,m) as Point of Euclid 2 by Lm1,TOPMETR:16;
A6:   (Upper_Appr C).m = Upper_Arc L~Cage(C,m) by JORDAN19:def 1;
A7:   E-max L~Cage(C,m) in Upper_Arc L~Cage(C,m) by JORDAN7:1;
A8:   E-max L~Cage(C,m) in rng Cage(C,m) by SPRECT_2:50;
      len Cage(C,m) > 4 by GOBOARD7:36;
      then
A9:   1+1 <= len Cage(C,m) by AXIOMS:22;
A10:   E-max L~Cage(C,m) = Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
        by A8,FINSEQ_6:98;
A11:   1+1 <= len Rotate(Cage(C,m),E-max L~Cage(C,m)) by A9,REVROT_1:14; then
A12:   Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
        in LSeg(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) by TOPREAL1:27;
      left_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) /\
        right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) =
        LSeg(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
          by A11,GOBOARD5:32;
      then
A13:   E-max L~Cage(C,m) in right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
        by A10,A12,XBOOLE_0:def 3;
A14:   E-max C in right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
        by JORDAN1I:9;
A15:   right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
       = right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1,
                GoB Rotate(Cage(C,m),E-max L~Cage(C,m))) by A11,JORDAN1H:29
      .= right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1,GoB Cage(C,m))
         by REVROT_1:28
      .= right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1,Gauge(C,m))
         by JORDAN1H:52;
      Cage(C,m) is_sequence_on Gauge(C,m) by JORDAN9:def 1;
      then
A16:   Rotate(Cage(C,m),E-max L~Cage(C,m)) is_sequence_on Gauge(C,m)
       by REVROT_1:34;
      Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
       = E-max L~Rotate(Cage(C,m),E-max L~Cage(C,m)) by A10,REVROT_1:33;
      then
      consider i, j being Nat such that
A17:     [i,j+1] in Indices Gauge(C,m) &
        [i,j] in Indices Gauge(C,m) and
A18:     Rotate(Cage(C,m),E-max L~Cage(C,m))/.1 = Gauge(C,m)*(i,j+1) &
        Rotate(Cage(C,m),E-max L~Cage(C,m))/.(1+1) = Gauge(C,m)*(i,j)
          by A16,A11,JORDAN1I:25;
A19:   right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
       = cell(Gauge(C,m),i-'1,j) by A16,A11,A15,A17,A18,GOBRD13:29;
A20:   1 <= i & 1 <= j & j+1 <= width Gauge(C,m) by A17,GOBOARD5:1;
      i > 1 by A16,A17,A11,A18,JORDAN1I:18;
      then
A21:   i - 1 > 1 - 1 by REAL_1:92;
      then i - 1 is Nat by INT_1:16;
      then
A22:   i - 1 >= 0 + 1 by A21,NAT_1:38;
      then
A23:   i-'1+1 = i by JORDAN3:6;
A24:   i <= len Gauge(C,m) by A17,GOBOARD5:1;
A25:   i-'1 = i-1 by A21,BINARITH:def 3;
      i-1 <= i by INT_1:26;
      then
A26:   i-'1 <= len Gauge(C,m) & j <= width Gauge(C,m)
        by A17,A25,A24,AXIOMS:22,GOBOARD5:1;
      then
A27:   [i-'1,j] in Indices Gauge(C,m) by A20,A25,A22,GOBOARD7:10;
      1 <= j+1 by NAT_1:29;
      then
A28:   [i-'1,j+1] in Indices Gauge(C,m) by A20,A25,A22,A26,GOBOARD7:10;
      Gauge(C,m)*(i-'1,j)`1 <= (E-max C)`1 &
        (E-max C)`1 <= Gauge(C,m)*(i-'1+1,j)`1 &
        Gauge(C,m)*(i-'1,j)`2 <= (E-max C)`2 &
        (E-max C)`2 <= Gauge(C,m)*(i-'1,j+1)`2 &
        Gauge(C,m)*(i-'1,j)`1 <= (E-max L~Cage(C,m))`1 &
        (E-max L~Cage(C,m))`1 <= Gauge(C,m)*(i-'1+1,j)`1 &
        Gauge(C,m)*(i-'1,j)`2 <= (E-max L~Cage(C,m))`2 &
        (E-max L~Cage(C,m))`2 <= Gauge(C,m)*(i-'1,j+1)`2
          by A14,A13,A19,A20,A23,A22,A24,JORDAN9:19;
      then
A29:   dist(E-max C,E-max L~Cage(C,m)) <=
       (Gauge(C,m)*(i-'1+1,j)`1-Gauge(C,m)*(i-'1,j)`1) +
       (Gauge(C,m)*(i-'1,j+1)`2-Gauge(C,m)*(i-'1,j)`2) by GOBRD14:12;
A30:   [1,1] in Indices Gauge(C,m) by JORDAN21:14;
A31:   [1,1+1] in Indices Gauge(C,m) by JORDAN21:15;
A32:   [1+1,1] in Indices Gauge(C,m) by JORDAN21:16;
A33:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
          = (E-bound C - W-bound C)/2|^m by A30,A32,GOBRD14:20;
A34:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
          = (N-bound C - S-bound C)/2|^m by A30,A31,GOBRD14:19;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) by A5,JORDAN21:18;
      then
A35:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) < r/2 by A2,AXIOMS:22;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) by A5,JORDAN21:20;
      then
A36:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) < r/2 by A3,AXIOMS:22;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
         = dist(Gauge(C,m)*(i-'1,j),Gauge(C,m)*(i-'1,j+1))
           by A34,A27,A28,GOBRD14:19;
      then
A37:   Gauge(C,m)*(i-'1,j+1)`2 - Gauge(C,m)*(i-'1,j)`2 < r/2
        by A35,A27,A28,GOBRD14:16;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
         = dist(Gauge(C,m)*(i-'1,j),Gauge(C,m)*(i-'1+1,j))
           by A23,A17,A33,A27,GOBRD14:20;
      then
      Gauge(C,m)*(i-'1+1,j)`1 - Gauge(C,m)*(i-'1,j)`1 < r/2
        by A23,A17,A36,A27,GOBRD14:15;
      then
      (Gauge(C,m)*(i-'1+1,j)`1-Gauge(C,m)*(i-'1,j)`1) +
       (Gauge(C,m)*(i-'1,j+1)`2-Gauge(C,m)*(i-'1,j)`2) < r/2 + r/2
        by A37,REAL_1:67;
      then dist(E-max C,E-max L~Cage(C,m)) < r by A29,AXIOMS:22;
      then dist(w,p) < r by GOBRD14:def 1;
      then p in Ball(w,r) by METRIC_1:12;
      hence (Upper_Appr C).m meets Ball(w,r) by A6,A7,XBOOLE_0:3;
    end;
    hence E-max C in North_Arc C by A1,KURATO_2:50;
  end;

theorem
  W-min C in South_Arc C
  proof
A1: South_Arc C = Lim_inf Lower_Appr C by JORDAN19:def 4;
    reconsider w = W-min C as Point of Euclid 2 by Lm1,TOPMETR:16;
    for r being real number st r > 0
      ex k being Nat st
     for m being Nat st m > k holds (Lower_Appr C).m meets Ball(w,r)
    proof
      let r be real number;
      assume r > 0;
      then r/2 > 0 by SEQ_2:3;
      then consider k being Nat such that
        1 < k and
A2:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < r/2 and
A3:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < r/2 by GOBRD14:21;
      take k;
      let m be Nat such that
A5:     m > k;
      reconsider p = W-min L~Cage(C,m) as Point of Euclid 2 by Lm1,TOPMETR:16;
A6:   (Lower_Appr C).m = Lower_Arc L~Cage(C,m) by JORDAN19:def 2;
A7:   W-min L~Cage(C,m) in Lower_Arc L~Cage(C,m) by JORDAN7:1;
A8:   W-min L~Cage(C,m) in rng Cage(C,m) by SPRECT_2:47;
      len Cage(C,m) > 4 by GOBOARD7:36;
      then
A9:   1+1 <= len Cage(C,m) by AXIOMS:22;
A10:   W-min L~Cage(C,m) = Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
        by A8,FINSEQ_6:98;
A11:   1+1 <= len Rotate(Cage(C,m),W-min L~Cage(C,m)) by A9,REVROT_1:14; then
A12:   Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
        in LSeg(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) by TOPREAL1:27;
      left_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) /\
        right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) =
        LSeg(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
          by A11,GOBOARD5:32;
      then
A13:   W-min L~Cage(C,m) in right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
        by A10,A12,XBOOLE_0:def 3;
A14:   W-min C in right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
        by JORDAN1I:8;
A15:   right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
       = right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,
                GoB Rotate(Cage(C,m),W-min L~Cage(C,m))) by A11,JORDAN1H:29
      .= right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,GoB Cage(C,m))
         by REVROT_1:28
      .= right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,Gauge(C,m))
         by JORDAN1H:52;
      Cage(C,m) is_sequence_on Gauge(C,m) by JORDAN9:def 1;
      then
A16:   Rotate(Cage(C,m),W-min L~Cage(C,m)) is_sequence_on Gauge(C,m)
       by REVROT_1:34;
      Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
       = W-min L~Rotate(Cage(C,m),W-min L~Cage(C,m)) by A10,REVROT_1:33;
      then
      consider i, j being Nat such that
A17:     [i,j] in Indices Gauge(C,m) &
        [i,j+1] in Indices Gauge(C,m) and
A18:     Rotate(Cage(C,m),W-min L~Cage(C,m))/.1 = Gauge(C,m)*(i,j) &
        Rotate(Cage(C,m),W-min L~Cage(C,m))/.(1+1) = Gauge(C,m)*(i,j+1)
          by A16,A11,JORDAN1I:23;
A19:   right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
       = cell(Gauge(C,m),i,j) by A16,A11,A15,A17,A18,GOBRD13:23;
A20:   1 <= i & 1 <= j & j+1 <= width Gauge(C,m) by A17,GOBOARD5:1;
A21:   1 <= i+1 by NAT_1:29;
A22:   j <= width Gauge(C,m) by A17,GOBOARD5:1;
      i < len Gauge(C,m) by A16,A17,A11,A18,JORDAN1I:16;
      then
A23:   i+1 <= len Gauge(C,m) by NAT_1:38;
      then
A24:   [i+1,j] in Indices Gauge(C,m) by A20,A22,A21,GOBOARD7:10;
      Gauge(C,m)*(i,j)`1 <= (W-min C)`1 &
        (W-min C)`1 <= Gauge(C,m)*(i+1,j)`1 &
        Gauge(C,m)*(i,j)`2 <= (W-min C)`2 &
        (W-min C)`2 <= Gauge(C,m)*(i,j+1)`2 &
        Gauge(C,m)*(i,j)`1 <= (W-min L~Cage(C,m))`1 &
        (W-min L~Cage(C,m))`1 <= Gauge(C,m)*(i+1,j)`1 &
        Gauge(C,m)*(i,j)`2 <= (W-min L~Cage(C,m))`2 &
        (W-min L~Cage(C,m))`2 <= Gauge(C,m)*(i,j+1)`2
          by A14,A13,A19,A23,A20,JORDAN9:19;
      then
A25:   dist(W-min C,W-min L~Cage(C,m)) <=
       (Gauge(C,m)*(i+1,j)`1-Gauge(C,m)*(i,j)`1) +
       (Gauge(C,m)*(i,j+1)`2-Gauge(C,m)*(i,j)`2) by GOBRD14:12;
A26:   [1,1] in Indices Gauge(C,m) by JORDAN21:14;
A27:   [1,1+1] in Indices Gauge(C,m) by JORDAN21:15;
A28:   [1+1,1] in Indices Gauge(C,m) by JORDAN21:16;
A29:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
          = (E-bound C - W-bound C)/2|^m by A26,A28,GOBRD14:20;
A30:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
          = (N-bound C - S-bound C)/2|^m by A26,A27,GOBRD14:19;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) by A5,JORDAN21:18;
      then
A31:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) < r/2 by A2,AXIOMS:22;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) by A5,JORDAN21:20;
      then
A32:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) < r/2 by A3,AXIOMS:22;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
         = dist(Gauge(C,m)*(i,j),Gauge(C,m)*(i,j+1)) by A17,A30,GOBRD14:19;
      then
A33:   Gauge(C,m)*(i,j+1)`2 - Gauge(C,m)*(i,j)`2 < r/2 by A17,A31,GOBRD14:16;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
         = dist(Gauge(C,m)*(i,j),Gauge(C,m)*(i+1,j)) by A17,A24,A29,GOBRD14:20;
      then
      Gauge(C,m)*(i+1,j)`1 - Gauge(C,m)*(i,j)`1 < r/2 by A17,A24,A32
,GOBRD14:15;
      then
      (Gauge(C,m)*(i+1,j)`1-Gauge(C,m)*(i,j)`1) +
       (Gauge(C,m)*(i,j+1)`2-Gauge(C,m)*(i,j)`2) < r/2 + r/2
        by A33,REAL_1:67;
      then dist(W-min C,W-min L~Cage(C,m)) < r by A25,AXIOMS:22;
      then dist(w,p) < r by GOBRD14:def 1;
      then p in Ball(w,r) by METRIC_1:12;
      hence (Lower_Appr C).m meets Ball(w,r) by A6,A7,XBOOLE_0:3;
    end;
    hence W-min C in South_Arc C by A1,KURATO_2:50;
  end;

theorem
  E-max C in South_Arc C
  proof
A1: South_Arc C = Lim_inf Lower_Appr C by JORDAN19:def 4;
    reconsider w = E-max C as Point of Euclid 2 by Lm1,TOPMETR:16;
    for r being real number st r > 0
      ex k being Nat st
     for m being Nat st m > k holds (Lower_Appr C).m meets Ball(w,r)
    proof
      let r be real number;
      assume r > 0;
      then r/2 > 0 by SEQ_2:3;
      then consider k being Nat such that
        1 < k and
A2:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < r/2 and
A3:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < r/2 by GOBRD14:21;
      take k;
      let m be Nat such that
A5:     m > k;
      reconsider p = E-max L~Cage(C,m) as Point of Euclid 2 by Lm1,TOPMETR:16;
A6:   (Lower_Appr C).m = Lower_Arc L~Cage(C,m) by JORDAN19:def 2;
A7:   E-max L~Cage(C,m) in Lower_Arc L~Cage(C,m) by JORDAN7:1;
A8:   E-max L~Cage(C,m) in rng Cage(C,m) by SPRECT_2:50;
      len Cage(C,m) > 4 by GOBOARD7:36;
      then
A9:   1+1 <= len Cage(C,m) by AXIOMS:22;
A10:   E-max L~Cage(C,m) = Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
        by A8,FINSEQ_6:98;
A11:   1+1 <= len Rotate(Cage(C,m),E-max L~Cage(C,m)) by A9,REVROT_1:14; then
A12:   Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
        in LSeg(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) by TOPREAL1:27;
      left_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) /\
