    hence thesis by Th18;
  end;

theorem Th27:
  i <= j implies (UMP L~Cage(C,j))`2 <= (UMP L~Cage(C,i))`2
  proof
    assume
A1:   i <= j;
    set w = (E-bound C + W-bound C) / 2,
        ui = UMP L~Cage(C,i),
        uj = UMP L~Cage(C,j);
A2: W-bound L~Cage(C,j) + E-bound L~Cage(C,j) =
     W-bound C + E-bound C by JORDAN1G:41;
    then
A3: uj`2 = sup (proj2.:(L~Cage(C,j) /\ Vertical_Line w)) by EUCLID:56;
A4: W-bound L~Cage(C,i) + E-bound L~Cage(C,i) =
     W-bound C + E-bound C by JORDAN1G:41;
    then
A5: ui`2 = sup (proj2.:(L~Cage(C,i) /\ Vertical_Line w)) by EUCLID:56;
A6: proj2.:(L~Cage(C,j) /\ Vertical_Line w) is non empty bounded_above
      by A2,JORDAN21:12,13;
A7: proj2.:(L~Cage(C,i) /\ Vertical_Line w) is non empty bounded_above
      by A4,JORDAN21:12,13;
    assume (UMP L~Cage(C,j))`2 > (UMP L~Cage(C,i))`2;
    then uj`2 - ui`2 > 0 by XREAL_1:52;
    then consider r being real number such that
A8:   r in proj2.:(L~Cage(C,j) /\ Vertical_Line w) and
A9:   sup (proj2.:(L~Cage(C,j) /\ Vertical_Line w)) - (uj`2 - ui`2) < r
        by A6,SEQ_4:def 4;
    consider x being Point of TOP-REAL 2 such that
A10:   x in L~Cage(C,j) /\ Vertical_Line w and
A11:   proj2.x = r by A8,Lm1;
A12: LeftComp(Cage(C,i)) c= LeftComp(Cage(C,j)) by A1,JORDAN1H:55;
A13: x`2 = r by A11,PSCOMP_1:def 29;
    north_halfline x misses L~Cage(C,i)
    proof
      assume north_halfline x meets L~Cage(C,i);
      then consider y being set such that
A14:     y in north_halfline x and
A15:     y in L~Cage(C,i) by XBOOLE_0:3;
      reconsider y as Point of TOP-REAL 2 by A15;
A16:   y`2 >= x`2 by A14,TOPREAL1:def 12;
A17:   x in Vertical_Line w by A10,XBOOLE_0:def 3;
      y`1 = x`1 by A14,TOPREAL1:def 12
         .= w by A17,JORDAN6:34;
      then y in Vertical_Line w;
      then
A18:   y in L~Cage(C,i) /\ Vertical_Line w by A15,XBOOLE_0:def 3;
      proj2.y = y`2 by PSCOMP_1:def 29;
      then y`2 in proj2.:(L~Cage(C,i) /\ Vertical_Line w) by A18,FUNCT_2:43;
      then y`2 <= ui`2 by A5,A7,SEQ_4:def 4;
      hence contradiction by A3,A9,A13,A16,XXREAL_0:2;
    end;
    then
A19: north_halfline x c= UBD L~Cage(C,i) by JORDAN2C:137;
    x in north_halfline x by TOPREAL1:45;
    then x in UBD L~Cage(C,i) by A19;
    then
A20: x in LeftComp Cage(C,i) by GOBRD14:46;
    x in L~Cage(C,j) by A10,XBOOLE_0:def 3;
    then LeftComp Cage(C,j) meets L~Cage(C,j) by A12,A20,XBOOLE_0:3;
    hence thesis by SPRECT_3:43;
  end;

theorem Th28:
  i <= j implies (LMP L~Cage(C,i))`2 <= (LMP L~Cage(C,j))`2
  proof
    assume
A1:   i <= j;
    set w = (E-bound C + W-bound C) / 2,
        ui = LMP L~Cage(C,i),
        uj = LMP L~Cage(C,j);
A2: W-bound L~Cage(C,j) + E-bound L~Cage(C,j) =
     W-bound C + E-bound C by JORDAN1G:41;
    then
A3: uj`2 = inf (proj2.:(L~Cage(C,j) /\ Vertical_Line w)) by EUCLID:56;
A4: W-bound L~Cage(C,i) + E-bound L~Cage(C,i) =
     W-bound C + E-bound C by JORDAN1G:41;
    then
A5: ui`2 = inf (proj2.:(L~Cage(C,i) /\ Vertical_Line w)) by EUCLID:56;
A6: proj2.:(L~Cage(C,j) /\ Vertical_Line w) is non empty bounded_below
      by A2,JORDAN21:12,13;
A7: proj2.:(L~Cage(C,i) /\ Vertical_Line w) is non empty bounded_below
      by A4,JORDAN21:12,13;
    assume (LMP L~Cage(C,i))`2 > (LMP L~Cage(C,j))`2;
    then ui`2 - uj`2 > 0 by XREAL_1:52;
    then consider r being real number such that
A8:   r in proj2.:(L~Cage(C,j) /\ Vertical_Line w) and
A9:   r < inf (proj2.:(L~Cage(C,j) /\ Vertical_Line w)) + (ui`2 - uj`2)
        by A6,SEQ_4:def 5;
    consider x being Point of TOP-REAL 2 such that
A10:   x in L~Cage(C,j) /\ Vertical_Line w and
A11:   proj2.x = r by A8,Lm1;
A12: LeftComp(Cage(C,i)) c= LeftComp(Cage(C,j)) by A1,JORDAN1H:55;
A13: x`2 = r by A11,PSCOMP_1:def 29;
    south_halfline x misses L~Cage(C,i)
    proof
      assume south_halfline x meets L~Cage(C,i);
      then consider y being set such that
A14:     y in south_halfline x and
A15:     y in L~Cage(C,i) by XBOOLE_0:3;
      reconsider y as Point of TOP-REAL 2 by A15;
A16:   y`2 <= x`2 by A14,TOPREAL1:def 14;
A17:   x in Vertical_Line w by A10,XBOOLE_0:def 3;
      y`1 = x`1 by A14,TOPREAL1:def 14
         .= w by A17,JORDAN6:34;
      then y in Vertical_Line w;
      then
A18:   y in L~Cage(C,i) /\ Vertical_Line w by A15,XBOOLE_0:def 3;
      proj2.y = y`2 by PSCOMP_1:def 29;
      then y`2 in proj2.:(L~Cage(C,i) /\ Vertical_Line w) by A18,FUNCT_2:43;
      then y`2 >= ui`2 by A5,A7,SEQ_4:def 5;
      hence contradiction by A3,A9,A13,A16,XXREAL_0:2;
    end;
    then
A19: south_halfline x c= UBD L~Cage(C,i) by JORDAN2C:136;
    x in south_halfline x by TOPREAL1:45;
    then x in UBD L~Cage(C,i) by A19;
    then
A20: x in LeftComp Cage(C,i) by GOBRD14:46;
    x in L~Cage(C,j) by A10,XBOOLE_0:def 3;
    then LeftComp Cage(C,j) meets L~Cage(C,j) by A12,A20,XBOOLE_0:3;
    hence thesis by SPRECT_3:43;
  end;

theorem Th29:
  0 < i & i <= j implies
  (UMP Upper_Arc L~Cage(C,j))`2 <= (UMP Upper_Arc L~Cage(C,i))`2
  proof
    assume that
A1:   0 < i and
A2:   i <= j;
      (UMP Upper_Arc L~Cage(C,i))`2 = (UMP L~Cage(C,i))`2 &
      (UMP Upper_Arc L~Cage(C,j))`2 = (UMP L~Cage(C,j))`2 by A1,A2,Th23;
    hence thesis by A2,Th27;
  end;

theorem Th30:
  0 < i & i <= j implies
  (LMP Lower_Arc L~Cage(C,i))`2 <= (LMP Lower_Arc L~Cage(C,j))`2
  proof
    assume that
A1:   0 < i and
A2:   i <= j;
      (LMP Lower_Arc L~Cage(C,i))`2 = (LMP L~Cage(C,i))`2 &
      (LMP Lower_Arc L~Cage(C,j))`2 = (LMP L~Cage(C,j))`2 by A1,A2,Th24;
    hence thesis by A2,Th28;
  end;

begin :: On Special Points of Approximations

theorem Th31:
  W-min C in North_Arc C
  proof
A1: North_Arc C = Lim_inf Upper_Appr C by JORDAN19:def 3;
    reconsider w = W-min C as Point of Euclid 2 by TOPMETR:16;
    for r being real number st r > 0
      ex k being Element of NAT st
     for m being Element of NAT st m > k holds (Upper_Appr C).m meets Ball(w,r)
    proof
      let r be real number;
      assume r > 0;
      then r/2 > 0 by SEQ_2:3;
      then consider k being Element of NAT such that
        1 < k and
A2:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < r/2 and
A3:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < r/2 by GOBRD14:21;
      take k;
      let m be Element of NAT such that
A4:     m > k;
      reconsider p = W-min L~Cage(C,m) as Point of Euclid 2 by TOPMETR:16;
A5:   (Upper_Appr C).m = Upper_Arc L~Cage(C,m) by JORDAN19:def 1;
A6:   W-min L~Cage(C,m) in Upper_Arc L~Cage(C,m) by JORDAN7:1;
A7:   W-min L~Cage(C,m) in rng Cage(C,m) by SPRECT_2:47;
      len Cage(C,m) > 4 by GOBOARD7:36;
      then
A8:   1+1 <= len Cage(C,m) by XXREAL_0:2;
A9:   W-min L~Cage(C,m) = Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
        by A7,FINSEQ_6:98;
A10:   1+1 <= len Rotate(Cage(C,m),W-min L~Cage(C,m)) by A8,REVROT_1:14; then
A11:   Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
        in LSeg(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) by TOPREAL1:27;
      left_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) /\
        right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) =
        LSeg(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
          by A10,GOBOARD5:32;
      then
A12:   W-min L~Cage(C,m) in right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
        by A9,A11,XBOOLE_0:def 3;
A13:   W-min C in right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
        by JORDAN1I:8;
A14:   right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
       = right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,
                GoB Rotate(Cage(C,m),W-min L~Cage(C,m))) by A10,JORDAN1H:29
      .= right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,GoB Cage(C,m))
         by REVROT_1:28
      .= right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,Gauge(C,m))
         by JORDAN1H:52;
      Cage(C,m) is_sequence_on Gauge(C,m) by JORDAN9:def 1;
      then
A15:   Rotate(Cage(C,m),W-min L~Cage(C,m)) is_sequence_on Gauge(C,m)
       by REVROT_1:34;
      Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
       = W-min L~Rotate(Cage(C,m),W-min L~Cage(C,m)) by A9,REVROT_1:33;
      then
      consider i, j being Element of NAT such that
A16:     [i,j] in Indices Gauge(C,m) &
        [i,j+1] in Indices Gauge(C,m) and
A17:     Rotate(Cage(C,m),W-min L~Cage(C,m))/.1 = Gauge(C,m)*(i,j) &
        Rotate(Cage(C,m),W-min L~Cage(C,m))/.(1+1) = Gauge(C,m)*(i,j+1)
          by A10,A15,JORDAN1I:23;
A18:   right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
       = cell(Gauge(C,m),i,j) by A10,A14,A15,A16,A17,GOBRD13:23;
A19:   1 <= i & 1 <= j & j+1 <= width Gauge(C,m) by A16,GOBOARD5:1;
A20:   1 <= i+1 by NAT_1:11;
A21:   j <= width Gauge(C,m) by A16,GOBOARD5:1;
      i < len Gauge(C,m) by A10,A15,A16,A17,JORDAN1I:16;
      then
A22:   i+1 <= len Gauge(C,m) by NAT_1:13;
      then
A23:   [i+1,j] in Indices Gauge(C,m) by A19,A20,A21,GOBOARD7:10;
      Gauge(C,m)*(i,j)`1 <= (W-min C)`1 &
        (W-min C)`1 <= Gauge(C,m)*(i+1,j)`1 &
        Gauge(C,m)*(i,j)`2 <= (W-min C)`2 &
        (W-min C)`2 <= Gauge(C,m)*(i,j+1)`2 &
        Gauge(C,m)*(i,j)`1 <= (W-min L~Cage(C,m))`1 &
        (W-min L~Cage(C,m))`1 <= Gauge(C,m)*(i+1,j)`1 &
        Gauge(C,m)*(i,j)`2 <= (W-min L~Cage(C,m))`2 &
        (W-min L~Cage(C,m))`2 <= Gauge(C,m)*(i,j+1)`2
          by A12,A13,A18,A19,A22,JORDAN9:19;
      then
A24:   dist(W-min C,W-min L~Cage(C,m)) <=
       (Gauge(C,m)*(i+1,j)`1-Gauge(C,m)*(i,j)`1) +
       (Gauge(C,m)*(i,j+1)`2-Gauge(C,m)*(i,j)`2) by TOPREAL6:104;
A25:   [1,1] in Indices Gauge(C,m) by Th5;
A26:   [1,1+1] in Indices Gauge(C,m) by Th6;
A27:   [1+1,1] in Indices Gauge(C,m) by Th7;
A28:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
          = (E-bound C - W-bound C)/2|^m by A25,A27,GOBRD14:20;
A29:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
          = (N-bound C - S-bound C)/2|^m by A25,A26,GOBRD14:19;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) by A4,Th9;
      then
A30:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) < r/2 by A2,XXREAL_0:2;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) by A4,Th11;
      then
A31:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) < r/2 by A3,XXREAL_0:2;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
         = dist(Gauge(C,m)*(i,j),Gauge(C,m)*(i,j+1)) by A16,A29,GOBRD14:19;
      then
A32:   Gauge(C,m)*(i,j+1)`2 - Gauge(C,m)*(i,j)`2 < r/2 by A16,A30,GOBRD14:16;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
         = dist(Gauge(C,m)*(i,j),Gauge(C,m)*(i+1,j)) by A16,A23,A28,GOBRD14:20;
      then
      Gauge(C,m)*(i+1,j)`1 - Gauge(C,m)*(i,j)`1 < r/2 by A16,A23,A31,GOBRD14:15
;
      then
      (Gauge(C,m)*(i+1,j)`1-Gauge(C,m)*(i,j)`1) +
       (Gauge(C,m)*(i,j+1)`2-Gauge(C,m)*(i,j)`2) < r/2 + r/2
        by A32,XREAL_1:10;
      then dist(W-min C,W-min L~Cage(C,m)) < r by A24,XXREAL_0:2;
      then dist(w,p) < r by TOPREAL6:def 1;
      then p in Ball(w,r) by METRIC_1:12;
      hence (Upper_Appr C).m meets Ball(w,r) by A5,A6,XBOOLE_0:3;
    end;
    hence W-min C in North_Arc C by A1,KURATO_2:50;
  end;

theorem Th32:
  E-max C in North_Arc C
  proof
A1: North_Arc C = Lim_inf Upper_Appr C by JORDAN19:def 3;
    reconsider w = E-max C as Point of Euclid 2 by TOPMETR:16;
    for r being real number st r > 0
      ex k being Element of NAT st
     for m being Element of NAT st m > k holds (Upper_Appr C).m meets Ball(w,r)
    proof
      let r be real number;
      assume r > 0;
      then r/2 > 0 by SEQ_2:3;
      then consider k being Element of NAT such that
        1 < k and
A2:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < r/2 and
A3:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < r/2 by GOBRD14:21;
      take k;
      let m be Element of NAT such that
A4:     m > k;
      reconsider p = E-max L~Cage(C,m) as Point of Euclid 2 by TOPMETR:16;
A5:   (Upper_Appr C).m = Upper_Arc L~Cage(C,m) by JORDAN19:def 1;
A6:   E-max L~Cage(C,m) in Upper_Arc L~Cage(C,m) by JORDAN7:1;
A7:   E-max L~Cage(C,m) in rng Cage(C,m) by SPRECT_2:50;
      len Cage(C,m) > 4 by GOBOARD7:36;
      then
A8:   1+1 <= len Cage(C,m) by XXREAL_0:2;
A9:   E-max L~Cage(C,m) = Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
        by A7,FINSEQ_6:98;
A10:   1+1 <= len Rotate(Cage(C,m),E-max L~Cage(C,m)) by A8,REVROT_1:14; then
A11:   Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
        in LSeg(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) by TOPREAL1:27;
      left_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) /\
        right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) =
        LSeg(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
          by A10,GOBOARD5:32;
      then
A12:   E-max L~Cage(C,m) in right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
        by A9,A11,XBOOLE_0:def 3;
A13:   E-max C in right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
        by JORDAN1I:9;
A14:   right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
       = right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1,
                GoB Rotate(Cage(C,m),E-max L~Cage(C,m))) by A10,JORDAN1H:29
      .= right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1,GoB Cage(C,m))
         by REVROT_1:28
      .= right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1,Gauge(C,m))
         by JORDAN1H:52;
      Cage(C,m) is_sequence_on Gauge(C,m) by JORDAN9:def 1;
      then
A15:   Rotate(Cage(C,m),E-max L~Cage(C,m)) is_sequence_on Gauge(C,m)
       by REVROT_1:34;
      Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
       = E-max L~Rotate(Cage(C,m),E-max L~Cage(C,m)) by A9,REVROT_1:33;
      then
      consider i, j being Element of NAT such that
A16:     [i,j+1] in Indices Gauge(C,m) &
        [i,j] in Indices Gauge(C,m) and
A17:     Rotate(Cage(C,m),E-max L~Cage(C,m))/.1 = Gauge(C,m)*(i,j+1) &
        Rotate(Cage(C,m),E-max L~Cage(C,m))/.(1+1) = Gauge(C,m)*(i,j)
          by A10,A15,JORDAN1I:25;
A18:   right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
       = cell(Gauge(C,m),i-'1,j) by A10,A14,A15,A16,A17,GOBRD13:29;
A19:   1 <= i & 1 <= j & j+1 <= width Gauge(C,m) by A16,GOBOARD5:1;
      i > 1 by A10,A15,A16,A17,JORDAN1I:18;
      then
A20:   i - 1 > 1 - 1 by REAL_1:92;
      then i - 1 is Element of NAT by INT_1:16;
      then
A21:   i - 1 >= 0 + 1 by A20,NAT_1:13;
      then
A22:   i-'1+1 = i by BINARITH:61;
A23:   i <= len Gauge(C,m) by A16,GOBOARD5:1;
A24:   i-'1 = i-1 by A20,BINARITH:def 3;
      i-1 <= i by XREAL_1:148;
      then
A25:   i-'1 <= len Gauge(C,m) & j <= width Gauge(C,m)
        by A16,A23,A24,XXREAL_0:2,GOBOARD5:1;
      then
A26:   [i-'1,j] in Indices Gauge(C,m) by A19,A21,A24,GOBOARD7:10;
      1 <= j+1 by NAT_1:11;
      then
A27:   [i-'1,j+1] in Indices Gauge(C,m) by A19,A21,A24,A25,GOBOARD7:10;
      Gauge(C,m)*(i-'1,j)`1 <= (E-max C)`1 &
        (E-max C)`1 <= Gauge(C,m)*(i-'1+1,j)`1 &
        Gauge(C,m)*(i-'1,j)`2 <= (E-max C)`2 &
        (E-max C)`2 <= Gauge(C,m)*(i-'1,j+1)`2 &
        Gauge(C,m)*(i-'1,j)`1 <= (E-max L~Cage(C,m))`1 &
        (E-max L~Cage(C,m))`1 <= Gauge(C,m)*(i-'1+1,j)`1 &
        Gauge(C,m)*(i-'1,j)`2 <= (E-max L~Cage(C,m))`2 &
        (E-max L~Cage(C,m))`2 <= Gauge(C,m)*(i-'1,j+1)`2
          by A12,A13,A18,A19,A21,A22,A23,JORDAN9:19;
      then
A28:   dist(E-max C,E-max L~Cage(C,m)) <=
       (Gauge(C,m)*(i-'1+1,j)`1-Gauge(C,m)*(i-'1,j)`1) +
       (Gauge(C,m)*(i-'1,j+1)`2-Gauge(C,m)*(i-'1,j)`2) by TOPREAL6:104;
A29:   [1,1] in Indices Gauge(C,m) by Th5;
A30:   [1,1+1] in Indices Gauge(C,m) by Th6;
A31:   [1+1,1] in Indices Gauge(C,m) by Th7;
A32:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
          = (E-bound C - W-bound C)/2|^m by A29,A31,GOBRD14:20;
A33:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
          = (N-bound C - S-bound C)/2|^m by A29,A30,GOBRD14:19;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) by A4,Th9;
      then
A34:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) < r/2 by A2,XXREAL_0:2;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) by A4,Th11;
      then
A35:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) < r/2 by A3,XXREAL_0:2;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
         = dist(Gauge(C,m)*(i-'1,j),Gauge(C,m)*(i-'1,j+1))
           by A26,A27,A33,GOBRD14:19;
      then
A36:   Gauge(C,m)*(i-'1,j+1)`2 - Gauge(C,m)*(i-'1,j)`2 < r/2
        by A26,A27,A34,GOBRD14:16;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
         = dist(Gauge(C,m)*(i-'1,j),Gauge(C,m)*(i-'1+1,j))
           by A16,A22,A26,A32,GOBRD14:20;
      then
      Gauge(C,m)*(i-'1+1,j)`1 - Gauge(C,m)*(i-'1,j)`1 < r/2
        by A16,A22,A26,A35,GOBRD14:15;
      then
      (Gauge(C,m)*(i-'1+1,j)`1-Gauge(C,m)*(i-'1,j)`1) +
       (Gauge(C,m)*(i-'1,j+1)`2-Gauge(C,m)*(i-'1,j)`2) < r/2 + r/2
        by A36,XREAL_1:10;
      then dist(E-max C,E-max L~Cage(C,m)) < r by A28,XXREAL_0:2;
      then dist(w,p) < r by TOPREAL6:def 1;
      then p in Ball(w,r) by METRIC_1:12;
      hence (Upper_Appr C).m meets Ball(w,r) by A5,A6,XBOOLE_0:3;
    end;
    hence E-max C in North_Arc C by A1,KURATO_2:50;
  end;

theorem Th33:
  W-min C in South_Arc C
  proof
A1: South_Arc C = Lim_inf Lower_Appr C by JORDAN19:def 4;
    reconsider w = W-min C as Point of Euclid 2 by TOPMETR:16;
    for r being real number st r > 0
      ex k being Element of NAT st
     for m being Element of NAT st m > k holds (Lower_Appr C).m meets Ball(w,r)
    proof
      let r be real number;
      assume r > 0;
      then r/2 > 0 by SEQ_2:3;
      then consider k being Element of NAT such that
        1 < k and
A2:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < r/2 and
A3:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < r/2 by GOBRD14:21;
      take k;
      let m be Element of NAT such that
A4:     m > k;
      reconsider p = W-min L~Cage(C,m) as Point of Euclid 2 by TOPMETR:16;
A5:   (Lower_Appr C).m = Lower_Arc L~Cage(C,m) by JORDAN19:def 2;
A6:   W-min L~Cage(C,m) in Lower_Arc L~Cage(C,m) by JORDAN7:1;
A7:   W-min L~Cage(C,m) in rng Cage(C,m) by SPRECT_2:47;
      len Cage(C,m) > 4 by GOBOARD7:36;
      then
A8:   1+1 <= len Cage(C,m) by XXREAL_0:2;
A9:   W-min L~Cage(C,m) = Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
        by A7,FINSEQ_6:98;
A10:   1+1 <= len Rotate(Cage(C,m),W-min L~Cage(C,m)) by A8,REVROT_1:14; then
A11:   Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
        in LSeg(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) by TOPREAL1:27;
      left_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) /\
        right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1) =
        LSeg(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
          by A10,GOBOARD5:32;
      then
A12:   W-min L~Cage(C,m) in right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
        by A9,A11,XBOOLE_0:def 3;
A13:   W-min C in right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
        by JORDAN1I:8;
A14:   right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
       = right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,
                GoB Rotate(Cage(C,m),W-min L~Cage(C,m))) by A10,JORDAN1H:29
      .= right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,GoB Cage(C,m))
         by REVROT_1:28
      .= right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1,Gauge(C,m))
         by JORDAN1H:52;
      Cage(C,m) is_sequence_on Gauge(C,m) by JORDAN9:def 1;
      then
A15:   Rotate(Cage(C,m),W-min L~Cage(C,m)) is_sequence_on Gauge(C,m)
       by REVROT_1:34;
      Rotate(Cage(C,m),W-min L~Cage(C,m))/.1
       = W-min L~Rotate(Cage(C,m),W-min L~Cage(C,m)) by A9,REVROT_1:33;
      then
      consider i, j being Element of NAT such that
A16:     [i,j] in Indices Gauge(C,m) &
        [i,j+1] in Indices Gauge(C,m) and
A17:     Rotate(Cage(C,m),W-min L~Cage(C,m))/.1 = Gauge(C,m)*(i,j) &
        Rotate(Cage(C,m),W-min L~Cage(C,m))/.(1+1) = Gauge(C,m)*(i,j+1)
          by A10,A15,JORDAN1I:23;
A18:   right_cell(Rotate(Cage(C,m),W-min L~Cage(C,m)),1)
       = cell(Gauge(C,m),i,j) by A10,A14,A15,A16,A17,GOBRD13:23;
A19:   1 <= i & 1 <= j & j+1 <= width Gauge(C,m) by A16,GOBOARD5:1;
A20:   1 <= i+1 by NAT_1:11;
A21:   j <= width Gauge(C,m) by A16,GOBOARD5:1;
      i < len Gauge(C,m) by A10,A15,A16,A17,JORDAN1I:16;
      then
A22:   i+1 <= len Gauge(C,m) by NAT_1:13;
      then
A23:   [i+1,j] in Indices Gauge(C,m) by A19,A20,A21,GOBOARD7:10;
      Gauge(C,m)*(i,j)`1 <= (W-min C)`1 &
        (W-min C)`1 <= Gauge(C,m)*(i+1,j)`1 &
        Gauge(C,m)*(i,j)`2 <= (W-min C)`2 &
        (W-min C)`2 <= Gauge(C,m)*(i,j+1)`2 &
        Gauge(C,m)*(i,j)`1 <= (W-min L~Cage(C,m))`1 &
        (W-min L~Cage(C,m))`1 <= Gauge(C,m)*(i+1,j)`1 &
        Gauge(C,m)*(i,j)`2 <= (W-min L~Cage(C,m))`2 &
        (W-min L~Cage(C,m))`2 <= Gauge(C,m)*(i,j+1)`2
          by A12,A13,A18,A19,A22,JORDAN9:19;
      then
A24:   dist(W-min C,W-min L~Cage(C,m)) <=
       (Gauge(C,m)*(i+1,j)`1-Gauge(C,m)*(i,j)`1) +
       (Gauge(C,m)*(i,j+1)`2-Gauge(C,m)*(i,j)`2) by TOPREAL6:104;
A25:   [1,1] in Indices Gauge(C,m) by Th5;
A26:   [1,1+1] in Indices Gauge(C,m) by Th6;
A27:   [1+1,1] in Indices Gauge(C,m) by Th7;
A28:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
          = (E-bound C - W-bound C)/2|^m by A25,A27,GOBRD14:20;
A29:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
          = (N-bound C - S-bound C)/2|^m by A25,A26,GOBRD14:19;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) by A4,Th9;
      then
A30:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) < r/2 by A2,XXREAL_0:2;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) by A4,Th11;
      then
A31:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) < r/2 by A3,XXREAL_0:2;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
         = dist(Gauge(C,m)*(i,j),Gauge(C,m)*(i,j+1)) by A16,A29,GOBRD14:19;
      then
A32:   Gauge(C,m)*(i,j+1)`2 - Gauge(C,m)*(i,j)`2 < r/2 by A16,A30,GOBRD14:16;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
         = dist(Gauge(C,m)*(i,j),Gauge(C,m)*(i+1,j)) by A16,A23,A28,GOBRD14:20;
      then
      Gauge(C,m)*(i+1,j)`1 - Gauge(C,m)*(i,j)`1 < r/2 by A16,A23,A31,GOBRD14:15
;
      then
      (Gauge(C,m)*(i+1,j)`1-Gauge(C,m)*(i,j)`1) +
       (Gauge(C,m)*(i,j+1)`2-Gauge(C,m)*(i,j)`2) < r/2 + r/2
        by A32,XREAL_1:10;
      then dist(W-min C,W-min L~Cage(C,m)) < r by A24,XXREAL_0:2;
      then dist(w,p) < r by TOPREAL6:def 1;
      then p in Ball(w,r) by METRIC_1:12;
      hence (Lower_Appr C).m meets Ball(w,r) by A5,A6,XBOOLE_0:3;
    end;
    hence W-min C in South_Arc C by A1,KURATO_2:50;
  end;

theorem Th34:
  E-max C in South_Arc C
  proof
A1: South_Arc C = Lim_inf Lower_Appr C by JORDAN19:def 4;
    reconsider w = E-max C as Point of Euclid 2 by TOPMETR:16;
    for r being real number st r > 0
      ex k being Element of NAT st
     for m being Element of NAT st m > k holds (Lower_Appr C).m meets Ball(w,r)
    proof
      let r be real number;
      assume r > 0;
      then r/2 > 0 by SEQ_2:3;
      then consider k being Element of NAT such that
        1 < k and
A2:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < r/2 and
A3:     dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < r/2 by GOBRD14:21;
      take k;
      let m be Element of NAT such that
A4:     m > k;
      reconsider p = E-max L~Cage(C,m) as Point of Euclid 2 by TOPMETR:16;
A5:   (Lower_Appr C).m = Lower_Arc L~Cage(C,m) by JORDAN19:def 2;
A6:   E-max L~Cage(C,m) in Lower_Arc L~Cage(C,m) by JORDAN7:1;
A7:   E-max L~Cage(C,m) in rng Cage(C,m) by SPRECT_2:50;
      len Cage(C,m) > 4 by GOBOARD7:36;
      then
A8:   1+1 <= len Cage(C,m) by XXREAL_0:2;
A9:   E-max L~Cage(C,m) = Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
        by A7,FINSEQ_6:98;
A10:   1+1 <= len Rotate(Cage(C,m),E-max L~Cage(C,m)) by A8,REVROT_1:14; then
A11:   Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
        in LSeg(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) by TOPREAL1:27;
      left_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) /\
        right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1) =
        LSeg(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
          by A10,GOBOARD5:32;
      then
A12:   E-max L~Cage(C,m) in right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
        by A9,A11,XBOOLE_0:def 3;
A13:   E-max C in right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
        by JORDAN1I:9;
A14:   right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
       = right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1,
                GoB Rotate(Cage(C,m),E-max L~Cage(C,m))) by A10,JORDAN1H:29
      .= right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1,GoB Cage(C,m))
         by REVROT_1:28
      .= right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1,Gauge(C,m))
         by JORDAN1H:52;
      Cage(C,m) is_sequence_on Gauge(C,m) by JORDAN9:def 1;
      then
A15:   Rotate(Cage(C,m),E-max L~Cage(C,m)) is_sequence_on Gauge(C,m)
       by REVROT_1:34;
      Rotate(Cage(C,m),E-max L~Cage(C,m))/.1
       = E-max L~Rotate(Cage(C,m),E-max L~Cage(C,m)) by A9,REVROT_1:33;
      then
      consider i, j being Element of NAT such that
A16:     [i,j+1] in Indices Gauge(C,m) &
        [i,j] in Indices Gauge(C,m) and
A17:     Rotate(Cage(C,m),E-max L~Cage(C,m))/.1 = Gauge(C,m)*(i,j+1) &
        Rotate(Cage(C,m),E-max L~Cage(C,m))/.(1+1) = Gauge(C,m)*(i,j)
          by A10,A15,JORDAN1I:25;
A18:   right_cell(Rotate(Cage(C,m),E-max L~Cage(C,m)),1)
       = cell(Gauge(C,m),i-'1,j) by A10,A14,A15,A16,A17,GOBRD13:29;
A19:   1 <= i & 1 <= j & j+1 <= width Gauge(C,m) by A16,GOBOARD5:1;
      i > 1 by A10,A15,A16,A17,JORDAN1I:18;
      then
A20:   i - 1 > 1 - 1 by REAL_1:92;
      then i - 1 is Element of NAT by INT_1:16;
      then
A21:   i - 1 >= 0 + 1 by A20,NAT_1:13;
      then
A22:   i-'1+1 = i by BINARITH:61;
A23:   i <= len Gauge(C,m) by A16,GOBOARD5:1;
A24:   i-'1 = i-1 by A20,BINARITH:def 3;
      i-1 <= i by XREAL_1:148;
      then
A25:   i-'1 <= len Gauge(C,m) & j <= width Gauge(C,m)
        by A16,A23,A24,XXREAL_0:2,GOBOARD5:1;
      then
A26:   [i-'1,j] in Indices Gauge(C,m) by A19,A21,A24,GOBOARD7:10;
      1 <= j+1 by NAT_1:11;
      then
A27:   [i-'1,j+1] in Indices Gauge(C,m) by A19,A21,A24,A25,GOBOARD7:10;
      Gauge(C,m)*(i-'1,j)`1 <= (E-max C)`1 &
        (E-max C)`1 <= Gauge(C,m)*(i-'1+1,j)`1 &
        Gauge(C,m)*(i-'1,j)`2 <= (E-max C)`2 &
        (E-max C)`2 <= Gauge(C,m)*(i-'1,j+1)`2 &
        Gauge(C,m)*(i-'1,j)`1 <= (E-max L~Cage(C,m))`1 &
        (E-max L~Cage(C,m))`1 <= Gauge(C,m)*(i-'1+1,j)`1 &
        Gauge(C,m)*(i-'1,j)`2 <= (E-max L~Cage(C,m))`2 &
        (E-max L~Cage(C,m))`2 <= Gauge(C,m)*(i-'1,j+1)`2
          by A12,A13,A18,A19,A21,A22,A23,JORDAN9:19;
      then
A28:   dist(E-max C,E-max L~Cage(C,m)) <=
       (Gauge(C,m)*(i-'1+1,j)`1-Gauge(C,m)*(i-'1,j)`1) +
       (Gauge(C,m)*(i-'1,j+1)`2-Gauge(C,m)*(i-'1,j)`2) by TOPREAL6:104;
A29:   [1,1] in Indices Gauge(C,m) by Th5;
A30:   [1,1+1] in Indices Gauge(C,m) by Th6;
A31:   [1+1,1] in Indices Gauge(C,m) by Th7;
A32:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
          = (E-bound C - W-bound C)/2|^m by A29,A31,GOBRD14:20;
A33:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
          = (N-bound C - S-bound C)/2|^m by A29,A30,GOBRD14:19;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) by A4,Th9;
      then
A34:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,2)) < r/2 by A2,XXREAL_0:2;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) <
        dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) by A4,Th11;
      then
A35:   dist(Gauge(C,m)*(1,1),Gauge(C,m)*(2,1)) < r/2 by A3,XXREAL_0:2;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1,1+1))
         = dist(Gauge(C,m)*(i-'1,j),Gauge(C,m)*(i-'1,j+1))
           by A26,A27,A33,GOBRD14:19;
      then
A36:   Gauge(C,m)*(i-'1,j+1)`2 - Gauge(C,m)*(i-'1,j)`2 < r/2
        by A26,A27,A34,GOBRD14:16;
      dist(Gauge(C,m)*(1,1),Gauge(C,m)*(1+1,1))
         = dist(Gauge(C,m)*(i-'1,j),Gauge(C,m)*(i-'1+1,j))
           by A16,A22,A26,A32,GOBRD14:20;
      then
      Gauge(C,m)*(i-'1+1,j)`1 - Gauge(C,m)*(i-'1,j)`1 < r/2
        by A16,A22,A26,A35,GOBRD14:15;
      then
      (Gauge(C,m)*(i-'1+1,j)`1-Gauge(C,m)*(i-'1,j)`1) +
       (Gauge(C,m)*(i-'1,j+1)`2-Gauge(C,m)*(i-'1,j)`2) < r/2 + r/2
        by A36,XREAL_1:10;
      then dist(E-max C,E-max L~Cage(C,m)) < r by A28,XXREAL_0:2;
      then dist(w,p) < r by TOPREAL6:def 1;
      then p in Ball(w,r) by METRIC_1:12;
      hence (Lower_Appr C).m meets Ball(w,r) by A5,A6,XBOOLE_0:3;
    end;
    hence E-max C in South_Arc C by A1,KURATO_2:50;
  end;

theorem Th35:
  UMP C in North_Arc C
  proof
    set w = (W-bound C + E-bound C) / 2;
    set p = UMP C;
    set U = {UMP Upper_Arc L~Cage(C,n) where n is Element of NAT: 0 < n};
    {UMP Upper_Arc L~Cage(C,n) where n is Element of NAT: 0 < n} c=
      the carrier of TOP-REAL 2
    proof
      let x be set;
      assume x in {UMP Upper_Arc L~Cage(C,n) where n is Element of NAT: 0 < n};
      then ex n being Element of NAT st x = UMP Upper_Arc L~Cage(C,n) & 0 < n;
      hence thesis;
    end;
    then reconsider U as Subset of TOP-REAL 2;
    set n0 = N-bound L~Cage(C,1);
    set H = LSeg(p,|[w,n0]|);
    set q = inf(proj2.:(H /\ U));
    set S = LSeg(|[w,q]|, |[w,n0]|);
A1: |[w,N-bound C]|`1 = w by EUCLID:56;
A2: |[w,N-bound C]|`2 = N-bound C by EUCLID:56;
A3: |[w,n0]|`1 = w by EUCLID:56;
A4: |[w,n0]|`2 = n0 by EUCLID:56;
A5: |[w,q]|`1 = w by EUCLID:56;
A6: |[w,q]|`2 = q by EUCLID:56;
A7: |[w,q]| in S by TOPREAL1:6;
A8: p`1 = w by EUCLID:56;
A9: S is vertical by A3,A5,SPPOL_1:37;
A10: LSeg(p, |[w, N-bound C]|) /\ C = {p} by JORDAN21:47;
A11: for n being Element of NAT holds (UMP Upper_Arc L~Cage(C,n))`1 = w
    proof
      let n be Element of NAT;
A12:   W-bound L~Cage(C,n) + E-bound L~Cage(C,n) = W-bound C + E-bound C
        by JORDAN1G:41;
      thus (UMP Upper_Arc L~Cage(C,n))`1
         = (W-bound Upper_Arc L~Cage(C,n) + E-bound Upper_Arc L~Cage(C,n))/2
           by EUCLID:56
        .= (W-bound L~Cage(C,n) + E-bound Upper_Arc L~Cage(C,n))/2
           by JORDAN21:26
        .= w by A12,JORDAN21:27;
    end;
A13: for n being Element of NAT st 0 < n
       holds (UMP Upper_Arc L~Cage(C,n))`2 in proj2.:(H /\ U)
    proof
      let n be Element of NAT;
      set f = Cage(C,n);
      set s = UMP Upper_Arc L~f;
A14:   s`2 <= N-bound L~f by JORDAN21:60;
      assume
A15:     0 < n;
      then 0+1 <= n by NAT_1:13;
      then 1 < n or n = 1 by REAL_1:def 5;
      then N-bound L~Cage(C,n) <= N-bound L~Cage(C,1) by JORDAN10:7;
      then
A16:   s`2 <= n0 by A14,XXREAL_0:2;
A17:   s`1 = w by A11;
      p`2 <= s`2 by A15,Th25;
      then
A18:   s in H by A3,A4,A8,A16,A17,GOBOARD7:8;
      s in U by A15;
      then
A19:   s in H /\ U by A18,XBOOLE_0:def 3;
      s`2 = proj2.s by PSCOMP_1:def 29;
      hence thesis by A19,FUNCT_2:43;
    end;
    H /\ U is Bounded by TOPREAL6:98;
    then proj2.:(H /\ U) is bounded by JCT_MISC:23;
    then
A20: proj2.:(H /\ U) is bounded_below by SEQ_4:def 3;
A21: (UMP Upper_Arc L~Cage(C,1))`2 <= n0 by JORDAN21:60;
A22: p`2 <= (UMP Upper_Arc L~Cage(C,1))`2 by Th25;
A23: (UMP Upper_Arc L~Cage(C,1))`2 in proj2.:(H /\ U) by A13;
    then q <= (UMP Upper_Arc L~Cage(C,1))`2 by A20,SEQ_4:def 5;
    then
A24: q <= n0 by A21,XXREAL_0:2;
    per cases;
    suppose
A25:   p <> |[w,q]|;
A26: now
      assume
A27:     p`2 >= q;
      per cases by A27,REAL_1:def 5;
      suppose p`2 = q;
      hence contradiction by A25,EUCLID:56;
      end;
      suppose p`2 > q;
      then 0 < p`2 - q by XREAL_1:52;
      then consider r being real number such that
A28:     r in proj2.:(H /\ U) and
A29:     r < q+(p`2 - q) by A20,A23,SEQ_4:def 5;
      consider t being Point of TOP-REAL 2 such that
A30:     t in H /\ U and
A31:     proj2.t = r by A28,Lm1;
A32:   t`2 = r by A31,PSCOMP_1:def 29;
A33:   p`2 <= n0 by A21,A22,XXREAL_0:2;
      t in H by A30,XBOOLE_0:def 3;
      hence contradiction by A4,A29,A32,A33,TOPREAL1:10;
    end;
    end;
    consider S',C' being Subset of TopSpaceMetr Euclid 2 such that
A34:   S = S' & C = C' and
A35:   dist_min(S,C) = min_dist_min(S',C') by JORDAN1K:def 1;
    S misses C
    proof
      assume S meets C;
      then consider x being set such that
A36:     x in S and
A37:     x in C by XBOOLE_0:3;
      reconsider x as Point of TOP-REAL 2 by A36;
A38:   x`2 <= N-bound C by A37,PSCOMP_1:71;
A39:   q <= x`2 by A4,A6,A24,A36,TOPREAL1:10;
      then
A40:   p`2 < x`2 by A26,XXREAL_0:2;
      x`1 = w by A5,A7,A9,A36,SPPOL_1:def 3;
      then
      x in LSeg(p, |[w, N-bound C]|) by A1,A2,A8,A38,A40,GOBOARD7:8;
      then x in {p} by A10,A37,XBOOLE_0:def 3;
      hence contradiction by A26,A39,TARSKI:def 1;
    end;
    then dist_min(S,C) > 0 by A34,A35,JGRAPH_1:55;
    then dist_min(S,C)/2 > 0 by SEQ_2:3;
    then consider k being Element of NAT such that
A41:   1 < k and
A42:   dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < dist_min(S,C)/2 &
      dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < dist_min(S,C)/2 by GOBRD14:21;
    set f = Cage(C,k),
        G = Gauge(C,k);
A43: dist(G*(1,1),G*(1+1,1)) + dist(G*(1,1),G*(1,1+1))
      < dist_min(S,C)/2 + dist_min(S,C)/2 by A42,XREAL_1:10;
    set s = UMP Upper_Arc L~f;
A44: s`1 = w by A11;
A45: s`2 <= N-bound L~f by JORDAN21:60;
    N-bound L~Cage(C,k) <= N-bound L~Cage(C,1) by A41,JORDAN10:7;
    then
A46: s`2 <= n0 by A45,XXREAL_0:2;
    0 < k by A41;
    then s`2 in proj2.:(H /\ U) by A13;
    then
A47: q <= s`2 by A20,SEQ_4:def 5;
A48: s in S by A3,A4,A5,A6,A44,A46,A47,GOBOARD7:8;
A49: Upper_Arc L~f c= L~f by JORDAN6:76;
    s in Upper_Arc L~f by Lm4;
    then consider i being Element of NAT such that
A50:   1 <= i & i+1 <= len f and
A51:   s in LSeg(f,i) by A49,SPPOL_2:13;
    right_cell(f,i,G) meets C by A50,JORDAN9:33;
    then consider c being set such that
A52:   c in right_cell(f,i,G) and
A53:   c in C by XBOOLE_0:3;
    reconsider c as Point of TOP-REAL 2 by A53;
A54: f is_sequence_on G by JORDAN9:def 1;
    consider i1,j1,i2,j2 being Element of NAT such that
A55:   [i1,j1] in Indices G and
A56:   f/.i = G*(i1,j1) and
A57:   [i2,j2] in Indices G and
A58:   f/.(i+1) = G*(i2,j2) and
A59:   i1 = i2 & j1+1 = j2 or i1+1 = i2 & j1 = j2 or
        i1 = i2+1 & j1 = j2 or i1 = i2 & j1 = j2+1 by A50,A54,JORDAN8:6;
    left_cell(f,i,G) /\ right_cell(f,i,G) = LSeg(f,i) by A50,A54,GOBRD13:30;
    then
A60: s in right_cell(f,i,G) by A51,XBOOLE_0:def 3;
A61: i1+1<>i1+0;
A62: i2+1+1<>i2;
A63: j2+1+1<>j2;
A64: 1 <= i1 by A55,GOBOARD5:1;
A65: 1 <= j1 by A55,GOBOARD5:1;
A66: 1 <= i2 by A57,GOBOARD5:1;
A67: 1 <= j2 by A57,GOBOARD5:1;
A68: 1 <= i1+1 by NAT_1:11;
A69: 1 <= j1+1 by NAT_1:11;
A70: i1 <= len G by A55,GOBOARD5:1;
A71: j1 <= width G by A55,GOBOARD5:1;
A72: i2 <= len G by A57,GOBOARD5:1;
A73: j2 <= width G by A57,GOBOARD5:1;
    reconsider s' = s, c' = c as Point of Euclid 2 by TOPMETR:16;
A74: min_dist_min(S',C') <= dist(s',c') by A34,A48,A53,WEIERSTR:40;
A75: dist(s',c') = dist(s,c) by TOPREAL6:def 1;
A76: [1,1] in Indices G by Th5;
    [1,1+1] in Indices G by Th6;
    then
A77: dist(G*(1,1),G*(1,1+1)) = (N-bound C - S-bound C)/2|^k by A76,GOBRD14:19;
    [1+1,1] in Indices G by Th7;
    then
A78: dist(G*(1,1),G*(1+1,1)) = (E-bound C - W-bound C)/2|^k by A76,GOBRD14:20;
    now
      per cases by A59;
      suppose
A79:     i1 = i2 & j1+1 = j2;
      then
A80:   j1+1 <= width G by A57,GOBOARD5:1;
      i1 < len G by A50,A55,A56,A57,A58,A79,JORDAN10:1;
      then
A81:   i1+1 <= len G by NAT_1:13;
      right_cell(f,i,G) = cell(G,i1,j1)
        by A50,A54,A55,A56,A57,A58,A61,A63,A79,GOBRD13:def 2;
      then
      G*(i1,j1)`1 <= c`1 & c`1 <= G*(i1+1,j1)`1 &
      G*(i1,j1)`2 <= c`2 & c`2 <= G*(i1,j1+1)`2 &
      G*(i1,j1)`1 <= s`1 & s`1 <= G*(i1+1,j1)`1 &
      G*(i1,j1)`2 <= s`2 & s`2 <= G*(i1,j1+1)`2
        by A52,A60,A64,A65,A80,A81,JORDAN9:19;
      then
A82:   dist(s,c) <=
        (G*(i1+1,j1)`1-G*(i1,j1)`1) + (G*(i1,j1+1)`2-G*(i1,j1)`2)
          by TOPREAL6:104;
A83:   [i1+1,j1] in Indices G by A65,A68,A71,A81,GOBOARD7:10;
      then
A84:   dist(G*(i1,j1),G*(i1+1,j1)) = G*(i1+1,j1)`1 - G*(i1,j1)`1
        by A55,GOBRD14:15;
A85:   dist(G*(i1,j1),G*(i1,j1+1)) = G*(i1,j1+1)`2 - G*(i1,j1)`2
        by A55,A57,A79,GOBRD14:16;
A86:   dist(G*(i1,j1),G*(i1,j1+1)) = (N-bound C - S-bound C)/2|^k
        by A55,A57,A79,GOBRD14:19;
      dist(G*(i1,j1),G*(i1+1,j1)) = (E-bound C - W-bound C)/2|^k
        by A55,A83,GOBRD14:20;
      hence contradiction by A35,A43,A74,A75,A77,A78,A82,A84,A85,A86,XXREAL_0:2
;
      end;
      suppose
A87:     i1+1 = i2 & j1 = j2;
      then
A88:   i1+1 <= len G by A57,GOBOARD5:1;
      1 < j1 by A50,A55,A56,A57,A58,A87,JORDAN10:3;
      then
A89:   1 <= j1-'1 by BINARITH:67;
A90:   j1-'1 <= width G by A71,BINARITH:62;
A91:   j1-'1+1 = j1 by A89,BINARITH:61;
      right_cell(f,i,G) = cell(G,i1,j1-'1)
        by A50,A54,A55,A56,A57,A58,A61,A62,A87,GOBRD13:def 2;
      then
      G*(i1,j1-'1)`1 <= c`1 & c`1 <= G*(i1+1,j1-'1)`1 &
      G*(i1,j1-'1)`2 <= c`2 & c`2 <= G*(i1,j1-'1+1)`2 &
      G*(i1,j1-'1)`1 <= s`1 & s`1 <= G*(i1+1,j1-'1)`1 &
      G*(i1,j1-'1)`2 <= s`2 & s`2 <= G*(i1,j1-'1+1)`2
        by A52,A60,A64,A71,A88,A89,A91,JORDAN9:19;
      then
A92:   dist(s,c) <=
        (G*(i1+1,j1-'1)`1-G*(i1,j1-'1)`1) + (G*(i1,j1-'1+1)`2-G*(i1,j1-'1)`2)
          by TOPREAL6:104;
A93:   [i1,j1-'1] in Indices G by A64,A70,A89,A90,GOBOARD7:10;
A94:   [i1+1,j1-'1] in Indices G by A66,A72,A87,A89,A90,GOBOARD7:10;
      then
A95:   dist(G*(i1,j1-'1),G*(i1+1,j1-'1)) = G*(i1+1,j1-'1)`1 - G*(i1,j1-'1)`1
        by A93,GOBRD14:15;
A96:   dist(G*(i1,j1-'1),G*(i1,j1-'1+1)) = G*(i1,j1-'1+1)`2 - G*(i1,j1-'1)`2
        by A55,A91,A93,GOBRD14:16;
A97:   dist(G*(i1,j1-'1),G*(i1,j1-'1+1)) = (N-bound C - S-bound C)/2|^k
        by A55,A91,A93,GOBRD14:19;
      dist(G*(i1,j1-'1),G*(i1+1,j1-'1)) = (E-bound C - W-bound C)/2|^k
        by A93,A94,GOBRD14:20;
      hence contradiction by A35,A43,A74,A75,A77,A78,A92,A95,A96,A97,XXREAL_0:2
;
      end;
      suppose
A98:     i1 = i2+1 & j1 = j2;
      then
A99:   i2+1 <= len G by A55,GOBOARD5:1;
      j1 < width G by A50,A55,A56,A57,A58,A98,JORDAN10:4;
      then
A100:   j1+1 <= width G by NAT_1:13;
      right_cell(f,i,G) = cell(G,i2,j2)
        by A50,A54,A55,A56,A57,A58,A61,A62,A98,GOBRD13:def 2;
      then
      G*(i2,j2)`1 <= c`1 & c`1 <= G*(i2+1,j2)`1 &
      G*(i2,j2)`2 <= c`2 & c`2 <= G*(i2,j2+1)`2 &
      G*(i2,j2)`1 <= s`1 & s`1 <= G*(i2+1,j2)`1 &
      G*(i2,j2)`2 <= s`2 & s`2 <= G*(i2,j2+1)`2
        by A52,A60,A65,A66,A98,A99,A100,JORDAN9:19;
      then
A101:   dist(s,c) <=
        (G*(i2+1,j2)`1-G*(i2,j2)`1) + (G*(i2,j2+1)`2-G*(i2,j2)`2)
          by TOPREAL6:104;
A102:   dist(G*(i2,j2),G*(i2+1,j2)) = G*(i2+1,j2)`1 - G*(i2,j2)`1
        by A55,A57,A98,GOBRD14:15;
A103:   [i2,j2+1] in Indices G by A66,A69,A72,A98,A100,GOBOARD7:10;
      then
A104:   dist(G*(i2,j2),G*(i2,j2+1)) = G*(i2,j2+1)`2 - G*(i2,j2)`2
        by A57,GOBRD14:16;
A105:   dist(G*(i2,j2),G*(i2,j2+1)) = (N-bound C - S-bound C)/2|^k
        by A57,A103,GOBRD14:19;
      dist(G*(i2,j2),G*(i2+1,j2)) = (E-bound C - W-bound C)/2|^k
        by A55,A57,A98,GOBRD14:20;
      hence contradiction
        by A35,A43,A74,A75,A77,A78,A101,A102,A104,A105,XXREAL_0:2;
      end;
      suppose
A106:     i1 = i2 & j1 = j2+1;
      then
A107:   j2+1 <= width G by A55,GOBOARD5:1;
      1 < i1 by A50,A55,A56,A57,A58,A106,JORDAN10:2;
      then
A108:   1 <= i1-'1 by BINARITH:67;
A109:   i1-'1 <= len G by A70,BINARITH:62;
A110:   i1-'1+1 = i1 by A108,BINARITH:61;
      right_cell(f,i,G) = cell(G,i1-'1,j2)
        by A50,A54,A55,A56,A57,A58,A61,A63,A106,GOBRD13:def 2;
      then
      G*(i1-'1,j2)`1 <= c`1 & c`1 <= G*(i1-'1+1,j2)`1 &
      G*(i1-'1,j2)`2 <= c`2 & c`2 <= G*(i1-'1,j2+1)`2 &
      G*(i1-'1,j2)`1 <= s`1 & s`1 <= G*(i1-'1+1,j2)`1 &
      G*(i1-'1,j2)`2 <= s`2 & s`2 <= G*(i1-'1,j2+1)`2
        by A52,A60,A67,A70,A107,A108,A110,JORDAN9:19;
      then
A111:   dist(s,c) <=
        (G*(i1-'1+1,j2)`1-G*(i1-'1,j2)`1) + (G*(i1-'1,j2+1)`2-G*(i1-'1,j2)`2)
          by TOPREAL6:104;
A112:   [i1-'1,j2] in Indices G by A67,A73,A108,A109,GOBOARD7:10;
A113:   [i1-'1+1,j2] in Indices G by A64,A67,A70,A73,A110,GOBOARD7:10;
      then
A114:   dist(G*(i1-'1,j2),G*(i1-'1+1,j2)) = G*(i1-'1+1,j2)`1 - G*(i1-'1,j2)`1
        by A112,GOBRD14:15;
A115:   [i1-'1,j2+1] in Indices G by A65,A71,A106,A108,A109,GOBOARD7:10;
      then
A116:   dist(G*(i1-'1,j2),G*(i1-'1,j2+1)) = G*(i1-'1,j2+1)`2 - G*(i1-'1,j2)`2
        by A112,GOBRD14:16;
A117:   dist(G*(i1-'1,j2),G*(i1-'1,j2+1)) = (N-bound C - S-bound C)/2|^k
        by A112,A115,GOBRD14:19;
      dist(G*(i1-'1,j2),G*(i1-'1+1,j2)) = (E-bound C - W-bound C)/2|^k
        by A112,A113,GOBRD14:20;
      hence contradiction
        by A35,A43,A74,A75,A77,A78,A111,A114,A116,A117,XXREAL_0:2;
    end;
    end;
    hence thesis;
    end;

    suppose p = |[w,q]|;
    then
A118: p`2 = q by EUCLID:56;
A119: North_Arc C = Lim_inf Upper_Appr C by JORDAN19:def 3;
    reconsider e = p as Point of Euclid 2 by TOPMETR:16;

    ex S being Real_Sequence of 2 st S is convergent &
      (for x being Element of NAT holds S.x in (Upper_Appr C).x) & p = lim S
    proof
      set R = {(UMP Upper_Arc L~Cage(C,n))`2 where n is Element of NAT: 0 < n};
      R c= REAL
      proof
        let x be set;
        assume x in R;
        then ex n being Element of NAT
        st x = (UMP Upper_Arc L~Cage(C,n))`2 & 0 < n;
        hence thesis;
      end;
      then reconsider R as Subset of REAL;
A120:   (UMP Upper_Arc L~Cage(C,1))`2 in R;
A121:   R is bounded_below
      proof
        take q;
        let r be real number;
        assume r in R;
        then ex n being Element of NAT
        st r = (UMP Upper_Arc L~Cage(C,n))`2 & 0 < n;
        then r in proj2.:(H /\ U) by A13;
        hence q<=r by A20,SEQ_4:def 5;
      end;
      deffunc f(Element of NAT) = UMP Upper_Arc L~Cage(C,$1);
      consider S being Real_Sequence of 2 such that
A122:     for n being Element of NAT holds S.n = f(n) from TOPRNS_1:sch 1;
      take S;
A123:   for r being Real st 0<r
       ex n being Element of NAT st for m being Element of NAT st n<=m holds
        |. S.m-p .| < r
      proof
        let r be Real;
        assume
A124:       0 < r;
A125:     for r being real number st r in R holds q<=r
        proof
          let r be real number;
          assume r in R;
          then ex n being Element of NAT
          st r = (UMP Upper_Arc L~Cage(C,n))`2 & 0 < n;
          then r in proj2.:(H /\ U) by A13;
          hence thesis by A20,SEQ_4:def 5;
        end;
        for s being real number st 0<s
         ex r being real number st r in R & r<q+s
        proof
          let s be real number;
          assume 0<s;
          then consider r being real number such that
A126:         r in proj2.:(H /\ U) and
A127:         r<q+s by A20,A23,SEQ_4:def 5;
          take r;
          consider x being Point of TOP-REAL 2 such that
A128:         x in H /\ U and
A129:         proj2.x = r by A126,Lm1;
          x in U by A128,XBOOLE_0:def 3;
          then consider n being Element of NAT such that
A130:         x = UMP Upper_Arc L~Cage(C,n) and
A131:         0 < n;
          r = (UMP Upper_Arc L~Cage(C,n))`2 by A129,A130,PSCOMP_1:def 29;
          hence thesis by A127,A131;
        end;
        then
A132:     inf R = q by A120,A121,A125,SEQ_4:def 5;
        consider v being real number such that
A133:       v in R and
A134:       v < inf R + r by A120,A121,A124,SEQ_4:def 5;
A135:   v - inf R < inf R + r - inf R by A134,REAL_1:92;
        consider n being Element of NAT such that
A136:       v = (UMP Upper_Arc L~Cage(C,n))`2 and
A137:       0 < n by A133;
        take n;
        let m be Element of NAT;
A138:     S.m = UMP Upper_Arc L~Cage(C,m) by A122;
        assume
A139:       n <= m;
        then (UMP Upper_Arc L~Cage(C,m))`2 <= (UMP Upper_Arc L~Cage(C,n))`2
          by A137,Th29;
        then (S.m)`2 <= v by A122,A136;
        then (S.m)`2 - p`2 <= v - p`2 by REAL_1:92;
        then
A140:     (S.m)`2-p`2 < r by A118,A132,A135,XXREAL_0:2;
        0 < m by A137,A139;
        then (UMP C)`2 < (UMP Upper_Arc L~Cage(C,m))`2 by Th25;
        then
A141:     0 < (S.m)`2-p`2 by A138,XREAL_1:52;
        (S.m)`1 = w by A11,A138;
        then abs((S.m)`1-p`1) = 0 by A8,ABSVALUE:def 1;
        then |. S.m-p .|<=0+abs((S.m)`2-p`2) by JGRAPH_1:49;
        then |. S.m-p .|<=(S.m)`2-p`2 by A141,ABSVALUE:def 1;
        hence |. S.m-p .| < r by A140,XXREAL_0:2;
      end;
      thus
A142:     S is convergent
      proof
        take p;
        thus thesis by A123;
      end;
      hereby
        let x be Element of NAT;
A143:     S.x = UMP Upper_Arc L~Cage(C,x) by A122;
        (Upper_Appr C).x = Upper_Arc L~Cage(C,x) by JORDAN19:def 1;
        hence S.x in (Upper_Appr C).x by A143,Lm4;
      end;
      thus p = lim S by A123,A142,TOPRNS_1:def 10;
    end;
    hence thesis by A119,KURATO_2:58;
  end;
  end;

theorem Th36:
  LMP C in South_Arc C
  proof
    set w = (W-bound C + E-bound C) / 2;
    set p = LMP C;
    set U = {LMP Lower_Arc L~Cage(C,n) where n is Element of NAT: 0 < n};
    {LMP Lower_Arc L~Cage(C,n) where n is Element of NAT: 0 < n} c=
      the carrier of TOP-REAL 2
    proof
      let x be set;
      assume x in {LMP Lower_Arc L~Cage(C,n) where n is Element of NAT: 0 < n};
      then ex n being Element of NAT st x = LMP Lower_Arc L~Cage(C,n) & 0 < n;
      hence thesis;
    end;
    then reconsider U as Subset of TOP-REAL 2;
    set n0 = S-bound L~Cage(C,1);
    set H = LSeg(p,|[w,n0]|);
    set q = sup(proj2.:(H /\ U));
    set S = LSeg(|[w,q]|, |[w,n0]|);
A1: |[w,S-bound C]|`1 = w by EUCLID:56;
A2: |[w,S-bound C]|`2 = S-bound C by EUCLID:56;
A3: |[w,n0]|`1 = w by EUCLID:56;
A4: |[w,n0]|`2 = n0 by EUCLID:56;
A5: |[w,q]|`1 = w by EUCLID:56;
A6: |[w,q]|`2 = q by EUCLID:56;
A7: |[w,q]| in S by TOPREAL1:6;
A8: p`1 = w by EUCLID:56;
A9: S is vertical by A3,A5,SPPOL_1:37;
A10: LSeg(p, |[w, S-bound C]|) /\ C = {p} by JORDAN21:48;
A11: for n being Element of NAT holds (LMP Lower_Arc L~Cage(C,n))`1 = w
    proof
      let n be Element of NAT;
A12:   W-bound L~Cage(C,n) + E-bound L~Cage(C,n) = W-bound C + E-bound C
        by JORDAN1G:41;
      thus (LMP Lower_Arc L~Cage(C,n))`1
         = (W-bound Lower_Arc L~Cage(C,n) + E-bound Lower_Arc L~Cage(C,n))/2
           by EUCLID:56
        .= (W-bound L~Cage(C,n) + E-bound Lower_Arc L~Cage(C,n))/2
           by JORDAN21:28
        .= w by A12,JORDAN21:29;
    end;
A13: for n being Element of NAT st 0 < n holds
      (LMP Lower_Arc L~Cage(C,n))`2 in proj2.:(H /\ U)
    proof
      let n be Element of NAT;
      set f = Cage(C,n);
      set s = LMP Lower_Arc L~f;
A14:   S-bound L~f <= s`2 by JORDAN21:61;
      assume
A15:     0 < n;
      then 0+1 <= n by NAT_1:13;
      then n = 1 or n > 1 by REAL_1:def 5;
      then S-bound L~Cage(C,n) >= S-bound L~Cage(C,1) by JORDAN1A:90;
      then
A16:   s`2 >= n0 by A14,XXREAL_0:2;
A17:   s`1 = w by A11;
      p`2 >= s`2 by A15,Th26;
      then
A18:   s in H by A3,A4,A8,A16,A17,GOBOARD7:8;
      s in U by A15;
      then
A19:   s in H /\ U by A18,XBOOLE_0:def 3;
      s`2 = proj2.s by PSCOMP_1:def 29;
      hence thesis by A19,FUNCT_2:43;
    end;
    H /\ U is Bounded by TOPREAL6:98;
    then proj2.:(H /\ U) is bounded by JCT_MISC:23;
    then
A20: proj2.:(H /\ U) is bounded_above by SEQ_4:def 3;
A21: (LMP Lower_Arc L~Cage(C,1))`2 >= n0 by JORDAN21:61;
A22: p`2 >= (LMP Lower_Arc L~Cage(C,1))`2 by Th26;
A23: (LMP Lower_Arc L~Cage(C,1))`2 in proj2.:(H /\ U) by A13;
    then q >= (LMP Lower_Arc L~Cage(C,1))`2 by A20,SEQ_4:def 4;
    then
A24: q >= n0 by A21,XXREAL_0:2;
    per cases;
    suppose
A25:   p <> |[w,q]|;
A26: now
      assume
A27:     p`2 <= q;
      per cases by A27,REAL_1:def 5;
      suppose p`2 = q;
      hence contradiction by A25,EUCLID:56;
      end;
      suppose p`2 < q;
      then 0 < q - p`2 by XREAL_1:52;
      then consider r being real number such that
A28:     r in proj2.:(H /\ U) and
A29:     q-(q-p`2) < r by A20,A23,SEQ_4:def 4;
      consider t being Point of TOP-REAL 2 such that
A30:     t in H /\ U and
A31:     proj2.t = r by A28,Lm1;
A32:   t`2 = r by A31,PSCOMP_1:def 29;
A33:   p`2 >= n0 by A21,A22,XXREAL_0:2;
      t in H by A30,XBOOLE_0:def 3;
      hence contradiction by A4,A29,A32,A33,TOPREAL1:10;
    end;
    end;
    consider S',C' being Subset of TopSpaceMetr Euclid 2 such that
A34:   S = S' & C = C' and
A35:   dist_min(S,C) = min_dist_min(S',C') by JORDAN1K:def 1;
    S misses C
    proof
      assume S meets C;
      then consider x being set such that
A36:     x in S and
A37:     x in C by XBOOLE_0:3;
      reconsider x as Point of TOP-REAL 2 by A36;
A38:   x`2 >= S-bound C by A37,PSCOMP_1:71;
A39:   q >= x`2 by A4,A6,A24,A36,TOPREAL1:10;
      then
A40:   p`2 > x`2 by A26,XXREAL_0:2;
      x`1 = w by A5,A7,A9,A36,SPPOL_1:def 3;
      then
      x in LSeg(p, |[w, S-bound C]|) by A1,A2,A8,A38,A40,GOBOARD7:8;
      then x in {p} by A10,A37,XBOOLE_0:def 3;
      hence contradiction by A26,A39,TARSKI:def 1;
    end;
    then dist_min(S,C) > 0 by A34,A35,JGRAPH_1:55;
    then dist_min(S,C)/2 > 0 by SEQ_2:3;
    then consider k being Element of NAT such that
A41:   1 < k and
A42:   dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < dist_min(S,C)/2 &
      dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < dist_min(S,C)/2 by GOBRD14:21;
    set f = Cage(C,k),
        G = Gauge(C,k);
A43: dist(G*(1,1),G*(1+1,1)) + dist(G*(1,1),G*(1,1+1))
      < dist_min(S,C)/2 + dist_min(S,C)/2 by A42,XREAL_1:10;
    set s = LMP Lower_Arc L~f;
A44: s`1 = w by A11;
A45: s`2 >= S-bound L~f by JORDAN21:61;
    S-bound L~Cage(C,k) >= S-bound L~Cage(C,1) by A41,JORDAN1A:90;
    then
A46: s`2 >= n0 by A45,XXREAL_0:2;
    0 < k by A41;
    then s`2 in proj2.:(H /\ U) by A13;
    then
A47: q >= s`2 by A20,SEQ_4:def 4;
A48: s in S by A3,A4,A5,A6,A44,A46,A47,GOBOARD7:8;
A49: Lower_Arc L~f c= L~f by JORDAN6:76;
    s in Lower_Arc L~f by Lm5;
    then consider i being Element of NAT such that
A50:   1 <= i & i+1 <= len f and
A51:   s in LSeg(f,i) by A49,SPPOL_2:13;
    right_cell(f,i,G) meets C by A50,JORDAN9:33;
    then consider c being set such that
A52:   c in right_cell(f,i,G) and
A53:   c in C by XBOOLE_0:3;
    reconsider c as Point of TOP-REAL 2 by A53;
A54: f is_sequence_on G by JORDAN9:def 1;
    consider i1,j1,i2,j2 being Element of NAT such that
A55:   [i1,j1] in Indices G and
A56:   f/.i = G*(i1,j1) and
A57:   [i2,j2] in Indices G and
A58:   f/.(i+1) = G*(i2,j2) and
A59:   i1 = i2 & j1+1 = j2 or i1+1 = i2 & j1 = j2 or
        i1 = i2+1 & j1 = j2 or i1 = i2 & j1 = j2+1 by A50,A54,JORDAN8:6;
    left_cell(f,i,G) /\ right_cell(f,i,G) = LSeg(f,i) by A50,A54,GOBRD13:30;
    then
A60: s in right_cell(f,i,G) by A51,XBOOLE_0:def 3;
A61: i1+1<>i1+0;
A62: i2+1+1<>i2;
A63: j2+1+1<>j2;
A64: 1 <= i1 by A55,GOBOARD5:1;
A65: 1 <= j1 by A55,GOBOARD5:1;
A66: 1 <= i2 by A57,GOBOARD5:1;
A67: 1 <= j2 by A57,GOBOARD5:1;
A68: 1 <= i1+1 by NAT_1:11;
A69: 1 <= j1+1 by NAT_1:11;
A70: i1 <= len G by A55,GOBOARD5:1;
A71: j1 <= width G by A55,GOBOARD5:1;
A72: i2 <= len G by A57,GOBOARD5:1;
A73: j2 <= width G by A57,GOBOARD5:1;
    reconsider s' = s, c' = c as Point of Euclid 2 by TOPMETR:16;
A74: min_dist_min(S',C') <= dist(s',c') by A34,A48,A53,WEIERSTR:40;
A75: dist(s',c') = dist(s,c) by TOPREAL6:def 1;
A76: [1,1] in Indices G by Th5;
    [1,1+1] in Indices G by Th6;
    then
A77: dist(G*(1,1),G*(1,1+1)) = (N-bound C - S-bound C)/2|^k by A76,GOBRD14:19;
    [1+1,1] in Indices G by Th7;
    then
A78: dist(G*(1,1),G*(1+1,1)) = (E-bound C - W-bound C)/2|^k by A76,GOBRD14:20;
    now
      per cases by A59;
      suppose
A79:     i1 = i2 & j1+1 = j2;
      then
A80:   j1+1 <= width G by A57,GOBOARD5:1;
      i1 < len G by A50,A55,A56,A57,A58,A79,JORDAN10:1;
      then
A81:   i1+1 <= len G by NAT_1:13;
      right_cell(f,i,G) = cell(G,i1,j1)
        by A50,A54,A55,A56,A57,A58,A61,A63,A79,GOBRD13:def 2;
      then
      G*(i1,j1)`1 <= c`1 & c`1 <= G*(i1+1,j1)`1 &
      G*(i1,j1)`2 <= c`2 & c`2 <= G*(i1,j1+1)`2 &
      G*(i1,j1)`1 <= s`1 & s`1 <= G*(i1+1,j1)`1 &
      G*(i1,j1)`2 <= s`2 & s`2 <= G*(i1,j1+1)`2
        by A52,A60,A64,A65,A80,A81,JORDAN9:19;
      then
A82:   dist(s,c) <=
        (G*(i1+1,j1)`1-G*(i1,j1)`1) + (G*(i1,j1+1)`2-G*(i1,j1)`2)
          by TOPREAL6:104;
A83:   [i1+1,j1] in Indices G by A65,A68,A71,A81,GOBOARD7:10;
      then
A84:   dist(G*(i1,j1),G*(i1+1,j1)) = G*(i1+1,j1)`1 - G*(i1,j1)`1
        by A55,GOBRD14:15;
A85:   dist(G*(i1,j1),G*(i1,j1+1)) = G*(i1,j1+1)`2 - G*(i1,j1)`2
        by A55,A57,A79,GOBRD14:16;
A86:   dist(G*(i1,j1),G*(i1,j1+1)) = (N-bound C - S-bound C)/2|^k
        by A55,A57,A79,GOBRD14:19;
      dist(G*(i1,j1),G*(i1+1,j1)) = (E-bound C - W-bound C)/2|^k
        by A55,A83,GOBRD14:20;
      hence contradiction by A35,A43,A74,A75,A77,A78,A82,A84,A85,A86,XXREAL_0:2
;
      end;
      suppose
A87:     i1+1 = i2 & j1 = j2;
      then
A88:   i1+1 <= len G by A57,GOBOARD5:1;
      1 < j1 by A50,A55,A56,A57,A58,A87,JORDAN10:3;
      then
A89:   1 <= j1-'1 by BINARITH:67;
A90:   j1-'1 <= width G by A71,BINARITH:62;
A91:   j1-'1+1 = j1 by A89,BINARITH:61;
      right_cell(f,i,G) = cell(G,i1,j1-'1)
        by A50,A54,A55,A56,A57,A58,A61,A62,A87,GOBRD13:def 2;
      then
      G*(i1,j1-'1)`1 <= c`1 & c`1 <= G*(i1+1,j1-'1)`1 &
      G*(i1,j1-'1)`2 <= c`2 & c`2 <= G*(i1,j1-'1+1)`2 &
      G*(i1,j1-'1)`1 <= s`1 & s`1 <= G*(i1+1,j1-'1)`1 &
      G*(i1,j1-'1)`2 <= s`2 & s`2 <= G*(i1,j1-'1+1)`2
        by A52,A60,A64,A71,A88,A89,A91,JORDAN9:19;
      then
A92:   dist(s,c) <=
        (G*(i1+1,j1-'1)`1-G*(i1,j1-'1)`1) + (G*(i1,j1-'1+1)`2-G*(i1,j1-'1)`2)
          by TOPREAL6:104;
A93:   [i1,j1-'1] in Indices G by A64,A70,A89,A90,GOBOARD7:10;
A94:   [i1+1,j1-'1] in Indices G by A66,A72,A87,A89,A90,GOBOARD7:10;
      then
A95:   dist(G*(i1,j1-'1),G*(i1+1,j1-'1)) = G*(i1+1,j1-'1)`1 - G*(i1,j1-'1)`1
        by A93,GOBRD14:15;
A96:   dist(G*(i1,j1-'1),G*(i1,j1-'1+1)) = G*(i1,j1-'1+1)`2 - G*(i1,j1-'1)`2
        by A55,A91,A93,GOBRD14:16;
A97:   dist(G*(i1,j1-'1),G*(i1,j1-'1+1)) = (N-bound C - S-bound C)/2|^k
        by A55,A91,A93,GOBRD14:19;
      dist(G*(i1,j1-'1),G*(i1+1,j1-'1)) = (E-bound C - W-bound C)/2|^k
        by A93,A94,GOBRD14:20;
      hence contradiction by A35,A43,A74,A75,A77,A78,A92,A95,A96,A97,XXREAL_0:2
;
      end;
      suppose
A98:     i1 = i2+1 & j1 = j2;
      then
A99:   i2+1 <= len G by A55,GOBOARD5:1;
      j1 < width G by A50,A55,A56,A57,A58,A98,JORDAN10:4;
      then
A100:   j1+1 <= width G by NAT_1:13;
      right_cell(f,i,G) = cell(G,i2,j2)
        by A50,A54,A55,A56,A57,A58,A61,A62,A98,GOBRD13:def 2;
      then
      G*(i2,j2)`1 <= c`1 & c`1 <= G*(i2+1,j2)`1 &
      G*(i2,j2)`2 <= c`2 & c`2 <= G*(i2,j2+1)`2 &
      G*(i2,j2)`1 <= s`1 & s`1 <= G*(i2+1,j2)`1 &
      G*(i2,j2)`2 <= s`2 & s`2 <= G*(i2,j2+1)`2
        by A52,A60,A65,A66,A98,A99,A100,JORDAN9:19;
      then
A101:   dist(s,c) <=
        (G*(i2+1,j2)`1-G*(i2,j2)`1) + (G*(i2,j2+1)`2-G*(i2,j2)`2)
          by TOPREAL6:104;
A102:   dist(G*(i2,j2),G*(i2+1,j2)) = G*(i2+1,j2)`1 - G*(i2,j2)`1
        by A55,A57,A98,GOBRD14:15;
A103:   [i2,j2+1] in Indices G by A66,A69,A72,A98,A100,GOBOARD7:10;
      then
A104:   dist(G*(i2,j2),G*(i2,j2+1)) = G*(i2,j2+1)`2 - G*(i2,j2)`2
        by A57,GOBRD14:16;
A105:   dist(G*(i2,j2),G*(i2,j2+1)) = (N-bound C - S-bound C)/2|^k
        by A57,A103,GOBRD14:19;
      dist(G*(i2,j2),G*(i2+1,j2)) = (E-bound C - W-bound C)/2|^k
        by A55,A57,A98,GOBRD14:20;
      hence contradiction
        by A35,A43,A74,A75,A77,A78,A101,A102,A104,A105,XXREAL_0:2;
      end;
      suppose
A106:     i1 = i2 & j1 = j2+1;
      then
A107:   j2+1 <= width G by A55,GOBOARD5:1;
      1 < i1 by A50,A55,A56,A57,A58,A106,JORDAN10:2;
      then
A108:   1 <= i1-'1 by BINARITH:67;
A109:   i1-'1 <= len G by A70,BINARITH:62;
A110:   i1-'1+1 = i1 by A108,BINARITH:61;
      right_cell(f,i,G) = cell(G,i1-'1,j2)
        by A50,A54,A55,A56,A57,A58,A61,A63,A106,GOBRD13:def 2;
      then
      G*(i1-'1,j2)`1 <= c`1 & c`1 <= G*(i1-'1+1,j2)`1 &
      G*(i1-'1,j2)`2 <= c`2 & c`2 <= G*(i1-'1,j2+1)`2 &
      G*(i1-'1,j2)`1 <= s`1 & s`1 <= G*(i1-'1+1,j2)`1 &
      G*(i1-'1,j2)`2 <= s`2 & s`2 <= G*(i1-'1,j2+1)`2
        by A52,A60,A67,A70,A107,A108,A110,JORDAN9:19;
      then
A111:   dist(s,c) <=
        (G*(i1-'1+1,j2)`1-G*(i1-'1,j2)`1) + (G*(i1-'1,j2+1)`2-G*(i1-'1,j2)`2)
          by TOPREAL6:104;
A112:   [i1-'1,j2] in Indices G by A67,A73,A108,A109,GOBOARD7:10;
A113:   [i1-'1+1,j2] in Indices G by A64,A67,A70,A73,A110,GOBOARD7:10;
      then
A114:   dist(G*(i1-'1,j2),G*(i1-'1+1,j2)) = G*(i1-'1+1,j2)`1 - G*(i1-'1,j2)`1
        by A112,GOBRD14:15;
A115:   [i1-'1,j2+1] in Indices G by A65,A71,A106,A108,A109,GOBOARD7:10;
      then
A116:   dist(G*(i1-'1,j2),G*(i1-'1,j2+1)) = G*(i1-'1,j2+1)`2 - G*(i1-'1,j2)`2
        by A112,GOBRD14:16;
A117:   dist(G*(i1-'1,j2),G*(i1-'1,j2+1)) = (N-bound C - S-bound C)/2|^k
        by A112,A115,GOBRD14:19;
      dist(G*(i1-'1,j2),G*(i1-'1+1,j2)) = (E-bound C - W-bound C)/2|^k
        by A112,A113,GOBRD14:20;
      hence contradiction
        by A35,A43,A74,A75,A77,A78,A111,A114,A116,A117,XXREAL_0:2;
    end;
    end;
    hence thesis;
    end;

    suppose p = |[w,q]|;
    then
A118: p`2 = q by EUCLID:56;
A119: South_Arc C = Lim_inf Lower_Appr C by JORDAN19:def 4;
    reconsider e = p as Point of Euclid 2 by TOPMETR:16;

    ex S being Real_Sequence of 2 st S is convergent &
      (for x being Element of NAT holds S.x in (Lower_Appr C).x) & p = lim S
    proof
      set R = {(LMP Lower_Arc L~Cage(C,n))`2 where n is Element of NAT: 0 < n};
      R c= REAL
      proof
        let x be set;
        assume x in R;
        then ex n being Element of NAT
        st x = (LMP Lower_Arc L~Cage(C,n))`2 & 0 < n;
        hence thesis;
      end;
      then reconsider R as Subset of REAL;
A120:   (LMP Lower_Arc L~Cage(C,1))`2 in R;
A121:   R is bounded_above
      proof
        take q;
        let r be real number;
        assume r in R;
        then ex n being Element of NAT
        st r = (LMP Lower_Arc L~Cage(C,n))`2 & 0 < n;
        then r in proj2.:(H /\ U) by A13;
        hence q>=r by A20,SEQ_4:def 4;
      end;
      deffunc g(Element of NAT) = LMP Lower_Arc L~Cage(C,$1);
      consider S being Real_Sequence of 2 such that
A122:     for n being Element of NAT holds S.n = g(n) from TOPRNS_1:sch 1;
      take S;
A123:   for r being Real st 0<r
       ex n being Element of NAT st for m being Element of NAT st n<=m holds
        |. S.m-p .| < r
      proof
        let r be Real;
        assume
A124:       0 < r;
A125:     for r being real number st r in R holds q>=r
        proof
          let r be real number;
          assume r in R;
          then ex n being Element of NAT
          st r = (LMP Lower_Arc L~Cage(C,n))`2 & 0 < n;
          then r in proj2.:(H /\ U) by A13;
          hence thesis by A20,SEQ_4:def 4;
        end;
        for s being real number st 0<s
         ex r being real number st r in R & q-s<r
        proof
          let s be real number;
          assume 0<s;
          then consider r being real number such that
A126:         r in proj2.:(H /\ U) and
A127:         q-s<r by A20,A23,SEQ_4:def 4;
          take r;
          consider x being Point of TOP-REAL 2 such that
A128:         x in H /\ U and
A129:         proj2.x = r by A126,Lm1;
          x in U by A128,XBOOLE_0:def 3;
          then consider n being Element of NAT such that
A130:         x = LMP Lower_Arc L~Cage(C,n) and
A131:         0 < n;
          r = (LMP Lower_Arc L~Cage(C,n))`2 by A129,A130,PSCOMP_1:def 29;
          hence thesis by A127,A131;
        end;
        then
A132:     sup R = q by A120,A121,A125,SEQ_4:def 4;
        consider v being real number such that
A133:       v in R and
A134:       sup R - r < v by A120,A121,A124,SEQ_4:def 4;
        sup R - r + r < v + r by A134,XREAL_1:8;
        then
A135:   sup R - v < v + r - v by REAL_1:92;
        consider n being Element of NAT such that
A136:       v = (LMP Lower_Arc L~Cage(C,n))`2 and
A137:       0 < n by A133;
        take n;
        let m be Element of NAT;
A138:     S.m = LMP Lower_Arc L~Cage(C,m) by A122;
        assume
A139:       n <= m;
        then (LMP Lower_Arc L~Cage(C,n))`2 <= (LMP Lower_Arc L~Cage(C,m))`2
          by A137,Th30;
        then (S.m)`2 >= v by A122,A136;
        then p`2 - (S.m)`2 <= p`2 - v by REAL_1:92;
        then
A140:     p`2-(S.m)`2 < r by A118,A132,A135,XXREAL_0:2;
        0 < m by A137,A139;
        then (LMP C)`2 > (LMP Lower_Arc L~Cage(C,m))`2 by Th26;
        then
A141:     0 > (S.m)`2-p`2 by A138,REAL_2:105;
        (S.m)`1 = w by A11,A138;
        then abs((S.m)`1-p`1) = 0 by A8,ABSVALUE:def 1;
        then |. S.m-p .|<=0+abs((S.m)`2-p`2) by JGRAPH_1:49;
        then |. S.m-p .|<=-((S.m)`2-p`2) by A141,ABSVALUE:def 1;
        hence |. S.m-p .| < r by A140,XXREAL_0:2;
      end;
      thus
A142:   S is convergent
      proof
        take p;
        thus thesis by A123;
      end;
      hereby
        let x be Element of NAT;
A143:     S.x = LMP Lower_Arc L~Cage(C,x) by A122;
        (Lower_Appr C).x = Lower_Arc L~Cage(C,x) by JORDAN19:def 2;
        hence S.x in (Lower_Appr C).x by A143,Lm5;
      end;
      thus p = lim S by A123,A142,TOPRNS_1:def 10;
    end;
    hence thesis by A119,KURATO_2:58;
  end;
  end;

theorem Th37:
  North_Arc C c= C
  proof
    let x be set;
    assume
A1:   x in North_Arc C;
    assume
A2:   not x in C;
    reconsider x as Point of TOP-REAL 2 by A1;
    reconsider e = x as Point of Euclid 2 by TOPMETR:16;
A3: North_Arc C = Lim_inf Upper_Appr C by JORDAN19:def 3;
A4: x in C` by A2,SUBSET_1:50;
A5: (BDD C) \/ (UBD C) = C` by JORDAN2C:31;
    per cases by A4,A5,XBOOLE_0:def 2;
    suppose x in BDD C;
    then consider r being real number such that
A6:   r > 0 and
A7:   Ball(e,r) c= BDD C by TOPMETR:22;
    consider k being Element of NAT such that
A8:   for m being Element of NAT st m > k
holds (Upper_Appr C).m meets Ball(e,r)
        by A1,A3,A6,KURATO_2:50;
A9: k+0 < k+1 by XREAL_1:10;
    BDD C misses L~Cage(C,k+1) by JORDAN10:19;
    then
A10: Ball(e,r) misses L~Cage(C,k+1) by A7,XBOOLE_1:63;
A11: (Upper_Appr C).(k+1) = Upper_Arc L~Cage(C,k+1) by JORDAN19:def 1;
    Upper_Arc L~Cage(C,k+1) c= L~Cage(C,k+1) by JORDAN6:76;
    then
    (Upper_Appr C).(k+1) misses Ball(e,r) by A10,A11,XBOOLE_1:63;
    hence thesis by A8,A9;
    end;
    suppose
A12:   x in UBD C;
    union UBD-Family C = UBD C by JORDAN10:14;
    then consider A being set such that
A13:   x in A and
A14:   A in UBD-Family C by A12,TARSKI:def 4;
    UBD-Family C =
     {UBD L~Cage(C,m) where m is Element of NAT : not contradiction}
      by JORDAN10:def 1;
    then consider m being Element of NAT such that
A15:   A = UBD L~Cage(C,m) by A14;
    x in LeftComp Cage(C,m) by A13,A15,GOBRD14:46;
    then consider r being real number such that
A16:   r > 0 and
A17:   Ball(e,r) c= LeftComp Cage(C,m) by TOPMETR:22;
    consider k being Element of NAT such that
A18:   for m being Element of NAT st m > k
holds (Upper_Appr C).m meets Ball(e,r)
        by A1,A3,A16,KURATO_2:50;
    thus thesis
    proof
      per cases;
      suppose
A19:     m > k;
      LeftComp Cage(C,m) misses L~Cage(C,m) by SPRECT_3:43;
      then
A20:   Ball(e,r) misses L~Cage(C,m) by A17,XBOOLE_1:63;
A21:   (Upper_Appr C).m = Upper_Arc L~Cage(C,m) by JORDAN19:def 1;
      Upper_Arc L~Cage(C,m) c= L~Cage(C,m) by JORDAN6:76;
      then
      (Upper_Appr C).m misses Ball(e,r) by A20,A21,XBOOLE_1:63;
      hence thesis by A18,A19;
      end;
      suppose m <= k;
      then
A22:   LeftComp Cage(C,m) c= LeftComp Cage(C,k) by JORDAN1H:55;
A23:   k+0 < k+1 by XREAL_1:10;
      then
A24:   LeftComp Cage(C,k) c= LeftComp Cage(C,k+1) by JORDAN1H:55;
      Ball(e,r) c= LeftComp Cage(C,k) by A17,A22,XBOOLE_1:1;
      then
A25:   Ball(e,r) c= LeftComp Cage(C,k+1) by A24,XBOOLE_1:1;
      LeftComp Cage(C,k+1) misses L~Cage(C,k+1) by SPRECT_3:43;
      then
A26:   Ball(e,r) misses L~Cage(C,k+1) by A25,XBOOLE_1:63;
A27:   (Upper_Appr C).(k+1) = Upper_Arc L~Cage(C,k+1) by JORDAN19:def 1;
      Upper_Arc L~Cage(C,k+1) c= L~Cage(C,k+1) by JORDAN6:76;
      then
      (Upper_Appr C).(k+1) misses Ball(e,r) by A26,A27,XBOOLE_1:63;
      hence thesis by A18,A23;
    end;
  end;
  end;
  end;

theorem Th38:
