theorem Th86:
  UMP C in North_Arc C
  proof
    set w = (W-bound C + E-bound C) / 2;
    set p = UMP C;
    set U = {UMP Upper_Arc L~Cage(C,n) where n is Nat: 0 < n};
    {UMP Upper_Arc L~Cage(C,n) where n is Nat: 0 < n} c=
      the carrier of TOP-REAL 2
    proof
      let x be set;
      assume x in {UMP Upper_Arc L~Cage(C,n) where n is Nat: 0 < n};
      then ex n being Nat st x = UMP Upper_Arc L~Cage(C,n) & 0 < n;
      hence thesis;
    end;
    then reconsider U as Subset of TOP-REAL 2;
    set n0 = N-bound L~Cage(C,1);
    set H = LSeg(p,|[w,n0]|);
    set q = inf(proj2.:(H /\ U));
    set S = LSeg(|[w,q]|, |[w,n0]|);
A1: |[w,N-bound C]|`1 = w by EUCLID:56;
A2: |[w,N-bound C]|`2 = N-bound C by EUCLID:56;
A3: |[w,n0]|`1 = w by EUCLID:56;
A4: |[w,n0]|`2 = n0 by EUCLID:56;
A5: |[w,q]|`1 = w by EUCLID:56;
A6: |[w,q]|`2 = q by EUCLID:56;
A7: |[w,q]| in S by TOPREAL1:6;
A8: p`1 = w by JORDAN21:33;
A9: p = |[p`1,p`2]| by EUCLID:57;
A10: S is vertical by A5,A3,SPPOL_1:37;
A11: LSeg(p, |[w, N-bound C]|) /\ C = {p} by JORDAN21:47;
A12: for n being Nat holds (UMP Upper_Arc L~Cage(C,n))`1 = w
    proof
      let n be Nat;
A13:   W-bound L~Cage(C,n) + E-bound L~Cage(C,n) = W-bound C + E-bound C
        by JORDAN1G:41;
      thus (UMP Upper_Arc L~Cage(C,n))`1
         = (W-bound Upper_Arc L~Cage(C,n) + E-bound Upper_Arc L~Cage(C,n))/2
           by JORDAN21:33
        .= (W-bound L~Cage(C,n) + E-bound Upper_Arc L~Cage(C,n))/2
           by JORDAN21:26
        .= w by A13,JORDAN21:27;
    end;
A14: for n being Nat st 0 < n
       holds (UMP Upper_Arc L~Cage(C,n))`2 in proj2.:(H /\ U)
    proof
      let n be Nat;
      set f = Cage(C,n);
      set s = UMP Upper_Arc L~f;
A15:   s`2 <= N-bound L~f by JORDAN21:60;
      assume
A16:     0 < n;
      then 0+1 <= n by NAT_1:38;
      then 1 < n or n = 1 by REAL_1:def 5;
      then N-bound L~Cage(C,n) <= N-bound L~Cage(C,1) by JORDAN10:7;
      then
A17:   s`2 <= n0 by A15,AXIOMS:22;
A18:   s`1 = w by A12;
      p`2 <= s`2 by A16,JORDAN21:76;
      then
A19:   s in H by A8,A18,A3,A4,A17,GOBOARD7:8;
      s in U by A16;
      then
A20:   s in H /\ U by A19,XBOOLE_0:def 3;
      s`2 = proj2.s by PSCOMP_1:def 29;
      hence thesis by A20,FUNCT_2:43;
    end;
    H is Bounded by JORDAN2C:73;
    then H /\ U is Bounded by JORDAN1I:1;
    then proj2.:(H /\ U) is bounded by JORDAN1A:27;
    then
A21: proj2.:(H /\ U) is bounded_below by SEQ_4:def 3;
A22: (UMP Upper_Arc L~Cage(C,1))`2 <= n0 by JORDAN21:60;
A23: p`2 <= (UMP Upper_Arc L~Cage(C,1))`2 by JORDAN21:76;
A24: (UMP Upper_Arc L~Cage(C,1))`2 in proj2.:(H /\ U) by A14;
    then q <= (UMP Upper_Arc L~Cage(C,1))`2 by A21,SEQ_4:def 5;
    then
A25: q <= n0 by A22,AXIOMS:22;
    per cases;
    suppose
A26:   p <> |[w,q]|;
A27: now
      assume
A28:     p`2 >= q;
      per cases by A28,REAL_1:def 5;
      suppose p`2 = q;
      hence contradiction by A26,A9,JORDAN21:33;
      end;
      suppose p`2 > q;
      then 0 < p`2 - q by SQUARE_1:11;
      then consider r being real number such that
A29:     r in proj2.:(H /\ U) and
A30:     r < q+(p`2 - q) by A24,A21,SEQ_4:def 5;
      consider t being Point of TOP-REAL 2 such that
A32:     t in H /\ U and
A33:     proj2.t = r by A29,Lm3;
A34:   t`2 = r by A33,PSCOMP_1:def 29;
A35:   p`2 <= n0 by A22,A23,AXIOMS:22;
      t in H by A32,XBOOLE_0:def 3;
      hence contradiction by A30,A34,A4,A35,TOPREAL1:10;
    end;
    end;
    consider S',C' being Subset of TopSpaceMetr Euclid 2 such that
A36:   S = S' & C = C' and
A37:   dist_min(S,C) = min_dist_min(S',C') by JORDAN1K:def 1;
    S misses C
    proof
      assume S meets C;
      then consider x being set such that
A38:     x in S and
A39:     x in C by XBOOLE_0:3;
      reconsider x as Point of TOP-REAL 2 by A38;
A40:   x`2 <= N-bound C by A39,PSCOMP_1:71;
A41:   q <= x`2 by A6,A38,A25,A4,TOPREAL1:10;
      then
A42:   p`2 < x`2 by A27,AXIOMS:22;
      x`1 = w by A38,A10,A5,A7,SPPOL_1:def 3;
      then
      x in LSeg(p, |[w, N-bound C]|) by A1,A2,A8,A42,A40,GOBOARD7:8;
      then x in {p} by A11,A39,XBOOLE_0:def 3;
      hence contradiction by A41,A27,TARSKI:def 1;
    end;
    then dist_min(S,C) > 0 by A36,A37,Lm1,JGRAPH_1:55;
    then dist_min(S,C)/2 > 0 by SEQ_2:3;
    then consider k being Nat such that
A43:   1 < k and
A44:   dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < dist_min(S,C)/2 &
      dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < dist_min(S,C)/2 by GOBRD14:21;
    set f = Cage(C,k),
        G = Gauge(C,k);
A45: dist(G*(1,1),G*(1+1,1)) + dist(G*(1,1),G*(1,1+1))
      < dist_min(S,C)/2 + dist_min(S,C)/2 by A44,REAL_1:67;
    set s = UMP Upper_Arc L~f;
A46: s`1 = w by A12;
A47: s`2 <= N-bound L~f by JORDAN21:60;
    N-bound L~Cage(C,k) <= N-bound L~Cage(C,1) by A43,JORDAN10:7;
    then
A48: s`2 <= n0 by A47,AXIOMS:22;
    0 < k by A43;
    then s`2 in proj2.:(H /\ U) by A14;
    then
A49: q <= s`2 by A21,SEQ_4:def 5;
A50: s in S by A5,A6,A46,A48,A3,A4,A49,GOBOARD7:8;
A51: Upper_Arc L~f c= L~f by JORDAN1A:16;
    s in Upper_Arc L~f by JORDAN21:70;
    then consider i being Nat such that
A52:   1 <= i & i+1 <= len f and
A53:   s in LSeg(f,i) by A51,SPPOL_2:13;
    right_cell(f,i,G) meets C by A52,JORDAN9:33;
    then consider c being set such that
A54:   c in right_cell(f,i,G) and
A55:   c in C by XBOOLE_0:3;
    reconsider c as Point of TOP-REAL 2 by A55;
A56: f is_sequence_on G by JORDAN9:def 1;
    consider i1,j1,i2,j2 being Nat such that
A57:   [i1,j1] in Indices G and
A58:   f/.i = G*(i1,j1) and
A59:   [i2,j2] in Indices G and
A60:   f/.(i+1) = G*(i2,j2) and
A61:   i1 = i2 & j1+1 = j2 or i1+1 = i2 & j1 = j2 or
        i1 = i2+1 & j1 = j2 or i1 = i2 & j1 = j2+1 by A52,A56,JORDAN8:6;
    left_cell(f,i,G) /\ right_cell(f,i,G) = LSeg(f,i) by A56,A52,GOBRD13:30;
    then
A62: s in right_cell(f,i,G) by A53,XBOOLE_0:def 3;
A63: i1+1<>i1+0 by NAT_1:38;
A64: i2+1+1<>i2 by Lm4;
A65: j2+1+1<>j2 by Lm4;
A66: 1 <= i1 by A57,GOBOARD5:1;
A67: 1 <= j1 by A57,GOBOARD5:1;
A68: 1 <= i2 by A59,GOBOARD5:1;
A69: 1 <= j2 by A59,GOBOARD5:1;
A70: 1 <= i1+1 by NAT_1:29;
A71: 1 <= j1+1 by NAT_1:29;
A72: i1 <= len G by A57,GOBOARD5:1;
A73: j1 <= width G by A57,GOBOARD5:1;
A74: i2 <= len G by A59,GOBOARD5:1;
A75: j2 <= width G by A59,GOBOARD5:1;
    reconsider s' = s, c' = c as Point of Euclid 2 by Lm1,TOPMETR:16;
A76: min_dist_min(S',C') <= dist(s',c') by A50,A55,A36,Lm1,WEIERSTR:40;
A77: dist(s',c') = dist(s,c) by GOBRD14:def 1;
A78: [1,1] in Indices G by JORDAN21:14;
    [1,1+1] in Indices G by JORDAN21:15;
    then
A79: dist(G*(1,1),G*(1,1+1)) = (N-bound C - S-bound C)/2|^k by A78,GOBRD14:19;
    [1+1,1] in Indices G by JORDAN21:16;
    then
A80: dist(G*(1,1),G*(1+1,1)) = (E-bound C - W-bound C)/2|^k by A78,GOBRD14:20;
    now
      per cases by A61;
      suppose
A81:     i1 = i2 & j1+1 = j2;
      then
A82:   j1+1 <= width G by A59,GOBOARD5:1;
      i1 < len G by A81,A52,A57,A58,A59,A60,JORDAN10:1;
      then
A83:   i1+1 <= len G by NAT_1:38;
      right_cell(f,i,G) = cell(G,i1,j1)
        by A56,A52,A57,A58,A59,A60,A63,A65,A81,GOBRD13:def 2;
      then
      G*(i1,j1)`1 <= c`1 & c`1 <= G*(i1+1,j1)`1 &
      G*(i1,j1)`2 <= c`2 & c`2 <= G*(i1,j1+1)`2 &
      G*(i1,j1)`1 <= s`1 & s`1 <= G*(i1+1,j1)`1 &
      G*(i1,j1)`2 <= s`2 & s`2 <= G*(i1,j1+1)`2
        by A66,A83,A67,A82,A54,A62,JORDAN9:19;
      then
A84:   dist(s,c) <=
        (G*(i1+1,j1)`1-G*(i1,j1)`1) + (G*(i1,j1+1)`2-G*(i1,j1)`2)
          by GOBRD14:12;
A85:   [i1+1,j1] in Indices G by A83,A67,A70,A73,GOBOARD7:10;
      then
A86:   dist(G*(i1,j1),G*(i1+1,j1)) = G*(i1+1,j1)`1 - G*(i1,j1)`1
        by A57,GOBRD14:15;
A87:   dist(G*(i1,j1),G*(i1,j1+1)) = G*(i1,j1+1)`2 - G*(i1,j1)`2
        by A57,A59,A81,GOBRD14:16;
A88:   dist(G*(i1,j1),G*(i1,j1+1)) = (N-bound C - S-bound C)/2|^k
        by A57,A59,A81,GOBRD14:19;
      dist(G*(i1,j1),G*(i1+1,j1)) = (E-bound C - W-bound C)/2|^k
        by A57,A85,GOBRD14:20;
      hence contradiction by A37,A76,A77,A45,A88,A86,A87,A84,A79,A80,AXIOMS:22;
      end;
      suppose
A89:     i1+1 = i2 & j1 = j2;
      then
A90:   i1+1 <= len G by A59,GOBOARD5:1;
      1 < j1 by A89,A52,A57,A58,A59,A60,JORDAN10:3;
      then
A91:   1 <= j1-'1 by JORDAN3:12;
A92:   j1-'1 <= width G by A73,JORDAN3:7;
A93:   j1-'1+1 = j1 by A91,JORDAN3:6;
      right_cell(f,i,G) = cell(G,i1,j1-'1)
        by A89,A56,A52,A57,A58,A59,A60,A63,A64,GOBRD13:def 2;
      then
      G*(i1,j1-'1)`1 <= c`1 & c`1 <= G*(i1+1,j1-'1)`1 &
      G*(i1,j1-'1)`2 <= c`2 & c`2 <= G*(i1,j1-'1+1)`2 &
      G*(i1,j1-'1)`1 <= s`1 & s`1 <= G*(i1+1,j1-'1)`1 &
      G*(i1,j1-'1)`2 <= s`2 & s`2 <= G*(i1,j1-'1+1)`2
        by A66,A90,A54,A62,A91,A93,A73,JORDAN9:19;
      then
A94:   dist(s,c) <=
        (G*(i1+1,j1-'1)`1-G*(i1,j1-'1)`1) + (G*(i1,j1-'1+1)`2-G*(i1,j1-'1)`2)
          by GOBRD14:12;
A95:   [i1,j1-'1] in Indices G by A66,A72,A91,A92,GOBOARD7:10;
A96:   [i1+1,j1-'1] in Indices G by A89,A68,A74,A92,A91,GOBOARD7:10;
      then
A97:   dist(G*(i1,j1-'1),G*(i1+1,j1-'1)) = G*(i1+1,j1-'1)`1 - G*(i1,j1-'1)`1
        by A95,GOBRD14:15;
A98:   dist(G*(i1,j1-'1),G*(i1,j1-'1+1)) = G*(i1,j1-'1+1)`2 - G*(i1,j1-'1)`2
        by A57,A93,A95,GOBRD14:16;
A99:   dist(G*(i1,j1-'1),G*(i1,j1-'1+1)) = (N-bound C - S-bound C)/2|^k
        by A57,A93,A95,GOBRD14:19;
      dist(G*(i1,j1-'1),G*(i1+1,j1-'1)) = (E-bound C - W-bound C)/2|^k
        by A96,A95,GOBRD14:20;
      hence contradiction by A37,A76,A77,A45,A99,A97,A98,A94,A79,A80,AXIOMS:22;
      end;
      suppose
A100:     i1 = i2+1 & j1 = j2;
      then
A101:   i2+1 <= len G by A57,GOBOARD5:1;
      j1 < width G by A100,A52,A57,A58,A59,A60,JORDAN10:4;
      then
A102:   j1+1 <= width G by NAT_1:38;
      right_cell(f,i,G) = cell(G,i2,j2)
        by A100,A56,A52,A57,A58,A59,A60,A63,A64,GOBRD13:def 2;
      then
      G*(i2,j2)`1 <= c`1 & c`1 <= G*(i2+1,j2)`1 &
      G*(i2,j2)`2 <= c`2 & c`2 <= G*(i2,j2+1)`2 &
      G*(i2,j2)`1 <= s`1 & s`1 <= G*(i2+1,j2)`1 &
      G*(i2,j2)`2 <= s`2 & s`2 <= G*(i2,j2+1)`2
        by A100,A102,A67,A101,A54,A62,A68,JORDAN9:19;
      then
A103:   dist(s,c) <=
        (G*(i2+1,j2)`1-G*(i2,j2)`1) + (G*(i2,j2+1)`2-G*(i2,j2)`2)
          by GOBRD14:12;
A104:   dist(G*(i2,j2),G*(i2+1,j2)) = G*(i2+1,j2)`1 - G*(i2,j2)`1
        by A100,A57,A59,GOBRD14:15;
A105:   [i2,j2+1] in Indices G by A100,A102,A71,A68,A74,GOBOARD7:10;
      then
A106:   dist(G*(i2,j2),G*(i2,j2+1)) = G*(i2,j2+1)`2 - G*(i2,j2)`2
        by A59,GOBRD14:16;
A107:   dist(G*(i2,j2),G*(i2,j2+1)) = (N-bound C - S-bound C)/2|^k
        by A59,A105,GOBRD14:19;
      dist(G*(i2,j2),G*(i2+1,j2)) = (E-bound C - W-bound C)/2|^k
        by A59,A100,A57,GOBRD14:20;
      hence contradiction
        by A37,A76,A77,A45,A107,A104,A106,A103,A79,A80,AXIOMS:22;
      end;
      suppose
A108:     i1 = i2 & j1 = j2+1;
      then
A109:   j2+1 <= width G by A57,GOBOARD5:1;
      1 < i1 by A108,A52,A57,A58,A59,A60,JORDAN10:2;
      then
A110:   1 <= i1-'1 by JORDAN3:12;
A111:   i1-'1 <= len G by A72,JORDAN3:7;
A112:   i1-'1+1 = i1 by A110,JORDAN3:6;
      right_cell(f,i,G) = cell(G,i1-'1,j2)
        by A108,A56,A52,A57,A58,A59,A60,A63,A65,GOBRD13:def 2;
      then
      G*(i1-'1,j2)`1 <= c`1 & c`1 <= G*(i1-'1+1,j2)`1 &
      G*(i1-'1,j2)`2 <= c`2 & c`2 <= G*(i1-'1,j2+1)`2 &
      G*(i1-'1,j2)`1 <= s`1 & s`1 <= G*(i1-'1+1,j2)`1 &
      G*(i1-'1,j2)`2 <= s`2 & s`2 <= G*(i1-'1,j2+1)`2
        by A109,A54,A62,A110,A112,A72,A69,JORDAN9:19;
      then
A113:   dist(s,c) <=
        (G*(i1-'1+1,j2)`1-G*(i1-'1,j2)`1) + (G*(i1-'1,j2+1)`2-G*(i1-'1,j2)`2)
          by GOBRD14:12;
A114:   [i1-'1,j2] in Indices G by A110,A111,A69,A75,GOBOARD7:10;
A115:   [i1-'1+1,j2] in Indices G by A69,A75,A112,A66,A72,GOBOARD7:10;
      then
A116:   dist(G*(i1-'1,j2),G*(i1-'1+1,j2)) = G*(i1-'1+1,j2)`1 - G*(i1-'1,j2)`1
        by A114,GOBRD14:15;
A117:   [i1-'1,j2+1] in Indices G by A67,A73,A110,A111,A108,GOBOARD7:10;
      then
A118:   dist(G*(i1-'1,j2),G*(i1-'1,j2+1)) = G*(i1-'1,j2+1)`2 - G*(i1-'1,j2)`2
        by A114,GOBRD14:16;
A119:   dist(G*(i1-'1,j2),G*(i1-'1,j2+1)) = (N-bound C - S-bound C)/2|^k
        by A117,A114,GOBRD14:19;
      dist(G*(i1-'1,j2),G*(i1-'1+1,j2)) = (E-bound C - W-bound C)/2|^k
        by A115,A114,GOBRD14:20;
      hence contradiction
        by A37,A76,A77,A45,A119,A116,A118,A113,A79,A80,AXIOMS:22;
    end;
    end;
    hence thesis;
    end;

    suppose p = |[w,q]|;
    then
A120: p`2 = q by EUCLID:56;
A121: North_Arc C = Lim_inf Upper_Appr C by JORDAN19:def 3;
    reconsider e = p as Point of Euclid 2 by Lm1,TOPMETR:16;

    ex S being Real_Sequence of 2 st S is convergent &
      (for x being Nat holds S.x in (Upper_Appr C).x) & p = lim S
    proof
      set R = {(UMP Upper_Arc L~Cage(C,n))`2 where n is Nat: 0 < n};
      R c= REAL
      proof
        let x be set;
        assume x in R;
        then ex n being Nat st x = (UMP Upper_Arc L~Cage(C,n))`2 & 0 < n;
        hence thesis;
      end;
      then reconsider R as Subset of REAL;
A122:   (UMP Upper_Arc L~Cage(C,1))`2 in R;
A123:   R is bounded_below
      proof
        take q;
        let r be real number;
        assume r in R;
        then ex n being Nat st r = (UMP Upper_Arc L~Cage(C,n))`2 & 0 < n;
        then r in proj2.:(H /\ U) by A14;
        hence q<=r by A21,SEQ_4:def 5;
      end;
      deffunc f(Nat) = UMP Upper_Arc L~Cage(C,$1);
      consider S being Real_Sequence of 2 such that
A124:     for n being Nat holds S.n = f(n) from TOPRNS_1:sch 1;
      take S;
A125:   for r being Real st 0<r
       ex n being Nat st for m being Nat st n<=m holds
        |. S.m-p .| < r
      proof
        let r be Real;
        assume
A126:       0 < r;
A127:     for r being real number st r in R holds q<=r
        proof
          let r be real number;
          assume r in R;
          then ex n being Nat st r = (UMP Upper_Arc L~Cage(C,n))`2 & 0 < n;
          then r in proj2.:(H /\ U) by A14;
          hence thesis by A21,SEQ_4:def 5;
        end;
        for s being real number st 0<s
         ex r being real number st r in R & r<q+s
        proof
          let s be real number;
          assume 0<s;
          then consider r being real number such that
A128:         r in proj2.:(H /\ U) and
A129:         r<q+s by A24,A21,SEQ_4:def 5;
          take r;
          consider x being Point of TOP-REAL 2 such that
A130:         x in H /\ U and
A131:         proj2.x = r by A128,Lm3;
          x in U by A130,XBOOLE_0:def 3;
          then consider n being Nat such that
A132:         x = UMP Upper_Arc L~Cage(C,n) and
A133:         0 < n;
          r = (UMP Upper_Arc L~Cage(C,n))`2 by A131,A132,PSCOMP_1:def 29;
          hence thesis by A129,A133;
        end;
        then
A134:     inf R = q by A127,A123,A122,SEQ_4:def 5;
        consider v being real number such that
A135:       v in R and
A136:       v < inf R + r by A126,A122,A123,SEQ_4:def 5;
A137:   v - inf R < inf R + r - inf R by A136,REAL_1:92;
        consider n being Nat such that
A138:       v = (UMP Upper_Arc L~Cage(C,n))`2 and
A139:       0 < n by A135;
        take n;
        let m be Nat;
A140:     S.m = UMP Upper_Arc L~Cage(C,m) by A124;
        assume
A141:       n <= m;
        then (UMP Upper_Arc L~Cage(C,m))`2 <= (UMP Upper_Arc L~Cage(C,n))`2
          by A139,JORDAN21:80;
        then (S.m)`2 <= v by A124,A138;
        then (S.m)`2 - p`2 <= v - p`2 by REAL_1:92;
        then
A142:     (S.m)`2-p`2 < r by A120,A137,A134,AXIOMS:22;
        0 < m by A139,A141;
        then (UMP C)`2 < (UMP Upper_Arc L~Cage(C,m))`2 by JORDAN21:76;
        then
A143:     0 < (S.m)`2-p`2 by A140,SQUARE_1:11;
        (S.m)`1 = w by A140,A12;
        then abs((S.m)`1-p`1) = 0 by A8,ABSVALUE:def 1;
        then |. S.m-p .|<=0+abs((S.m)`2-p`2) by JGRAPH_1:49;
        then |. S.m-p .|<=(S.m)`2-p`2 by A143,ABSVALUE:def 1;
        hence |. S.m-p .| < r by A142,AXIOMS:22;
      end;
      thus
A144:     S is convergent
      proof
        take p;
        thus thesis by A125;
      end;
      hereby
        let x be Nat;
A145:     S.x = UMP Upper_Arc L~Cage(C,x) by A124;
        (Upper_Appr C).x = Upper_Arc L~Cage(C,x) by JORDAN19:def 1;
        hence S.x in (Upper_Appr C).x by A145,JORDAN21:70;
      end;
      thus p = lim S by A144,A125,TOPRNS_1:def 10;
    end;
    hence thesis by A121,KURATO_2:58;
  end;
  end;

theorem Th87:
  LMP C in South_Arc C
  proof
    set w = (W-bound C + E-bound C) / 2;
    set p = LMP C;
    set U = {LMP Lower_Arc L~Cage(C,n) where n is Nat: 0 < n};
    {LMP Lower_Arc L~Cage(C,n) where n is Nat: 0 < n} c=
      the carrier of TOP-REAL 2
    proof
      let x be set;
      assume x in {LMP Lower_Arc L~Cage(C,n) where n is Nat: 0 < n};
      then ex n being Nat st x = LMP Lower_Arc L~Cage(C,n) & 0 < n;
      hence thesis;
    end;
    then reconsider U as Subset of TOP-REAL 2;
    set n0 = S-bound L~Cage(C,1);
    set H = LSeg(p,|[w,n0]|);
    set q = sup(proj2.:(H /\ U));
    set S = LSeg(|[w,q]|, |[w,n0]|);
A1: |[w,S-bound C]|`1 = w by EUCLID:56;
A2: |[w,S-bound C]|`2 = S-bound C by EUCLID:56;
A3: |[w,n0]|`1 = w by EUCLID:56;
A4: |[w,n0]|`2 = n0 by EUCLID:56;
A5: |[w,q]|`1 = w by EUCLID:56;
A6: |[w,q]|`2 = q by EUCLID:56;
A7: |[w,q]| in S by TOPREAL1:6;
A8: p`1 = w by JORDAN21:35;
A9: p = |[p`1,p`2]| by EUCLID:57;
A10: S is vertical by A5,A3,SPPOL_1:37;
A11: LSeg(p, |[w, S-bound C]|) /\ C = {p} by JORDAN21:48;
A12: for n being Nat holds (LMP Lower_Arc L~Cage(C,n))`1 = w
    proof
      let n be Nat;
A13:   W-bound L~Cage(C,n) + E-bound L~Cage(C,n) = W-bound C + E-bound C
        by JORDAN1G:41;
      thus (LMP Lower_Arc L~Cage(C,n))`1
         = (W-bound Lower_Arc L~Cage(C,n) + E-bound Lower_Arc L~Cage(C,n))/2
           by JORDAN21:35
        .= (W-bound L~Cage(C,n) + E-bound Lower_Arc L~Cage(C,n))/2
           by JORDAN21:28
        .= w by A13,JORDAN21:29;
    end;
A14: for n being Nat st 0 < n holds
      (LMP Lower_Arc L~Cage(C,n))`2 in proj2.:(H /\ U)
    proof
      let n be Nat;
      set f = Cage(C,n);
      set s = LMP Lower_Arc L~f;
A15:   S-bound L~f <= s`2 by JORDAN21:61;
      assume
A16:     0 < n;
      then 0+1 <= n by NAT_1:38;
      then n = 1 or n > 1 by REAL_1:def 5;
      then S-bound L~Cage(C,n) >= S-bound L~Cage(C,1) by JORDAN1A:90;
      then
A17:   s`2 >= n0 by A15,AXIOMS:22;
A18:   s`1 = w by A12;
      p`2 >= s`2 by A16,JORDAN21:77;
      then
A19:   s in H by A8,A18,A3,A4,A17,GOBOARD7:8;
      s in U by A16;
      then
A20:   s in H /\ U by A19,XBOOLE_0:def 3;
      s`2 = proj2.s by PSCOMP_1:def 29;
      hence thesis by A20,FUNCT_2:43;
    end;
    H is Bounded by JORDAN2C:73;
    then H /\ U is Bounded by JORDAN1I:1;
    then proj2.:(H /\ U) is bounded by JORDAN1A:27;
    then
A21: proj2.:(H /\ U) is bounded_above by SEQ_4:def 3;
A22: (LMP Lower_Arc L~Cage(C,1))`2 >= n0 by JORDAN21:61;
A23: p`2 >= (LMP Lower_Arc L~Cage(C,1))`2 by JORDAN21:77;
A24: (LMP Lower_Arc L~Cage(C,1))`2 in proj2.:(H /\ U) by A14;
    then q >= (LMP Lower_Arc L~Cage(C,1))`2 by A21,SEQ_4:def 4;
    then
A25: q >= n0 by A22,AXIOMS:22;
    per cases;
    suppose
A26:   p <> |[w,q]|;
A27: now
      assume
A28:     p`2 <= q;
      per cases by A28,REAL_1:def 5;
      suppose p`2 = q;
      hence contradiction by A26,A9,JORDAN21:35;
      end;
      suppose p`2 < q;
      then 0 < q - p`2 by SQUARE_1:11;
      then consider r being real number such that
A29:     r in proj2.:(H /\ U) and
A30:     q-(q-p`2) < r by A24,A21,SEQ_4:def 4;
      consider t being Point of TOP-REAL 2 such that
A32:     t in H /\ U and
A33:     proj2.t = r by A29,Lm3;
A34:   t`2 = r by A33,PSCOMP_1:def 29;
A35:   p`2 >= n0 by A22,A23,AXIOMS:22;
      t in H by A32,XBOOLE_0:def 3;
      hence contradiction by A30,A34,A4,A35,TOPREAL1:10;
    end;
    end;
    consider S',C' being Subset of TopSpaceMetr Euclid 2 such that
A36:   S = S' & C = C' and
A37:   dist_min(S,C) = min_dist_min(S',C') by JORDAN1K:def 1;
    S misses C
    proof
      assume S meets C;
      then consider x being set such that
A38:     x in S and
A39:     x in C by XBOOLE_0:3;
      reconsider x as Point of TOP-REAL 2 by A38;
A40:   x`2 >= S-bound C by A39,PSCOMP_1:71;
A41:   q >= x`2 by A6,A38,A25,A4,TOPREAL1:10;
      then
A42:   p`2 > x`2 by A27,AXIOMS:22;
      x`1 = w by A38,A10,A5,A7,SPPOL_1:def 3;
      then
      x in LSeg(p, |[w, S-bound C]|) by A1,A2,A8,A42,A40,GOBOARD7:8;
      then x in {p} by A11,A39,XBOOLE_0:def 3;
      hence contradiction by A41,A27,TARSKI:def 1;
    end;
    then dist_min(S,C) > 0 by A36,A37,Lm1,JGRAPH_1:55;
    then dist_min(S,C)/2 > 0 by SEQ_2:3;
    then consider k being Nat such that
A43:   1 < k and
A44:   dist(Gauge(C,k)*(1,1),Gauge(C,k)*(1,2)) < dist_min(S,C)/2 &
      dist(Gauge(C,k)*(1,1),Gauge(C,k)*(2,1)) < dist_min(S,C)/2 by GOBRD14:21;
    set f = Cage(C,k),
        G = Gauge(C,k);
A45: dist(G*(1,1),G*(1+1,1)) + dist(G*(1,1),G*(1,1+1))
      < dist_min(S,C)/2 + dist_min(S,C)/2 by A44,REAL_1:67;
    set s = LMP Lower_Arc L~f;
A46: s`1 = w by A12;
A47: s`2 >= S-bound L~f by JORDAN21:61;
    S-bound L~Cage(C,k) >= S-bound L~Cage(C,1) by A43,JORDAN1A:90;
    then
A48: s`2 >= n0 by A47,AXIOMS:22;
    0 < k by A43;
    then s`2 in proj2.:(H /\ U) by A14;
    then
A49: q >= s`2 by A21,SEQ_4:def 4;
A50: s in S by A5,A6,A46,A48,A3,A4,A49,GOBOARD7:8;
A51: Lower_Arc L~f c= L~f by JORDAN1A:16;
    s in Lower_Arc L~f by JORDAN21:71;
    then consider i being Nat such that
A52:   1 <= i & i+1 <= len f and
A53:   s in LSeg(f,i) by A51,SPPOL_2:13;
    right_cell(f,i,G) meets C by A52,JORDAN9:33;
    then consider c being set such that
A54:   c in right_cell(f,i,G) and
A55:   c in C by XBOOLE_0:3;
    reconsider c as Point of TOP-REAL 2 by A55;
A56: f is_sequence_on G by JORDAN9:def 1;
    consider i1,j1,i2,j2 being Nat such that
A57:   [i1,j1] in Indices G and
A58:   f/.i = G*(i1,j1) and
A59:   [i2,j2] in Indices G and
A60:   f/.(i+1) = G*(i2,j2) and
A61:   i1 = i2 & j1+1 = j2 or i1+1 = i2 & j1 = j2 or
        i1 = i2+1 & j1 = j2 or i1 = i2 & j1 = j2+1 by A52,A56,JORDAN8:6;
    left_cell(f,i,G) /\ right_cell(f,i,G) = LSeg(f,i) by A56,A52,GOBRD13:30;
    then
A62: s in right_cell(f,i,G) by A53,XBOOLE_0:def 3;
A63: i1+1<>i1+0 by NAT_1:38;
A64: i2+1+1<>i2 by Lm4;
A65: j2+1+1<>j2 by Lm4;
A66: 1 <= i1 by A57,GOBOARD5:1;
A67: 1 <= j1 by A57,GOBOARD5:1;
A68: 1 <= i2 by A59,GOBOARD5:1;
A69: 1 <= j2 by A59,GOBOARD5:1;
A70: 1 <= i1+1 by NAT_1:29;
A71: 1 <= j1+1 by NAT_1:29;
A72: i1 <= len G by A57,GOBOARD5:1;
A73: j1 <= width G by A57,GOBOARD5:1;
A74: i2 <= len G by A59,GOBOARD5:1;
A75: j2 <= width G by A59,GOBOARD5:1;
    reconsider s' = s, c' = c as Point of Euclid 2 by Lm1,TOPMETR:16;
A76: min_dist_min(S',C') <= dist(s',c') by A50,A55,A36,Lm1,WEIERSTR:40;
A77: dist(s',c') = dist(s,c) by GOBRD14:def 1;
A78: [1,1] in Indices G by JORDAN21:14;
    [1,1+1] in Indices G by JORDAN21:15;
    then
A79: dist(G*(1,1),G*(1,1+1)) = (N-bound C - S-bound C)/2|^k by A78,GOBRD14:19;
    [1+1,1] in Indices G by JORDAN21:16;
    then
A80: dist(G*(1,1),G*(1+1,1)) = (E-bound C - W-bound C)/2|^k by A78,GOBRD14:20;
    now
      per cases by A61;
      suppose
A81:     i1 = i2 & j1+1 = j2;
      then
A82:   j1+1 <= width G by A59,GOBOARD5:1;
      i1 < len G by A81,A52,A57,A58,A59,A60,JORDAN10:1;
      then
A83:   i1+1 <= len G by NAT_1:38;
      right_cell(f,i,G) = cell(G,i1,j1)
        by A56,A52,A57,A58,A59,A60,A63,A65,A81,GOBRD13:def 2;
      then
      G*(i1,j1)`1 <= c`1 & c`1 <= G*(i1+1,j1)`1 &
      G*(i1,j1)`2 <= c`2 & c`2 <= G*(i1,j1+1)`2 &
      G*(i1,j1)`1 <= s`1 & s`1 <= G*(i1+1,j1)`1 &
      G*(i1,j1)`2 <= s`2 & s`2 <= G*(i1,j1+1)`2
        by A66,A83,A67,A82,A54,A62,JORDAN9:19;
      then
A84:   dist(s,c) <=
        (G*(i1+1,j1)`1-G*(i1,j1)`1) + (G*(i1,j1+1)`2-G*(i1,j1)`2)
          by GOBRD14:12;
A85:   [i1+1,j1] in Indices G by A83,A67,A70,A73,GOBOARD7:10;
      then
A86:   dist(G*(i1,j1),G*(i1+1,j1)) = G*(i1+1,j1)`1 - G*(i1,j1)`1
        by A57,GOBRD14:15;
A87:   dist(G*(i1,j1),G*(i1,j1+1)) = G*(i1,j1+1)`2 - G*(i1,j1)`2
        by A57,A59,A81,GOBRD14:16;
A88:   dist(G*(i1,j1),G*(i1,j1+1)) = (N-bound C - S-bound C)/2|^k
        by A57,A59,A81,GOBRD14:19;
      dist(G*(i1,j1),G*(i1+1,j1)) = (E-bound C - W-bound C)/2|^k
        by A57,A85,GOBRD14:20;
      hence contradiction by A37,A76,A77,A45,A88,A86,A87,A84,A79,A80,AXIOMS:22;
      end;
      suppose
A89:     i1+1 = i2 & j1 = j2;
      then
A90:   i1+1 <= len G by A59,GOBOARD5:1;
      1 < j1 by A89,A52,A57,A58,A59,A60,JORDAN10:3;
      then
A91:   1 <= j1-'1 by JORDAN3:12;
A92:   j1-'1 <= width G by A73,JORDAN3:7;
A93:   j1-'1+1 = j1 by A91,JORDAN3:6;
      right_cell(f,i,G) = cell(G,i1,j1-'1)
        by A89,A56,A52,A57,A58,A59,A60,A63,A64,GOBRD13:def 2;
      then
      G*(i1,j1-'1)`1 <= c`1 & c`1 <= G*(i1+1,j1-'1)`1 &
      G*(i1,j1-'1)`2 <= c`2 & c`2 <= G*(i1,j1-'1+1)`2 &
      G*(i1,j1-'1)`1 <= s`1 & s`1 <= G*(i1+1,j1-'1)`1 &
      G*(i1,j1-'1)`2 <= s`2 & s`2 <= G*(i1,j1-'1+1)`2
        by A66,A90,A54,A62,A91,A93,A73,JORDAN9:19;
      then
A94:   dist(s,c) <=
        (G*(i1+1,j1-'1)`1-G*(i1,j1-'1)`1) + (G*(i1,j1-'1+1)`2-G*(i1,j1-'1)`2)
          by GOBRD14:12;
A95:   [i1,j1-'1] in Indices G by A66,A72,A91,A92,GOBOARD7:10;
A96:   [i1+1,j1-'1] in Indices G by A89,A68,A74,A92,A91,GOBOARD7:10;
      then
A97:   dist(G*(i1,j1-'1),G*(i1+1,j1-'1)) = G*(i1+1,j1-'1)`1 - G*(i1,j1-'1)`1
        by A95,GOBRD14:15;
A98:   dist(G*(i1,j1-'1),G*(i1,j1-'1+1)) = G*(i1,j1-'1+1)`2 - G*(i1,j1-'1)`2
        by A57,A93,A95,GOBRD14:16;
A99:   dist(G*(i1,j1-'1),G*(i1,j1-'1+1)) = (N-bound C - S-bound C)/2|^k
        by A57,A93,A95,GOBRD14:19;
      dist(G*(i1,j1-'1),G*(i1+1,j1-'1)) = (E-bound C - W-bound C)/2|^k
        by A96,A95,GOBRD14:20;
      hence contradiction by A37,A76,A77,A45,A99,A97,A98,A94,A79,A80,AXIOMS:22;
      end;
      suppose
A100:     i1 = i2+1 & j1 = j2;
      then
A101:   i2+1 <= len G by A57,GOBOARD5:1;
      j1 < width G by A100,A52,A57,A58,A59,A60,JORDAN10:4;
      then
A102:   j1+1 <= width G by NAT_1:38;
      right_cell(f,i,G) = cell(G,i2,j2)
        by A100,A56,A52,A57,A58,A59,A60,A63,A64,GOBRD13:def 2;
      then
      G*(i2,j2)`1 <= c`1 & c`1 <= G*(i2+1,j2)`1 &
      G*(i2,j2)`2 <= c`2 & c`2 <= G*(i2,j2+1)`2 &
      G*(i2,j2)`1 <= s`1 & s`1 <= G*(i2+1,j2)`1 &
      G*(i2,j2)`2 <= s`2 & s`2 <= G*(i2,j2+1)`2
        by A100,A102,A67,A101,A54,A62,A68,JORDAN9:19;
      then
A103:   dist(s,c) <=
        (G*(i2+1,j2)`1-G*(i2,j2)`1) + (G*(i2,j2+1)`2-G*(i2,j2)`2)
          by GOBRD14:12;
A104:   dist(G*(i2,j2),G*(i2+1,j2)) = G*(i2+1,j2)`1 - G*(i2,j2)`1
        by A100,A57,A59,GOBRD14:15;
A105:   [i2,j2+1] in Indices G by A100,A102,A71,A68,A74,GOBOARD7:10;
      then
A106:   dist(G*(i2,j2),G*(i2,j2+1)) = G*(i2,j2+1)`2 - G*(i2,j2)`2
        by A59,GOBRD14:16;
A107:   dist(G*(i2,j2),G*(i2,j2+1)) = (N-bound C - S-bound C)/2|^k
        by A59,A105,GOBRD14:19;
      dist(G*(i2,j2),G*(i2+1,j2)) = (E-bound C - W-bound C)/2|^k
        by A59,A100,A57,GOBRD14:20;
      hence contradiction
        by A37,A76,A77,A45,A107,A104,A106,A103,A79,A80,AXIOMS:22;
      end;
      suppose
A108:     i1 = i2 & j1 = j2+1;
      then
A109:   j2+1 <= width G by A57,GOBOARD5:1;
      1 < i1 by A108,A52,A57,A58,A59,A60,JORDAN10:2;
      then
A110:   1 <= i1-'1 by JORDAN3:12;
A111:   i1-'1 <= len G by A72,JORDAN3:7;
A112:   i1-'1+1 = i1 by A110,JORDAN3:6;
      right_cell(f,i,G) = cell(G,i1-'1,j2)
        by A108,A56,A52,A57,A58,A59,A60,A63,A65,GOBRD13:def 2;
      then
      G*(i1-'1,j2)`1 <= c`1 & c`1 <= G*(i1-'1+1,j2)`1 &
      G*(i1-'1,j2)`2 <= c`2 & c`2 <= G*(i1-'1,j2+1)`2 &
      G*(i1-'1,j2)`1 <= s`1 & s`1 <= G*(i1-'1+1,j2)`1 &
      G*(i1-'1,j2)`2 <= s`2 & s`2 <= G*(i1-'1,j2+1)`2
        by A109,A54,A62,A110,A112,A72,A69,JORDAN9:19;
      then
A113:   dist(s,c) <=
        (G*(i1-'1+1,j2)`1-G*(i1-'1,j2)`1) + (G*(i1-'1,j2+1)`2-G*(i1-'1,j2)`2)
          by GOBRD14:12;
A114:   [i1-'1,j2] in Indices G by A110,A111,A69,A75,GOBOARD7:10;
A115:   [i1-'1+1,j2] in Indices G by A69,A75,A112,A66,A72,GOBOARD7:10;
      then
A116:   dist(G*(i1-'1,j2),G*(i1-'1+1,j2)) = G*(i1-'1+1,j2)`1 - G*(i1-'1,j2)`1
        by A114,GOBRD14:15;
A117:   [i1-'1,j2+1] in Indices G by A67,A73,A110,A111,A108,GOBOARD7:10;
      then
A118:   dist(G*(i1-'1,j2),G*(i1-'1,j2+1)) = G*(i1-'1,j2+1)`2 - G*(i1-'1,j2)`2
        by A114,GOBRD14:16;
A119:   dist(G*(i1-'1,j2),G*(i1-'1,j2+1)) = (N-bound C - S-bound C)/2|^k
        by A117,A114,GOBRD14:19;
      dist(G*(i1-'1,j2),G*(i1-'1+1,j2)) = (E-bound C - W-bound C)/2|^k
        by A115,A114,GOBRD14:20;
      hence contradiction
        by A37,A76,A77,A45,A119,A116,A118,A113,A79,A80,AXIOMS:22;
    end;
    end;
    hence thesis;
    end;

    suppose p = |[w,q]|;
    then
A120: p`2 = q by EUCLID:56;
A121: South_Arc C = Lim_inf Lower_Appr C by JORDAN19:def 4;
    reconsider e = p as Point of Euclid 2 by Lm1,TOPMETR:16;

    ex S being Real_Sequence of 2 st S is convergent &
      (for x being Nat holds S.x in (Lower_Appr C).x) & p = lim S
    proof
      set R = {(LMP Lower_Arc L~Cage(C,n))`2 where n is Nat: 0 < n};
      R c= REAL
      proof
        let x be set;
        assume x in R;
        then ex n being Nat st x = (LMP Lower_Arc L~Cage(C,n))`2 & 0 < n;
        hence thesis;
      end;
      then reconsider R as Subset of REAL;
A122:   (LMP Lower_Arc L~Cage(C,1))`2 in R;
A123:   R is bounded_above
      proof
        take q;
        let r be real number;
        assume r in R;
        then ex n being Nat st r = (LMP Lower_Arc L~Cage(C,n))`2 & 0 < n;
        then r in proj2.:(H /\ U) by A14;
        hence q>=r by A21,SEQ_4:def 4;
      end;
      deffunc g(Nat) = LMP Lower_Arc L~Cage(C,$1);
      consider S being Real_Sequence of 2 such that
A124:     for n being Nat holds S.n = g(n) from TOPRNS_1:sch 1;
      take S;
A125:   for r being Real st 0<r
       ex n being Nat st for m being Nat st n<=m holds
        |. S.m-p .| < r
      proof
        let r be Real;
        assume
A126:       0 < r;
A127:     for r being real number st r in R holds q>=r
        proof
          let r be real number;
          assume r in R;
          then ex n being Nat st r = (LMP Lower_Arc L~Cage(C,n))`2 & 0 < n;
          then r in proj2.:(H /\ U) by A14;
          hence thesis by A21,SEQ_4:def 4;
        end;
        for s being real number st 0<s
         ex r being real number st r in R & q-s<r
        proof
          let s be real number;
          assume 0<s;
          then consider r being real number such that
A128:         r in proj2.:(H /\ U) and
A129:         q-s<r by A24,A21,SEQ_4:def 4;
          take r;
          consider x being Point of TOP-REAL 2 such that
A130:         x in H /\ U and
A131:         proj2.x = r by A128,Lm3;
          x in U by A130,XBOOLE_0:def 3;
          then consider n being Nat such that
A132:         x = LMP Lower_Arc L~Cage(C,n) and
A133:         0 < n;
          r = (LMP Lower_Arc L~Cage(C,n))`2 by A131,A132,PSCOMP_1:def 29;
          hence thesis by A129,A133;
        end;
        then
A134:     sup R = q by A127,A123,A122,SEQ_4:def 4;
        consider v being real number such that
A135:       v in R and
A136:       sup R - r < v by A126,A122,A123,SEQ_4:def 4;
        sup R - r + r < v + r by A136,REAL_1:53;
        then
A137:   sup R - v < v + r - v by REAL_1:92;
        consider n being Nat such that
A138:       v = (LMP Lower_Arc L~Cage(C,n))`2 and
A139:       0 < n by A135;
        take n;
        let m be Nat;
A140:     S.m = LMP Lower_Arc L~Cage(C,m) by A124;
        assume
A141:       n <= m;
        then (LMP Lower_Arc L~Cage(C,n))`2 <= (LMP Lower_Arc L~Cage(C,m))`2
          by A139,JORDAN21:81;
        then (S.m)`2 >= v by A124,A138;
        then p`2 - (S.m)`2 <= p`2 - v by REAL_1:92;
        then
A142:     p`2-(S.m)`2 < r by A120,A137,A134,AXIOMS:22;
        0 < m by A139,A141;
        then (LMP C)`2 > (LMP Lower_Arc L~Cage(C,m))`2 by JORDAN21:77;
        then
A143:     0 > (S.m)`2-p`2 by A140,REAL_2:105;
        (S.m)`1 = w by A140,A12;
        then abs((S.m)`1-p`1) = 0 by A8,ABSVALUE:def 1;
        then |. S.m-p .|<=0+abs((S.m)`2-p`2) by JGRAPH_1:49;
        then |. S.m-p .|<=-((S.m)`2-p`2) by A143,ABSVALUE:def 1;
        hence |. S.m-p .| < r by A142,AXIOMS:22;
      end;
      thus
A144:   S is convergent
      proof
        take p;
        thus thesis by A125;
      end;
      hereby
        let x be Nat;
A145:     S.x = LMP Lower_Arc L~Cage(C,x) by A124;
        (Lower_Appr C).x = Lower_Arc L~Cage(C,x) by JORDAN19:def 2;
        hence S.x in (Lower_Appr C).x by A145,JORDAN21:71;
      end;
      thus p = lim S by A144,A125,TOPRNS_1:def 10;
    end;
    hence thesis by A121,KURATO_2:58;
  end;
  end;

theorem Th88:
  North_Arc C c= C
  proof
    let x be set;
    assume
A1:   x in North_Arc C;
    assume
A2:   not x in C;
    reconsider x as Point of TOP-REAL 2 by A1;
    reconsider e = x as Point of Euclid 2 by Lm1,TOPMETR:16;
A3: North_Arc C = Lim_inf Upper_Appr C by JORDAN19:def 3;
A4: x in C` by A2,SUBSET_1:50;
A5: (BDD C) \/ (UBD C) = C` by JORDAN2C:31;
    per cases by A4,A5,XBOOLE_0:def 2;
    suppose x in BDD C;
    then consider r being real number such that
A6:   r > 0 and
A7:   Ball(e,r) c= BDD C by Lm1,TOPMETR:22;
    consider k being Nat such that
A8:   for m being Nat st m > k holds (Upper_Appr C).m meets Ball(e,r)
        by A6,A1,A3,KURATO_2:50;
A9: k+0 < k+1 by REAL_1:67;
    BDD C misses L~Cage(C,k+1) by JORDAN10:19;
    then
A10: Ball(e,r) misses L~Cage(C,k+1) by A7,XBOOLE_1:63;
A11: (Upper_Appr C).(k+1) = Upper_Arc L~Cage(C,k+1) by JORDAN19:def 1;
    Upper_Arc L~Cage(C,k+1) c= L~Cage(C,k+1) by JORDAN1A:16;
    then
    (Upper_Appr C).(k+1) misses Ball(e,r) by A11,A10,XBOOLE_1:63;
    hence thesis by A8,A9;
    end;
    suppose
A12:   x in UBD C;
    union UBD-Family C = UBD C by JORDAN10:14;
    then consider A being set such that
A13:   x in A and
A14:   A in UBD-Family C by A12,TARSKI:def 4;
    UBD-Family C = {UBD L~Cage(C,m) where m is Nat : not contradiction}
      by JORDAN10:def 1;
    then consider m being Nat such that
A15:   A = UBD L~Cage(C,m) by A14;
    x in LeftComp Cage(C,m) by A13,A15,GOBRD14:46;
    then consider r being real number such that
A16:   r > 0 and
A17:   Ball(e,r) c= LeftComp Cage(C,m) by Lm1,TOPMETR:22;
    consider k being Nat such that
A18:   for m being Nat st m > k holds (Upper_Appr C).m meets Ball(e,r)
        by A16,A1,A3,KURATO_2:50;
    thus thesis
    proof
      per cases;
      suppose
A19:     m > k;
      LeftComp Cage(C,m) misses L~Cage(C,m) by SPRECT_3:43;
      then
A20:   Ball(e,r) misses L~Cage(C,m) by A17,XBOOLE_1:63;
A21:   (Upper_Appr C).m = Upper_Arc L~Cage(C,m) by JORDAN19:def 1;
      Upper_Arc L~Cage(C,m) c= L~Cage(C,m) by JORDAN1A:16;
      then
      (Upper_Appr C).m misses Ball(e,r) by A21,A20,XBOOLE_1:63;
      hence thesis by A18,A19;
      end;
      suppose m <= k;
      then
A22:   LeftComp Cage(C,m) c= LeftComp Cage(C,k) by JORDAN1H:55;
A23:   k+0 < k+1 by REAL_1:67;
      then
A24:   LeftComp Cage(C,k) c= LeftComp Cage(C,k+1) by JORDAN1H:55;
      Ball(e,r) c= LeftComp Cage(C,k) by A17,A22,XBOOLE_1:1;
      then
A25:   Ball(e,r) c= LeftComp Cage(C,k+1) by A24,XBOOLE_1:1;
      LeftComp Cage(C,k+1) misses L~Cage(C,k+1) by SPRECT_3:43;
      then
A26:   Ball(e,r) misses L~Cage(C,k+1) by A25,XBOOLE_1:63;
A27:   (Upper_Appr C).(k+1) = Upper_Arc L~Cage(C,k+1) by JORDAN19:def 1;
      Upper_Arc L~Cage(C,k+1) c= L~Cage(C,k+1) by JORDAN1A:16;
      then
      (Upper_Appr C).(k+1) misses Ball(e,r) by A27,A26,XBOOLE_1:63;
      hence thesis by A18,A23;
    end;
  end;
  end;
  end;

