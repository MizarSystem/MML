SCMPDS_1:def 24
            .= SS.l9 by SCMPDS_1:28
            .= S.l9 by SCMPDS_1:32;
        end;
        case
A9:       I1 = 8;
          set SS=SCM-Chg(S,Address_Add(S,c P31address,c P32const), S.
