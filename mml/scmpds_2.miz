theorem
  for i being Element of NAT holds IC SCMPDS <> dl.i & IC SCMPDS <> i
proof
  let i be Element of NAT;
  hereby
    assume IC SCMPDS = dl.i;
    then NAT = [1,i] by Th6,AMI_3:def 19;
    hence contradiction by ARYTM_3:36;
  end;
  assume IC SCMPDS = i;
  then NAT = i by Th6;
  then NAT in NAT;
  hence contradiction;
end;

