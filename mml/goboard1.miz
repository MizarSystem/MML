theorem Th7:
  for f being FinSequence,n,m,k st len f = m+1 & n in dom f & k in Seg m holds
  Del(f,n).k = f.k or Del(f,n).k = f.(k+1)
proof
  let f be FinSequence,n,m,k such that
A1: len f = m+1 & n in dom f & k in Seg m;
  set X = dom f \ {n};
A2: dom f=Seg len f & dom Sgm(X)=Seg len Sgm(X) by FINSEQ_1:def 3;
  then
A3: Del(f,n) = f*Sgm(X) & X c= Seg len f by FINSEQ_3:def 2,XBOOLE_1:36;
  then rng Sgm(X) = X by FINSEQ_1:def 13;
  then
A4: dom (f*Sgm(X)) = dom Sgm(X) & len Sgm(X) = m
  by A1,A2,A3,FINSEQ_3:116,RELAT_1:46;
  (1<=k & k<n implies Sgm(X).k=k) & (n<=k & k<=m implies Sgm(X).k=k+1)
  by A1,A2,FINSEQ_3:117;
  hence thesis by A1,A2,A3,A4,FINSEQ_3:27,FUNCT_1:22;
end;

canceled 2;

definition
  let IT be real-yielding Function;
  attr IT is increasing means
  :Def1:
  for n,m st n in dom IT & m in dom IT & n<m holds IT.n < IT.m;
end;

definition
  let f be FinSequence;
  redefine attr f is constant means
  :Def2:
  for n,m st n in dom f & m in dom f holds f.n=f.m;
  compatibility
  proof
    thus f is constant implies
    for n,m st n in dom f & m in dom f holds f.n=f.m by FUNCT_1:def 16;
    assume
A1: for n,m st n in dom f & m in dom f holds f.n=f.m;
    let n,m be set;
    assume n in dom f & m in dom f;
    hence thesis by A1;
  end;
end;

registration
  cluster non empty increasing FinSequence of REAL;
  existence
  proof
    take v = <* 0 qua Real *>;
A1: dom<* 0 *> = { 1 } by FINSEQ_1:4,55;
    thus v is non empty;
    let n,m;
    assume n in dom v & m in dom v;
    then n = 1 & m = 1 by A1,TARSKI:def 1;
    hence thesis;
  end;
end;

registration
  cluster constant FinSequence of REAL;
  existence
  proof
    take v = <*>REAL;
    let n,m;
    assume n in dom v & m in dom v;
    hence thesis;
  end;
end;

definition
  let f;
  func X_axis(f) -> FinSequence of REAL means
  :Def3:
  len it = len f & for n st n in dom it holds it.n = (f/.n)`1;
  existence
  proof
    defpred P[Nat,set] means $2 = (f/.$1)`1;
A1: for k be Nat st k in Seg len f ex r st P[k,r];
    consider v such that
A2: dom v = Seg len f and
A3: for k be Nat st k in Seg len f holds P[k,v.k] from FINSEQ_1:sch 5(A1);
    take v;
    thus len v = len f by A2,FINSEQ_1:def 3;
    let n;
    assume n in dom v;
    hence v.n = (f/.n)`1 by A2,A3;
  end;
  uniqueness
  proof
    let v1,v2;
    assume that
A4: len v1 = len f & for n st n in dom v1 holds v1.n = (f/.n)`1 and
A5: len v2 = len f & for n st n in dom v2 holds v2.n = (f/.n)`1;
A6: dom v1 = Seg len v1 & dom v2 = Seg len v2 & dom f = Seg len f
    by FINSEQ_1:def 3;
    now
      let n be Nat;
      assume
A7:   n in (dom f);
      hence v1.n = (f/.n)`1 by A4,A6
        .= v2.n by A5,A6,A7;
    end;
    hence thesis by A4,A5,A6,FINSEQ_1:17;
  end;
  func Y_axis(f) -> FinSequence of REAL means
  :Def4:
  len it = len f & for n st n in dom it holds it.n = (f/.n)`2;
  existence
  proof
    defpred P[Nat,set] means $2 = (f/.$1)`2;
A8: for k be Nat st k in Seg len f ex r st P[k,r];
    consider v such that
A9: dom v = Seg len f and
A10: for k be Nat st k in Seg len f holds P[k,v.k] from FINSEQ_1:sch 5(A8);
    take v;
    thus len v = len f by A9,FINSEQ_1:def 3;
    let n;
    assume n in dom v;
    hence v.n = (f/.n)`2 by A9,A10;
  end;
  uniqueness
  proof
    let v1,v2;
    assume that
A11: len v1 = len f & for n st n in dom v1 holds v1.n = (f/.n)`2 and
A12: len v2 = len f & for n st n in dom v2 holds v2.n = (f/.n)`2;
A13: dom v1 = Seg len v1 & dom v2 = Seg len v2 & dom f = Seg len f
    by FINSEQ_1:def 3;
    now
      let n be Nat;
      assume
A14:  n in (dom f);
      hence v1.n = (f/.n)`2 by A11,A13
        .= v2.n by A12,A13,A14;
    end;
    hence thesis by A11,A12,A13,FINSEQ_1:17;
  end;
end;

canceled 4;

theorem Th14:
  v<>{} & rng v c= Seg n & v.(len v) = n & (for k st 1<=k & k<=len v - 1 holds
  for r,s st r = v.k & s = v.(k+1) holds abs(r-s) = 1 or r=s) & i in Seg n &
  i+1 in Seg n & m in dom v & v.m = i &
  (for k st k in dom v & v.k = i holds k<=m) implies
  m+1 in dom v & v.(m+1)=i+1
proof
  assume that
A1: v<>{} and
A2: rng v c= Seg n and
A3: v.(len v) = n and
A4: for k st 1<=k & k<=len v - 1 holds
  for r,s st r = v.k & s = v.(k+1) holds abs(r-s) = 1 or r=s and
A5: i in Seg n and
A6: i+1 in Seg n and
A7: m in dom v and
A8: v.m = i and
A9: for k st k in dom v & v.k = i holds k<=m;
  len v <> 0 by A1,CARD_2:59;
  then 0 < len v;
  then 0+1<=len v & len v<=len v by NAT_1:13;
  then
A10: len v in dom v by FINSEQ_3:27;
A11: 1<=i & i<=n & 1<=i+1 & i+1<=n by A5,A6,FINSEQ_1:3;
A12: 1<=m & m<=len v & m<=m+1 by A7,FINSEQ_3:27,NAT_1:11;
A13: now
    assume not m+1 in dom v;
    then 1>m+1 or m+1>len v by FINSEQ_3:27;
    then len v <=m & i<n by A11,NAT_1:11,13;
    hence contradiction by A3,A8,A12,XXREAL_0:1;
  end;
  reconsider r=v.(m+1) as Real;
A14: m+1<=len v by A13,FINSEQ_3:27;
  then
A15: m<=len v - 1 & m<len v by NAT_1:13,XREAL_1:21;
  now per cases by A4,A8,A12,A15;
    case
A16:  abs(i - r) = 1;
      now per cases by A16,Th1;
        case
A17:      i>r & i=r+1;
          defpred P[set] means
          for k,r st k=$1 & k>0 & m+k in dom v & r=v.(m+k) holds r<i;
A18:      P[0];
A19:      for k st P[k] holds P[k+1]
          proof
            let k such that
A20:        P[k];
            let j,s such that
A21:        j=k+1 & j>0 & m+j in dom v & s=v.(m+j);
            now per cases;
              suppose k=0;
                hence s<i by A17,A21;
              end;
              suppose k<>0;
                then
A22:            0<k & m<=m+k & m+k<=m+k+1 & m+k+1 = m+(k+1 ) by NAT_1:11;
                then
A23:            1<=m+k & m+(k+1)<=len v by A12,A21,FINSEQ_3:27,XXREAL_0:2;
                then 1<=m+k & m+k<=len v by A22,XXREAL_0:2;
                then
A24:            m+k in dom v by FINSEQ_3:27;
                then v.(m+k) in rng v by FUNCT_1:def 5;
                then v.(m+k) in Seg n & Seg n c= NAT by A2;
                then reconsider r1=v.(m+k) as Element of NAT;
A25:            r1<i by A20,A22,A24;
A26:            m+k<=len v - 1 by A22,A23,XREAL_1:21;
                now per cases by A4,A21,A22,A23,A26;
                  suppose
A27:                abs(r1-s)=1;
                    now per cases by A27,Th1;
                      suppose r1>s & r1=s+1;
                        hence s<i by A25,XXREAL_0:2;
                      end;
                      suppose r1<s & s=r1+1;
                        then
A28:                    s<=i by A25,NAT_1:13;
                        now per cases by A28,REAL_1:def 5;
                          case s<i;
                            hence thesis;
                          end;
                          case s=i;
                            then m+j<=m by A9,A21;
                            then j<=m-m by XREAL_1:21;
                            hence contradiction by A21;
                          end;
                        end;
                        hence thesis;
                      end;
                    end;
                    hence thesis;
                  end;
                  suppose r1=s;
                    hence thesis by A20,A22,A24;
                  end;
                end;
                hence thesis;
              end;
            end;
            hence thesis;
          end;
A29:      for k holds P[k] from NAT_1:sch 1(A18,A19);
A30:      0< len v - m by A14,XREAL_1:21;
          reconsider l = len v - m as Element of NAT by A15,INT_1:18;
          m+l = len v;
          hence contradiction by A3,A10,A11,A29,A30;
        end;
        case i<r & r=i+1;
          hence m+1 in dom v & v.(m+1) = i+1 by A13;
        end;
      end;
      hence thesis;
    end;
    case i=r;
      then m+1<=m by A9,A13;
      then 1<=m-m by XREAL_1:21;
      then 1<=0;
      hence contradiction;
    end;
  end;
  hence thesis;
end;

theorem
  v<>{} & rng v c= Seg n & v.1 = 1 & v.(len v) = n &
  (for k st 1<=k & k<=len v - 1 holds
  for r,s st r = v.k & s = v.(k+1) holds abs(r-s) = 1 or r=s) implies
  (for i st i in Seg n ex k st k in dom v & v.k = i) &
  for m,k,i,r st m in dom v & v.m = i &
  (for j st j in dom v & v.j = i holds j<=m) &
  m<k & k in dom v & r = v.k holds i<r
proof
  assume that
A1: v<>{} and
A2: rng v c= Seg n and
A3: v.1 = 1 and
A4: v.(len v) = n and
A5: for k st 1<=k & k<=len v - 1 holds
  for r,s st r = v.k & s = v.(k+1) holds abs(r-s) = 1 or r=s;
  len v <> 0 by A1,CARD_2:59;
  then 0 < len v;
  then
A6: 1<=1 & 0+1<=len v & len v<=len v by NAT_1:13;
  then
A7: 1 in dom v & len v in dom v by FINSEQ_3:27;
  defpred P[Element of NAT] means
  $1 in Seg n implies ex k st k in dom v & v.k = $1;
A8: P[0] by FINSEQ_1:3;
A9: for i st P[i] holds P[i+1]
  proof
    let i such that
A10: i in Seg n implies ex k st k in dom v & v.k = i;
    assume
A11: i+1 in Seg n;
    now per cases;
      suppose
A12:    i = 0;
        take k=1;
        thus k in dom v & v.k = i+1 by A3,A6,A12,FINSEQ_3:27;
      end;
      suppose i<>0;
        then 0<i;
        then 0+1<=i & i<=i+1 & i+1<=n by A11,FINSEQ_1:3,NAT_1:13;
        then
A13:    1<=i & i<=n by XXREAL_0:2;
        then
A14:    i in Seg n by FINSEQ_1:3;
        defpred R[set] means $1 in dom v & v.$1 = i;
A15:    ex k be Nat st R[k] by A10,A13,FINSEQ_1:3;
A16:    for k be Nat holds R[k] implies k<=len v by FINSEQ_3:27;
        consider m be Nat such that
A17:    R[m] & for k be Nat st R[k] holds k<=m from NAT_1:sch 6(A16,A15);
A18:    R[m] & for k st R[k] holds k<=m by A17;
        reconsider m as Element of NAT by ORDINAL1:def 13;
        take k = m+1;
        thus k in dom v & v.k = i+1 by A1,A2,A4,A5,A11,A14,A18,Th14;
      end;
    end;
    hence thesis;
  end;
  thus for i holds P[i] from NAT_1:sch 1(A8,A9);
  let m,k,i,r;
  assume
A19: m in dom v & v.m = i & (for j st j in dom v & v.j = i holds j<=m) &
  m<k & k in dom v & r = v.k;
  then
A20: 1<=m & m<=len v & 1<=k & k<=len v by FINSEQ_3:27;
A21: i in rng v by A19,FUNCT_1:def 5;
  then
A22: 1<=i & i<=n & i<=i+1 by A2,FINSEQ_1:3,NAT_1:11;
  now per cases;
    suppose i=n;
      then len v<=m by A4,A7,A19;
      hence thesis by A19,A20,XXREAL_0:1;
    end;
    suppose i<>n;
      then i<n by A22,REAL_1:def 5;
      then 1<=i+1 & i+1<=n by NAT_1:11,13;
      then i+1 in Seg n by FINSEQ_1:3;
      then
A23:  m+1 in dom v & v.(m+1) = i+1 by A1,A2,A4,A5,A19,A21,Th14;
      defpred P[set] means
      for j,s st j=$1 & j>0 & m+j in dom v & s=v.(m+j) holds i<s;
A24:  P[0];
A25:  for k st P[k] holds P[k+1]
      proof
        let k such that
A26:    P[k];
        let j,s such that
A27:    j=k+1 & j>0 & m+j in dom v & s=v.(m+j);
        per cases;
        suppose k=0;
          hence i<s by A23,A27,NAT_1:13;
        end;
        suppose k<>0;
          then
A28:      0<k & m+(k+1)<=len v & m+k+1 = m+(k+1) & m+k<=m+k+1 & m<=m+k
          by A27,FINSEQ_3:27,NAT_1:11;
          then
A29:      1<=m+k & m+k<=len v & m+k<= len v - 1 by A20,XREAL_1:21,XXREAL_0:2;
          then
A30:      m+k in dom v by FINSEQ_3:27;
          then v.(m+k) in rng v by FUNCT_1:def 5;
          then v.(m+k) in Seg n & Seg n c= NAT by A2;
          then reconsider r1=v.(m+k) as Element of NAT;
          s in rng v by A27,FUNCT_1:def 5;
          then s in Seg n & Seg n c= NAT by A2;
          then reconsider s1=s as Element of NAT;
A31:      i<r1 by A26,A28,A30;
          now per cases by A5,A27,A28,A29;
            suppose
A32:          abs(r1-s)=1;
              now per cases by A32,Th1;
                suppose r1>s & r1=s+1;
                  then
A33:              i<=s1 by A31,NAT_1:13;
                  now per cases by A33,REAL_1:def 5;
                    case i<s;
                      hence thesis;
                    end;
                    case s=i;
                      then m+j<=m by A19,A27;
                      then j<=m-m by XREAL_1:21;
                      hence contradiction by A27;
                    end;
                  end;
                  hence thesis;
                end;
                suppose r1<s & s=r1+1;
                  hence i<s by A31,XXREAL_0:2;
                end;
              end;
              hence thesis;
            end;
            suppose r1=s;
              hence thesis by A26,A28,A30;
            end;
          end;
          hence thesis;
        end;
      end;
A34:  for k holds P[k] from NAT_1:sch 1(A24,A25);
A35:  0< k - m & m<=k by A19,XREAL_1:52;
      reconsider l = k - m as Element of NAT by A19,INT_1:18;
      m+l = k;
      hence i<r by A19,A34,A35;
    end;
  end;
  hence thesis;
end;

theorem Th16:
  i in dom f & 2<=len f implies f/.i in L~f
proof
  assume
A1: i in dom f & 2<=len f;
  then
A2: 1<=i & i<=len f by FINSEQ_3:27;
  per cases by A2,REAL_1:def 5;
  suppose
A3: i=len f;
    reconsider l=i-1 as Element of NAT by A2,INT_1:18;
    1+1<=i & 0<=1 by A1,A3;
    then 1<=l & l<=i by XREAL_1:21,45;
    then f/.(l+1) in LSeg(f,l) & LSeg(f,l) c=L~f
    by A2,TOPREAL1:27,TOPREAL3:26;
    hence f/.i in L~f;
  end;
  suppose i<len f;
    then 1<=i+1 & i+1<=len f by NAT_1:11,13;
    then f/.i in LSeg(f,i) & LSeg(f,i) c= L~f by A2,TOPREAL1:27,TOPREAL3:26;
    hence thesis;
  end;
end;

