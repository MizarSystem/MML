theorem Th1:
  abs(r-s)=1 iff r>s & r=s+1 or r<s & s=r+1
proof
  thus abs(r-s)=1 implies r>s & r=s+1 or r<s & s=r+1
  proof
    assume
A1: abs(r-s) = 1;
    now per cases by XXREAL_0:1;
      case
A2:     r>s;
        then 0<r-s by XREAL_1:52;
        then r-s=1 by A1,ABSVALUE:def 1;
        hence r>s & r=s+1 or r<s & s=r+1 by A2;
      end;
      case r=s;
        hence contradiction by A1,ABSVALUE:7;
      end;
      case
A3:     r<s;
        then
A4:     0<s-r by XREAL_1:52;
        1=abs(-(r-s)) by A1,COMPLEX1:138
          .=s-r by A4,ABSVALUE:def 1;
        hence r>s & r=s+1 or r<s & s=r+1 by A3;
      end;
    end;
    hence thesis;
  end;
  assume
A5: r>s & r=s+1 or r<s & s=r+1;
  per cases by A5;
  suppose r>s & r=s+1;
    hence abs(r-s)= 1 by ABSVALUE:def 1;
  end;
  suppose
A6: s>r & s=r+1;
    thus abs(r-s)=abs(-(r-s)) by COMPLEX1:138
      .= 1 by A6,ABSVALUE:def 1;
  end;
end;

theorem Th2:
  abs(i-j)+abs(n-m)=1 iff abs(i-j)=1 & n=m or abs(n-m)=1 & i=j
proof
  thus abs(i-j)+abs(n-m)=1 implies abs(i-j)=1 & n=m or abs(n-m)=1 & i=j
  proof
    assume
A1: abs(i-j)+abs(n-m)=1;
    now per cases;
      suppose
A2:     n=m;
        then 1=abs(i-j)+0 by A1,ABSVALUE:7
          .= abs(i-j);
        hence thesis by A2;
      end;
      suppose
A3:     n<>m;
        now per cases by A3,REAL_1:def 5;
          suppose
A4:         n<m;
            then reconsider l=m-n as Element of NAT by INT_1:18;
            0<l & abs(-(m-n))=abs(l) by A4,COMPLEX1:138,XREAL_1:52;
            then
A5:         0+1<=l & abs(l)=l & 0<=abs(i-j) & abs(n-m)=abs(l)
            by ABSVALUE:def 1,COMPLEX1:132,NAT_1:13;
            then
A6:         l<=1 by A1,REAL_2:173;
            then l=1 by A5,XXREAL_0:1;
            then i-j=0 by A1,A5,ABSVALUE:7;
            hence thesis by A5,A6,XXREAL_0:1;
          end;
          suppose
A7:         n>m;
            then reconsider l=n-m as Element of NAT by INT_1:18;
            0<l by A7,XREAL_1:52;
            then
A8:         0+1<=l & abs(n-m)=l & 0<=abs(i-j)by ABSVALUE:def 1,COMPLEX1:132
            ,NAT_1:13;
            then
A9:         l<=1 by A1,REAL_2:173;
            then l=1 by A8,XXREAL_0:1;
            then i-j=0 by A1,A8,ABSVALUE:7;
            hence thesis by A8,A9,XXREAL_0:1;
          end;
        end;
        hence thesis;
      end;
    end;
    hence thesis;
  end;
  assume
A10: abs(i-j)=1 & n=m or abs(n-m)=1 & i=j;
  now per cases by A10;
    suppose
A11:  abs(i-j)=1 & n=m;
      then abs(n-m)=0 by ABSVALUE:7;
      hence abs(i-j)+abs(n-m)=1 by A11;
    end;
    suppose
A12:  abs(n-m)=1 & i=j;
      then abs(i-j)=0 by ABSVALUE:7;
      hence abs(i-j)+abs(n-m)=1 by A12;
    end;
  end;
  hence thesis;
end;

theorem Th3:
  n>1 iff ex m st n=m+1 & m>0
proof
  thus n>1 implies ex m st n=m+1 & m>0
  proof
    assume
A1: 1<n;
    then consider m being Nat such that
A2: n = m+1 by NAT_1:6;
    reconsider m as Element of NAT by ORDINAL1:def 13;
    take m;
    m <> 0 by A1,A2;
    hence thesis by A2;
  end;
  given m such that
A3: n=m+1 & m>0;
  0+1<n by A3,XREAL_1:8;
  hence thesis;
end;

