           then D1.(k+1) < D1.k by A1,A104,A116,Def21;
           hence contradiction by A96,A101,A104,GOBOARD2:18;
         end;
         then consider ID being Nat such that
A117:    indx(D2,D1,k+1) = indx(D2,D1,k) + ID by NAT_1:28;
A118:    ID = indx(D2,D1,k+1) - indx(D2,D1,k) by A117;
A119:     1 <= k & k <= len upper_volume(f,D1) by A102,Def7;
            indx(D2,D1,k) in dom D2 by A1,A104,Def21;
         then indx(D2,D1,k)in
Seg(len upper_volume(f,D2)) by A106,FINSEQ_1:def 3;
then A120:     1<=indx(D2,D1,k) & indx(D2,D1,k)<=len upper_volume(f,D2)
         by FINSEQ_1:3;
         set K1D1=upper_volume(f,D1)|(k+1);
         set KD1 =upper_volume(f,D1)|k;
         set K1D2=upper_volume(f,D2)|indx(D2,D1,k+1);
         set KD2 =upper_volume(f,D2)|indx(D2,D1,k);
         set IDK1=indx(D2,D1,k+1);
         set IDK =indx(D2,D1,k);
A121:     len K1D1=k+1 by A105,FINSEQ_1:80;
         then consider p1,q1 being FinSequence of REAL such that
A122:      len p1=k & len q1=1 & K1D1=p1^q1 by FINSEQ_2:26;
            len K1D2=IDK + (IDK1-IDK) by A111,FINSEQ_1:80;
         then consider p2,q2 being FinSequence of REAL such that
A123:      len p2=IDK & len q2=IDK1-IDK & K1D2=p2^q2 by A117,FINSEQ_2:26;
A124:      p1=KD1
         proof
A125:       len p1 = len KD1 by A119,A122,FINSEQ_1:80;
              for i st 1 <= i & i <= len p1 holds p1.i=KD1.i
           proof
             let i; assume 1 <= i & i <= len p1;
then A126:         i in Seg(len p1) by FINSEQ_1:3;
then A127:          i in dom p1 by FINSEQ_1:def 3;
A128:         i in dom KD1 by A125,A126,FINSEQ_1:def 3;
then A129:         i in dom (upper_volume(f,D1)|Seg k) by FINSEQ_1:def 15;
A130:         KD1.i = (upper_volume(f,D1)|Seg k).i by FINSEQ_1:def 15
              .= upper_volume(f,D1).i by A129,FUNCT_1:68;
A131:     dom KD1=Seg(len KD1) by FINSEQ_1:def 3.= Seg k by A119,FINSEQ_1:80;
                k <= k+1 by NAT_1:29;
then A132:         Seg k c= Seg(k+1) by FINSEQ_1:7;
                dom K1D1 =Seg(len K1D1) by FINSEQ_1:def 3.= Seg(k+1)
               by A105,FINSEQ_1:80;
             then i in dom K1D1 by A128,A131,A132;
then A133:         i in dom (upper_volume(f,D1)|Seg(k+1)) by FINSEQ_1:def 15;
                K1D1.i = (upper_volume(f,D1)|Seg (k+1)).i by FINSEQ_1:def 15
              .= upper_volume(f,D1).i by A133,FUNCT_1:68;
             hence thesis by A122,A127,A130,FINSEQ_1:def 7;
           end;
           hence thesis by A125,FINSEQ_1:18;
         end;
A134:      p2=KD2
         proof
A135:       len p2 = len KD2 by A120,A123,FINSEQ_1:80;
              for i st 1 <= i & i <= len p2 holds p2.i=KD2.i
           proof
             let i; assume 1 <= i & i <= len p2;
then A136:         i in Seg(len p2) by FINSEQ_1:3;
then A137:          i in dom p2 by FINSEQ_1:def 3;
A138:         i in dom KD2 by A135,A136,FINSEQ_1:def 3;
then A139:         i in
 dom (upper_volume(f,D2)|Seg indx(D2,D1,k)) by FINSEQ_1:def 15;
A140:         KD2.i=(upper_volume(f,D2)|Seg indx(D2,D1,k)).i by FINSEQ_1:def 15
              .= upper_volume(f,D2).i by A139,FUNCT_1:68;
A141:         dom KD2 =Seg(len KD2) by FINSEQ_1:def 3
              .= Seg indx(D2,D1,k) by A120,FINSEQ_1:80;
A142:         Seg indx(D2,D1,k) c= Seg indx(D2,D1,k+1) by A115,FINSEQ_1:7;
                dom K1D2 = Seg(len K1D2) by FINSEQ_1:def 3
              .= Seg indx(D2,D1,k+1) by A111,FINSEQ_1:80;
             then i in dom K1D2 by A138,A141,A142;
then
A143:      i in dom (upper_volume(f,D2)|Seg indx(D2,D1,k+1))
                   by FINSEQ_1:def 15;
              K1D2.i=(upper_volume(f,D2)|Seg indx(D2,D1,k+1)).i
                   by FINSEQ_1:def 15
              .=upper_volume(f,D2).i by A143,FUNCT_1:68;
             hence thesis by A123,A137,A140,FINSEQ_1:def 7;
           end;
           hence thesis by A135,FINSEQ_1:18;
         end;
A144:      Sum q1 >= Sum q2
         proof
           set DD1 = divset(D1,k+1);
           set MD1 = mid(D1,k+1,k+1);
           set MD2 = mid(D2,indx(D2,D1,k)+1,indx(D2,D1,k+1));
           set g = f|DD1;
           consider S1 being non empty Division of DD1;
 A145:       k+1 in dom D1 &
           D1.(k+1) >= inf DD1 & D1.(k+1) = sup DD1
           proof
           (k+1)-1=k;
             then inf DD1=D1.k by A96,A99,Def5;
             hence thesis by A96,A99,A101,A104,Def5,GOBOARD2:18;
           end;
           then reconsider MD1 as Element of S1 by Th39;
     A146:  indx(D2,D1,k)+1 <= indx(D2,D1,k+1) by A112,NAT_1:38;
     then A147:  indx(D2,D1,k)+1 <= len D2 by A110,AXIOMS:22;
     A148: indx(D2,D1,k) <= indx(D2,D1,k)+1 by NAT_1:29;
     A149:  1 <= indx(D2,D1,k)+1 by NAT_1:29;
           then indx(D2,D1,k)+1 in Seg(len D2) by A147,FINSEQ_1:3;
     then A150:  indx(D2,D1,k)+1 in dom D2 by FINSEQ_1:def 3;
     A151:  D2.indx(D2,D1,k+1)=D1.(k+1) by A1,A96,Def21;
         (k+1)-1=k;
     then A152:   inf DD1=D1.k by A96,A99,Def5;
         D2.(indx(D2,D1,k)+1)>=D2.indx(D2,D1,k)by A107,A148,A150,GOBOARD2:18;
     then A153:  D2.(indx(D2,D1,k)+1) >= inf DD1 by A1,A104,A152,Def21;
          D2.indx(D2,D1,k+1) = sup DD1 by A96,A99,A151,Def5;
           then reconsider MD2 as Element of S1 by A108,A146,A150,A153,Th39;
           reconsider g as PartFunc of DD1,REAL by PARTFUN1:43;
 A154:      g is total & g is_bounded_above_on DD1
           proof
             dom g = dom f /\ divset(D1,k+1) by FUNCT_1:68;
       then A155:   dom g = A /\ divset(D1,k+1) by FUNCT_2:def 1;
                divset(D1,k+1) c= A by A145,Th10;
             then dom g = divset(D1,k+1) by A155,XBOOLE_1:28; hence
                g is total by PARTFUN1:def 4;
                g is_bounded_above_on divset(D1,k+1)
             proof
               consider a be real number such that
       A156:     for x being Element of A st x in A /\ dom f
               holds f.x <= a by A2,RFUNCT_1:def 9;
                  for x being Element of divset(D1,k+1)
               st x in divset(D1,k+1) /\ dom g
               holds g.x <= a
               proof
                 let x be Element of divset(D1,k+1);
                 assume x in divset(D1,k+1) /\ dom g;
       then A157:      x in dom g by XBOOLE_0:def 3;
       A158:      A /\ dom f = dom f by XBOOLE_1:28;
       A159:       dom g c= dom f by FUNCT_1:76;
                 then x in A /\ dom f by A157,A158;
                 then reconsider x as Element of A by A158;
                    f.x <= a by A156,A157,A158,A159;
                 hence thesis by A157,FUNCT_1:68;
               end;
               hence thesis by RFUNCT_1:def 9;
             end;
             hence thesis;
           end;
then A160:         g is Function of divset(D1,k+1),REAL by FUNCT_2:131;
 A161:      q1=upper_volume(g,MD1)
           proof
                  len MD1 = (k+1) -'(k+1) + 1 by A98,JORDAN3:27;
               then A162: len MD1 = ((k+1)-(k+1)) +1 by SCMFSA_7:3;
   then A163:      len q1 = len(upper_volume(g,MD1)) by A122,Def7;
                for n st 1 <= n & n <= len q1 holds q1.n=upper_volume(g,MD1).n
             proof
               let n; assume
   A164:          1 <= n & n <= len q1;
   then A165:        n = 1 by A122,AXIOMS:21;
   A166:        n in Seg(len q1) by A164,FINSEQ_1:3;
               then A167: n in dom q1 by FINSEQ_1:def 3;
                  k+1 in Seg(len K1D1) by A121,FINSEQ_1:6;
               then k+1 in dom K1D1 by FINSEQ_1:def 3;
   then
A168:       k+1 in dom (upper_volume(f,D1)|Seg(k+1)) by FINSEQ_1:def 15;
            K1D1.(k+1)=(upper_volume(f,D1)|Seg(k+1)).(k+1) by FINSEQ_1:def 15
                .=upper_volume(f,D1).(k+1) by A168,FUNCT_1:68;
   then A169:       q1.n = upper_volume(f,D1).(k+1) by A122,A165,A167,
FINSEQ_1:def 7
             .=(sup(rng(f|divset(D1,k+1))))*vol(divset(D1,k+1)) by A97,Def7;
    A170:       divset(MD1,1)=[.inf divset(MD1,1),sup divset(MD1,1).] by Th5;
    A171:        1 in dom MD1
               proof
                    1 in Seg(len MD1) by A162,FINSEQ_1:5;
                 hence thesis by FINSEQ_1:def 3;
               end;
    A172:      MD1.1 =D1.(k+1) by A98,JORDAN3:27;
    A173:      divset(MD1,1)=[.inf DD1,sup divset(MD1,1).] by A170,A171,Def5
                .=[.inf DD1,D1.(k+1).] by A171,A172,Def5;
             (k+1)-1=k;
    then A174:      inf DD1 = D1.k by A96,A99,Def5;
            sup DD1 = D1.(k+1) by A96,A99,Def5;
           then divset(D1,k+1)=[. D1.k,D1.(k+1).] by A174,Th5;
               then upper_volume(g,MD1).n
              =(sup(rng(g|divset(D1,k+1))))*vol(divset(D1,k+1))
                 by A122,A162,A165,A166,A173,A174,Def7;
               hence thesis by A169,FUNCT_1:82;
             end;
             hence thesis by A163,FINSEQ_1:18;
           end;
 A175:      q2=upper_volume(g,MD2)
           proof
   A176:        indx(D2,D1,k+1)-'(indx(D2,D1,k)+1)+1
                =indx(D2,D1,k+1)-(indx(D2,D1,k)+1)+1 by A146,SCMFSA_7:3
                .=indx(D2,D1,k+1)-indx(D2,D1,k);
   A177:         len(upper_volume(g,MD2))=
len mid(D2,indx(D2,D1,k)+1,indx(D2,D1,k+1)) by Def7
                .=indx(D2,D1,k+1)-indx(D2,D1,k) by A110,A146,A147,A149,A176,
JORDAN3:27;
                for n st 1 <= n & n <= len q2 holds q2.n=upper_volume(g,MD2).n
             proof
               let n; assume A178:1 <= n & n <= len q2;
               then n in Seg(len q2) by FINSEQ_1:3;
   then A179:        n in dom q2 by FINSEQ_1:def 3;
   A180:        dom K1D2 = Seg(len K1D2) by FINSEQ_1:def 3
                .= Seg indx(D2,D1,k+1) by A111,FINSEQ_1:80;
   A181:        indx(D2,D1,k)+n in dom K1D2 by A123,A179,FINSEQ_1:41;
   then A182:       indx(D2,D1,k)+n in dom (upper_volume(f,D2)|Seg indx(D2,D1,k
+1))
               by FINSEQ_1:def 15;
   A183:         dom K1D2 c= Seg(len D2) by A110,A180,FINSEQ_1:7;
               then indx(D2,D1,k)+n in Seg(len D2) by A181;
   then A184:       indx(D2,D1,k)+n in dom D2 by FINSEQ_1:def 3;
   A185:        1 <=indx(D2,D1,k)+n & indx(D2,D1,k)+n <= indx(D2,D1,k+1)
                by A180,A181,FINSEQ_1:3;
   then A186:         n <= ID by A118,REAL_1:84;
   then A187:       n in Seg ID by A178,FINSEQ_1:3;
  A188:          len mid(D2,indx(D2,D1,k)+1,indx(D2,D1,k+1))
           =ID by A110,A117,A146,A147,A149,A176,JORDAN3:27;
                 then A189:       n in Seg(len mid(D2,indx(D2,D1,k)+1,indx(D2,
D1,k+1)))
                by A178,A186,FINSEQ_1:3;
   A190:      n in dom MD2 by A187,A188,FINSEQ_1:def 3;
   A191:       q2.n=K1D2.(indx(D2,D1,k)+n) by A123,A179,FINSEQ_1:def 7
.=(upper_volume(f,D2)|Seg indx(D2,D1,k+1)).(indx(D2,D1,k)+n) by FINSEQ_1:def 15
                .=upper_volume(f,D2).(indx(D2,D1,k)+n) by A182,FUNCT_1:68
   .=(sup(rng(f|divset(D2,indx(D2,D1,k)+n))))*vol(divset(D2,indx(D2,D1,k)+n))
                 by A181,A183,Def7;
   A192:       divset(MD2,n)=divset(D2,indx(D2,D1,k)+n) &
               divset(D2,indx(D2,D1,k)+n) c= divset(D1,k+1)
               proof
                    now per cases;
                   suppose A193:n=1;
     A194:           (k+1)-1=k;
     A195:          1<=indx(D2,D1,k)+1 & indx(D2,D1,k)+1<=len D2
                     by A181,A183,A193,FINSEQ_1:3;
     A196:          indx(D2,D1,k)+1 <> 1 by A120,NAT_1:38;
     A197:           inf divset(MD2,1)=inf divset(D1,k+1) by A190,A193,Def5
                    .= D1.k by A96,A99,A194,Def5;
     A198:           sup divset(MD2,1)=
 mid(D2,indx(D2,D1,k)+1,indx(D2,D1,k+1)).1 by A190,A193,Def5
                  .= D2.(1+indx(D2,D1,k)) by A110,A195,JORDAN3:27;
     then
A199:           divset(MD2,n)=[.D1.k,D2.(indx(D2,D1,k)+1).] by A193,A197,Th5;
     A200:         divset(D2,indx(D2,D1,k)+n)=
[.inf divset(D2,indx(D2,D1,k)+1),sup divset(D2,indx(D2,D1,k)+1).] by A193,Th5
              .=[.D2.(indx(D2,D1,k)+1-1),sup divset(D2,indx(D2,D1,k)+1).] by
A150,A196,Def5
           .=[.D2.indx(D2,D1,k),D2.(indx(D2,D1,k)+1).] by A150,A196,Def5
                    .=[.D1.k,D2.(indx(D2,D1,k)+1).] by A1,A104,Def21;
                   hence divset(MD2,n)=divset(D2,indx(D2,D1,k)+n) by A193,A197
,A198,Th5;
                   thus thesis by A190,A199,A200,Th10;
                   end;
                   suppose A201:n<>1;
            A202:   indx(D2,D1,k)+1 <= indx(D2,D1,k+1) by A112,NAT_1:38;
                   consider n1 being Nat such that
            A203:  n=1+n1 by A178,NAT_1:28;
            A204:   n1+(indx(D2,D1,k)+1)-'1=indx(D2,D1,k)+n-1
                     by A185,A203,SCMFSA_7:3;
            A205:   1<=n-1 & n-1<=len MD2
                   proof
                        n <> 0 by A178;
                     then n is non trivial by A201,NAT_2:def 1;
                     then n >= 1+1 by NAT_2:31;
                     hence n-1>=1 by REAL_1:84;
                        n<=n+1 by NAT_1:37;
                     then n-1 <= n by REAL_1:86;
                     hence thesis by A186,A188,AXIOMS:22;
                   end;
                        n+(indx(D2,D1,k)+1)=(indx(D2,D1,k)+n)+1;
            then n+(indx(D2,D1,k)+1) >= 1 by NAT_1:29;
then A206:                n+(indx(D2,D1,k)+1)-'1=n+indx(D2,D1,k)+1-1 by
SCMFSA_7:3
                      .=indx(D2,D1,k)+n;
            A207:  indx(D2,D1,k)+n <> 1
                   proof
                     assume indx(D2,D1,k)+n = 1;
                     then indx(D2,D1,k)=1-n;
                     then n+1 <= 1 by A120,REAL_1:84;
                     then n <= 1-1 by REAL_1:84;
                     hence contradiction by A178,NAT_1:19;
                   end;
            A208:   inf divset(MD2,n)=MD2.(n-1) by A190,A201,Def5
        .=D2.(indx(D2,D1,k)+n-1) by A110,A147,A149,A202,A203,A204,A205,
JORDAN3:27;
            A209:   sup divset(MD2,n)=MD2.n by A190,A201,Def5
              .=D2.(indx(D2,D1,k)+n) by A110,A147,A149,A178,A186,A188,A202,A206
,JORDAN3:27;
            then A210:divset(MD2,n)=
[. D2.(indx(D2,D1,k)+n-1),D2.(indx(D2,D1,k)+n) .] by A208,Th5;
     A211:             divset(D2,indx(D2,D1,k)+n)
    =[.inf divset(D2,indx(D2,D1,k)+n),sup divset(D2,indx(D2,D1,k)+n).] by Th5
.=[.D2.(indx(D2,D1,k)+n-1),sup divset(D2,indx(D2,D1,k)+n).] by A184,A207,Def5
    .=[. D2.(indx(D2,D1,k)+n-1),D2.(indx(D2,D1,k)+n) .] by A184,A207,Def5;
                   hence divset(MD2,n)=divset(D2,indx(D2,D1,k)+n) by A208,A209,
Th5;
                   thus thesis by A190,A210,A211,Th10;
                 end;
                 end;
                 hence thesis;
               end;
          then g|divset(MD2,n)
                 =f|divset(D2,indx(D2,D1,k)+n) by FUNCT_1:82;
               hence thesis by A189,A191,A192,Def7;
             end;
             hence thesis by A123,A177,FINSEQ_1:18;
           end;
              len MD1 = 1 & MD1 <= MD2
           proof
                len MD1 = (k+1) -'(k+1) + 1 by A98,JORDAN3:27;
             then A212: len MD1 = (k+1) - (k+1) + 1 by SCMFSA_7:3;
             hence len MD1 = 1;
             thus thesis by A212,Th31;
           end;
           then upper_sum(g,MD1) >= upper_sum(g,MD2) by A154,A160,Th32;
           then Sum(upper_volume(g,MD1)) >= upper_sum(g,MD2) by Def9;
           hence thesis by A161,A175,Def9;
         end;
A213:      Sum K1D1=Sum p1+Sum q1 by A122,RVSUM_1:105;
A214:      Sum K1D2=Sum p2+Sum q2 by A123,RVSUM_1:105;
            Sum q1 = Sum K1D1 - Sum p1 by A213;
         then Sum K1D1 >= Sum q2 + Sum p1 by A144,REAL_1:84;
         then Sum K1D1 - Sum q2 >= Sum p1 by REAL_1:84;
         then Sum K1D1 - Sum q2 >= Sum
p2 by A95,A103,A124,A134,AXIOMS:22,FINSEQ_1:def 3;
         hence thesis by A214,REAL_1:84;
       end;
       end;
       hence thesis;
     end;
     thus for k being non empty Nat holds P[k] from BINARITH:sch 1(A3,A94);
   end;
   hence thesis;
 end;

theorem Th41:
 for A be closed-interval Subset of REAL,
 f be Function of A,REAL, S be non empty Division of A,
 D1,D2 be Element of S st D1 <= D2 & f is_bounded_below_on A
 holds for i be non empty Nat holds (i in dom D1 implies
 Sum(lower_volume(f,D1)|i) <= Sum(lower_volume(f,D2)|indx(D2,D1,i)))
 proof
   let A be closed-interval Subset of REAL;
   let f be Function of A,REAL;
   let S be non empty Division of A;
   let D1,D2 be Element of S;
   assume that A1:D1 <= D2 and A2:f is_bounded_below_on A;
      for i be non empty Nat holds i in dom D1 implies
   Sum(lower_volume(f,D1)|i) <= Sum(lower_volume(f,D2)|indx(D2,D1,i))
   proof
defpred P[Nat] means $1 in dom D1 implies
     Sum(lower_volume(f,D1)|$1) <= Sum(lower_volume(f,D2)|indx(D2,D1,$1));
A3:  P[1]
     proof
       assume A4:1 in dom D1;
 then A5:  1 in Seg(len D1) by FINSEQ_1:def 3;
 then A6:   1 <= len D1 by FINSEQ_1:3;
 then A7:   len mid(D1,1,1)=1-'1+1 by JORDAN3:27;
 then A8:   len mid(D1,1,1)=1 by AMI_5:4;
 A9:   mid(D1,1,1)=D1|1
       proof
 A10:     len mid(D1,1,1)=len (D1|1) by A6,A8,FINSEQ_1:80;
            for k st 1 <= k & k <= len mid(D1,1,1) holds mid(D1,1,1).k=(D1|1).k
         proof
           let k; assume A11:1 <= k & k <= len mid(D1,1,1);
 then A12:       k = 1 by A8,AXIOMS:21;
           then A13: mid(D1,1,1).k = D1.(1+1-1) by A6,JORDAN3:27;
              k in Seg(len(D1|1)) by A10,A11,FINSEQ_1:3;
           then k in dom (D1|1) by FINSEQ_1:def 3;
           then k in dom (D1|Seg 1) by FINSEQ_1:def 15;
           then (D1|Seg 1).k = D1.k by FUNCT_1:68;
           hence thesis by A12,A13,FINSEQ_1:def 15;
         end;
         hence thesis by A10,FINSEQ_1:18;
       end;
          indx(D2,D1,1) in dom D2 by A1,A4,Def21;
       then indx(D2,D1,1) in Seg(len D2) by FINSEQ_1:def 3;
 then A14:   1 <= indx(D2,D1,1) & indx(D2,D1,1) <= len D2 by FINSEQ_1:3;
 then A15:   1 <= len D2 by AXIOMS:22;
       then len mid(D2,1,indx(D2,D1,1))=indx(D2,D1,1)-'1+1 by A14,JORDAN3:27;
