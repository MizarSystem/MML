      .= n*((sin.x) #Z (n-1))*(sin.x*cos.(n*x)+cos.x*sin.(n*x))
      .= n*((sin.x) #Z (n-1))*(sin.(x+n*x)) by SIN_COS:79
      .= n*((sin.x) #Z (n-1))*sin.((n+1)*x);
     hence (((sin*f)(#)(( #Z n)*sin))`|Z).x =n*(sin.x) #Z (n-1) *sin.((n+1)*x);
   end;
   hence thesis by A11,A7,A1,FDIFF_1:29;
