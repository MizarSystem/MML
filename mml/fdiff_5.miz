      .=((cos*(f^)).x)*(2*x) +((x #Z 1)*(x #Z 1))*((1/x^2)*sin.(1/x))
         by TAYLOR_1:1
      .=((cos*(f^)).x)*(2*x) +(x*(x #Z 1))*((1/x^2)*sin.(1/x)) by PREPOWER:45
      .=((cos*(f^)).x)*(2*x) +(x*x)*(((1*1)/(x*x))*sin.(1/x)) by PREPOWER:45
      .=((cos*(f^)).x)*(2*x) +(x*x)*(((1/x)*(1/x))*sin.(1/x)) by XCMPLX_1:103
      .=((cos*(f^)).x)*(2*x) +(x*(1/x))*(x*(1/x))*sin.(1/x)
      .=((cos*(f^)).x)*(2*x) +(x*(1/x))*(1)*sin.(1/x)by A9,XCMPLX_1:107
      .=((cos*(f^)).x)*(2*x) +(1)*(1)*sin.(1/x) by A9,XCMPLX_1:107
      .=(cos.((f^).x))*(2*x) +sin.(1/x) by A2,A8,FUNCT_1:22
      .=(cos.((f.x)"))*(2*x) +sin.(1/x) by A8,RFUNCT_1:def 8,A3
      .=(cos.(1*x"))*(2*x) +sin.(1/x) by A1,A8
      .=2*x*cos.(1/x)+sin.(1/x) by XCMPLX_0:def 9;
     hence ((g(#)(cos*(f^)))`|Z).x = 2*x*cos.(1/x)+sin.(1/x);
   end;
   hence thesis by A1,A4,A5,FDIFF_1:29;
end;

:: 19 f.x=ln(x)
theorem Th19:
  Z c= dom (ln*f) &
  (for x st x in Z holds f.x = x & f.x > 0) implies
    ln*f is_differentiable_on Z &
    for x st x in Z holds ((ln*f)`|Z).x = 1/x
proof
  assume A1: Z c= dom (ln*f) &
  for x st x in Z holds f.x = x & f.x > 0;
  for y st y in Z holds y in dom f by FUNCT_1:21,A1;
  then A2:Z c= dom f by TARSKI:def 3;
A3:for x st x in Z holds f.x = 1*x+0 by A1;
  then A4:f is_differentiable_on Z &
  for x st x in Z holds (f`|Z).x = 1 by FDIFF_1:31,A2;
A5:for x st x in Z holds ln*f is_differentiable_in x
  proof
    let x;
    assume A6:x in Z;
    then A7:f is_differentiable_in x by FDIFF_1:16,A4;
    f.x >0 by A1,A6;
    hence ln*f is_differentiable_in x by TAYLOR_1:20,A7;
  end;
  then A8:ln*f is_differentiable_on Z by A1,FDIFF_1:16;
  for x st x in Z holds ((ln*f)`|Z).x = 1/x
  proof
    let x;
    assume A9:x in Z;
    then A10:f is_differentiable_in x by FDIFF_1:16,A4;
A11: f.x = x & f.x >0 by A1,A9;
    diff(ln*f,x) =diff(f,x)/( f.x) by TAYLOR_1:20,A11,A10
      .=(f`|Z).x/( f.x) by A4,A9,FDIFF_1:def 8
      .=1/x by FDIFF_1:31,A2,A3,A9,A11;
    hence thesis by A9,A8,FDIFF_1:def 8;
  end;
  hence thesis by A1,FDIFF_1:16,A5;
end;

:: 20 f.x=x*ln(x)
theorem
  Z c= dom ((id Z)(#)f) & f = ln*f1 &
  (for x st x in Z holds f1.x=x & f1.x >0) implies
  (id Z)(#)f is_differentiable_on Z &
  for x st x in Z holds (((id Z)(#)f)`|Z).x = 1+ln.x
proof
   assume
A1:Z c= dom ((id Z)(#)f) & f = ln*f1 &
   (for x st x in Z holds f1.x=x & f1.x >0); then
   Z c= dom id Z /\ dom f by SEQ_1:def 5;then
A2:Z c= dom id Z & Z c= dom f by XBOOLE_1:18;
A3: for x st x in Z holds (id Z).x = 1*x+0 by FUNCT_1:35;then
A4:id Z is_differentiable_on Z & for x st x in Z holds
     ((id Z)`|Z).x = 1 by FDIFF_1:31,A2;
A5:f is_differentiable_on Z & for x st x in Z holds
     (f`|Z).x = 1/x by A1,A2,Th19;
   for x st x in Z holds (((id Z)(#)f)`|Z).x =1+ln.x
   proof
     let x;
     assume
A6:  x in Z;then f1.x=x by A1;then
A7:  x<>0 by A1,A6;
     (((id Z)(#)f)`|Z).x =((id Z).x)*diff(f,x)+(f.x)*diff((id Z),x)
       by A5,A1,A4,FDIFF_1:29,A6
      .=((id Z).x)*((f`|Z).x)+(f.x)*diff((id Z),x) by A5,A6,FDIFF_1:def 8
      .=((id Z).x)*(1/x) + (f.x)*diff((id Z),x) by A6,A1,A2,Th19
      .=x*(1/x) + (f.x)*diff((id Z),x) by FUNCT_1:35,A6
      .=x*(1/x) + (f.x)*(((id Z)`|Z).x) by A4,A6,FDIFF_1:def 8
      .=x*(1/x) + (f.x)*1 by A3,A6,FDIFF_1:31,A2
      .=x*(1/x) + ln.(f1.x) by A1,A6,FUNCT_1:22,A2
      .=x*(1/x) + ln.x by A1,A6
      .= 1 + ln.x by A7,XCMPLX_1:107;
     hence thesis;
   end;
   hence thesis by A5,A1,A4,FDIFF_1:29;
end;

:: 21 f.x=x^2*ln(x)
theorem
  Z c= dom (g(#)f) & g=#Z 2 & f = ln*f1 &
  (for x st x in Z holds f1.x=x & f1.x>0) implies
  g(#)f is_differentiable_on Z &
  for x st x in Z holds ((g(#)f)`|Z).x = x+2*x*ln.x
proof
   assume
A1:Z c= dom (g(#)f) & g=#Z 2 & f = ln*f1 &
   (for x st x in Z holds f1.x=x & f1.x>0);
   then Z c= dom (g) /\ dom (f) by SEQ_1:def 5; then
A2:Z c= dom g & Z c= dom (f) by XBOOLE_1:18;
   for x st x in Z holds g is_differentiable_in x by A1,TAYLOR_1:2; then
A3:g is_differentiable_on Z by FDIFF_1:16,A2;
A4:for x st x in Z holds (g`|Z).x = 2*x
   proof
     let x;
     assume
A5:  x in Z;
     diff(g,x) = 2 * (x #Z (2-1)) by A1,TAYLOR_1:2
              .=2*x by PREPOWER:45;
     hence (g`|Z).x =2*x by A5,A3,FDIFF_1:def 8;
   end;
A6:f is_differentiable_on Z & for x st x in Z holds
     (f`|Z).x = 1/x by A1,A2,Th19;
   now let x;
     assume
A7:  x in Z; then f1.x=x by A1; then
A8:  x<>0 by A1,A7;
     ((g(#)f)`|Z).x =(g.x)*diff(f,x) + (f.x)*diff(g,x)
       by A3,A1,A6,A7,FDIFF_1:29
     .=(g.x)*((f`|Z).x)+(f.x)*diff(g,x) by A6,A7,FDIFF_1:def 8
     .=(g.x)*(1/x) + (f.x)*diff(g,x) by A7,A1,A2,Th19
     .=(x #Z 2)*(1/x) + (f.x)*diff(g,x) by A1,TAYLOR_1:def 1
     .=(x #Z 2)*(1/x) + (f.x)*((g`|Z).x) by A3,A7,FDIFF_1:def 8
     .=(x #Z 2)*(1/x) + ((ln*f1).x)*(2*x) by A1,A7,A4
     .=(x #Z 2)*(1/x) + (ln.(f1.x))*(2*x) by A1,A2,A7,FUNCT_1:22
     .=(x #Z (1+1))*(1/x) + 2*x*(ln.x) by A1,A7
     .=((x #Z 1)*(x #Z 1))*(1/x) + 2*x*(ln.x) by TAYLOR_1:1
     .=((x #Z 1)*x)*(1/x) + 2*x*(ln.x) by PREPOWER:45
     .=(x*x)*(1/x) + 2*x*(ln.x) by PREPOWER:45
     .=x*(x*(1/x)) + 2*x*(ln.x)
     .=x*1 + 2*x*(ln.x) by XCMPLX_1:107,A8
     .=x+2*x*(ln.x);
     hence ((g(#)f)`|Z).x = x+2*x*ln.x;
   end;
   hence thesis by A3,A1,A6,FDIFF_1:29;
end;

:: 22 f.x=(a+x^2)/(a-x^2)
theorem Th22:
  Z c= dom ((f1+f2)/(f1-f2)) &
  (for x st x in Z holds f1.x=a) & f2=#Z 2 &
  (for x st x in Z holds (f1-f2).x>0) implies
  (f1+f2)/(f1-f2) is_differentiable_on Z &
  for x st x in Z holds (((f1+f2)/(f1-f2))`|Z).x = (4*a*x)/(a-x|^2)|^2
  proof
    assume
A1: Z c= dom ((f1+f2)/(f1-f2)) &
    (for x st x in Z holds f1.x=a) & f2=#Z 2 &
    (for x st x in Z holds (f1-f2).x>0);
    Z c= dom (f1+f2) /\ (dom (f1-f2)\(f1-f2)"{0}) by A1,RFUNCT_1:def 4; then
A2: Z c= dom (f1+f2) & Z c= dom (f1-f2) \ (f1-f2)"{0} by XBOOLE_1:18;
A3: Z c= dom (f1-f2) by XBOOLE_1:1,A2;
A4: f1+f2 is_differentiable_on Z &
    for x st x in Z holds ((f1+f2)`|Z).x = 2*x by A2,A1,Lm1;
A5: f1-f2 is_differentiable_on Z &
    for x st x in Z holds ((f1-f2)`|Z).x = -2*x by A3,A1,Lm2;
A6:  for x st x in Z holds (f1-f2).x <> 0 by A1;then
A7:(f1+f2)/(f1-f2) is_differentiable_on Z by A4,A5,FDIFF_2:21;
    for x st x in Z holds (((f1+f2)/(f1-f2))`|Z).x = (4*a*x)/(a-x|^2)|^2
    proof
      let x;
      assume
A8:   x in Z;then
A9:  f1+f2 is_differentiable_in x by A4,FDIFF_1:16;
A10:  f1-f2 is_differentiable_in x by A5,A8,FDIFF_1:16;
A11:  f1.x=a & f2=#Z 2 & (f1-f2).x <>0 by A1,A8;
      then diff((f1+f2)/(f1-f2),x)
        =(diff((f1+f2),x)*(f1-f2).x -
          diff((f1-f2),x)*(f1+f2).x)/((f1-f2).x)^2 by FDIFF_2:14,A10,A9
       .=(diff((f1+f2),x)*(f1.x-f2.x)-
          diff((f1-f2),x)*(f1+f2).x)/((f1-f2).x)^2 by A8,SEQ_1:def 4,A3
       .=(diff((f1+f2),x)*(f1.x-f2.x)- diff((f1-f2),x)*
         (f1.x+f2.x))/((f1-f2).x)^2 by A8,SEQ_1:def 3,A2
       .=(diff((f1+f2),x)*(f1.x-f2.x)- diff((f1-f2),x)*
         (f1.x+f2.x))/(f1.x-f2.x)^2 by A8,SEQ_1:def 4,A3
       .=((((f1+f2)`|Z).x)*(f1.x-f2.x)- diff((f1-f2),x)*
         (f1.x+f2.x))/(f1.x-f2.x)^2 by A8,A4,FDIFF_1:def 8
       .=((2*x)*(f1.x-f2.x)-diff((f1-f2),x)*(f1.x+f2.x))/(f1.x-f2.x)^2
         by A8,A2,A1,Lm1
       .=((2*x)*(f1.x-f2.x)-(((f1-f2)`|Z).x)*(f1.x+f2.x))/(f1.x-f2.x)^2
         by A8,A5,FDIFF_1:def 8
       .=((2*x)*(f1.x-f2.x)-(-2*x)*(f1.x+f2.x))/(f1.x-f2.x)^2 by A8,A3,A1,Lm2
       .=(4*x*a)/(a-(x #Z 2))^2 by A11,TAYLOR_1:def 1
       .=(4*x*a)/(a-x|^2)^2 by PREPOWER:46
       .=(4*x*a)/((a-x|^2)|^1*(a- x|^2)) by NEWTON:10
       .=(4*x*a)/(a-x|^2)|^(1+1)by NEWTON:11
       .= (4*a*x)/(a-x|^2)|^2;
      hence (((f1+f2)/(f1-f2))`|Z).x = (4*a*x)/(a-x|^2)|^2
        by FDIFF_1:def 8,A8,A7;
    end;
    hence thesis by A6,A4,A5,FDIFF_2:21;
  end;

:: 23 f.x=ln((a+x^2)/(a-x^2))
theorem
  Z c= dom (ln*((f1+f2)/(f1-f2))) &
    (for x st x in Z holds f1.x=a) & f2=#Z 2 &
    (for x st x in Z holds (f1-f2).x>0) &
    (for x st x in Z holds (f1+f2).x>0) implies
  ln*((f1+f2)/(f1-f2)) is_differentiable_on Z &
  for x st x in Z holds
    ((ln*((f1+f2)/(f1-f2)))`|Z).x = (4*a*x)/(a|^2-x|^4)
  proof
    assume
A1: Z c= dom (ln*((f1+f2)/(f1-f2))) &
    (for x st x in Z holds f1.x=a) & f2=#Z 2 &
    (for x st x in Z holds (f1-f2).x>0) &
    (for x st x in Z holds (f1+f2).x >0);
    for y st y in Z holds y in dom ((f1+f2)/(f1-f2)) by FUNCT_1:21,A1; then
A2: Z c= dom ((f1+f2)/(f1-f2)) by TARSKI:def 3;
    then Z c= dom (f1+f2) /\ (dom (f1-f2) \ (f1-f2)"{0}) by RFUNCT_1:def 4;
    then
A3: Z c= dom (f1+f2) & Z c= dom (f1-f2) \ (f1-f2)"{0} by XBOOLE_1:18;
A4: Z c= dom (f1-f2) by XBOOLE_1:1,A3;
A5: (f1+f2)/(f1-f2) is_differentiable_on Z &
    for x st x in Z holds (((f1+f2)/(f1-f2))`|Z).x = (4*a*x)/(a-x|^2)|^2
      by Th22,A1,A2;
A6: for x st x in Z holds ((f1+f2)/(f1-f2)).x >0
    proof
      let x;
      assume
A7:   x in Z;then
A8:   x in dom ((f1+f2)/(f1-f2)) by FUNCT_1:21,A1;
A9:   (f1+f2).x >0 & (f1-f2).x >0 by A1,A7;
      ((f1+f2)/(f1-f2)).x=(f1+f2).x * ((f1-f2).x)" by A8,RFUNCT_1:def 4
      .=(f1+f2).x / (f1-f2).x by XCMPLX_0:def 9;
      hence thesis by XREAL_1:141,A9;
    end;
A10: for x st x in Z holds
      ln*((f1+f2)/(f1-f2)) is_differentiable_in x
    proof
      let x;
      assume
A11:  x in Z;then
A12:  (f1+f2)/(f1-f2) is_differentiable_in x by FDIFF_1:16,A5;
      ((f1+f2)/(f1-f2)).x >0 by A11,A6;
      hence ln*((f1+f2)/(f1-f2))
        is_differentiable_in x by TAYLOR_1:20,A12;
    end;then
A13:  ln*((f1+f2)/(f1-f2)) is_differentiable_on Z
      by A1,FDIFF_1:16;
    for x st x in Z holds
      ((ln*((f1+f2)/(f1-f2)))`|Z).x = (4*a*x)/(a|^2-x|^4)
    proof
      let x;
      assume
A14:  x in Z;then
A15:  ((f1+f2)/(f1-f2)) is_differentiable_in x by FDIFF_1:16,A5;
A16:  x in dom ((f1+f2)/(f1-f2)) by FUNCT_1:21,A1,A14;
A17:  f1.x=a & f2=#Z 2 & ((f1+f2)/(f1-f2)).x > 0 by A1,A14,A6;
A18:  (f1-f2).x <>0 & (f1+f2).x <>0 by A1,A14;
      diff(ln*((f1+f2)/(f1-f2)),x) =
        diff(((f1+f2)/(f1-f2)),x)/(((f1+f2)/(f1-f2)).x) by TAYLOR_1:20,A15,A17
    .=(((f1+f2)/(f1-f2))`|Z).x/(((f1+f2)/(f1-f2)).x) by A5,A14,FDIFF_1:def 8
    .=(((f1+f2)/(f1-f2))`|Z).x/((f1+f2).x *((f1-f2).x)") by A16,RFUNCT_1:def 4
    .= ((4*a*x)/(f1.x-x|^2)|^2)/(((f1+f2).x) *((f1-f2).x)")
        by A17,Th22,A1,A2,A14
    .= ((4*a*x)/(f1.x-(x #Z 2))|^2)/(((f1+f2).x) *((f1-f2).x)") by PREPOWER:46
    .= ((4*a*x)/(f1.x-f2.x)|^2)/(((f1+f2).x) *((f1-f2).x)")
        by A1,TAYLOR_1:def 1
    .= ((4*a*x)/((f1-f2).x)|^2)/(((f1+f2).x) *((f1-f2).x)")
        by A4,A14,SEQ_1:def 4
    .= ((4*a*x)/(((f1-f2).x)|^(1+1)))/((f1+f2).x/(f1-f2).x) by XCMPLX_0:def 9
    .= ((4*a*x)/(((f1-f2).x)|^1*(f1-f2).x))/((f1+f2).x/(f1-f2).x) by NEWTON:11
    .= ((4*a*x)/((f1-f2).x*(f1-f2).x))/((f1+f2).x/(f1-f2).x) by NEWTON:10
    .= ((4*a*x)/(f1-f2).x/(f1-f2).x)/((f1+f2).x/(f1-f2).x) by XCMPLX_1:79
    .= ((4*a*x)/(f1-f2).x)/((f1+f2).x/(f1-f2).x)/(f1-f2).x by XCMPLX_1:48
    .= (4*a*x)/(f1+f2).x/(f1-f2).x by A18,XCMPLX_1:55
    .= (4*a*x)/((f1+f2).x*(f1-f2).x) by XCMPLX_1:79
    .= (4*a*x)/((f1.x+f2.x)*(f1-f2).x) by A14,SEQ_1:def 3,A3
    .= (4*a*x)/((f1.x+f2.x)*(f1.x-f2.x)) by A4,A14,SEQ_1:def 4
    .= (4*a*x)/(a*a-f2.x*f2.x) by A17
    .= (4*a*x)/(a*a-(x #Z 2)*f2.x) by A1,TAYLOR_1:def 1
    .= (4*a*x)/(a*a-(x #Z 2)*(x #Z 2)) by A1,TAYLOR_1:def 1
    .= (4*a*x)/(a|^1*a- (x #Z 2)*(x #Z 2)) by NEWTON:10
    .= (4*a*x)/(a|^(1+1)- (x #Z 2)*(x #Z 2)) by NEWTON:11
    .= (4*a*x)/(a|^(1+1)- (x|^2)*(x #Z 2)) by PREPOWER:46
    .= (4*a*x)/(a|^2- (x|^2)*(x|^2)) by PREPOWER:46
    .= (4*a*x)/(a|^2- x|^(2+2)) by NEWTON:13
    .= (4*a*x)/(a|^2- x|^4);
       hence thesis by A14,A13,FDIFF_1:def 8;
     end;
     hence thesis by A1,FDIFF_1:16,A10;
   end;

:: 24 f.x=(1/x)*ln(x)
theorem
  Z c= dom ((f^)(#)g) & (for x st x in Z holds f.x = x) &
    g = ln*f1 & (for x st x in Z holds f1.x=x & f1.x >0)
      implies (f^)(#)g is_differentiable_on Z &
  for x st x in Z holds (((f^)(#)g)`|Z).x =(1/x^2)*(1-ln.x)
proof
    assume
A1: Z c= dom ((f^)(#)g) & (for x st x in Z holds f.x = x) &
      g = ln*f1 & (for x st x in Z holds f1.x=x & f1.x >0); then
    Z c= dom (f^) /\ dom g by SEQ_1:def 5;then
A2: Z c= dom (f^) & Z c= dom g by XBOOLE_1:18;
    dom (f^) c= dom f by RFUNCT_1:11;then
A3: Z c= dom f by XBOOLE_1:1,A2;
A4:  for x st x in Z holds f.x = x & f.x<>0
    proof
      let x; assume A5:x in Z; then
A6:   f.x = x by A1;
      f1.x = x by A5,A1;
      hence thesis by A6,A5,A1;
    end;
A7: f^ is_differentiable_on Z & for x st x in Z holds
    ((f^)`|Z).x= -1/x^2 by Th4,A3,A4;
A8: g is_differentiable_on Z & for x st x in Z holds
     (g`|Z).x = 1/x by A1,A2,Th19;
    now let x;
      assume
A9:   x in Z;
      (((f^)(#)g)`|Z).x
      = (g.x)*diff((f^),x)+((f^).x)*diff(g,x) by A7,A1,FDIFF_1:29,A8,A9
     .=(g.x)*((f^)`|Z).x+ ((f^).x)*diff(g,x) by A7,A9,FDIFF_1:def 8
     .=(g.x)*(-1/x^2)+ ((f^).x)*diff(g,x) by A9,Th4,A3,A4
     .=(g.x)*(-1/x^2)+ ((f^).x)*((g`|Z).x) by A9,FDIFF_1:def 8,A8
     .=(g.x)*(-1/x^2)+((f^).x)*(1/x) by A9,A1,A2,Th19
     .=(g.x)*(-1/x^2)+((f.x)")*(1/x) by A2,A9,RFUNCT_1:def 8
     .=(g.x)*(-1/x^2)+(1*x")*(1/x) by A9,A1
     .=-(1/x^2)*(g.x)+(1/x)*(1/x) by XCMPLX_0:def 9
     .=-(1/x^2)*((ln*f1).x)+1/(x^2) by  XCMPLX_1:103,A1
     .=-(1/x^2)*(ln.(f1.x))+1/(x^2) by A1,A2,A9,FUNCT_1:22
     .=-(1/x^2)*ln.x+1/(x^2) by A1,A9
     .=(1/x^2)*(1-ln.x);
      hence (((f^)(#)g)`|Z).x =(1/x^2)*(1-ln.x);
    end;
    hence thesis by A7,A1,FDIFF_1:29,A8;
  end;

:: 25 f.x=1/ln(x)
theorem
  Z c= dom (f^) & f = ln*f1 &
  (for x st x in Z holds f1.x=x & f1.x >0) &
  (for x st x in Z holds f.x<>0) implies
  (f^) is_differentiable_on Z &
  for x st x in Z holds ((f^)`|Z).x =-1/(x*(ln.x)^2)
proof
    assume
A1: Z c= dom (f^) & f = ln*f1 &
    (for x st x in Z holds f1.x=x & f1.x >0) &
    (for x st x in Z holds f.x<>0);
    dom (f^) c= dom f by RFUNCT_1:11;then
A2: Z c= dom f by XBOOLE_1:1,A1;
A3: f is_differentiable_on Z & for x st x in Z holds
      (f`|Z).x = 1/x by A1,A2,Th19;
A4: f^ is_differentiable_on Z by A3,FDIFF_2:22,A1;
    for x st x in Z holds ((f^)`|Z).x= - 1/(x*(ln.x)^2)
    proof
      let x;
      assume
A5:   x in Z;
A6:   f1.x= x & f1.x>0 & f.x<>0 by A1,A5;
A7:   f is_differentiable_in x by A5,A3,FDIFF_1:16;
      ((f^)`|Z).x= diff(f^,x) by A4,A5,FDIFF_1:def 8
      .= -diff(f,x)/(f.x)^2 by A6,FDIFF_2:15,A7
      .= -((f`|Z).x)/(f.x)^2 by A3,FDIFF_1:def 8,A5
      .= -(1/x)/((ln*f1).x)^2 by A1,A2,Th19,A5
      .= -((1/x)/(ln.x)^2) by A5,A6,A1,A2,FUNCT_1:22
      .= -1/(x*(ln.x)^2) by XCMPLX_1:79;
      hence thesis;
    end;
    hence thesis by A3,FDIFF_2:22,A1;
  end;
