   assume
A1:Z c= dom ((sin*f)(#)(( #Z n)*sin)) & n>=1 &
   (for x st x in Z holds f.x=n*x); then
   Z c= dom (sin*f) /\ dom (( #Z n)*sin) by SEQ_1:def 5; then
A2:Z c= dom (sin*f) & Z c= dom (( #Z n)*sin) by XBOOLE_1:18;
   for y st y in Z holds y in dom f by FUNCT_1:21,A2; then
A3:Z c= dom f by TARSKI:def 3;
A4: for x st x in Z holds f.x=n*x+0 by A1;
A5:f is_differentiable_on Z &
    for x st x in Z holds (f`|Z).x = n by A4,A3,FDIFF_1:31;
   for x st x in Z holds sin*f is_differentiable_in x
   proof
     let x;
     assume x in Z; then
A6:  f is_differentiable_in x by FDIFF_1:16,A5;
     sin is_differentiable_in f.x by SIN_COS:69;
     hence sin*f is_differentiable_in x by A6,FDIFF_2:13;
   end; then
A7:sin*f is_differentiable_on Z by FDIFF_1:16,A2;
A8:for x st x in Z holds ((sin*f)`|Z).x = n*cos.(n*x)
   proof
     let x;
     assume
A9:  x in Z; then
A10:  f is_differentiable_in x by FDIFF_1:16,A5;
     sin is_differentiable_in f.x by SIN_COS:69;
     then diff(sin*f,x) = diff(sin, f.x)*diff(f,x) by FDIFF_2:13,A10
      .=cos.( f.x)*diff(f,x) by SIN_COS:69
      .=cos.(n*x)*diff(f,x) by A1,A9
      .=cos.(n*x)*(f`|Z).x by FDIFF_1:def 8,A9,A5
      .=n* cos.(n*x) by A9,A4,A3,FDIFF_1:31;
     hence ((sin*f)`|Z).x = n* cos.(n*x) by FDIFF_1:def 8,A7,A9;
   end;
   now let x;
     assume x in Z;
     sin is_differentiable_in x by SIN_COS:69;
     hence ( #Z n)*sin is_differentiable_in x by TAYLOR_1:3;
   end; then
A11:( #Z n)*sin is_differentiable_on Z by A2,FDIFF_1:16;
A12:for x st x in Z holds
     ((( #Z n)*sin)`|Z).x =n*((sin.x) #Z (n-1))*cos.x
   proof
     let x;
     assume
A13: x in Z;
A14: sin is_differentiable_in x by SIN_COS:69;
     diff((( #Z n)*sin),x) =(n*((sin.x) #Z (n-1)) * diff(sin,x))
       by TAYLOR_1:3,A14
      .=n*((sin.x) #Z (n-1)) *cos.x by SIN_COS:69;
     hence ((( #Z n)*sin)`|Z).x =n*((sin.x) #Z (n-1)) *cos.x
       by FDIFF_1:def 8,A13,A11;
   end;
   now let x;
     assume
A15:  x in Z;
A16:  n-1 is Element of NAT by A1,NAT_1:21;
     (((sin*f)(#)(( #Z n)*sin))`|Z).x
       =(( #Z n)*sin).x*diff(sin*f,x)+(sin*f).x*diff((( #Z n)*sin),x)
        by A11,A1,FDIFF_1:29,A7,A15
      .=(( #Z n).(sin.x))*diff(sin*f,x)+(sin*f).x*diff((( #Z n)*sin),x)
        by FUNCT_1:22,A15,A2
      .=((sin.x) #Z n)*diff(sin*f,x)+(sin*f).x*diff((( #Z n)*sin),x)
        by TAYLOR_1:def 1
      .=((sin.x) #Z n)*((sin*f)`|Z).x+(sin*f).x*diff((( #Z n)*sin),x)
        by A7,A15,FDIFF_1:def 8
      .=((sin.x) #Z n)*(n* cos.(n*x))+(sin*f).x*diff((( #Z n)*sin),x) by A8,A15
      .=((sin.x) #Z n)*(n* cos.(n*x))+(sin.(f.x))*diff((( #Z n)*sin),x)
         by A15,FUNCT_1:22, A2
      .=((sin.x) #Z n)*(n* cos.(n*x))+(sin.(n*x))*diff((( #Z n)*sin),x)
         by A1,A15
      .=((sin.x) #Z n)*(n* cos.(n*x))+(sin.(n*x))*((( #Z n)*sin)`|Z).x
         by A11,A15,FDIFF_1:def 8
      .=((sin.x) #Z n)*(n* cos.(n*x))+(sin.(n*x))*(n*((sin.x) #Z (n-1))*cos.x)
         by A15,A12
      .=((sin.x) #Z ((n-1)+1))*(n* cos.(n*x))+
