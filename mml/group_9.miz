A44:      j in dom(s1|i) by A43,FINSEQ_1:def 3;
A45:      dom(s1|i) c= dom s1 by RELAT_1:89;
A46:      s2.j = s2/.j by A25,PARTFUN1:def 8
            .= s1/.j by A44,FINSEQ_5:75
