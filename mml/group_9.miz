      end;
      len s1 in Seg len s1 by A1;
      then
A60:  len s1 in dom s1 by FINSEQ_1:def 3;
      1 in Seg len s1 by A1;
      then
A61:  1 in dom s1 by FINSEQ_1:def 3;
      1 <= len s1 + 1 by A1,NAT_1:13;
