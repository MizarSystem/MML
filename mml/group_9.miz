          end;
          suppose
A112:       i=len s1-1;
            then
A113:       k1 = (len s1-1)*(len s2-1) by A65,A99;
A114:       K9 = (1).G by A99,Def31;
            H9 = (1).G by A112,Def31;
            then JH = (1).G"\/"(1).G by A114,Th21
              .= (1).G by Th33;
