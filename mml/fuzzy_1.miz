    A /\ (B \/ D) = (A /\ B) \/ (A /\ D) by Th17;
 then A /\ (B \/ D) c= (A /\ B) \/ (A /\ D)
 & (A /\ B) \/ (A /\ D) c= A /\ (B \/ D) by Lm2;
 then (A /\ B) \/ (A /\ D) c= A & A c= (A /\ B) \/ (A /\ D) by A2,Th5;
 then A3:(A /\ B) \/ (A /\ D) = A by Lm1;
    for x being Element of C holds f.x <= g.x
