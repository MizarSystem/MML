  thus O_el(Y) 'eqv' I_el(Y) = 'not' (O_el(Y) 'xor' I_el(Y)) by BVFUNC25:12
    .= 'not' (O_el(Y) 'xor' 'not' O_el(Y)) by BVFUNC_1:5
    .= 'not' I_el(Y) by BVFUNC25:14
    .= O_el(Y) by BVFUNC_1:5;
end;

theorem
