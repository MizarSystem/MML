SCM_HALT:82;

A23:  ff.n=gg.m by A6,A11,A13,A8,A17,FUNCT_7:115;
      hereby
        let i be Element of NAT;
        assume that
A24:    s.b3-(k+1)<i and
A25:    i<=s.b3;
