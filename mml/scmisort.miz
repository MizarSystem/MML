  end;
  then for s1,s2 being State of SCM+FSA st p c= s1 & p c= s2 for i holds
  Comput(ProgramPart( s1), s1,i)|dom p = Comput(ProgramPart( s2), s2,i)|dom p;
  hence thesis by EXTPRO_1:def 9;
end;

registration
 cluster Insert-Sort-Algorithm -> non halt-free;
 coherence;
end;

theorem
  Insert-Sort-Algorithm, Initialized Insert-Sort-Algorithm
     computes Sorting-Function
proof
  set q = Insert-Sort-Algorithm,
   p = Initialized Insert-Sort-Algorithm;
F2: q c= p by SCMFSA6A:26;
  let x be set;
  assume x in dom Sorting-Function;
  then consider w being FinSequence of INT such that
A1: x = fsloc 0 .--> w by SCMBSORT:60;
  reconsider d = x as FinPartState of SCM+FSA by A1;
  consider t being State of SCM+FSA such that
A2: p +* d c= t by PBOOLE:156;
A3: dom p misses dom d by A1,SCMBSORT:46;
A4: now
    set bf=insert-sort fsloc 0;
    let t be State of SCM+FSA;
    assume
A5: p+*d c= t;
    p c=p+*d by A3,FUNCT_4:33;
    then p c= t by A5,XBOOLE_1:1;
    hence ProgramPart t halts_on t by Lm12,EXTPRO_1:def 10;
  end;
  take d;
  thus x = d;
  d c= p +* d by FUNCT_4:26;
  then
A7: dom d c= dom(p +* d) by RELAT_1:25;
A10: dom d = { fsloc 0 } by A1,FUNCOP_1:19;
xx:
  now assume dom d meets NAT;
    then consider x being set such that
W1:  x in dom d and
W2:  x in NAT by XBOOLE_0:3;
    x = fsloc 0 by W1,A10,TARSKI:def 1;
   hence contradiction by W2,SCMFSA_2:85;
  end;
   fsloc 0 in FinSeq-Locations by SCMFSA_2:10;
   then
X1:  {fsloc 0} c= FinSeq-Locations by ZFMISC_1:37;
X3:  dom q c= NAT by RELAT_1:def 18;
  dom d misses NAT by XBOOLE_1:63,X1,A10,SCMFSA_2:14;
     then
XX:dom d misses dom q by X3,XBOOLE_1:63;
 p +* d +* q
           = p +* (d +* q) by FUNCT_4:15
          .= p +* (q +* d) by XX,FUNCT_4:36
          .= p +* q +* d by FUNCT_4:15
          .= p+*d by F2,FUNCT_4:79;
   then p +* d +* q is halting autonomic by A1,A4,Th48,EXTPRO_1:def 10;
  hence
LL: p +* d is Autonomy of q by EXTPRO_1:def 11;
K: dom d misses {IC SCM+FSA} by A10,SCMFSA_2:82,ZFMISC_1:17;
  dom d misses {IC SCM+FSA} \/ NAT by K,XBOOLE_1:70,xx;
  then d is data-only by COMPOS_1:def 23;
  then dom d misses NAT by COMPOS_1:40;
  then
NN:  ProgramPart(p +* d)= ProgramPart p by FUNCT_4:76;
KK: p +* d is Autonomy of q by LL;
  fsloc 0 in the carrier of SCM+FSA;
  then
A8: fsloc 0 in dom Result(ProgramPart(t),t) by PARTFUN1:def 4;
VV: q = ProgramPart p by SCMFSA8C:25;
A9: Result(q,p +* d) = (Result(ProgramPart(t),t))|dom(p +* d)
        by A2,EXTPRO_1:def 12,KK,VV,NN;
  consider u being FinSequence of REAL such that
A11: w,u are_fiberwise_equipotent and
A12: u is non-increasing and
  u is FinSequence of INT and
A13: (Result(ProgramPart(t),t)).(fsloc 0) = u by A1,A2,Th47;
  consider z being FinSequence of REAL such that
A14: w,z are_fiberwise_equipotent and
A15: z is non-increasing and
  z is FinSequence of INT and
A16: Sorting-Function.d = fsloc 0 .--> z by A1,SCMBSORT:61;
A17: dom(fsloc 0 .--> z) = { fsloc 0 } by FUNCOP_1:19;
u: u=z by A14,A15,A11,A12,CLASSES1:84,RFINSEQ:36;
x1: dom(fsloc 0 .--> z) c= dom(p +* d)
     by A1,A7,A17,FUNCOP_1:19;
  thus thesis by A16,x1,A9,A13,A8,u,FUNCT_4:90,RELAT_1:186;
end;

