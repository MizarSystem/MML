 constructors REAL_1, INT_1, FINSEQ_4, GRAPH_5, GRAPH_4, DOMAIN_1, BINARITH,
      GOBOARD1, FUNCT_7, MEMBERED, RELAT_2, PARTFUN1, NAT_1;
 registrations FRAENKEL, INT_1, GRAPH_5, FINSEQ_1, FINSET_1, RELSET_1, GRAPH_1,
