      .=n*(sin.x) #Z (n-1)/((cos.x) #Z (n-1)*(cos.x)^2) by XCMPLX_1:79
      .=n*((sin.x) #Z (n-1))/((cos.x) #Z (n-1)*(cos.x) #Z 2) by FDIFF_7:1
      .=n*((sin.x) #Z (n-1))/((cos.x) #Z (n-1+2)) by A4,A8,PREPOWER:54
