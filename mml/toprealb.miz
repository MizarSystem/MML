theorem Th27:
  for p being Point of Tunit_circle(2),
      x being Point of TOP-REAL 2 st x is Point of Topen_unit_circle(p) holds
  -1 <= x`1 & x`1 <= 1 & -1 <= x`2 & x`2 <= 1
proof
  let p be Point of Tunit_circle(2),
      x be Point of TOP-REAL 2;
  assume x is Point of Topen_unit_circle(p);
  then
A1: x in the carrier of Topen_unit_circle(p);
  the carrier of Topen_unit_circle(p) is Subset of cS1 by TSEP_1:1;
  hence thesis by A1,Th13;
end;

theorem Th28:
  for x being Point of TOP-REAL 2 st x is Point of Topen_unit_circle(c[10])
  holds -1 <= x`1 & x`1 < 1
proof
  let x be Point of TOP-REAL 2;
  assume
A1: x is Point of Topen_unit_circle(c[10]);
  then
A2: x`1 <= 1 by Th27;
  now
    assume
A3:  x`1 = 1;
A4: the carrier of TOUC = cS1 \ {c[10]} by Def10;
  then
A5: x in cS1 by A1,XBOOLE_0:def 4;
A6: not x in {c[10]} by A1,A4,XBOOLE_0:def 4;
    x = |[x`1,x`2]| by EUCLID:57;
    then x = c[10] by A3,A5,Th14;
    hence contradiction by A6,TARSKI:def 1;
  end;
  hence thesis by A1,A2,Th27,REAL_1:def 5;
end;

theorem Th29:
  for x being Point of TOP-REAL 2 st x is Point of Topen_unit_circle(c[-10])
  holds -1 < x`1 & x`1 <= 1
proof
  let x be Point of TOP-REAL 2;
  assume
A1: x is Point of Topen_unit_circle(c[-10]);
  then
A2: -1 <= x`1 by Th27;
  now
    assume
A3:  x`1 = -1;
A4: the carrier of TOUCm = cS1 \ {c[-10]} by Def10;
  then
A5: x in cS1 by A1,XBOOLE_0:def 4;
A6: not x in {c[-10]} by A1,A4,XBOOLE_0:def 4;
    x = |[x`1,x`2]| by EUCLID:57;
    then x = c[-10] by A3,A5,Th15;
    hence contradiction by A6,TARSKI:def 1;
  end;
  hence thesis by A1,A2,Th27,REAL_1:def 5;
end;

registration
  let p be Point of Tunit_circle(2);
  cluster Topen_unit_circle(p) -> open;
coherence
proof
  let A be Subset of TUC such that
A1: A = the carrier of Topen_unit_circle(p);
A2: TUC is being_T2 by TOPMETR:3;
  A` = cS1 \ (cS1 \ {p}) by A1,Def10
    .= cS1 /\ {p} by XBOOLE_1:48
    .= {p} by ZFMISC_1:52;
  then A` is closed by A2,PCOMPS_1:10;
  hence A is open by TOPS_1:30;
end;
end;

theorem
  for p being Point of Tunit_circle(2) holds
  Topen_unit_circle(p), I(01) are_homeomorphic
proof
  let p be Point of Tunit_circle(2);
  set P = Topen_unit_circle(p);
  set D = Sphere(0.REAL 2,p1);
  reconsider p2 = p as Point of TOP-REAL 2 by TOPMETR:2;
A1: [#]TUC = D by Lm14,EUCLID:58;
 D\{p} c= D by XBOOLE_1:36;
  then reconsider A = D\{p} as Subset of Tcircle(0.REAL 2,1)
    by Th9;
  reconsider A1 = A as Subset of (TOP-REAL 2) | D;
  P = Tcircle(0.REAL 2,1) | A by  Th23,A1
   .= (TOP-REAL 2) | (D \ {p2}) by GOBOARD9:4;
  hence P,I(01) are_homeomorphic by Lm14,BORSUK_4:77,EUCLID:58;
end;

theorem
  for p, q being Point of Tunit_circle(2) holds
  Topen_unit_circle(p), Topen_unit_circle(q) are_homeomorphic
proof
  let p, q be Point of Tunit_circle(2);
  set P = Topen_unit_circle(p);
  set Q = Topen_unit_circle(q);
  set D = Sphere(0.REAL 2,p1);
  reconsider p2 = p, q2 = q as Point of TOP-REAL 2 by TOPMETR:2;
A1: [#]TUC = D by Lm14,EUCLID:58;
 D\{p} c= D & D\{q} c= D by XBOOLE_1:36;
  then reconsider A = D\{p}, B = D\{q} as Subset of Tcircle(0.REAL 2,1)
    by Th9;
  reconsider A1 = A, B1 = B as Subset of (TOP-REAL 2) | D;
A2: P = Tcircle(0.REAL 2,1) | A by  Th23,A1
     .= (TOP-REAL 2) | (D \ {p2}) by GOBOARD9:4;
  Q = Tcircle(0.REAL 2,1) | B by  Th23,A1
     .= (TOP-REAL 2) | (D \ {q2}) by GOBOARD9:4;
  hence P,Q are_homeomorphic by A2,Lm14,BORSUK_4:78,EUCLID:58;
end;

begin :: Correspondence between the real line and circles

definition
  func CircleMap -> Function of R^1, Tunit_circle(2) means :Def11:
  for x being real number holds it.x = |[ cos(2*PI*x), sin(2*PI*x) ]|;
existence
proof
  defpred P[real number,set] means $2 = |[ cos(2*PI*$1), sin(2*PI*$1) ]|;
A1: for x being Element of R^1
   ex y being Element of cS1 st P[x,y]
  proof
    let x be Element of R^1;
    set y = |[ cos(2*PI*x), sin(2*PI*x) ]|;
    |. y - o .| = |. y .| by EUCLID:58,JORDAN2C:13
      .= sqrt((y`1)^2+(y`2)^2) by JGRAPH_1:47
      .= sqrt((cos(2*PI*x))^2+(y`2)^2) by EUCLID:56
      .= sqrt((cos(2*PI*x))^2+(sin(2*PI*x))^2) by EUCLID:56
      .= 1 by SIN_COS:32,SQUARE_1:83;
    then y is Element of cS1 by Lm14,TOPREAL9:9;
    hence thesis;
  end;
  consider f being Function of R, cS1 such that
A2: for x being Element of R^1 holds P[x,f.x] from FUNCT_2:sch 3(A1);
  take f;
  let x be real number;
  x is Point of R^1 by TOPMETR:24,XREAL_0:def 1;
  hence thesis by A2;
end;
uniqueness
proof
  let f, g be Function of R^1, TUC such that
A3: for x being real number holds f.x = |[cos(2*PI*x),sin(2*PI*x)]|
  and
A4: for x being real number holds g.x = |[cos(2*PI*x),sin(2*PI*x)]|;
  for x being Point of R^1 holds f.x = g.x
  proof
    let x be Point of R^1;
    thus f.x = |[cos(2*PI*x),sin(2*PI*x)]| by A3
      .= g.x by A4;
  end;
  hence f = g by FUNCT_2:113;
end;
end;

Lm19: dom CircleMap = REAL by FUNCT_2:def 1,TOPMETR:24;

theorem Th32:
  CircleMap.r = CircleMap.(r+i)
proof
  defpred P[Integer] means CircleMap.r = CircleMap.(r+$1);
A1: P[0];
A2: for i holds P[i] implies P[i-1] & P[i+1]
  proof
    let i such that
A3:   P[i];
    thus CircleMap.(r+(i-1)) = |[ cos(2*PI*(r+i-1)), sin(2*PI*(r+i-1)) ]|
    by Def11
    .= |[ cos(2*PI*(r+i)), sin(2*PI*(r+i)+2*PI*(-1)) ]| by COMPLEX2:10
    .= |[ cos(2*PI*(r+i)), sin(2*PI*(r+i)) ]| by COMPLEX2:9
    .= CircleMap.r by A3,Def11;
    thus CircleMap.(r+(i+1)) = |[ cos(2*PI*(r+i+1)), sin(2*PI*(r+i+1)) ]|
    by Def11
    .= |[ cos(2*PI*(r+i)), sin(2*PI*(r+i)+2*PI*1) ]| by COMPLEX2:10
    .= |[ cos(2*PI*(r+i)), sin(2*PI*(r+i)) ]| by COMPLEX2:9
    .= CircleMap.r by A3,Def11;
  end;
  for i holds P[i] from INT_1:sch 4(A1,A2);
  hence thesis;
end;

theorem Th33:
  CircleMap.i = c[10]
proof
  thus CircleMap.i = |[ cos(2*PI*i+0), sin(2*PI*i) ]| by Def11
    .= |[ cos(0), sin(2*PI*i+0) ]| by COMPLEX2:10
    .= c[10] by COMPLEX2:9,SIN_COS:34;
end;

theorem Th34:
  CircleMap"{c[10]} = INT
proof
  hereby
    let i be set;
    assume
A1:   i in CircleMap"{c[10]};
    then CircleMap.i in {c[10]} by FUNCT_2:46;
    then
A2: CircleMap.i = c[10] by TARSKI:def 1;
    reconsider x = i as Real by A1,TOPMETR:24;
    |[ cos(2*PI*x), sin(2*PI*x) ]| = |[1,0]| by A2,Def11;
    then cos(2*PI*x) = 1 by SPPOL_2:1;
    hence i in INT by SIN_COS6:44;
  end;
  let i be set;
  assume i in INT;
  then reconsider i as Integer by INT_1:def 2;
A3: i in R by TOPMETR:24,XREAL_0:def 1;
  CircleMap.i = c[10] by Th33;
  then CircleMap.i in {c[10]} by TARSKI:def 1;
  hence thesis by A3,FUNCT_2:46;
end;

Lm20:
CircleMap.(1/2) = |[-1,0]|
proof
  thus CircleMap.(1/2) = |[ cos(2*PI*(1/2)), sin(2*PI*(1/2)) ]| by Def11
  .= |[-1,0]| by SIN_COS:82;
end;

Lm21:
CircleMap.1 = |[1,0]| by Th33;

theorem Th35:
  frac r = 1/2 implies CircleMap.r = |[-1,0]|
proof
  assume
A1: frac r = 1/2;
  thus CircleMap.r = CircleMap.(r+-[\r/]) by Th32
    .= CircleMap.(r-[\r/])
    .= |[-1,0]| by A1,Lm20,INT_1:def 6;
end;

theorem
  frac r = 1/4 implies CircleMap.r = |[0,1]|
proof
  assume frac r = 1/4;
  then
A1: r-[\r/] = 1/4 by INT_1:def 6;
  thus CircleMap.r = CircleMap.(r+-[\r/]) by Th32
    .= |[ cos(2*PI*(1/4)), sin(2*PI*(1/4)) ]| by A1,Def11
    .= |[0,1]| by SIN_COS:82;
end;

theorem
  frac r = 3/4 implies CircleMap.r = |[0,-1]|
proof
  assume frac r = 3/4;
  then
A1: r-[\r/] = 3/4 by INT_1:def 6;
  thus CircleMap.r = CircleMap.(r+-[\r/]) by Th32
    .= |[ cos(2*PI*(3/4)), sin(2*PI*(3/4)) ]| by A1,Def11
    .= |[0,-1]| by SIN_COS:82;
end;

Lm22:
  CircleMap.r = |[ (cos*AffineMap(2*PI,0)).r, (sin*AffineMap(2*PI,0)).r ]|
proof
  thus CircleMap.r = |[ cos(2*PI*r+0), sin(2*PI*r) ]| by Def11
    .= |[ (cos*AffineMap(2*PI,0)).r, sin(2*PI*r+0) ]| by Th2
    .= |[ (cos*AffineMap(2*PI,0)).r, (sin*AffineMap(2*PI,0)).r ]| by Th1;
end;

theorem
  for i, j being Integer holds
  CircleMap.r =
    |[ (cos*AffineMap(2*PI,2*PI*i)).r, (sin*AffineMap(2*PI,2*PI*j)).r ]|
proof
  let i, j be Integer;
  thus CircleMap.r = |[ cos(2*PI*r+0), sin(2*PI*r) ]| by Def11
    .= |[ cos(2*PI*r+2*PI*i), sin(2*PI*r+0) ]| by COMPLEX2:10
    .= |[ cos(2*PI*r+2*PI*i), sin(2*PI*r+2*PI*j) ]| by COMPLEX2:9
    .= |[ (cos*AffineMap(2*PI,2*PI*i)).r, sin(2*PI*r+2*PI*j) ]| by Th2
    .= |[ (cos*AffineMap(2*PI,2*PI*i)).r, (sin*AffineMap(2*PI,2*PI*j)).r ]|
       by Th1;
end;

registration
  cluster CircleMap -> continuous;
coherence
proof
A1: AffineMap(2*PI,0) = R^1(AffineMap(2*PI,0));
  dom sin = REAL by SIN_COS:def 20;
  then
A2: R^1|(R^1 dom sin) = R^1 by Th6;
  dom cos = REAL by SIN_COS:def 22;
  then
A3: R^1|(R^1 dom cos) = R^1 by Th6;
  set sR = R^1(sin), cR = R^1(cos);
  reconsider sR, cR as Function of R^1,R^1 by Lm11,Lm12;
  reconsider l = AffineMap(2*PI,0) as Function of R^1,R^1 by Th8;
  dom AffineMap(2*PI,0) = REAL by FUNCT_2:def 1;
  then
A4: R^1|(R^1 dom AffineMap(2*PI,0)) = R^1 by Th6;
A5: rng AffineMap(2*PI,0) = [#]REAL by JORDAN16:32;
  set s = sR*l;
  set c = cR*l;
  reconsider g = CircleMap as Function of R^1,TOP-REAL 2 by TOPREALA:28;
  for p being Point of R^1, V being Subset of TOP-REAL 2
   st g.p in V & V is open
  ex W being Subset of R^1 st p in W & W is open & g.:W c= V
  proof
    let p be Point of R^1,
        V be Subset of TOP-REAL 2 such that
A6:   g.p in V and
A7:   V is open;
A8: V = Int V by A7,TOPS_1:55;
    reconsider e = g.p as Point of Euclid 2 by TOPREAL3:13;
    consider r being real number such that
A9:   r > 0 and
A10:   Ball(e,r) c= V by A6,A8,GOBOARD6:8;
    set A = ].(g.p)`1-r/sqrt 2,(g.p)`1+r/sqrt 2.[;
    set B = ].(g.p)`2-r/sqrt 2,(g.p)`2+r/sqrt 2.[;
    set F = (1,2) --> (A,B);
    sqrt 2 > 0 by SQUARE_1:84;
    then
A11: r/sqrt 2 > 0 by A9,REAL_2:127;
A12: product F c= Ball(e,r) by TOPREAL6:49;
    reconsider A, B as Subset of R^1 by TOPMETR:24;
A13: B is open by JORDAN6:46;
A14: g.p = |[ c.p, s.p ]| by Lm22;
    then
A15: (g.p)`2 = s.p by EUCLID:56;
    sR is continuous by A2,TOPMETR:7;
    then
A16: s is continuous by A1,A4,A5,Th5,TOPS_2:58;
    s.p in B by A11,A15,TOPREAL6:20;
    then consider Ws being Subset of R^1 such that
A17:   p in Ws and
A18:   Ws is open and
A19:   s.:Ws c= B by A13,A16,JGRAPH_2:20;
A20: A is open by JORDAN6:46;
A21: (g.p)`1 = c.p by A14,EUCLID:56;
    cR is continuous by A3,TOPMETR:7;
    then
A22: c is continuous by A1,A4,A5,Th5,TOPS_2:58;
    c.p in A by A11,A21,TOPREAL6:20;
    then consider Wc being Subset of R^1 such that
A23:  p in Wc and
A24:  Wc is open and
A25:  c.:Wc c= A by A20,A22,JGRAPH_2:20;
    set W = Ws /\ Wc;
    take W;
    thus p in W by A17,A23,XBOOLE_0:def 3;
    thus W is open by A18,A24,TOPS_1:38;
    let y be set;
    assume y in g.:W;
    then consider x being Element of R^1 such that
A26:  x in W and
A27:  y = g.x by FUNCT_2:116;
A28: g.x = |[ c.x, s.x ]| by Lm22;
    x in Ws by A26,XBOOLE_0:def 3;
    then
A29: s.x in s.:Ws by FUNCT_2:43;
    x in Wc by A26,XBOOLE_0:def 3;
    then
A30: c.x in c.:Wc by FUNCT_2:43;
    |[c.x,s.x]| = (1,2) --> (c.x,s.x) by TOPREALA:49;
    then |[c.x,s.x]| in product F by A19,A25,A29,A30,HILBERT3:11;
    then |[c.x,s.x]| in Ball(e,r) by A12;
    hence y in V by A10,A27,A28;
  end;
  then g is continuous by JGRAPH_2:20;
  hence thesis by TOPMETR:8;
end;
end;

Lm23:
  for A being Subset of R^1 holds CircleMap|A is Function
 of R^1|A, Tunit_circle(2)
proof
  let A be Subset of R^1;
A1: dom (CircleMap|A) = A by Lm19,RELAT_1:91,TOPMETR:24
  .= the carrier of R^1|A by PRE_TOPC:29;
  rng (CircleMap|A) c= cS1;
  hence CircleMap|A is Function of R^1|A, TUC by A1,FUNCT_2:4;
end;

Lm24:
-1 <= r & r <= 1 implies 0 <= arccos r/(2*PI) & arccos r/(2*PI) <= 1/2
proof
  assume -1 <= r & r <= 1;
  then 0 <= arccos r & arccos r <= PI by SIN_COS6:101;
  then 0/(2*PI) <= arccos r/(2*PI) &
    arccos r/(2*PI) <= 1*PI/(2*PI) by REAL_1:73;
  hence thesis by XCMPLX_1:92;
end;

theorem Th39:
  for A being Subset of R^1, f being Function of R^1|A, Tunit_circle(2)
   st [.0,1.[ c= A & f = CircleMap | A
  holds f is onto
proof
  let A be Subset of R^1,
      f be Function of R^1|A, Tunit_circle(2) such that
A1: [.0,1.[ c= A and
A2: f = CircleMap | A;
  thus rng f c= cS1;
  let y be set;
  assume
A3: y in cS1;
  then reconsider z = y as Point of TOP-REAL 2 by TOPMETR:2;
  set z1 = z`1, z2 = z`2;
  set x = arccos z1/(2*PI);
A4: dom f = A by A2,Lm19,RELAT_1:91,TOPMETR:24;
A5: |. z .| = 1 by A3,Th12;
A6: z1^2 + z2^2 = |. z .|^2 by JGRAPH_1:46;
A7: -1 <= z1 & z1 <= 1 by A3,Th13;
A8: 0 <= x by A7,Lm24;
   x <= 1/2 by A7,Lm24;
   then
A9: x < 1 by XXREAL_0:2;
  per cases;
  suppose
A10: z2 < 0;
A11: now
    assume x = 0;
    then arccos z1 = 0 by XCMPLX_1:50;
    then z1 = 1 by A7,SIN_COS6:98;
    then 1 + z2^2 - 1 = 0 by A5,A6;
    hence contradiction by A10,XCMPLX_1:6;
  end;
  1-0 > 1-x & 1-x > 1-1 by A8,A9,A11,REAL_1:92;
  then
A12: 1-x in [.0,1.[ by RCOMP_2:3;
  then f.(1-x) = CircleMap.(-x+1) by A1,A2,FUNCT_1:72
    .= CircleMap.-x by Th32
    .= |[cos(-2*PI*x), sin(2*PI*(-x))]| by Def11
    .= |[cos(2*PI*x), sin(2*PI*(-x))]| by SIN_COS:34
    .= |[cos arccos z1, sin(-2*PI*x)]| by XCMPLX_1:88
    .= |[cos arccos z1, -sin(2*PI*x)]| by SIN_COS:34
    .= |[cos arccos z1, -sin arccos z1]| by XCMPLX_1:88
    .= |[z1, -sin arccos z1]| by A7,SIN_COS6:93
    .= |[z1, --z2]| by A5,A6,A10,SIN_COS6:105
    .= y by EUCLID:57;
  hence thesis by A1,A4,A12,FUNCT_1:def 5;
  end;
  suppose
A13: z2 >= 0;
A14: x in [.0,1.[ by A8,A9,RCOMP_2:3;
  then f.x = CircleMap.x by A1,A2,FUNCT_1:72
    .= |[cos(2*PI*x), sin(2*PI*x)]| by Def11
    .= |[cos arccos z1, sin(2*PI*x)]| by XCMPLX_1:88
    .= |[cos arccos z1, sin arccos z1]| by XCMPLX_1:88
    .= |[z1, sin arccos z1]| by A7,SIN_COS6:93
    .= |[z1, z2]| by A5,A6,A13,SIN_COS6:104
    .= y by EUCLID:57;
  hence thesis by A1,A4,A14,FUNCT_1:def 5;
  end;
end;

registration
  cluster CircleMap -> onto;
coherence
proof
A1: [#]R^1 = REAL by TOPMETR:24;
A2: R^1 | [#]R^1 = R^1 by TSEP_1:3;
  CircleMap|REAL = CircleMap by Lm19,RELAT_1:98;
  hence thesis by A1,A2,Th39;
end;
end;

Lm25:
CircleMap | [.0,1.[ is one-to-one
proof
  set f = CircleMap | [.0,1.[;
A1: dom f = [.0,1.[ by Lm19,RELAT_1:91;
  let x1, y1 be set;
  assume
A2: x1 in dom f;
  then reconsider x = x1 as Real by A1;
  assume
A3: y1 in dom f;
  then reconsider y = y1 as Real by A1;
  assume
A4: f.x1 = f.y1;
A5: f.x = CircleMap.x by A1,A2,FUNCT_1:72
     .= |[cos(2*PI*x),sin(2*PI*x)]| by Def11;
A6: f.y = CircleMap.y by A1,A3,FUNCT_1:72
     .= |[cos(2*PI*y),sin(2*PI*y)]| by Def11;
  then
A7: cos(2*PI*x) = cos(2*PI*y) by A4,A5,SPPOL_2:1;
A8: sin(2*PI*x) = sin(2*PI*y) by A4,A5,A6,SPPOL_2:1;
A9: sin(2*PI*x) = sin.(2*PI*x) by SIN_COS:def 21;
A10: sin(2*PI*y) = sin.(2*PI*y) by SIN_COS:def 21;
A11: cos(2*PI*x) = cos.(2*PI*x) by SIN_COS:def 23;
A12: cos(2*PI*y) = cos.(2*PI*y) by SIN_COS:def 23;
A13: [.0,PI/2.] c= [.-PI/2,PI/2.] by RCOMP_1:49;
A14: sin | [.PI/2,3/2*PI.] is one-to-one;
  per cases by A1,A2,RCOMP_2:3;
  suppose 0 <= x & x <= 1/4;
  then 2*PI*0 <= 2*PI*x & 2*PI*x <= 2*PI*(1/4) by XREAL_1:66;
  then
A15: 2*PI*x in [.0,2*PI*1/4.] by RCOMP_1:48;
  then
A16: cos(2*PI*x) >= 0 by A11,A13,COMPTRIG:28;
  [.0,PI/2.] c= [.0,PI.] by Lm5,RCOMP_1:49;
  then
A17: sin(2*PI*x) >= 0 by A9,A15,COMPTRIG:24;
  per cases by A1,A3,RCOMP_2:3;
  suppose 0 <= y & y <= 1/4;
  then 2*PI*0 <= 2*PI*y & 2*PI*y <= 2*PI*(1/4) by XREAL_1:66;
  then
A18: 2*PI*y in [.0,2*PI*1/4.] by RCOMP_1:48;
  set g = sin | [.0,PI/2.];
A19: dom g = [.0,PI/2.] by Lm8,RELAT_1:91;
  g.(2*PI*x) = sin.(2*PI*x) by A15,FUNCT_1:72
     .= sin.(2*PI*y) by A8,A9,SIN_COS:def 21
     .= g.(2*PI*y) by A18,FUNCT_1:72;
  then 2*PI*x = 2*PI*y by A15,A18,A19,FUNCT_1:def 8;
  then x = 2*PI*y/P2 by XCMPLX_1:90;
  hence thesis by XCMPLX_1:90;
  end;
  suppose 1/4 < y & y < 3/4;
  then 2*PI*(1/4) < 2*PI*y & 2*PI*y < 2*PI*(3/4) by XREAL_1:70;
  then 2*PI*y in ].PI/2,3/2*PI.[ by RCOMP_1:47;
  hence thesis by A7,A12,A16,COMPTRIG:29;
  end;
  suppose 3/4 <= y & y < 1;
  then 2*PI*(3/4) <= 2*PI*y & 2*PI*y < 2*PI*1 by XREAL_1:66,70;
  then
A20: 2*PI*y in [.3/2*PI,2*PI.[ by RCOMP_2:3;
  [.3/2*PI,2*PI.[ c= ].PI,2*PI.[ by Lm6,TOPREALA:14;
  hence thesis by A8,A10,A17,A20,COMPTRIG:25;
  end;
  end;
  suppose 1/4 < x & x <= 1/2;
  then 2*PI*(1/4) < 2*PI*x & 2*PI*x <= 2*PI*(1/2) by XREAL_1:66,70;
  then
A21: 2*PI*x in ].PI/2,2*PI*(1/2).] by RCOMP_2:4;
  ].PI/2,PI.] c= ].PI/2,3/2*PI.[ by Lm6,TOPREALA:18;
  then
A22: cos(2*PI*x) < 0 by A11,A21,COMPTRIG:29;
  ].PI/2,PI.] c= [.0,PI.] by TOPREALA:15;
  then
A23: sin(2*PI*x) >= 0 by A9,A21,COMPTRIG:24;
  per cases by A1,A3,RCOMP_2:3;
  suppose 0 <= y & y <= 1/4;
  then 2*PI*0 <= 2*PI*y & 2*PI*y <= 2*PI*(1/4) by XREAL_1:66;
  then 2*PI*y in [.0,2*PI*(1/4).] by RCOMP_1:48;
  hence thesis by A7,A12,A13,A22,COMPTRIG:28;
  end;
  suppose 1/4 < y & y <= 1/2;
  then 2*PI*(1/4) < 2*PI*y & 2*PI*y <= 2*PI*(1/2) by XREAL_1:66,70;
  then
A24: 2*PI*y in ].2*PI*(1/4),2*PI*(1/2).] by RCOMP_2:4;
  set g = sin | ].PI/2,PI.];
  ].PI/2,PI.] c= [.PI/2,3/2*PI.] by Lm6,TOPREALA:15;
  then
A25: g is one-to-one by A14,SIN_COS6:2;
A26: dom g = ].PI/2,PI.] by Lm8,RELAT_1:91;
  g.(2*PI*x) = sin.(2*PI*x) by A21,FUNCT_1:72
     .= sin.(2*PI*y) by A8,A9,SIN_COS:def 21
     .= g.(2*PI*y) by A24,FUNCT_1:72;
  then 2*PI*x = 2*PI*y by A21,A24,A25,A26,FUNCT_1:def 8;
  then x = 2*PI*y/P2 by XCMPLX_1:90;
  hence thesis by XCMPLX_1:90;
  end;
  suppose 1/2 < y & y < 1;
  then 2*PI*(1/2) < 2*PI*y & 2*PI*y < 2*PI*1 by XREAL_1:70;
  then 2*PI*y in ].PI,2*PI.[ by RCOMP_1:47;
  hence thesis by A8,A10,A23,COMPTRIG:25;
  end;
  end;
  suppose 1/2 < x & x <= 3/4;
  then 2*PI*(1/2) < 2*PI*x & 2*PI*x <= 2*PI*(3/4) by XREAL_1:66,70;
  then
A27: 2*PI*x in ].PI,2*PI*(3/4).] by RCOMP_2:4;
  ].PI,3/2*PI.] c= ].PI,2*PI.[ by Lm7,TOPREALA:18;
  then
A28: sin(2*PI*x) < 0 by A9,A27,COMPTRIG:25;
  ].PI,3/2*PI.] c= [.PI/2,3/2*PI.] by Lm5,TOPREALA:15;
  then
A29: cos(2*PI*x) <= 0 by A11,A27,COMPTRIG:30;
  per cases by A1,A3,RCOMP_2:3;
  suppose 0 <= y & y <= 1/2;
  then 2*PI*0 <= 2*PI*y & 2*PI*y <= 2*PI*(1/2) by XREAL_1:66;
  then 2*PI*y in [.0,PI.] by RCOMP_1:48;
  hence thesis by A8,A10,A28,COMPTRIG:24;
  end;
  suppose 1/2 < y & y <= 3/4;
  then 2*PI*(1/2) < 2*PI*y & 2*PI*y <= 2*PI*(3/4) by XREAL_1:66,70;
  then
A30: 2*PI*y in ].PI,2*PI*(3/4).] by RCOMP_2:4;
  set g = sin | ].PI,3/2*PI.];
  ].PI,3/2*PI.] c= [.PI/2,3/2*PI.] by Lm5,TOPREALA:15;
  then
A31: g is one-to-one by A14,SIN_COS6:2;
A32: dom g = ].PI,3/2*PI.] by Lm8,RELAT_1:91;
  g.(2*PI*x) = sin.(2*PI*x) by A27,FUNCT_1:72
     .= sin.(2*PI*y) by A8,A9,SIN_COS:def 21
     .= g.(2*PI*y) by A30,FUNCT_1:72;
  then 2*PI*x = 2*PI*y by A27,A30,A31,A32,FUNCT_1:def 8;
  then x = 2*PI*y/P2 by XCMPLX_1:90;
  hence thesis by XCMPLX_1:90;
  end;
  suppose 3/4 < y & y < 1;
  then 2*PI*(3/4) < 2*PI*y & 2*PI*y < 2*PI*1 by XREAL_1:70;
  then 2*PI*y in ].3/2*PI,2*PI.[ by RCOMP_1:47;
  hence thesis by A7,A12,A29,COMPTRIG:31;
  end;
  end;
  suppose 3/4 < x & x < 1;
  then 2*PI*(3/4) < 2*PI*x & 2*PI*x < 2*PI*1 by XREAL_1:70;
  then
A33: 2*PI*x in ].3/2*PI,2*PI.[ by RCOMP_1:47;
  ].3/2*PI,2*PI.[ c= ].PI,2*PI.[ by Lm6,RCOMP_1:52;
  then
A34: sin(2*PI*x) < 0 by A9,A33,COMPTRIG:25;
A35: cos(2*PI*x) > 0 by A11,A33,COMPTRIG:31;
  per cases by A1,A3,RCOMP_2:3;
  suppose 0 <= y & y <= 1/2;
  then 2*PI*0 <= 2*PI*y & 2*PI*y <= 2*PI*(1/2) by XREAL_1:66;
  then 2*PI*y in [.0,PI.] by RCOMP_1:48;
  hence thesis by A8,A10,A34,COMPTRIG:24;
  end;
  suppose 1/2 < y & y <= 3/4;
  then 2*PI*(1/2) < 2*PI*y & 2*PI*y <= 2*PI*(3/4) by XREAL_1:66,70;
  then
A36: 2*PI*y in ].PI,3/2*PI.] by RCOMP_2:4;
  ].PI,3/2*PI.] c= [.PI/2,3/2*PI.] by Lm5,TOPREALA:15;
  hence thesis by A7,A12,A35,A36,COMPTRIG:30;
  end;
  suppose 3/4 < y & y < 1;
  then 2*PI*(3/4) < 2*PI*y & 2*PI*y < 2*PI*1 by XREAL_1:70;
  then
A37: 2*PI*y in ].3/2*PI,2*PI.[ by RCOMP_1:47;
  set g = sin | ].3/2*PI,2*PI.[;
A38: dom g = ].3/2*PI,2*PI.[ by Lm8,RELAT_1:91;
  g.(2*PI*x) = sin.(2*PI*x) by A33,FUNCT_1:72
     .= sin.(2*PI*y) by A8,A9,SIN_COS:def 21
     .= g.(2*PI*y) by A37,FUNCT_1:72;
  then 2*PI*x = 2*PI*y by A33,A37,A38,FUNCT_1:def 8;
  then x = 2*PI*y/P2 by XCMPLX_1:90;
  hence thesis by XCMPLX_1:90;
  end;
  end;
end;

registration
  let r be real number;
  cluster CircleMap | [.r,r+1.[ -> one-to-one;
coherence
proof
  set f = CircleMap | [.r,r+1.[;
  set g = CircleMap | [.0,1.[;
A1: dom f = [.r,r+1.[ by Lm19,RELAT_1:91;
A2: dom g = [.0,1.[ by Lm19,RELAT_1:91;
  let x, y be set;
  assume that
A3: x in dom f and
A4: y in dom f and
A5: f.x = f.y;
  reconsider x, y as Real by A1,A3,A4;
  set x1 = frac x;
  set y1 = frac y;
A6: x1 = x-[\x/] by INT_1:def 6;
A7: y1 = y-[\y/] by INT_1:def 6;
   0 <= x1 & x1 < 1 by INT_1:69;
   then
A8: x1 in [.0,1.[ by RCOMP_2:3;
   0 <= y1 & y1 < 1 by INT_1:69;
   then
A9: y1 in [.0,1.[ by RCOMP_2:3;
A10: r <= x & x < r+1 by A1,A3,RCOMP_2:3;
A11: r <= y & y < r+1 by A1,A4,RCOMP_2:3;
A12: f.x = CircleMap.x by A3,FUNCT_1:70
     .= CircleMap.(x+-[\x/]) by Th32
     .= g.x1 by A2,A6,A8,FUNCT_1:70;
  f.y = CircleMap.y by A4,FUNCT_1:70
     .= CircleMap.(y+-[\y/]) by Th32
     .= g.y1 by A2,A7,A9,FUNCT_1:70;
  then x1 = y1 by A2,A5,A8,A9,A12,Lm25,FUNCT_1:def 8;
  hence thesis by A10,A11,TOPREALA:7;
end;
end;

registration
  let r be real number;
  cluster CircleMap | ].r,r+1.[ -> one-to-one;
coherence
proof
A1: CircleMap | [.r,r+1.[ is one-to-one;
  ].r,r+1.[ c= [.r,r+1.[ by TOPREALA:20;
  hence thesis by A1,SIN_COS6:2;
end;
end;

theorem Th40:
  b-a <= 1 implies
  for d being set st d in IntIntervals(a,b) holds CircleMap|d is one-to-one
proof
  assume that
A1: b-a <= 1;
  let d be set;
  assume d in IntIntervals(a,b);
  then consider n being Element of INT such that
A2: d = ].a+n,b+n.[;
A3: CircleMap | [.a+n,a+n+1.[ is one-to-one;
  b-a+(a+n) <= 1+(a+n) by A1,XREAL_1:8;
  then ].a+n,b+n.[ c= [.a+n,a+n+1.[ by TOPREALA:20;
  hence thesis by A2,A3,SIN_COS6:2;
end;

theorem Th41:
  for d being set st d in IntIntervals(a,b)
  holds CircleMap.:d = CircleMap.:union IntIntervals(a,b)
proof
  set D = IntIntervals(a,b);
  let d be set;
  assume
A1: d in D;
  then d c= union D by ZFMISC_1:92;
  hence CircleMap.:d c= CircleMap.:union D by RELAT_1:156;
  let y be set;
  assume y in CircleMap.:union D;
  then consider x being Element of R^1 such that
A2:  x in union D and
A3:  y = CircleMap.x by FUNCT_2:116;
  consider Z being set such that
A4: x in Z and
A5: Z in D by A2,TARSKI:def 4;
  consider n being Element of INT such that
A6: Z = ]. a+n, b+n .[ by A5;
  consider i being Element of INT such that
A7: d = ]. a+i, b+i .[ by A1;
  set k = x+i-n;
A8: k in R by TOPMETR:24,XREAL_0:def 1;
  a+n < x & x < b+n by A4,A6,RCOMP_1:47;
  then a+n+i < x+i & x+i < b+n+i by XREAL_1:8;
  then a+n+i-n < x+i-n & x+i-n < b+n+i-n by XREAL_1:11;
  then
A9: k in d by A7,RCOMP_1:47;
  CircleMap.k = CircleMap.(x+(i-n))
    .= y by A3,Th32;
  hence y in CircleMap.:d by A8,A9,FUNCT_2:43;
end;

definition
  let r be Point of R^1;
  func CircleMap(r) ->
       Function of R^1 | R^1(].r,r+1.[), Topen_unit_circle(CircleMap.r) equals

  CircleMap | ].r,r+1.[;
coherence
proof
  set X = Topen_unit_circle(CircleMap.r);
  set A = ].r,r+1.[;
  set B = [.r,r+1.[;
  set f = CircleMap | A;
  set g = CircleMap | B;
A1: A c= B by TOPREALA:20;
A2: dom f = A by Lm19,RELAT_1:91;
A3: the carrier of X = cS1 \ {CircleMap.r} by Def10;
A4: rng f c= the carrier of X
  proof
    let y be set;
    assume
A5:   y in rng f;
    now
      assume
A6:    y = CircleMap.r;
      consider x being set such that
A7:     x in dom f and
A8:     f.x = y by A5,FUNCT_1:def 5;
      r+0 < r+1 by XREAL_1:10;
      then
A9:  r in B by RCOMP_2:3;
A10:  dom g = B by Lm19,RELAT_1:91;
      g.x = CircleMap.x by A1,A2,A7,FUNCT_1:72
         .= CircleMap.r by A2,A6,A7,A8,FUNCT_1:72
         .= g.r by A9,FUNCT_1:72;
      then x = r by A1,A2,A7,A9,A10,FUNCT_1:def 8;
      hence contradiction by A2,A7,RCOMP_1:47;
    end;
    then not y in {CircleMap.r} by TARSKI:def 1;
    hence thesis by A3,A5,XBOOLE_0:def 4;
  end;
  the carrier of R^1 | R^1(A) = A by PRE_TOPC:29;
  hence thesis by A2,A4,FUNCT_2:4;
end;
end;

Lm26:
rng (AffineMap(1,-a) | ].r+a,r+a+1.[) = ].r,r+1.[
proof
  set F = AffineMap(1,-a);
  set f = F | ].r+a,r+a+1.[;
  dom F = REAL by FUNCT_2:def 1;
  then
A1: dom f = ].r+a,r+a+1.[ by RELAT_1:91;
  thus rng f = ].r,r+1.[
  proof
    thus rng f c= ].r,r+1.[
    proof
      let y be set;
      assume y in rng f;
      then consider x being set such that
A2:     x in dom f and
A3:     f.x = y by FUNCT_1:def 5;
      reconsider x as Real by A2;
A4:   y = F.x by A2,A3,FUNCT_1:70
       .= 1*x+-a by JORDAN16:def 3;
      r+a < x & x < r+a+1 by A1,A2,RCOMP_1:47;
      then r+a-a < x-a & x-a < r+a+1-a by XREAL_1:11;
      hence thesis by A4,RCOMP_1:47;
    end;
    let y be set;
    assume
A5:   y in ].r,r+1.[;
    then reconsider y as Real;
    r < y & y < r+1 by A5,RCOMP_1:47;
    then r+a < y+a & y+a < r+1+a by XREAL_1:8;
    then
A6: y+a in ].r+a,r+a+1.[ by RCOMP_1:47;
    then f.(y+a) = F.(y+a) by FUNCT_1:72
      .= 1*(y+a)+-a by JORDAN16:def 3
      .= y;
    hence thesis by A1,A6,FUNCT_1:def 5;
  end;
end;

theorem Th42:
  CircleMap(R^1(a+i)) = CircleMap(R^1(a)) * (AffineMap(1,-i) | ].a+i,a+i+1.[)
proof
  set W = ].a,a+1.[;
  set Q = ].a+i,a+i+1.[;
  set h = CircleMap(R^1(a+i));
  set g = CircleMap(R^1(a));
  set F = AffineMap(1,-i);
  set f = F|Q;
A1: dom h = Q by Lm19,RELAT_1:91;
A2: dom g = W by Lm19,RELAT_1:91;
  dom F = REAL by FUNCT_2:def 1;
  then
A3: dom f = Q by RELAT_1:91;
  rng f = ].a,a+1.[ by Lm26;
  then
A4: dom (g*f) = dom f by A2,RELAT_1:46;
  for x being set st x in dom h holds h.x = (g*f).x
  proof
    let x be set;
    assume
A5:   x in dom h;
    reconsider y = x as Real by A1,A5;
    a+i < y & y < a+i+1 by A1,A5,RCOMP_1:47;
    then a+i-i < y-i & y-i < a+i+1-i by XREAL_1:11;
    then
A6: y-i in W by RCOMP_1:47;
    thus (g*f).x = g.(f.x) by A1,A3,A5,FUNCT_1:23
      .= g.(F.x) by A1,A5,FUNCT_1:72
      .= g.(1*y+-i) by JORDAN16:def 3
      .= CircleMap.(y+-i) by A6,FUNCT_1:72
      .= CircleMap.y by Th32
      .= h.x by A1,A5,FUNCT_1:72;
  end;
  hence thesis by A1,A3,A4,FUNCT_1:9;
end;

registration
  let r be Point of R^1;
  cluster CircleMap(r) -> one-to-one onto continuous;
coherence
proof
  thus CircleMap(r) is one-to-one;
  thus CircleMap(r) is onto
  proof
  set A = ].r,r+1.[;
  set f = CircleMap | A;
  set TOUC = Topen_unit_circle(CircleMap.r);
  set X = the carrier of TOUC;
  thus rng CircleMap(r) c= X;
  let y be set;
  assume
A1: y in X;
  reconsider z = y as Point of TOP-REAL 2 by A1,Lm9;
  set z1 = z`1, z2 = z`2;
  set x = arccos z1/(2*PI);
A2: [\r/] <= r by INT_1:def 4;
A3: dom f = ].r,r+1.[ by Lm19,RELAT_1:91;
  z is Point of TUC by A1,TOPMETR:2;
  then
A4: |. z .| = 1 by Th12;
A5: z1^2 + z2^2 = |. z .|^2 by JGRAPH_1:46;
A6: -1 <= z1 & z1 <= 1 by A1,Th27;
A7: 0 <= x by A6,Lm24;
   x <= 1/2 by A6,Lm24;
   then
A8: x < 1 by XXREAL_0:2;
A9: x-x < 1-x by A8,REAL_1:92;
  per cases;
  suppose
A10: z2 < 0;
A11: CircleMap.-x = |[cos(-2*PI*x), sin(2*PI*(-x))]| by Def11
    .= |[cos(2*PI*x), sin(2*PI*(-x))]| by SIN_COS:34
    .= |[cos arccos z1, sin(-2*PI*x)]| by XCMPLX_1:88
    .= |[cos arccos z1, -sin(2*PI*x)]| by SIN_COS:34
    .= |[cos arccos z1, -sin arccos z1]| by XCMPLX_1:88
    .= |[z1, -sin arccos z1]| by A6,SIN_COS6:93
    .= |[z1, --z2]| by A4,A5,A10,SIN_COS6:105
    .= y by EUCLID:57;
  per cases;
  suppose
A12: 1-x+[\r/] in A;
  then f.(1-x+[\r/]) = CircleMap.(-x+([\r/]+1)) by FUNCT_1:72
    .= CircleMap.-x by Th32;
  hence thesis by A3,A11,A12,FUNCT_1:def 5;
  end;
  suppose
A13: not 1-x+[\r/] in A;
  now
    assume x = 0;
    then arccos z1 = 0 by XCMPLX_1:50;
    then z1 = 1 by A6,SIN_COS6:98;
    then 1 + z2^2 - 1 = 0 by A4,A5;
    hence contradiction by A10,XCMPLX_1:6;
  end;
  then [\r/]-x < r-0 by A2,A7,REAL_1:92;
  then -x+[\r/]+1 < r+1 by XREAL_1:8;
  then
A14: r >= 1-x+[\r/] by A13,RCOMP_1:47;
A15: now
    assume -x+[\r/]+1 = r;
    then CircleMap.r = CircleMap.(-x+([\r/]+1))
      .= CircleMap.-x by Th32;
    hence contradiction by A1,A11,Th22;
  end;
A16: r < [\r/]+1 by INT_1:52;
  [\r/]+1+0 < [\r/]+1+(1-x) by A9,XREAL_1:8;
  then
A17: r < 2-x+[\r/] by A16,XXREAL_0:2;
  1-x+[\r/] < r by A14,A15,REAL_1:def 5;
  then 1-x+[\r/]+1 < r+1 by XREAL_1:8;
  then
A18: -x+[\r/]+2 in A by A17,RCOMP_1:47;
  then f.(-x+([\r/]+2)) = CircleMap.(-x+([\r/]+2)) by FUNCT_1:72
    .= CircleMap.-x by Th32;
  hence thesis by A3,A11,A18,FUNCT_1:def 5;
  end;
  end;
  suppose
A19: z2 >= 0;
A20: CircleMap.x = |[cos(2*PI*x), sin(2*PI*x)]| by Def11
    .= |[cos arccos z1, sin(2*PI*x)]| by XCMPLX_1:88
    .= |[cos arccos z1, sin arccos z1]| by XCMPLX_1:88
    .= |[z1, sin arccos z1]| by A6,SIN_COS6:93
    .= |[z1, z2]| by A4,A5,A19,SIN_COS6:104
    .= y by EUCLID:57;
  per cases;
  suppose
A21: x+[\r/] in A;
  then f.(x+[\r/]) = CircleMap.(x+[\r/]) by FUNCT_1:72
    .= CircleMap.x by Th32;
  hence thesis by A3,A20,A21,FUNCT_1:def 5;
  end;
  suppose
A22: not x+[\r/] in A;
A23: r < [\r/]+1 by INT_1:52;
  0+([\r/]+1) <= x+([\r/]+1) by A7,XREAL_1:8;
  then
A24: r < x+[\r/]+1 by A23,XXREAL_0:2;
  x+[\r/] < 1+r by A2,A8,XREAL_1:10;
  then x+[\r/] <= r by A22,RCOMP_1:47;
  then
A25: x+[\r/]+1 <= r+1 by XREAL_1:8;
  now
    assume x+[\r/]+1 = r+1;
    then CircleMap.r = CircleMap.x by Th32;
    hence contradiction by A1,A20,Th22;
  end;
  then x+[\r/]+1 < r+1 by A25,REAL_1:def 5;
  then
A26: x+[\r/]+1 in A by A24,RCOMP_1:47;
  then f.(x+[\r/]+1) = CircleMap.(x+([\r/]+1)) by FUNCT_1:72
    .= CircleMap.x by Th32;
  hence thesis by A3,A20,A26,FUNCT_1:def 5;
  end;
  end;
end;
A27: Topen_unit_circle(CircleMap.r) =
    (Tunit_circle(2)) | (([#]Tunit_circle(2)) \ {CircleMap.r}) by Th23;
  thus thesis by A27,TOPREALA:29;
end;
end;

definition
  func Circle2IntervalR -> Function
 of Topen_unit_circle(c[10]), R^1 | R^1(].0,1.[)
    means :Def13:
  for p being Point of Topen_unit_circle(c[10])
    ex x, y being real number st p = |[x,y]| &
    (y >= 0 implies it.p = arccos x/(2*PI)) &
    (y <= 0 implies it.p = 1-arccos x/(2*PI));
existence
proof
  reconsider A as non empty Subset of R^1;
  defpred P[set,set] means
    ex x, y being real number st $1 = |[x,y]| &
    (y >= 0 implies $2 = arccos x/P2) &
    (y <= 0 implies $2 = 1-arccos x/P2);
A1: Y = A by PRE_TOPC:29;
A2: for x being Element of X ex y being Element of Y st P[x,y]
  proof
    let x be Element of X;
A3: X = cS1 \ {c[10]} by Def10;
    then
A4: x in cS1 by XBOOLE_0:def 4;
A5: cS1 is Subset of TOP-REAL 2 by TSEP_1:1;
    then consider a, b being Real such that
A6:   x = <*a,b*> by A4,EUCLID:55;
A7: not x in {c[10]} by A3,XBOOLE_0:def 4;
    set k = arccos a;
    reconsider x1 = x as Point of TOP-REAL 2 by A4,A5;
A8: x = |[a,b]| by A6;
    then
A9: a = x1`1 & b = x1`2 by EUCLID:56;
    then
A10: -1 <= a & a <= 1 by Th27;
    then
A11: 0 <= k & k <= PI by SIN_COS6:101;
A12: 0 <= k/P2 & k/P2 <= 1/2 by A10,Lm24;
A13: 1^2 = |. x1 .|^2 by A4,Th12
       .= a^2+b^2 by A9,JGRAPH_3:10;
A14: now
      assume
A15:     k = 0;
      then
A16:   a = 1 by A10,SIN_COS6:98;
      1-1 = 1+b^2-1 by A10,A13,A15,SIN_COS6:98;
      then b = 0 by XCMPLX_1:6;
      then x = c[10] by A6,A16;
      hence contradiction by A7,TARSKI:def 1;
    end;
    per cases;
    suppose
A17:   b = 0;
A18: a <> 1 by A7,A8,A17,TARSKI:def 1;
    set y = k/P2;
    y <> 0 by A14,XCMPLX_1:50;
    then 0 < y & y < 1 by A12,XXREAL_0:2;
    then reconsider y as Element of Y by A1,RCOMP_1:47;
    take y, a, b;
    thus x = |[a,b]| by A6;
    thus b >= 0 implies y = arccos a/P2;
    assume b <= 0;
    thus y = 1*PI/P2 by A13,A17,A18,SIN_COS6:95,SQUARE_1:110
      .= 1-1/2 by XCMPLX_1:92
      .= 1-1*PI/P2 by XCMPLX_1:92
      .= 1-arccos a/P2 by A13,A17,A18,SIN_COS6:95,SQUARE_1:110;
    end;
    suppose
A19:   b > 0;
    set y = k/P2;
    y <> 0 by A14,XCMPLX_1:50;
    then 0 < y & y < 1 by A12,XXREAL_0:2;
    then reconsider y as Element of Y by A1,RCOMP_1:47;
    take y, a, b;
    thus thesis by A6,A19;
    end;
    suppose
A20:   b < 0;
    set y = 1-k/P2;
A21: (P2-k)/P2 = P2/P2-k/P2 by XCMPLX_1:121
      .= y by XCMPLX_1:60;
    1*k < P2 by A11,REAL_2:199;
    then k-k < P2-k & P2-k < P2-0 by A11,A14,REAL_1:92;
    then 0/P2 < y & y < P2/P2 by A21,REAL_1:73;
    then 0 < y & y < 1 by XCMPLX_1:60;
    then reconsider y as Element of Y by A1,RCOMP_1:47;
    take y, a, b;
    thus thesis by A6,A20;
    end;
  end;
  ex G being Function of TOUC, R^1|R^1(].0,0+p1.[) st
    for p being Point of TOUC holds P[p,G.p] from FUNCT_2:sch 3(A2);
  hence thesis;
end;
uniqueness
proof
  let f, g be Function of TOUC, R^1 | A such that
A22: for p being Point of TOUC
    ex x, y being real number st p = |[x,y]| &
    (y >= 0 implies f.p = arccos x/(2*PI)) &
    (y <= 0 implies f.p = 1-arccos x/(2*PI)) and
A23: for p being Point of TOUC
    ex x, y being real number st p = |[x,y]| &
    (y >= 0 implies g.p = arccos x/(2*PI)) &
    (y <= 0 implies g.p = 1-arccos x/(2*PI));
  now
    let p be Point of TOUC;
A24: ex x1, y1 being real number st p = |[x1,y1]| &
     (y1 >= 0 implies f.p = arccos x1/(2*PI)) &
     (y1 <= 0 implies f.p = 1-arccos x1/(2*PI)) by A22;
    ex x2, y2 being real number st p = |[x2,y2]| &
     (y2 >= 0 implies g.p = arccos x2/(2*PI)) &
     (y2 <= 0 implies g.p = 1-arccos x2/(2*PI)) by A23;
    hence f.p = g.p by A24,SPPOL_2:1;
  end;
  hence thesis by FUNCT_2:113;
end;
end;

set A1 = R^1(].1/2,1/2+p1.[);

definition
  func Circle2IntervalL ->
    Function of Topen_unit_circle(c[-10]), R^1 | R^1(].1/2,3/2.[)
    means :Def14:
  for p being Point of Topen_unit_circle(c[-10])
    ex x, y being real number st p = |[x,y]| &
    (y >= 0 implies it.p = 1+arccos x/(2*PI)) &
    (y <= 0 implies it.p = 1-arccos x/(2*PI));
existence
proof
  reconsider A1 as non empty Subset of R^1;
  defpred P[set,set] means
    ex x, y being real number st $1 = |[x,y]| &
    (y >= 0 implies $2 = 1+arccos x/P2) &
    (y <= 0 implies $2 = 1-arccos x/P2);
A1: Ym = A1 by PRE_TOPC:29;
A2: for x being Element of Xm ex y being Element of Ym st P[x,y]
  proof
    let x be Element of Xm;
A3: Xm = cS1 \ {c[-10]} by Def10;
    then
A4: x in cS1 by XBOOLE_0:def 4;
A5: cS1 is Subset of TOP-REAL 2 by TSEP_1:1;
    then consider a, b being Real such that
A6:   x = <*a,b*> by A4,EUCLID:55;
A7: not x in {c[-10]} by A3,XBOOLE_0:def 4;
    set k = arccos a;
    reconsider x1 = x as Point of TOP-REAL 2 by A4,A5;
A8: x = |[a,b]| by A6;
    then
A9: a = x1`1 & b = x1`2 by EUCLID:56;
    then
A10: -1 <= a & a <= 1 by Th27;
A11: 0 <= k/P2 & k/P2 <= 1/2 by A10,Lm24;
A12: 1^2 = |. x1 .|^2 by A4,Th12
       .= a^2+b^2 by A9,JGRAPH_3:10;
A13: now
      assume
A14:     k = PI;
      then
A15:   a = -1 by A10,SIN_COS6:100;
      1-1 = (-1)^2+b^2-1 by A10,A12,A14,SIN_COS6:100
         .= 1^2+b^2-1;
      then b = 0 by XCMPLX_1:6;
      then x = c[-10] by A6,A15;
      hence contradiction by A7,TARSKI:def 1;
    end;
A16: now
      assume k/P2 = 1/2;
      then k/P2*2 = 1/2*2;
      then k/PI = 1 by XCMPLX_1:93;
      hence contradiction by A13,XCMPLX_1:58;
    end;
A17: now
      let y be real number;
      assume
A18:    y = 1+k/P2;
      then
A19:  1+0 <= y & y <= 1+1/2 by A11,XREAL_1:8;
      y <> 1+1/2 by A16,A18;
      then 1/2 < y & y < 3/2 by A19,XXREAL_0:2,REAL_1:def 5;
      hence y is Element of Ym by A1,RCOMP_1:47;
    end;
    per cases;
    suppose
A20:   b = 0;
    then a <> -1 by A7,A8,TARSKI:def 1;
    then
A21: a = 1 by A12,A20,SQUARE_1:110;
    reconsider y = 1+k/P2 as Element of Ym by A17;
    take y, a, b;
    thus x = |[a,b]| by A6;
    thus thesis by A21,SIN_COS6:97;
    end;
    suppose
A22:   b > 0;
    reconsider y = 1+k/P2 as Element of Ym by A17;
    take y, a, b;
    thus thesis by A6,A22;
    end;
    suppose
A23:   b < 0;
    set y = 1-k/P2;
A24: 1-0 >= y & y >= 1-1/2 by A11,REAL_1:92;
    y <> 1/2 by A16;
    then 1/2 < y & y < 3/2 by A24,XXREAL_0:2,REAL_1:def 5;
    then reconsider y as Element of Ym by A1,RCOMP_1:47;
    take y, a, b;
    thus thesis by A6,A23;
    end;
  end;
  ex G being Function of TOUCm, R^1|R^1(].1/2,1/2+p1.[) st
    for p being Point of TOUCm holds P[p,G.p] from FUNCT_2:sch 3(A2);
  hence thesis;
end;
uniqueness
proof
  let f, g be Function of TOUCm, R^1 | R^1(].1/2,3/2.[) such that
A25: for p being Point of TOUCm
    ex x, y being real number st p = |[x,y]| &
    (y >= 0 implies f.p = 1+arccos x/(2*PI)) &
    (y <= 0 implies f.p = 1-arccos x/(2*PI)) and
A26: for p being Point of TOUCm
    ex x, y being real number st p = |[x,y]| &
    (y >= 0 implies g.p = 1+arccos x/(2*PI)) &
    (y <= 0 implies g.p = 1-arccos x/(2*PI));
  now
    let p be Point of TOUCm;
A27: ex x1, y1 being real number st p = |[x1,y1]| &
     (y1 >= 0 implies f.p = 1+arccos x1/(2*PI)) &
     (y1 <= 0 implies f.p = 1-arccos x1/(2*PI)) by A25;
    ex x2, y2 being real number st p = |[x2,y2]| &
     (y2 >= 0 implies g.p = 1+arccos x2/(2*PI)) &
     (y2 <= 0 implies g.p = 1-arccos x2/(2*PI)) by A26;
    hence f.p = g.p by A27,SPPOL_2:1;
  end;
  hence thesis by FUNCT_2:113;
end;
end;

set C = Circle2IntervalR;
set Cm = Circle2IntervalL;

theorem Th43:
  (CircleMap(R^1(0)))" = Circle2IntervalR
proof
  set f = CircleMap(R^1(0));
  reconsider r0 = 0 as Point of R^1 by TOPMETR:24;
  set F = AffineMap(2*PI,0);
  reconsider A as non empty Subset of R^1;
A1: CircleMap.r0 = c[10] by Th33;
  then reconsider f as Function of R^1|A, TOUC;
  set Y = the carrier of (R^1|A);
A2: Y = A by PRE_TOPC:29;
A3: rng f = X by A1,FUNCT_2:def 3;
A4: dom (C*f) = Y by FUNCT_2:def 1;
A5: dom id Y = Y by RELAT_1:71;
  now
    let a be set;
    assume
A6:   a in dom (C*f);
    reconsider b = a as Point of R^1|A by A6;
    consider x, y being real number such that
A7:   f.b = |[x,y]| and
A8:   y >= 0 implies C.(f.b) = arccos x/P2 and
A9:  y <= 0 implies C.(f.b) = 1-arccos x/P2 by Def13;
    reconsider c = b as Real by XREAL_0:def 1;
    f.b = CircleMap.b by A2,FUNCT_1:72
       .= |[ (cos*F).c, (sin*F).c ]| by Lm22;
    then x = (cos*F).c & y = (sin*F).c by A7,SPPOL_2:1;
    then
A10: x = cos.(F.c) & y = sin.(F.c) by FUNCT_2:21;
    then
A11: x = cos(F.c) & y = sin(F.c) by SIN_COS:def 21,def 23;
A12: F.c = 2*PI*c+0 by JORDAN16:def 3;
     then
A13: F.c/(2*PI*1) = c/1 by XCMPLX_1:92;
A14: F.(1-c) = 2*PI*(1-c)+0 by JORDAN16:def 3;
     then
A15: F.(1-c)/(2*PI*1) = (1-c)/1 by XCMPLX_1:92;
A16: 0 < c & c < 1 by A2,RCOMP_1:47;
A17: now per cases;
      suppose
A18:    y >= 0;
      then not F.c in ].PI,2*PI.[ by A10,COMPTRIG:25;
      then F.c <= PI or F.c >= 2*PI by RCOMP_1:47;
      then F.c/P2 <= (1*PI)/P2 or F.c/P2 >= 2*PI/P2 by REAL_1:73;
      then 0 <= c & c <= 1/2 by A13,A16,XCMPLX_1:60,92;
      then 2*PI*0 <= 2*PI*c+0 & 2*PI*c <= 2*PI*(1/2) by XREAL_1:66;
      hence C.(f.b) = F.c/P2 by A8,A11,A12,A18,SIN_COS6:94
        .= b by A12,XCMPLX_1:90;
      end;
      suppose
A19:    y < 0;
A20:  cos.(F.(1-c)) = cos(-2*PI*c+2*PI*1) by A14,SIN_COS:def 23
        .= cos(-2*PI*c) by COMPLEX2:10
        .= cos(2*PI*c) by SIN_COS:34;
      not F.c in [.0,PI.] by A10,A19,COMPTRIG:24;
      then F.c < 0 or F.c > PI by RCOMP_1:48;
      then F.c/P2 < 0/P2 or F.c/P2 > (1*PI)/P2 by REAL_1:73;
      then c < 0 or c > 1/2 by A13,XCMPLX_1:92;
      then 1-c < 1-1/2 & 1-c > 1-1 by A16,REAL_1:92;
      then
A21:  2*PI*0 <= 2*PI*(1-c)+0 & 2*PI*(1-c) <= 2*PI*(1/2) by XREAL_1:66;
      arccos x = arccos cos F.(1-c) by A11,A12,A20,SIN_COS:def 23
        .= F.(1-c) by A14,A21,SIN_COS6:94;
      hence C.(f.b) = b by A9,A15,A19;
      end;
    end;
    thus (C*f).a = C.(f.b) by A6,FUNCT_1:22
      .= (id Y).a by A17,FUNCT_1:35;
  end;
  then C*f = id Y by A4,A5,FUNCT_1:9;
  then C = f qua Function" by A3,FUNCT_2:36;
  hence thesis by A1,A3,TOPS_2:def 4;
end;

theorem Th44:
  (CircleMap(R^1(1/2)))" = Circle2IntervalL
proof
  set f = CircleMap(R^1(1/2));
  set G = AffineMap(2*PI,0);
  reconsider A1 as non empty Subset of R^1;
  reconsider f as Function of R^1|A1, TOUCm by Lm20;
  set Y = the carrier of (R^1|A1);
A1: Y = A1 by PRE_TOPC:29;
A2: rng f = Xm by Lm20,FUNCT_2:def 3;
A3: dom (Cm*f) = Ym by FUNCT_2:def 1;
A4: dom id Ym = Ym by RELAT_1:71;
  now
    let a be set;
    assume
A5:   a in dom (Cm*f);
    then reconsider b = a as Point of R^1|A1;
    consider x, y being real number such that
A6:   f.b = |[x,y]| and
A7:   y >= 0 implies Cm.(f.b) = 1+arccos x/P2 and
A8:  y <= 0 implies Cm.(f.b) = 1-arccos x/P2 by Def14;
    reconsider c = b as Real by XREAL_0:def 1;
A9: f.b = CircleMap.b by A1,FUNCT_1:72
       .= |[ cos(2*PI*c), sin(2*PI*c) ]| by Def11;
    then
A10: x = cos(2*PI*c) by A6,SPPOL_2:1
      .= cos(2*PI*c+2*PI*(-1)) by COMPLEX2:10
      .= cos(2*PI*(c-1));
A11: y = sin(2*PI*c) by A6,A9,SPPOL_2:1;
A12: G.(1-c) = 2*PI*(1-c)+0 by JORDAN16:def 3;
     then
A13: G.(1-c)/(2*PI*1) = (1-c)/1 by XCMPLX_1:92;
A14: 1/2 < c & c < 3/2 by A1,RCOMP_1:47;
    then c-1 < 3/2-1 by XREAL_1:11;
    then
A15: 2*PI*(c-1) <= 2*PI*(1/2) by XREAL_1:66;
    2*PI*c <= 2*PI*(1/2+1) by A14,XREAL_1:66;
    then
A16: 2*PI*c <= PI+2*PI*1;
    2*PI*(1/2) < 2*PI*c by A14,XREAL_1:70;
    then
A17: PI+2*PI*0 < 2*PI*c;
A18: now per cases;
      suppose
A19:     c >= 1;
      then
A20:  2*PI*c >= 2*PI*1 by XREAL_1:66;
      1-1 <= c-1 by A19,XREAL_1:11;
      then
A21:  2*PI*0 <= 2*PI*(c-1)+0 by XREAL_1:66;
      thus Cm.(f.b) = 1+(2*PI*(c-1))/P2 by  A7,A10,A11,A16,A20,SIN_COS6:16,
A15,A21,SIN_COS6:94
        .= 1+(c-1) by XCMPLX_1:90
        .= b;
      end;
      suppose
A22:     c < 1;
      then 2*PI*c < 2*PI*1 by XREAL_1:70;
      then
A23:  2*PI*c < 2*PI+2*PI*0;
A24:  1-c < 1-1/2 by A14,REAL_1:92;
      1-1 <= 1-c by A22,REAL_1:92;
      then
A25:  2*PI*0 <= 2*PI*(1-c)+0 by XREAL_1:66;
A26:  2*PI*(1-c) <= 2*PI*(1/2) by A24,XREAL_1:66;
      arccos x = arccos cos(2*PI*c) by A6,A9,SPPOL_2:1
        .= arccos cos(-2*PI*c) by SIN_COS:34
        .= arccos cos(2*PI*(-c)+2*PI*1) by COMPLEX2:10
        .= G.(1-c) by A12,A25,A26,SIN_COS6:94;
      hence Cm.(f.b) = b by A8,A11,A13,A17,A23,SIN_COS6:12;
    end;
    end;
    thus (Cm*f).a = Cm.(f.b) by A5,FUNCT_1:22
      .= (id Ym).a by A18,FUNCT_1:35;
  end;
  then Cm*f = id Ym by A3,A4,FUNCT_1:9;
  then Cm = f qua Function" by A2,FUNCT_2:36;
  hence thesis by A2,Lm20,TOPS_2:def 4;
end;

set A = ].0,1.[;
set Q = [.-1,1.[;
set E = ].0,PI.];
set j = 1/P2;
reconsider Q, E as non empty Subset of REAL;
Lm27: the carrier of R^1 | R^1(Q) = R^1(Q) by PRE_TOPC:29;
Lm28: the carrier of R^1 | R^1(E) = R^1(E) by PRE_TOPC:29;
Lm29: the carrier of R^1 | R^1(A) = R^1(A) by PRE_TOPC:29;
set Af = AffineMap(j,0) | R^1(E);
  dom AffineMap(j,0) = R by FUNCT_2:def 1,TOPMETR:24;
  then
Lm30: dom Af = R^1(E) by RELAT_1:91;
  rng Af c= A
  proof
    let y be set;
    assume y in rng Af;
    then consider x being set such that
A1:   x in dom Af and
A2:   Af.x = y by FUNCT_1:def 5;
    reconsider x as Real by A1;
A3: y = AffineMap(j,0).x by A1,A2,Lm30,FUNCT_1:72
     .= j*x+0 by JORDAN16:def 3
     .= x/P2 by XCMPLX_1:100;
    then reconsider y as Real;
    0 < x & x <= PI by A1,Lm30,RCOMP_2:4;
    then 0/P2 < x/P2 & x/P2 <= PI/P2 by REAL_1:73;
    then 0 < y & y <= (1*PI)/P2 by A3;
    then 0 < y & y <= 1/2 by XCMPLX_1:92;
    then 0 < y & y < 1 by XXREAL_0:2;
    hence thesis by RCOMP_1:47;
  end;
then reconsider Af as Function of R^1 | R^1(E), R^1 | R^1(A)
  by Lm28,Lm29,Lm30,FUNCT_2:4;
Lm31: R^1(AffineMap(j,0)) = AffineMap(j,0);
Lm32: dom AffineMap(j,0) = REAL by FUNCT_2:def 1;
Lm33: rng AffineMap(j,0) = REAL by JORDAN16:32;
Lm34: [#]R^1 = REAL by TOPMETR:24;
R^1| [#](R^1) = R^1 by TSEP_1:3;
 then
  R^1 = R^1|R^1(dom AffineMap(j,0)) & R^1 = R^1|R^1(rng AffineMap(j,0))
 by Lm32,Lm33,Lm34;
then reconsider Af as continuous Function of R^1 | R^1(E), R^1 | R^1(A)
by Lm31,TOPREALA:29;

set L = (R^1(AffineMap(-1,1))) | R^1(A);
Lm35: dom AffineMap(-1,1) = REAL by FUNCT_2:def 1;
  then
Lm36: dom L = A by RELAT_1:91;
  rng L c= A
  proof
    let y be set;
    assume y in rng L;
    then consider x being set such that
A1:  x in dom L and
A2:  L.x = y by FUNCT_1:def 5;
    reconsider x as Real by A1,Lm36;
A3: y = AffineMap(-1,1).x by A1,A2,Lm36,FUNCT_1:72
     .= (-1)*x+1 by JORDAN16:def 3;
    then reconsider y as Real;
    0 < x & x < 1 by A1,Lm36,RCOMP_1:47;
    then 1-1 < 1-x & 1-x < 1-0 by REAL_1:92;
    then 0 < y & y < 1 by A3;
    hence thesis by RCOMP_1:47;
  end;
  then reconsider L as Function of R^1 | R^1(A), R^1 | R^1(A)
  by Lm29,Lm36,FUNCT_2:4;
Lm37: rng AffineMap(-1,1) = REAL by JORDAN16:32;
Lm38: [#]R^1 = REAL by TOPMETR:24;
Lm39: R^1| [#](R^1) = R^1 by TSEP_1:3;
  then R^1 = R^1|R^1(dom AffineMap(-1,1)) & R^1 = R^1|R^1(rng AffineMap(-1,1))
    by Lm35,Lm37,Lm38;
  then reconsider L as continuous Function of R^1 | R^1(A), R^1 | R^1(A)
  by TOPREALA:29;

  reconsider ac = R^1(arccos) as
   continuous Function of R^1 | R^1[.-1,1.], R^1 | R^1[.0,PI.]
     by SIN_COS6:87,88;

  set c = ac | R^1(Q);
  Q c= [.-1,1.] by TOPREALA:11;
  then
Lm40: dom c = Q by RELAT_1:91,SIN_COS6:88;
Lm41: rng c c= E
  proof
    let y be set;
    assume
A1:   y in rng c;
    then consider x being set such that
A2:   x in dom c and
A3:   c.x = y by FUNCT_1:def 5;
    reconsider x as Real by A2,Lm40;
A4: rng c c= rng ac by RELAT_1:99;
    y in [.0,PI.] by A1,A4,SIN_COS6:87;
    then reconsider y as Real;
A5: 0 <= y & y <= PI by A1,A4,SIN_COS6:87,RCOMP_1:48;
A6: -1 <= x & x < 1 by A2,Lm40,RCOMP_2:3;
    y = arccos.x by A2,A3,Lm40,FUNCT_1:72
     .= arccos x by SIN_COS6:def 4;
    then y <> 0 by A6,SIN_COS6:98;
    hence thesis by A5,RCOMP_2:4;
  end;
  then reconsider c as Function of R^1 | R^1(Q), R^1 | R^1(E)
    by Lm27,Lm28,Lm40,FUNCT_2:4;
  the carrier of R^1 | R^1[.-1,1.] = [.-1,1.] by PRE_TOPC:29;
  then reconsider QQ = R^1(Q) as Subset of R^1 | R^1[.-1,1.] by
TOPREALA:11;
  the carrier of R^1 | R^1[.0,PI.] = [.0,PI.] by PRE_TOPC:29;
  then reconsider EE = R^1(E) as Subset of R^1 | R^1[.0,PI.] by
TOPREALA:15;
  (R^1 | R^1[.-1,1.]) | QQ = R^1 | R^1(Q) &
    (R^1 | R^1[.0,PI.]) | EE = R^1 | R^1(E) by GOBOARD9:4;
  then
Lm42: c is continuous by TOPREALA:29;

reconsider p = proj1 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
Lm43: dom p = the carrier of TOP-REAL 2 by FUNCT_2:def 1;
Lm44: p is continuous by TOPREAL6:83;

Lm45:
 for aX1 being Subset of TOUC st
  aX1 = {q where q is Point of TOP-REAL 2: q in X & 0 <= q`2} holds
  Circle2IntervalR | (TOUC|aX1) is continuous
proof
  let aX1 being Subset of TOUC such that
A1:  aX1 = {q where q is Point of TOP-REAL 2: q in X & 0 <= q`2};
  reconsider c1 = c[-10] as Point of TOP-REAL 2;
A2: c1`2 = 0 by EUCLID:56;
  c[-10] is Point of Topen_unit_circle(c[10]) by Lm16,Th24;
  then c[-10] in aX1 by A1,A2;
  then reconsider aX1 as non empty Subset of TOUC;
  set X1 = TOUC | aX1;
A3: aX1 = the carrier of X1 by PRE_TOPC:29;
A4: [#]X1 is Subset of TUC by Lm10;
  cS1 is Subset of TOP-REAL 2 by TSEP_1:1;
  then reconsider B = [#]X1 as non empty Subset of TOP-REAL 2 by A4,XBOOLE_1:1;
A5: B = aX1 by A3;
  X1 is SubSpace of TUC by TSEP_1:7;
  then X1 is SubSpace of TOP-REAL 2 by TSEP_1:7;
  then
A6: (TOP-REAL 2) | B = X1 by PRE_TOPC:def 10;
  set f = p|B;
A7: the carrier of (TOP-REAL 2) | B = B by PRE_TOPC:29;
A8: dom f = B by Lm43,RELAT_1:91;
  rng f c= Q
  proof
    let y be set;
    assume y in rng f;
    then consider x being set such that
A9:  x in dom f and
A10:  f.x = y by FUNCT_1:def 5;
    consider q being Point of TOP-REAL 2 such that
A11:  q = x and
A12:  q in X and 0 <= q`2 by A1,A5,A8,A9;
A13: y = p.x by A8,A9,A10,FUNCT_1:72
      .= q`1 by A11,PSCOMP_1:def 28;
    -1 <= q`1 & q`1 < 1 by A12,Th28;
    hence thesis by A13,RCOMP_2:3;
  end;
  then reconsider f as Function of (TOP-REAL 2) | B, R^1 | R^1(Q)
    by A7,A8,Lm27,FUNCT_2:4;
  f is continuous by Lm44,TOPREALA:29;
  then c*f is continuous by Lm42,TOPS_2:58;
  then
A14: Af*(c*f) is continuous by TOPS_2:58;
  for a being Point of X1 holds (C|X1).a = (Af*(c*f)).a
  proof
    let a be Point of X1;
    reconsider b = a as Point of TOUC by TOPMETR:2;
    consider x, y being real number such that
A15:   b = |[x,y]| and
A16:   y >= 0 implies C.b = arccos x/P2 and
      y <= 0 implies C.b = 1-arccos x/P2 by Def13;
    a in aX1 by A3;
    then
A17: ex q being Point of TOP-REAL 2 st a = q & q in X & 0 <= q`2 by A1;
A18: |[x,y]|`1 = x by EUCLID:56;
A19: -1 <= |[x,y]|`1 by A15,Th27;
    |[x,y]|`1 < 1 by A15,Th28;
    then
A20: x in Q by A18,A19,RCOMP_2:3;
    then arccos.x = c.x by FUNCT_1:72;
    then
A21: arccos.x in rng c by A20,Lm40,FUNCT_1:def 5;
    thus (C|X1).a = arccos x/P2 by A15,A16,A17,EUCLID:56,FUNCT_1:72
      .= arccos.x/P2 by SIN_COS6:def 4
      .= 1/P2 * (arccos.x) + 0 by XCMPLX_1:100
      .= AffineMap(j,0).(arccos.x) by JORDAN16:def 3
      .= Af.(arccos.x) by A21,Lm41,FUNCT_1:72
      .= Af.(c.x) by A20,FUNCT_1:72
      .= Af.(c.(|[x,y]|`1)) by EUCLID:56
      .= Af.(c.(p.a)) by A15,PSCOMP_1:def 28
      .= Af.(c.(f.a)) by FUNCT_1:72
      .= Af.((c*f).a) by A6,FUNCT_2:21
      .= (Af*(c*f)).a by A6,FUNCT_2:21;
  end;
  hence thesis by A6,A14,FUNCT_2:113;
end;

Lm46:
 for aX1 being Subset of TOUC st
  aX1 = {q where q is Point of TOP-REAL 2: q in X & 0 >= q`2} holds
  Circle2IntervalR | (TOUC|aX1) is continuous
proof
  let aX1 being Subset of TOUC such that
A1:  aX1 = {q where q is Point of TOP-REAL 2: q in X & 0 >= q`2};
