theorem
  ex F being Subset-Family of Tunit_circle(2) st
   F = { CircleMap.:].0,1.[, CircleMap.:].1/2,3/2.[ } &
   F is_a_cover_of Tunit_circle(2) & F is open &
   for U being Subset of Tunit_circle(2) holds
   ( U = CircleMap.:].0,1.[ implies
     union IntIntervals(0,1) = (CircleMap)"U &
     for d being Subset of R^1 st d in IntIntervals(0,1)
      for f being Function of R^1 | d, (Tunit_circle(2)) | U st f = CircleMap|d
       holds f is_homeomorphism ) &
   ( U = CircleMap.:].1/2,3/2.[ implies
     union IntIntervals(1/2,3/2) = (CircleMap)"U &
     for d being Subset of R^1 st d in IntIntervals(1/2,3/2)
      for f being Function of R^1 | d, (Tunit_circle(2)) | U st f = CircleMap|d
       holds f is_homeomorphism )
proof
  set D1 = IntIntervals(0,1);
  set D2 = IntIntervals(1/2,3/2);
A1: c[10] in {c[10]} by TARSKI:def 1;
  set F1 = CircleMap.:union D1;
  set F2 = CircleMap.:union D2;
  set F = {F1,F2};
  reconsider F as Subset-Family of TUC;
  take F;
A2: ].0+0,1+0.[ in D1 by Lm1;
  then
A3: F1 = CircleMap.:].0,1.[ by Th41;
  ].1/2+0,3/2+0.[ in D2 by Lm1;
  then
A4: F2 = CircleMap.:].1/2,3/2.[ by Th41;
  hence F = {CircleMap.:].0,1.[, CircleMap.:].1/2,3/2.[} by A2,Th41;
  thus F is_a_cover_of TUC
  proof
    thus [#]TUC c= union F
    proof
      let a be set;
      assume
A5:     a in [#]TUC;
      reconsider A = [.0,0+1.[ as Subset of R^1 by TOPMETR:24;
      reconsider f = CircleMap | A as Function of R^1|A,TUC by Lm23;
      f is onto by Th39;
      then rng f = cS1 by FUNCT_2:def 3;
      then consider x being set such that
A6:     x in dom f and
A7:     f.x = a by A5,FUNCT_1:def 5;
A8:   dom f = A by Lm19,RELAT_1:91;
      then reconsider x as Real by A6;
A9:   CircleMap.x = f.x by A6,FUNCT_1:70;
A10:   F2 in F by TARSKI:def 2;
      per cases by A6,A8,RCOMP_2:3;
      suppose
A11:     x = 0;
      0 in A by RCOMP_2:3;
      then
A12:   f.0 = CircleMap.0 by FUNCT_1:72
        .= c[10] by Th33
        .= CircleMap.1 by Th33;
      1 in ].1/2,3/2.[ by RCOMP_1:47;
      then a in CircleMap.:].1/2,3/2.[ by A7,A11,A12,Lm19,FUNCT_1:def 12;
      hence a in union F by A4,A10,TARSKI:def 4;
      end;
      suppose 0 < x & x < 1;
      then
A13:   x in ].0,1.[ by RCOMP_1:47;
      ].0+0,1+0.[ in D1 by Lm1;
      then x in union D1 by A13,TARSKI:def 4;
      then
A14:   a in F1 by A7,A9,Lm19,FUNCT_1:def 12;
      F1 in F by TARSKI:def 2;
      hence a in union F by A14,TARSKI:def 4;
      end;
      suppose x >= 1 & x < 1;
      hence a in union F;
      end;
    end;
    thus thesis;
  end;
  thus F is open
  proof
    let P be Subset of TUC;
A15: CircleMap.:].0,1.[ = (CircleMap(R^1(0))).:].0,1.[ by RELAT_1:162;
A16: CircleMap.:].1/2,3/2.[ = (CircleMap(R^1(1/2))).:].1/2,3/2.[ by RELAT_1:162
;
    reconsider r = 0 as Integer;
A17: now
      let A be Subset of REAL;
A18:  the carrier of R^1 | R^1(A) = A by PRE_TOPC:29;
      A c= A;
      hence A is Subset of R^1 | R^1(A) by A18;
    end;
    then reconsider M = ].0,1.[ as Subset of R^1 | R^1(].r,r+1.[);
    reconsider N = ].1/2,3/2.[ as Subset of R^1 | R^1(].1/2+r,1/2+r+1.[)
    by A17;
A19: now
      let A be open Subset of REAL;
      reconsider B = A as Subset of R^1 | R^1(A) by A17;
A20:  the carrier of R^1 | R^1(A) = A by PRE_TOPC:29;
      B = [#](R^1 | R^1(A)) /\ R^1(A) by A20;
      hence A is open Subset of R^1 | R^1(A) by TOPS_2:32;
    end;
    then M is open;
    then
A21: (CircleMap(R^1(r))).:M is open by T_0TOPSP:def 2;
    N is open by A19;
    then (CircleMap(R^1(1/2+r))).:N is open by T_0TOPSP:def 2;
    then F1 is open & F2 is open by A3,A4,A15,A16,A21,TSEP_1:17;
    hence thesis by TARSKI:def 2;
  end;
  let U be Subset of TUC;
  thus U = CircleMap.:].0,1.[ implies
     union IntIntervals(0,1) = (CircleMap)"U &
     for d being Subset of R^1 st d in IntIntervals(0,1)
      for f being Function of R^1 | d, (Tunit_circle(2)) | U st f = CircleMap|d
       holds f is_homeomorphism
  proof
    assume
A22:  U = CircleMap.:].0,1.[;
    ].0+0,1+0.[ in D1 by Lm1;
    then
A23: CircleMap.:].0,1.[ = CircleMap.:union D1 by Th41;
  now
    let x1, x2 be Element of R^1;
    assume x1 in union D1;
    then consider Z being set such that
A24:   x1 in Z and
A25:   Z in D1 by TARSKI:def 4;
    consider n being Element of INT such that
A26:   Z = ].0+n,1+n.[ by A25;
    assume
A27:   CircleMap.x1 = CircleMap.x2;
    set k = [\x2/];
    set K = ].0+k,1+k.[;
    k in INT by INT_1:def 2;
    then
A28: K in D1;
A29: n < x1 by A24,A26,RCOMP_1:47;
    x1 < 1+n by A24,A26,RCOMP_1:47;
    then
A30: x1-1 < 1+n-1 by XREAL_1:11;
    then
A31: [\x1/] = n by A29,INT_1:def 4;
A32: k <= x2 by INT_1:def 4;
    not x1 is integer by A29,A31,INT_1:48;
    then
A33: not x1 in INT by INT_1:def 2;
    now
      assume k = x2;
      then CircleMap.x1 = c[10] by A27,Th33;
      hence contradiction by A1,A33,Lm19,Th34,FUNCT_1:def 13,TOPMETR:24;
    end;
    then
A34: k < x2 by A32,REAL_1:def 5;
A35: CircleMap.x1 = CircleMap.(x1+-n) & CircleMap.x2 = CircleMap.(x2+-k) by
Th32;
    0 < x1+-n by A29,REAL_2:105;
    then
A36: 2*PI*0 <= 2*PI*(x1-n) by XREAL_1:66;
    x1-1-n < n-n by A30,XREAL_1:11;
    then x1-n-1+1 < 0+1 by XREAL_1:10;
    then
A37: 2*PI*(x1-n) < 2*PI*1 by XREAL_1:70;
    0 <= x2+-k by A34,REAL_2:105;
    then
A38: 2*PI*0 <= 2*PI*(x2-k) by XREAL_1:66;
    x2-1 < k by INT_1:def 4;
    then x2-1-k < k-k by XREAL_1:11;
    then x2-1-k+1 < 0+1 by XREAL_1:8;
    then
A39: 2*PI*(x2-k) < 2*PI*1 by XREAL_1:70;
    CircleMap.(x1-n) = |[ cos(2*PI*(x1-n)), sin(2*PI*(x1-n)) ]| &
      CircleMap.(x2-k) = |[ cos(2*PI*(x2-k)), sin(2*PI*(x2-k)) ]| by Def11;
    then cos(2*PI*(x1-n)) = cos(2*PI*(x2-k)) &
         sin(2*PI*(x1-n)) = sin(2*PI*(x2-k)) by A27,A35,SPPOL_2:1;
    then 2*PI*(x1-n) = 2*PI*(x2-k) by A36,A37,A38,A39,COMPLEX2:12;
    then x1-n = x2-k by XCMPLX_1:5;
    then
A40: x2 = x1-n+k;
    x1 < 1+n by A24,A26,RCOMP_1:47;
    then x1-n < 1 by XREAL_1:21;
    then x2 < 1+k by A40,XREAL_1:8;
    then x2 in K by A34,RCOMP_1:47;
    hence x2 in union D1 by A28,TARSKI:def 4;
  end;
  hence union D1 = (CircleMap)"U by A22,A23,T_0TOPSP:2;
  let d be Subset of R^1;
  assume
A41: d in D1;
  let f be Function of R^1 | d, TUC | U;
  assume
A42: f = CircleMap|d;
  consider n being Element of INT such that
A43:  d = ].0+n,1+n.[ by A41;
  0+n < 1/2+n & 1/2+n < 1+n by XREAL_1:8;
  then reconsider d1 = d as non empty Subset of R^1 by A43,RCOMP_1:47;
A44: 1/2 in ].0+0,1+0.[ by RCOMP_1:47;
  reconsider U1 = U as non empty Subset of TUC
    by A22,A44,Lm19,Lm20,FUNCT_1:def 12;
  reconsider f1 = f as Function of R^1 | d1, TUC | U1;
A45: [#](TUC | U) = U by PRE_TOPC:def 10;
A46: F1 = CircleMap.:d by A41,Th41;
A47: f is one-to-one by A41,A42,Lm3,Th40;
A48: f is continuous by A42,TOPREALA:29;
  reconsider J = ].n,n+p1.[ as non empty Subset of R^1 by TOPMETR:24;
  reconsider F = CircleMap|J as Function of R^1|J,TUC by Lm23;
  d c= J by A43;
  then reconsider d2 = d as Subset of R^1|J by PRE_TOPC:29;
A49: R^1|J|d2 = R^1|d by A43,PRE_TOPC:28;
A50: CircleMap|d = CircleMap|J|d1 by A43,RELAT_1:103;
A51: f1 is onto
  proof
    thus rng f1 c= the carrier of TUC | U1;
    let b be set;
    assume b in the carrier of TUC | U1;
    then consider a being Element of R^1 such that
A52:   a in d and
A53:   b = CircleMap.a by A22,A23,A45,A46,FUNCT_2:116;
A54: dom (CircleMap|d) = d by Lm19,RELAT_1:91,TOPMETR:24;
    (CircleMap|d).a = CircleMap.a by A52,FUNCT_1:72;
    hence b in rng f1 by A42,A52,A53,A54,FUNCT_1:def 5;
  end;
  reconsider nn = n as Point of R^1 by TOPMETR:24,XREAL_0:def 1;
  CircleMap(R^1(n)) is open;
  then
A55: F is open by TOPREALA:33;
  CircleMap | ].n,n+1.[ is one-to-one;
  then f is open by A42,A49,A50,A51,A55,TOPREALA:31;
  hence f is_homeomorphism by A47,A48,A51,TOPREALA:37;
  end;
  assume
A56:  U = CircleMap.:].1/2,3/2.[;
  now
    let x1, x2 be Element of R^1;
    assume x1 in union D2;
    then consider Z being set such that
A57:   x1 in Z and
A58:   Z in D2 by TARSKI:def 4;
    consider n being Element of INT such that
A59:   Z = ].1/2+n,3/2+n.[ by A58;
    assume
A60:   CircleMap.x1 = CircleMap.x2;
A61: 1/2+n < x1 by A57,A59,RCOMP_1:47;
    0+n < 1/2+n by XREAL_1:10;
    then
A62: n < x1 by A61,XXREAL_0:2;
A63: x1 < 3/2+n by A57,A59,RCOMP_1:47;
    then
A64: x1-n < 3/2 by XREAL_1:21;
    set k = [\x2/];
A65: k <= x2 by INT_1:def 4;
    per cases by REAL_1:def 5;
    suppose x1 = 1+n;
    then CircleMap.x2 = c[10] by A60,Th33;
    then reconsider w = x2 as Element of INT
      by A1,Lm19,Th34,FUNCT_1:def 13,TOPMETR:24;
    w-1 in INT by INT_1:def 2;
    then
A66: ].1/2+(w-1),3/2+(w-1).[ in D2;
A67: -1/2+w < 0+w by XREAL_1:10;
    0+w < 1/2+w by XREAL_1:10;
    then x2 in ].1/2+(w-1),3/2+(w-1).[ by A67,RCOMP_1:47;
    hence x2 in union D2 by A66,TARSKI:def 4;
    end;
    suppose
A68:   x1 < 1+n;
    set K = ].1/2+k,3/2+k.[;
    k in INT by INT_1:def 2;
    then
A69: K in D2;
A70: CircleMap.x1 = CircleMap.(x1+-n) & CircleMap.x2 = CircleMap.(x2+-k) by
Th32;
    CircleMap.(x1-n) = |[ cos(2*PI*(x1-n)), sin(2*PI*(x1-n)) ]| &
      CircleMap.(x2-k) = |[ cos(2*PI*(x2-k)), sin(2*PI*(x2-k)) ]| by Def11;
    then
A71: cos(2*PI*(x1-n)) = cos(2*PI*(x2-k)) &
      sin(2*PI*(x1-n)) = sin(2*PI*(x2-k)) by A60,A70,SPPOL_2:1;
    0 < x1+-n by A62,REAL_2:105;
    then
A72: 2*PI*0 <= 2*PI*(x1-n) by XREAL_1:66;
    x1-1 < n+1-1 by A68,XREAL_1:11;
    then x1-1-n < n-n by XREAL_1:11;
    then x1-n-1+1 < 0+1 by XREAL_1:10;
    then
A73: 2*PI*(x1-n) < 2*PI*1 by XREAL_1:70;
    x2-x2 <= x2-k by A65,REAL_1:92;
    then
A74: 2*PI*0 <= 2*PI*(x2-k) by XREAL_1:66;
    x2-1 < k by INT_1:def 4;
    then x2-1-k < k-k by XREAL_1:11;
    then x2-1-k+1 < 0+1 by XREAL_1:8;
    then 2*PI*(x2-k) < 2*PI*1 by XREAL_1:70;
    then 2*PI*(x1-n) = 2*PI*(x2-k) by A71,A72,A73,A74,COMPLEX2:12;
    then
A75: x1-n = x2-k by XCMPLX_1:5;
    then
A76: x2 = x1-n+k;
    1/2+n-n < x1-n by A61,XREAL_1:11;
    then
A77: 1/2+k < x1-n+k by XREAL_1:10;
    x2 < 3/2+k by A64,A76,XREAL_1:8;
    then x2 in K by A75,A77,RCOMP_1:47;
    hence x2 in union D2 by A69,TARSKI:def 4;
    end;
    suppose
A78:   x1 > 1+n;
    set K = ].1/2+(k-1),3/2+(k-1).[;
    k-1 in INT by INT_1:def 2;
    then
A79: K in D2;
A80: CircleMap.x1 = CircleMap.(x1+(-1-n)) & CircleMap.x2 = CircleMap.(x2+-k)
by Th32;
    CircleMap.(x1-1-n) = |[ cos(2*PI*(x1-1-n)), sin(2*PI*(x1-1-n)) ]| &
      CircleMap.(x2-k) = |[ cos(2*PI*(x2-k)), sin(2*PI*(x2-k)) ]| by Def11;
    then
A81: cos(2*PI*(x1-1-n)) = cos(2*PI*(x2-k)) &
      sin(2*PI*(x1-1-n)) = sin(2*PI*(x2-k)) by A60,A80,SPPOL_2:1;
A82: x1-1 < 3/2+n-1 by A63,XREAL_1:11;
A83: n+1-1 < x1-1 by A78,XREAL_1:11;
    then n-n < x1-1-n by XREAL_1:11;
    then
A84: 2*PI*0 <= 2*PI*(x1-1-n) by XREAL_1:66;
    x1-1-n < 1/2+n-n by A82,XREAL_1:11;
    then x1-1-n < 1 by XXREAL_0:2;
    then
A85: 2*PI*(x1-1-n) < 2*PI*1 by XREAL_1:70;
    x2-x2 <= x2-k by A65,REAL_1:92;
    then
A86: 2*PI*0 <= 2*PI*(x2-k) by XREAL_1:66;
    x2-1 < k by INT_1:def 4;
    then x2-1-k < k-k by XREAL_1:11;
    then x2-1-k+1 < 0+1 by XREAL_1:8;
    then 2*PI*(x2-k) < 2*PI*1 by XREAL_1:70;
    then 2*PI*(x1-1-n) = 2*PI*(x2-k) by A81,A84,A85,A86,COMPLEX2:12;
    then
A87: x1-1-n = x2-k by XCMPLX_1:5;
    then
A88: x2 = x1-1-n+k;
A89: n-n < x1-1-n by A83,XREAL_1:11;
    -1/2 < 0;
    then -1/2 < x1-1-n by A89;
    then
A90: -1/2+k < x1-1-n+k by XREAL_1:10;
    x1-n-1 < 3/2-1 by A64,XREAL_1:11;
    then x2 < 1/2+k by A88,XREAL_1:8;
    then x2 in K by A87,A90,RCOMP_1:47;
    hence x2 in union D2 by A79,TARSKI:def 4;
    end;
  end;
  hence union D2 = (CircleMap)"U by A4,A56,T_0TOPSP:2;
  let d be Subset of R^1;
  assume
A91: d in D2;
  let f be Function of R^1 | d, TUC | U;
  assume
A92: f = CircleMap|d;
  consider n being Element of INT such that
A93:  d = ].1/2+n,3/2+n.[ by A91;
  1/2+n < 1+n & 1+n < 3/2+n by XREAL_1:8;
  then reconsider d1 = d as non empty Subset of R^1 by A93,RCOMP_1:47;
A94: 1 in ].1/2+0,3/2+0.[ by RCOMP_1:47;
  reconsider U1 = U as non empty Subset of TUC
    by A56,A94,Lm19,Lm21,FUNCT_1:def 12;
  reconsider f1 = f as Function of R^1 | d1, TUC | U1;
A95: [#](TUC | U) = U by PRE_TOPC:def 10;
A96: F2 = CircleMap.:d by A91,Th41;
A97: f is one-to-one by A91,A92,Lm4,Th40;
A98: f is continuous by A92,TOPREALA:29;
  reconsider J = ].1/2+n,1/2+n+p1.[ as non empty Subset of R^1 by TOPMETR:24;
  reconsider F = CircleMap|J as Function of R^1|J,TUC by Lm23;
  d c= J by A93;
  then reconsider d2 = d as Subset of R^1|J by PRE_TOPC:29;
A99: R^1|J|d2 = R^1|d by A93,PRE_TOPC:28;
A100: CircleMap|d = CircleMap|J|d1 by A93,RELAT_1:103;
A101: f1 is onto
  proof
    thus rng f1 c= the carrier of TUC | U1;
    let b be set;
    assume b in the carrier of TUC | U1;
    then consider a being Element of R^1 such that
A102:   a in d and
A103:   b = CircleMap.a by A4,A56,A95,A96,FUNCT_2:116;
A104: dom (CircleMap|d) = d by Lm19,RELAT_1:91,TOPMETR:24;
    (CircleMap|d).a = CircleMap.a by A102,FUNCT_1:72;
    hence b in rng f1 by A92,A102,A103,A104,FUNCT_1:def 5;
  end;
  reconsider nn = 1/2+n as Point of R^1 by TOPMETR:24,XREAL_0:def 1;
  CircleMap(R^1(1/2+n)) is open;
  then
A105: F is open by TOPREALA:33;
  CircleMap | ].1/2+n,1/2+n+1.[ is one-to-one;
  then f is open by A92,A99,A100,A101,A105,TOPREALA:31;
  hence f is_homeomorphism by A97,A98,A101,TOPREALA:37;
end;

