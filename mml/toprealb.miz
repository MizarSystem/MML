theorem Th19:
for n being non empty Element of NAT,
    r being positive (real number),
    x being Point of TOP-REAL n,
    f being Function of Tunit_circle(n), Tcircle(x,r) st
  for a being Point of Tunit_circle(n),
      b being Point of TOP-REAL n st a = b holds f.a = r*b+x
  holds f is_homeomorphism
proof
  let n be non empty Element of NAT,
      r be positive (real number),
      x be Point of TOP-REAL n,
      f being Function of Tunit_circle(n), Tcircle(x,r) such that
A1: for a being Point of Tunit_circle(n),
        b being Point of TOP-REAL n st a = b holds f.a = r*b+x;
  set U = Tunit_circle(n), C = Tcircle(x,r);
  defpred P[Point of TOP-REAL n,set] means $2 = r*$1+x;
A2: 1/r is Real by XREAL_0:def 1;
A3: 1/r*r = 1 by XCMPLX_1:88;
A4: the carrier of U = Sphere(0.REAL n,1) by Th9;
A5: the carrier of C = Sphere(x,r) by Th9;
A6: dom f = the carrier of U by FUNCT_2:def 1;
  hence dom f = [#]U;
  thus
A7: rng f = [#]C
  proof
    thus rng f c= [#]C;
    let b be set;
    assume
A8:   b in [#]C;
    then reconsider c = b as Point of TOP-REAL n by TOPMETR:2;
    set a = 1/r * (c - x);
    |.a-0.REAL n.| = |.a.| by JORDAN2C:13
      .= abs(1/r)*|. c-x .| by A2,TOPRNS_1:8
      .= 1/r*|. c-x .| by ABSVALUE:def 1
      .= 1/r*r by A5,A8,TOPREAL9:9;
    then
A9: a in Sphere(0.REAL n,1) by A3;
    then f.a = r*a+x by A1,A4
      .= r*(1/r)*(c-x)+x by EUCLID:34
      .= c-x+x by A3,EUCLID:33
      .= b by EUCLID:52;
    hence b in rng f by A4,A6,A9,FUNCT_1:def 5;
  end;
  thus
A10: f is one-to-one
  proof
    let a, b be set such that
A11:   a in dom f and
A12:   b in dom f and
A13:   f.a = f.b;
    reconsider a1 = a, b1 = b as Point of TOP-REAL n by A4,A6,A11,A12;
A14: f.a1 = r*a1+x by A1,A11;
    f.b1 = r*b1+x by A1,A12;
    then r*a1 = r*b1+x-x by A13,A14,EUCLID:52;
    then r*a1 = r*b1 by EUCLID:52;
    hence a = b by EUCLID:38;
  end;
A15: for u being Point of TOP-REAL n ex y being Point of TOP-REAL n st P[u,y];
  consider F being Function of TOP-REAL n, TOP-REAL n such that
A16: for x being Point of TOP-REAL n holds P[x,F.x] from FUNCT_2:sch 3(A15);
  set f1 = id TOP-REAL n;
  set f2 = (TOP-REAL n) --> x;
  for p being Point of TOP-REAL n holds F.p = r * f1.p + 1 * f2.p
  proof
    let p be Point of TOP-REAL n;
    thus r * f1.p + 1 * f2.p = r*f1.p + f2.p by EUCLID:33
      .= r*p + f2.p by FUNCT_1:35
      .= r*p + x by FUNCOP_1:13
      .= F.p by A16;
  end;
  then
A17: F is continuous by TOPALG_1:17;
  dom F = the carrier of TOP-REAL n by FUNCT_2:def 1;
  then
A18: dom (F|Sphere(0.REAL n,1)) = Sphere(0.REAL n,1) by RELAT_1:91;
  for a being set st a in dom f holds f.a = (F|Sphere(0.REAL n,1)).a
  proof
    let a be set such that
A19:   a in dom f;
    reconsider y = a as Point of TOP-REAL n by A19,TOPMETR:2;
    thus f.a = r*y+x by A1,A19
      .= F.y by A16
      .= (F|Sphere(0.REAL n,1)).a by A4,A19,FUNCT_1:72;
  end;
  then f = F|Sphere(0.REAL n,1) by A4,A6,A18,FUNCT_1:9;
  hence f is continuous by A17,BORSUK_4:69;
  defpred R[Point of TOP-REAL n,set] means $2 = 1/r*($1-x);
A20: for u being Point of TOP-REAL n ex y being Point of TOP-REAL n st R[u,y];
  consider G being Function of TOP-REAL n, TOP-REAL n such that
A21: for a being Point of TOP-REAL n holds R[a,G.a] from FUNCT_2:sch 3(A20);
A22: dom (f") = the carrier of C by FUNCT_2:def 1;
  dom G = the carrier of TOP-REAL n by FUNCT_2:def 1;
  then
A23: dom (G|Sphere(x,r)) = Sphere(x,r) by RELAT_1:91;
  for p being Point of TOP-REAL n holds G.p = 1/r * f1.p + (-1/r) * f2.p
  proof
    let p be Point of TOP-REAL n;
    thus 1/r * f1.p + (-1/r) * f2.p = 1/r*p + (-1/r)*f2.p by FUNCT_1:35
      .= 1/r*p + (-1/r)*x by FUNCOP_1:13
      .= 1/r*p + -1/r*x by EUCLID:44
      .= 1/r*p - 1/r*x by EUCLID:45
      .= 1/r*(p-x) by EUCLID:53
      .= G.p by A21;
  end;
  then
A24: G is continuous by TOPALG_1:17;
  for a being set st a in dom (f") holds f".a = (G|Sphere(x,r)).a
  proof
    let a be set such that
A25:   a in dom (f");
    reconsider y = a as Point of TOP-REAL n by A25,TOPMETR:2;
    reconsider ff = f as Function;
    set e = 1/r * (y - x);
    |.e-0.REAL n.| = |.e.| by JORDAN2C:13
      .= abs(1/r)*|. y-x .| by A2,TOPRNS_1:8
      .= 1/r*|. y-x .| by ABSVALUE:def 1
      .= 1/r*r by A5,A25,TOPREAL9:9;
    then
A26: 1/r * (y - x) in Sphere(0.REAL n,1) by A3;
    then f.e = r*e+x by A1,A4
      .= r*(1/r)*(y-x)+x by EUCLID:34
      .= y-x+x by A3,EUCLID:33
      .= y by EUCLID:52;
    then ff".a = 1/r * (y - x) by A4,A6,A10,A26,FUNCT_1:54;
    hence f".a = 1/r*(y-x) by A7,A10,TOPS_2:def 4
      .= G.y by A21
      .= (G|Sphere(x,r)).a by A5,A25,FUNCT_1:72;
  end;
  then f" = G|Sphere(x,r) by A5,A22,A23,FUNCT_1:9;
  hence f" is continuous by A24,BORSUK_4:69;
end;

registration
  cluster Tunit_circle(2) -> being_simple_closed_curve;
coherence;
