          then (f|X).x0 + r/2 in {s: (f|X).x0 - r < s & s < (f|X).x0 + r} by
A72;
          then A73: M2 in ].(f|X).x0 - r,(f|X).x0 + r.[;
          then A74: [.M1,M2.] c= ].(f|X).x0 - r,(f|X).x0 + r.[ by A71,
RCOMP_1:17;
             M1 in N2 by A64,A65,A71;
          then M1 in L by A63;
          then consider r1 such that
          A75: r1 in dom (f|X) & r1 in
 X & M1 = (f|X).r1 by A2,A6,A8,PARTFUN2:78;
             M2 in N2 by A64,A65,A73;
          then M2 in L by A63;
          then consider r2 such that
          A76: r2 in dom (f|X) & r2 in
 X & M2 = (f|X).r2 by A2,A6,A8,PARTFUN2:78;
          set R = min(r1 - x0,x0 - r2);
A77:          (f|X).x0 < (f|X).x0 + r/2 by A66,XREAL_1:31;
          then A78: M1 < (f|X).x0 by XREAL_1:21;
          A79: r1 <> x0 by A75,A77,XREAL_1:21;
              now assume
            A80: x0 > r1;
            A81: x0 in X /\ dom (f|X) by A3,A4,XBOOLE_0:def 3;
               r1 in X /\ dom(f|X) by A75,XBOOLE_0:def 3;
            hence contradiction by A62,A75,A78,A80,A81,RFUNCT_2:def 5;
           end;
