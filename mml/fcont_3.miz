              end;
              x0 <> r2 by A13,A20,XREAL_1:31,217;
              then x0 < r2 by A22,XXREAL_0:1;
              then
A32:          r2 - x0 > 0 by XREAL_1:52;
              set R = min(x0 - r1,r2 - x0);
A33:          R <= r2 - x0 by XXREAL_0:17;
              r1 <> x0 by A27,A28,XREAL_1:21;
              then r1 < x0 by A30,XXREAL_0:1;
