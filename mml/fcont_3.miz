               r1 in X /\ dom(f|X) by A22,XBOOLE_0:def 3;
            hence contradiction by A9,A22,A25,A27,A28,RFUNCT_2:def 4;
           end;
          then r1 < x0 by A26,REAL_1:def 5;
          then A29: x0 - r1 > 0 by XREAL_1:52;
          A30: M2 > (f|X).x0 by A13,XREAL_1:31;
          A31: x0 <> r2 by A13,A23,XREAL_1:31;
              now assume
            A32: r2 < x0;
            A33: x0 in X /\ dom(f|X) by A3,A4,XBOOLE_0:def 3;
               r2 in X /\ dom (f|X) by A23,XBOOLE_0:def 3;
            hence contradiction by A9,A23,A30,A32,A33,RFUNCT_2:def 4;
           end;
          then x0 < r2 by A31,REAL_1:def 5;
          then r2 - x0 > 0 by XREAL_1:52;
          then R > 0 by A29,XXREAL_0:15;
          then reconsider N = ].x0 - R,x0+ R.[ as Neighbourhood of x0 by
RCOMP_1:def 7;
          take N;
          let x be real number; assume
          A34: x in dom (f|X) & x in N;
