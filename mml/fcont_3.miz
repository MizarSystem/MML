        A40: r/2 > 0 by A39,SEQ_2:3;
        then A41: (f|X).x0 - r < (f|X).x0 - r + r/2 by XREAL_1:31;
        A42: (f|X).x0 < (f|X).x0 + r/2 by A40,XREAL_1:31;
        then A43: (f|X).x0 - r/2 < (f|X).x0 by XREAL_1:21;
        A44: (f|X).x0 < (f|X).x0 + r by A39,XREAL_1:31;
        then (f|X).x0 - r/2 < (f|X).x0 + r by A43,XXREAL_0:2;
        then (f|X).x0 - r/2 in {s: (f|X).x0 - r < s & s < (f|X).x0 + r} by A41
;
        then A45: M1 in ].(f|X).x0 - r,(f|X).x0 + r.[;
          A46: (f|X).x0 + r/2 < (f|X).x0 + r/2 + r/2 by A40,XREAL_1:31;
           (f|X).x0 - r < (f|X).x0 + r - r by A44,XREAL_1:11;
        then (f|X).x0 - r < (f|X).x0 + r/2 by A42,XXREAL_0:2;
        then (f|X).x0 + r/2 in {s: (f|X).x0 - r < s & s < (f|X).x0 + r} by A46
;
        then M2 in ].(f|X).x0 - r,(f|X).x0 + r.[;
        then A47: [.M1,M2.] c= ].(f|X).x0 - r,(f|X).x0 + r.[ by A45,RCOMP_1:17;
        consider r1 such that
        A48: r1 in dom (f|X) & r1 in X & M1 = (f|X).r1 by A1,A38,PARTFUN2:78;
        consider r2 such that
        A49: r2 in dom (f|X) & r2 in X & M2 = (f|X).r2 by A1,A38,PARTFUN2:78;
        set R = min(r1 - x0,x0 - r2);
A50:        (f|X).x0 < (f|X).x0 + r/2 by A40,XREAL_1:31;
        then A51: M1 < (f|X).x0 by XREAL_1:21;
        A52: r1 <> x0 by A48,A50,XREAL_1:21;
            now assume
          A53: x0 > r1;
          A54: x0 in X /\ dom (f|X) by A36,A37,XBOOLE_0:def 3;
             r1 in X /\ dom(f|X) by A48,XBOOLE_0:def 3;
          hence contradiction by A35,A48,A51,A53,A54,RFUNCT_2:def 5;
         end;
        then r1 > x0 by A52,REAL_1:def 5;
        then A55: r1 - x0 > 0 by XREAL_1:52;
        A56: M2 > (f|X).x0 by A40,XREAL_1:31;
        A57: x0 <> r2 by A40,A49,XREAL_1:31;
            now assume
          A58: r2 > x0;
          A59: x0 in X /\ dom (f|X) by A36,A37,XBOOLE_0:def 3;
             r2 in X /\ dom (f|X) by A49,XBOOLE_0:def 3;
          hence contradiction by A35,A49,A56,A58,A59,RFUNCT_2:def 5;
         end;
        then x0 > r2 by A57,REAL_1:def 5;
        then x0 - r2 > 0 by XREAL_1:52;
        then R > 0 by A55,XXREAL_0:15;
        then reconsider N = ].x0 - R,x0 + R.[ as Neighbourhood of x0 by
RCOMP_1:def 7;
        take N;
        let x be real number; assume
        A60: x in dom (f|X) & x in N;
        then x in {s: x0 - R < s & s < x0 + R};
then A61:        ex s st s = x & x0 - R < s & s < x0 + R;
        then x0 < R + x by XREAL_1:21;
        then A62: x0 - x < R + x - x by XREAL_1:11;
        R <= x0 - r2 by XXREAL_0:17;
        then x0 - x < x0 - r2 by A62,XXREAL_0:2;
        then -(x0 - x) > -(x0 - r2) by XREAL_1:26;
        then A63: x - x0 + x0 > r2 - x0 + x0 by XREAL_1:8;
        A64: x - x0 < R by A61,XREAL_1:21;  R <= r1 - x0 by XXREAL_0:17;
        then x - x0 < r1-x0 by A64,XXREAL_0:2;
        then A65: x - x0 + x0 < r1 - x0 + x0 by XREAL_1:8;
           dom (f|X) c= X by RELAT_1:87;
        then A66: x in X /\ dom(f|X) by A60,XBOOLE_1:28;
           r2 in X /\ dom(f|X) by A49,XBOOLE_0:def 3;
        then A67: (f|X).x <= (f|X).r2 by A35,A63,A66,RFUNCT_2:def 5;
           r1 in X /\ dom(f|X) by A48,XBOOLE_0:def 3;
        then (f|X).r1 <= (f|X).x by A35,A65,A66,RFUNCT_2:def 5;
        then (f|X).x in {s: M1 <= s & s <= M2} by A48,A49,A67;
        then (f|X).x in [.M1,M2.];
        hence (f|X).x in N1 by A39,A47;
       end;
      hence thesis by A37,FCONT_1:4;
     end;
    hence f is_continuous_on X by A1,FCONT_1:def 2;
   end;
   end;
  hence thesis;
 end;

