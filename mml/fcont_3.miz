    then A14: x0 + r/2 in ].x0 - r, x0 + r.[;
    then A15: [.x0 - r/2, x0 + r/2.] c= ].x0 - r, x0 + r.[ by A10,RCOMP_1:17;
    then A16: f is_continuous_on [.x0 - r/2, x0 + r/2.] by A4,A5,FCONT_1:17;
    set fm = f.(x0 - r/2);
    set fp = f.(x0 + r/2);
    A17: x0 - r/2 < x0 + r/2 by A9,A13,XXREAL_0:2;
    A18: N c= dom f by A5,FCONT_1:def 2;
then A19: x0 + r/2 in L /\ dom f by A4,A14,XBOOLE_0:def 3;
     A20: x0 - r/2 in L /\ dom f by A4,A10,A18,XBOOLE_0:def 3;
    A21: x0 in L /\ dom f by A3,XBOOLE_0:def 3;
        now per cases by A2;
     suppose A22: f is_increasing_on L;
       then fm < fp by A17,A19,A20,RFUNCT_2:def 2;
      then [.fp,fm.] = {} by RCOMP_1:13;
      then A23: [.fm,fp.] \/ [.fp,fm.] = [.fm,fp.];
         fm < f.x0 by A9,A20,A21,A22,RFUNCT_2:def 2;
      then A24: 0 < f.x0 - fm by XREAL_1:52;
      set R = min(f.x0 - fm,fp - f.x0);
         f.x0 < fp by A13,A19,A21,A22,RFUNCT_2:def 2;
      then 0 < fp - f.x0 by XREAL_1:52;
      then 0 < R by A24,XXREAL_0:15;
      then reconsider N1 = ].r1 - R, r1 + R.[ as Neighbourhood of r1 by
RCOMP_1:def 7;
      take N1;
      thus N1 c= rng f
       proof let x; assume x in N1;
        then x in {r2: r1 - R < r2 & r2 < r1 + R};
        then consider r2 such that
        A25: r2 = x & f.x0 - R < r2 & r2 < f.x0 + R by A3;
        A26: R <= f.x0 - fm & R <= fp - f.x0 by XXREAL_0:17;
        then f.x0 - (f.x0 - fm) <= f.x0 - R by REAL_1:92;
        then A27: fm < r2 by A25,XXREAL_0:2;
           f.x0 + R <= f.x0 + (fp - f.x0) by A26,XREAL_1:9;
        then r2 < fp by A25,XXREAL_0:2;
        then r2 in {g2: fm < g2 & g2 < fp} by A27;
        then A28: r2 in ].fm,fp.[;
           ].fm,fp.[ c= [.fm,fp.] by RCOMP_1:15;
        then consider s such that
        A29: s in [.x0 - r/2, x0 + r/2.] & x = f.s by A16,A17,A23,A25,A28,
FCONT_2:16;
           s in N by A4,A15,A29; hence thesis by A18,A29,FUNCT_1:def 5;
       end;
     end;
     suppose A30: f is_decreasing_on L;
       then fp < fm by A17,A19,A20,RFUNCT_2:def 3;
      then [.fm,fp.] = {} by RCOMP_1:13;
      then A31: [.fm,fp.] \/ [.fp,fm.] = [.fp,fm.];
      f.x0 < fm by A9,A20,A21,A30,RFUNCT_2:def 3;
      then A32: 0 < fm - f.x0 by XREAL_1:52;
      set R = min(fm - f.x0,f.x0 - fp);
      fp < f.x0 by A13,A19,A21,A30,RFUNCT_2:def 3;
      then 0 < f.x0 - fp by XREAL_1:52;
      then 0 < R by A32,XXREAL_0:15;
      then reconsider N1 = ].r1 - R, r1 + R.[ as Neighbourhood of r1 by
RCOMP_1:def 7;
      take N1;
      thus N1 c= rng f
       proof let x; assume x in N1;
        then x in {r2: r1 - R < r2 & r2 < r1 + R};
        then consider r2 such that
        A33: r2 = x & f.x0 - R < r2 & r2 < f.x0 + R by A3;
        A34: R <= fm - f.x0 & R <= f.x0 - fp by XXREAL_0:17;
        then f.x0 - (f.x0 - fp) <= f.x0 - R by REAL_1:92;
        then A35: fp < r2 by A33,XXREAL_0:2;
        f.x0 + R <= f.x0 + (fm - f.x0) by A34,XREAL_1:9;
        then r2 < fm by A33,XXREAL_0:2;
        then r2 in {g2: fp < g2 & g2 < fm} by A35;
        then A36: r2 in ].fp,fm.[;
        ].fp,fm.[ c= [.fp,fm.] by RCOMP_1:15;
        then consider s such that
        A37: s in [.x0 - r/2, x0 + r/2.] & x = f.s by A16,A17,A31,A33,A36,
FCONT_2:16;
        s in N by A4,A15,A37; hence thesis by A18,A37,FUNCT_1:def 5;
       end;
     end;
     end;
    hence ex N be Neighbourhood of r1 st N c= rng f;
   end;
  hence thesis by RCOMP_1:41;
 end;
