          A50: p - R < p by A44,XREAL_1:46;  p < p + r by A43,XREAL_1:31;
          then A51: p - R < p + r by A50,XXREAL_0:2;
             p - r < p - R by A45,REAL_1:92;
          then p - R in {s: p - r < s & s < p + r} by A51;
          then p - R in ].p - r, p + r.[;
          then A52: [.p - R, p + R.] c= N1 by A43,A49,RCOMP_1:17;
             p + R in {s : p < s} by A48;
          then p + R in L by LIMFUNC1:def 3;
          then consider r1 such that
          A53: r1 in dom (f|X) & r1 in
 X & p + R = (f|X).r1 by A2,A6,A8,PARTFUN2:78;
          A54: r1 <> x0 by A42,A44,A53,XREAL_1:31;
              now assume
            A55: x0 > r1;
            A56: x0 in X /\ dom (f|X) by A3,A4,XBOOLE_0:def 3;
               r1 in X /\ dom (f|X) by A53,XBOOLE_0:def 3;
            hence contradiction by A9,A42,A48,A53,A55,A56,RFUNCT_2:def 4;
           end;
          then r1 > x0 by A54,REAL_1:def 5; then r1 - x0 > 0 by XREAL_1:52;
          then reconsider N = ].x0 - (r1 - x0),x0 + (r1 - x0).[ as
           Neighbourhood of x0 by RCOMP_1:def 7;
          take N;
          let x be real number such that
          A57: x in dom(f|X) & x in N;
          A58: dom (f|X) c= X by RELAT_1:87;
          then (f|X).x in l by A2,A6,A57,FUNCT_1:def 12;
          then (f|X).x in {s: p <= s} by LIMFUNC1:def 2;
          then ex s st s = (f|X).x & p <= s;
          then A59: p - R <= (f|X).x by A50,XXREAL_0:2;
          A60: x in X /\ dom (f|X) by A57,A58,XBOOLE_0:def 3;
          A61: r1 in X /\ dom (f|X) by A53,XBOOLE_0:def 3;
          x in {s: x0 - (r1-x0) < s & s < x0 + (r1-x0)} by A57;
          then ex s st s = x & x0 - (r1-x0) < s & s < x0 + (r1-x0);
          then p + R >= (f|X).x by A9,A53,A60,A61,RFUNCT_2:def 4;
