          then A82: r1 - x0 > 0 by XREAL_1:52;
          A83: M2 > (f|X).x0 by A66,XREAL_1:31;
          A84: x0 <> r2 by A66,A76,XREAL_1:31;
              now assume
            A85: r2 > x0;
            A86: x0 in X /\ dom (f|X) by A3,A4,XBOOLE_0:def 3;
               r2 in X /\ dom (f|X) by A76,XBOOLE_0:def 3;
            hence contradiction by A62,A76,A83,A85,A86,RFUNCT_2:def 5;
           end;
          then x0 > r2 by A84,REAL_1:def 5;
          then x0 - r2 > 0 by XREAL_1:52;
