      COMPLEX1, PROB_1, REAL_1, FUNCT_1, FUNCOP_1, SEQ_1, RELSET_1, SEQ_2,
      PARTFUN1, PARTFUN2, RCOMP_1, RFUNCT_2, FCONT_1, LIMFUNC1, XXREAL_0;
 constructors PARTFUN1, REAL_1, SQUARE_1, COMPLEX1, SEQ_2, PROB_1, RCOMP_1,
      PARTFUN2, RFUNCT_2, FCONT_1, LIMFUNC1, NAT_D;
 registrations RELSET_1, PARTFUN2, RFUNCT_2, XREAL_0, ARYTM_3, MEMBERED,
      ORDINAL1, RCOMP_1, SEQM_3, XXREAL_0;
 requirements REAL, NUMERALS, BOOLE, SUBSET, ARITHM;
