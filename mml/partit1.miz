              case 1<=i & i<len fd;
              hence thesis by A62;
              case 1<=i & i=len fd;
              hence thesis by A48,A50,A55,A57,A61;
            end;
            hence thesis;
          end;
        for i st 1<=i & i<len f holds
             (ex p1,p2,x0 being set st p1 in PA & p2 in PB
              & f.i in p1 & x0 in p1 & x0 in p2 & f.(i+1) in p2)
      proof
        let i;
