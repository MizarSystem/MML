canceled 6;

theorem Th18:
 for a,b being Element of multL_Trivial holds a = b
  proof
   let a,b be Element of multL_Trivial;
   thus a = {} by Def11,TARSKI:def 1 .= b by Def11,TARSKI:def 1;
