::   Generalization "increasing   ::
::::::::::::::::::::::::::::::::::::

definition
  let Ne,Ke;let f be Function of Ne,Ke;
  attr f is "increasing means :Def5:
  for l,m st l in rng f & m in rng f & l < m holds min* f"{l} < min* f"{m};
end;

theorem Th74:
  for F be Function of Ne,Ke st F is "increasing
    holds min* rng F=F.min* dom F
proof
  let F be Function of Ne,Ke such that
  A1:F is "increasing;
  now per cases;
    suppose A2:rng F is empty;
      then dom F={} by RELAT_1:65;
      then min* dom F=0 & min* rng F=0 & F.0=0
        by A2,HENMODEL:def 1,FUNCT_1:def 4;
      hence thesis;
    end;
    suppose A2:rng F is non empty;
      then reconsider rngF=rng F,Ke as non empty Subset of NAT by XBOOLE_1:1,3;
      Ke is non empty;
      then reconsider domF=dom F as non empty Subset of NAT
        by A2,FUNCT_2:def 1,RELAT_1:65;
      set md=min* domF;set mr=min* rngF;
      mr=F.md
      proof
        assume A3:mr<>F.md;
        A4:md in dom F by HENMODEL:def 1;
        then F.md in rngF by FUNCT_1:def 5;
        then mr <=F.md by HENMODEL:def 1;
        then mr < F.md & mr in rngF & F.md in rng F
          by A3,A4,REAL_1:def 5,HENMODEL:def 1,FUNCT_1:def 5;
        then A5:min* F"{mr}< min* F"{F.md} by A1,Def5;
        mr in rngF by HENMODEL:def 1;
        then consider x such that
          A6:x in dom F & F.x=mr by FUNCT_1:def 5;
        F.x in {mr} & F.md in {F.md} & md in dom F
          by A6,TARSKI:def 1,HENMODEL:def 1;
        then reconsider Fmr=F"{mr},Fmd=F"{F.md} as non empty Subset of NAT
          by XBOOLE_1:1,A6,FUNCT_1:def 13;
        min* Fmr in Fmr & F.md in {F.md} & md in domF
          by HENMODEL:def 1,TARSKI:def 1;
        then min* Fmr in domF & md in Fmd by FUNCT_1:def 13;
        then min* Fmr >= md & md >= min* Fmd by HENMODEL:def 1;
        hence contradiction by A5,AXIOMS:22;
      end;
      hence thesis;
    end;
  end;
  hence thesis;
end;

:: existence of an expansion 

theorem
  for F be Function of Ne,Ke st rng F is finite
    ex I be Function of Ne,Ke,P be Permutation of rng F st
      F=P*I & rng F=rng I & I is "increasing
proof
  defpred P[set] means
    for Ne,Ke be Subset of NAT,F be Function of Ne,Ke st F=$1 & rng F is finite
    ex P be Permutation of rng F,G be Function of Ne,Ke st
      F=P*G & rng F = rng G &
      for i,j st i in rng G & j in rng G & i<j holds min* G"{i} < min* G"{j};
  A1:P[{}]
  proof
    let Ne,Me be Subset of NAT,F be Function of Ne,Me such that
    A2:F={}& rng F is finite;
    reconsider R=rng F as empty set by A2,RELAT_1:60;
    set P={};
    rng P={} & dom P={};
    then reconsider P as Function of R,R by FUNCT_2:3;
    dom P={} & rng R= {} by FUNCT_2:def 1;
    then P is one-to-one onto by FUNCT_2:def 3;
    then reconsider P as Permutation of rng F by FUNCT_2:def 4;    
    take P,F;
    A3:for i,j st i in rng F & j in rng F & i<j holds min* F"{i} < min* F"{j}
      proof
        let i,j such that A4:i in rng F & j in rng F & i<j;
        i in R & j in R by A4;
        hence thesis;
      end;
    rng F=R;
    then F={} by RELAT_1:64;
    hence thesis by A3,RELAT_1:62;    
  end;
  defpred Q[set,Function] means $1=$2.min* dom $2;
  A2:for F be Function st
     for x st x in rng F & Q[x,F] holds P[F|(dom F\F"{x})] holds P[F]
     proof
       let F' be Function such that
       A3:for x st x in rng F' & Q[x,F'] holds P[F'|(dom F'\F'"{x})];
       let N,K be Subset of NAT,F be Function of N,K such that
             A4:F=F' & rng F is finite;
       now per cases;
         suppose rng F' is empty;
           then F'={} by RELAT_1:64;
           hence thesis by A1,A4;   
         end;
         suppose A5:rng F' is non empty;
           reconsider K as non empty Subset of NAT by A4,A5,XBOOLE_1:3;
           reconsider domF=dom F as non empty Subset of NAT by
             A4,A5,XBOOLE_1:1,RELAT_1:65;
           set m=min* domF;
           set D=dom F\F"{F.m};
           min* domF in domF by HENMODEL:def 1;
           then A6:F.m in rng F by FUNCT_1:def 5;
           now per cases;
             suppose rng (F|D) is empty;
               then rng F\{F.m}={} by Th66;
               then A7:rng F={F.m} by A6,ZFMISC_1:66;
               set P=id rng F;
               rng P=rng F by RELAT_1:71;
               then P is one-to-one onto by FUNCT_1:52,FUNCT_2:def 3;
               then reconsider P as Permutation of rng F by FUNCT_2:def 4;
               F is Function of dom F,rng F & rng F <>0 by FUNCT_2:3,A6;  
               then A8:P*F = F by FUNCT_2:23;                                  
               for i,j st i in rng F & j in rng F & i<j holds
                 min* F"{i} < min* F"{j}
               proof
                 let i,j such that A9:i in rng F & j in rng F & i<j;
                 i =F.m & j=F.m by A7,A9,TARSKI:def 1;
                 hence thesis by A9; 
               end;  
               hence thesis by A8;
             end;
             suppose A7:rng (F|D) is non empty;              
               rng (F|D) c= rng F by RELAT_1:99;
               then reconsider rFD=rng (F|D) as non empty finite Subset of NAT
                 by A7,XBOOLE_1:1,A4,FINSET_1:13;
               reconsider rF=rng F as non empty finite Subset of NAT
                 by A6,XBOOLE_1:1,A4;
               reconsider dFD=dom (F|D) as Subset of NAT by XBOOLE_1:1;
               reconsider FD=F|D as Function of dFD,rFD by FUNCT_2:3;
               A8:dFD=dom F\F"{F.m}
               proof
                 dFD= dom FD & dom FD = dom F /\(dom F\F"{F.m}) &
                   (dom F\F"{F.m}) c= dom F
                   by FUNCT_1:68,XBOOLE_1:36;
                 hence thesis by XBOOLE_1:28;
               end;       
               consider P be Permutation of rng FD,
                 G be Function of dFD,rFD such that
                 A9:FD=P*G & rng FD = rng G and
                 A10:for i,j st i in rng G & j in rng G & i<j holds
                    min* G"{i} < min* G"{j} by A3,A4,A6;
               consider Orde be Function of rF,card rF such that
                 A11:Orde is  bijective and
                 A12:for n,k st n in dom Orde & k in dom Orde & n<k holds
                         Orde.n < Orde.k by Th71;
               Orde is one-to-one onto by A11,FUNCT_2:def 4;
               then Orde is one-to-one & rng Orde=card rF by FUNCT_2:def 3;
               then reconsider Orde'=Orde" as Function of card rF,rF
               by FUNCT_2:31;
               consider P1 be Permutation of rF such that
                 A13:for k st k in rF holds
                 (k < F.m implies P1.k=Orde".((Orde.k)+1)) &
                 (k = F.m implies P1.k=Orde".0) &
                 (k > F.m implies P1.k=k) by A6,A11,A12,Lm6;
               deffunc G(set)=F.m;
               A14:for x st x in N\dFD holds G(x) in K by A6;
               A15:dFD c= N & rFD c= K;
               A16:rFD is empty implies dFD is empty;    
               consider G2 be Function of N,K such that
                 A17:G2|dFD=G & for x st x in N\dFD holds G2.x = G(x)
                 from Sch2(A14,A15,A16);
               not F.m in rng F\{F.m} by ZFMISC_1:64;
               then not F.m in rng FD by Th66;
               then consider P2 be Function of rng FD\/{F.m},rng FD\/{F.m}
                  such that
                  A19:P2|(rng FD) = P & P2.(F.m)=(F.m) by Th69;
               P is one-to-one onto & rng FD =rng F\{F.m} &
               not F.m in rng F\{F.m} by ZFMISC_1:64,Th66;
               then P2 is one-to-one onto & rng FD\/{F.m} = rng F
                 by A6,Lm4,A19,Th70;
               then reconsider P2 as Permutation of rng F by FUNCT_2:def 4; 
               A20:rng G2 c= rng F
               proof
                 let Gx be set such that A21:Gx in rng G2;
                 consider x such that
                   A22:x in dom G2 & G2.x=Gx by A21,FUNCT_1:def 5;
                 dom G2=N by FUNCT_2:def 1;
                 then dom G2/\dFD \/(N\dFD)=dom G2 by XBOOLE_1:51;
                 then dom G=dom G2/\dFD & x in dom G2/\dFD or x in N\dFD
                   by A22,XBOOLE_0:def 2,A17,FUNCT_1:68;
                 then G.x in rng FD & G.x=G2.x & rng FD=rng F\{F.m}
                   or G2.x=F.m & m in domF
                   by A9,A17,FUNCT_1:68,def 5,Th66,HENMODEL:def 1;
                 hence thesis by A22,XBOOLE_0:def 4,FUNCT_1:def 5;
               end;
               rng F c= rng G2
               proof
                 let Fx be set such that A20:Fx in rng F;
                 rng FD=rng F\{F.m} by Th66;
                 then A21:rng F=rng FD\/{F.m} by A6,Lm4;
                 now per cases by A20,A21,XBOOLE_0:def 2;
                   suppose Fx in rng FD;
                     then Fx in rng G & rng (G2|dFD) c=rng G2 by A9,RELAT_1:99;
                     hence thesis by A17;
                   end;
                   suppose A22:Fx in {F.m};
                     m in dom F & F.m in {F.m} & K is non empty
                       by HENMODEL:def 1,TARSKI:def 1;
                     then m in F"{F.m}&dom F=N by FUNCT_1:def 13,FUNCT_2:def 1;
                     then not m in dom F\F"{F.m} & m in N & N=dom G2 
                       by XBOOLE_0:def 4,FUNCT_2:def 1;
                     then m in N\dFD & Fx=F.m & m in dom G2
                       by A22,A8,XBOOLE_0:def 4,TARSKI:def 1;
                     then G2.m=Fx & G2.m in rng G2 by A17,FUNCT_1:def 5;
                     hence thesis;
                   end;
                 end;
                 hence thesis;
               end;   
               then A21:rng G2=rng F by A20,XBOOLE_0:def 10;
               A22:F=P2*G2
                 proof
                   K is non empty;
                   then A23:dom F=N & dom G2=N by FUNCT_2:def 1;
                   A24:for x holds x in dom F iff x in dom G2&G2.x in dom P2
                     proof
                       let x;
                       thus x in dom F implies x in dom G2 & G2.x in dom P2
                       proof
                         assume x in dom F;
                         then x in dom G2 & dom P2=rng F
                           by A6,A23,FUNCT_2:def 1;
                         hence thesis by A21,FUNCT_1:def 5;
                       end;
                       thus x in dom G2&G2.x in dom P2 implies x in dom F
                         by A23;
                     end;
                   for x st x in dom F holds F.x = P2.(G2.x)
                     proof
                       let x such that A25:x in dom F;
                       now per cases by A25,XBOOLE_0:def 4;
                         suppose x in N\dFD;
                           then A26:P2.(G2.x)=F.m & x in dom F &
                             not x in dom F\F"{F.m}
                             by A17,A23,A8,A19,XBOOLE_0:def 4;
                           then x in F"{F.m} by XBOOLE_0:def 4;
                           then F.x in {F.m} by FUNCT_1:def 13;
                           hence thesis by A26,TARSKI:def 1;
                         end;
                         suppose x in dFD;
                           then x in dom FD & x in dom G
                             by FUNCT_2:def 1;
                           then F.x=FD.x & G.x=G2.x & FD.x=P.(G.x) &
                             G.x in rng FD & dom P=rng FD
                             by A9,A17,FUNCT_1:22,68,def 5,FUNCT_2:def 1;
                           hence thesis by A19,FUNCT_1:68;
                         end;
                       end;
                       hence thesis;  
                     end;
                   hence F=P2*G2 by A24,FUNCT_1:20;
                 end;
               A23:G2"{F.m}=F"{F.m}
               proof
                 thus G2"{F.m}c=F"{F.m}
                 proof
                   let x such that A24:x in G2"{F.m}; 
                   x in N\dFD
                   proof
                     assume not x in N\dFD;
                     then x in dom G2 & dom G2=N & not x in N or x in dFD
                        by A24,XBOOLE_0:def 4;
                     then x in dom G by FUNCT_2:def 1;
                     then G.x in rng G & G.x=G2.x by A17,FUNCT_1:68,def 5;
                     then G2.x in rng FD & rng FD=rng F\{F.m} by Th66;
                     then not G2.x in {F.m} by XBOOLE_0:def 4;
                     hence thesis by A24,FUNCT_1:def 13;   
                   end;
                   then x in N\(dom F\F"{F.m}) &dom F=N by A8,A6,FUNCT_2:def 1;
                   then x in dom F & not x in dom F\F"{F.m} by XBOOLE_0:def 4;
                   hence x in F"{F.m} by XBOOLE_0:def 4;    
                 end;
                 thus F"{F.m}c=G2"{F.m}
                 proof
                   let x such that A24:x in F"{F.m};
                   not x in dom F\F"{F.m} & dom F=N
                     by A24,XBOOLE_0:def 4,A6,FUNCT_2:def 1;
                   then x in N\dFD & x in N by A24,XBOOLE_0:def 4,A8;
                   then G2.x=F.m & x in dom G2 by A17,FUNCT_2:def 1;
                   then G2.x in {F.m} & x in dom G2 by TARSKI:def 1;
                   hence thesis by FUNCT_1:def 13;
                 end;
               end;  
               A24:for n st n in rng FD holds G"{n}=G2"{n}
                 proof
                   let n such that A25:n in rng FD;
                   K is non empty;
                   then dFD=dom F\F"{F.m} & dom F=N & dom G2=N &
                     rng FD=rng F\{F.m} by FUNCT_2:def 1,A8,Th66;
                   then dFD= dom G2\G2"{F.m} & not n in {F.m}
                     by A23,A25,XBOOLE_0:def 4;
                   then G2|(dom G2\G2"{F.m})=G & n<>F.m by A17,TARSKI:def 1;
                   hence G"{n}=G2"{n} by Th66;
                 end;
                reconsider P21=P2*P1" as Function of rF,rF by FUNCT_2:19;   
                rng (P1")=dom P1 & dom P1=rF & rng P2=rF
                  by FUNCT_2:def 1,def 3,FUNCT_1:55;
                then rng P21=rF by FUNCT_2:20;
                then P21 is onto one-to-one by FUNCT_1:46,FUNCT_2:def 3;
                then reconsider P21 as Permutation of rF by FUNCT_2:def 4;
                dom G2=N by FUNCT_2:def 1;
                then G2 is Function of N,rF & rng P1=rF & rng G2=rF
                  by A21,FUNCT_2:3,def 3;
                then A25:(P1*G2) is Function of N,rF & rng (P1*G2) c= K &
                  rng (P1*G2)=rF 
                  by FUNCT_2:19,20,XBOOLE_1:1;
                then reconsider PG=P1*G2 as Function of N,K by FUNCT_2:8;     
                dom G2=N by FUNCT_2:def 1;
                then G2 is Function of N,rF & dom P1=rF by A21,FUNCT_2:def 1,3;
                then (id rF)*G2 = G2 & P1"*P1=id rF
                  by FUNCT_2:23,FUNCT_1:61;  
                then P1"*(P1*G2)=G2 by RELAT_1:55;
                then A26:F=P21*PG by A22,RELAT_1:55;
                for i,j st i in rng PG & j in rng PG & i<j holds
                  min* PG"{i} < min* PG"{j}
                proof
                  let i,j such that A27:i in rng PG & j in rng PG & i<j;
                  consider i1 be set such that
                    A28:i1 in dom P1 & i1 in rng G2 &P1"{i}={i1} &
                      G2"{i1}=PG"{i} by A27,Th73;
                  consider j1 be set such that
                    A29:j1 in dom P1 & j1 in rng G2 & P1"{j}={j1} &
                        G2"{j1}=PG"{j} by A27,Th73;              
                  dom P1=rF by FUNCT_2:def 1;
                  then reconsider i1,j1 as Nat by A28,A29;
                  rng FD =rng F\{F.m} & not F.m in rng F\{F.m}
                    by ZFMISC_1:64,Th66;
                  then A30:rng FD\/{F.m} = rng G2 by A6,A21,Lm4;
                  A31:for n,k st n in dom Orde & k in dom Orde & Orde.n<Orde.k
                    holds n < k
                  proof
                    let n,k such that A32:
                      n in dom Orde & k in dom Orde & Orde.n<Orde.k;
                    assume n>=k;
                    then n>k by A32,REAL_1:def 5;
                    hence contradiction by A12,A32;
                  end;
                  A32:for n,k st n in rng Orde & k in rng Orde holds
                  n < k implies Orde'.n<Orde'.k 
                  proof
                    let n,k such that A33:n in rng Orde & k in rng Orde;
                    assume A34:n < k;
                    A35:Orde is one-to-one onto by A11,FUNCT_2:def 4;
                    then rng Orde = dom Orde' & dom Orde = rng Orde' &
                      rng Orde=card rF
                      by FUNCT_1:55,FUNCT_2:def 3; 
                    then n = Orde.(Orde'.n) & k = Orde.(Orde'.k) &
                    Orde'.k in dom Orde & Orde'.n in dom Orde &
                     dom Orde is Subset of NAT by XBOOLE_1:1,
                      A33,A35,FUNCT_1:57,def 5;
                    hence Orde'.n < Orde'.k by A31,A34;
                  end; 
                 i1 in P1"{i} & j1 in P1"{j} by A28,A29,TARSKI:def 1;
                 then A33':i1 in dom P1 & P1.i1 in {i} & j1 in dom P1 &
                 P1.j1 in {j} & dom P1=rF & (card rF={} implies rF={})
                   by FUNCT_1:def 13,FUNCT_2:def 1,Th33;
                 then A33:P1.i1=i & P1.j1=j & i1 in rF & j1 in rF & dom Orde=rF
                 by TARSKI:def 1,FUNCT_2:def 1;
                 A34:for l st l in rF & l < F.m holds
                   Orde.l+1 in rng Orde & Orde.l+1 is Nat &
                   Orde.l+1<=Orde.(F.m) & Orde.(F.m) is Nat & dom Orde=rF
                   proof
                     let l such that A35:l in rF & l < F.m;
                     Orde is onto & (card rF={} implies rF={})
                       by A11,Th33,FUNCT_2:def 4;
                     then A36:dom Orde=rF & card rF=rng Orde
                       by FUNCT_2:def 1,def 3;
                     then A37':Orde.l < Orde.(F.m) & Orde.(F.m) in card rF &
                       card rF is Subset of NAT
                       by A6,A12,A35,FUNCT_1:def 5,Th8;
                     then Orde.l+1<=Orde.(F.m) & Orde.(F.m) < card rF &
                       Orde.(F.m) is Nat
                       by NAT_1:38,EULER_1:1;
                     then Orde.l+1 < card rF by AXIOMS:22;
                     then Orde.l+1 in rng Orde & Orde.l+1 in card rF &
                       card rF is Subset of NAT by A36,EULER_1:1,Th8;
                     hence thesis by A37',FUNCT_2:def 1,NAT_1:38;
                   end;
                  now per cases by REAL_1:def 5;
                    suppose A35:i1 < F.m & j1<F.m;
                      i1<j1
                      proof
                        assume i1>=j1;
                        then i1>j1 by A33,A27,REAL_1:def 5;
                        then Orde.i1 >Orde.j1 by A33,A12;
                        then Orde.i1+1 >Orde.j1+1 & Orde.i1+1 in rng Orde &
                          Orde.j1+1 in rng Orde & Orde.i1+1 is Nat &
                          Orde.j1+1 is Nat
                          by A34,A35,REAL_1:67,A28,A29;
                        then Orde'.(Orde.i1+1)>Orde'.(Orde.j1+1) &
                          Orde'.(Orde.i1+1)=i & Orde'.(Orde.j1+1)= j
                          by A32,A33,A35,A13;
                        hence contradiction by A27;
                      end;
                      then i1<j1 &(i1 in rng FD or i1 in {F.m})&
                        not i1 in {F.m} &(j1 in rng FD or j1 in {F.m})
                        & not j1 in {F.m}
                        by A28,A29,A30,A35,XBOOLE_0:def 2,TARSKI:def 1;
                      then min* G"{i1}<min* G"{j1}&G"{i1}=PG"{i}&G"{j1}=PG"{j}
                        by A9,A10,A24,A28,A29;
                      hence thesis;
                    end;
                    suppose A35:i1 = F.m & j1<>F.m;
                      then (j1 in rng FD or j1 in {F.m})&
                        not j1 in {F.m} & F.m in {F.m} & m in domF
                        by HENMODEL:def 1,A29,A30,XBOOLE_0:def 2,TARSKI:def 1;
                      then A36:m in F"{F.m} & F"{F.m}=PG"{i} & G"{j1}=PG"{j}& 
                      PG"{i} is Subset of NAT&G"{j1} c= dom G&dFD=dom F\F"{F.m}
                      by A35,A8,A28,A29,A23,A24,RELAT_1:167,
                        XBOOLE_1:1,FUNCT_1:def 13;
                      consider x such that
                        A37:x in dom G2 & G2.x=j1 by A29,FUNCT_1:def 5;
                      G2.x in {j1} by A37,TARSKI:def 1;  
                      then PG"{j} is non empty Subset of NAT
                        by XBOOLE_1:1,A37,FUNCT_1:def 13,A29;
                      then min* PG"{j} in PG"{j} by HENMODEL:def 1;
                      then min* PG"{j} in dom F & not min* PG"{j} in F"{F.m} &
                        m in F"{F.m} by A36,XBOOLE_0:def 4;
                      then m<=min* PG"{j} & m<>min* PG"{j} by HENMODEL:def 1;
                      then m<min* PG"{j} & min* PG"{i} <= m
                        by A36,HENMODEL:def 1,REAL_1:def 5;
                      hence thesis by AXIOMS:22;
                    end;
                    suppose A35:i1 < F.m & j1=F.m;
                      (i1 in rng FD or i1 in {F.m}) & not i1 in {F.m} &
                      card rF <>0 & Orde is onto & rng FD c= rF
                        by A28,A30,A35,XBOOLE_0:def 2,TARSKI:def 1,A11,
                        FUNCT_2:def 4,Th33,RELAT_1:99;
                      then i1 in rF & card rF >0  & card rF=rng Orde
                        by NAT_1:18,FUNCT_2:def 3;
                      then Orde.i1+1 > 0 & Orde.i1+1 in rng Orde &
                        Orde.i1+1 is Nat & 0 in rng Orde by A34,A35,EULER_1:1;
                      then Orde'.(Orde.i1+1)>Orde'.0 & P1.j1=Orde'.0 &
                        Orde'.(Orde.i1+1)=i by A13,A32,A33,A35; 
                      hence thesis by A27,A33',TARSKI:def 1;
                    end;
                    suppose i1 = F.m & j1=F.m;
                      hence thesis by A27,A33;
                    end;
                    suppose A35:i1 > F.m & j1>F.m;
                      P1.i1=i1 & P1.j1=j1 & (i1 in rng FD or i1 in {F.m}) &
                        not i1 in {F.m} & (j1 in rng FD or j1 in {F.m}) &
                        not j1 in {F.m}
                        by A30,A13,A35,A28,A29,TARSKI:def 1,XBOOLE_0:def 2;
                      then min* G"{i1}< min* G"{j1}&G"{i1}=PG"{i}&G"{j1}=PG"{j}
                        by A10,A24,A28,A29,A9,A33,A27;
                      hence thesis;
                    end;
                    suppose A35:i1 > F.m & j1=F.m;
                      A36:P1.i1=i1 & P1.j1=Orde".0 by A13,A35,A28,A29;
                      Orde is one-to-one onto & card rF is non empty
                        by A11,FUNCT_2:def 4,Th33;
                      then rng (Orde")=dom Orde & dom (Orde")=rng Orde &
                        rng Orde=card rF & dom Orde=rF & i1 in rF
                        by A28,FUNCT_1:55,FUNCT_2:def 1,def 3;
                      then consider x such that
                        A38:x in dom Orde' & Orde'.x=i1 by FUNCT_1:def 5;
                      x in card rF & card rF is Subset of NAT by A38,Th8;
                      then reconsider x as Nat;
                      card rF <>0 & Orde is onto by A11,FUNCT_2:def 4,Th33;
                     then card rF>0&card rF=rng Orde by NAT_1:18,FUNCT_2:def 3;
                      then 0 in rng Orde & x in rng Orde & Orde'.x < Orde'.0
                        by A27,A33,A36,A38,EULER_1:1;
                      then x <=0 & Orde'.x <> Orde'.0 by A32;
                      then x<0 by REAL_1:def 5;
                      hence thesis by NAT_1:18;                
                    end;
                    suppose i1 < F.m & j1>F.m;
                      then i1<j1 &(i1 in rng FD or i1 in {F.m})&
                        not i1 in {F.m} &(j1 in rng FD or j1 in {F.m})
                        & not j1 in {F.m}
                      by A28,A29,A30,XBOOLE_0:def 2,TARSKI:def 1,AXIOMS:22;
                      then min* G"{i1}<min* G"{j1}&G"{i1}=PG"{i}&G"{j1}=PG"{j}
                        by A9,A10,A24,A28,A29;
                      hence thesis;
                    end;
                    suppose A35:i1 > F.m & j1<F.m;  
                      Orde.j1+1 in rng Orde &  Orde.j1+1 is Nat &
                      Orde.j1+1 <= Orde.(F.m)&Orde.(F.m) is Nat & dom Orde=rF
                         by A29,A34,A35;
                      then Orde.j1+1 in rng Orde &  Orde.j1+1 is Nat &
                        (Orde.j1+1 < Orde.(F.m) or Orde.j1+1 = Orde.(F.m))&
                        Orde.(F.m) is Nat & Orde.(F.m) in rng Orde &
                        F.m in dom Orde & Orde is one-to-one & j1 in rF
                        by A6,A29,REAL_1:def 5,A11,FUNCT_2:def 4,FUNCT_1:def 5;
                      then (Orde'.(Orde.j1+1)<Orde'.(Orde.(F.m)) or
                        Orde'.(Orde.j1+1)=Orde'.(Orde.(F.m))) &
                        Orde'.(Orde.(F.m))=F.m & Orde'.(Orde.j1+1)=P1.j1
                        by A13,A32,A35,FUNCT_1:56;
                      then P1.j1<=F.m & P1.i1=i1 by A13,A28,A35;
                      hence thesis by A27,A33,A35,AXIOMS:22;
                    end;
                  end;
                  hence thesis;
                 end;    
                 hence thesis by A25,A26;
                 end;
              end;
           hence thesis;            
         end;
       end;
       hence thesis;
     end;  
  A3:for F be Function st rng F is finite holds P[F] from Sch9(A1,A2);
  let F be Function of Ne,Ke such that A4:rng F is finite; 
  consider P be Permutation of rng F,G be Function of Ne,Ke such that
    A5:F=P*G & rng F=rng G &
    for i,j st i in rng G& j in rng G & i<j holds
                                         min* G"{i}<min* G"{j} by A3,A4;
  G is "increasing by A5,Def5;
  hence thesis by A5;
end;

:: uniqueness of expansion 

theorem Th76:
  for F be Function of Ne,Ke st rng F is finite
    for I1,I2 be Function of Ne,Me,P1,P2 be Function st
      P1 is one-to-one & P2 is one-to-one &
      rng I1=rng I2 & rng I1=dom P1 & dom P1=dom P2 &
      F=P1*I1 & F=P2*I2 &
      I1 is "increasing & I2 is "increasing
    holds
      P1=P2 & I1=I2
proof
  defpred P[set] means
    for Ne,Ke,Me be Subset of NAT
      for F be Function of Ne,Ke st F=$1
        for I1,I2 be Function of Ne,Me, P1,P2 be Function st
          P1 is one-to-one & P2 is one-to-one &
          rng I1=rng I2 & rng I1=dom P1 & dom P1=dom P2 &
          F=P1*I1 & F=P2*I2 & 
          I1 is "increasing & I2 is "increasing
      holds
        P1=P2 & I1=I2;
  A1:P[{}]
  proof
    let Ne,Ke,Me be Subset of NAT;
    let F be Function of Ne,Ke such that
      A2:F={};
    let I1,I2 be Function of Ne,Me, P1,P2 be Function such that
      P1 is one-to-one & P2 is one-to-one and
      A4:rng I1=rng I2 & rng I1=dom P1 & dom P1=dom P2 and
      A5:F=P1*I1 & F=P2*I2 and 
      I1 is "increasing & I2 is "increasing;
    rng P1={} & rng P2={} & dom I1={} & dom I2={} by A2,A4,A5,RELAT_1:46,47,60;
    then I1={} & I2={} & P1={} & P2={} by RELAT_1:64;
    hence thesis;
  end;
  defpred Q[set,Function] means $1=$2.(min* dom $2);
  A2:for F be Function st
     for x st x in rng F & Q[x,F] holds P[F|(dom F\F"{x})] holds P[F]
  proof
    let F' be Function such that
    A3:for x st x in rng F' & Q[x,F'] holds P[F'|(dom F'\F'"{x})];
    now per cases;
      suppose F'={};
        hence thesis by A1;
      end;
      suppose A4:F'<>{};
        let Ne,Ke,Me be Subset of NAT;
        let F be Function of Ne,Ke such that A5:F=F';
        let I1,I2 be Function of Ne,Me, P1,P2 be Function such that
        A6:P1 is one-to-one & P2 is one-to-one and
        A7:rng I1=rng I2 & rng I1=dom P1 & dom P1=dom P2 and
        A8:F=P1*I1 & F=P2*I2 and
        A9:I1 is "increasing & I2 is "increasing;
        rng F c= Ke & rng F<>{} by A4,A5,RELAT_1:64;
        then A10:Ke is non empty & rng F<>{} by XBOOLE_1:3;  
        then reconsider domF=dom F as non empty Subset of NAT
          by FUNCT_2:def 1,RELAT_1:65;  
        set m=min* dom F;
        reconsider D=dom F\F"{F.m} as Subset of NAT by XBOOLE_1:1;
        A11:for I be Function of Ne,Me,P be Function st
          P is one-to-one & rng I=dom P & F=P*I & I is "increasing
        holds
          dom (I|D)=D & rng (I|D)=rng I\{I.m} &
          dom (P|(rng I\{I.m}))=rng I\{I.m} &
          F|D=(P|(rng I\{I.m}))*(I|D) &
          P|(rng I\{I.m}) is one-to-one &
          for M be Subset of NAT st M=rng I\{I.m}
            for I1 be Function of D,M st I1=I|D holds I1 is "increasing
         proof
           let I be Function of Ne,Me,P be Function such that
           A12:P is one-to-one and
           A13:rng I=dom P and
           A14:F=P*I and
           A15:I is "increasing;
           m in domF by HENMODEL:def 1;
           then F.m in rng F by FUNCT_1:def 5;
           then consider x such that
             A16:x in dom P & x in rng I & P"{F.m}={x} & I"{x}=F"{F.m}
               by A12,A14,Th73;
           m in domF & F.m in {F.m} by HENMODEL:def 1,TARSKI:def 1;
           then m in I"{x} by A16,FUNCT_1:def 13;
           then I.m in {x} by FUNCT_1:def 13;
           then A17:I.m=x & dom F=dom I by A13,A14,RELAT_1:46,TARSKI:def 1; 
           then D c= dom I & rng I\{I.m} c= rng I by XBOOLE_1:36;
           then A18:dom (I|D)=dom I/\D & dom I/\D=D &
           dom (P|(rng I\{I.m}))=dom P/\(rng I\{I.m}) &
           dom P/\(rng I\{I.m})=rng I\{I.m} &
           rng (I|D)=rng I\{I.m} by A13,A16,A17,FUNCT_1:68,XBOOLE_1:28,Th66;
           set rI=rng I\{I.m};
           A19:F|D=(P|rI)*(I|D)
           proof
             D c= dom F by XBOOLE_1:36;
             then A20:dom F/\D=D by XBOOLE_1:28;
             then dom (F|D)=D by FUNCT_1:68;
             then A21:for x holds x in dom (F|D) iff
                  x in dom (I|D) & (I|D).x in dom (P|rI) by A18,FUNCT_1:def 5;
             for x st x in dom (F|D) holds (F|D).x = (P|rI).((I|D).x)
             proof
               let x such that A22:x in dom (F|D);
               x in dom F/\D by A22,FUNCT_1:68;
               then x in dom (I|D) & (I|D).x in dom (P|rI) & x in dom F
                 by A18,A20,FUNCT_1:def 5,XBOOLE_0:def 3;
               then I.x=(I|D).x & (P|rI).((I|D).x)=P.((I|D).x) & (F|D).x=F.x &
                 F.x=P.(I.x) by A14,A22,FUNCT_1:68,22;
               hence thesis;
             end;
             hence thesis by A21,FUNCT_1:20;
           end;
           for M be Subset of NAT st M=rng I\{I.m}
             for I1 be Function of D,M st I1=I|D holds I1 is "increasing
           proof
             let M be Subset of NAT such that M=rng I\{I.m};
             let I1 be Function of D,M such that A20:I1=I|D;
             let l,n such that A21:l in rng I1 & n in rng I1 & l < n;
             rng I1=rng I\{I.m} by A20,Th66,A16,A17;
             then l in rng I & l<>I.m & n in rng I & n<>I.m & D=dom I\I"{I.m}
               by A16,A17,A21,ZFMISC_1:64;
             then min* I"{l} < min* I"{n} & I1"{l}=I"{l} & I1"{n}=I"{n}
               by A15,A21,Def5,A20,Th66;
             hence thesis;
           end;
           hence thesis by A12,A18,A19,FUNCT_1:84;
         end; 
         dom I1=dom F & dom I2=dom F & dom F=Ne
           by A7,A8,A10,RELAT_1:46,FUNCT_2:def 1;
         then A12:min* rng I1=I1.m & min* rng I2=I2.m by A9,Th74;
         rng I1 is Subset of NAT & rng I1\{I1.m} c= rng I1 by XBOOLE_1:1,36;
         then reconsider I=rng I1\{I1.m} as Subset of NAT by XBOOLE_1:1;
         dom (I1|D)=D & dom (I2|D)=D & rng (I1|D)=I & rng (I2|D)=I
           by A6,A7,A8,A9,A11,A12; 
         then reconsider I1D=I1|D,I2D=I2|D as Function of D,I by FUNCT_2:3;
         reconsider rFD=rng (F|D) as Subset of NAT by XBOOLE_1:1;
         D c= dom F by XBOOLE_1:36;
         then dom F/\D=D by XBOOLE_1:28;
         then dom (F|D)=D by FUNCT_1:68;
         then reconsider FD=F|D as Function of D,rFD by FUNCT_2:3; 
         m in domF by HENMODEL:def 1;
         then dom I1D=D & dom I2D=D & rng I1D=I & rng I2D=I & dom (P1|I)=I &
           dom (P2|I)=I & F.m in rng F by FUNCT_1:def 5,A6,A7,A8,A9,A11,A12;  
         then A13:P1|I is one-to-one & P2|I is one-to-one & rng I1D=rng I2D &
           rng I1D=dom (P1|I)&dom (P1|I)=dom (P2|I)&FD=(P1|I)*I1D&FD=(P2|I)*I2D
           & I1D is "increasing & I2D is "increasing & P[FD]
           by A3,A5,A6,A7,A8,A9,A11,A12;
        A14:P1=P2
        proof
          for x st x in dom P1 holds P1.x = P2.x
          proof
            let x such that A15:x in dom P1;
            m in domF & dom I1=dom F by A7,A8,RELAT_1:46,HENMODEL:def 1;
            then I1.m in rng I1 by FUNCT_1:def 5;
            then A16:dom P1=I\/{I1.m} by Lm4,A7;
            now per cases by A15,A16,XBOOLE_0:def 2;
              suppose A17:x in I;
                I c= dom P1 by A7,XBOOLE_1:36;
                then dom P1/\I=I & dom P2/\I=I by A7,XBOOLE_1:28;
                then x in dom (P1|I) & x in dom (P2|I)by A17,FUNCT_1:68;
                then (P1|I).x=P1.x & (P2|I).x=P2.x by FUNCT_1:68;
                hence thesis by A13;
              end;
              suppose x in {I1.m};
                then x=I1.m & m in domF by TARSKI:def 1,HENMODEL:def 1;
                then F.m=P1.x & F.m=P2.x by A7,A8,A12,FUNCT_1:22;
                hence thesis;
              end;
            end;
            hence thesis;
          end;
          hence thesis by A7,FUNCT_1:9;
        end;
        dom I1=dom F & m in domF
           by A7,A8,RELAT_1:46,HENMODEL:def 1;
        then I1.m in rng I1 by FUNCT_1:def 5;
        then I1 is Function of dom I1,rng I1 &
        I2 is Function of dom I2,rng I2& rng I1 is non empty by FUNCT_2:3;
        then P1"*P1 = id dom P1 & I1=(id rng I1)*I1 &
        P2"*P2 = id dom P2 & I2=(id rng I2)*I2 by A6,FUNCT_1:61,FUNCT_2:23;
        then I1=P1"*(P1*I1) & I2=P2"*(P2*I2) by A7,RELAT_1:55;
        hence P1=P2 & I1=I2 by A8,A14;
      end;
    end;
    hence thesis;
  end;
  for F be Function st rng F is finite holds P[F] from Sch9(A1,A2);
  hence thesis;
end;  

theorem
  for F be Function of Ne,Ke st rng F is finite
    for I1,I2 be Function of Ne,Ke, P1,P2 be Permutation of rng F st
      F=P1*I1 & F=P2*I2 & rng F=rng I1 & rng F=rng I2 &
      I1 is "increasing & I2 is "increasing
    holds
      P1=P2 & I1=I2
proof
  let F be Function of Ne,Ke such that A1:rng F is finite;
  let I1,I2 be Function of Ne,Ke, P1,P2 be Permutation of rng F such that
  A2:F=P1*I1 & F=P2*I2 and
  A3:rng F=rng I1 & rng F=rng I2 and
  A4:I1 is "increasing & I2 is "increasing;
  rng F ={} implies rng F={};
  then dom P1=rng F & dom P2=rng F by FUNCT_2:def 1;
  hence thesis by A1,A2,A3,A4,Th76;
end;

