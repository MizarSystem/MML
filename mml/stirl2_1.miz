 vocabularies FUNCT_1, BOOLE, FINSEQ_1, FINSET_1, RELAT_1, ORDINAL2, CARD_1,
      TARSKI, ALGSEQ_1, ARYTM_1, AFINSQ_1, FUNCT_2, HENMODEL, INT_1, STIRL2_1,
      SGRAPH1, SQUARE_1, BINOP_1, RLVECT_1, MONOID_0, SETWISEO, FINSOP_1,
      ARYTM_3, GROUP_1, NEWTON, QC_LANG1, FUNCOP_1;
 notations TARSKI, XBOOLE_0, ZFMISC_1, SUBSET_1, NUMBERS, XREAL_0, RELAT_1,
      FUNCT_1, RELSET_1, ORDINAL1, ORDINAL2, REAL_1, NAT_1, PARTFUN1, FINSET_1,
      CARD_1, AFINSQ_1, XCMPLX_0, FUNCT_2, CQC_SIM1, INT_1, WELLORD2, ENUMSET1,
      SQUARE_1, HENMODEL, BINOP_1, BINOP_2, CARD_2, NEWTON, PREPOWER, SETWISEO,
      FUNCOP_1;
 constructors REAL_1, NAT_1, WELLORD2, FUNCT_7, XREAL_0, MEMBERED, AFINSQ_1,
      RELAT_1, CQC_SIM1, PARTFUN1, FUNCT_2, FUNCT_1, CARD_1, ORDINAL2,
      XBOOLE_0, ZFMISC_1, ORDINAL1, TARSKI, SQUARE_1, RELAT_2, HENMODEL,
      BINOP_1, SETWISEO, FINSOP_1, BINOP_2, CARD_2, PREPOWER, POWER, FUNCOP_1;
 registrations FUNCT_1, RELAT_1, RELSET_1, CARD_1, SUBSET_1, NAT_1, XREAL_0,
      MEMBERED, NUMBERS, ORDINAL2, AFINSQ_1, BINOP_2, XBOOLE_0, FINSET_1,
      INT_1, CHAIN_1, FUNCT_2, FUNCOP_1;
 requirements REAL, NUMERALS, SUBSET, BOOLE, ARITHM;
 definitions AFINSQ_1, TARSKI, FINSET_1, XBOOLE_0, FUNCT_1, SUBSET_1, ZFMISC_1;
 theorems AFINSQ_1, TARSKI, FINSEQ_1, FUNCT_1, XBOOLE_0, ZFMISC_1, RELAT_1,
      XBOOLE_1, NAT_1, FUNCT_2, RELSET_1, FINSET_1, CQC_THE1, AXIOMS, XCMPLX_1,
      REAL_1, CARD_2, INT_1, ORDINAL1, WELLORD2, EULER_1, FRAENKEL, SUBSET_1,
      SQUARE_1, CQC_SIM1, HENMODEL, ENUMSET1, CARD_1, BINOP_1, CARD_5, BINOP_2,
      NEWTON, WSIERP_1, SETWISEO, TMAP_1, FUNCOP_1;
 schemes FUNCT_1, FUNCT_2, NAT_1, INT_1, INT_2;

begin
 
 reserve k, l, m, n, i, j for Nat,
         K, N for non empty Subset of NAT,
         Ke, Ne, Me for Subset of NAT,
         X,Y for set;
         
definition let k;
  redefine func {k} -> Subset of NAT;
  coherence by SUBSET_1:55;
  let l;
  redefine func {k,l} -> Subset of NAT;
  coherence by SUBSET_1:56;
  let m;
  redefine func {k,l,m} -> non empty Subset of NAT;
  coherence by SUBSET_1:57,ENUMSET1:def 1;
end;

theorem Th1:
  min N = min* N
proof
  min* N in N & for n st n in N holds min* N <= n by HENMODEL:def 1;
  hence thesis by CQC_SIM1:def 8;
end;

theorem Th2:
  min(min K,min N) = min(K\/N)
proof
     set m=min(min N,min K);
   A1:m<=min (N\/K) 
     proof
        m=min N or m=min K by SQUARE_1:38;
        then m in N or m in K by CQC_SIM1:def 8;
        then A2:m in N\/K by XBOOLE_0:def 2;
        for k st k in N\/K holds m<=k
          proof
            let k such that A3:k in N\/K;
            k in N or k in K by A3,XBOOLE_0:def 2;
            then (min N<=k or min K <=k) & m<=min N &m<=min K
              by CQC_SIM1:def 8,SQUARE_1:35;
            hence thesis by AXIOMS:22;
          end;
        hence thesis by A2,CQC_SIM1:def 8;
      end;   
   min (N\/K) <= min(min N,min K)
     proof
       N c= N\/K & K c= N\/K by XBOOLE_1:7;
       then min (N\/K)<=min N & min (N\/K)<=min K by CQC_SIM1:19;
       hence thesis by SQUARE_1:39;
     end;
  hence thesis by A1,AXIOMS:21;
end;

theorem
  min(min* Ke,min* Ne) <= min* (Ke\/Ne)
proof
  now per cases;
    suppose Ke is empty or Ne is empty;
      then (min* Ke=0 & min* Ne >=0) or (min* Ne=0 & min*Ke>=0)
        by HENMODEL:def 1, NAT_1:18;
      then min(min* Ke,min* Ne)=0 by SQUARE_1:def 1;
      hence thesis by NAT_1:18;
    end;
    suppose Ke is non empty & Ne is non empty;
      then reconsider K=Ke,N=Ne as non empty Subset of NAT;
      min K=min* Ke & min N=min* Ne & min(K\/N)=min*(Ke\/Ne) by Th1;
      hence thesis by Th2;
    end;
  end;
  hence thesis;
end;

theorem
  not min* Ne in Ne/\Ke implies min* Ne = min* (Ne\Ke)
proof
  assume A1:not min* Ne in Ne/\Ke;
  now per cases;
    suppose Ne is empty; 
      hence thesis; 
    end;
    suppose Ne is non empty;
      then A2:min* Ne in Ne by HENMODEL:def 1;
      then not min* Ne in Ke by A1,XBOOLE_0:def 3;
      then A3:min* Ne in Ne\Ke by A2,XBOOLE_0:def 4;
      then min* (Ne\Ke) in Ne\Ke & Ne\Ke c=Ne by XBOOLE_1:36,HENMODEL:def 1;
      then min* (Ne\Ke)<=min* Ne & min* Ne<=min* (Ne\Ke) by A3,HENMODEL:def 1;
      hence thesis by AXIOMS:21;   
    end;
  end;
  hence thesis;   
end;

theorem Th5:
  min* {n} = n & min {n} = n
proof
  min* {n} in {n} & min*{n}=min {n} by Th1,HENMODEL:def 1;
  hence thesis by TARSKI:def 1;
end;

theorem Th6:
  min* {n,k} = min(n,k) & min {n,k} = min(n,k)
proof
  {n,k}={n}\/{k} by ENUMSET1:41;
  then min {n,k} = min(min {n},min {k})
     & min {n,k} = min* {n,k} & min {n} = n by Th1,Th2,Th5;
  hence thesis by Th5;
end;

theorem
  min* {n,k,l} = min(n,min(k,l))
proof
  {n,k,l}={n}\/{k,l} by ENUMSET1:42;
  then min{n,k,l} = min(min {n},min {k,l}) & min {k,l}=min(k,l)
     & min {n,k,l}=min* {n,k,l} by Th1,Th2,Th6;
  hence thesis by Th5;
end;

theorem Th8:
  n is Subset of NAT
proof
  now let x be set such that A1:x in {l: l<n};
    ex l st x=l & l<n by A1;
    hence x in NAT;
  end;  
  then {l:l<n} c= NAT by TARSKI:def 3;
  hence thesis by AXIOMS:30;
end;

registration
  let n;
  cluster -> natural Element of n;
  coherence
proof
  per cases by NAT_1:18;
    suppose A1:n=0;
      let k be Element of n;
      thus thesis by A1,SUBSET_1:def 2;
    end;
    suppose A1:n>0;
      let k be Element of n;
      k in n & n is Subset of NAT by A1,Th8;
      then k is Nat;
      hence thesis;
    end;    
end;
end;
  
theorem
  N c= n implies n-1 is Nat
proof
  assume A1:N c= n;
  min* N in N by HENMODEL:def 1;
  then min*N in n by A1;
  then min* N in {l:l<n} by AXIOMS:30;
  then ex l st min* N = l & l<n;
  hence thesis by NAT_1:60;
end;

theorem Th10:
  k in n implies k<=n-1 & n-1 is Nat
proof
  assume k in n;
  then k in {l:l<n} by AXIOMS:30;
  then ex l st k=l & l<n;
  then k<(n-1)+1 & n-1 is Nat by NAT_1:60;
  hence thesis by NAT_1:38;
end;

theorem
  min* n = 0
proof
  now per cases by NAT_1:18;
    suppose 0=n;
      hence thesis by HENMODEL:def 1;
    end;
    suppose 0<n;
      then 0 in {l:l<n};
      then 0 in n & n is Subset of NAT by Th8,AXIOMS:30;
      then min* n <=0 by HENMODEL:def 1;            
      hence thesis by NAT_1:18;
    end;
  end;
  hence thesis;
end;    
        
theorem Th12:
  N c= n implies min* N <= n-1
proof
  assume A1:N c= n;
  min* N in N by HENMODEL:def 1;
  hence thesis by A1,Th10;
end;
  
theorem
  N c= n & N <> {n-1} implies min* N < n-1
proof
  assume that A1:N c= n and A2:N<>{n-1} and A3:min* N >=n-1;
  now let k be set such that A4:k in N;
    reconsider k'=k as Nat by A4;
    min* N <=k' & k' in n by A1,A4,HENMODEL:def 1;
    then n-1 <=k' & k' <= n-1 by A3,Th10,AXIOMS:22;
    then n-1 = k by AXIOMS:21;
    hence k in {n-1} by TARSKI:def 1;
  end;
  then N c= {n-1} by TARSKI:def 3;
  hence thesis by A2,ZFMISC_1:39;  
end;

theorem Th14:
  Ne c= n & n>0 implies min* Ne <= n-1
proof
  assume that A1:Ne c= n and A2:n>0;
  now per cases;
    suppose Ne is non empty;
      hence thesis by A1,Th12;
    end;
    suppose Ne is empty;
      then min* Ne =0 & n-1 is Nat by A2,HENMODEL:def 1,NAT_1:60;
      hence thesis by NAT_1:18;
    end;
  end;
  hence thesis;
end;

reserve f,g for Function of n,k;  
      
definition let n,X; let f be Function of n,X; let x be set;
  redefine func f"x -> Subset of NAT;
  coherence
  proof
    now let y be set such that A1:y in f"x;
      y in dom f & f.y in x & n is Subset of NAT by A1,Th8,FUNCT_1:def 13;
      hence y in NAT by TARSKI:def 3;      
    end;
    hence thesis by TARSKI:def 3;
end;
end;

definition let X,k; let f be Function of X,k; let x be set;
  redefine func f.x -> Element of k;
  coherence
  proof
    per cases;
      suppose x in dom f;
        then f.x in rng f & rng f c= k by FUNCT_1:12;
        hence f.x is Element of k;
      end;
      suppose not x in dom f;
        then f.x=0 & (k=0 or k>0) by FUNCT_1:def 4,NAT_1:18;
        hence thesis by SUBSET_1:def 2,EULER_1:1;
      end;
  end;
end;

registration
  let X,Ne;
  let f be Function of X,Ne,x be set;
  cluster f.x -> natural;
coherence
proof
  per cases;
  suppose x in dom f;
    then f.x in rng f & rng f is Subset of NAT by FUNCT_1:def 5,XBOOLE_1:1;
    then f.x is Nat;
    hence thesis;
  end;
  suppose not x in dom f;
  hence thesis by FUNCT_1:def 4;
end;
end;
end;
  
definition
  let n,k;let f be Function of n,k;
  attr f is "increasing means :Def1:
   (n = 0 iff k = 0) &
   for l,m st l in rng f & m in rng f & l < m holds min* f"{l} < min* f"{m};
end;
  
theorem Th15:
  n=0 & k=0 implies f is onto "increasing
proof
  assume A1:n=0 & k=0;
  then rng f = {} & for l,m st l in k & m in k & l < m holds
                                 min* f"{l} < min* f"{m} by XBOOLE_1:3;
  hence thesis by A1,Def1,FUNCT_2:def 3;
end;

theorem Th16:
  n>0 implies min* f"{m} <= n-1
proof
  assume A1:n>0;
  f"{m} c= n
  proof
    let x be set such that A6:x in f"{m};
    x in dom f by A6,FUNCT_1:def 13;
    hence thesis; 
  end;
  hence thesis by A1,Th14;
end;
  
theorem Th17:
  f is onto implies n>=k
proof
  assume A1:f is onto;
  now per cases;
    suppose k=0;      
      hence thesis by NAT_1:18;
    end;
    suppose k<>0;
      then dom f =n & rng f = k by A1,FUNCT_2:def 1,def 3;
      then Card k <=` Card n by CARD_1:28; 
      then card k <= card n & card n = n by CARD_2:57,FINSEQ_1:78;
      hence thesis by FINSEQ_1:78;
    end;
  end;
  hence thesis;
end;

theorem Th18:
  f is onto "increasing implies
    for m st m < k holds m <= min* f"{m}
proof
  assume that A1:f is onto "increasing;  
  now per cases by NAT_1:18;
    suppose k=0;
      hence thesis by NAT_1:18;
    end;
    suppose k>0;
      defpred M[Nat] means
        $1<k implies $1<=min* f"{$1};
      A2:M[0] by EULER_1:1;
      A3:for m st M[m] holds M[m + 1]
        proof
          let m such that A4:M[m];
          assume A5:m+1<k;
          m<m+1 by NAT_1:59;
          then m < k & k=rng f by A1,A5,AXIOMS:22,FUNCT_2:def 3;
          then m <= min* f"{m} & m in rng f & m+1 in rng f & m < m+1
            by A4,A5,EULER_1:1,NAT_1:59;
          then min* f"{m}< min* f"{m+1} & m < k by A1,A5,Def1,AXIOMS:22;
          then m < min* f"{m+1} by A4,AXIOMS:22;
          hence m+1 <=min* f"{m+1} by NAT_1:38;            
        end;       
      for m holds M[m] from NAT_1:sch 1(A2,A3);                  
      hence thesis;
    end;
  end;
  hence thesis;
end;  

theorem Th19:
  f is onto "increasing implies
    for m st m < k holds min* f"{m} <= n-k+m
proof
  assume that A1:f is onto "increasing;
  now per cases by NAT_1:18;
    suppose k=0;
      hence thesis by NAT_1:18;
    end;
    suppose A2:k>0;
      defpred I[Integer] means
        0<=$1 implies min* f"{$1} <= n-k+$1;         
      reconsider k1=k-1 as Nat by A2,NAT_1:60;        
      A3:0<=k1 & k1<k1+1 by NAT_1:18,38;
      A4:I[k1]
        proof
          assume 0<= k1;
          k=0 iff n=0 by A1,Def1;
          then n>0 by A2,NAT_1:18;
          then min* f"{k1}<=n-1 by Th16;
          hence thesis;         
        end;        
      A5:for m be Integer st m <= k1 holds I[m] implies I[m-1]
        proof
          let m be Integer such that A6:m <= k1;
          assume A7:I[m];
          n>=k by A1,Th17;
          then reconsider nk=n-k as Nat by NAT_1:61;
          assume that A8:0<=m-1;
          reconsider m1=m-1 as Nat by A8,INT_1:16;          
          A9:m1<m1+1 & m < k by A3,A6,NAT_1:59,AXIOMS:22;
          then m1<k by AXIOMS:22;
          then m1<k & m1 in k & m1+1 in k & k = rng f
            by A1,A9,EULER_1:1,FUNCT_2:def 3;
          then min* f"{m1}<min* f"{m1+1} & min* f"{m1+1}<=n-k+m
            by A1,A7,A8,A9,Def1;
          then min* f"{m1} < (nk+m1)+1 by AXIOMS:22;
          hence thesis by NAT_1:38;
        end;
      A6:for m be Integer st m <= k1 holds I[m] from INT_1:sch 3(A4,A5);
      now let m such that A7:m < k;
        m<k1+1 by A7;
        then 0<=m & m<=k1 by NAT_1:18,38;
        hence min* f"{m} <= n-k+m by A6;
      end; 
      hence thesis;
    end;
  end;
  hence thesis;
end;

theorem Th20:
  f is onto "increasing & n = k implies f = id n
proof
  assume that A1:f is onto "increasing and A2:n=k;
   now per cases by NAT_1:18;
    suppose A3:n=0;
      then dom f=0 & for x be set st x in 0 holds f.x=x by FUNCT_2:def 1;
      hence thesis by A3,FUNCT_1:34;
    end;
    suppose A3:n>0;
      A4:now let m' be set such that A5:m' in n;
          n is Subset of NAT by Th8; 
          then reconsider m=m' as Nat by A5;        
          m in rng f by A1,A2,A5,FUNCT_2:def 3;
          then m in {m} & ex x be set st x in dom f & f.x =m
            by FUNCT_1:def 5,TARSKI:def 1;
          then reconsider F=f"{m} as non empty Subset of NAT by FUNCT_1:def 13;
          0<=m & m < k & n-k+m =m by A2,A5,EULER_1:1;
          then m <= min* f"{m} & min* f"{m}<= m by A1,Th18,Th19;
          then min* F=m & min* F in F by HENMODEL:def 1,AXIOMS:21;
          then f.m in {m} by FUNCT_1:def 13;
          hence f.m'=m' by TARSKI:def 1;
        end;
      dom f = n by A2,A3,FUNCT_2:def 1;   
      hence thesis by A4,FUNCT_1:34;
    end;
  end;
  hence thesis;
end;  

theorem
  f = id n & n > 0 implies f is "increasing
proof
  assume that A1:f = id n and A2:n>0;  
  rng f=n by A1,RELAT_1:71;
  then A3:n c= k & ex x be set st x in n by A2,XBOOLE_0:def 1;
  now let l,m such that A4:l in rng f & m in rng f & l<m;
    consider l' be set such that
      A5:l' in dom f & f.l'=l by A4,FUNCT_1:def 5;
    consider m' be set such that
    A6:m' in dom f & f.m'=m by A4,FUNCT_1:def 5;
    l = l'& l in {l} & m = m'& m in {m} & f is one-to-one
      by A1,A5,A6,TARSKI:def 1,FUNCT_1:35,52;
    then l in f"{l} & m in f"{m} &
      (ex x be set st f"{l} = {x}) & ex x be set st f"{m} = {x}
       by A4,A5,A6,FUNCT_1:def 13,144;
    then f"{l}={l} & f"{m}={m} by TARSKI:def 1;
    then min* f"{l} = l & min* f"{m}=m by Th5;
    hence min* f"{l} < min* f"{m} by A4;
   end;
   hence thesis by A3,Def1;
 end;
 
theorem
  (n=0 iff k=0) implies
    ex f be Function of n,k st f is "increasing
proof
  assume A1:n=0 iff k=0;
  now per cases by NAT_1:18;
    suppose A2:n=0;set f={};
      rng f= k & dom f = n by A1,A2;
      then reconsider f as Function of n,k by FUNCT_2:3;
      f is "increasing by A1,A2,Th15;
      hence thesis;
    end;
    suppose A2:n>0;
      then consider f be Function such that
        A3:dom f=n & rng f={0} by FUNCT_1:15;
      reconsider f as Function of n,{0} by A3,FUNCT_2:3;  
      A5:now let l,m such that A6:l in rng f & m in rng f & l < m;
          l=0 & m =0 by A6,TARSKI:def 1;
          hence min* f"{l} < min* f"{m} by A6;                    
      end;    
      rng f c= k
      proof
        let x be set such that A6:x in rng f;
        x in {0} & 0<k by A1,A2,A6,NAT_1:18;
        then x=0 & 0 in k by TARSKI:def 1,EULER_1:1;
        hence thesis;
      end;    
      then reconsider f as Function of n,k by FUNCT_2:8;
      for l,m st l in rng f & m in rng f & l < m
      holds min* f"{l} < min* f"{m} by A5;
      then f is "increasing by A1,Def1;
      hence thesis;    
    end;
  end;
  hence thesis;
end;

theorem Th23:
  (n=0 iff k=0) & n>=k implies
    ex f be Function of n,k st f is onto "increasing
proof
  assume A1:(n=0 iff k=0) & n>=k;
  now per cases by NAT_1:18;
    suppose A2:n=0;set f={};
      rng f= k & dom f = n by A1,A2;
      then reconsider f as Function of n,k by FUNCT_2:3;
      f is onto "increasing by A1,A2,Th15;
      hence thesis;
    end;
    suppose A2:n>0;
      then k>0 by A1,NAT_1:18;
      then reconsider k1=k-1 as Nat by NAT_1:60;
      reconsider k,n as non empty Nat by A1,A2;
      defpred F[Element of n,Element of k] means $2=min($1,k1);
      A3:for x be Element of n ex y be Element of k st F[x,y]
        proof
          let x be Element of n;
          min(x,k1)<=k1 & k1<k1+1 by SQUARE_1:35,NAT_1:38;
          then min(x,k1)<k & min(x,k1) is natural by AXIOMS:22,SQUARE_1:38;
          then min(x,k1) in k by EULER_1:1;
          hence ex y be Element of k st F[x,y];
        end;  
      consider f be Function of n,k such that
        A4:for m be Element of n holds F[m,f.m] from FUNCT_2:sch 3(A3);
      A5:f is onto
        proof
          now let m' be set such that A6:m' in k;
            k is Subset of NAT by Th8;
            then reconsider m=m' as Nat by A6;
            m<k1+1 by A6,EULER_1:1;
            then m <n & m<=k1 by A1,AXIOMS:22,NAT_1:38;
            then m in n & min(m,k1)=m by EULER_1:1,SQUARE_1:def 1;
            then f.m=m & m in dom f by A4,FUNCT_2:def 1;
            hence m' in rng f by FUNCT_1:def 5;
          end;
          then k c= rng f & rng f c= k by TARSKI:def 3;
          then k=rng f by XBOOLE_0:def 10;
          hence thesis by FUNCT_2:def 3;
        end;
      A6:for m st m in rng f holds min* f"{m} =m
        proof
          let m such that A7: m in rng f;
          A8:m<k1+1 by A7,EULER_1:1;
          then m <n & m<=k1 by A1,AXIOMS:22,NAT_1:38;
          then m in n & min(m,k1)=m by EULER_1:1,SQUARE_1:def 1;
          then f.m=m & m in dom f by A4,FUNCT_2:def 1;
          then f.m in {m} & m in dom f by TARSKI:def 1;
          then A9:f"{m} is non empty & m in f"{m} by FUNCT_1:def 13;
          A10:m<=k1 by A8,NAT_1:38;
          now per cases by A10,REAL_1:def 5;
            suppose A11:m<k1;
              now let l' be set such that A12:l' in f"{m};
                l' in dom f by A12,FUNCT_1:def 13;
                then l' in n & n is Subset of NAT by Th8;       
                then reconsider l=l' as Nat;
                l in dom f & f.l in {m} by A12,FUNCT_1:def 13;
                then l in n & f.l=m by TARSKI:def 1;
                then min(l,k1)=m by A4;
                then l=m by SQUARE_1:38,A11;
                hence l' in {m} by TARSKI:def 1;
              end;
              then f"{m} c= {m} & min* f"{m} in f"{m}
                by A9,TARSKI:def 3,HENMODEL:def 1;
              hence thesis by TARSKI:def 1;
            end;
            suppose A11:m=k1;
              for l st l in f"{m} holds m <= l
              proof
                let l such that A12:l in f"{m};
                l in dom f & dom f =n & f.l in {m}
                  by A12,FUNCT_1:def 13,FUNCT_2:def 1;
                then f.l=min(l,m) & f.l=m by A4,A11,TARSKI:def 1;
                hence thesis by SQUARE_1:def 1;
              end;
              hence thesis by A9,HENMODEL:def 1;
            end;
          end;
          hence thesis;
        end;
      now let l,m such that A7:l in rng f & m in rng f & l < m;
        min* f"{l}=l & min* f"{m}=m by A6,A7;
        hence min* f"{l} < min* f"{m} by A7;        
      end;   
      then f is "increasing by Def1;
      hence thesis by A5; 
    end;
  end;
  hence thesis;
end;

scheme Sch1{n,k() -> Nat,P[set]}:
  {f where f is Function of n(),k():P[f]} is finite 
proof
  set F= {f where f is Function of n(),k():P[f]};
  now per cases by NAT_1:18;
    suppose A1:k()=0;
      F c= {{}}
      proof
        let x be set such that A2:x in F;        
        consider f be Function of n(),k() such that
          A3:x=f & P[f] by A2;
        rng f={} by A1,XBOOLE_1:3;
        then f ={} by RELAT_1:64;
        hence thesis by A3,TARSKI:def 1;
      end;     
      hence thesis by FINSET_1:13;      
    end;
    suppose A1:k()>0;
      A2:F c= Funcs(n(),k())
      proof
        let x be set such that A3:x in F;
        ex f be Function of n(),k() st x=f & P[f] by A3;
        hence thesis by A1,FUNCT_2:11;     
      end;
      Funcs(n(),k()) is finite by FRAENKEL:16;
      hence thesis by A2,FINSET_1:13;   
    end;
  end;
  hence thesis;
end;

theorem Th24:
  for n,k holds
    {f where f is Function of n,k:f is onto "increasing} is finite
proof
  let n,k;  
  defpred P[Function of n,k] means $1 is onto "increasing;
  {f where f is Function of n,k:P[f]} is finite from Sch1;
  hence thesis;
end;
    
theorem Th25:
  for n,k holds
     Card {f where f is Function of n,k:f is onto "increasing} is Nat
proof
  let n,k;
  set F= {f where f is Function of n,k:f is onto "increasing};
  F is finite by Th24;
  then consider m such that
    A2:F,m are_equipotent by CARD_1:74;
  Card F=Card m & Card m=m by A2,CARD_1:21,66;
  hence thesis; 
end;

