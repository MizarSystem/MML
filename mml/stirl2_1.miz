definition
  let n,k;
  func n block k -> Nat equals:Def2:
    Card {f where f is Function of n,k:f is onto "increasing};
  coherence by Th25;
end;

theorem Th26:
  n block n = one
proof
  set F={f where f is Function of n,n:f is onto "increasing};
  A1:F c= {id n}
  proof
    let x be set such that A2:x in F;
    consider f be Function of n,n such that
      A3:x=f & f is onto "increasing by A2;
    f=id n by A3,Th20;
    hence thesis by A3,TARSKI:def 1;
  end;
  (n=0 iff n=0 ) & n>=n;
  then consider f be Function of n,n such that
    A3:f is onto "increasing by Th23;
  f in F by A3;
  then F={id n} by A1,ZFMISC_1:39;
  then Card F =one by CARD_1:50;
  hence thesis by Def2;
end;

theorem Th27:
  k<>0 implies 0 block k = 0
proof
  assume A1:k<>0;  
  set F={f where f is Function of 0,k:f is onto "increasing};
  F ={}
  proof
    assume F<>{};
    then consider x be set such that
      A2:x in F by XBOOLE_0:def 1;
    consider f be Function of 0,k such that
      A3:x=f & f is onto "increasing by A2;
    thus thesis by A1,A3,Def1;
  end;
  hence thesis by Def2,CARD_1:47;
end;

theorem
  0 block k = one iff k=0 by Th26,Th27;

theorem Th29:
  n<k implies n block k = 0
proof
  assume A1:n<k;
  set F={f where f is Function of n,k:f is onto "increasing};
  F ={}
  proof
    assume F<>{};
    then consider x be set such that
      A2:x in F by XBOOLE_0:def 1;
    consider f be Function of n,k such that
      A3:x=f & f is onto "increasing by A2;
    thus contradiction by A1,A3,Th17;
  end;
  hence thesis by Def2,CARD_1:47;
end;
  
theorem
  n block 0 = one iff n=0
proof
  n block 0 = one implies n=0
  proof
    set F={f where f is Function of n,0:f is onto "increasing};
    assume n block 0=one;
    then Card F =one & Card {{}}= one by Def2,CARD_1:50;
    then consider x be set such that
      A1:F={x} by CARD_1:49;
    x in F by A1,TARSKI:def 1;
    then consider f be Function of n,0 such that
      A2:x=f & f is onto "increasing;
    thus thesis by A2,Def1;
  end;
  hence thesis by Th26;
end;  

theorem Th31:
  n<>0 implies n block 0 = 0
proof
  assume A1:n<>0;
  set F={f where f is Function of n,0:f is onto "increasing};
  F ={}
  proof
    assume F<>{};
    then consider x be set such that
      A2:x in F by XBOOLE_0:def 1;
    consider f be Function of n,0 such that
      A3:x=f & f is onto "increasing by A2;
    thus contradiction by A1,A3,Def1;
  end;
  hence thesis by CARD_1:47,Def2;
end;
  
theorem Th32:
  n<>0 implies n block 1 = one
proof
  assume n<>0;
  then n>0 by NAT_1:18;
  then (n=0 iff 1=0) & n >=1+0 by NAT_1:38;
  then consider f be Function of n,1 such that
    A2:f is onto "increasing by Th23;
  set F={g where g is Function of n,1:g is onto "increasing};
  A3:F c= {f}
  proof
    let x be set such that A4:x in F;
    consider g be Function of n,1 such that
      A5:x=g & g is onto "increasing by A4;  
    f=g by CARD_5:1,FUNCT_2:66;  
    hence x in {f} by A5,TARSKI:def 1;
  end;
  f in F by A2;
  then F={f} by A3,ZFMISC_1:39;
  then Card F =one by CARD_1:50;
  hence thesis by Def2;
end;

Th33:
  Card X = 0 iff X is empty by CARD_2:59,CARD_1:47;

theorem
  (1<=k & k <=n) or k=n iff n block k > 0
proof
  thus (1<=k & k <=n) or k=n implies n block k > 0
  proof
    assume (1<=k & k <=n) or k=n;
    then (k=0 iff n=0) & k <=n;
    then consider f such that
      A1:f is onto "increasing by Th23;
    set F={g where g is Function of n,k:g is onto "increasing};
    f in F by A1;
    then Card F <>0 by Th33;
    then n block k <>0 by Def2;
    hence thesis by NAT_1:18;
  end;
  thus n block k > 0 implies (1<=k & k <=n) or k=n
  proof
    assume A1:n block k >0;
    assume not ((1<=k & k <=n) or k=n);
    then (1+0>k or k>n) & k<>n;
    then (0>=k & k>=0 or k>n) & k<>n by NAT_1:18,38;
    then (k=0 & n<>k) or k>n;
    hence contradiction by A1,Th29,Th31;
  end;  
end;

reserve x,y for set;

scheme Sch2{X,Y,X1,Y1()->set,f()->Function of X(),Y(),F(set)->set}:
  ex h be Function of X1(),Y1() st
   h|X() = f() & for x st x in X1()\X() holds h.x = F(x)
provided
A1:for x st x in X1()\X() holds F(x) in Y1() and
A2:X() c= X1() & Y() c= Y1() and
A3:Y() is empty implies X() is empty
proof
  defpred P[set,set] means
    ($1 in X() implies $2 = f().$1) &
    ($1 in X1()\X() implies $2=F($1));
  A4:for x st x in X1() ex y st y in Y1() & P[x,y]  
  proof
    let x such that A5:x in X1();
    now per cases;
      suppose x in X();
        then x in dom f() by A3,FUNCT_2:def 1;
        then f().x in rng f() & not x in X1()\X()
          by FUNCT_1:def 5,XBOOLE_0:def 4;
        then P[x,f().x] & f().x in Y1() by A2,TARSKI:def 3;        
        hence thesis;
      end;
      suppose A6:not x in X();
        then x in X1()\X() by A5,XBOOLE_0:def 4;
        then P[x,F(x)] & F(x) in Y1() by A1,A6;
        hence thesis;
      end;
    end;
    hence thesis;
  end;
  consider h be Function of X1(),Y1() such that
    A5:for x st x in X1() holds P[x,h.x] from FUNCT_2:sch 1(A4);
  take h;
  thus h|X() = f()
  proof
    A6:dom f() = dom h /\ X()
    proof
      now per cases;
        suppose Y() is empty;
          hence thesis by A3,FUNCT_2:def 1;
        end;
        suppose A7:Y() is non empty;
          then Y1() is non empty by XBOOLE_1:3,A2;
          then dom f()=X() & dom h =X1() by A7,FUNCT_2:def 1;          
          hence thesis by A2,XBOOLE_1:28;
        end;
      end;
      hence thesis;      
    end;
    for x st x in dom f() holds h.x = f().x
    proof
      let x such that A7:x in dom f();
      x in X() by A7;
      hence thesis by A2,A5;
    end;
    hence thesis by A6,FUNCT_1:68;
    end;
    thus for x st x in X1()\X() holds h.x = F(x)
    proof
      let x such that A6:x in X1()\X();
      x in X1() by A6,XBOOLE_0:def 4;
      hence thesis by A6,A5;
    end;  
end;

scheme Sch3{X,Y,X1,Y1()->set,F(set)->set,P[set,set,set]}:
  Card{f where f is Function of X(),Y():
                P[f,X(),Y()]}=
  Card{f where f is Function of X1(),Y1():
                P[f,X1(),Y1()]&
                rng (f|X()) c=Y()&                
                (for x st x in X1()\X() holds f.x=F(x))}
provided
Z1:for x st x in X1()\X() holds F(x) in Y1() and
Z2:X() c= X1() & Y() c= Y1() and
Z3:Y() is empty implies X() is empty and
Z5:for f be Function of X1(),Y1() st
     (for x st x in X1()\X() holds F(x)=f.x)
  holds
    P[f,X1(),Y1()] iff P[f|X(),X(),Y()]
proof
  set F1={f where f is Function of X(),Y():
                               P[f,X(),Y()]};
  set F2={f where f is Function of X1(),Y1():
                               P[f,X1(),Y1()]&
                               rng (f|X()) c=Y()& 
                               for x st x in X1()\X() holds f.x=F(x)};
Z4:Y1() is empty implies X1() is empty                            
  proof
    assume A1:Y1() is empty;
    assume X1() is non empty;
    then consider x such that
      A2:x in X1() by XBOOLE_0:def 1;
    thus contradiction by A1,A2,Z1,Z2,Z3,XBOOLE_1:3;
  end;                                                              
   defpred FF[set,set] means
     for f be Function of X(),Y(),h be Function of X1(),Y1()
        st f=$1 & h=$2
          holds h|X()=f;
  A1:for f' be set st f' in F1 ex g' be set st g' in F2 & FF[f',g']
  proof
    let f' be set such that A2:f' in F1;
    consider f be Function of X(),Y() such that
      A3:f=f' & P[f,X(),Y()] by A2;
    consider h be Function of X1(),Y1() such that
      A4:h|X() = f & for x st x in X1()\X() holds h.x=F(x) from Sch2(Z1,Z2,Z3);
    P[h,X1(),Y1()] & rng f c= Y() by A3,A4,Z5;
    then h in F2 & FF[f',h] by A3,A4;
    hence thesis;
  end;
  consider ff be Function of F1,F2 such that
    A2:for x be set st x in F1 holds FF[x,ff.x] from FUNCT_2:sch 1(A1);
  now per cases;
    suppose A3:Y1() is empty;
      A4:F1 c= {{}}
      proof
        let x such that A5:x in F1;
        consider f be Function of X(),Y() such that
          A6:f=x & P[f,X(),Y()] by A5;
        Y() is empty by A3,Z2,XBOOLE_1:3;
        then rng f={} by XBOOLE_1:3;
        then x={} by A6,RELAT_1:64;
        hence thesis by TARSKI:def 1;
      end;
      A5:F2 c= {{}}
      proof
        let x such that A6:x in F2;
        consider f be Function of X1(),Y1() such that
          A7:f=x & P[f,X1(),Y1()] & rng (f|X()) c=Y() &
             for x st x in X1()\X() holds f.x=F(x) by A6;
        rng f={} by A3,XBOOLE_1:3;
        then x={} by A7,RELAT_1:64;
        hence thesis by TARSKI:def 1;
      end;
      set Empty={};
      now per cases;
        suppose A6:P[{},{},{}];
          Y() is empty & dom {}={} & rng {}={} by A3,Z2,XBOOLE_1:3;
          then reconsider Empty as Function of X1(),Y1() by A3,Z4,FUNCT_2:3;
          Y() is empty & dom {}={} & rng {}={} & {}|X() c= {}
          by A3,Z2,XBOOLE_1:3,RELAT_1:88;
          then {} is Function of X(),Y() & rng (Empty|X()) c= Y() &
               X()={} & Y()={} & (for x st x in X1()\X() holds Empty.x=F(x))
               by A3,Z3,Z4,FUNCT_2:3;
          then {} in F1 & {} in F2 by A6,A3,Z4;
          then F1={{}} & F2={{}} by A4,A5,ZFMISC_1:39;
          hence thesis; 
          end;
          suppose A6:not P[{},{},{}];
            A7:not {} in F1
            proof
              assume {} in F1;
              then Y() is empty & X() is empty &
                ex f be Function of X(),Y() st f={} & P[f,X(),Y()]
                by Z3,A3,Z2,XBOOLE_1:3;
              hence contradiction by A6;
            end;
            A8:not {} in F2
            proof
              assume {} in F2;
              then X1()={} & Y1()={} & ex f be Function of X1(),Y1()
                st f={} & P[f,X1(),Y1()] & rng (f|X()) c=Y() &
                for x st x in X1()\X() holds f.x=F(x) by A3,Z4;
              hence contradiction by A6;
            end;
            (F1={} or F1={{}}) & (F2={} or F2={{}}) by A4,A5,ZFMISC_1:39;
            hence thesis by A7,A8,TARSKI:def 1;
          end;
        end; 
        hence thesis;
      end;
      suppose A3:Y1() is non empty;
        now per cases;
          suppose A4:F2 is empty;
            F1 is empty
            proof
              assume F1 is non empty;
              then consider x such that
                A5:x in F1 by XBOOLE_0:def 1;
              consider f be Function of X(),Y() such that
                A6:f=x & P[f,X(),Y()] by A5;
              consider h be Function of X1(),Y1() such that
                A7:h|X() = f & for x st x in X1()\X() holds h.x=F(x)
                from Sch2(Z1,Z2,Z3);
              P[h,X1(),Y1()] & rng f c= Y() by A6,A7,Z5;
              then h in F2 by A7;
              hence thesis by A4;  
            end;
            hence thesis by A4;
          end;
          suppose A4:F2 is non empty;
            F2 c= rng ff
            proof
              let y such that A5:y in F2;
              consider f be Function of X1(),Y1() such that
                A6:f=y & P[f,X1(),Y1()]& rng (f|X()) c=Y()& 
                   for x st x in X1()\X() holds f.x=F(x) by A5;
              dom (f|X())=dom f /\ X() & dom f =X1()
                by A3,FUNCT_1:68,FUNCT_2:def 1;
              then A7:dom (f|X())=X() by Z2,XBOOLE_1:28;
              then reconsider h=f|X() as Function of X(),rng(f|X())
                by FUNCT_2:3;              
              rng (f|X()) is empty implies X() is empty by A7,RELAT_1:65;
              then reconsider h as Function of X(),Y() by A6,FUNCT_2:8;
              P[f|X(),X(),Y()] by A6,Z5;
              then A8:h in F1 & dom ff=F1 by A4,FUNCT_2:def 1;
              then ff.h in rng ff by FUNCT_1:def 5;
              then ff.h in F2;
              then consider ffh be Function of X1(),Y1() such that
                A9:ffh=ff.h & P[ffh,X1(),Y1()]& rng (ffh|X()) c=Y()& 
                  for x st x in X1()\X() holds ffh.x=F(x);
              now let x such that A10:x in X1();
                now per cases;
                  suppose x in X();
                    then x in dom h & ffh|X()=h by Z3,FUNCT_2:def 1,A2,A8,A9;
                    then h.x=ffh.x & f.x=h.x by FUNCT_1:68;
                    hence ffh.x=f.x;
                  end;
                  suppose not x in X();
                    then x in X1()\X() by A10,XBOOLE_0:def 4;
                    then ffh.x=F(x) & f.x=F(x) by A6,A9;
                    hence ffh.x=f.x;
                  end;
                end;
                hence ffh.x=f.x;
              end;      
              then ffh=f by FUNCT_2:18;
              hence thesis by A6,A8,A9,FUNCT_1:def 5; 
            end;
            then A5:rng ff=F2 & dom ff=F1 by A4,XBOOLE_0:def 10,FUNCT_2:def 1;
            for x1,x2 be set st x1 in F1 & x2 in F1 & ff.x1=ff.x2 holds x1=x2
              proof
                let x1,x2 be set such that A6:x1 in F1& x2 in F1 & ff.x1=ff.x2;
                consider f1 be Function of X(),Y() such that
                  A7:x1=f1 &P[f1,X(),Y()] by A6;
                consider f2 be Function of X(),Y() such that
                  A8:x2=f2 & P[f2,X(),Y()] by A6;      
                dom ff= F1 by A4,FUNCT_2:def 1;
                then ff.x1 in rng ff & ff.x2 in rng ff by A6,FUNCT_1:def 5;
                then A9:ff.x1 in F2 & ff.x2 in F2;
                then consider F1 be Function of X1(),Y1() such that
                  A10:ff.x1=F1 & P[F1,X1(),Y1()]& rng (F1|X()) c=Y()& 
                      for x st x in X1()\X() holds F1.x=F(x);
                consider F2 be Function of X1(),Y1() such that
                  A11:ff.x2=F2 & P[F2,X1(),Y1()]& rng (F2|X()) c=Y()& 
                      for x st x in X1()\X() holds F2.x=F(x) by A9;
                F1|X()=f1 & F2|X()=f2 & F1=F2 by A2,A6,A7,A8,A10,A11;
                hence thesis by A7,A8;                                
              end;
            then ff is one-to-one by A4, FUNCT_2:25;
            then F1,F2 are_equipotent by A5,WELLORD2:def 4;  
            hence thesis by CARD_1:21;
          end;
        end;  
        hence thesis;
      end;
    end;
    hence thesis;
end;

scheme Sch4{X,Y,x,y()->set,P[set,set,set]}:
  Card{f where f is Function of X(),Y():
                P[f,X(),Y()]}=
  Card{f where f is Function of (X()\/{x()}),(Y()\/{y()}):
                P[f,X()\/{x()},Y()\/{y()}]&
                rng (f|X()) c=Y() &                
                f.x()=y()}
provided
Z1:Y() is empty implies X() is empty and
Z2:not x() in X() and
Z3:for f be Function of X()\/{x()},Y()\/{y()}
st f.x()=y()
  holds
    P[f,X()\/{x()},Y()\/{y()}] iff P[f|X(),X(),Y()] 
proof
  deffunc F(set)=y();
  set X1=X()\/{x()};
  set Y1=Y()\/{y()};
  A1:for x st x in X1\X() holds F(x) in Y1
  proof
    let x such that x in X1\X();
    y() in {y()} & {y()} c=Y1 by TARSKI:def 1,XBOOLE_1:7;
    hence thesis;
  end;
  A2:X()c=X1 & Y()c=Y1 by XBOOLE_1:7;
  A3:for f be Function of X1,Y1 holds
    (for x st x in X1\X() holds F(x)=f.x) iff f.x()=y()
    proof
      let f be Function of X1,Y1;
      A4:X1\X()={x()}\X() by XBOOLE_1:40
      .={x()} by Z2,ZFMISC_1:67;
      thus (for x st x in X1\X() holds F(x)=f.x) implies f.x()=y()
      proof
        assume A5:(for x st x in X1\X() holds F(x)=f.x);
        x() in {x()} by TARSKI:def 1;
        hence thesis by A4,A5;
      end;
      thus f.x()=y() implies (for x st x in X1\X() holds F(x)=f.x)
        by A4,TARSKI:def 1;      
    end;
  A4:for f be Function of X1,Y1 st
     (for x st x in X1\X() holds F(x)=f.x)
  holds
    P[f,X1,Y1] iff P[f|X(),X(),Y()]
    proof
      let f be Function of X1,Y1 such that
        A5:for x st x in X1\X() holds F(x)=f.x;
      y()=f.x() by A3,A5;
      hence thesis by Z3;
    end;
  set F1={f where f is Function of X1,Y1:
                       P[f,X1,Y1]&
                       rng (f|X()) c=Y()&
                       for x st x in X1\X() holds f.x=F(x)};
  set F2={f where f is Function of X1,Y1:
                       P[f,X1,Y1]&
                       rng (f|X()) c=Y()&
                       f.x()=y()};    
  A5:Card{f where f is Function of X(),Y():P[f,X(),Y()]}= Card F1
                from Sch3(A1,A2,Z1,A4);  
  A6:F1 c= F2
  proof
    let x such that A7:x in F1;
    consider f be Function of X1,Y1 such that
      A8:x=f & P[f,X1,Y1] & rng (f|X()) c=Y() &
      for x st x in X1\X() holds f.x=F(x) by A7;
    f.x()=y() by A3,A8;
    hence thesis by A8; 
  end;
  F2 c= F1
  proof
    let x such that A7:x in F2;
    consider f be Function of X1,Y1 such that
      A8:x=f & P[f,X1,Y1] & rng (f|X()) c=Y() & f.x()=y() by A7;
    for x st x in X1\X() holds F(x)=f.x by A3,A8;
    hence thesis by A8; 
  end;
  hence thesis by A5,A6,XBOOLE_0:def 10;
end;

theorem Th35:
  for f be Function of n+1,k+1 st
    f is onto "increasing & f"{f.n}={n} holds f.n=k
  proof
    let f be Function of n+1,k+1 such that
    A1:f is onto "increasing & f"{f.n}={n};   
    assume A2:f.n<>k;
    now per cases by A2,REAL_1:def 5;
      suppose A3:f.n>k;
        f.n<k+1 by EULER_1:1;
        hence contradiction by A3,NAT_1:38;
      end;
      suppose A3:f.n<k;
        then f.n<k+1 & k<k+1 & rng f = k+1 & f.n in k+1 &
          k+1 is Subset of NAT by A1,NAT_1:38,FUNCT_2:def 3,Th8;
        then f.n in rng f & k in rng f & f.n in NAT by EULER_1:1;
        then A4:min* f"{f.n} < min* f"{k} & min* f"{f.n} = n
          by A1,A3,Def1,Th5;
        n+1 >0 by NAT_1:18;
        then min* f"{k} <=(n+1)-1 by Th16;        
        hence contradiction by A4;
      end;
    end;
    hence contradiction;
  end;
    
theorem Th36:
  for f be Function of n+1,k st
    k<>0 & f"{f.n}<>{n} holds
      ex m st m in f"{f.n} & m<>n
proof
  let f be Function of n+1,k such that
    A1:k<>0 and
    A2: f"{f.n}<>{n};
  n<n+1 & dom f =n+1 by A1,NAT_1:38,FUNCT_2:def 1;
  then n in dom f & f.n in {f.n} by EULER_1:1,TARSKI:def 1;
  then n in f"{f.n} by FUNCT_1:def 13;
  then ex m be set st m in f"{f.n} & m<>n by A2,ZFMISC_1:41;
  hence thesis;
end;

theorem Th37:
  for f be Function of n,k, g be Function of n+m,k+l
    st g is "increasing & f=g|n
    holds
      for i,j st i in rng f & j in rng f & i<j holds min* f"{i} < min* f"{j}
  proof
    let f be Function of n,k, g be Function of n+m,k+l
      such that A1:g is "increasing and A2: f=g|n;
    let i,j such that A3:i in rng f & j in rng f & i<j;
    A4:for k1 be Nat st k1 in rng f holds k1 in rng  g& min* f"{k1}=min* g"{k1}
       proof
         let k1 be Nat such that A5:k1 in rng f;         
         consider x be set such that
           A6:x in dom f & f.x=k1 by A5,FUNCT_1:def 5;
         k<=k+l by NAT_1:29;
         then k c= k+l by CARD_1:56;  
         then k1 in k & k1 in k+l & n <= n+m by A5,TARSKI:def 3,NAT_1:29;
         then A7:dom f = n & dom g =n+m & n c= n+m by FUNCT_2:def 1,CARD_1:56;
         x in n & n is Subset of NAT by A6,Th8;
         then reconsider x'=x as Nat;
         A8:f.x'=g.x' & f.x'= k1 & k1 in {k1} & x' in dom g
           by A2,A6,A7,TARSKI:def 1,FUNCT_1:68;
         then x' in f"{k1} & x' in g"{k1} & k1 in rng g
           by A6,FUNCT_1:def 13,def 5;
         then min* g"{k1} <= x' & x' <n & min* g"{k1} in g"{k1}
           by A6,HENMODEL:def 1,EULER_1:1;
         then min* g"{k1} < n & g.(min* g"{k1}) in {k1}
           by AXIOMS:22,FUNCT_1:def 13;
         then min* g"{k1} in dom f & g.(min* g"{k1}) in {k1}
           by A7,EULER_1:1;
         then min* g"{k1} in n & f.(min* g"{k1}) in {k1} by A2,FUNCT_1:68;
         then A9:min* g"{k1} in f"{k1} by A7,FUNCT_1:def 13;
         now let n1 be Nat such that A10:n1 in f"{k1};
           n1 in n by A7,A10,FUNCT_1:def 13;
           then n1 in dom f & n1 in dom g & f.n1 in {k1}
             by A7,A10,FUNCT_1:def 13;  
           then g.n1 in {k1} & n1 in dom g by A2,FUNCT_1:68;
           then n1 in g"{k1} by FUNCT_1:def 13;
           hence min* g"{k1}<=n1 by HENMODEL:def 1;           
         end;
         hence thesis by A8,A9, HENMODEL:def 1,FUNCT_1:def 5;                 
       end;
   i in rng g & j in rng g & min* f"{i}=min* g"{i} &  min* f"{j}=min* g"{j}
     by A3,A4;
   hence thesis by A1,A3,Def1;
 end;
    
theorem Th38:
  for f be Function of n+1,k+1 st
    f is onto "increasing & f"{f.n}={n} holds
      rng (f|n) c= k & 
      for g be Function of n,k st g=f|n
        holds g is onto "increasing
proof
  let f be Function of n+1,k+1 such that
  A1:f is onto "increasing & f"{f.n}={n};
  now per cases by NAT_1:18;
    suppose A2:n=0;
      then 0+1>=k+1 & dom (f|n)=dom f /\0 by A1,Th17,FUNCT_1:68;
      then 0>=k & dom (f|n) ={} by REAL_1:53;
      then k=0 & rng (f|n)=0 by NAT_1:18,RELAT_1:65;
      hence thesis by A2,Th15;
     end;
     suppose A2:n>0;
       thus A3:rng (f|n) c= k
         proof
           let fi be set such that A4:fi in rng (f|n);
           fi in k+1 & k+1 is Subset of NAT by A4,Th8;
           then reconsider fi as Nat;
           consider i be set such that
             A5:i in dom (f|n) & (f|n).i=fi by A4,FUNCT_1:def 5;
           i in dom f /\n by A5,FUNCT_1:68;
           then i in n & n is Subset of NAT by XBOOLE_0:def 3,Th8;
           then reconsider i as Nat;
           f.i<k
           proof
             assume A6:f.i>=k;  
             i in dom f/\n by A5,FUNCT_1:68;
             then A7:i in dom f & f.i<k+1 & i in n 
               by EULER_1:1,XBOOLE_0:def 3;
             then f.i<=k by NAT_1:38;
             then f.i=k & f.i in {f.i} & f.n=k
               by A1,A6,AXIOMS:21,TARSKI:def 1,Th35;
             then i in f"{f.n} by A7,FUNCT_1:def 13;
             then i >= min* f"{f.n} by HENMODEL:def 1;
             then i >= n by Th5,A1;
             hence contradiction by A7,EULER_1:1;
           end;
           then f.i=(f|n).i & f.i in k by A5,FUNCT_1:68,EULER_1:1;
           hence thesis by A5;           
         end;
       thus for g be Function of n,k st g=f|n
          holds g is onto "increasing
          proof
            let g be Function of n,k such that A4:g=f|n;
            thus g is onto
              proof
                k c=rng g
                 proof
                   let k1 be set such that A5:k1 in k;
                   k is Subset of NAT by Th8;
                   then reconsider k'=k1 as Nat by A5;
                   k'<k by A5,EULER_1:1;
                   then k' < k+1 by NAT_1:38;
                   then k' in k+1 & rng f = k+1
                     by A1,EULER_1:1,FUNCT_2:def 3;
                   then consider n1 be set such that
                     A6:n1 in dom f & f.n1=k' by FUNCT_1:def 5;
                   dom f=n+1 & n+1 is Subset of NAT by FUNCT_2:def 1,Th8;
                   then reconsider n1 as Nat by A6;
                   n1<n+1 & f.n=k & n<n+1
                     by A1,A6,EULER_1:1,Th35,NAT_1:38;
                   then n1<=n & n1<>n  & n c= n+1 & dom f=n+1
                     by A5,A6,NAT_1:38,CARD_1:56,FUNCT_2:def 1;
                   then n1 < n & n=dom f /\n & dom f /\n=dom (f|n) 
                      by REAL_1:def 5,FUNCT_1:68,XBOOLE_1:28;
                   then n1 in dom g by A4,EULER_1:1;
                   then g.n1 in rng g & g.n1=f.n1 by A4,FUNCT_1:def 5,68;
                   hence thesis by A6;
                 end;
                then k=rng g by XBOOLE_0:def 10;
                hence thesis by FUNCT_2:def 3;                      
              end;
            thus g is "increasing
              proof
                n=0 iff k=0
                proof
                  n<n+1 by NAT_1:38;
                  then n c= n+1 & dom f=n+1
                    by CARD_1:56,FUNCT_2:def 1;
                  then n=dom f /\n & dom f /\n=dom (f|n) & 0 in n  
                    by FUNCT_1:68,XBOOLE_1:28,A2,EULER_1:1;
                  then (f|n).0 in rng (f|n) by FUNCT_1:def 5;
                  hence thesis by A2,A3;
                end;
                then (n=0 iff k=0) & for i,j st i in rng g & j in rng g & i<j
                holds min* g"{i} < min* g"{j} by A1,A4,Th37;
                hence thesis by Def1;
              end;  
           end;
      end;
    end;
  hence thesis;
end;
     
theorem Th39:
  for f be Function of n+1,k,g be Function of n,k st
    f is onto "increasing & f"{f.n}<>{n} & f|n =g 
    holds g is onto "increasing
proof
  let f be Function of n+1,k, g be Function of n,k such that
    A1:f is onto "increasing and
    A2:f"{f.n}<>{n} and
    A3:f|n =g;
  now per cases by NAT_1:18;
    suppose k=0;
      hence thesis by A1,Def1;
    end;
    suppose A4:k>0;
      then k>0 & n<n+1 by NAT_1:38;
      then dom f=n+1 & n in n+1 by FUNCT_2:def 1,EULER_1:1;
      then A5:f.n in rng f & rng f = k by A1,FUNCT_1:def 5,FUNCT_2:def 3;
      now per cases by NAT_1:18;
        suppose A6:n=0;
          then dom f=1 & 1 ={0}
            by CARD_5:1,FUNCT_2:def 1,A4;
          then f.0 in rng f & {f.0}=k & Card k =k & Card {f.0} = one
            by A5,CARD_1:66,50,FUNCT_1:14;
          then A7:k=1 & f"{f.0} <>{} by CARD_2:20,FUNCT_1:142;
          now let x be set such that A8:x in f"{f.0};
            x in dom f by A8,FUNCT_1:def 13;
            hence x in {0} by A6,CARD_5:1;
          end;
          then f"{f.0} c= {0} & f"{f.0}<>{0} by A2,A6,TARSKI:def 3;
          hence thesis by ZFMISC_1:39,A7;
        end;
        suppose A6:n>0;
          k c= rng g
            proof
              let k1 be set such that A7:k1 in k;
              k is Subset of NAT by Th8;
              then reconsider k'=k1 as Nat by A7;
              consider x be set such that
                A8:x in dom f & f.x=k1 by A5,A7,FUNCT_1:def 5;
              dom f =n+1 & n+1 is Subset of NAT by A7,FUNCT_2:def 1,Th8;
              then reconsider x as Nat by A8;
              x<n+1 by A8,EULER_1:1;
              then A9:x<=n by NAT_1:38;
              now per cases by A9,REAL_1:def 5;
                suppose x<n;
                  then x in n & dom g = n by A4,EULER_1:1,FUNCT_2:def 1;
                  then x in dom g & g.x=f.x by A3,FUNCT_1:68;
                  hence k1 in rng g by A8,FUNCT_1:def 5;
                end;
                suppose x=n;
                  then consider m such that
                    A10:m in f"{k1} & m<>n by Th36, A2,  A4,A8;
                  m in dom f by A10,FUNCT_1:def 13; 
                  then m<n+1 by EULER_1:1;
                  then m<=n by NAT_1:38;
                  then m < n by A10,REAL_1:def 5;
                  then m in n & n=dom g  & f.m in {k1}
                    by A4,EULER_1:1,FUNCT_2:def 1,A10,FUNCT_1:def 13;
                  then m in dom g & g.m=f.m & f.m=k1
                    by A3,FUNCT_1:68,TARSKI:def 1;
                  hence k1 in rng g by FUNCT_1:def 5;
                end;
              end;
              hence thesis;
            end;
            then rng g =k & k=k+0 by XBOOLE_0:def 10;
            then g is onto & (n=0 iff k=0) &
              for i,j st i in rng g & j in rng g & i<j
              holds min* g"{i} < min* g"{j} by A1,A3,A4,Th37,A6,FUNCT_2:def 3;
            hence thesis by Def1;
          end;
        end;
      hence thesis;
    end;
  end;
  hence thesis;
end;
       
theorem Th40:
  for f be Function of n,k, g be Function of n+1,k+m
    st f is onto "increasing & f=g|n
    holds
      for i,j st i in rng g & j in rng g & i<j holds min* g"{i} < min* g"{j}
proof
  let f be Function of n,k, g be Function of n+1,k+m
  such that
  A1:f is onto "increasing and
  A2:f=g|n;
  A2':for i st i<n holds f.i=g.i
  proof
    let i such that A3:i <n;
    k=0 iff n=0 by A1,Def1;
    then i in n & dom f=n by A3,EULER_1:1,FUNCT_2:def 1;
    hence f.i=g.i by A2,FUNCT_1:68;    
  end; 
  let i,j such that A3:i in rng g & j in rng g & i<j;
  A4:for l st l in rng g & not l in rng f holds l=g.n
  proof
    let l such that A4:l in rng g & not l in rng f;
    assume A5:l<>g.n;
    consider x be set such that
      A6:x in dom g & g.x=l by A4,FUNCT_1:def 5;            
    dom g=n+1 & n+1 is Subset of NAT by FUNCT_2:def 1,Th8,A4;
    then reconsider x as Nat by A6;        
    x<n+1 by A6,EULER_1:1;
    then x<=n by NAT_1:38;
    then A7:x<n by A5,A6,REAL_1:def 5;
    then n>0 by NAT_1:18;
    then k<>0 by A1,Def1;
    then dom f=n & x in n & f.x=g.x by A2',FUNCT_2:def 1,A7,EULER_1:1;
    hence contradiction by A4,A6,FUNCT_1:def 5;
  end;
  A5:for l st l in rng g & not l in rng f holds min* g"{l}=n
  proof
    let l such that A6:l in rng g & not l in rng f;
    assume A7: min* g"{l}<>n;    
    n <n+1 & dom g=n+1 by A6,NAT_1:38,FUNCT_2:def 1;
    then n+1>0 & n in dom g & l in {l} & g.n=l by A4,A6,EULER_1:1,TARSKI:def 1;
    then min* g"{l} <= (n+1)-1  & n in g"{l} by Th16,FUNCT_1:def 13;
    then min* g"{l}< n & min* g"{l} in g"{l} by A7,REAL_1:def 5,HENMODEL:def 1;
    then A8:min* g"{l} in n & g.min* g"{l} in {l} & 0<n
      & f.min* g"{l}=g.min* g"{l} by A2',EULER_1:1,FUNCT_1:def 13;
    then k<>0 by A1,Def1;
    then dom f=n by FUNCT_2:def 1;
    then f.min* g"{l} in rng f by A8,FUNCT_1:def 5;
    hence contradiction by A6,A8,TARSKI:def 1;  
  end;
  A6:for l st l in rng g & not l in rng f holds l>=k
  proof
    let l such that A6:l in rng g & not l in rng f;
    assume l<k;
    then l in k & rng f=k by A1,FUNCT_2:def 3,EULER_1:1;
    hence thesis by A6;    
  end;
  A7:for k1 be Nat st k1 in rng f holds min* g"{k1}=min* f"{k1}
  proof
    let k1 be Nat such that A7:k1 in rng f;
    consider x be set such that
      A8:x in dom f & f.x=k1 by A7,FUNCT_1:def 5;
    k<=k+m by NAT_1:29;
    then k c= k+m by CARD_1:56;
    then k1 in k & k1 in k+m & n <= n+1 by A7,TARSKI:def 3,NAT_1:29;
    then A9:dom f = n & dom g =n+1 & n c= n+1 by FUNCT_2:def 1,CARD_1:56;
    x in n & n is Subset of NAT by A8,Th8;
    then reconsider x as Nat;
    x < n by A8,EULER_1:1;
    then f.x=g.x & f.x= k1 & k1 in {k1} & x in dom g
       by A2',A8,A9,TARSKI:def 1;
    then x in f"{k1} & x in g"{k1} & k1 in rng g
       by A8,FUNCT_1:def 13,def 5;
    then A11:min* f"{k1} <= x & x <n & min* f"{k1} in f"{k1}
      by A8,HENMODEL:def 1,EULER_1:1;
    then min* f"{k1} < n & f.(min* f"{k1}) in {k1}
       by AXIOMS:22,FUNCT_1:def 13;
    then min* f"{k1} in n & g.(min* f"{k1}) in {k1} by A2',EULER_1:1;
    then A12:min* f"{k1} in g"{k1} by A9,FUNCT_1:def 13;
    now let n1 be Nat such that A13:n1 in g"{k1};
      n1 in n+1 by A9,A13,FUNCT_1:def 13;
      then n1<n+1 by EULER_1:1;
      then A14:n1<=n by NAT_1:38;
      now per cases by A14,REAL_1:def 5;
        suppose n1<n;
           then n1<n & n1 in dom f & g.n1 in {k1}
             by A9,A13,EULER_1:1,FUNCT_1:def 13;  
           then f.n1 in {k1} & n1 in dom f by A2';
           then n1 in f"{k1} by FUNCT_1:def 13;
           hence min* f"{k1}<=n1 by HENMODEL:def 1;           
         end;
         suppose n1=n;
           hence min* f"{k1} <= n1 by A11,AXIOMS:22;
         end;
       end;
       hence min* f"{k1} <= n1;
     end;                  
     hence thesis by A12, HENMODEL:def 1;                 
   end;         
  now per cases;
    suppose A8:i in rng f & j in rng f;
      then min* g"{i}=min* f"{i} &  min* g"{j}=min* f"{j} by A7;
      hence thesis by A1,A3,A8,Def1;
    end;
    suppose A8:i in rng f & not j in rng f;
      then n<>0 by A1,Def1;
      then n>0 by NAT_1:18;
      then min* f"{i} <= n-1 & min* g"{i}=min* f"{i} & n-1 is Nat
        by A8,A7,Th16,NAT_1:60;
      then min* g"{i} <=n-1 & (n-1)<(n-1)+1 & min* g"{j}=n
        by A3,A5,A8,NAT_1:38;
      hence thesis by AXIOMS:22;
    end;    
    suppose not i in rng f  & j in rng f;
      then i>=k & j< k by A3,A6,EULER_1:1;
      hence thesis by A3,AXIOMS:22;
    end;
    suppose not i in rng f & not j in rng f;
      then i=g.n & j=g.n by A3,A4; 
      hence thesis by A3;
    end;
  end;
  hence thesis;
end;

theorem Th41:  
  for f be Function of n,k, g be Function of n+1,k+1
      st f is onto "increasing & f=g|n & g.n = k
    holds
      g is onto "increasing & g"{g.n}={n}
proof
  let f be Function of n,k, g be Function of n+1,k+1 such that
  A1:f is onto "increasing and
  A2:f=g|n and
  A3:g.n = k;
  thus g is onto
  proof
    k+1 c= rng g
    proof
      let x' be set such that A4:x' in k+1;
      k+1 is Subset of NAT by Th8;
      then reconsider x=x' as Nat by A4;
      x<k+1 by A4,EULER_1:1;
      then A5:x<=k by NAT_1:38;
      now per cases by A5,REAL_1:def 5;
        suppose x<k;
          then x in k & rng f=k by A1,FUNCT_2:def 3,EULER_1:1;
          then consider y be set such that
            A6:y in dom f & f.y =x by FUNCT_1:def 5;
          (n=0 iff k=0) & n<=n+1 by A1,Def1,NAT_1:29;  
          then dom f = n & n c= n+1 & dom g = n+1
            by FUNCT_2:def 1,CARD_1:56;
          then y in dom g & f.y=g.y by A2,A6,FUNCT_1:68;
          hence thesis by A6,FUNCT_1:def 5;
        end;
        suppose A6:x=k;
          n<n+1 by NAT_1:38;
          then n in n+1 & dom g =n+1 by EULER_1:1,FUNCT_2:def 1;
          hence thesis by A3,A6,FUNCT_1:def 5;
        end;
      end;
      hence thesis;
    end;
    then k+1=rng g by XBOOLE_0:def 10;
    hence thesis by FUNCT_2:def 3;
  end;
  for i,j st i in rng g & j in rng g & i<j holds
    min* g"{i} < min* g"{j} by A1,A2,Th40;
  hence g is "increasing by Def1;
  thus g"{g.n}={n}
    proof
      assume g"{g.n}<>{n};
      then consider m such that
        A4:m in g"{g.n} & m<>n by Th36;
      m in dom g & dom g=n+1 by A4,FUNCT_1:def 13,FUNCT_2:def 1;
      then m<n+1 by EULER_1:1;
      then m<=n & (n=0 iff k=0) by A1,Def1,NAT_1:38;
      then m<n & dom f=n  by A4,FUNCT_2:def 1,REAL_1:def 5;
      then m in dom f & g.m in {g.n} by A4,EULER_1:1,FUNCT_1:def 13;
      then f.m in rng f & f.m=g.m & g.m=k
        by A2,FUNCT_1:68,def 5,TARSKI:def 1,A3;
      hence contradiction by EULER_1:1;
    end;
end;

theorem Th42:  
  for f be Function of n,k, g be Function of n+1,k
      st f is onto "increasing & f=g|n & g.n < k
    holds
      g is onto "increasing & g"{g.n}<>{n}
proof
  let f be Function of n,k, g be Function of n+1,k such that
  A1:f is onto "increasing and
  A2:f=g|n and
  A3:g.n < k;
  thus g is onto
  proof
    k c= rng g
    proof
      let x' be set such that A4:x' in k;
      k is Subset of NAT by Th8;
      then reconsider x=x' as Nat by A4;
      x in k & rng f=k by A1,A4,FUNCT_2:def 3;
      then consider y be set such that
        A5:y in dom f & f.y =x by FUNCT_1:def 5;
      (n=0 iff k=0) & n<=n+1 & k<>0 by A1,A3,Def1,NAT_1:18,38;  
      then dom f = n & n c= n+1 & dom g = n+1
        by FUNCT_2:def 1,CARD_1:56;
      then y in dom g & f.y=g.y by A2,A5,FUNCT_1:68;
      hence thesis by A5,FUNCT_1:def 5;         
    end;
    then k=rng g by XBOOLE_0:def 10;
    hence thesis by FUNCT_2:def 3;
  end;
  k<>0 & k=k+0 by A3,NAT_1:18;
  then (n+1=0 iff k=0) & for i,j st i in rng g & j in rng g & i<j holds
    min* g"{i} < min* g"{j} by A1,A2,Th40;
  hence g is "increasing by Def1;        
  g.n in k & k=rng f by A1,A3,EULER_1:1,FUNCT_2:def 3;
  then consider x such that
    A4:x in dom f & f.x=g.n by FUNCT_1:def 5;
  k<>0 by A3,NAT_1:18;  
  then n<=n+1 & dom f=n & k<>0 by NAT_1:29,FUNCT_2:def 1;
  then x in n & n c=  n+1 & dom g=n+1 & g.n=g.x
    by A2,FUNCT_1:68,A4,CARD_1:56,FUNCT_2:def 1;  
  then x in dom g & g.x in {g.n} & x<>n by TARSKI:def 1;
  then x in g"{g.n} & not x in {n} by FUNCT_1:def 13,TARSKI:def 1;
  hence g"{g.n}<>{n}; 
end;

Lm1:n+1=n\/{n} by AFINSQ_1:4;

Lm2:k<n implies n\/{k}=n
  proof
    assume k<n;
    then k in n by EULER_1:1;
    then {k} c= n by ZFMISC_1:37;
    hence thesis by XBOOLE_1:12;
  end;

theorem Th43:
  Card{f where f is Function of n+1,k+1:
    f is onto "increasing & f"{f.n}={n}}=
  Card{f where f is Function of n,k: f is onto "increasing}
proof
  set F1={f where f is Function of n+1,k+1:
          f is onto "increasing &
          f"{f.n}={n}};
  set F2={f where f is Function of n,k: f is onto "increasing};
  now per cases;
    suppose A1:k=0 & n<>0;
      F1 is empty
        proof
          assume F1 is non empty;
          then consider x be set such that
            A2:x in F1 by XBOOLE_0:def 1;
          consider f be Function of n+1,k+1 such that
            A3:x=f & f is onto "increasing & f"{f.n}={n} by A2;
          0<n+1 & n<n+1 by NAT_1:18,38;
          then k+1={0} & 0 in n+1 & n in n+1 & dom f = n+1
            by A1,CARD_5:1,EULER_1:1,FUNCT_2:def 1;
          then f.n=0 & f.0=0 & 0 in dom f & 0 in {0} by TARSKI:def 1;
          then 0 in {n} by A3,FUNCT_1:def 13;
          hence thesis by A1,TARSKI:def 1;              
        end;
      then Card F1=0 & n block k =0 by A1,Th33,Th31;
      hence thesis by Def2;
    end;
    suppose k=0 implies n=0;
      then A1:k is empty implies n is empty;
      defpred P[set,set,set] means
        for i,j st i=$2 & j=$3 holds
          ex f be Function of i,j st
            f=$1 & 
            f is onto "increasing &
            (n < i implies f"{f.n}={n});
      A2:not n in n;
      A3:for f be Function of n\/{n},k\/{k}
           st f.n=k
             holds P[f,n\/{n},k\/{k}] iff P[f|n,n,k]
         proof
           let f' be Function of n\/{n},k\/{k} such that A4:f'.n=k;
           thus P[f',n\/{n},k\/{k}] implies P[f'|n,n,k]
           proof
             assume A5:P[f',n\/{n},k\/{k}];
             n+1=n\/{n} & k+1=k\/{k} by Lm1;
             then consider f be Function of n+1,k+1 such that
               A6:f=f' & f is onto "increasing &
               (n < n+1 implies f"{f.n}={n}) by A5;
             let i,j such that A7:i=n & j=k;             
             n<=n+1 by NAT_1:29;
             then dom f=n+1 & n c=n+1 & dom (f|n)=dom f /\ n
               by FUNCT_2:def 1,CARD_1:56,FUNCT_1:68;
             then dom (f|n)=n & rng (f|n) c= k by A6,NAT_1:38,XBOOLE_1:28,Th38;
             then reconsider fn=f|n as Function of n,k by FUNCT_2:4;
             reconsider fi=fn as Function of i,j by A7;
             fi=f'|n & fi is onto "increasing & (n < i implies fi"{fi.n}={n})
               by A7,A6,NAT_1:38,Th38;
             hence thesis;
           end;         
           thus P[f'|n,n,k] implies P[f',n\/{n},k\/{k}]
           proof
             assume P[f'|n,n,k];
             then consider fn be Function of n,k such that
               A5:fn=f'|n  and 
               A6:fn is onto "increasing and
                 (n < n implies fn"{fn.n}={n});
             let i,j such that A7:i=n\/{n} & j=k\/{k};
             n\/{n}=n+1 & k\/{k}=k+1 by Lm1;
             then reconsider f=f' as Function of n+1,k+1;
             reconsider f1=f as Function of i,j by A7;
             n+1=i & k+1 =j by A7,Lm1;
             then f1 is onto "increasing & (n < i implies f1"{f1.n}={n})
               by A4,A5,A6,Th41;
             hence thesis;
           end;        
         end;  
      set FF1={f where f is Function of (n\/{n}),(k\/{k}):
                P[f,n\/{n},k\/{k}]&
                rng (f|n) c=k &
                f.n=k};       
      set FF2={f where f is Function of n,k: P[f,n,k]};
      A4:Card FF2 = Card FF1 from Sch4(A1,A2,A3);
      A5:F1=FF1
      proof
        thus F1 c=FF1
          proof
            let x such that A6:x in F1;
            consider f be Function of n+1,k+1 such that
              A7:f=x & f is onto "increasing & f"{f.n}={n} by A6;
            P[f,n\/{n},k\/{k}]
              proof
                let i,j such that A8:i=n\/{n} & j=k\/{k};
                i=n+1 & j=k+1 by A8,Lm1;
                then reconsider f'=f as Function of i,j;
                i=n+1 & j=k+1 by A8,Lm1;
                hence thesis by A7;
              end;
            then P[f,n\/{n},k\/{k}] & f.n=k & rng (f|n) c= k
              & n+1=n\/{n} & k+1=k\/{k} by A7,Th38,Th35,Lm1;
            hence thesis by A7;
          end;
        thus FF1 c=F1
          proof
            let x such that A6:x in FF1;
            consider f be Function of (n\/{n}),(k\/{k})such that
              A7:x=f & P[f,n\/{n},k\/{k}] & rng (f|n) c=k & f.n=k by A6;
            n+1=n\/{n} & k+1=k\/{k} by Lm1;
            then ex f' be Function of n+1,k+1 st f=f' & 
              f' is onto "increasing & (n < n+1 implies f'"{f'.n}={n}) by A7;
            hence thesis by A7,NAT_1:38;  
          end;
      end;
      F2=FF2
      proof
        thus F2 c=FF2
          proof
            let x such that A6:x in F2;
            consider f be Function of n,k such that
              A7:x=f & f is onto "increasing by A6;
            P[x,n,k] by A7;
            hence thesis by A7;
          end;
        thus FF2 c= F2
          proof
            let x such that A6:x in FF2;
            consider f be Function of n,k such that
              A7:x=f & P[f,n,k] by A6;
            ex g be Function of n,k st g=f & 
              g is onto "increasing & (n < n implies g"{g.n}={n}) by A7;
            hence thesis by A7;
          end;  
        end;
      hence thesis by A4,A5;
    end;
  end;
  hence thesis;
end;

theorem Th44:
  for l st l<k holds
    Card{f where f is Function of n+1,k:
      f is onto "increasing & f"{f.n}<>{n} & f.n=l}=
    Card{f where f is Function of n,k: f is onto "increasing}
proof
  let l such that A0:l<k;      
  set F1={f where f is Function of n+1,k:
          f is onto "increasing &
          f"{f.n}<>{n}&
          f.n=l};
  set F2={f where f is Function of n,k: f is onto "increasing};
  now per cases;
    suppose k=0 & n<>0;
      hence thesis by A0,NAT_1:18;
    end;
    suppose k=0 implies n=0;
      then A1:k is empty implies n is empty;
      defpred P[set,set,set] means
        for i,j st i=$2 & j=$3 holds
          ex f be Function of i,j st
            f=$1 & 
            f is onto "increasing &
            (n < i implies f"{f.n}<>{n});
      A2:not n in n;
      A3:for f be Function of n\/{n},k\/{l}
           st f.n=l
             holds P[f,n\/{n},k\/{l}] iff P[f|n,n,k]
         proof
           let f' be Function of n\/{n},k\/{l} such that A4:f'.n=l;
           thus P[f',n\/{n},k\/{l}] implies P[f'|n,n,k]
           proof
             assume A5:P[f',n\/{n},k\/{l}];
             n+1=n\/{n} & k=k\/{l} by A0,Lm1,Lm2;
             then consider f be Function of n+1,k such that
               A6:f=f' & f is onto "increasing &
               (n < n+1 implies f"{f.n}<>{n}) by A5;
             let i,j such that A7:i=n & j=k;             
             n<=n+1 & k<>0 by A0,NAT_1:18,38;
             then dom f=n+1 & n c=n+1 & dom (f|n)=dom f /\ n
               by FUNCT_2:def 1,CARD_1:56,FUNCT_1:68;
             then dom (f|n)=n & rng (f|n) c= k by XBOOLE_1:28;
             then reconsider fn=f|n as Function of n,k by FUNCT_2:4;
             reconsider fi=fn as Function of i,j by A7;
             fi=f'|n & fi is onto "increasing & (n < i implies fi"{fi.n}<>{n})
               by A7,A6,NAT_1:38,Th39;
             hence thesis;
           end;         
           thus P[f'|n,n,k] implies P[f',n\/{n},k\/{l}]
           proof
             assume P[f'|n,n,k];
             then consider fn be Function of n,k such that
               A5:fn=f'|n  and 
               A6:fn is onto "increasing and
                 (n < n implies fn"{fn.n}<>{n});
             let i,j such that A7:i=n\/{n} & j=k\/{l};  
             n\/{n}=n+1 & k\/{l}=k by A0,Lm1,Lm2;
             then reconsider f=f' as Function of n+1,k;
             reconsider f1=f as Function of i,j by A7;             
             n+1=i & k =j by A0,A7,Lm1,Lm2;
             then f1 is onto "increasing & (n < i implies f1"{f1.n}<>{n})
               by A0,A4,A5,A6,Th42;
             hence thesis;
           end;        
         end;  
      set FF1={f where f is Function of (n\/{n}),(k\/{l}):
                P[f,n\/{n},k\/{l}]&
                rng (f|n) c=k &
                f.n=l};
      set FF2={f where f is Function of n,k: P[f,n,k]};
      A4:Card FF2 = Card FF1 from Sch4(A1,A2,A3);
      A5:F1=FF1
      proof
        thus F1 c=FF1
          proof
            let x such that A6:x in F1;
            consider f be Function of n+1,k such that
              A7:f=x & f is onto "increasing & f"{f.n}<>{n}& f.n=l by A6;
            P[f,n\/{n},k\/{l}]
              proof
                let i,j such that A8:i=n\/{n} & j=k\/{l};
                i=n+1 & j=k by A0,A8,Lm1,Lm2;
                then reconsider f'=f as Function of i,j;
                i=n+1 & j=k by A0,A8,Lm1,Lm2;
                hence thesis by A7;
              end;
            then P[f,n\/{n},k\/{l}] & f.n=l & rng (f|n) c= k
              & n+1=n\/{n} & k=k\/{l} by A0,A7,Lm1,Lm2;
            hence thesis by A7;
          end;
        thus FF1 c=F1
          proof
            let x such that A6:x in FF1;
            consider f be Function of (n\/{n}),(k\/{l})such that
              A7:x=f & P[f,n\/{n},k\/{l}] & rng (f|n) c=k & f.n=l by A6;
            n+1=n\/{n} & k=k\/{l} by A0,Lm1,Lm2;
            then ex f' be Function of n+1,k st f=f' & 
              f' is onto "increasing & (n < n+1 implies f'"{f'.n}<>{n}) by A7;
            hence thesis by A7,NAT_1:38;  
          end;
      end;
      F2=FF2
      proof
        thus F2 c=FF2
          proof
            let x such that A6:x in F2;
            consider f be Function of n,k such that
              A7:x=f & f is onto "increasing by A6;
            P[x,n,k] by A7;
            hence thesis by A7;
          end;
        thus FF2 c= F2
          proof
            let x such that A6:x in FF2;
            consider f be Function of n,k such that
              A7:x=f & P[f,n,k] by A6;
            ex g be Function of n,k st g=f & 
              g is onto "increasing & (n < n implies g"{g.n}<>{n}) by A7;
            hence thesis by A7;
          end;  
        end;
      hence thesis by A4,A5;               
    end;
  end;
  hence thesis;
end;

reserve D for non empty set,
        F,G,H for XFinSequence of D,
        Fn for XFinSequence of NAT,
        b for BinOp of D,
        d,d1,d2 for Element of D;

definition let D,F,b;
 assume A1:b has_a_unity or len F >= 1;
 func b "**" F -> Element of D means :Def3:
   it = the_unity_wrt b if b has_a_unity & len F = 0 otherwise
    ex f be Function of NAT,D st f.0 = F.0 &
     (for n st n+1 < len F holds
     f.(n + 1) = b.(f.n,F.(n + 1))) & it = f.(len F-1);
 existence
 proof
   now per cases;
     suppose len F = 0;
      hence thesis by A1;
     end;
     suppose A2: len F <> 0;
       defpred P[Nat] means
         for F st len F = $1 & len F <> 0
           ex d be Element of D,f be Function of NAT,D st
             f.0 = F.0 &
             (for n st n+1 < len F holds f.(n + 1) = b.(f.n,F.(n + 1))) &
             d = f.(len F-1);
       A3:P[0];
       A4:for k st P[k] holds P[k + 1]
         proof
           let k such that A5:P[k];
           let F such that A6:len F = k + 1 & len F <> 0;
           reconsider G = F|k as XFinSequence of D by AFINSQ_1:16;
           k<k+1 by NAT_1:38;
           then A7:len G = k by A6,AFINSQ_1:14;
           now per cases;
             suppose A8: len G = 0;
             0 in len F by A6,A7,A8,TARSKI:def 1,CARD_5:1;
             then 0 in dom F by AFINSQ_1:def 1;
             then
X:            F.0 in rng F by FUNCT_1:def 5;
             rng F c= D by ORDINAL1:def 8;
             then F.0 in D by X;
             then reconsider f = NAT --> F.0 as Function of NAT,D
                by FUNCOP_1:57;
A10:            for x st x in NAT holds f.x = F.0 by FUNCOP_1:13;
               take d = f.0,f;
               thus f.0 = F.0 by A10;
               thus for n st  n+1 < len F holds
                 f.(n + 1) = b.(f.n,F.(n + 1)) by A6,A7,A8,NAT_1:39;
               k<k+1 by NAT_1:38;
               hence d = f.(len F-1) by A6,A8,AFINSQ_1:14;
             end;
             suppose A8:len G <> 0;
               then consider
                 d be Element of D,f be Function of NAT,D such that
                 A9: f.0 = G.0 and
                 A10:(for n st n+1<len G holds f.(n + 1)=b.(f.n,G.(n + 1)))and
                 A11:d = f.(len G-1) by A5,A7;
               k < len F & len F= dom F by A6,NAT_1:38,AFINSQ_1:def 1; 
               then k in dom F by EULER_1:1;
               then F.k in rng F & rng F c= D
                 by FUNCT_1:def 5,ORDINAL1:def 8;
               then reconsider d1 = F.k as Element of D;
               deffunc F(Element of NAT) = f.$1;
               consider h be Function of NAT,D such that
                 A12: h.k = b.(d,d1) and
                 A13: for n be Element of NAT st n <> k holds
                   h.n = F(n) from FUNCT_2:sch 10;
               take a = h.k, h;                   
               0 < k by A7,A8,NAT_1:18;
               then h.0=f.0 & 0 in len G & len G=dom G
                 by A13,A7,EULER_1:1,AFINSQ_1:def 1;
               hence h.0 =F.0 by A9,FUNCT_1:70;
               thus for n st n+1 < len F holds
                 h.(n + 1) = b.(h.n,F.(n + 1))
               proof
                 let n such that A14:n+1 < len F;
                 A15:n+1 <= len G by NAT_1:38,A6,A7,A14;
                 now per cases by A15,REAL_1:def 5;
                   suppose A16: n+1 = len G;
                     n+1=k & (n+1)-1=len G - 1 & n<k & (n+1)-1=n
                       by A7,A16,NAT_1:38;
                     hence thesis by A11,A12,A13;                    
                   end;
                   suppose A14:n+1 < len G;
                     n+1 in len G by A14,EULER_1:1;
                     then n+1 in dom G & n<=n+1 by AFINSQ_1:def 1,NAT_1:29;
                     then G.(n+1)=F.(n+1) & f.n=h.n & f.(n+1)=h.(n+1)
                       by A13,A7,A14,FUNCT_1:68;
                     hence thesis by A10,A14;
                   end;
                 end;
                 hence thesis;
               end;              
               thus a = h.(len F-1) by A6;
             end;
           end;
           hence thesis;
         end;
       for k holds P[k] from NAT_1:sch 1(A3,A4);
       hence thesis by A2;
     end;
    end;
    hence thesis;  
  end;
 uniqueness
 proof
   let d1,d2 be Element of D;
   thus b has_a_unity & len F=0 & d1 = the_unity_wrt b & d2 = the_unity_wrt b
     implies d1 = d2;
   assume A2:not b has_a_unity or len F <> 0;
   given f1 be Function of NAT,D such that
     A3:f1.0 = F.0 and
     A4:for n st n+1<len F holds f1.(n + 1) = b.(f1.n,F.(n + 1)) and
     A5:d1 = f1.(len F-1);
   given f2 be Function of NAT,D such that
     A6:f2.0 = F.0 and
     A7:for n st n+1<len F holds f2.(n + 1) = b.(f2.n,F.(n + 1)) and
     A8:d2 = f2.(len F-1);    
   defpred P[Nat] means $1+1 <= len F implies f1.$1 = f2.$1;
   A9:P[0] by A3,A6;
   A10:for n st P[n] holds P[n + 1]
     proof
       let n such that A11:P[n];
       assume (n+1)+1 <= len F;
       then n+1<=len F & n+1<len F by NAT_1:38;
       then f1.n=f2.n & f2.(n+1)=b.(f2.n,F.(n+1)) & f1.(n+1)=b.(f1.n,F.(n+1))
         by A11,A4,A7;
       hence thesis;
     end;
   A11:for n holds P[n] from NAT_1:sch 1(A9,A10);
   0< len F by A1,A2,NAT_1:18;
   then (len F-1) +1 =len F & len F-1 is Nat by NAT_1:60;
   hence d1=d2 by A5,A8,A11;
 end;
 consistency;
end;

theorem Th45:
  b "**" <%d%> = d
proof
  len<%d%> = 1 by AFINSQ_1:37;
  then ex f be Function of NAT,D st f.0=<%d%>.0&
    (for n st n+1 < len <%d%> holds f.(n+1) = b.(f.n,<%d%>.(n+1)))&
    b "**" <%d%>=f.(1-1) by Def3;
  hence thesis by AFINSQ_1:38;
end;

theorem Th46:
  b has_a_unity or len F > 0 implies b "**" (F ^ <% d %>) = b.(b "**" F,d)
proof
  assume A1:b has_a_unity or len F > 0;
  now per cases;
    suppose A2:len F<0+1;
      then len F<=0 by NAT_1:38;
      then A3:len F=0 by NAT_1:18;
      then F={} by Th33;
      then b "**" F = the_unity_wrt b & F ^ <% d %>=<%d%>
        by A1,A3,Def3,AFINSQ_1:32;
      then b "**" (F ^<% d %>)=d & b.(b "**" F,d)=d
        by A1,A2,Th45,SETWISEO:23, NAT_1:38;
      hence thesis;
    end;
    suppose A2:len F>=1;
      then consider f be Function of NAT,D such that
        A3:f.0 = F.0 and
        A4:for n st n+1 < len F holds f.(n+1)=b.(f.n,F.(n+1)) and
        A5:b "**" F = f.(len F-1) by Def3;
      reconsider lenF1=len F-1 as Nat by A2,NAT_1:61;         
      set G = F ^ <% d %>;
      A6:len G=len F+len <%d%> by AFINSQ_1:20
          .=len F+1 by AFINSQ_1:37;
      then 1 <= len G by NAT_1:37;
      then consider f1 be Function of NAT,D such that
        A7:f1.0 = G.0 and
        A8:for n st n+1 < len G holds f1.(n+1)=b.(f1.n,G.(n+1)) and
        A9:b "**" G = f1.(len G-1) by Def3;  
      defpred P[Nat] means $1+1 < len G implies f.$1 = f1.$1;
      0 in len F by A2,EULER_1:1;
      then 0 in dom F by AFINSQ_1:def 1;
      then A10:P[0] by A3,A7,AFINSQ_1:def 4;
      A11:for n st P[n] holds P[n + 1]
      proof
        let n such that A12:P[n];
        set n1=n+1;assume n1+1<len G;
        then n1<len G & (n1+1)+(-1)<(len F+1)+(-1) by A6,NAT_1:38,REAL_1:67;
        then A13:f1.n1=b.(f1.n,G.(n+1)) & f.n1=b.(f.n,F.n1) & n1 in len F &
          f.n=f1.n by A4,A8,A12,EULER_1:1;
        then n1 in dom F by AFINSQ_1:def 1;
        hence thesis by A13,AFINSQ_1:def 4;
      end;
      A12:for n holds P[n] from NAT_1:sch 1(A10,A11);
      lenF1+1<len G & lenF1+1=len G-1 by A6,NAT_1:38;
      then b "**" G = b.(f1.(lenF1),G.(lenF1+1)) & G.(len F)=d &
        f1.(lenF1)=f.(lenF1) by A12,A8,A9,AFINSQ_1:40;            
      hence thesis by A5;
    end;
  end;
  hence thesis;
end;

theorem Th47:
  F <> <%>D implies ex G,d st F = G^<%d%>
proof
  assume F <> <%>D;
  then consider G be XFinSequence,x such that
    A1:F=G^<%x%> by AFINSQ_1:44;
  len <%x%> =1 by AFINSQ_1:37;
  then A2:len G + 1 = len F & len G < len G+1 & dom F=len F
    by A1,AFINSQ_1:def 1,20,NAT_1:38;
  A3:rng G c= D
    proof
      let y such that A4:y in rng G;
      consider x such that
        A5:x in dom G & G.x=y by A4,FUNCT_1:def 5;
      dom G=len G by AFINSQ_1:def 1;
      then dom G c= dom F  & x is Nat by A2,A5,CARD_1:56;
      then F.x in rng F & rng F c= D & F.x=G.x
        by A1,A5,FUNCT_1:def 5,ORDINAL1:def 8,AFINSQ_1:def 4;
      hence thesis by A5;
    end;
  len G in dom F by A2,EULER_1:1;
  then F.(len G) in rng F & rng F c= D by FUNCT_1:def 5,ORDINAL1:def 8;
  then G is XFinSequence of D & x is Element of D
    by A1,A3,AFINSQ_1:40,ORDINAL1:def 8;
  hence thesis by A1;  
end;
    
scheme Sch5{D()->non empty set, P[set]}:
  for F be XFinSequence of D() holds P[F]
 provided
   A1:P[<%>D()] and
   A2:for F be XFinSequence of D(),d be Element of D()
      st P[F] holds P[F^<%d%>]
proof
  defpred R[set] means
  for F be XFinSequence of D() st len F = $1 holds P[F];
  A3:R[0] by A1,Th33;
  A4:for n st R[n] holds R[n+1]
    proof
      let n such that A5:for F be XFinSequence of D() st len F=n holds P[F];
      let F be XFinSequence of D();
      assume A6:len F = n + 1;
      then F<><%>D() by Th33;
      then consider G be XFinSequence of D(), d be Element of D() such that
        A7:F = G^<%d%> by Th47;
      len<%d%>=1 by AFINSQ_1:38;
      then len F = len G + 1 by A7,AFINSQ_1:20;
      then len G = n by A6,XCMPLX_1:2;
      then P[G] by A5;
      hence thesis by A2,A7;
    end;
  A8:for i holds R[i] from NAT_1:sch 1(A3,A4);
  let F be XFinSequence of D();
  len F=len F;
  hence thesis by A8;
end;
          
theorem
 b is associative & (b has_a_unity or len F >= 1 & len G >= 1) implies
   b "**" (F ^ G) = b.(b "**" F,b "**" G)
proof
  defpred P[XFinSequence of D] means
    for F,b st b is associative & (b has_a_unity or len F >= 1 & len $1 >= 1)
      holds b "**" (F^$1)=b.(b "**" F,b "**" $1);
  A1:P[<%>D]
    proof
      let F,b;
      assume A2:b is associative &
        (b has_a_unity or len F >= 1 & len <%>D >= 1);
      thus b "**" (F ^ <%>D) = b "**" F by AFINSQ_1:32
                      .= b.(b "**" F,the_unity_wrt b) by A2,Th33,SETWISEO:23
                      .= b.(b "**" F,b "**" <%>D) by A2,Def3,CARD_1:47;
    end;
  A2:for G,d st P[G] holds P[G ^ <%d%>]
    proof
      let G,d such that A3:P[G];
      let F,b such that A4:b is associative &
        (b has_a_unity or len F >= 1 & len(G ^ <% d %>) >= 1);
      now per cases;
        suppose A5:len G<>0;
          A6:b "**" (F ^ (G ^ <% d %>)) = b.(b "**" (F ^ G),d)
          proof
            len G>0 by A5,NAT_1:18;
            then b has_a_unity or len F>=1&len (F^G)=len F+len G &
              len F+len G >len F+0 by A4,AFINSQ_1:20,REAL_1:67;
            then b.(b "**" (F ^ G),d)=b "**" ((F ^ G)^<%d%>) by Th46;
            hence thesis by AFINSQ_1:30;
          end;
          A7:len G>=1 by A5,NAT_1:39;
          then b "**" (F ^ (G ^ <% d %>))=b.(b.(b "**" F,b "**" G),d)
            by A3,A4,A6
                  .= b.(b "**" F,b.(b "**" G,d)) by A4,BINOP_1:def 3
                  .= b.(b "**" F,b "**" (G ^ <% d %>)) by A7,Th46;          
          hence thesis;
        end;
        suppose len G=0;
           then A5: G = {} by Th33;
           hence b "**" (F ^(G ^ <% d %>)) = b "**"(F^<% d %>) by AFINSQ_1:32
                  .= b.(b "**" F,d) by A4,Th46
                  .= b.(b "**" F,b "**" <%d%>) by Th45
                  .= b.(b "**" F,b "**" (G ^ <% d %>)) by A5,AFINSQ_1:32;
        end;
      end;
      hence thesis;      
    end;
  for G holds P[G] from Sch5(A1,A2);
  hence thesis;
end;
  
definition let D; let d,d1;
  redefine func <%d,d1%> -> XFinSequence of D;
  coherence
  proof
    <%d,d1%>=<%d%>^<%d1%> by AFINSQ_1:def 6;
    hence thesis;
  end;
  let d2;
  redefine func <%d,d1,d2%> -> XFinSequence of D;
  coherence
  proof
    <%d,d1,d2%>=<%d%>^<%d1%>^<%d2%> by AFINSQ_1:def 7;
    hence thesis;
  end;
end;  
  
theorem
  b "**" <%d1,d2%> = b.(d1,d2)
  proof
    len <%d1,d2%>=2 by AFINSQ_1:42;
    then consider f be Function of NAT,D such that
      A1:f.0 = <%d1,d2%>.0 and
      A2:for n st n+1 < 2 holds f.(n + 1) = b.(f.n,<%d1,d2%>.(n + 1)) and
      A3:b "**" <%d1,d2%> = f.(2-1) by Def3;
    f.(0+1)=b.(f.0,<%d1,d2%>.(0+1)) & <%d1,d2%>.0=d1 & <%d1,d2%>.1=d2
      by A2,AFINSQ_1:42;
    hence thesis by A1,A3;
  end;
  
theorem
  b "**" <%d,d1,d2%> = b.(b.(d,d1),d2)
  proof
    set F=<%d,d1,d2%>;
    len F=3 by AFINSQ_1:43;
    then consider f be Function of NAT,D such that
      A1:f.0 = F.0 and
      A2:for n st n+1 < 3 holds f.(n + 1) = b.(f.n,F.(n + 1)) and
      A3:b "**" F = f.(3-1) by Def3;
    f.(0+1)=b.(f.0,F.(0+1)) & f.(1+1)=b.(f.1,F.(1+1)) & F.0=d & F.1=d1 & F.2=d2
      by A2,AFINSQ_1:43;
    hence thesis by A1,A3;
  end;

definition let Fn;
  func Sum(Fn) -> Nat equals :Def4:
    addnat "**" Fn;
  coherence;
end;

definition let Fn,x;
  redefine func Fn.x -> Nat;
  coherence
  proof
    per cases;
      suppose x in dom Fn;
        then Fn.x in rng Fn & rng Fn c= NAT by FUNCT_1:def 5,ORDINAL1:def 8;
        hence Fn.x is Nat;
      end;
      suppose not x in dom Fn;
        hence thesis by FUNCT_1:def 4,NAT_1:18;
      end;
    end;
  end;      
  
theorem Th51:
  (for n st n in dom Fn holds Fn.n <= k)
    implies
      Sum (Fn)<=len Fn * k
proof
  assume A1:for n st n in dom Fn holds Fn.n <= k;
  now per cases by NAT_1:18;
    suppose dom Fn=0;
      then len Fn=0 &  Sum Fn= addnat "**" Fn by AFINSQ_1:def 1,Def4;
      hence thesis by BINOP_2:5,Def3;
    end;
    suppose dom Fn>0;
      then A2:len Fn>0 by AFINSQ_1:def 1;
      then consider f be Function of NAT,NAT such that
        A3:f.0 = Fn.0 and
        A4:(for n st n+1 < len Fn holds f.(n + 1) = addnat.(f.n,Fn.(n + 1)))and
        A5:addnat "**" Fn= f.(len Fn-1) by Def3;
      defpred P[Nat] means
        $1 < len Fn implies f.$1 <=($1+1)*k;
      A6:P[0]
        proof
          assume 0<len Fn;
          then 0< dom Fn by AFINSQ_1:def 1;
          then 0 in dom Fn by EULER_1:1;
          hence thesis by A1,A3;
        end;
      A7:for m st P[m] holds P[m+1]
        proof
          let m such that A8:P[m];
          assume m + 1 < len Fn;
          then m < len Fn & m+1 in len Fn & len Fn=dom Fn &
            f.(m+1)=addnat.(f.m,Fn.(m+1))
            by NAT_1:38,A4,EULER_1:1,AFINSQ_1:def 1;
          then f.m <=(m+1)*k & Fn.(m+1) <= k &
            f.(m + 1) = f.m + Fn.(m+1) by A1,A8,BINOP_2:def 23;
          then f.(m+1) <=(m+1) * k +k by REAL_1:55;
          hence thesis;
        end;
      A8:for m holds P[m] from NAT_1:sch 1(A6,A7);
      reconsider lenFn1=len Fn -1 as Nat by A2,NAT_1:60;
      lenFn1<lenFn1+1 by NAT_1:38;
      then addnat "**" Fn <= (lenFn1+1) * k by A5,A8;
      hence thesis by Def4;
    end;
  end;
  hence thesis;
end;

theorem Th52:
  (for n st n in dom Fn holds Fn.n >= k)
    implies
      Sum (Fn) >= len Fn * k
proof
  assume A1:for n st n in dom Fn holds Fn.n >= k;
  now per cases by NAT_1:18;
    suppose dom Fn=0;
      then len Fn=0 &  Sum Fn= addnat "**" Fn by AFINSQ_1:def 1,Def4;
      hence thesis by BINOP_2:5,Def3;
    end;
    suppose dom Fn>0;
      then A2:len Fn>0 by AFINSQ_1:def 1;
      then consider f be Function of NAT,NAT such that
        A3:f.0 = Fn.0 and
        A4:(for n st n+1 < len Fn holds f.(n + 1) = addnat.(f.n,Fn.(n + 1)))and
        A5:addnat "**" Fn= f.(len Fn-1) by Def3;
      defpred P[Nat] means
        $1 < len Fn implies f.$1 >= ($1+1)*k;
      A6:P[0]
        proof
          assume 0<len Fn;
          then 0< dom Fn by AFINSQ_1:def 1;
          then 0 in dom Fn by EULER_1:1;
          hence thesis by A1,A3;
        end;
      A7:for m st P[m] holds P[m+1]
        proof
          let m such that A8:P[m];
          assume m + 1 < len Fn;
          then m < len Fn & m+1 in len Fn & len Fn=dom Fn &
            f.(m+1)=addnat.(f.m,Fn.(m+1))
            by NAT_1:38,A4,EULER_1:1,AFINSQ_1:def 1;
          then f.m >=(m+1)*k & Fn.(m+1) >= k &
            f.(m + 1) = f.m + Fn.(m+1) by A1,A8,BINOP_2:def 23;
          then f.(m+1) >=(m+1) * k +k by REAL_1:55;
          hence thesis;
        end;
      A8:for m holds P[m] from NAT_1:sch 1(A6,A7);
      reconsider lenFn1=len Fn -1 as Nat by A2,NAT_1:60;
      lenFn1<lenFn1+1 by NAT_1:38;
      then addnat "**" Fn >= (lenFn1+1) * k by A5,A8;
      hence thesis by Def4;
    end;
  end;
  hence thesis;      
end;

theorem Th53:
  (len Fn > 0 & ex x st x in dom Fn & Fn.x = k)
    implies
      Sum Fn >= k
proof
  assume that A1:len Fn > 0 and A2: ex x st x in dom Fn & Fn.x = k;
  consider x such that
    A3:x in dom Fn & Fn.x = k by A2;
  reconsider x as Nat by A3;
  reconsider lenFn1=len Fn-1 as Nat by A1,NAT_1:60;  
  consider f be Function of NAT,NAT such that
    A4:f.0 = Fn.0 and
    A5:for n st n+1 < len Fn holds f.(n + 1) = addnat.(f.n,Fn.(n + 1))and
    A6:addnat "**" Fn= f.(len Fn-1) by A1,Def3; 
  defpred P[Nat] means
    x <= $1 & $1 < len Fn implies f.$1 >= k;
  A7:P[x]
    proof
      now per cases by NAT_1:18;
        suppose A8:x=0;
          assume x <= x & x < len Fn;
          thus f.x>=k by A3,A4,A8;
        end;
        suppose x>0;
          then reconsider x1=x-1 as Nat by NAT_1:60;
          assume x <= x & x < len Fn;
          then x1+1 < len Fn;
          then f.x = addnat.(f.x1,Fn.x) by A5;
          then f.x=f.x1+Fn.x & f.x1>=0 by NAT_1:18,BINOP_2:def 23;
          then f.x>=k+0 by A3,REAL_1:55;
          hence f.x>=k;
        end;
      end;
      hence thesis;
    end;
  A8:for m st m>=x & P[m] holds P[m+1]
    proof
      let m such that A9:m>=x & P[m];
      assume x <= m+1 & m+1 < len Fn;
      then f.(m + 1) = addnat.(f.m,Fn.(m + 1)) & m <len Fn by A5,NAT_1:38;
      then f.(m+1)=f.m+Fn.(m+1) & f.m >=k & Fn.(m+1)>=0
        by A9,BINOP_2:def 23,NAT_1:18;
      then f.(m+1) >= k+0 by REAL_1:55;
      hence thesis;
    end;
  A9:for m st m>=x holds P[m] from INT_2:sch 1(A7,A8);  
  x < dom Fn & dom Fn=lenFn1+1 by A3,EULER_1:1,AFINSQ_1:def 1;
  then x<=lenFn1 & lenFn1 < lenFn1+1 by NAT_1:38;
  then addnat "**" Fn >=k by A6,A9;
  hence thesis by Def4;  
end;

theorem
  Sum Fn=0 iff (len Fn=0 or for n st n in dom Fn holds Fn.n=0)
proof
  A1:(len Fn=0 or for n st n in dom Fn holds Fn.n=0) implies Sum Fn=0
  proof
    assume A2:len Fn=0 or for n st n in dom Fn holds Fn.n=0;
    now per cases;
      suppose len Fn=0;
        then len Fn=0 & Sum Fn= addnat "**" Fn by Def4;
        hence thesis by BINOP_2:5,Def3;
      end;
      suppose len Fn<>0;
        then (for n st n in dom Fn holds Fn.n<=0) &
        (for n st n in dom Fn holds Fn.n >= 0) by A2;
        then Sum Fn <= len Fn*0 & Sum Fn >= len Fn*0 by Th51,Th52;
        hence thesis;
      end;
    end;
    hence thesis;    
  end;
  Sum Fn=0 implies (len Fn=0 or for n st n in dom Fn holds Fn.n=0)
  proof
    assume A2:Sum Fn=0;
    now per cases by NAT_1:18;
      suppose len Fn=0;
        hence thesis;
      end;
      suppose A3:len Fn>0;
        assume not (len Fn=0 or for n st n in dom Fn holds Fn.n=0);
        then consider n such that
          A4:n in dom Fn & Fn.n <>0;
        Sum Fn >=Fn.n by A3,A4,Th53;
        hence contradiction by A2,A4,NAT_1:18; 
      end;
    end;
    hence thesis;
  end;
  hence thesis by A1;
end;

theorem Th55:
  for f be Function, n holds
    union (rng (f|n)) \/ f.n=union rng (f|(n+1))
proof
  let f be Function,n;
  thus union (rng (f|n))\/f.n c=union rng (f|(n+1))
    proof
      let x such that A2:x in union (rng (f|n))\/f.n;
      now per cases by A2,XBOOLE_0:def 2;
        suppose x in union rng (f|n);
          then consider Y be set such that
            A3:x in Y & Y in rng (f|n) by TARSKI:def 4;
          consider X be set such that
            A4:X in dom (f|n) & (f|n).X=Y by A3,FUNCT_1:def 5;
          n<=n+1 by NAT_1:29;
          then n c= n+1 by CARD_1:56;
          then X in dom f /\n & dom f /\n c= dom f /\(n+1)
              by XBOOLE_1:26,A4,FUNCT_1:68;
          then X in dom f /\(n+1);
          then X in dom (f|(n+1)) by FUNCT_1:68;
          then (f|(n+1)).X in rng (f|(n+1)) &
            (f|(n+1)).X=f.X & (f|n).X=f.X by A4,FUNCT_1:def 5,68;
          hence thesis by A3,A4,TARSKI:def 4;
        end;
        suppose A3:x in f.n;
          n<n+1 by NAT_1:38;
          then n in dom f & n in n+1 by A3,FUNCT_1:def 4,EULER_1:1;
          then n in dom f /\ (n+1) by XBOOLE_0:def 3;
          then n in dom (f|(n+1)) by FUNCT_1:68;
          then (f|(n+1)).n in rng (f|(n+1)) &
            (f|(n+1)).n=f.n by FUNCT_1:def 5,68;         
          hence thesis by A3,TARSKI:def 4;
        end;
      end;
      hence thesis;
    end;
    thus union rng (f|(n+1))c=union (rng (f|n))\/f.n
    proof
      let x such that A2:x in union rng (f|(n+1));
      consider Y be set such that
        A3:x in Y & Y in rng (f|(n+1)) by A2,TARSKI:def 4;
      consider X be set such that
        A4:X in dom (f|(n+1)) & (f|(n+1)).X=Y by A3,FUNCT_1:def 5;
      X in dom f /\(n+1) by A4,FUNCT_1:68;
      then A5:X in n+1 & X in dom f by XBOOLE_0:def 3;
      n+1 is Subset of NAT by Th8;
      then reconsider X as Nat by A5;
      X < n+1 by A5,EULER_1:1;
      then A6:X<=n by NAT_1:38;
      now per cases by A6,REAL_1:def 5;
        suppose X < n;
          then X in n by EULER_1:1;
          then X in dom f/\n by A5,XBOOLE_0:def 3;
          then X in dom (f|n) by FUNCT_1:68;
          then (f|n).X=f.X & f.X=(f|(n+1)).X &
            (f|n).X in rng (f|n) by A4,FUNCT_1:68,def 5;
          then x in union rng (f|n) by A3,A4,TARSKI:def 4;
          hence thesis by XBOOLE_0:def 2;
        end;
        suppose X=n;
          then X=n & f.X=(f|(n+1)).X by A4,FUNCT_1:68;
          hence thesis by A3,A4,XBOOLE_0:def 2;
        end;
      end;
      hence thesis;
    end;
end;

scheme Sch6{D()->non empty set,n()->Nat,P[set,set]}:
  ex p be XFinSequence of D() st dom p = n() &
                     for k st k in n() holds P[k,p.k]
 provided
  A1:for k st k in n() ex x be Element of D() st P[k,x]
proof
  A2:for k be set st k in n() ex x be set st x in D() & P[k,x]
    proof
      let k be set such that A3:k in n();
      n() is Subset of NAT by Th8;
      then reconsider k'=k as Nat by A3;
      ex x be Element of D() st P[k',x] by A1,A3;
      hence thesis;
    end;
  consider f be Function of n(),D() such that
  A3:for x st x in n() holds P[x,f.x] from FUNCT_2:sch 1(A2);
  dom f=n() by FUNCT_2:def 1;
  then reconsider p=f as XFinSequence by AFINSQ_1:7;
  take p;
  rng f c= D() & dom f=n() by FUNCT_2:def 1;
  hence thesis by A3,ORDINAL1:def 8;  
end;

scheme Sch7{D()->non empty set,F()-> XFinSequence of D()}:
    ex CardF be XFinSequence of NAT st
      dom CardF = dom F()
      & (for i st i in dom CardF holds CardF.i=Card (F().i))
      & Card union rng F() = Sum(CardF)
provided      
  A1:for i st i in dom F() holds F().i is finite and
  A2:for i,j st i in dom F() & j in dom F() & i<>j holds F().i misses F().j
  proof
    defpred FF[Nat,set] means $2=Card (F().$1);
    A3:for k st k in len F() ex x be Element of NAT st FF[k,x]
      proof
        let k such that A4:k in len F();        
        k in dom F() by A4,AFINSQ_1:def 1;
        then F().k is finite by A1;
        then consider m such that
          A5:(F().k),m are_equipotent by CARD_1:74;
        Card (F().k)=Card m & Card m =m by A5,CARD_1:21,66;
        hence thesis;    
      end;
    consider CardF be XFinSequence of NAT such that
      A4:dom CardF=len F() & for k st k in len F() holds FF[k,CardF.k]
         from Sch6(A3);
    take CardF;
    thus A5:dom CardF = dom F() by A4,AFINSQ_1:def 1;
    thus for i st i in dom CardF holds CardF.i=Card (F().i) by A4;  
    now per cases;
      suppose A6:len CardF=0;
        then dom CardF={} by RELAT_1:60,Th33;
        then union rng F() is empty & addnat "**" CardF = the_unity_wrt addnat
          by A6,ZFMISC_1:2,RELAT_1:65,A5,Def3;
        then Card (union rng F())=0 & Sum(CardF)=0
          by Th33,Def4,BINOP_2:5;
        hence Card union rng F() = Sum(CardF);
      end;
      suppose A6:len CardF<>0;
        then consider f be Function of NAT,NAT such that
          A7:f.0 = CardF.0 &
            (for n st n+1<len CardF holds f.(n+1)=addnat.(f.n,CardF.(n + 1)))&
             addnat "**" CardF = f.(len CardF-1) by Def3;           
        defpred P[Nat] means
          $1<len CardF implies
          Card union rng (F()|($1+1))=f.$1 &
          union rng (F()|($1+1)) is finite;
        A8:P[0]
        proof
          assume 0<len CardF;
          dom F()/\0={};
          then dom (F()|0)={} & 0<len CardF by A6,FUNCT_1:68,NAT_1:18;
          then union rng (F()|0)={} & 0 in len CardF &
            union rng (F()|(0+1)) = union rng (F()|(0))\/F().0
            by Th55,ZFMISC_1:2,EULER_1:1,RELAT_1:65;
          then union rng (F()|(0+1))=F().0 & 0 in dom CardF
                 by AFINSQ_1:def 1;
          hence thesis by A1,A4,A5,A7;
          end;
        A9:for k st P[k] holds P[k + 1]
        proof
          let k such that A10:P[k];
          set k1=k+1;
          assume that A11:k1<len CardF;           
          set Fk1=F()|k1;
          A12:union (rng Fk1) misses F().k1
            proof
              assume union (rng Fk1) meets F().k1;
              then consider x such that
                A13:x in union (rng Fk1)/\F().k1 by XBOOLE_0:4;
              x in union (rng Fk1) by A13,XBOOLE_0:def 3;
              then consider Y be set such that
                A14:x in Y & Y in rng Fk1 by TARSKI:def 4;
              consider X be set such that
                A15:X in dom Fk1 & Fk1.X=Y by A14,FUNCT_1:def 5;
              X in dom F()/\k1 by A15,FUNCT_1:68;
              then X in k1 & k1 is Subset of NAT by XBOOLE_0:def 3,Th8;
              then reconsider X as Nat;
              X in dom F()/\k1 by A15,FUNCT_1:68;
              then A16:X in dom F() & X in k1 by XBOOLE_0:def 3;
              then k1<len CardF & X <k1 by A11,EULER_1:1;
              then k1 in len CardF & X<=k & k<k1
                  by NAT_1:38,EULER_1:1;
              then k1 in dom F() & X<>k1 by A5,AFINSQ_1:def 1;
              then Fk1.X=F().X & F().X misses F().k1
                  by A2,A15,A16,FUNCT_1:68;
              then Y misses F().k1 & x in F().k1 by A13,A15,XBOOLE_0:def 3;
              hence contradiction by A14,XBOOLE_0:3; 
            end;
          set rFk1=rng Fk1;set rFk2=rng (F()|(k1+1));
          reconsider urFk1=union rFk1 as finite set by A10,A11,NAT_1:38;
          k1 in len CardF by A11,EULER_1:1;
          then k1 in dom F() by A5,AFINSQ_1:def 1;
          then reconsider Fk1=F().k1 as finite set by A1;
              urFk1 \/Fk1=union rFk2 by Th55;
          then reconsider urFk2=union rFk2 as finite set;
          len CardF = len F() by A4,AFINSQ_1:def 1;
          then k<len CardF & k1 in len F() & k1 <len CardF
                by A11,NAT_1:38,EULER_1:1;
          then f.k1=addnat.(f.k,CardF.k1) & card Fk1=CardF.k1 by A4,A7;
          then f.k1=f.k + card Fk1 & card (urFk1 \/ Fk1) = f.k + card Fk1
            by BINOP_2:def 23,A10,A11,A12,NAT_1:38,CARD_2:53;
          hence thesis by Th55;                         
        end;
        A10:for k being Nat holds P[k] from NAT_1:sch 1(A8,A9);
        0<len CardF by A6,NAT_1:18;
        then reconsider C1=len CardF-1 as Nat by NAT_1:60;
        dom F() c= len CardF by A5,AFINSQ_1:def 1;         
        then C1 <C1+1 & F()|len CardF =F() by RELAT_1:97,NAT_1:38;
        then Card union rng F()=addnat "**" CardF by A10,A7;         
        hence thesis by Def4;
      end;
    end;
  hence thesis;
end;

scheme Sch8{X,Y()->finite set,x()->set,P[set],f()->Function of card Y(),Y()}:
    ex F be XFinSequence of NAT st
      dom F = card Y() &
      Card{g where g is Function of X(),Y():P[g]} = Sum(F) &
      for i st i in dom F holds F.i = Card{g where g is Function of X(),Y():
                                         P[g] & g.x()=f().i}
provided
  A0:f() is onto one-to-one and                                       
  A1:Y() is non empty and
  A2:x() in X()
proof
  consider n such that
    A3:Y(),n are_equipotent by CARD_1:74;    
  defpred FF[Nat,set] means
    $2={g where g is Function of X(),Y():P[g]& g.x()=f().$1};
  A4:for k st k in n ex x be Subset of Funcs(X(),Y()) st FF[k,x]
  proof
    let k such that k in n;        
    set F0={g where g is Function of X(),Y():P[g]& g.x()=f().k};
    F0 c= Funcs(X(),Y())
    proof
      let x be set such that A6:x in F0;
      ex g be Function of X(),Y() st x=g & P[g]& g.x()=f().k by A6;
      hence thesis by A1,FUNCT_2:11;    
    end;
    hence thesis;
  end;
  consider F be XFinSequence of bool Funcs(X(),Y()) such that
    A5:dom F = n & for k st k in n holds FF[k,F.k] from Sch6(A4);
  A6:for i st i in dom F holds F.i is finite
  proof
    let i such that A7:i in dom F;
    F.i in rng F & rng F c= bool Funcs(X(),Y())
      by A7,FUNCT_1:def 5,ORDINAL1:def 8;
    then F.i c= Funcs(X(),Y()) & Funcs(X(),Y()) is finite
      by FRAENKEL:16;
    hence thesis by FINSET_1:13;
  end;
  A7:for i,j st i in dom F & j in dom F & i<>j holds F.i misses F.j
  proof
    let i,j such that A8:i in dom F & j in dom F & i<>j;
    assume F.i meets F.j;
    then consider x such that
      A9:x in F.i/\F.j by XBOOLE_0:4;
    x in F.i by A9,XBOOLE_0:def 3;
    then x in {g where g is Function of X(),Y():P[g]& g.x()=f().i}
      by A5,A8;  
    then consider gi be Function of X(),Y() such that
      A10:x=gi & P[gi]& gi.x()=f().i;  
    x in F.j by A9,XBOOLE_0:def 3;
    then x in {g where g is Function of X(),Y():P[g]& g.x()=f().j}
      by A5,A8;  
    then consider gj be Function of X(),Y() such that
      A11:x=gj & P[gj]& gj.x()=f().j;       
    dom f()= card Y() & card Y()=card n & card n=n
      by A1,A3,FUNCT_2:def 1,CARD_1:21,66;
    hence contradiction by A0,A8,A5,A10,A11,FUNCT_1:def 8;      
  end;
  consider CardF be XFinSequence of NAT such that
    A8:dom CardF = dom F and
    A9: (for i st i in dom CardF holds CardF.i=Card (F.i)) and
    A10: Card union rng F = Sum(CardF) from Sch7(A6,A7);
  take CardF;
  card Y()=card n & card n=n by A3,CARD_1:21,66;
  hence dom CardF = card Y() by A8,A5;
  A11:for i st i in dom CardF holds
      CardF.i = Card{g where g is Function of X(),Y():P[g] & g.x()=f().i}
  proof
    let i such that A12:i in dom CardF;
    F.i={g where g is Function of X(),Y():P[g] & g.x()=f().i}
     by A8,A5,A12;
    hence thesis by A9,A12;
  end;
  thus Card{g where g is Function of X(),Y():P[g]} = Sum(CardF)
  proof
    set G={g where g is Function of X(),Y():P[g]};
    A12:union rng F c=G
    proof
      let x such that A13:x in union rng F;
      consider Y be set such that
        A14:x in Y & Y in rng F by A13,TARSKI:def 4;
      consider X be set such that
        A15:X in dom F & F.X=Y by A14,FUNCT_1:def 5;
      Y={g where g is Function of X(),Y():P[g] & g.x()=f().X} by A5,A15;
      then ex gX be Function of X(),Y() st x=gX& P[gX] &gX.x()=f().X by A14;
      hence thesis;
    end;
    G c= union rng F
    proof
      let x such that A13:x in G;
      consider g be Function of X(),Y() such that
        A14:x=g  & P[g] by A13;
      dom g=X() by A1,FUNCT_2:def 1;
      then g.x() in rng g by A2,FUNCT_1:def 5;
      then g.x() in Y() & rng f()=Y() by A0,FUNCT_2:def 3;
      then consider y such that
        A15:y in dom f() & f().y=g.x() by FUNCT_1:def 5;
      y in card Y() & card Y()=card n & card n=n
        by A3,CARD_1:21,66,A15;
      then F.y={g1 where g1 is Function of X(),Y():P[g1] & g1.x()=f().y}
        & F.y in rng F by A5,FUNCT_1:def 5;
      then g in F.y & F.y in rng F by A14,A15;
      hence thesis by A14,TARSKI:def 4;
    end;
    hence thesis by A10,A12,XBOOLE_0:def 10;
  end;
  thus for i st i in dom CardF holds
  CardF.i=Card{g where g is Function of X(),Y():P[g]&g.x()=f().i} by A11;
end;

theorem Th56:
  k * (n block k)=
  Card{f where f is Function of n+1,k: f is onto "increasing & f"{f.n}<>{n}}
proof
  now per cases by NAT_1:18;
    suppose A1:k=0;
      set F={f where f is Function of n+1,k:
                             f is onto "increasing & f"{f.n}<>{n}};
      set F1={f where f is Function of n+1,k: f is onto "increasing};
      A2:F c= F1
      proof
        let x such that A3:x in F;
        ex f be Function of n+1,k st x=f
          & f is onto "increasing & f"{f.n}<>{n} by A3;
        hence thesis;
      end;
      Card F1=(n+1) block k by Def2;
      then Card F1=0 by A1,Th31;
      then F1 is empty by Th33;
      then F is empty by A2,XBOOLE_1:3;          
      hence thesis by A1,Th33;
    end;
    suppose A1:k>0;
      defpred P[set] means
        ex f be Function of n+1,k st f=$1& f is onto "increasing&f"{f.n}<>{n};
      card k,k are_equipotent & card k=k by CARD_1:66;
      then consider f be Function such that
        A2:f is one-to-one & dom f = card k & rng f = k by WELLORD2:def 4;
      reconsider f as Function of card k,k by A2,FUNCT_2:3;
      A3:f is onto one-to-one by A2,FUNCT_2:def 3;
      A4:k is non empty by A1;
      n<n+1 by NAT_1:38;
      then A5:n in n+1 by EULER_1:1;
      consider F be XFinSequence of NAT such that
        A6:dom F = card k and
        A7:Card{g where g is Function of n+1,k:P[g]} = Sum(F) and
        A8:for i st i in dom F holds
              F.i = Card{g where g is Function of n+1,k:P[g] & g.n=f.i}
              from Sch8(A3,A4,A5);
      for i st i in dom F holds F.i = n block k
        proof
          let i such that A10:i in dom F;
          A11:f.i in rng f & rng f=k & k is Subset of NAT
            by A2,A6,A10,FUNCT_1:def 5,Th8;
          then reconsider fi=f.i as Nat;
          set F={g where g is Function of n+1,k:g is onto "increasing &
                                                g"{g.n}<>{n} & g.n=fi};
          set F1={g where g is Function of n+1,k:P[g] & g.n=fi};
          set F2={g where g is Function of n,k:g is onto "increasing};
          A12:F c= F1
            proof
               let x such that A13:x in F;
               ex g be Function of n+1,k st
                x=g &g is onto "increasing & g"{g.n}<>{n} & g.n=fi by A13;
               hence thesis;
            end;
          F1 c= F
            proof
              let x such that A13:x in F1;
              ex g be Function of n+1,k st
                x=g & P[g] & g.n=fi by A13;
              hence thesis;
            end;
          then F=F1 & fi<k by A11,A12,EULER_1:1,XBOOLE_0:def 10;
          then Card F1=Card F2 & Card F2= n block k by Th44,Def2;
          hence thesis by A8,A10;
        end;        
        then (for i st i in dom F holds F.i <= n block k)&
        (for i st i in dom F holds F.i >= n block k);
        then Sum F <= len F * (n block k) &
             Sum F >= len F * (n block k) & len F = card k & card k =k
          by A6,AFINSQ_1:def 1,CARD_1:66,Th51,Th52;
        then A9:Sum F= k*(n block k) by AXIOMS:21;
        set G={g where g is Function of n+1,k:P[g]};
        set G1={g where g is Function of n+1,k:
                     g is onto "increasing & g"{g.n}<>{n}};   
        A10:G1 c= G
            proof
               let x such that A11:x in G1;
               ex g be Function of n+1,k st
                x=g &g is onto "increasing & g"{g.n}<>{n} by A11;
               hence thesis;
            end;
          G c= G1
            proof
              let x such that A11:x in G;
              ex g be Function of n+1,k st x=g & P[g] by A11;
              hence thesis;
            end;
        hence thesis by A7,A9,A10,
          XBOOLE_0:def 10;
     end;
   end;
   hence thesis;
 end;

