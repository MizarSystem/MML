::     Recursive dependence      ::
::  number Stirling second kind  ::
:::::::::::::::::::::::::::::::::::

theorem Th57:
  (n+1) block (k+1) = (k+1)*(n block (k+1)) + (n block k)
proof
  set F={f where f is Function of n+1,k+1:f is onto "increasing};
  set F1={f where f is Function of n+1,k+1:f is onto "increasing&f"{f.n}={n}};
  set F2={f where f is Function of n+1,k+1:f is onto "increasing&f"{f.n}<>{n}};
  A1:F c= F1\/F2
  proof
    let x such that A2:x in F;
    consider f be Function of n+1,k+1 such that
    A3:f =x & f is onto "increasing by A2;
    f"{f.n}={n} or f"{f.n}<>{n};
    then f in F1 or f in F2 by A3; 
    hence thesis by A3,XBOOLE_0:def 2;
  end;
  F1\/F2 c= F
  proof
    let x such that A2:x in F1\/F2;
    x in F1 or x in F2 by A2,XBOOLE_0:def 2;
    then (ex f be Function of n+1,k+1 st f = x & f is onto "increasing & 
      f"{f.n}={n})
      or (ex f be Function of n+1,k+1 st f = x & f is onto "increasing & 
      f"{f.n}<>{n});
    hence thesis;    
  end;
  then A2:F1\/F2=F by A1,XBOOLE_0:def 10;
  A3:F1 misses F2
  proof
    assume F1 meets F2;
    then consider x such that
      A4:x in F1/\F2 by XBOOLE_0:4;
    x in F1 & x in F2 by A4,XBOOLE_0:def 3;
    then (ex f be Function of n+1,k+1 st f = x & f is onto "increasing & 
      f"{f.n}={n}) & (ex f be Function of n+1,k+1 st f = x &
      f is onto "increasing & f"{f.n}<>{n});
    hence contradiction;
  end;
  A4:F1 c= Funcs(n+1,k+1)
  proof
    let x such that A5:x in F1;
    ex f be Function of n+1,k+1 st f = x & f is onto "increasing & f"{f.n}={n} 
      by A5;
    hence thesis by FUNCT_2:11;
  end;
  A5:F2 c= Funcs(n+1,k+1)
  proof
    let x such that A5:x in F2;
    ex f be Function of n+1,k+1 st f = x & f is onto "increasing &
      f"{f.n}<>{n} by A5;
    hence thesis by FUNCT_2:11;
  end;
  Funcs(n+1,k+1) is finite by FRAENKEL:16;
  then reconsider F1,F2 as finite set by A4,A5,FINSET_1:13;  
  card F1=Card{f where f is Function of n,k: f is onto "increasing}
    by Th43;
  then card F1= n block k & card F2= (k+1) * (n block (k+1)) &
    Card F =(n+1) block (k+1) by Th56,Def2;  
  hence thesis by A2,A3,CARD_2:53;
end;

theorem Th58:        
  n>=1 implies n block 2 = 1/2 * (2 |^ n - 2)
proof
  defpred P[Nat] means ($1) block 2 = 1/2 * (2 |^ $1-2);
  A1:P[1]
    proof
      2 |^1= 2 by NEWTON:10;
      hence thesis by Th29;
    end;
  A2:for k st k>=1 & P[k] holds P[k + 1]
    proof
      let k such that A3:k >=1 & P[k];
      (k+1) block 2 = 2*( k block (1+1)) + ( k block 1) by Th57
      .=2 * (1/2 * (2 |^ k - 2)) + 1 by A3,Th32,CARD_2:20
      .=1/2* (2* 2 |^ k - 2)
      .=1/2* (2 |^ (k+1)-2) by NEWTON:11;
      hence thesis;                 
    end;
  for k st k>=1 holds P[k] from INT_2:sch 1(A1,A2);
  hence thesis;   
end;

theorem Th59:
  n >= 2 implies n block 3 = 1/6 * ( 3 |^ n - 3 * 2 |^ n + 3 )
proof
  defpred P[Nat] means $1 block 3=1/6*(3|^$1-3*2|^$1+3);
  A1:P[2]
    proof
      1/6*(3|^2-3*2|^2+3)=1/6*(3*3-3*2|^2+3) by WSIERP_1:2
      .=1/6*(3*3-3*(2*2)+3) by WSIERP_1:2
      .=2 block 3 by Th29;
      hence thesis;  
    end;
  A2:for k st k>=2 & P[k] holds P[k+1]
    proof
      let k such that A3:k>=2 & P[k];
      k>=1 by A3,AXIOMS:22;
      then k block 2 = 1/2 * (2 |^ k - 2) by Th58;
      hence (k+1) block 3 = 3*(k block (2+1)) +(1/2*(2|^k-2)) by Th57
      .=3* (1/6* (3|^k-3*2|^k+3)) + (1/2*(2|^k-2)) by A3
      .=1/6*((3*3|^k)-3*2*2|^k +3)
      .=1/6*(3|^(k+1)-3*(2*2|^k) +3) by NEWTON:11
      .=1/6*(3|^(k+1)-3*2|^(k+1) +3) by NEWTON:11;           
    end;
  for k st k>=2 holds P[k] from INT_2:sch 1(A1,A2);
  hence thesis;     
end;

Lm03: n|^3=n*n*n
proof
  thus n|^3=n|^(2+1)
  .=n|^2*n by NEWTON:11
  .=n*n*n by WSIERP_1:2;  
end; 

theorem
  n >= 3 implies n block 4 = 1/24 * ( 4 |^ n - 4 * 3 |^ n + 6* 2|^ n - 4 )
proof
  defpred P[Nat] means $1 block 4=
    1/24 * ( 4 |^ $1 - 4 * 3 |^ $1 + 6* 2|^ $1 - 4 );
  A1:P[3]
    proof
      1/24*(4|^3-4*3|^3+6*2|^3-4)=
        1/24*(4*4*4-4*3|^3+6*2|^3-4) by Lm03
      .=1/24*(64-4*(3*3*3)+6*2|^3-4) by Lm03
      .=1/24*(64-4*27+6*(2*2*2)-4) by Lm03   
      .=3 block 4 by Th29;
      hence thesis;  
    end;
  A2:for k st k>=3 & P[k] holds P[k+1]
    proof
      let k such that A3:k>=3 & P[k];
      k>=2 by A3,AXIOMS:22;
      then k block 3 = 1/6 * (3|^k-3*2|^k+3) by Th59;
      hence (k+1) block 4 = 4*(k block (3+1)) +(1/6*(3|^k-3*2|^k+3)) by Th57
      .=4*(1/24*(4|^k-4*3|^k+6*2|^k-4)) +(1/24*4*(3|^k-3*2|^k+3)) by A3
      .=1/24*((4*4|^k)- 4*(3*3|^k)+ 6*(2*2|^k) - 4)
      .=1/24*(4|^(k+1)- 4*(3*3|^k)+ 6*(2*2|^k) - 4) by NEWTON:11
      .=1/24*(4|^(k+1)- 4*3|^(k+1)+ 6*(2*2|^k) - 4) by NEWTON:11
      .=1/24*(4|^(k+1)- 4*(3|^(k+1))+ 6*2|^(k+1) - 4) by NEWTON:11;           
    end; 
  for k st k>=3 holds P[k] from INT_2:sch 1(A1,A2);
  hence thesis;    
end;

theorem Th61:
  3! = 6 & 4! = 24
proof
  (2+1)!=2!*3 & (3+1)!=3!*4 by NEWTON:21;
  hence thesis by NEWTON:20;
end;

theorem Th62:
  n choose 1=n &
  n choose 2=n*(n-1)/2 &
  n choose 3=n*(n-1)*(n-2)/6 &
  n choose 4=n*(n-1)*(n-2)*(n-3)/24
proof
  now per cases by CQC_THE1:4;
    suppose n=0;
      hence thesis by NEWTON:def 3;
    end;    
    suppose n=1;
      hence thesis by NEWTON:31,def 3;
    end;
    suppose n=2;
      hence thesis by NEWTON:31,33,def 3;
    end;    
    suppose A1:n=3;
      then reconsider n1=n-1,n2=n-2 as Nat by NAT_1:60;
      n2!<>0 by NEWTON:23;
      then A2:(n2!)/(n2!)=1 by XCMPLX_1:60;
      (n1+1)!=(n1)!*n & (n2+1)!=(n2)!*(n2+1) by NEWTON:21;
      then n!=(n2)!*(n-1)*n;     
      then n choose 2=((n2)!*((n-1)*n))/((n2!)*(2!)) by A1,NEWTON:def 3
      .=((n2!)/(n2!)*((n-1)*n))/2 by XCMPLX_1:84,NEWTON:20
      .=((n-1)*n)/2 by A2;
      hence thesis by A1,NEWTON:31,33,def 3;
    end;
    suppose A1:n>3;
      then n>1 & n>2 & n>3 & n>=3+1 by NAT_1:38,AXIOMS:22;
      then reconsider n1=n-1,n2=n-2,n3=n-3,n4=n-4 as Nat by NAT_1:61;
      n2!<>0 & n3!<>0 & n4!<>0 by NEWTON:23;
      then A2:(n2!)/(n2!)=1 & (n3!)/(n3!)=1 & (n4!)/(n4!)=1 by XCMPLX_1:60;
      A3:(n1+1)!=(n1)!*n & (n2+1)!=(n2)!*(n2+1) & (n3+1)!=(n3)!*(n3+1) &
        (n4+1)!=(n4)!*(n4+1) by NEWTON:21;
      n>=2 by A1,AXIOMS:22;
      then A4:n choose 2=((n2)!*n1*n)/((n2!)*(2!)) by A3,NEWTON:def 3
      .=((n2)!*(n1*n))/((n2!)*(2!))
      .=((n2!)/(n2!)*(n1*n))/2 by XCMPLX_1:84,NEWTON:20
      .=n*n1/2 by A2;
      A5:n choose 3=((n3)!*n2*n1*n)/((n3!)*(3!))
        by A1,A3,NEWTON:def 3
      .=((n3)!*(n2*n1*n))/((n3!)*(3!))
      .=((n3!)/(n3!)*(n2*n1*n))/6 by XCMPLX_1:84,Th61
      .=n*n1*n2/6 by A2;
      n>=3+1 by A1,NAT_1:38;
      then A6:n choose 4=((n4)!*n3*n2*n1*n)/((n4!)*(4!))
        by A3,NEWTON:def 3
      .=((n4!)*(n3*n2*n1*n))/((n4!)*(4!))
      .=((n4!)/(n4!)*(n3*n2*n1*n))/24 by XCMPLX_1:84,Th61
      .=n*n1*n2*n3/24 by A2;
      n>=1 by A1,AXIOMS:22;
      hence thesis by A4,A5,A6,NEWTON:33;
    end;
  end;
  hence thesis;      
end;

theorem Th63:
  (n + 1) block n = (n + 1) choose 2
proof
  defpred P[Nat] means ($1+1) block $1 = ($1+1) choose 2;
  A1:P[0]
    proof
      1 block 0 =0 & 1 choose 2 =0 by Th31,NEWTON:def 3;
      hence thesis;
    end;
  A2:for k st P[k] holds P[k+1]
    proof
      let k such that A3:P[k];set k1=k+1;
      thus (k1+1) block k1=k1*(k1 block k1)+(k1 block k) by Th57
      .=k1*1+(k1 choose 2) by A3,Th26,CARD_2:20
      .=k1+k1*(k1-1)/2 by Th62
      .=(k1+1)*(k1+1-1)/2
      .=(k1+1) choose 2 by Th62;
    end;
  for k holds P[k] from NAT_1:sch 1(A1,A2);
  hence thesis;
end;    
        
theorem
  (n + 2) block n = 3*((n + 2) choose 4) + ((n + 2) choose 3)
proof
  defpred P[Nat] means ($1+2)block $1=3*(($1+2)choose 4)+(($1+2) choose 3);
  A1:P[0]
    proof
      2 choose 4=0 & 2 choose 3=0 & 2 block 0=0 by NEWTON:def 3,Th31;
      hence thesis;
    end;
  A2:for k st P[k] holds P[k+1]
    proof
      let k such that A3:P[k];set k2=k+2;set k1=k+1;set k3=k2+1;
      A4:k2 block k=3*(k2*(k2-1)*(k2-2)*(k2-3)/24)+(k2 choose 3) by A3,Th62
      .=3*(k2*(k2-1)*(k2-2)*(k2-3)/24)+(k2*(k2-1)*(k2-2)/6) by Th62
      .=k2*(k2-1)*(3*(k2-2)*(k2-3)/24 +4*(k2-2)/24);
      k1*((k1+1) block k1)=k1*(k2 choose 2) by Th63
      .=k1*(k2*(k2-1)/2) by Th62
      .=k2*(k2-1)*(k1*12/24);
      then k3 block k1=
        k2*k1*(k1*12/24)+k2*k1*(3*k*(k2-3)/24+4*k/24) by A4,Th57
      .=3*(k3*(k3-1)*(k3-2)*(k3-3)/24)+k3*k2*k1/6
      .=3*(k3 choose 4) +(k3*(k3-1)*(k3-2)/6) by Th62
      .=3*(k3 choose 4)+(k3 choose 3) by Th62;     
      hence thesis;
    end;
  for k holds P[k] from NAT_1:sch 1(A1,A2);
  hence thesis;
end;
      
theorem Th66:
  for F be Function,y holds
    rng (F|(dom F\F"{y}))=rng F\{y} &
    for x st x<>y holds (F|(dom F\F"{y}))"{x}=F"{x}
proof
  let F be Function,y;
  set D=dom F\F"{y};
  thus rng (F|D)=rng F\{y}
    proof
      A1:rng F\{y} c=rng (F|D)
        proof
          let Fx be set such that A2:Fx in rng F\{y};
          Fx in rng F by A2,XBOOLE_0:def 4;
          then consider x such that
            A3:x in dom F & F.x=Fx by FUNCT_1:def 5;
          not Fx in {y} by A2,XBOOLE_0:def 4;
          then not x in F"{y} by A3,FUNCT_1:def 13;
          then x in D by A3,XBOOLE_0:def 4;
          then x in dom F /\D by A3,XBOOLE_0:def 3;
          then x in dom (F|D) by FUNCT_1:68;
          then (F|D).x in rng (F|D)&(F|D).x=F.x by FUNCT_1:70,def 5;
          hence thesis by A3;
        end;
      rng(F|D) c= rng F\{y}
        proof
          let Fx be set such that A2:Fx in rng (F|D);
          consider x such that
            A3:x in dom (F|D) & Fx=(F|D).x by A2,FUNCT_1:def 5;
          x in dom F /\ D by A3,FUNCT_1:68;
          then x in (dom F/\dom F)\F"{y} by XBOOLE_1:49;
          then x in dom F & not x in F"{y} by XBOOLE_0:def 4;
          then F.x in rng F & not F.x in {y} by FUNCT_1:def 5,def 13;
          then Fx in rng F & not Fx in {y} by A3,FUNCT_1:68;
          hence thesis by XBOOLE_0:def 4;
        end;
      hence thesis  by A1,XBOOLE_0:def 10;
    end;
    let x such that A1:x<>y;
    now let z be set such that A2:z in F"{x};
      A3:z in dom F & F.z in {x} by A2,FUNCT_1:def 13;
      then F.z <> y by A1,TARSKI:def 1;
      then not F.z in {y} by TARSKI:def 1;
      then not z in F"{y} by FUNCT_1:def 13;
      hence z in dom F\F"{y} by A3,XBOOLE_0:def 4;
    end;
    then F"{x} c= D by TARSKI:def 3;  
    hence F"{x} = (F|D)"{x} by TMAP_1:5;
  end;
  
theorem Th67:
  Card X=k+1 & x in X implies Card (X\{x})=k
  proof
    assume A1:Card X=k+1 & x in X;
    then Card X= Card (k+1) by CARD_1:66;
    then X,(k+1)are_equipotent by CARD_1:21;
    then reconsider X'=X as finite set by CARD_1:68;
    reconsider Xx=X'\{x} as finite set;
    {x} c= X by A1,ZFMISC_1:37;
    then {x} /\X ={x} by XBOOLE_1:28;
    then {x} misses Xx & Xx \/ {x} = X by XBOOLE_1:79,51;
    then card {x}+ card Xx= k+1 & card {x}=one by A1,CARD_2:53,CARD_1:50;
    hence thesis by XCMPLX_1:2,CARD_2:20;
  end;  
  
scheme Sch9{P[set],Q[set,Function]}:
  for F be Function st rng F is finite holds P[F]
provided  
A1:P[{}] and
A2:for F be Function st
     for x st x in rng F & Q[x,F] holds P[F|(dom F\F"{x})] holds P[F]
proof
  defpred R[Nat] means
    for F be Function st Card rng F=$1 holds P[F];
  A3:R[0]
  proof
    let F be Function such that A4:Card rng F=0;
    rng F={} by A4,Th33;
    hence thesis by A1,RELAT_1:64;
  end;
  A4:for n st R[n] holds R[n+1]
  proof
    let n such that A5:R[n];
    let F be Function such that A7:Card rng F=n+1;   
    for x st x in rng F & Q[x,F] holds P[F|(dom F\F"{x})]
      proof
        let x such that A8:x in rng F & Q[x,F];
        set D=dom F\F"{x};
        Card (rng F\{x})=n by A7,A8,Th67;
        then Card rng (F|D)=n by Th66;
        hence thesis by A5;
      end;     
    hence thesis by A2;
  end;
  let F be Function such that A5:rng F is finite;
  consider n such that
    A6:rng F,n are_equipotent by A5,CARD_1:74;
  Card n=n by CARD_1:66;
  then A7:Card rng F=n by A6,CARD_1:21;
  for k holds R[k] from NAT_1:sch 1(A3,A4);
  hence thesis by A7;          
end;     
    
theorem Th68:
  for N be Subset of NAT st N is finite
    ex k st for n st n in N holds n<=k
proof
  let N be Subset of NAT such that A1:N is finite;
  consider n such that
    A2:N,n are_equipotent by A1,CARD_1:74;
  consider F be Function such that
    A3:F is one-to-one & dom F = n & rng F = N by A2,WELLORD2:def 4;
  reconsider F as XFinSequence by A3,AFINSQ_1:7;
  reconsider F as XFinSequence of NAT by A3,ORDINAL1:def 8;
  take k=Sum(F);  
  let n such that A4:n in N;
  Card F <>0 by Th33,A3,A4,RELAT_1:60;
  then len F>0 & ex x st x in dom F & F.x=n by A3,A4,FUNCT_1:def 5,NAT_1:18;
  hence thesis by Th53;
end;
  
theorem  Th69:
  for X,Y,x,y st (Y is empty implies X is empty) & not x in X
    for F be Function of X,Y ex G be Function of X\/{x},Y\/{y} st
      G|X = F & G.x=y
proof
  let X,Y,x,y such that
    A1:Y is empty implies X is empty and
    A2:not x in X;
  let F be Function of X,Y;
  deffunc F(set)=y;set X1=X\/{x};set Y1=Y\/{y};
  A3:X c= X1 & Y c= Y1 by XBOOLE_1:7;
  A4:for x' be set st x' in X1\X holds F(x') in Y1
  proof
    y in {y} by TARSKI:def 1;
    hence thesis by XBOOLE_0:def 2;
  end;
  consider G be Function of X1,Y1 such that
    A5:G|X = F & for x' be set st x' in X1\X holds G.x' = F(x')
    from Sch2(A4,A3,A1);
  x in {x} by TARSKI:def 1;
  then x in X1 by XBOOLE_0:def 2;
  then x in X1\X by A2,XBOOLE_0:def 4;
  then G.x=y by A5;
  hence thesis by A5;
end;
  
theorem Th70:
  for X,Y,x,y st (Y is empty implies X is empty)
    for F be Function of X,Y,G be Function of X\/{x},Y\/{y} st
      G|X=F & G.x=y holds
        ( F is onto implies G is onto) &
        ( not y in Y & F is one-to-one implies G is one-to-one)
proof
  let X,Y,x,y such that
    A1:Y is empty implies X is empty;
  let F be Function of X,Y,G be Function of (X\/{x}),(Y\/{y}) such that
    A3:G|X=F and
    A4:G.x=y;
  thus F is onto implies G is onto
    proof
      assume A5:F is onto;
      Y\/{y} c= rng G
      proof
        let Fx be set such that A6:Fx in Y\/{y};
        now per cases by A6,XBOOLE_0:def 2;
          suppose Fx in Y;
            then Fx in rng F by A5,FUNCT_2:def 3;
            then consider x' be set such that
              A7:x' in dom F & F.x'=Fx by FUNCT_1:def 5;  
            x' in X & X c= X\/{x} & dom G=X\/{x}
              by A7,FUNCT_2:def 1,XBOOLE_1:7;
            then x' in dom G & G.x'=F.x'
              by A3,A7,FUNCT_1:68;
            hence thesis by A7,FUNCT_1:def 5;
          end;
          suppose Fx in {y};
            then Fx=y & dom G =X\/{x} & x in {x} & {x}c=X\/{x}
              by TARSKI:def 1,FUNCT_2:def 1,XBOOLE_1:7;
            hence thesis by A4,FUNCT_1:def 5;
          end;
        end;
        hence thesis;
      end;
      then Y\/{y}=rng G by XBOOLE_0:def 10;
      hence thesis by FUNCT_2:def 3;
    end;
  thus not y in Y & F is one-to-one implies G is one-to-one
    proof
      assume A5:not y in Y & F is one-to-one;
      let x1,x2 be set such that A6:x1 in dom G & x2 in dom G & G.x1=G.x2;
      A8:for x' be set st x' in X holds
        x' in dom F & F.x'=G.x' & G.x'<>y
        proof
          let x' be set such that A9:x' in X;
          x' in dom F by A1,A9,FUNCT_2:def 1;
          then F.x'=G.x' & F.x' in rng F by A3,FUNCT_1:def 5,68;
          hence thesis by A5,A9,FUNCT_2:def 1;
        end;
      now per cases by A6,XBOOLE_0:def 2;
        suppose x1 in X & x2 in X;
          then x1 in dom F & x2 in dom F & F.x1=G.x1 & F.x2=G.x2 by A8;
          hence thesis by A6,A5,FUNCT_1:def 8;
        end;
        suppose x1 in X & x2 in {x};
          then G.x2=y & G.x1<>y by A4,A8,TARSKI:def 1;
          hence thesis by A6;
        end;
        suppose x1 in {x} & x2 in X;
          then G.x1=y & G.x2<>y by A4,A8,TARSKI:def 1;
          hence thesis by A6;
        end;
        suppose x1 in {x} & x2 in {x};
          then x=x1 & x=x2 by TARSKI:def 1;
          hence thesis;
        end;
      end;
      hence thesis;
    end;  
  end;
  
Lm4:x in X implies (X\{x})\/{x}=X by ZFMISC_1:140;

theorem Th71:
  for N be finite Subset of NAT holds
    ex Order be Function of N,card N st
      Order is bijective &
      for n,k st n in dom Order & k in dom Order & n<k holds Order.n < Order.k
proof
  defpred P[Nat] means
   for N be finite Subset of NAT st card N = $1 holds
    ex F be Function of N,card N st
      F is  bijective &
      for n,k st n in dom F & k in dom F & n<k holds F.n < F.k;
  A1:P[0]
  proof
    let N be finite Subset of NAT such that A2:card N=0;
    set P={};
    rng P={} & dom P={}& N is empty by A2,Th33;
    then reconsider P as Function of N,card N by A2,FUNCT_2:3;
    take P;
    N is empty by A2,Th33;
    then dom P={} & rng P={} by A2,XBOOLE_1:3;
    then P is one-to-one onto by A2,FUNCT_2:def 3;
    hence P is bijective by FUNCT_2:def 4;
    thus for i,j st i in dom P & j in dom P & i<j holds P.i<P.j by A2,Th33; 
  end;
  A2:for k st P[k] holds P[k+1]
  proof
    let k such that A3:P[k];
    let N be finite Subset of NAT such that A4:card N =k+1;
    defpred M[Nat] means $1 in N;
    consider m' be Nat such that
      A5:for n st n in N holds n<=m' by Th68;
    A6:for n st M[n] holds n<=m' by A5;
    N is non empty by A4,Th33;
    then consider x such that
      A7:x in N by XBOOLE_0:def 1;
    A8:ex n st M[n] by A7;
    consider m such that
      A9:M[m] & for n st M[n] holds n <= m from NAT_1:sch 6(A6,A8);
    set Nm=N\{m};
    card Nm=k by A4,A9,Th67;
    then consider F be Function of Nm,card Nm such that
      A10:F is bijective and
      A11:for n,k st n in dom F & k in dom F & n<k holds F.n < F.k by A3;
    m in {m} by TARSKI:def 1;
    then A12:not m in Nm & (card Nm is empty implies Nm is empty)
      by XBOOLE_0:def 4,Th33;
    then consider G be Function of Nm\/{m},card Nm\/{k} such that
    A13:G|Nm=F & G.m=k by Th69;
    card Nm=k by A4,A9,Th67;
    then F is one-to-one onto & not k in card Nm &
      (card Nm)\/{k}=k+1 by A10,FUNCT_2:def 4,Lm1;
    then A14:G is one-to-one onto & N=Nm\/{m} & (card Nm)\/{k}=card N
      by A4,A9,A12,A13,Lm4,Th70;
    then reconsider G'=G as Function of N,card N;
    take G';
    thus G' is bijective by A14,FUNCT_2:def 4;
    thus for n,k st n in dom G' & k in dom G' & n<k holds G'.n < G'.k
    proof
      let i,j such that A15:i in dom G' & j in dom G' & i<j;
      A16:for i st i in Nm holds i < m & G'.i <k
      proof
        let i such that A17:i in Nm;
        card Nm<>0 by A17,Th33;
        then i in dom F by A17,FUNCT_2:def 1;
        then M[i] & not i in {m} & F.i=G'.i & F.i in rng F & card Nm=k
          by A4,A9,Th67,A13,XBOOLE_0:def 4,FUNCT_1:68,def 5;
        then i <= m & i<>m & G'.i in k by A9,TARSKI:def 1;
        hence i < m & G'.i < k by EULER_1:1,REAL_1:def 5;
      end;
      A17:dom G'=Nm\/{m} by FUNCT_2:def 1;
      now per cases by A15,A17,XBOOLE_0:def 2;
        suppose A17:i in Nm & j in Nm;
          then card Nm is non empty by Th33;
          then A18:i in dom F & j in dom F 
            by A17,FUNCT_2:def 1;
          then F.i=G'.i & F.j=G'.j by FUNCT_1:68,A13;
          hence thesis by A11,A15,A18;
        end;
        suppose i in Nm & j in {m};
          then G'.i<k & G'.j=k by A13,A16,TARSKI:def 1;
          hence thesis;
        end;
        suppose i in {m} & j in Nm;
          then i=m & j<m by A16,TARSKI:def 1;
          hence thesis by A15;
        end;
        suppose i in {m} & j in {m};
          then i=m & j=m by TARSKI:def 1;
          hence thesis by A15;
        end;
      end;
      hence thesis;
    end;
  end;
  for k holds P[k] from NAT_1:sch 1(A1,A2);
  hence thesis;
end;

Lm5:for X be finite set,x st x in X holds card (X\{x}) < card X
proof
  let X be finite set,x such that A1:x in X;
  card X <>0 by A1,Th33;
  then card X>0 by NAT_1:18;
  then reconsider c1=card X-1 as Nat by NAT_1:60;
  card X=c1+1;
  then card (X\{x})=c1 & c1<c1+1 by A1,Th67,NAT_1:38;
  hence thesis;
end;

theorem Th72:
  for X,Y be finite set,F be Function of X,Y st card X=card Y holds
  F is onto iff F is one-to-one
proof
  let X,Y be finite set,F be Function of X,Y such that A1:card X=card Y;
  thus F is onto implies F is one-to-one
  proof
    assume A2:F is onto;
    assume F is not one-to-one;
    then consider x1,x2 be set such that
      A3:x1 in dom F & x2 in dom F & F.x1=F.x2 & x1<>x2 by FUNCT_1:def 8;
    reconsider Xx=X\{x1} as finite set;   
    Y c= F.:Xx
    proof
      let Fy be set such that A4:Fy in Y;
      Fy in rng F by A2,A4,FUNCT_2:def 3;
      then consider y such that
        A5:y in dom F & F.y=Fy by FUNCT_1:def 5;
      now per cases;
        suppose y=x1;
          then Fy=F.x2 & x2 in Xx by A3,A5,ZFMISC_1:64;
          hence thesis by A3,FUNCT_1:def 12;
        end;
        suppose y<>x1;
          then y in Xx by A5,ZFMISC_1:64;
          hence thesis by A5,FUNCT_1:def 12;
        end;
      end;
      hence thesis;
    end;
    then Card Y <=` Card Xx & x1 in X by A3,CARD_2:2;
    then card Y <= card Xx & card Xx < card X by CARD_2:57,Lm5;
    hence thesis by A1;
  end;
  thus F is one-to-one implies F is onto
  proof
    assume A2:F is one-to-one;
    assume F is not onto;
    then not rng F = Y by FUNCT_2:def 3;
    then not Y c= rng F by XBOOLE_0:def 10;
    then consider y such that
      A3:y in Y & not y in rng F by TARSKI:def 3;
    rng F c= Y\{y} & dom F,F.:dom F are_equipotent
      by A2,CARD_1:60,A3,ZFMISC_1:40;
    then card rng F <=card (Y\{y}) & card (Y\{y})< card Y &
      card dom F=card (F.:dom F) & F.:dom F= rng F & dom F=X 
      by CARD_1:81,RELAT_1:146,A3,Lm5,CARD_1:80,FUNCT_2:def 1;
    hence thesis by A1;
  end;
end;

Lm6:
  for n be Nat,N be finite Subset of NAT,
      F be Function of N,card N st
    n in N & F is bijective &
    for n,k st n in dom F & k in dom F & n<k holds F.n < F.k
  ex P be Permutation of N st
    for k st k in N holds
      (k < n implies P.k=F".((F.k)+1)) &
      (k = n implies P.k=F".0) &
      (k > n implies P.k=k)
proof
  let n be Nat,N be finite Subset of NAT,
      F be Function of N,card N such that
    A1:n in N and
    A3:F is  bijective and
    A4:for n,k st n in dom F & k in dom F & n<k holds F.n < F.k;
    defpred P[set,set] means
      for k st k=$1 holds
        (k < n implies $2=F".((F.k)+1)) &
        (k = n implies $2=F".0) &
        (k > n implies $2=k);
    F is onto by A3,FUNCT_2:def 4;
    then F is one-to-one & rng F=card N by A3,FUNCT_2:def 3,def 4;
    then reconsider F'=F" as Function of card N,N by FUNCT_2:31;
    card N is non empty by A1,Th33;    
    then A5:dom F=N & dom F'=card N by A1,FUNCT_2:def 1;   
    A6:for x st x in N ex y st y in N & P[x,y]
      proof
        let x' be set such that A7:x' in N;
        reconsider x=x' as Nat by A7;
        now per cases by REAL_1:def 5;
          suppose A8:x < n;
            set FF=F".((F.x)+1);
            F.x < F.n & F.n in rng F & F is onto
              by A1,A3,A4,A5,A7,A8,FUNCT_1:def 5,FUNCT_2:def 4;
            then (F.x)+1 <=F.n & F.n < card N by EULER_1:1, NAT_1:38;
            then (F.x)+1 < card N by AXIOMS:22;
            then (F.x)+1 in dom F' by A5,EULER_1:1;
            then F'.((F.x)+1) in rng F' & P[x',FF] by A8,FUNCT_1:def 5;
            hence thesis;
          end;
          suppose A8:x = n;
            card N<>0 by A1,Th33;
            then 0< card N by NAT_1:18;
            then 0 in dom F' by A5,EULER_1:1;
            then F'.0 in rng F' by FUNCT_1:def 5;
            then F'.0 in N & P[x',F".0] by A8;
            hence thesis;
          end;
          suppose x>n;
            then x in N & P[x,x] by A7;
            hence thesis;
          end;
        end;
        hence thesis;  
      end;
    consider P be Function of N,N such that
      A7:for x st x in N holds P[x,P.x] from FUNCT_2:sch 1(A6);
    N c= rng P
    proof
      let Px' be set such that A8:Px' in N;
      reconsider Px=Px' as Nat by A8;
      now per cases;
        suppose A9:Px<=n;
          F is one-to-one by A3,FUNCT_2:def 4;
          then rng (F')=N by A5,FUNCT_1:55;
          then consider x such that
            A10:x in dom F' & F'.x=Px by A8,FUNCT_1:def 5;
          card N is Subset of NAT & F is one-to-one by A3,FUNCT_2:def 4,Th8;
          then reconsider x as Nat by A10, A5;
          now per cases by NAT_1:18;
            suppose A11:x=0;
              dom P=N by A1,FUNCT_2:def 1;
              then P.n=F".0 & F'.0=Px & P.n in rng P
                by A1,A7,A10,A11,FUNCT_1:def 5;
              hence thesis;
            end;
            suppose x>0;
              then reconsider x1=x-1 as Nat by NAT_1:60;
               x1<x1+1 & x < card N
              by NAT_1:38,EULER_1:1,A10;
              then x1 < card N & F is onto by AXIOMS:22,A3,FUNCT_2:def 4;
              then x1 in card N & card N=rng F by EULER_1:1,FUNCT_2:def 3;
              then consider y such that
                A11:y in dom F & F.y=x1 by FUNCT_1:def 5;
              reconsider y as Nat by A5,A11;   
              A12:y<n
              proof
                assume y>=n;
                then (y>n or y=n) & F is one-to-one
                  by A3,REAL_1:def 5,FUNCT_2:def 4;
                then x1>=F.n & x1+1>x1 & x in rng F & F is one-to-one &
                  Px in dom F & n in dom F & (Px <n or Px=n)
                  by A8,A9,A10,FUNCT_1:55,A1,A5,A11,A4,NAT_1:38,REAL_1:def 5;
                then x>F.n & F.Px=x & F.Px<=F.n
                  by A4,A10,FUNCT_1:54,AXIOMS:22;
                hence contradiction;
              end;
              F.y=x1 by A11;
              then F".(F.y+1)=Px & y in dom P
                by A5,A11,FUNCT_2:def 1,A10;
              then P.y=Px & P.y in rng P by A7,A12,FUNCT_1:def 5;
              hence thesis;
            end;
          end;
          hence thesis;
        end;
        suppose Px>n;
          then Px in dom P & Px=P.Px by A8,A7,FUNCT_2:def 1;
          hence thesis by FUNCT_1:def 5;
        end;
      end;
      hence thesis;        
    end;
    then N=rng P by XBOOLE_0:def 10;
    then P is onto & card N=card N by FUNCT_2:def 3;
    then P is onto one-to-one by Th72;
    then reconsider P as Permutation of N by FUNCT_2:def 4;
    take P;
    thus thesis by A7;
end;

theorem Th73:
  for F,G be Function,y st y in rng (G*F) & G is one-to-one holds
    ex x st x in dom G & x in rng F & G"{y}={x} & F"{x}=(G*F)"{y}
proof
  let F,G be Function,y such that
  A1:y in rng (G*F) and
  A2:G is one-to-one;
  consider x such that
    A3:x in dom (G*F) & (G*F).x=y by A1,FUNCT_1:def 5;
  A4:F.x in dom G & G.(F.x)=y by A3,FUNCT_1:22,21;
  y in rng G by A1,FUNCT_1:25;
  then consider Fx be set such that
    A5:G"{y}={Fx} by A2,FUNCT_1:144;
  G.(F.x) in {y} & F.x in dom G by A4,TARSKI:def 1;
  then A6:F.x in G"{y} by FUNCT_1:def 13;
  A7:F"{F.x} c= (G*F)"{y}
  proof
    let d be set such that A8:d in F"{F.x};
    F.d in {F.x} by A8,FUNCT_1:def 13;
    then d in dom F & F.d = F.x by A8,TARSKI:def 1,FUNCT_1:def 13;
    then d in dom F & d in dom (G*F) & G.(F.d) in {y}
      by A4,TARSKI:def 1,FUNCT_1:21;
    then d in dom (G*F) & (G*F).d in {y}  by FUNCT_1:23;
    hence thesis by FUNCT_1:def 13;  
  end;
  (G*F)"{y} c= F"{F.x}
  proof
    let d be set such that A8:d in (G*F)"{y};
    d in dom (G*F) & (G*F).d in {y} by A8,FUNCT_1:def 13;
    then A9:d in dom F & F.d in dom G & G.(F.d) in {y} & F.x=Fx
      by A5,A6,FUNCT_1:21,22,TARSKI:def 1;
    then F.d in {F.x} by A5,FUNCT_1:def 13;
    hence thesis by A9,FUNCT_1:def 13;    
  end;
  then x in dom F & F"{F.x} = (G*F)"{y}
    by A7,XBOOLE_0:def 10,A3,FUNCT_1:21;
  then F.x in dom G & F.x in rng F & G"{y}={F.x} & F"{F.x} = (G*F)"{y}
    by A3,A6,A5,TARSKI:def 1,FUNCT_1:21,def 5;
  hence thesis;
end;

