definition
  let f,x0;
  assume
A1: f is_left_convergent_in x0;
  func lim_left(f,x0)->Real means
  :Def7:
  for seq st seq is convergent & lim
seq=x0 & rng seq c= dom f /\ left_open_halfline(x0) holds f/*seq is convergent
  & lim(f/*seq)=it;
  existence by A1,Def1;
  uniqueness
  proof
    defpred X[Element of NAT,real number] means x0-1/($1+1)<$2 & $2<x0 & $2 in
    dom f;
    let g1,g2 such that
A2: for seq st seq is convergent & lim seq=x0 & rng seq c=dom f/\
    left_open_halfline(x0) holds f/*seq is convergent & lim(f/*seq)=g1 and
A3: for seq st seq is convergent & lim seq=x0 & rng seq c=dom f/\
    left_open_halfline(x0) holds f/*seq is convergent & lim(f/*seq)=g2;
A4: now
      let n;
      x0-1/(n+1)<x0 by Lm3;
      then consider g such that
A5:   x0-1/(n+1)<g and
A6:   g<x0 and
A7:   g in dom f by A1,Def1;
      take g;
      thus X[n,g] by A5,A6,A7;
    end;
    consider s be Real_Sequence such that
A8: for n holds X[n,s.n] from FUNCT_2:sch 3(A4);
A9: rng s c=dom f/\left_open_halfline(x0) by A8,Th5;
A10: lim s=x0 by A8,Th5;
A11: s is convergent by A8,Th5;
    then lim(f/*s)=g1 by A10,A9,A2;
