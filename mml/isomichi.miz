theorem Th35:
  for a,b being real number st a >= b holds IRRAT (a,b) = {}
proof
  let a, b be real number;
  assume
A1: a >= b;
  IRRAT (a,b) = IRRAT /\ ]. a, b .[ by BORSUK_5:def 5
    .= IRRAT /\ {} by A1,XXREAL_1:28;
  hence thesis;
