theorem Th19:
  for P being Subset of TOP-REAL 2,
  p1,p2,q1,q2 being Point of TOP-REAL 2 st P is_an_arc_of p1,p2
  & LE q1,q2,P,p1,p2 holds LE q2,q1,P,p2,p1
proof
  let P be Subset of TOP-REAL 2, p1,p2,q1,q2 be Point of TOP-REAL 2;
  assume that
A1: P is_an_arc_of p1,p2 and
A2: LE q1,q2,P,p1,p2;
  thus q2 in P & q1 in P by A2,JORDAN5C:def 3;
  reconsider P9 = P as non empty Subset of TOP-REAL 2 by A1,TOPREAL1:4;
  let f be Function of I[01], (TOP-REAL 2)|P, s1, s2 be Real;
  assume that
A3: f is being_homeomorphism and
A4: f.0 = p2 and
A5: f.1 = p1 and
A6: f.s1 = q2 and
A7: 0 <= s1 and
A8: s1 <= 1 and
A9: f.s2 = q1 and
A10: 0 <= s2 and
A11: s2 <= 1;
A12: 1-0>=1-s1 by A7,XREAL_1:15;
A13: 1-0>=1-s2 by A10,XREAL_1:15;
A14: 1-1<=1-s1 by A8,XREAL_1:15;
A15: 1-1<=1-s2 by A11,XREAL_1:15;
  set Ex = L[01]((0,1)(#),(#)(0,1));
A16: Ex is being_homeomorphism by TREAL_1:21;
  set g = f * Ex;
A17: Ex.(0,1)(#) = 0 by BORSUK_1:def 17,TREAL_1:8,12;
A18: Ex.(#)(0,1) = 1 by BORSUK_1:def 18,TREAL_1:8,12;
  dom f = [#]I[01] by A3,TOPS_2:def 5;
  then rng Ex = dom f by A16,TOPMETR:27,TOPS_2:def 5;
  then dom g = dom Ex by RELAT_1:46;
  then
A19: dom g = [#]I[01] by A16,TOPMETR:27,TOPS_2:def 5;
  reconsider g as Function
  of I[01], (TOP-REAL 2)|P9 by TOPMETR:27;
A20: g is being_homeomorphism by A3,A16,TOPMETR:27,TOPS_2:71;
A21: (1-s1) in dom g by A12,A14,A19,BORSUK_1:86;
A22: (1-s2) in dom g by A13,A15,A19,BORSUK_1:86;
A23: g.0 = p1 by A5,A18,A19,BORSUK_1:def 17,FUNCT_1:22,TREAL_1:8;
A24: g.1 = p2 by A4,A17,A19,BORSUK_1:def 18,FUNCT_1:22,TREAL_1:8;
  reconsider qs1=1-s1,qs2=1-s2 as Point of Closed-Interval-TSpace(0,1)
  by A12,A13,A14,A15,BORSUK_1:86,TOPMETR:27;
A25: Ex.qs1 = (1-(1-s1))*1+(1-s1)*0 by BORSUK_1:def 17,def 18,TREAL_1:8,def 3
    .=s1;
A26: Ex.qs2 = (1-(1-s2))*1+(1-s2)*0 by BORSUK_1:def 17,def 18,TREAL_1:8,def 3
    .=s2;
A27: g.(1-s1) = q2 by A6,A21,A25,FUNCT_1:22;
  g.(1-s2) = q1 by A9,A22,A26,FUNCT_1:22;
  then 1-s2<=1-s1 by A2,A12,A13,A14,A20,A23,A24,A27,JORDAN5C:def 3;
  then s1<=1-(1-s2) by XREAL_1:13;
  hence thesis;
end;

definition
  let P be Subset of TOP-REAL 2, p1,p2,q1 be Point of TOP-REAL 2;
  func L_Segment(P,p1,p2,q1) -> Subset of TOP-REAL 2 equals
  {q : LE q,q1,P,p1,p2};
  coherence
  proof
    {q : LE q,q1,P,p1,p2} c= the carrier of TOP-REAL 2
    proof
      let x;
      assume x in {q : LE q,q1,P,p1,p2};
      then ex q st ( q=x)&( LE q,q1,P,p1,p2);
      hence thesis;
    end;
    hence thesis;
  end;
end;

