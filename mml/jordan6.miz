theorem
  for P being Subset of R^1, r,s being real number st P=].r,s.[ holds P is open
proof
  let P be Subset of R^1, r,s be real number;
  assume
A1: P=].r,s.[;
  {r1:r<r1} c= REAL
  proof
    let x;
    assume x in {r1:r<r1};
    then ex r1 st ( r1=x)&( r<r1);
    hence thesis;
  end;
  then reconsider P1={r1 where r1 is Real:r<r1} as Subset of R^1
  by TOPMETR:24;
  {r2:s>r2} c= REAL
  proof
    let x;
    assume x in {r2:s>r2};
    then ex r2 st ( r2=x)&( s>r2);
    hence thesis;
  end;
  then reconsider P2={r2 where r2 is Real:s>r2} as Subset of R^1
  by TOPMETR:24;
A2: P1 is open by JORDAN2B:31;
A3: P2 is open by JORDAN2B:30;
A4: P c= P1 /\ P2
  proof
    let x;
    assume
A5: x in P;
    then reconsider r1=x as Real by TOPMETR:24;
A6: r<r1 by A1,A5,XXREAL_1:4;
A7: r1<s by A1,A5,XXREAL_1:4;
A8: x in P1 by A6;
    x in P2 by A7;
    hence thesis by A8,XBOOLE_0:def 4;
  end;
  P1 /\ P2 c= P
  proof
    let x;
    assume
A9: x in P1 /\ P2;
    then
A10: x in P1 by XBOOLE_0:def 4;
A11: x in P2 by A9,XBOOLE_0:def 4;
A12: ex r1 st ( r1=x)&( r<r1) by A10;
    ex r2 st ( r2=x)&( s>r2) by A11;
    hence thesis by A1,A12,XXREAL_1:4;
  end;
  then P=P1 /\ P2 by A4,XBOOLE_0:def 10;
  hence thesis by A2,A3,TOPS_1:38;
end;

