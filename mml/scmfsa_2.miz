  hereby
    let c such that
A6: c <> a & c <> b;
    reconsider C = c as Data-Location by Th25;
    thus Exec(Divide(a,b), s).c = Exec(Divide(A,B), S).C by A4,Th79
      .= S.C by A1,A2,A6,AMI_3:12
      .= s.c by Th80;
