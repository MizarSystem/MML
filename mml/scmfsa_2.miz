end;

theorem Th76:
  S = s|SCM-Memory +* (NAT --> I) implies s = s +* S +* s|NAT
proof
A1: dom(NAT --> I) = NAT by FUNCOP_1:19;
A2: dom s = the carrier of SCM+FSA by PARTFUN1:def 4
    .= SCM+FSA-Memory;
  dom(s|NAT) = dom s /\ NAT by RELAT_1:90
    .= NAT by A2,XBOOLE_1:28;
  then
A3: (NAT --> I) +* s|NAT = s|NAT by A1,FUNCT_4:20;
  assume S = s|SCM-Memory +* (NAT --> I);
  hence s +* S +* s|NAT = s +* s|SCM-Memory +* (NAT --> I) +* s|NAT by
FUNCT_4:15
    .= s +* (NAT --> I) +* s|NAT by FUNCT_4:80
    .= s +* ((NAT --> I) +* s|NAT) by FUNCT_4:15
    .= s by A3,FUNCT_4:80;
end;

