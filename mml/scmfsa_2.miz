A1:   a>0_goto lb = A>0_goto La by Def18;
   thus InsCode(a>0_goto lb) = 8 by A1,MCART_1:7;
  end;

theorem
      InsCode (c:=(fa,a)) = 9 by MCART_1:7;

theorem
      InsCode ((fa,a):=c) = 10 by MCART_1:7;

