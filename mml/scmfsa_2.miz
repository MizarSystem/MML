theorem Th98:
  Exec(c:=(g,a), s).IC SCM+FSA = succ IC s & (ex k st k = abs(s.a)
& Exec(c:=(g,a), s).c = (s.g)/.k) & (for b st b <> c holds Exec(c:=(g,a), s).b
  = s.b) & for f holds Exec(c:=(g,a), s).f = s.f
proof
  reconsider p = g as Element of SCM+FSA-Data*-Loc by Def5;
  reconsider mk = a, ml = c as Element of SCM+FSA-Data-Loc by Def4;
  reconsider I = c:=(g,a) as Element of SCM+FSA-Instr;
  reconsider S = s as SCM+FSA-State by PBOOLE:155;
  reconsider J = 9 as Element of Segm 13 by NAT_1:45;
  InsCode I = 9 by RECDEF_2:def 1;
  then consider i being Integer, k such that
A1: k = abs(S.(I int_addr2)) and
A2: i = (S.(I coll_addr1))/.k and
A3: SCM+FSA-Exec-Res(I,S) = SCM+FSA-Chg(SCM+FSA-Chg(S,I int_addr1,i),
  succ IC S) by SCMFSA_1:def 17;
  set S1 = SCM+FSA-Chg(S,I int_addr1,i);
A4: Exec(c:=(g,a), s) = (SCM+FSA-Chg(S1, succ IC S)) by A3,SCMFSA_1:def 18;
  hence Exec(c:=(g,a), s).IC SCM+FSA = succ IC s by Th7,SCMFSA_1:20;
A5: I = [J,{},<*ml,p,mk*>] & I`3_3 = <*ml,p,mk*> by RECDEF_2:def 3;
  then
A6: I int_addr1 = ml by SCMFSA_1:def 10;
A7: I coll_addr1 = p by A5,SCMFSA_1:def 12;
  hereby
    take k;
    thus k = abs(s.a) by A5,A1,SCMFSA_1:def 11;
    thus Exec(c:=(g,a), s).c = S1.ml by A4,SCMFSA_1:21
      .= (s.g)/.k by A2,A6,A7,SCMFSA_1:25;
  end;
  hereby
    let b;
    reconsider mn = b as Element of SCM+FSA-Data-Loc by Def4;
    assume
A8: b <> c;
    thus Exec(c:=(g,a), s).b = S1.mn by A4,SCMFSA_1:21
      .= s.b by A6,A8,SCMFSA_1:26;
  end;
  let f;
  reconsider q = f as Element of SCM+FSA-Data*-Loc by Def5;
  thus Exec(c:=(g,a), s).f = S1.q by A4,SCMFSA_1:22
    .= s.f by SCMFSA_1:27;
end;

theorem Th99:
  Exec((g,a):=c, s).IC SCM+FSA = succ IC s & (ex k st k = abs(s.a)
& Exec((g,a):=c, s).g = s.g+*(k,s.c)) & (for b holds Exec((g,a):=c, s).b = s.b)
  & for f st f <> g holds Exec((g,a):=c, s).f = s.f
proof
  reconsider p = g as Element of SCM+FSA-Data*-Loc by Def5;
  reconsider mk = a, ml = c as Element of SCM+FSA-Data-Loc by Def4;
  reconsider I = (g,a):=c as Element of SCM+FSA-Instr;
  reconsider S = s as SCM+FSA-State by PBOOLE:155;
  reconsider J = 10 as Element of Segm 13 by NAT_1:45;
  InsCode I = 10 by RECDEF_2:def 1;
  then consider F being FinSequence of INT,k such that
A1: k = abs(S.(I int_addr2)) and
A2: F = S.(I coll_addr1)+*(k,S.(I int_addr1)) and
A3: SCM+FSA-Exec-Res(I,S)=SCM+FSA-Chg(SCM+FSA-Chg(S,I coll_addr1,F),succ
  IC S) by SCMFSA_1:def 17;
  set S1 = SCM+FSA-Chg(S,I coll_addr1,F);
A4: Exec((g,a):=c, s) = (SCM+FSA-Chg(S1, succ IC S)) by A3,SCMFSA_1:def 18;
  hence Exec((g,a):=c, s).IC SCM+FSA = succ IC s by Th7,SCMFSA_1:20;
A5: I = [J,{},<*ml,p,mk*>] & I`3_3 = <*ml,p,mk*> by RECDEF_2:def 3;
  then
A6: I coll_addr1 = p by SCMFSA_1:def 12;
A7: I int_addr1 = ml by A5,SCMFSA_1:def 10;
  hereby
    take k;
    thus k = abs(s.a) by A5,A1,SCMFSA_1:def 11;
    thus Exec((g,a):=c, s).g = S1.p by A4,SCMFSA_1:22
      .= s.g+*(k,s.c) by A2,A7,A6,SCMFSA_1:29;
  end;
  hereby
    let b;
    reconsider mn = b as Element of SCM+FSA-Data-Loc by Def4;
    thus Exec((g,a):=c, s).b = S1.mn by A4,SCMFSA_1:21
      .= s.b by SCMFSA_1:31;
  end;
  let f such that
A8: f <> g;
  reconsider q = f as Element of SCM+FSA-Data*-Loc by Def5;
  thus Exec((g,a):=c, s).f = S1.q by A4,SCMFSA_1:22
    .= s.f by A6,A8,SCMFSA_1:30;
end;

theorem Th100:
  Exec(c:=len g, s).IC SCM+FSA = succ IC s & Exec(c:=len g, s).c
  = len(s.g) & (for b st b <> c holds Exec(c:=len g, s).b = s.b) & for f holds
  Exec(c:=len g, s).f = s.f
proof
  reconsider S = s as SCM+FSA-State by PBOOLE:155;
  reconsider p = g as Element of SCM+FSA-Data*-Loc by Def5;
  reconsider I = c:=len g as Element of SCM+FSA-Instr;
  set S1 = SCM+FSA-Chg(S,I int_addr3,len(S.(I coll_addr2)));
  reconsider J = 11 as Element of Segm 13 by NAT_1:45;
  reconsider ml = c as Element of SCM+FSA-Data-Loc by Def4;
A1: InsCode I = 11 by RECDEF_2:def 1;
A2: Exec(c:=len g, s) = SCM+FSA-Exec-Res(I,S) by SCMFSA_1:def 18
    .= (SCM+FSA-Chg(S1, succ IC S)) by A1,SCMFSA_1:def 17;
  hence Exec(c:=len g, s).IC SCM+FSA = succ IC s by Th7,SCMFSA_1:20;
A3: I = [J,{},<*ml,p*>] & I`3_3 = <*ml,p*> by RECDEF_2:def 3;
  then
A4: I int_addr3 = ml by SCMFSA_1:def 13;
A5: I coll_addr2 = p by A3,SCMFSA_1:def 14;
  thus Exec(c:=len g, s).c = S1.ml by A2,SCMFSA_1:21
    .= len(s.g) by A4,A5,SCMFSA_1:25;
  hereby
    let b;
    reconsider mn = b as Element of SCM+FSA-Data-Loc by Def4;
    assume
A6: b <> c;
    thus Exec(c:=len g, s).b = S1.mn by A2,SCMFSA_1:21
      .= s.b by A4,A6,SCMFSA_1:26;
  end;
  let f;
  reconsider q = f as Element of SCM+FSA-Data*-Loc by Def5;
  thus Exec(c:=len g, s).f = S1.q by A2,SCMFSA_1:22
    .= s.f by SCMFSA_1:27;
end;

theorem Th101:
  Exec(g:=<0,...,0>c, s).IC SCM+FSA = succ IC s & (ex k st k =
abs(s.c) & Exec(g:=<0,...,0>c, s).g = k |-> 0) & (for b holds Exec(g:=<0,...,0>
  c, s).b = s.b) & for f st f <> g holds Exec(g:=<0,...,0>c, s).f = s.f
proof
  reconsider p = g as Element of SCM+FSA-Data*-Loc by Def5;
  reconsider ml = c as Element of SCM+FSA-Data-Loc by Def4;
  reconsider I = g:=<0,...,0>c as Element of SCM+FSA-Instr;
  reconsider S = s as SCM+FSA-State by PBOOLE:155;
  reconsider J = 12 as Element of Segm 13 by NAT_1:45;
  InsCode I = 12 by RECDEF_2:def 1;
  then consider F being FinSequence of INT,k such that
A1: k = abs(S.(I int_addr3)) and
A2: F = k |-> 0 and
A3: SCM+FSA-Exec-Res(I,S) = SCM+FSA-Chg(SCM+FSA-Chg(S,I coll_addr2,F),
  succ IC S) by SCMFSA_1:def 17;
  set S1 = SCM+FSA-Chg(S,I coll_addr2,F);
A4: Exec(g:=<0,...,0>c, s) = SCM+FSA-Chg(S1, succ IC S) by A3,SCMFSA_1:def 18;
  hence Exec(g:=<0,...,0>c, s).IC SCM+FSA = succ IC s by Th7,SCMFSA_1:20;
A5: I = [J,{},<*ml,p*>] & I`3_3 = <*ml,p*> by RECDEF_2:def 3;
  then
A6: I coll_addr2 = p by SCMFSA_1:def 14;
  hereby
    take k;
    thus k = abs(s.c) by A5,A1,SCMFSA_1:def 13;
    thus Exec(g:=<0,...,0>c, s).g = S1.p by A4,SCMFSA_1:22
      .= k|->0 by A2,A6,SCMFSA_1:29;
  end;
  hereby
    let b;
    reconsider mn = b as Element of SCM+FSA-Data-Loc by Def4;
    thus Exec(g:=<0,...,0>c, s).b = S1.mn by A4,SCMFSA_1:21
      .= s.b by SCMFSA_1:31;
  end;
  let f such that
A7: f <> g;
  reconsider q = f as Element of SCM+FSA-Data*-Loc by Def5;
  thus Exec(g:=<0,...,0>c, s).f = S1.q by A4,SCMFSA_1:22
    .= s.f by A6,A7,SCMFSA_1:30;
end;

