theorem Th78:
  s1 = s +* S +* s|NAT implies s1.IC SCM+FSA = S.IC SCM
proof
A1: dom S = the carrier of SCM by PARTFUN1:def 4
    .= SCM-Memory;
  dom(s|NAT) = dom s /\ NAT & not IC SCM+FSA in NAT by FUNCT_7:def 1,RELAT_1:90
,SCMFSA_1:5;
  then
A2: not IC SCM+FSA in dom(s|NAT) by XBOOLE_0:def 4;
  assume s1 = s +* S +* s|NAT;
  hence s1.IC SCM+FSA = (s +* S).IC SCM+FSA by A2,FUNCT_4:12
    .= S.IC SCM by A1,Th7,AMI_3:4,FUNCT_4:14;
end;

theorem Th79:
  s1 = s +* S +* s|NAT & A = a implies S.A = s1.a
proof
  assume that
A1: s1 = s +* S +* s|NAT and
A2: A = a;
A3: dom S = the carrier of SCM by PARTFUN1:def 4
    .= SCM-Memory;
A4: a in SCM+FSA-Data-Loc by Def4;
A5: now
    assume a in NAT;
    then SCM+FSA-OK.a = SCM+FSA-Instr by SCMFSA_1:11;
    hence contradiction by A4,SCMFSA_1:10,13;
  end;
  dom(s|NAT) = dom s /\ NAT by RELAT_1:90;
  then not a in dom(s|NAT) by A5,XBOOLE_0:def 4;
  hence s1.a = (s +* S).a by A1,FUNCT_4:12
    .= S.A by A2,A3,FUNCT_4:14;
end;

theorem Th80:
  S = s|SCM-Memory +* (NAT --> I) & A = a implies S.A = s.a
proof
  assume S = s|SCM-Memory +* (NAT --> I);
  then s = s +* S +* s|NAT by Th76;
  hence thesis by Th79;
end;

  [0,{},{}] in SCM+FSA-Instr by SCMFSA_1:4;
  then the haltF of SCM+FSA = [0,{},{}] by FUNCT_7:def 1;
  then
LmX: halt SCM+FSA = [0,{},{}];

registration
  cluster SCM+FSA -> proper-halt
   realistic IC-Ins-separated definite steady-programmed;
  coherence
  proof
    JumpPart halt SCM+FSA is empty by LmX,RECDEF_2:def 2;
    then halt SCM+FSA is ins-loc-free by COMPOS_1:def 37;
   hence SCM+FSA is proper-halt by COMPOS_1:def 39;
    thus SCM+FSA is realistic
    proof
      assume the Instruction-Counter of SCM+FSA in NAT;
      hence contradiction by FUNCT_7:def 1,SCMFSA_1:5;
    end;
    ObjectKind IC SCM+FSA = NAT by FUNCT_7:def 1,SCMFSA_1:5,9;
    hence SCM+FSA is IC-Ins-separated by COMPOS_1:def 6;
    thus SCM+FSA is definite
    proof
      let l be Element of NAT;
      thus thesis by SCMFSA_1:11;
    end;
    let s be State of SCM+FSA, i be Instruction of SCM+FSA, l be
    Element of NAT;
A1: i in SCM-Instr \/ { [L,{},<*dB,fA,dA*>] : L in {9,10} } or
i in { [K,{},<*dC,fB*>] : K in {11,12} } by XBOOLE_0:def 3;
    reconsider l9 = l as Element of NAT;
    reconsider S = s as Element of product SCM+FSA-OK by PBOOLE:155;
    reconsider I = i as Element of SCM+FSA-Instr;
    per cases by A1,XBOOLE_0:def 3;
    suppose
      i in SCM-Instr;
      then reconsider I = i as Instruction of SCM;
      reconsider S = s|SCM-Memory +* (NAT --> I) as State of SCM by Th73
;
      dom s = the carrier of SCM+FSA by PARTFUN1:def 4
        .= {NAT} \/ SCM+FSA-Data-Loc \/ SCM+FSA-Data*-Loc \/ NAT by SCMFSA_1:8;
      then dom(s|NAT)=dom s /\ NAT & l in dom s by RELAT_1:90,XBOOLE_0:def 3
;
      then
A3:   l in dom(s|NAT) by XBOOLE_0:def 4;
      thus Exec(i,s).l = (s +* Exec(I,S) +* s|NAT).l by Th75
        .= (s|NAT).l by A3,FUNCT_4:14
        .= s.l by A3,FUNCT_1:70;
    end;
    suppose
      i in { [J,{},<*dB,fA,dA*>] : J in {9,10} };
      then consider J,dB,dA,fA such that
A4:   i = [J,{},<*dB,fA,dA*>] and
A5:   J in {9,10};
      now
        per cases by A5,TARSKI:def 2;
        suppose
          J = 9;
          then InsCode I = 9 by A4,RECDEF_2:def 1;
          then consider i being Integer, k such that
          k = abs(S.(I int_addr2)) and
          i = (S.(I coll_addr1))/.k and
A6:       SCM+FSA-Exec-Res(I,S) = SCM+FSA-Chg(SCM+FSA-Chg(S,I
          int_addr1,i),succ IC S) by SCMFSA_1:def 17;
          thus SCM+FSA-Exec-Res(I,S).l9 = SCM+FSA-Chg(S,I int_addr1,i).l9 by A6
,SCMFSA_1:23
            .= S.l9 by SCMFSA_1:28;
        end;
        suppose
          J = 10;
          then InsCode I = 10 by A4,RECDEF_2:def 1;
          then consider f being FinSequence of INT,k such that
          k = abs(S.(I int_addr2)) and
          f = S.(I coll_addr1)+*(k,S.(I int_addr1)) and
A7:       SCM+FSA-Exec-Res(I,S) = SCM+FSA-Chg(SCM+FSA-Chg(S,I
          coll_addr1,f),succ IC S) by SCMFSA_1:def 17;
          thus SCM+FSA-Exec-Res(I,S).l9 = SCM+FSA-Chg(S,I coll_addr1,f).l9 by
A7,SCMFSA_1:23
            .= S.l9 by SCMFSA_1:32;
        end;
      end;
      hence thesis by SCMFSA_1:def 18;
    end;
    suppose
      i in { [K,{},<*dC,fB*>] : K in {11,12} };
      then consider J,dC,fB such that
A8:   i = [J,{},<*dC,fB*>] and
A9:   J in{11,12};
      now
        per cases by A9,TARSKI:def 2;
        suppose
          J = 11;
          then InsCode I = 11 by A8,RECDEF_2:def 1;
          hence SCM+FSA-Exec-Res(I,S).l9 = SCM+FSA-Chg(SCM+FSA-Chg(S,I
          int_addr3,len(S.(I coll_addr2))), succ IC S).l9 by SCMFSA_1:def 17
            .= SCM+FSA-Chg(S,I int_addr3,len(S.(I coll_addr2))).l9 by
SCMFSA_1:23
            .= S.l9 by SCMFSA_1:28;
        end;
        suppose
          J = 12;
          then InsCode I = 12 by A8,RECDEF_2:def 1;
          then consider f being FinSequence of INT,k such that
          k = abs(S.(I int_addr3)) and
          f = k |-> 0 and
A10:      SCM+FSA-Exec-Res(I,S) = SCM+FSA-Chg(SCM+FSA-Chg(S,I
          coll_addr2,f),succ IC S) by SCMFSA_1:def 17;
          thus SCM+FSA-Exec-Res(I,S).l9 = SCM+FSA-Chg(S,I coll_addr2,f).l9 by
A10,SCMFSA_1:23
            .= S.l9 by SCMFSA_1:32;
        end;
      end;
      hence thesis by SCMFSA_1:def 18;
    end;
  end;
