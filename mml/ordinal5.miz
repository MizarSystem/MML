    assume
A1: b <> {} & b is limit_ordinal;
    deffunc F(Ordinal) = epsilon_$1;
    deffunc D(Ordinal,Ordinal-Sequence) = lim $2;
    deffunc C(Ordinal,Ordinal) = $2|^|^omega;
