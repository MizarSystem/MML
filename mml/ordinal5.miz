Z3:   P[b] by Z0,Z2;
      per cases by ORDINAL3:10;
      suppose
B1:     b = 0;
        reconsider A = <%n*^exp(omega, omega-exponent(a))%> as
        non empty Cantor-normal-form Ordinal-Sequence by Z2,Th44;
        take A;
        thus a = n*^exp(omega, omega-exponent(a)) by Z2,B1,ORDINAL2:44
        .= Sum^ A by Th51;
