      abs(r)*||.f/.x1-f/.x2.||<p/abs(r)*abs(r) by A9,A11,A12,A13,A14,XREAL_1:70
;
      hence ||.(r(#)f)/.x1-(r(#)f)/.x2.|| < p by A9,A15,XCMPLX_1:88;
