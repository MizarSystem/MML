A7:0<r/2 by SEQ_2:3; then
   consider s such that
A8: 0<s &
    for x1,x2 be Point of CNS st
     x1 in X /\ X1 & x2 in X /\X1 & ||.x1-x2.||<s holds
      ||.f1/.x1-f1/.x2.||<r/2 by A3,Def2;
   consider g be Real such that
A9: 0<g &
    for x1,x2 be Point of CNS st
     x1 in X /\ X1 & x2 in X /\ X1 & ||.x1-x2.||<g holds
      ||.f2/.x1-f2/.x2.||<r/2 by A5,A7,Def2;
   take p=min(s,g);
   thus 0<p by A8,A9,SQUARE_1:38;
A10:p <= s & p <= g by SQUARE_1:35;
   let x1,x2 be Point of CNS;
   assume
A11:x1 in X/\X1 & x2 in X/\X1 & ||.x1-x2.||<p; then
   x1 in X/\X1 & x2 in X/\X1 & ||.x1-x2.||<s by A10,AXIOMS:22; then
A12: ||.f1/.x1-f1/.x2.||<r/2 by A8;
   x1 in X/\X1 & x2 in X/\X1 & ||.x1-x2.||<g by A10,A11,AXIOMS:22; then
   ||.f2/.x1-f2/.x2.||<r/2 by A9; then
A13: ||.f1/.x1-f1/.x2.||+||.f2/.x1-f2/.x2.||<r/2+r/2 by A12,REAL_1:67;
A14: ||.(f1-f2)/.x1-(f1-f2)/.x2.|| = ||.f1/.x1 - f2/.x1-(f1-f2)/.x2.||
      by A6,A11,VFUNCT_1:def 2
   .= ||.f1/.x1 - f2/.x1 - (f1/.x2-f2/.x2).|| by A6,A11,VFUNCT_1:def 2
   .= ||.f1/.x1 - (f2/.x1 + (f1/.x2-f2/.x2)).|| by RLVECT_1:41
   .= ||.f1/.x1 - (f1/.x2 + f2/.x1-f2/.x2).|| by RLVECT_1:42
   .= ||.f1/.x1 - (f1/.x2 + (f2/.x1-f2/.x2)).|| by RLVECT_1:42
   .= ||.f1/.x1 - f1/.x2 - (f2/.x1-f2/.x2).|| by RLVECT_1:41;
   ||.f1/.x1 - f1/.x2 - (f2/.x1-f2/.x2).||
     <= ||.f1/.x1-f1/.x2.|| + ||.f2/.x1-f2/.x2.|| by NORMSP_1:7;
   hence thesis by A13,A14,AXIOMS:22;
