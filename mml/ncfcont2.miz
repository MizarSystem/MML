theorem
for f be PartFunc of CNS1,CNS2, Y be Subset of CNS1 st
 Y is compact & f is_continuous_on Y holds
  f is_uniformly_continuous_on Y
proof
   let f be PartFunc of CNS1,CNS2;
   let Y be Subset of CNS1;
   assume that
A1: Y is compact and
A2: f is_continuous_on Y;
A3: Y c= dom f by A2,NCFCONT1:def 21;
   assume not thesis; then
   consider r such that
A4: 0<r and
A5: for s st 0<s ex x1,x2 be Point of CNS1 st
     x1 in Y & x2 in Y & ||.x1 -x2.|| < s &
     not ||.f/.x1 - f/.x2.||< r by A3,Def1;
A6:now let n be Nat;
     0 <> n+1 by NAT_1:21; then
     0 < n+1 by NAT_1:19;
     hence 0 < 1/(n+1) by SEQ_2:6;
   end;
   defpred P[Nat,Point of CNS1] means
    $2 in Y & ex x2 be Point of CNS1 st x2 in Y &
    ||.$2-x2.||<1/($1+1) & not ||.f/.($2)-f/.x2.|| < r;
A7:now let n be Nat;
     0 < 1/(n+1) by A6; then
     consider x1 be Point of CNS1 such that
A8:   ex x2 be Point of CNS1 st x1 in Y & x2 in Y & ||.x1 -x2.|| < 1/(n+1) &
      not ||.f/.x1 - f/.x2.||< r by A5;
     take x1;
     thus P[n,x1] by A8;
   end;
   consider s1 be sequence of CNS1 such that
A9: for n be Nat holds P[n,s1.n] from FUNCT_2:sch 3(A7);
   defpred P1[Nat, Point of CNS1] means
    $2 in Y & ||.s1.$1-$2.|| < 1/($1+1) & not ||.f/.(s1.$1)-f/.$2.|| < r;
A10:for n be Nat ex x2 be Point of CNS1 st P1[n,x2] by A9;
   consider s2 be sequence of CNS1 such that
A11:for n be Nat holds P1[n,s2.n] from FUNCT_2:sch 3(A10);
A12:now let p be Real such that
A13: 0<p;
     consider k be Nat such that
A14:  p" < k by SEQ_4:10;
A15: 0 < p" by A13,REAL_1:72;
A16: k < k+1 by NAT_1:38; then
     p" < k+1 by A14,AXIOMS:22; then
     1/(k+1) < 1/p" by A15,SEQ_2:10; then
A17: 1/(k+1) < p by XCMPLX_1:218;
     take k;
     let m be Nat such that
A18:  k <= m;

A19: 0 < k+1 by A14,A15,A16;
     k+1 <= m+1 by A18,AXIOMS:24; then
A20: 1/(m+1) <= 1/(k+1) by A19,SEQ_4:1;
A21: ||.(s1-s2).m - 0.CNS1.|| = ||.s1.m - s2.m - 0.CNS1.|| by CLVECT_1:def 12
                             .= ||.s1.m - s2.m .|| by RLVECT_1:26;
     ||.s1.m - s2.m.|| < 1/(m+1) by A11; then
     ||.s1.m - s2.m.|| < 1/(k+1) by A20,AXIOMS:22;
     hence ||.(s1-s2).m - 0.CNS1.|| < p by A17,A21,AXIOMS:22;
   end; then
A22:s1-s2 is convergent by CLVECT_1:def 15; then
A23:lim (s1-s2) = 0.CNS1 by A12,CLVECT_1:def 17;
    now let x be Point of CNS1;
     assume x in rng s1; then
     ex n be Nat st x=s1.n by NCFCONT1:8;
     hence x in Y by A9;
   end; then
   for x be set st x in rng s1 holds x in Y; then
A24:rng s1 c= Y by TARSKI:def 3; then
   consider q1 be sequence of CNS1 such that
A25:q1 is subsequence of s1 & q1 is convergent &
    (lim q1) in Y by A1,NCFCONT1:def 6;
A26:f|Y is_continuous_in (lim q1) by A2,A25,NCFCONT1:def 21;
   consider Ns1 be increasing Seq_of_Nat such that
A27:q1=s1*Ns1 by A25,BHSP_3:def 4;
A28:(s1-s2)*Ns1 is subsequence of s1-s2 by BHSP_3:def 4; then
A29:(s1-s2)*Ns1 is convergent by A22,CLOPBAN3:10;
A30:lim ((s1-s2)*Ns1) = 0.CNS1 by A22,A23,A28,CLOPBAN3:11;
   set q2 = q1 - (s1-s2)*Ns1;
A31:q2 is convergent by A25,A29,CLVECT_1:116;
A32:lim q2 = lim q1 - 0.CNS1 by A25,A29,A30,CLVECT_1:122
          .= lim q1 by RLVECT_1:26;
   now let n be Nat;
     thus q2.n = (s1*Ns1).n - ((s1-s2)*Ns1).n by A27,CLVECT_1:def 12
     .= s1.(Ns1.n) - ((s1-s2)*Ns1).n by LOPBAN_3:7
     .= s1.(Ns1.n) - (s1-s2).(Ns1.n) by LOPBAN_3:7
     .= s1.(Ns1.n) - (s1.(Ns1.n)-s2.(Ns1.n)) by CLVECT_1:def 12
     .= (s1.(Ns1.n) - s1.(Ns1.n)) + s2.(Ns1.n) by RLVECT_1:43
     .= 0.CNS1  + s2.(Ns1.n) by RLVECT_1:28
     .= s2.(Ns1.n) by RLVECT_1:10
     .= (s2*Ns1).n by LOPBAN_3:7;
   end; then
A33:q2 = s2*Ns1 by FUNCT_2:113; then
A34:q2 is subsequence of s2 by BHSP_3:def 4;
   now let x be set;
     assume x in rng s2; then
     ex n be Nat st x=s2.n by NCFCONT1:8;
     hence x in Y by A11;
   end; then
A35:rng s2 c= Y by TARSKI:def 3;
   rng q1 c= rng s1 by A25,NCFCONT1:9; then
A36:rng q1 c= Y by A24,XBOOLE_1:1; then
   rng q1 c= dom f by A3,XBOOLE_1:1; then
   rng q1 c= dom f /\ Y by A36,XBOOLE_1:19; then
A37:rng q1 c= dom (f|Y) by RELAT_1:90; then
A38:(f|Y)*q1 is convergent & (f|Y)/.(lim q1)=lim((f|Y)*q1)
     by A25,A26,NCFCONT1:def 15;
   rng q2 c= rng s2 by A34,NCFCONT1:9; then
A39:rng q2 c= Y by A35,XBOOLE_1:1; then
   rng q2 c= dom f by A3,XBOOLE_1:1; then
   rng q2 c= dom f /\ Y by A39,XBOOLE_1:19; then
A40:rng q2 c= dom (f|Y) by RELAT_1:90; then
A41:(f|Y)*q2 is convergent & (f|Y)/.(lim q1) = lim ((f|Y)*q2)
     by A26,A31,A32,NCFCONT1:def 15; then
A42:(f|Y)*q1 - (f|Y)*q2 is convergent by A38,CLVECT_1:116;
A43:lim ((f|Y)*q1 - (f|Y)*q2) = (f|Y)/.(lim q1) - (f|Y)/.(lim q1)
     by A38,A41,CLVECT_1:122
     .= 0.CNS2 by RLVECT_1:28;
   now let n be Nat;
     consider k be Nat such that
A44:  for m be Nat st k<=m holds
       ||.((f|Y)*q1 - (f|Y)*q2).m-0.CNS2 .||<r
        by A4,A42,A43,CLVECT_1:def 17;
A45: ||.((f|Y)*q1 - (f|Y)*q2).k - 0.CNS2.||<r by A44;
A46: q1.k in rng q1 by NCFCONT1:8;
A47: q2.k in rng q2 by NCFCONT1:8;
     ||.((f|Y)*q1 - (f|Y)*q2).k - 0.CNS2 .||
      = ||.((f|Y)*q1 - (f|Y)*q2).k  .|| by RLVECT_1:26
     .= ||.((f|Y)*q1).k - ((f|Y)*q2).k.|| by CLVECT_1:def 12
     .= ||.(f|Y)/.(q1.k) - ((f|Y)*q2).k.|| by A37,NCFCONT1:10
     .= ||.(f|Y)/.(q1.k) - (f|Y)/.(q2.k) .|| by A40,NCFCONT1:10
     .= ||.f/.(q1.k) - (f|Y)/.(q2.k).|| by A37,A46,PARTFUN2:32
     .= ||.f/.(q1.k) - f/.(q2.k).|| by A40,A47,PARTFUN2:32
     .= ||.f/.(s1.(Ns1.k)) - f/.((s2*Ns1).k).|| by A27,A33,LOPBAN_3:7
     .= ||.f/.(s1.(Ns1.k)) - f/.(s2.(Ns1.k)).|| by LOPBAN_3:7;
     hence contradiction by A11,A45;
   end;
   hence contradiction;
end;

