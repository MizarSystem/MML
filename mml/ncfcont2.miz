A2: 0 < s &
    for x1,x2 be Point of CNS st
     x1 in X & x2 in X & ||.x1-x2.|| < s holds ||.f/.x1-f/.x2.||<r by A1,Def2;
   take s;
   thus 0 < s by A2;
   let x1,x2 be Point of CNS;
   assume x1 in X1 & x2 in X1 & ||.x1-x2.||<s;
   hence thesis by A1,A2;
end;

