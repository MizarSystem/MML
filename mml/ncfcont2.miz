       .= 0 by NORMSP_1:def 2;
       hence ||.(r(#)f)/.x1-(r(#)f)/.x2.|| <p by A4;
     end;
     suppose
       r<>0; then
A7:    0<abs(r) & 0 <> abs(r) by ABSVALUE:6;
       let p be Real;
       assume 0<p; then
       0 < p/abs(r) by A7,SEQ_2:6; then
       consider s such that
