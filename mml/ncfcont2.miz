A7:0<r/2 by SEQ_2:3; then
   consider s such that
A8: 0<s & for x1,x2 be Point of RNS st
     x1 in X /\ X1 & x2 in X /\X1 &
      ||.x1-x2.||<s holds ||.f1/.x1-f1/.x2.||<r/2 by A3,Def3;
   consider g be Real such that
A9: 0<g &
    for x1,x2 be Point of RNS st x1 in X /\ X1 & x2 in X /\ X1 &
     ||.x1-x2.|| < g holds ||.f2/.x1-f2/.x2.|| < r/2 by A5,A7,Def3;
