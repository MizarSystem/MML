begin  :: Preliminaries

reserve i, i1, i2, j, j1, j2, k, m, n, t for Element of NAT,
  D for non empty Subset of TOP-REAL 2,
  E for compact non vertical non horizontal Subset of TOP-REAL 2,
  C for compact connected non vertical non horizontal Subset of TOP-REAL 2,
  G for Go-board,
  p, q, x for Point of TOP-REAL 2,
  r, s for real number;
3 = 2 * 1 + 1;
then

Lm1: 3 div 2 = 1 by NAT_D:def 1;
1 = 2 * 0 + 1;
then

Lm2: 1 div 2 = 0 by NAT_D:def 1;

definition
  let f be FinSequence;
  func Center f -> Element of NAT equals

  len f div 2 + 1;
  coherence;
end;

canceled 8;

theorem
  for f being FinSequence st len f is odd holds len f = 2 * Center f - 1
proof
  let f be FinSequence;
  assume len f is odd;
  then consider k being Element of NAT such that
A1: len f = 2*k+1 by ABIAN:9;
A2: 2*k mod 2 = 0 by NAT_D:13;
  thus len f = 2 * ((2*k div 2) + (1 div 2)) + 1 by A1,Lm2,NAT_D:18
    .= 2 * (len f div 2) + (2*1 - 1) by A1,A2,NAT_D:19
    .= 2 * Center f - 1;
end;

theorem
  for f being FinSequence st len f is even holds len f = 2 * Center f - 2
proof
  let f be FinSequence;
  assume ex k being Element of NAT st len f = 2*k;
  hence len f = 2 * (len f div 2) + 2*1 - 2 by NAT_D:18
    .= 2 * Center f - 2;
end;

begin  :: Some subsets of the plane

registration
  cluster compact non vertical non horizontal
    being_simple_closed_curve non empty Subset of TOP-REAL 2;
  existence
  proof consider f being non constant standard special_circular_sequence;
    take L~f;
    thus thesis;
  end;
end;

theorem Th11:
  p in N-most D implies p`2 = N-bound D
proof
  assume p in N-most D;
  then
A1: p in LSeg(NW-corner D, NE-corner D) by XBOOLE_0:def 4;
  (NE-corner D)`2 = N-bound D by EUCLID:56
    .= (NW-corner D)`2 by EUCLID:56;
  hence p`2 = (NW-corner D)`2 by A1,GOBOARD7:6
    .= N-bound D by EUCLID:56;
end;

theorem Th12:
  p in E-most D implies p`1 = E-bound D
proof
  assume p in E-most D;
  then
A1: p in LSeg(SE-corner D, NE-corner D) by XBOOLE_0:def 4;
  (SE-corner D)`1 = E-bound D by EUCLID:56
    .= (NE-corner D)`1 by EUCLID:56;
  hence p`1 = (SE-corner D)`1 by A1,GOBOARD7:5
    .= E-bound D by EUCLID:56;
end;

theorem Th13:
  p in S-most D implies p`2 = S-bound D
proof
  assume p in S-most D;
  then
A1: p in LSeg(SW-corner D, SE-corner D) by XBOOLE_0:def 4;
  (SE-corner D)`2 = S-bound D by EUCLID:56
    .= (SW-corner D)`2 by EUCLID:56;
  hence p`2 = (SW-corner D)`2 by A1,GOBOARD7:6
    .= S-bound D by EUCLID:56;
end;

theorem Th14:
  p in W-most D implies p`1 = W-bound D
proof
  assume p in W-most D;
  then
A1: p in LSeg(SW-corner D, NW-corner D) by XBOOLE_0:def 4;
  (SW-corner D)`1 = W-bound D by EUCLID:56
    .= (NW-corner D)`1 by EUCLID:56;
  hence p`1 = (SW-corner D)`1 by A1,GOBOARD7:5
    .= W-bound D by EUCLID:56;
end;

theorem
  for D being Subset of TOP-REAL 2 holds BDD D misses D
proof
  let D be Subset of TOP-REAL 2;
  D misses D` by SUBSET_1:44;
  hence BDD D misses D by JORDAN2C:29,XBOOLE_1:63;
end;

canceled;

theorem Th17:
  p in Vertical_Line(p`1)
proof
  p in {q where q is Point of TOP-REAL 2: p`1=q`1};
  hence thesis by JORDAN6:def 6;
end;

theorem
  |[r,s]| in Vertical_Line r
proof
  |[r,s]|`1 = r by EUCLID:56;
  hence |[r,s]| in Vertical_Line r by Th17;
end;

theorem
  for A being Subset of TOP-REAL 2 st A c= Vertical_Line s holds A is vertical
proof
  let A being Subset of TOP-REAL 2 such that
A1: A c= Vertical_Line s;
A2: Vertical_Line(s) = {p where p is Point of TOP-REAL 2: p`1=s}
  by JORDAN6:def 6;
  for p,q being Point of TOP-REAL 2 st p in A & q in A holds p`1=q`1
  proof
    let p,q be Point of TOP-REAL 2;
    assume p in A;
    then p in Vertical_Line s by A1;
    then
A3: ex p1 being Point of TOP-REAL 2 st p1 = p & p1`1 =s by A2;
    assume q in A;
    then q in Vertical_Line s by A1;
    then ex p1 being Point of TOP-REAL 2 st p1 = q & p1`1 =s by A2;
    hence p`1=q`1 by A3;
  end;
  hence A is vertical by SPPOL_1:def 3;
end;

canceled;

theorem
  p`1 = q`1 & r in [. proj2.p,proj2.q .] implies |[p`1,r]| in LSeg(p,q)
proof
  assume p`1 = q`1;
  then
A1: p`1 = |[p`1,r]|`1 & |[p`1,r]|`1 = q`1 by EUCLID:56;
  assume
A2: r in [. proj2.p,proj2.q .];
A3: proj2.p = p`2 & proj2.q = q`2 by PSCOMP_1:def 29;
  |[p`1,r]|`2 = r by EUCLID:56;
  then p`2 <= |[p`1,r]|`2 & |[p`1,r]|`2 <= q`2 by A2,A3,XXREAL_1:1;
  hence |[p`1,r]| in LSeg(p,q) by A1,GOBOARD7:8;
end;

theorem
  p`2 = q`2 & r in [. proj1.p,proj1.q .] implies |[r,p`2]| in LSeg(p,q)
proof
  assume p`2 = q`2;
  then
A1: p`2 = |[r,p`2]|`2 & |[r,p`2]|`2 = q`2 by EUCLID:56;
  assume
A2: r in [. proj1.p,proj1.q .];
A3: proj1.p = p`1 & proj1.q = q`1 by PSCOMP_1:def 28;
  |[r,p`2]|`1 = r by EUCLID:56;
  then p`1 <= |[r,p`2]|`1 & |[r,p`2]|`1 <= q`1 by A2,A3,XXREAL_1:1;
  hence |[r,p`2]| in LSeg(p,q) by A1,GOBOARD7:9;
end;

theorem
  p in Vertical_Line s & q in Vertical_Line s
  implies LSeg(p,q) c= Vertical_Line s
proof
A1: Vertical_Line(s) = {p1 where p1 is Point of TOP-REAL 2: p1`1=s}
  by JORDAN6:def 6;
  assume p in Vertical_Line s & q in Vertical_Line s;
  then
A2: p`1 = s & q`1 = s by JORDAN6:34;
  let u be set;
  assume
A3: u in LSeg(p,q);
  then reconsider p1 = u as Point of TOP-REAL 2;
  p1`1 = s by A2,A3,GOBOARD7:5;
  hence u in Vertical_Line s by A1;
end;

theorem
  for A,B being Subset of TOP-REAL 2 st A meets B
  holds proj2.:A meets proj2.:B
proof
  let A,B be Subset of TOP-REAL 2;
  assume A meets B;
  then consider e being set such that
A1: e in A and
A2: e in B by XBOOLE_0:3;
  reconsider e as Point of TOP-REAL 2 by A1;
A3: e`2 = proj2.e by PSCOMP_1:def 29;
  then
A4: e`2 in proj2.:A by A1,FUNCT_2:43;
  e`2 in proj2.:B by A2,A3,FUNCT_2:43;
  hence proj2.:A meets proj2.:B by A4,XBOOLE_0:3;
end;

theorem
  for A,B being Subset of TOP-REAL 2
  st A misses B & A c= Vertical_Line s & B c= Vertical_Line s
  holds proj2.:A misses proj2.:B
proof
  let A,B being Subset of TOP-REAL 2 such that
A1: A misses B and
A2: A c= Vertical_Line s and
A3: B c= Vertical_Line s;
  assume proj2.:A meets proj2.:B;
  then consider e being set such that
A4: e in proj2.:A and
A5: e in proj2.:B by XBOOLE_0:3;
  reconsider e as Real by A4;
  consider c being Point of TOP-REAL 2 such that
A6: c in A and
A7: e = proj2.c by A4,FUNCT_2:116;
  consider d being Point of TOP-REAL 2 such that
A8: d in B and
A9: e = proj2.d by A5,FUNCT_2:116;
  c`1 = s & d`1 = s by A2,A3,A6,A8,JORDAN6:34;
  then c = |[d`1,c`2]| by EUCLID:57
    .= |[d`1,e]| by A7,PSCOMP_1:def 29
    .= |[d`1,d`2]| by A9,PSCOMP_1:def 29
    .= d by EUCLID:57;
  hence contradiction by A1,A6,A8,XBOOLE_0:3;
end;

begin  :: Goboards

canceled 11;

theorem
  1 <= i & i <= len G & 1 <= j & j <= width G implies
  G*(i,j) in LSeg(G*(i,1),G*(i,width G))
proof
  assume that
A1: 1 <= i & i <= len G and
A2: 1 <= j & j <= width G;
  1 <= width G by A2,XXREAL_0:2;
  then
A3: G*(i,1)`1 = G*(i,j)`1 & G*(i,1)`1 = G* (i,width G)`1 by A1,A2,GOBOARD5:3;
  G*(i,1)`2 <= G*(i,j)`2 & G*(i,j)`2 <= G* (i,width G)`2 by A1,A2,SPRECT_3:24;
  hence thesis by A3,GOBOARD7:8;
end;

theorem
  1 <= i & i <= len G & 1 <= j & j <= width G implies
  G*(i,j) in LSeg(G*(1,j),G*(len G,j))
proof
  assume that
A1: 1 <= i & i <= len G and
A2: 1 <= j & j <= width G;
  1 <= len G by A1,XXREAL_0:2;
  then
A3: G*(1,j)`2 = G*(i,j)`2 & G*(1,j)`2 = G*(len G,j)`2 by A1,A2,GOBOARD5:2;
  G*(1,j)`1 <= G*(i,j)`1 & G*(i,j)`1 <= G* (len G,j)`1 by A1,A2,SPRECT_3:25;
  hence thesis by A3,GOBOARD7:9;
end;

theorem Th39:
  1 <= j1 & j1 <= width G & 1 <= j2 & j2 <= width G &
  1 <= i1 & i1 <= i2 & i2 <= len G implies G*(i1,j1)`1 <= G*(i2,j2)`1
proof
  assume that
A1: 1 <= j1 and
A2: j1 <= width G and
A3: 1 <= j2 and
A4: j2 <= width G and
A5: 1 <= i1 and
A6: i1 <= i2 and
A7: i2 <= len G;
A8: 1 <= i2 by A5,A6,XXREAL_0:2;
  then G*(i2,j1)`1 = G*(i2,1)`1 by A1,A2,A7,GOBOARD5:3
    .= G*(i2,j2)`1 by A3,A4,A7,A8,GOBOARD5:3;
  hence G*(i1,j1)`1 <= G*(i2,j2)`1 by A1,A2,A5,A6,A7,SPRECT_3:25;
end;

theorem Th40:
  1 <= i1 & i1 <= len G & 1 <= i2 & i2 <= len G &
  1 <= j1 & j1 <= j2 & j2 <= width G implies G*(i1,j1)`2 <= G*(i2,j2)`2
proof
  assume that
A1: 1 <= i1 and
A2: i1 <= len G and
A3: 1 <= i2 and
A4: i2 <= len G and
A5: 1 <= j1 and
A6: j1 <= j2 and
A7: j2 <= width G;
A8: 1 <= j2 by A5,A6,XXREAL_0:2;
  then G*(i1,j2)`2 = G*(1,j2)`2 by A1,A2,A7,GOBOARD5:2
    .= G*(i2,j2)`2 by A3,A4,A7,A8,GOBOARD5:2;
  hence G*(i1,j1)`2 <= G*(i2,j2)`2 by A1,A2,A5,A6,A7,SPRECT_3:24;
end;

theorem Th41:
  for f being non constant standard special_circular_sequence st
  f is_sequence_on G & 1 <= t & t <= len G holds
  G*(t,width G)`2 >= N-bound L~f
proof
  let f be non constant standard special_circular_sequence;
  assume
A1: f is_sequence_on G;
  assume
A2: 1 <= t & t <= len G;
A3: N-bound L~f = (N-min L~f)`2 by EUCLID:56;
  N-min L~f in rng f by SPRECT_2:43;
  then consider x being set such that
A4: x in dom f and
A5: f.x = N-min L~f by FUNCT_1:def 5;
  reconsider x as Element of NAT by A4;
  consider i,j such that
A6: [i,j] in Indices G and
A7: f/.x = G*(i,j) by A1,A4,GOBOARD1:def 11;
  1 <= j & j <= width G & 1 <= i & i <= len G by A6,MATRIX_1:39;
  then G*(t,width G)`2 >= G*(i,j)`2 by A2,Th40;
  hence G*(t,width G)`2 >= N-bound L~f by A3,A4,A5,A7,PARTFUN1:def 8;
end;

theorem Th42:
  for f being non constant standard special_circular_sequence st
  f is_sequence_on G & 1 <= t & t <= width G holds G*(1,t)`1 <= W-bound L~f
proof
  let f be non constant standard special_circular_sequence;
  assume
A1: f is_sequence_on G;
  assume
A2: 1 <= t & t <= width G;
A3: W-bound L~f = (W-min L~f)`1 by EUCLID:56;
  W-min L~f in rng f by SPRECT_2:47;
  then consider x being set such that
A4: x in dom f and
A5: f.x = W-min L~f by FUNCT_1:def 5;
  reconsider x as Element of NAT by A4;
  consider i,j such that
A6: [i,j] in Indices G and
A7: f/.x = G*(i,j) by A1,A4,GOBOARD1:def 11;
  1 <= j & j <= width G & 1 <= i & i <= len G by A6,MATRIX_1:39;
  then G*(1,t)`1 <= G*(i,j)`1 by A2,Th39;
  hence G*(1,t)`1 <= W-bound L~f by A3,A4,A5,A7,PARTFUN1:def 8;
end;

theorem Th43:
  for f being non constant standard special_circular_sequence st
  f is_sequence_on G & 1 <= t & t <= len G holds G*(t,1)`2 <= S-bound L~f
proof
  let f be non constant standard special_circular_sequence;
  assume
A1: f is_sequence_on G;
  assume
A2: 1 <= t & t <= len G;
A3: S-bound L~f = (S-min L~f)`2 by EUCLID:56;
  S-min L~f in rng f by SPRECT_2:45;
  then consider x being set such that
A4: x in dom f and
A5: f.x = S-min L~f by FUNCT_1:def 5;
  reconsider x as Element of NAT by A4;
  consider i,j such that
A6: [i,j] in Indices G and
A7: f/.x = G*(i,j) by A1,A4,GOBOARD1:def 11;
  1 <= j & j <= width G & 1 <= i & i <= len G by A6,MATRIX_1:39;
  then G*(t,1)`2 <= G*(i,j)`2 by A2,Th40;
  hence G*(t,1)`2 <= S-bound L~f by A3,A4,A5,A7,PARTFUN1:def 8;
end;

theorem Th44:
  for f being non constant standard special_circular_sequence st
  f is_sequence_on G & 1 <= t & t <= width G holds
  G*(len G,t)`1 >= E-bound L~f
proof
  let f be non constant standard special_circular_sequence;
  assume
A1: f is_sequence_on G;
  assume
A2: 1 <= t & t <= width G;
A3: E-bound L~f = (E-min L~f)`1 by EUCLID:56;
  E-min L~f in rng f by SPRECT_2:49;
  then consider x being set such that
A4: x in dom f and
A5: f.x = E-min L~f by FUNCT_1:def 5;
  reconsider x as Element of NAT by A4;
  consider i,j such that
A6: [i,j] in Indices G and
A7: f/.x = G*(i,j) by A1,A4,GOBOARD1:def 11;
  1 <= j & j <= width G & 1 <= i & i <= len G by A6,MATRIX_1:39;
  then G*(len G,t)`1 >= G*(i,j)`1 by A2,Th39;
  hence G*(len G,t)`1 >= E-bound L~f by A3,A4,A5,A7,PARTFUN1:def 8;
end;

theorem Th45:
  i<=len G & j<=width G implies cell(G,i,j) is non empty
proof
  assume i<=len G & j<=width G;
  then Int cell(G,i,j) is non empty by GOBOARD9:17;
  hence cell(G,i,j) is non empty by TOPS_1:44,XBOOLE_1:3;
end;

theorem Th46:
  i<=len G & j<=width G implies cell(G,i,j) is connected
proof
  assume that
A1: i<=len G & j<=width G;
  Cl Int cell(G,i,j) is connected by A1,CONNSP_1:20,GOBOARD9:21;
  hence cell(G,i,j) is connected by A1,GOBRD11:35;
end;

theorem Th47:
  i <= len G implies cell(G,i,0) is not Bounded
proof
  assume
A1: i <= len G;
  per cases by A1,NAT_1:14,XXREAL_0:1;
  suppose i = 0;
    then
A2: cell(G,i,0) = { |[r,s]| where r, s is Real :
    r <= G*(1,1)`1 & s <= G*(1,1)`2 } by GOBRD11:24;
    not ex r being Real st for q being Point of TOP-REAL 2 st
    q in cell(G,i,0) holds |.q.| < r
    proof
      let r be Real;
      take q = |[min(-r,G*(1,1)`1),min(-r,G*(1,1)`2)]|;
      min(-r,G*(1,1)`1) <= G*(1,1)`1 & min(-r,G*(1,1)`2) <= G*(1,1)`2
      by XXREAL_0:17;
      hence q in cell(G,i,0) by A2;
A3:   abs(q`1)<=|.q.| by JGRAPH_1:50;
      per cases;
      suppose r <= 0;
        hence thesis;
      end;
      suppose r > 0;
        then --r > 0;
        then
A4:     -r < 0;
        q`1 = min(-r,G*(1,1)`1) by EUCLID:56;
        then abs(-r) <= abs(q`1) by A4,TOPREAL6:8,XXREAL_0:17;
        then --r <= abs(q`1) by A4,ABSVALUE:def 1;
        hence thesis by A3,XXREAL_0:2;
      end;
    end;
    hence cell(G,i,0) is not Bounded by JORDAN2C:40;
  end;
  suppose
A5: i >= 1 & i < len G;
    then
A6: cell(G,i,0) = { |[r,s]| where r is Element of REAL, s is Element of REAL:
    G*(i,1)`1 <= r & r <= G*(i+1,1)`1 & s <= G*(1,1)`2 } by GOBRD11:30;
    not ex r being Real st for q being Point of TOP-REAL 2 st
    q in cell(G,i,0) holds |.q.| < r
    proof
      let r be Real;
      take q = |[G*(i,1)`1,min(-r,G*(1,1)`2)]|;
      width G <> 0 by GOBOARD1:def 5;
      then
A7:   1 <= width G by NAT_1:14;
      i < i+1 & i+1 <= len G by A5,NAT_1:13;
      then
A8:   G*(i,1)`1 <= G*(i+1,1)`1 by A5,A7,GOBOARD5:4;
      min(-r,G*(1,1)`2) <= G*(1,1)`2 by XXREAL_0:17;
      hence q in cell(G,i,0) by A6,A8;
A9:   abs(q`2)<=|.q.| by JGRAPH_1:50;
      per cases;
      suppose r <= 0;
        hence thesis;
      end;
      suppose r > 0;
        then --r > 0;
        then
A10:    -r < 0;
        q`2 = min(-r,G*(1,1)`2) by EUCLID:56;
        then abs(-r) <= abs(q`2) by A10,TOPREAL6:8,XXREAL_0:17;
        then --r <= abs(q`2) by A10,ABSVALUE:def 1;
        hence thesis by A9,XXREAL_0:2;
      end;
    end;
    hence cell(G,i,0) is not Bounded by JORDAN2C:40;
  end;
  suppose i = len G;
    then
A11: cell(G,i,0) = { |[r,s]| where r is Element of REAL, s is Element of
    REAL : G*(len G,1)`1 <= r & s <= G*(1,1)`2 } by GOBRD11:27;
    not ex r being Real st for q being Point of TOP-REAL 2 st
    q in cell(G,i,0) holds |.q.| < r
    proof
      let r be Real;
      take q = |[max(r,G*(len G,1)`1),G*(1,1)`2]|;
      G*(len G,1)`1 <= max(r,G*(len G,1)`1) by XXREAL_0:25;
      hence q in cell(G,i,0) by A11;
A12:  abs(q`1)<=|.q.| by JGRAPH_1:50;
      per cases;
      suppose r <= 0;
        hence thesis;
      end;
      suppose
A13:    r > 0;
        q`1 = max(r,G*(len G,1)`1) by EUCLID:56;
        then r <= q`1 by XXREAL_0:25;
        then r <= abs(q`1) by A13,ABSVALUE:def 1;
        hence thesis by A12,XXREAL_0:2;
      end;
    end;
    hence cell(G,i,0) is not Bounded by JORDAN2C:40;
  end;
end;

theorem Th48:
  i <= len G implies cell(G,i,width G) is not Bounded
proof
  assume
A1: i <= len G;
  per cases by A1,NAT_1:14,XXREAL_0:1;
  suppose
A2: i = 0;
A3: cell(G,0,width G) =
    { |[r,s]| where r is Element of REAL, s is Element of REAL :
    r <= G*(1,1)`1 & G*(1,width G)`2 <= s } by GOBRD11:25;
    not ex r being Real st for q being Point of TOP-REAL 2 st
    q in cell(G,0,width G) holds |.q.| < r
    proof
      let r be Real;
      take q = |[min(-r,G*(1,1)`1),G*(1,width G)`2]|;
      min(-r,G*(1,1)`1) <= G*(1,1)`1 by XXREAL_0:17;
      hence q in cell(G,0,width G) by A3;
A4:   abs(q`1)<=|.q.| by JGRAPH_1:50;
      per cases;
      suppose r <= 0;
        hence thesis;
      end;
      suppose r > 0;
        then --r > 0;
        then
A5:     -r < 0;
        q`1 = min(-r,G*(1,1)`1) by EUCLID:56;
        then abs(-r) <= abs(q`1) by A5,TOPREAL6:8,XXREAL_0:17;
        then --r <= abs(q`1) by A5,ABSVALUE:def 1;
        hence thesis by A4,XXREAL_0:2;
      end;
    end;
    hence cell(G,i,width G) is not Bounded by A2,JORDAN2C:40;
  end;
  suppose
A6: i >= 1 & i < len G;
    then
A7: cell(G,i,width G) =
    { |[r,s]| where r is Element of REAL, s is Element of REAL:
    G*(i,1)`1 <= r & r <= G*(i+1,1)`1 & G*(1,width G)`2 <= s } by GOBRD11:31;
    not ex r being Real st for q being Point of TOP-REAL 2 st
    q in cell(G,i,width G) holds |.q.| < r
    proof
      let r be Real;
      take q = |[G*(i,1)`1,max(r,G*(1,width G)`2)]|;
      width G <> 0 by GOBOARD1:def 5;
      then
A8:   1 <= width G by NAT_1:14;
      i < i+1 & i+1 <= len G by A6,NAT_1:13;
      then
A9:   G*(i,1)`1 <= G*(i+1,1)`1 by A6,A8,GOBOARD5:4;
      max(r,G*(1,width G)`2) >= G*(1,width G)`2 by XXREAL_0:25;
      hence q in cell(G,i,width G) by A7,A9;
A10:  abs(q`2)<=|.q.| by JGRAPH_1:50;
      per cases;
      suppose r <= 0;
        hence thesis;
      end;
      suppose
A11:    r > 0;
        q`2 = max(r,G*(1,width G)`2) by EUCLID:56;
        then q`2 >= r by XXREAL_0:25;
        then r <= abs(q`2) by A11,ABSVALUE:def 1;
        hence thesis by A10,XXREAL_0:2;
      end;
    end;
    hence cell(G,i,width G) is not Bounded by JORDAN2C:40;
  end;
  suppose
A12: i = len G;
A13: cell(G,len G,width G) = { |[r,s]| where r, s is Real :
    G*(len G,1)`1 <= r & G*(1,width G)`2 <= s } by GOBRD11:28;
    not ex r being Real st for q being Point of TOP-REAL 2 st
    q in cell(G,i,width G) holds |.q.| < r
    proof
      let r be Real;
      take q = |[max(r,G*(len G,1)`1),G*(1,width G)`2]|;
      G*(len G,1)`1 <= max(r,G*(len G,1)`1) by XXREAL_0:25;
      hence q in cell(G,i,width G) by A12,A13;
A14:  abs(q`1)<=|.q.| by JGRAPH_1:50;
      per cases;
      suppose r <= 0;
        hence thesis;
      end;
      suppose
A15:    r > 0;
        q`1 = max(r,G*(len G,1)`1) by EUCLID:56;
        then r <= q`1 by XXREAL_0:25;
        then r <= abs(q`1) by A15,ABSVALUE:def 1;
        hence thesis by A14,XXREAL_0:2;
      end;
    end;
    hence cell(G,i,width G) is not Bounded by JORDAN2C:40;
  end;
end;

begin  :: Gauges

theorem
  width Gauge(D,n) = 2|^n + 3
proof
  thus width Gauge(D,n) = len Gauge(D,n) by JORDAN8:def 1
    .= 2|^n + 3 by JORDAN8:def 1;
end;

theorem
  i < j implies len Gauge(D,i) < len Gauge(D,j)
proof
A1: len Gauge(D,i) = 2|^i + 3 & len Gauge(D,j) =2|^j + 3 by JORDAN8:def 1;
  assume i < j;
  then 2|^i < 2|^j by PEPIN:71;
  hence thesis by A1,XREAL_1:8;
end;

theorem
  i <= j implies len Gauge(D,i) <= len Gauge(D,j)
proof
A1: len Gauge(D,i) = 2|^i + 3 & len Gauge(D,j) =2|^j + 3 by JORDAN8:def 1;
  assume i <= j;
  then 2|^i <= 2|^j by PREPOWER:107;
  hence thesis by A1,XREAL_1:8;
end;

theorem Th52:
  m <= n & 1 < i & i < len Gauge(D,m)
  implies 1 < 2|^(n-'m)*(i-2)+2 & 2|^(n-'m)*(i-2)+2 < len Gauge(D,n)
proof
  assume that
A1: m <= n and
A2: 1 < i and
A3: i < len Gauge(D,m);
  1+1 <= i by A2,NAT_1:13;
  then reconsider i2 = i-2 as Element of NAT by INT_1:18;
  0 < 2|^(n-'m)*(i2)+1;
  then 0+1 < 2|^(n-'m)*(i-2)+1+1 by XREAL_1:8;
  hence 1 < 2|^(n-'m)*(i-2)+2;
  len Gauge(D,m) = 2|^m + (2+1) by JORDAN8:def 1
    .= 2|^m + 2+1;
  then i <= 2|^m + 2 by A3,NAT_1:13;
  then i2 <= 2|^m by XREAL_1:22;
  then 2|^(n-'m)*(i2) <= 2|^(n-'m)*2|^m by XREAL_1:66;
  then 2|^(n-'m)*(i2) <= 2|^(n-'m+m) by NEWTON:13;
  then 2|^(n-'m)*(i2) <= 2|^n by A1,XREAL_1:237;
  then 2|^(n-'m)*(i2) < 2|^n + 1 by NAT_1:13;
  then 2|^(n-'m)*(i-2)+2 < 2|^n + 1+2 by XREAL_1:8;
  then 2|^(n-'m)*(i-2)+2 < 2|^n + (1+2);
  hence 2|^(n-'m)*(i-2)+2 < len Gauge(D,n) by JORDAN8:def 1;
end;

theorem Th53:
  m <= n & 1 < i & i < width Gauge(D,m)
  implies 1 < 2|^(n-'m)*(i-2)+2 & 2|^(n-'m)*(i-2)+2 < width Gauge(D,n)
proof len Gauge(D,n) = width Gauge(D,n) & len Gauge(D,m) = width Gauge(D,m)
  by JORDAN8:def 1;
  hence thesis by Th52;
end;

theorem Th54:
  m <= n & 1 < i & i < len Gauge(D,m) & 1 < j & j < width Gauge(D,m) implies
  for i1,j1 being Element of NAT st
  i1 = 2|^(n-'m)*(i-2)+2 & j1 = 2|^(n-'m)*(j-2)+2
  holds Gauge(D,m)*(i,j) = Gauge(D,n)*(i1,j1)
proof
  assume that
A1: m <= n and
A2: 1 < i & i < len Gauge(D,m) and
A3: 1 < j & j < width Gauge(D,m);
  let i1,j1 be Element of NAT such that
A4: i1 = 2|^(n-'m)*(i-2)+2 and
A5: j1 = 2|^(n-'m)*(j-2)+2;
A6: 1 < j1 & j1 < width Gauge(D,n) by A1,A3,A5,Th53;
A7: 1 < i1 & i1 < len Gauge(D,n) by A1,A2,A4,Th52;
A8: [i,j] in Indices Gauge(D,m) by A2,A3,MATRIX_1:37;
A9: [i1,j1] in Indices Gauge(D,n) by A6,A7,MATRIX_1:37;
  (i-2)/(2|^m) = (i-2)/(2|^(n-'(n-'m))) by A1,NAT_D:58
    .= (i-2)/((2|^n)/(2|^(n-'m))) by NAT_D:50,TOPREAL6:15
    .= (i1-2)/(2|^n) by A4,XCMPLX_1:78;
  then
A10: (((E-bound D)-(W-bound D))/(2|^m))*(i-2)
  = ((E-bound D)-(W-bound D))*((i1-2)/(2|^n)) by XCMPLX_1:76
    .= (((E-bound D)-(W-bound D))/(2|^n))*(i1-2) by XCMPLX_1:76;
  (j-2)/(2|^m) = (j-2)/(2|^(n-'(n-'m))) by A1,NAT_D:58
    .= (j-2)/((2|^n)/(2|^(n-'m))) by NAT_D:50,TOPREAL6:15
    .= (j1-2)/(2|^n) by A5,XCMPLX_1:78;
  then
A11: (((N-bound D)-(S-bound D))/(2|^m))*(j-2)
  = ((N-bound D)-(S-bound D))*((j1-2)/(2|^n)) by XCMPLX_1:76
    .= (((N-bound D)-(S-bound D))/(2|^n))*(j1-2) by XCMPLX_1:76;
  thus Gauge(D,m)*(i,j) =
  |[(W-bound D)+(((E-bound D)-(W-bound D))/(2|^m))*(i-2),
  (S-bound D)+(((N-bound D)-(S-bound D))/(2|^m))*(j-2)]| by A8,JORDAN8:def 1
    .= Gauge(D,n)*(i1,j1) by A9,A10,A11,JORDAN8:def 1;
end;

theorem Th55:
  m <= n & 1 < i & i+1 < len Gauge(D,m)
  implies 1 < 2|^(n-'m)*(i-1)+2 & 2|^(n-'m)*(i-1)+2 <= len Gauge(D,n)
proof
  assume that
A1: m <= n and
A2: 1 < i and
A3: i+1 < len Gauge(D,m);
  reconsider i2 = i-1 as Element of NAT by A2,INT_1:18;
  0 < 2|^(n-'m)*(i2)+1;
  then 0+1 < 2|^(n-'m)*(i-1)+1+1 by XREAL_1:8;
  hence 1 < 2|^(n-'m)*(i-1)+2;
  len Gauge(D,m) = 2|^m + (2+1) by JORDAN8:def 1
    .= 2|^m + 2+1;
  then i+1 <= 2|^m + 1 + 1 by A3,NAT_1:13;
  then i <= 2|^m + 1 by XREAL_1:8;
  then i2 <= 2|^m by XREAL_1:22;
  then 2|^(n-'m)*(i2) <= 2|^(n-'m)*2|^m by XREAL_1:66;
  then 2|^(n-'m)*(i2) <= 2|^(n-'m+m) by NEWTON:13;
  then 2|^(n-'m)*(i2) <= 2|^n by A1,XREAL_1:237;
  then 2|^(n-'m)*(i2) <= 2|^n + 1 by NAT_1:13;
  then 2|^(n-'m)*(i-1)+2 <= 2|^n + 1+2 by XREAL_1:8;
  then 2|^(n-'m)*(i-1)+2 <= 2|^n + (1+2);
  hence 2|^(n-'m)*(i-1)+2 <= len Gauge(D,n) by JORDAN8:def 1;
end;

theorem Th56:
  m <= n & 1 < i & i+1 < width Gauge(D,m)
  implies 1 < 2|^(n-'m)*(i-1)+2 & 2|^(n-'m)*(i-1)+2 <= width Gauge(D,n)
proof len Gauge(D,n) = width Gauge(D,n) & len Gauge(D,m) = width Gauge(D,m)
  by JORDAN8:def 1;
  hence thesis by Th55;
end;

Lm3: now
  let D, n;
  set G = Gauge(D,n);
  0 + 1 <= len G div 2 + 1 by XREAL_1:8;
  hence 1 <= Center G;
  0 < len G by JORDAN8:13;
  then len G div 2 < len G by INT_1:83;
  hence Center G <= len G by NAT_1:13;
end;

Lm4: now
  let D, n, j;
  set G = Gauge(D,n);
  assume
A1: 1 <= j & j <= len G;
A2: len G = width G by JORDAN8:def 1;
A3: 0 + 1 <= len G div 2 + 1 by XREAL_1:8;
  Center G <= len G by Lm3;
  hence [Center G,j] in Indices G by A1,A2,A3,MATRIX_1:37;
end;

Lm5: now
  let D, n, j;
  set G = Gauge(D,n);
  assume
A1: 1 <= j & j <= len G;
A2: len G = width G by JORDAN8:def 1;
A3: 0 + 1 <= len G div 2 + 1 by XREAL_1:8;
  Center G <= len G by Lm3;
  hence [j,Center G] in Indices G by A1,A2,A3,MATRIX_1:37;
end;

Lm6: for w being real number holds
n > 0 implies w/(2|^n)*(Center Gauge(D,n) - 2) = w/2
proof
  let w be real number;
  set G = Gauge(D,n);
A1: 2|^n <> 0 by NEWTON:102;
  assume
A2: n > 0;
  then
A3: 2|^n mod 2 = 0 by PEPIN:37;
  thus w/(2|^n)*(Center G - 2) =
  w/(2|^n)*((2|^n + 3) div 2 + 1 - 2) by JORDAN8:def 1
    .= w/(2|^n)*((2|^n + 3) div 2 + (1 - 2))
    .= w/(2|^n)*((2|^n div 2) + 1 +- 1) by A3,Lm1,NAT_D:19
    .= w/(2|^n)*(2|^n / 2) by A2,PEPIN:69
    .= w/2 by A1,XCMPLX_1:99;
end;

Lm7: now
  let m, n;
  let c, d be real number;
  assume
A1: 0 <= c;
A2: 0 < 2|^m by NEWTON:102;
  assume m <= n;
  then 2|^m <= 2|^n by PREPOWER:107;
  hence c/(2|^n) <= c/(2|^m) by A1,A2,XREAL_1:120;
end;

Lm8: now
  let m, n;
  let c, d be real number;
  assume 0 <= c & m <= n;
  then c/(2|^n) <= c/(2|^m) by Lm7;
  hence d+c/2|^n <= d+c/2|^m by XREAL_1:8;
end;

Lm9: now
  let m, n;
  let c, d be real number;
  assume 0 <= c & m <= n;
  then c/(2|^n) <= c/(2|^m) by Lm7;
  hence d-c/2|^m <= d-c/2|^n by XREAL_1:15;
end;

theorem Th57:
  1 <= i & i <= len Gauge (D,n) & 1 <= j & j <= len Gauge (D,m) &
  (n > 0 & m > 0 or n = 0 & m = 0) implies
  Gauge(D,n)*(Center Gauge(D,n), i)`1 = Gauge(D,m)*(Center Gauge(D,m), j)`1
proof
  set a = N-bound D, s = S-bound D, w = W-bound D, e = E-bound D,
  G = Gauge(D,n), M = Gauge(D,m);
  assume
A1: 1 <= i & i <= len G;
  assume 1 <= j & j <= len M;
  then
A2: [Center M,j] in Indices M by Lm4;
A3: [Center G,i] in Indices G by A1,Lm4;
  assume
A4: n > 0 & m > 0 or n = 0 & m = 0;
  per cases by A4;
  suppose that
A5: n > 0 and
A6: m > 0;
    thus G*(Center G,i)`1
    = |[w+(e-w)/(2|^n)*(Center G - 2), s+(a-s)/(2|^n)*(i - 2)]|`1
    by A3,JORDAN8:def 1
      .= w+(e-w)/(2|^n)*(Center G - 2) by EUCLID:56
      .= w+(e-w)/2 by A5,Lm6
      .= w+(e-w)/(2|^m)*(Center M - 2) by A6,Lm6
      .= |[w+(e-w)/(2|^m)*(Center M - 2), s+(a-s)/(2|^m)*(j - 2)]|`1
    by EUCLID:56
      .= M*(Center M,j)`1 by A2,JORDAN8:def 1;
  end;
  suppose
A7: n = 0 & m = 0;
    thus G*(Center G,i)`1
    = |[w+(e-w)/(2|^n)*(Center G - 2), s+(a-s)/(2|^n)*(i - 2)]|`1
    by A3,JORDAN8:def 1
      .= w+(e-w)/(2|^m)*(Center M - 2) by A7,EUCLID:56
      .= |[w+(e-w)/(2|^m)*(Center M - 2), s+(a-s)/(2|^m)*(j - 2)]|`1
    by EUCLID:56
      .= M*(Center M,j)`1 by A2,JORDAN8:def 1;
  end;
end;

theorem
  1 <= i & i <= len Gauge (D,n) & 1 <= j & j <= len Gauge (D,m) &
  (n > 0 & m > 0 or n = 0 & m = 0) implies
  Gauge(D,n)*(i, Center Gauge(D,n))`2 = Gauge(D,m)*(j, Center Gauge(D,m))`2
proof
  set a = N-bound D, s = S-bound D, w = W-bound D, e = E-bound D,
  G = Gauge(D,n), M = Gauge(D,m);
  assume
A1: 1 <= i & i <= len G;
  assume 1 <= j & j <= len M;
  then
A2: [j,Center M] in Indices M by Lm5;
A3: [i,Center G] in Indices G by A1,Lm5;
  assume
A4: n > 0 & m > 0 or n = 0 & m = 0;
  per cases by A4;
  suppose that
A5: n > 0 and
A6: m > 0;
    thus G*(i,Center G)`2
    = |[w+(e-w)/(2|^n)*(i - 2), s+(a-s)/(2|^n)*(Center G - 2)]|`2
    by A3,JORDAN8:def 1
      .= s+(a-s)/(2|^n)*(Center G - 2) by EUCLID:56
      .= s+(a-s)/2 by A5,Lm6
      .= s+(a-s)/(2|^m)*(Center M - 2) by A6,Lm6
      .= |[w+(e-w)/(2|^m)*(j - 2), s+(a-s)/(2|^m)*(Center M - 2)]|`2
    by EUCLID:56
      .= M*(j,Center M)`2 by A2,JORDAN8:def 1;
  end;
  suppose
A7: n = 0 & m = 0;
    thus G*(i,Center G)`2
    = |[w+(e-w)/(2|^n)*(i - 2), s+(a-s)/(2|^n)*(Center G - 2)]|`2
    by A3,JORDAN8:def 1
      .= s+(a-s)/(2|^m)*(Center M - 2) by A7,EUCLID:56
      .= |[w+(e-w)/(2|^m)*(j - 2), s+(a-s)/(2|^m)*(Center M - 2)]|`2
    by EUCLID:56
      .= M*(j,Center M)`2 by A2,JORDAN8:def 1;
  end;
end;

Lm10: now
  let D, n;
  let e, w be real number;
A1: 2|^n <> 0 by NEWTON:102;
  thus w+(e-w)/(2|^n)*(len Gauge(D,n)-2)
  = w+(e-w)/(2|^n)*(2|^n+3-2) by JORDAN8:def 1
    .= w+(e-w)/(2|^n)*2|^n+(e-w)/(2|^n)
    .= w+(e-w)+(e-w)/(2|^n) by A1,XCMPLX_1:88
    .= e+(e-w)/(2|^n);
end;

Lm11: now
  let D, n, i;
  set a = N-bound D, s = S-bound D, w = W-bound D, e = E-bound D,
  G = Gauge(D,n);
  assume [i,1] in Indices G;
  hence G*(i,1)`2 = |[w+(e-w)/(2|^n)*(i - 2), s+(a-s)/(2|^n)*(1 - 2)]|`2
  by JORDAN8:def 1
    .= s-(a-s)/(2|^n) by EUCLID:56;
end;

Lm12: now
  let D, n, i;
  set a = N-bound D, s = S-bound D, w = W-bound D, e = E-bound D,
  G = Gauge(D,n);
  assume [1,i] in Indices G;
  hence G*(1,i)`1 = |[w+(e-w)/(2|^n)*(1 - 2), s+(a-s)/(2|^n)*(i - 2)]|`1
  by JORDAN8:def 1
    .= w-(e-w)/(2|^n) by EUCLID:56;
end;

Lm13: now
  let D, n, i;
  set a = N-bound D, s = S-bound D, w = W-bound D, e = E-bound D,
  G = Gauge(D,n);
  assume [i,len G] in Indices G;
  hence G*(i,len G)`2
  = |[w+(e-w)/(2|^n)*(i - 2), s+(a-s)/(2|^n)*(len G - 2)]|`2 by JORDAN8:def 1
    .= s+(a-s)/(2|^n)*(len G-2) by EUCLID:56
    .= a+(a-s)/(2|^n) by Lm10;
end;

Lm14: now
  let D, n, i;
  set a = N-bound D, s = S-bound D, w = W-bound D, e = E-bound D,
  G = Gauge(D,n);
  assume [len G,i] in Indices G;
  hence G*(len G,i)`1
  = |[w+(e-w)/(2|^n)*(len G - 2), s+(a-s)/(2|^n)*(i - 2)]|`1 by JORDAN8:def 1
    .= w+(e-w)/(2|^n)*(len G-2) by EUCLID:56
    .= e+(e-w)/(2|^n) by Lm10;
end;

theorem
  1 <= i & i <= len Gauge(C,1) implies
  Gauge(C,1)*(Center Gauge(C,1),i)`1 = (W-bound C + E-bound C) / 2
proof
  set a = N-bound C, s = S-bound C, w = W-bound C, e = E-bound C,
  G = Gauge(C,1);
  assume 1 <= i & i <= len G;
  then [Center G,i] in Indices G by Lm4;
  hence G*(Center G,i)`1
  = |[w+((e-w)/(2|^1))*(Center G-2),s+((a-s)/(2|^1))*(i-2)]|`1
  by JORDAN8:def 1
    .= w+(e-w)/(2|^1)*(Center G-2) by EUCLID:56
    .= w+(e-w)/2 by Lm6
    .= (w + e) / 2;
end;

theorem
  1 <= i & i <= len Gauge(C,1) implies
  Gauge(C,1)*(i,Center Gauge(C,1))`2 = (S-bound C + N-bound C) / 2
proof
  set a = N-bound C, s = S-bound C, w = W-bound C, e = E-bound C,
  G = Gauge(C,1);
  assume 1 <= i & i <= len G;
  then [i,Center G] in Indices G by Lm5;
  hence G*(i,Center G)`2
  = |[w+((e-w)/(2|^1))*(i-2),s+((a-s)/(2|^1))*(Center G-2)]|`2
  by JORDAN8:def 1
    .= s+(a-s)/(2|^1)*(Center G-2) by EUCLID:56
    .= s+(a-s)/2 by Lm6
    .= (s + a) / 2;
end;

theorem Th61:
  1 <= i & i <= len Gauge(E,n) & 1 <= j & j <= len Gauge(E,m) & m <= n implies
  Gauge(E,n)*(i,len Gauge(E,n))`2 <= Gauge(E,m)*(j,len Gauge(E,m))`2
proof
  set a = N-bound E, s = S-bound E, G = Gauge(E,n), M = Gauge(E,m);
  assume that
A1: 1 <= i & i <= len G & 1 <= j & j <= len M and
A2: m <= n;
A3: 0 < a - s by SPRECT_1:34,XREAL_1:52;
A4: 1 <= len G & 1 <= len M by A1,XXREAL_0:2;
  len G = width G & len M = width M by JORDAN8:def 1;
  then [i,len G] in Indices G & [j,len M] in Indices M by A1,A4,MATRIX_1:37;
  then G*(i,len G)`2 = a+(a-s)/(2|^n) & M*(j,len M)`2 = a+(a-s)/(2|^m)
  by Lm13;
  hence thesis by A2,A3,Lm8;
end;

theorem
  1 <= i & i <= len Gauge(E,n) & 1 <= j & j <= len Gauge(E,m) & m <= n implies
  Gauge(E,n)*(len Gauge(E,n),i)`1 <= Gauge(E,m)*(len Gauge(E,m),j)`1
proof
  set w = W-bound E, e = E-bound E, G = Gauge(E,n), M = Gauge(E,m);
  assume that
A1: 1 <= i & i <= len G & 1 <= j & j <= len M and
A2: m <= n;
A3: 0 < e - w by SPRECT_1:33,XREAL_1:52;
A4: 1 <= len G & 1 <= len M by A1,XXREAL_0:2;
  len G = width G & len M = width M by JORDAN8:def 1;
  then [len G,i] in Indices G & [len M,j] in Indices M by A1,A4,MATRIX_1:37;
  then G*(len G,i)`1 = e+(e-w)/(2|^n) & M*(len M,j)`1 = e+(e-w)/(2|^m)
  by Lm14;
  hence thesis by A2,A3,Lm8;
end;

theorem
  1 <= i & i <= len Gauge(E,n) & 1 <= j & j <= len Gauge(E,m) & m <= n implies
  Gauge(E,m)*(1,j)`1 <= Gauge(E,n)*(1,i)`1
proof
  set w = W-bound E, e = E-bound E, G = Gauge(E,n), M = Gauge(E,m);
  assume that
A1: 1 <= i & i <= len G & 1 <= j & j <= len M and
A2: m <= n;
A3: 0 < e - w by SPRECT_1:33,XREAL_1:52;
A4: 1 <= len G & 1 <= len M by A1,XXREAL_0:2;
  len G = width G & len M = width M by JORDAN8:def 1;
  then [1,i] in Indices G & [1,j] in Indices M by A1,A4,MATRIX_1:37;
  then G*(1,i)`1 = w-(e-w)/(2|^n) & M*(1,j)`1 = w-(e-w)/(2|^m) by Lm12;
  hence thesis by A2,A3,Lm9;
end;

theorem
  1 <= i & i <= len Gauge(E,n) & 1 <= j & j <= len Gauge(E,m) & m <= n implies
  Gauge(E,m)*(j,1)`2 <= Gauge(E,n)*(i,1)`2
proof
  set a = N-bound E, s = S-bound E, G = Gauge(E,n), M = Gauge(E,m);
  assume that
A1: 1 <= i & i <= len G & 1 <= j & j <= len M and
A2: m <= n;
A3: 0 < a - s by SPRECT_1:34,XREAL_1:52;
A4: 1 <= len G & 1 <= len M by A1,XXREAL_0:2;
  len G = width G & len M = width M by JORDAN8:def 1;
  then [i,1] in Indices G & [j,1] in Indices M by A1,A4,MATRIX_1:37;
  then G*(i,1)`2 = s-(a-s)/(2|^n) & M*(j,1)`2 = s-(a-s)/(2|^m) by Lm11;
  hence thesis by A2,A3,Lm9;
end;

theorem
  1 <= m & m <= n implies LSeg(Gauge(E,n)*(Center Gauge(E,n),1),
  Gauge(E,n)*(Center Gauge(E,n),len Gauge(E,n))) c=
  LSeg(Gauge(E,m)*(Center Gauge(E,m),1),
  Gauge(E,m)*(Center Gauge(E,m),len Gauge(E,m)))
proof
  set a = N-bound E, s = S-bound E, G = Gauge(E,n), M = Gauge(E,m),
  sn = Center G, sm = Center M;
  assume
A1: 1 <= m;
  assume
A2: m <= n;
  0 < a - s by SPRECT_1:34,XREAL_1:52;
  then
A3: (a-s)/(2|^n) <= (a-s)/(2|^m) by A2,Lm7;
A4: 1 <= len G & 1 <= len M by GOBRD11:34;
  then
A5: M*(sm,1)`1 = G*(sn,1)`1 & G*(sn,1)`1 = M*(sm,len M)`1 by A1,A2,Th57;
  [sn,1] in Indices G by A4,Lm4;
  then
A6: G*(sn,1)`2 = s-(a-s)/(2|^n) by Lm11;
  [sm,1] in Indices M by A4,Lm4;
  then M*(sm,1)`2 = s-(a-s)/(2|^m) by Lm11;
  then
A7: M*(sm,1)`2 <= G*(sn,1)`2 by A3,A6,XREAL_1:15;
  [sn,len G] in Indices G by A4,Lm4;
  then
A8: G*(sn,len G)`2 = a+(a-s)/(2|^n) by Lm13;
  [sm,len M] in Indices M by A4,Lm4;
  then M*(sm,len M)`2 = a+(a-s)/(2|^m) by Lm13;
  then
A9: G*(sn,len G)`2 <= M*(sm,len M)`2 by A3,A8,XREAL_1:9;
A10: len G = width G by JORDAN8:def 1;
  1 <= sn & sn <= len G by Lm3;
  then
A11: G*(sn,1)`2 <= G*(sn,len G)`2 by A4,A10,SPRECT_3:24;
  then G*(sn,1)`2 <= M*(sm,len M)`2 by A9,XXREAL_0:2;
  then
A12: G*(sn,1) in LSeg(M*(sm,1),M*(sm,len M)) by A5,A7,GOBOARD7:8;
A13: M*(sm,1)`1 = G*(sn,len G)`1 & G*(sn,len G)`1 = M*(sm,len M)`1
  by A1,A2,A4,Th57;
  M*(sm,1)`2 <= G*(sn,len G)`2 by A7,A11,XXREAL_0:2;
  then G*(sn,len G) in LSeg(M*(sm,1),M*(sm,len M)) by A9,A13,GOBOARD7:8;
  hence thesis by A12,TOPREAL1:12;
end;

theorem
  1 <= m & m <= n & 1 <= j & j <= width Gauge(E,n) implies
  LSeg(Gauge(E,n)*(Center Gauge(E,n),1), Gauge(E,n)*(Center Gauge(E,n),j)) c=
  LSeg(Gauge(E,m)*(Center Gauge(E,m),1), Gauge(E,n)*(Center Gauge(E,n),j))
proof
  set a = N-bound E, s = S-bound E, w = W-bound E, e = E-bound E,
  G = Gauge(E,n), M = Gauge(E,m), sn = Center G, sm = Center M;
  assume that
A1: 1 <= m and
A2: m <= n and
A3: 1 <= j and
A4: j <= width G;
  now
    let t be Element of NAT;
    assume that
A5: 1 <= t and
A6: t <= j;
A7: len G = width G by JORDAN8:def 1;
    then
A8: t <= len G by A4,A6,XXREAL_0:2;
A9: 0 < a - s by SPRECT_1:34,XREAL_1:52;
    then
A10: (a-s)/(2|^n) <= (a-s)/(2|^m) by A2,Lm7;
A11: 1 <= len M by GOBRD11:34;
    then
A12: M*(sm,1)`1 = G*(sn,t)`1 & G*(sn,t)`1 = G*(sn,j)`1
    by A1,A2,A3,A4,A5,A7,A8,Th57;
A13: [sn,t] in Indices G by A5,A8,Lm4;
    then
A14: G*(sn,t)`2 = |[w+(e-w)/(2|^n)*(sn - 2), s+(a-s)/(2|^n)*(t - 2)]| `2
    by JORDAN8:def 1
      .= s+(a-s)/(2|^n)*(t-2) by EUCLID:56;
    (a-s)/(2|^m) >= 0 by A9;
    then
A15: s-(a-s)/(2|^m) <= s-0 by XREAL_1:15;
    [sm,1] in Indices M by A11,Lm4;
    then
A16: M*(sm,1)`2 = s-(a-s)/(2|^m) by Lm11;
A17: (a-s)/(2|^n) >= 0 by A9;
A18: now per cases by A5,XXREAL_0:1;
      suppose t = 1;
        then G*(sn,t)`2 = s-(a-s)/(2|^n) by A13,Lm11;
        hence M*(sm,1)`2 <= G*(sn,t)`2 by A10,A16,XREAL_1:15;
      end;
      suppose t > 1;
        then t >= 1+1 by NAT_1:13;
        then t-2 >= 2-2 by XREAL_1:11;
        then (a-s)/(2|^n)*(t-2) >= 0 by A17;
        then s+0 <= s+(a-s)/(2|^n)*(t-2) by XREAL_1:8;
        hence M*(sm,1)`2 <= G*(sn,t)`2 by A14,A15,A16,XXREAL_0:2;
      end;
    end;
    1 <= sn & sn <= len G by Lm3;
    then G*(sn,t)`2 <= G*(sn,j)`2 by A4,A5,A6,SPRECT_3:24;
    hence G*(sn,t) in LSeg(M*(sm,1),G*(sn,j)) by A12,A18,GOBOARD7:8;
  end;
  then G*(sn,1) in LSeg(M*(sm,1),G*(sn,j)) &
  G*(sn,j) in LSeg(M*(sm,1),G*(sn,j)) by A3;
  hence thesis by TOPREAL1:12;
end;

theorem
  1 <= m & m <= n & 1 <= j & j <= width Gauge(E,n) implies
  LSeg(Gauge(E,m)*(Center Gauge(E,m),1), Gauge(E,n)*(Center Gauge(E,n),j)) c=
  LSeg(Gauge(E,m)*(Center Gauge(E,m),1),
  Gauge(E,m)*(Center Gauge(E,m),len Gauge(E,m)))
proof
  set a = N-bound E, s = S-bound E, w = W-bound E, e = E-bound E,
  G = Gauge(E,n), M = Gauge(E,m), sn = Center G, sm = Center M;
  assume that
A1: 1 <= m and
A2: m <= n and
A3: 1 <= j and
A4: j <= width G;
A5: 0 < a - s by SPRECT_1:34,XREAL_1:52;
  then
A6: (a-s)/(2|^n) <= (a-s)/(2|^m) by A2,Lm7;
A7: 1 <= len M by GOBRD11:34;
  then
A8: M*(sm,1)`1 = M*(sm,len M)`1 by A1,Th57;
A9: 1 <= sm & sm <= len M by Lm3;
  len M = width M by JORDAN8:def 1;
  then M*(sm,1)`2 <= M*(sm,len M)`2 by A7,A9,SPRECT_3:24;
  then
A10: M*(sm,1) in LSeg(M*(sm,1),M*(sm,len M)) by A8,GOBOARD7:8;
A11: len G = width G by JORDAN8:def 1;
  then
A12: M*(sm,1)`1 = G*(sn,j)`1 & G*(sn,j)`1 = M*(sm,len M)`1
  by A1,A2,A3,A4,A7,Th57;
  [sm,1] in Indices M by A7,Lm4;
  then
A13: M*(sm,1)`2 = s-(a-s)/(2|^m) by Lm11;
A14: [sn,j] in Indices G by A3,A4,A11,Lm4;
  then
A15: G*(sn,j)`2 = |[w+(e-w)/(2|^n)*(sn - 2), s+(a-s)/(2|^n)*(j - 2)]|`2
  by JORDAN8:def 1
    .= s+(a-s)/(2|^n)*(j-2) by EUCLID:56;
  (a-s)/(2|^m) >= 0 by A5;
  then
A16: s-(a-s)/(2|^m) <= s-0 by XREAL_1:15;
A17: (a-s)/(2|^n) >= 0 by A5;
A18: now per cases by A3,XXREAL_0:1;
    suppose j = 1;
      then G*(sn,j)`2 = s-(a-s)/(2|^n) by A14,Lm11;
      hence M*(sm,1)`2 <= G*(sn,j)`2 by A6,A13,XREAL_1:15;
    end;
    suppose j > 1;
      then j >= 1+1 by NAT_1:13;
      then j-2 >= 2-2 by XREAL_1:11;
      then (a-s)/(2|^n)*(j-2) >= 0 by A17;
      then s+0 <= s+(a-s)/(2|^n)*(j-2) by XREAL_1:8;
      hence M*(sm,1)`2 <= G*(sn,j)`2 by A13,A15,A16,XXREAL_0:2;
    end;
  end;
A19: len G = width G by JORDAN8:def 1;
A20: 1 <= sn & sn <= len G by Lm3;
  then
A21: G*(sn,j)`2 <= G*(sn,len G)`2 by A3,A4,A19,SPRECT_3:24;
  G*(sn,len G)`2 <= M*(sm,len M)`2 by A2,A9,A20,Th61;
  then G*(sn,j)`2 <= M*(sm,len M)`2 by A21,XXREAL_0:2;
  then G*(sn,j) in LSeg(M*(sm,1),M*(sm,len M)) by A12,A18,GOBOARD7:8;
  hence thesis by A10,TOPREAL1:12;
end;

theorem Th68:
  m <= n & 1 < i & i+1 < len Gauge(E,m) & 1 < j & j+1 < width Gauge(E,m)
  implies for i1,j1 being Element of NAT st
  2|^(n-'m)*(i-2)+2 <= i1 & i1 < 2|^(n-'m)*(i-1)+2 &
  2|^(n-'m)*(j-2)+2 <= j1 & j1 < 2|^(n-'m)*(j-1)+2
  holds cell(Gauge(E,n),i1,j1) c= cell(Gauge(E,m),i,j)
proof
  set G = Gauge(E,m), G1 = Gauge(E,n);
  assume that
A1: m <= n and
A2: 1 < i and
A3: i+1 < len G and
A4: 1 < j and
A5: j+1 < width G;
  i < i+1 by XREAL_1:31;
  then
A6: i < len G by A3,XXREAL_0:2;
  j < j+1 by XREAL_1:31;
  then
A7: j < width G by A5,XXREAL_0:2;
A8: 1+1 <= i by A2,NAT_1:13;
A9: 1+1 <= j by A4,NAT_1:13;
  let i1,j1 be Element of NAT such that
A10: 2|^(n-'m)*(i-2)+2 <= i1 and
A11: i1 < 2|^(n-'m)*(i-1)+2 and
A12: 2|^(n-'m)*(j-2)+2 <= j1 and
A13: j1 < 2|^(n-'m)*(j-1)+2;
  let e be set;
  assume
A14: e in cell(G1,i1,j1);
  then reconsider p = e as Point of TOP-REAL 2;
  set i2 = 2|^(n-'m)*(i-'2)+2, j2 = 2|^(n-'m)*(j-'2)+2,
  i3 = 2|^(n-'m)*(i-'1)+2, j3 = 2|^(n-'m)*(j-'1)+2;
  2|^(n-'m)*(i-1)+2 <= len G1 by A1,A2,A3,Th55;
  then
A15: i1 < len G1 by A11,XXREAL_0:2;
  then
A16: i1+1 <= len G1 by NAT_1:13;
  2|^(n-'m)*(j-1)+2 <= width G1 by A1,A4,A5,Th56;
  then
A17: j1 < width G1 by A13,XXREAL_0:2;
  then
A18: j1+1 <= width G1 by NAT_1:13;
A19: 1 <= 2|^(n-'m)*(i-2)+2 by A1,A2,A6,Th52;
  then
A20: 1 <= i1 by A10,XXREAL_0:2;
A21: 1 <= 2|^(n-'m)*(j-2)+2 by A1,A4,A7,Th53;
  then
A22: 1 <= j1 by A12,XXREAL_0:2;
  then
A23: G1*(i1,j1)`1 <= p`1 & p`1 <= G1*(i1+1,j1)`1 &
  G1*(i1,j1)`2 <= p`2 & p`2 <= G1* (i1,j1+1)`2 by A14,A16,A18,A20,JORDAN9:19;
A24: i2 = 2|^(n-'m)*(i-2)+2 & j2 = 2|^(n-'m)*(j-2)+2 by A8,A9,XREAL_1:235;
  then
A25: G*(i,j) = Gauge(E,n)*(i2,j2) by A1,A2,A4,A6,A7,Th54;
A26: i2 <= len G1 by A10,A15,A24,XXREAL_0:2;
A27: j2 <= width G1 by A12,A17,A24,XXREAL_0:2;
  i2 < i1 or i2 = i1 by A10,A24,XXREAL_0:1;
  then
A28: G1*(i2,j1)`1 <= G1*(i1,j1)`1 by A15,A17,A19,A22,A24,GOBOARD5:4;
  G1*(i2,j1)`1 = G1*(i2,1)`1 by A17,A19,A22,A24,A26,GOBOARD5:3
    .= G1*(i2,j2)`1 by A19,A21,A24,A26,A27,GOBOARD5:3;
  then
A29: G*(i,j)`1 <= p`1 by A23,A25,A28,XXREAL_0:2;
A30: 1 < i+1 by A2,XREAL_1:31;
A31: 1 < j+1 by A4,XREAL_1:31;
A32: 1 < i1+1 by A20,NAT_1:13;
A33: 1 < j1+1 by A22,NAT_1:13;
A34: i+1-(1+1) = i-1
    .= i-'1 by A2,XREAL_1:235;
A35: j+1-(1+1) = j-1
    .= j-'1 by A4,XREAL_1:235;
A36: i2 = 2|^(n-'m)*(i-2)+2 & j2 = 2|^(n-'m)*(j-2)+2 by A8,A9,XREAL_1:235;
  then
A37: G*(i+1,j) = G1*(i3,j2) by A1,A3,A4,A7,A30,A34,Th54;
A38: i-1 = i-'1 by A2,XREAL_1:235;
  then
A39: i3 <= len G1 by A1,A2,A3,Th55;
  i1+1 <= 2|^(n-'m)*(i-'1)+2 by A11,A38,NAT_1:13;
  then i1+1 < i3 or i1+1 = i3 by XXREAL_0:1;
  then
A40: G1*(i1+1,j1)`1 <= G1*(i3,j1)`1 by A17,A22,A32,A39,GOBOARD5:4;
A41: 1 < i3 by A11,A20,A38,XXREAL_0:2;
  then G1*(i3,j1)`1 = G1*(i3,1)`1 by A17,A22,A39,GOBOARD5:3
    .= G1*(i3,j2)`1 by A21,A27,A36,A39,A41,GOBOARD5:3;
  then
A42: p`1 <= G*(i+1,j)`1 by A23,A37,A40,XXREAL_0:2;
  j2 < j1 or j2 = j1 by A12,A36,XXREAL_0:1;
  then
A43: G1*(i1,j2)`2 <= G1*(i1,j1)`2 by A15,A17,A20,A21,A36,GOBOARD5:5;
  G1*(i1,j2)`2 = G1*(1,j2)`2 by A15,A20,A21,A27,A36,GOBOARD5:2
    .= G1*(i2,j2)`2 by A19,A21,A26,A27,A36,GOBOARD5:2;
  then
A44: G*(i,j)`2 <= p`2 by A23,A25,A43,XXREAL_0:2;
A45: i2 = 2|^(n-'m)*(i-2)+2 & j2 = 2|^(n-'m)*(j-2)+2 by A8,A9,XREAL_1:235;
  then
A46: G*(i,j+1) = G1*(i2,j3) by A1,A2,A5,A6,A31,A35,Th54;
A47: j-1 = j-'1 by A4,XREAL_1:235;
  then
A48: j3 <= width G1 by A1,A4,A5,Th56;
  j1+1 <= 2|^(n-'m)*(j-'1)+2 by A13,A47,NAT_1:13;
  then j1+1 < j3 or j1+1 = j3 by XXREAL_0:1;
  then
A49: G1*(i1,j1+1)`2 <= G1*(i1,j3)`2 by A15,A20,A33,A48,GOBOARD5:5;
A50: 1 < j3 by A13,A22,A47,XXREAL_0:2;
  then G1*(i1,j3)`2 = G1*(1,j3)`2 by A15,A20,A48,GOBOARD5:2
    .= G1*(i2,j3)`2 by A19,A26,A45,A48,A50,GOBOARD5:2;
  then p`2 <= G*(i,j+1)`2 by A23,A46,A49,XXREAL_0:2;
  hence e in cell(G,i,j) by A2,A3,A4,A5,A29,A42,A44,JORDAN9:19;
end;

theorem
  m <= n & 3 <= i & i < len Gauge(E,m) & 1 < j & j+1 < width Gauge(E,m)
  implies for i1,j1 being Element of NAT st
  i1 = 2|^(n-'m)*(i-2)+2 & j1 = 2|^(n-'m)*(j-2)+2
  holds cell(Gauge(E,n),i1-'1,j1) c= cell(Gauge(E,m),i-'1,j)
proof
  assume that
A1: m <= n and
A2: 3 <= i & i < len Gauge(E,m) and
A3: 1 < j & j+1 < width Gauge(E,m);
  let i1,j1 be Element of NAT such that
A4: i1 = 2|^(n-'m)*(i-2)+2 and
A5: j1 = 2|^(n-'m)*(j-2)+2;
A6: 2+1 <= i by A2;
  then 1+1 < i by NAT_1:13;
  then
A7: 1 < i-1 by XREAL_1:22;
A8: i-'1+1 < len Gauge(E,m) by A2,XREAL_1:237,XXREAL_0:2;
A9: 2|^(n-'m) > 0 by NEWTON:102;
  j-2 < j-1 by REAL_2:105;
  then 2|^(n-'m)*(j-2) < 2|^(n-'m)*(j-1) by A9,XREAL_1:70;
  then
A10: j1 < 2|^(n-'m)*(j-1)+2 by A5,XREAL_1:8;
A11: i-2 = i-'2 by A2,XREAL_1:235,XXREAL_0:2;
A12: i-3 = i-'3 by A2,XREAL_1:235;
  i > 2+0 by A6,NAT_1:13;
  then i-2 > 0 by XREAL_1:22;
  then
A13: 2|^(n-'m)*(i-'2) > 0 by A9,A11,REAL_2:122;
  then
A14: 2|^(n-'m)*(i-'2) >= 0+1 by NAT_1:13;
A15: i-'1 = i-1 by A2,XREAL_1:235,XXREAL_0:2;
  i -' 3 < i -' 2 by A11,A12,REAL_2:105;
  then 2|^(n-'m)*(i-'3) < 2|^(n-'m)*(i-'2) by A9,XREAL_1:70;
  then 2|^(n-'m)*(i-'3) + 1 <= 2|^(n-'m)*(i-'2) by NAT_1:13;
  then 2|^(n-'m)*(i-'3) <= 2|^(n-'m)*(i-'2)-'1 by NAT_D:55;
  then 2|^(n-'m)*(i-'3)+2 <= 2|^(n-'m)*(i-'2)-'1+2 by XREAL_1:8;
  then
A16: 2|^(n-'m)*(i-'1-2)+2 <= 2|^(n-'m)*(i-'2)+2-'1 by A12,A14,A15,NAT_D:38;
A17: i -'1 -1 = i-(1+1) by A15;
A18: i1 > 0+2 by A4,A11,A13,XREAL_1:8;
  i1 < i1+1 by XREAL_1:31;
  then i1-1 < i1 by XREAL_1:21;
  then i1-'1 < i1 by A18,XREAL_1:235,XXREAL_0:2;
  hence cell(Gauge(E,n),i1-'1,j1) c= cell(Gauge(E,m),i-'1,j)
  by A1,A3,A4,A5,A7,A8,A10,A11,A16,A17,Th68;
end;

theorem
  for C being compact non vertical non horizontal Subset of TOP-REAL 2 holds
  i <= len Gauge(C,n) implies cell(Gauge(C,n),i,0) c= UBD C
proof
  let C be compact non vertical non horizontal Subset of TOP-REAL 2;
  assume
A1: i <= len Gauge(C,n);
  then cell(Gauge(C,n),i,0) misses C by JORDAN8:20;
  then
A2: cell(Gauge(C,n),i,0) c= C` by SUBSET_1:43;
A3: 0 <= width Gauge(C,n);
  then
A4: cell(Gauge(C,n),i,0) is connected by A1,Th46;
A5: C` is non empty by JORDAN2C:74;
  cell(Gauge(C,n),i,0) is non empty by A1,A3,Th45;
  then consider W being Subset of TOP-REAL 2 such that
A6: W is_a_component_of C` and
A7: cell(Gauge(C,n),i,0) c= W by A2,A4,A5,GOBOARD9:5;
  W is not Bounded by A1,A7,Th47,JORDAN2C:16;
  then W is_outside_component_of C by A6,JORDAN2C:def 4;
  then W c= UBD C by JORDAN2C:27;
  hence cell(Gauge(C,n),i,0) c= UBD C by A7,XBOOLE_1:1;
end;

theorem
  for C being compact non vertical non horizontal Subset of TOP-REAL 2 holds
  i <= len Gauge(E,n) implies cell(Gauge(E,n),i,width Gauge(E,n)) c= UBD E
proof
  let C be compact non vertical non horizontal Subset of TOP-REAL 2;
A1: width Gauge(E,n) = len Gauge(E,n) by JORDAN8:def 1;
  assume
A2: i <= len Gauge(E,n);
  then cell(Gauge(E,n),i,width Gauge(E,n)) misses E by A1,JORDAN8:18;
  then
A3: cell(Gauge(E,n),i,width Gauge(E,n)) c= E` by SUBSET_1:43;
A4: cell(Gauge(E,n),i,width Gauge(E,n)) is connected by A2,Th46;
A5: E` is non empty by JORDAN2C:74;
  cell(Gauge(E,n),i,width Gauge(E,n)) is non empty by A2,Th45;
  then consider W being Subset of TOP-REAL 2 such that
A6: W is_a_component_of E` and
A7: cell(Gauge(E,n),i,width Gauge(E,n)) c= W by A3,A4,A5,GOBOARD9:5;
  W is not Bounded by A2,A7,Th48,JORDAN2C:16;
  then W is_outside_component_of E by A6,JORDAN2C:def 4;
  then W c= UBD E by JORDAN2C:27;
  hence cell(Gauge(E,n),i,width Gauge(E,n)) c= UBD E by A7,XBOOLE_1:1;
end;

begin  :: Cages

theorem Th72:
  p in C implies north_halfline p meets L~Cage(C,n)
proof
  set f = Cage(C,n);
  assume
A1: p in C;
  assume
A2: not thesis;
A3: C c= BDD L~f by JORDAN10:12;
  set X = {q where q is Point of TOP-REAL 2: q`1 = p`1 & q`2 >= p`2};
A4: X = north_halfline p by TOPREAL1:37;
  then reconsider X as connected Subset of TOP-REAL 2;
A5: p in X;
A6: f/.1 = N-min L~f by JORDAN9:34;
  max(N-bound (L~f),p`2)+1 > N-bound (L~f)+0 by XREAL_1:10,XXREAL_0:25;
  then |[p`1,max(N-bound (L~f),p`2)+1]|`2 > N-bound (L~f) by EUCLID:56;
  then |[p`1,max(N-bound (L~f),p`2)+1]| in LeftComp f by A6,JORDAN2C:121;
  then
A7: |[p`1,max(N-bound (L~f),p`2)+1]| in UBD L~f by GOBRD14:46;
  max(N-bound (L~f),p`2) >= p`2 by XXREAL_0:25;
  then max(N-bound (L~f),p`2)+1 >= p`2+0 by XREAL_1:9;
  then
A8: |[p`1,max(N-bound (L~f),p`2)+1]|`2 >= p`2 by EUCLID:56;
  |[p`1,max(N-bound (L~f),p`2)+1]|`1 = p`1 by EUCLID:56;
  then |[p`1,max(N-bound (L~f),p`2)+1]| in X by A8;
  then
A9: X meets UBD L~f by A7,XBOOLE_0:3;
  LeftComp f is_outside_component_of L~f by GOBRD14:44;
  then LeftComp f is_a_component_of (L~f)` by JORDAN2C:def 4;
  then
A10: UBD L~f is_a_component_of (L~f)` by GOBRD14:46;
  X c= (L~f)` by A2,A4,SUBSET_1:43;
  then X c= UBD L~f by A9,A10,GOBOARD9:6;
  then p in BDD L~f /\ UBD L~f by A1,A3,A5,XBOOLE_0:def 4;
  then BDD L~f meets UBD L~f by XBOOLE_0:4;
  hence contradiction by JORDAN2C:28;
end;

