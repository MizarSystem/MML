A2: p in west_halfline x /\ L~f;
  then
A3: p in west_halfline x by XBOOLE_0:def 4;
A4: p in L~f by A2,XBOOLE_0:def 4;
A5: p`1 <= x`1 by A3,TOPREAL1:def 15;
A6: p`2 = x`2 by A3,TOPREAL1:def 15;
  assume p`1 >= x`1;
  then p`1 = x`1 by A5,XXREAL_0:1;
