theorem Th99:
  x in N-most C & p in north_halfline x &
  1 <= i & i < len Cage(C,n) & p in LSeg(Cage(C,n),i) implies
  LSeg(Cage(C,n),i) is horizontal
proof
  set G = Gauge(C,n), f = Cage(C,n);
  assume that
A1: x in N-most C and
A2: p in north_halfline x and
A3: 1 <= i and
A4: i < len f and
A5: p in LSeg(f,i);
A6: i+1 <= len f by A4,NAT_1:13;
  then
A7: LSeg(f,i) = LSeg(f/.i,f/.(i+1)) by A3,TOPREAL1:def 5;
  4 <= len G by JORDAN8:13;
  then
A8: 1 < len G by XXREAL_0:2;
A9: len G = width G by JORDAN8:def 1;
  then
A10: 1 <= len G-'1 & len G-'1 <= width G by A8,NAT_D:35,49;
  assume
A11: not thesis;
A12: x in C by A1,XBOOLE_0:def 4;
  p in L~f by A5,SPPOL_2:17;
  then
A13: p in north_halfline x /\ L~f by A2,XBOOLE_0:def 4;
A14: x`1 = p`1 by A2,TOPREAL1:def 12
    .= (f/.(i+1))`1 by A5,A7,A11,SPPOL_1:41,64;
A15: x`1 = p`1 by A2,TOPREAL1:def 12
    .= (f/.i)`1 by A5,A7,A11,SPPOL_1:41,64;
A16: f is_sequence_on G by JORDAN9:def 1;
  i in Seg len f by A3,A4,FINSEQ_1:3;
  then
A17: i in dom f by FINSEQ_1:def 3;
  1 <= i+1 by A3,NAT_1:13;
  then i+1 in Seg len f by A6,FINSEQ_1:3;
  then
A18: i+1 in dom f by FINSEQ_1:def 3;
  per cases;
  suppose
A19: (f/.i)`2 <= (f/.(i+1))`2;
    then p`2 <= (f/.(i+1))`2 by A5,A7,TOPREAL1:10;
    then
A20: (f/.(i+1))`2 > x`2 by A12,A13,Th95,XXREAL_0:2;
    consider i1,i2 being Element of NAT such that
A21: [i1,i2] in Indices G and
A22: f/.(i+1) = G*(i1,i2) by A16,A18,GOBOARD1:def 11;
A23: 1 <= i2 & i2 <= width G & 1 <= i1 & i1 <= len G by A21,MATRIX_1:39;
    then
A24: 1 <= i2 & i2 <= len G by JORDAN8:def 1;
A25: x`2 = (N-min C)`2 by A1,PSCOMP_1:98
      .= N-bound C by EUCLID:56
      .= G*(i1,len G-'1)`2 by A23,JORDAN8:17;
    consider j1,j2 being Element of NAT such that
A26: [j1,j2] in Indices G and
A27: f/.i = G*(j1,j2) by A16,A17,GOBOARD1:def 11;
A28: 1 <= j2 & j2 <= width G & 1 <= j1 & j1 <= len G by A26,MATRIX_1:39;
    now
      assume (f/.i)`2 = (f/.(i+1))`2;
      then
A29:  f/.i = f/.(i+1) by A14,A15,TOPREAL3:11;
      then
A30:  i1 = j1 & i2=j2 by A21,A22,A26,A27,GOBOARD1:21;
      abs(i1-j1)+abs(i2-j2) = 1
      by A16,A17,A18,A21,A22,A26,A27,A29,GOBOARD1:def 11;
      then 1 = 0 + abs(i2-j2) by A30,GOBOARD7:2
        .= 0 + 0 by A30,GOBOARD7:2;
      hence contradiction;
    end;
    then (f/.i)`2 < (f/.(i+1))`2 by A19,XXREAL_0:1;
    then i2 > j2 by A22,A23,A27,A28,Th40;
    then len G > j2 by A24,XXREAL_0:2;
    then len G-'1 >= j2 by NAT_D:49;
    then x`2 >= (f/.i)`2 by A10,A23,A25,A27,A28,Th40;
    then x in L~f by A7,A14,A15,A20,GOBOARD7:8,SPPOL_2:17;
    then L~f meets C by A12,XBOOLE_0:3;
    hence contradiction by JORDAN10:5;
  end;
  suppose
A31: (f/.i)`2 >= (f/.(i+1))`2;
    then p`2 <= (f/.i)`2 by A5,A7,TOPREAL1:10;
    then
A32: (f/.i)`2 > x`2 by A12,A13,Th95,XXREAL_0:2;
    consider i1,i2 being Element of NAT such that
A33: [i1,i2] in Indices G and
A34: f/.i = G*(i1,i2) by A16,A17,GOBOARD1:def 11;
A35: 1 <= i2 & i2 <= width G & 1 <= i1 & i1 <= len G by A33,MATRIX_1:39;
A36: x`2 = (N-min C)`2 by A1,PSCOMP_1:98
      .= N-bound C by EUCLID:56
      .= G*(i1,len G-'1)`2 by A35,JORDAN8:17;
    consider j1,j2 being Element of NAT such that
A37: [j1,j2] in Indices G and
A38: f/.(i+1) = G*(j1,j2) by A16,A18,GOBOARD1:def 11;
A39: 1 <= j2 & j2 <= width G & 1 <= j1 & j1 <= len G by A37,MATRIX_1:39;
    now
      assume (f/.i)`2 = (f/.(i+1))`2;
      then
A40:  f/.i = f/.(i+1) by A14,A15,TOPREAL3:11;
      then
A41:  i1 = j1 & i2=j2 by A33,A34,A37,A38,GOBOARD1:21;
      abs(j1-i1)+abs(j2-i2) = 1
      by A16,A17,A18,A33,A34,A37,A38,A40,GOBOARD1:def 11;
      then 1 = 0 + abs(i2-j2) by A41,GOBOARD7:2
        .= 0 + 0 by A41,GOBOARD7:2;
      hence contradiction;
    end;
    then (f/.(i+1))`2 < (f/.i)`2 by A31,XXREAL_0:1;
    then i2 > j2 by A34,A35,A38,A39,Th40;
    then len G > j2 by A9,A35,XXREAL_0:2;
    then len G-'1 >= j2 by NAT_D:49;
    then x`2 >= (f/.(i+1))`2 by A10,A35,A36,A38,A39,Th40;
    then x in L~f by A7,A14,A15,A32,GOBOARD7:8,SPPOL_2:17;
    then x in L~f /\ C by A12,XBOOLE_0:def 4;
    then L~f meets C by XBOOLE_0:4;
    hence contradiction by JORDAN10:5;
  end;
end;

theorem Th100:
  x in E-most C & p in east_halfline x &
  1 <= i & i < len Cage(C,n) & p in LSeg(Cage(C,n),i) implies
  LSeg(Cage(C,n),i) is vertical
proof
  set G = Gauge(C,n), f = Cage(C,n);
  assume that
A1: x in E-most C and
A2: p in east_halfline x and
A3: 1 <= i and
A4: i < len f and
A5: p in LSeg(f,i);
A6: i+1 <= len f by A4,NAT_1:13;
  then
A7: LSeg(f,i) = LSeg(f/.i,f/.(i+1)) by A3,TOPREAL1:def 5;
  4 <= len G by JORDAN8:13;
  then
A8: 1 < len G by XXREAL_0:2;
A9: len G = width G by JORDAN8:def 1;
  then
A10: 1 <= len G-'1 & len G-'1 <= width G by A8,NAT_D:35,49;
  assume
A11: not thesis;
A12: x in C by A1,XBOOLE_0:def 4;
  p in L~f by A5,SPPOL_2:17;
  then
A13: p in east_halfline x /\ L~f by A2,XBOOLE_0:def 4;
A14: x`2 = p`2 by A2,TOPREAL1:def 13
    .= (f/.(i+1))`2 by A5,A7,A11,SPPOL_1:41,63;
A15: x`2 = p`2 by A2,TOPREAL1:def 13
    .= (f/.i)`2 by A5,A7,A11,SPPOL_1:41,63;
A16: f is_sequence_on G by JORDAN9:def 1;
  i in Seg len f by A3,A4,FINSEQ_1:3;
  then
A17: i in dom f by FINSEQ_1:def 3;
  1 <= i+1 by A3,NAT_1:13;
  then i+1 in Seg len f by A6,FINSEQ_1:3;
  then
A18: i+1 in dom f by FINSEQ_1:def 3;
  per cases;
  suppose
A19: (f/.i)`1 <= (f/.(i+1))`1;
    then p`1 <= (f/.(i+1))`1 by A5,A7,TOPREAL1:9;
    then
A20: (f/.(i+1))`1 > x`1 by A12,A13,Th96,XXREAL_0:2;
    consider i1,i2 being Element of NAT such that
A21: [i1,i2] in Indices G and
A22: f/.(i+1) = G*(i1,i2) by A16,A18,GOBOARD1:def 11;
A23: 1 <= i2 & i2 <= width G & 1 <= i1 & i1 <= len G by A21,MATRIX_1:39;
A24: x`1 = (E-min C)`1 by A1,PSCOMP_1:108
      .= E-bound C by EUCLID:56
      .= G*(len G-'1,i2)`1 by A9,A23,JORDAN8:15;
    consider j1,j2 being Element of NAT such that
A25: [j1,j2] in Indices G and
A26: f/.i = G*(j1,j2) by A16,A17,GOBOARD1:def 11;
A27: 1 <= j2 & j2 <= width G & 1 <= j1 & j1 <= len G by A25,MATRIX_1:39;
    now
      assume (f/.i)`1 = (f/.(i+1))`1;
      then
A28:  f/.i = f/.(i+1) by A14,A15,TOPREAL3:11;
      then
A29:  i1 = j1 & i2=j2 by A21,A22,A25,A26,GOBOARD1:21;
      abs(i1-j1)+abs(i2-j2) = 1
      by A16,A17,A18,A21,A22,A25,A26,A28,GOBOARD1:def 11;
      then 1 = 0 + abs(i2-j2) by A29,GOBOARD7:2
        .= 0 + 0 by A29,GOBOARD7:2;
      hence contradiction;
    end;
    then (f/.i)`1 < (f/.(i+1))`1 by A19,XXREAL_0:1;
    then i1 > j1 by A22,A23,A26,A27,Th39;
    then len G > j1 by A23,XXREAL_0:2;
    then len G-'1 >= j1 by NAT_D:49;
    then x`1 >= (f/.i)`1 by A9,A10,A23,A24,A26,A27,Th39;
    then x in L~f by A7,A14,A15,A20,GOBOARD7:9,SPPOL_2:17;
    then x in L~f /\ C by A12,XBOOLE_0:def 4;
    then L~f meets C by XBOOLE_0:4;
    hence contradiction by JORDAN10:5;
  end;
  suppose
A30: (f/.i)`1 >= (f/.(i+1))`1;
    then p`1 <= (f/.i)`1 by A5,A7,TOPREAL1:9;
    then
A31: (f/.i)`1 > x`1 by A12,A13,Th96,XXREAL_0:2;
    consider i1,i2 being Element of NAT such that
A32: [i1,i2] in Indices G and
A33: f/.i = G*(i1,i2) by A16,A17,GOBOARD1:def 11;
A34: 1 <= i2 & i2 <= width G & 1 <= i1 & i1 <= len G by A32,MATRIX_1:39;
A35: x`1 = (E-min C)`1 by A1,PSCOMP_1:108
      .= E-bound C by EUCLID:56
      .= G*(len G-'1,i2)`1 by A9,A34,JORDAN8:15;
    consider j1,j2 being Element of NAT such that
A36: [j1,j2] in Indices G and
A37: f/.(i+1) = G*(j1,j2) by A16,A18,GOBOARD1:def 11;
A38: 1 <= j2 & j2 <= width G & 1 <= j1 & j1 <= len G by A36,MATRIX_1:39;
    now
      assume (f/.i)`1 = (f/.(i+1))`1;
      then
A39:  f/.i = f/.(i+1) by A14,A15,TOPREAL3:11;
      then
A40:  i1 = j1 & i2=j2 by A32,A33,A36,A37,GOBOARD1:21;
      abs(j1-i1)+abs(j2-i2) = 1
      by A16,A17,A18,A32,A33,A36,A37,A39,GOBOARD1:def 11;
      then 1 = 0 + abs(i2-j2) by A40,GOBOARD7:2
