A2: p in south_halfline x /\ L~f;
  then
A3: p in south_halfline x by XBOOLE_0:def 4;
A4: p in L~f by A2,XBOOLE_0:def 4;
A5: p`1 = x`1 by A3,TOPREAL1:def 14;
A6: p`2 <= x`2 by A3,TOPREAL1:def 14;
  assume p`2 >= x`2;
  then p`2 = x`2 by A6,XXREAL_0:1;
  then p = x by A5,TOPREAL3:11;
