theorem
  for i,j being natural number holds
  j > 0 & i mod j = 0 implies i div j = i / j by PEPIN:68;

theorem Th5:
  for i,n being natural number holds
  n > 0 implies i |^ n div i = i |^ n / i by PEPIN:69;

theorem
 for n being natural number holds
 0 < n & 1 < r implies 1 < r|^n by PEPIN:70;

theorem Th7:
  for m,n being natural number holds
  r > 1 & m > n implies r|^m > r|^n by PEPIN:71;

theorem Th8:
 for T being non empty TopSpace,
     A being Subset of T,
     B, C being Subset of T st
   A is connected & C is_a_component_of B & A meets C & A c= B holds A c= C
   by GOBOARD9:6;

definition let f be FinSequence;
  func Center f -> Element of NAT equals
  len f div 2 + 1;
  coherence;
end;

theorem
  for f being FinSequence st len f is odd holds len f = 2 * Center f - 1
  proof
    let f be FinSequence;
    assume len f is odd;
    then consider k being Element of NAT such that
A1:   len f = 2*k+1 by ABIAN:9;
A2: 2*k mod 2 = 0 by NAT_1:63;
    thus len f
       = 2 * ((2*k div 2) + (1 div 2)) + 1 by A1,Lm2,NAT_1:68
      .= 2 * (len f div 2) + (2*1 - 1) by A1,A2,NAT_1:69
      .= 2 * Center f - 1;
  end;

theorem
  for f being FinSequence st len f is even holds len f = 2 * Center f - 2
  proof
    let f be FinSequence;
    assume ex k being Element of NAT st len f = 2*k;
    hence len f = 2 * (len f div 2) + 2*1 - 2 by NAT_1:68
       .= 2 * Center f - 2;
  end;

