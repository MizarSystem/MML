          suppose
A116:        x=dj & y=j;
               1 in dj & not 1 in j by Th2,TARSKI:def 1;
          then not dj c= j;
          hence thesis by A108,A116,YELLOW_1:3;
          end;
          suppose x=dj & y=dj;hence thesis;
          end;
          suppose
A117:        x=dj & y=td;
               1 in dj & not 1 in td by Th2,Th4,TARSKI:def 1;
          then not dj c= td;
          hence thesis by A108,A117,YELLOW_1:3;
          end;
          suppose x=dj & y=t;
          then A118:       f.x = c & f.y = e by A96;
          reconsider C=c,E=e as Element of K by A63,ENUMSET1:def 3;
               c <= e & C in the carrier of K & E in the carrier of K
                                          by A62,YELLOW_0:25;
          hence f.x <= f.y by A118,YELLOW_0:61;
          end;
          suppose x=td & y=z;
          then td c= z by A108,YELLOW_1:3;
          hence thesis by Th4,XBOOLE_1:3;
          end;
          suppose
A119:        x=td & y=j;
               2 in td & not 2 in j by Th4,CARD_5:1,TARSKI:def 1;
          then not td c= j;
          hence thesis by A108,A119,YELLOW_1:3;
          end;
          suppose
A120:        x=td & y=dj;
               2 in td & not 2 in dj by Th2,Th4,TARSKI:def 1;
          then not td c= dj;
          hence thesis by A108,A120,YELLOW_1:3;
          end;
          suppose x=td & y=td;hence thesis;
          end;
          suppose x=td & y=t;
          then A121:       f.x = d & f.y = e by A96;
          reconsider D=d,E=e as Element of K by A63,ENUMSET1:def 3;
               d <= e & D in the carrier of K & E in the carrier of K
                                          by A62,YELLOW_0:25;
          hence f.x <= f.y by A121,YELLOW_0:61;
