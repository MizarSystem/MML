          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose
A24:   x=2\1 & y=1;
         3 in Z by ENUMSET1:def 3;
     then reconsider z = 3 as Element of N by YELLOW_1:1;
     take z;
          x c= z & y c= z
      proof
       thus x c= z
        proof
         let X be set;
         assume X in x; then X=1 by A24,Th2,TARSKI:def 1;
         hence X in z by Th1,ENUMSET1:def 1;
        end;
        let X be set;
        assume X in y; then X=0 by A24,CARD_5:1,TARSKI:def 1;
        hence X in z by Th1,ENUMSET1:def 1;
      end;
     hence x <= z & y <= z by YELLOW_1:3;
     let z' be Element of N;
          z' is Element of Z by YELLOW_1:1;
     then A25: z'=0 or z'=1 or z'=2\1 or z'=3\2 or z'=3 by ENUMSET1:def 3;
     assume x <= z' & y <= z';
     then A26:  1 c= z' & 2\1 c= z' by A24,YELLOW_1:3;
A27:  now
      assume A28: z'= 0;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A26,A28;
     end;
A29:  now
      assume A30: z'= 1;
           1 in 2\1 by Th2,TARSKI:def 1;
      then 1 in z' by A26;
      hence contradiction by A30;
     end;
A31: now
      assume A32: z'= 2\1;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A26,A32,Th2,TARSKI:def 1;
     end;
          now
      assume A33: z'= 3\2;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A26,A33,Th4,TARSKI:def 1;
     end;
     hence z <= z' by A25,A27,A29,A31;
     end;
     suppose
       x=2\1 & y=2\1;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose
A34:   x=2\1 & y=3\2;
         3 in Z by ENUMSET1:def 3;
     then reconsider z = 3 as Element of N by YELLOW_1:1;
     take z;
          x c= z & y c= z
      proof
       thus x c= z
        proof
         let X be set;
         assume X in x; then X=1 by A34,Th2,TARSKI:def 1;
         hence X in z by Th1,ENUMSET1:def 1;
        end;
        let X be set;
        assume X in y; then X=2 by A34,Th4,TARSKI:def 1;
        hence X in z by Th1,ENUMSET1:def 1;
      end;
     hence x <= z & y <= z by YELLOW_1:3;
     let z' be Element of N;
          z' is Element of Z by YELLOW_1:1;
     then A35: z'=0 or z'=1 or z'=2\1 or z'=3\2 or z'=3 by ENUMSET1:def 3;
     assume x <= z' & y <= z';
     then A36:  2\1 c= z' & 3\2 c= z' by A34,YELLOW_1:3;
A37:  now
      assume A38: z'= 0;
           1 in 2\1 by Th2,TARSKI:def 1;
      hence contradiction by A36,A38;
     end;
A39:  now
      assume A40: z'= 1;
           1 in 2\1 by Th2,TARSKI:def 1;
      then 1 in z' by A36;
      hence contradiction by A40;
     end;
A41: now
      assume A42: z'= 2\1;
           2 in 3\2 by Th4,TARSKI:def 1;
      hence contradiction by A36,A42,Th2,TARSKI:def 1;
     end;
          now
      assume A43: z'= 3\2;
           1 in 2\1 by Th2,TARSKI:def 1;
      hence contradiction by A36,A43,Th4,TARSKI:def 1;
     end;
     hence z <= z' by A35,A37,A39,A41;
     end;
     suppose
A44:   x=2\1 & y=3;
          2\1 c= 3
      proof
       let X be set;assume X in 2\1; then X=1 by Th2,TARSKI:def 1;
       hence thesis by Th1,ENUMSET1:def 1;
      end;
     then reconsider z = x \/ y as Element of N by A44,XBOOLE_1:12;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose  x=3\2 & y=0;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose
A45:   x=3\2 & y=1;
         3 in Z by ENUMSET1:def 3;
     then reconsider z = 3 as Element of N by YELLOW_1:1;
     take z;
          x c= z & y c= z
      proof
       thus x c= z by A45;
       thus y c= z
        proof
         let X be set;
         assume X in y; then X=0 by A45,CARD_5:1,TARSKI:def 1;
         hence X in z by Th1,ENUMSET1:def 1;
        end;
      end;
     hence x <= z & y <= z by YELLOW_1:3;
     let z' be Element of N;
          z' is Element of Z by YELLOW_1:1;
     then A46: z'=0 or z'=1 or z'=2\1 or z'=3\2 or z'=3 by ENUMSET1:def 3;
     assume x <= z' & y <= z';
     then A47:  1 c= z' & 3\2 c= z' by A45,YELLOW_1:3;
A48:  now
      assume A49: z'= 0;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A47,A49;
     end;
A50:  now
      assume A51: z'= 1;
           2 in 3\2 by Th4,TARSKI:def 1;
      hence contradiction by A47,A51,CARD_5:1,TARSKI:def 1;
     end;
A52: now
      assume A53: z'= 2\1;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A47,A53,Th2,TARSKI:def 1;
     end;
          now
      assume A54: z'= 3\2;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A47,A54,Th4,TARSKI:def 1;
     end;
     hence z <= z' by A46,A48,A50,A52;
     end;
     suppose
A55:   x=3\2 & y=2\1;
         3 in Z by ENUMSET1:def 3;
     then reconsider z = 3 as Element of N by YELLOW_1:1;
     take z;
          x c= z & y c= z
      proof
       thus x c= z by A55,Th4,TARSKI:def 1,Th1,ENUMSET1:def 1;
        let X be set;
        assume X in y; then X=1 by A55,Th2,TARSKI:def 1;
        hence X in z by Th1,ENUMSET1:def 1;
      end;
     hence x <= z & y <= z by YELLOW_1:3;
     let z' be Element of N;
     z' is Element of Z by YELLOW_1:1;
     then A56: z'=0 or z'=1 or z'=2\1 or z'=3\2 or z'=3 by ENUMSET1:def 3;
     assume x <= z' & y <= z';
     then A57:  2\1 c= z' & 3\2 c= z' by A55,YELLOW_1:3;
A58:  now
      assume A59: z'= 0;
           1 in 2\1 by Th2,TARSKI:def 1;
      hence contradiction by A57,A59;
     end;
A60:  now
      assume A61: z'= 1;
           1 in 2\1 by Th2,TARSKI:def 1;
      then 1 in z' by A57;
      hence contradiction by A61;
     end;
A62: now
      assume A63: z'= 2\1;
           2 in 3\2 by Th4,TARSKI:def 1;
      hence contradiction by A57,A63,Th2,TARSKI:def 1;
     end;
          now
      assume A64: z'= 3\2;
           1 in 2\1 by Th2,TARSKI:def 1;
      hence contradiction by A57,A64,Th4,TARSKI:def 1;
     end;
     hence z <= z' by A56,A58,A60,A62;
     end;
     suppose
       x=3\2 & y=3\2;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose
A65:   x=3\2 & y=3;
     reconsider z = x \/ y as Element of N by A65,XBOOLE_1:12;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose  x=3 & y=0;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose
A66:   x=3 & y=1;
          1 c= 3
      proof
       let X be set;assume X in 1; then X=0 by CARD_5:1,TARSKI:def 1;
