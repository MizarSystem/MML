        end;
        suppose
          x=3 & y=3;
          then reconsider z = x \/ y as Element of N;
          take z;
A151:     y c= z by XBOOLE_1:7;
          x c= z by XBOOLE_1:7;
          hence x <= z & y <= z by A151,YELLOW_1:3;
          let w be Element of N;
          assume that
A152:     x <= w and
A153:     y <= w;
A154:     y c= w by A153,YELLOW_1:3;
          x c= w by A152,YELLOW_1:3;
          then x \/ y c= w by A154,XBOOLE_1:8;
