A147:     y c= z by XBOOLE_1:7;
          x c= z by XBOOLE_1:7;
          hence x <= z & y <= z by A147,YELLOW_1:3;
          let w be Element of N;
          assume that
A148:     x <= w and
A149:     y <= w;
A150:     y c= w by A149,YELLOW_1:3;
          x c= w by A148,YELLOW_1:3;
          then x \/ y c= w by A150,XBOOLE_1:8;
