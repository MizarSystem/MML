 cluster M_3 -> with_infima with_suprema;
correctness
 proof
  set Z = { 0, 1, 2 \ 1, 3 \ 2, 3 };
  set N = InclPoset Z;
A1: N is with_suprema
 proof
  let x,y be Element of N;
A2:  Z = the carrier of N by YELLOW_1:1;
   thus ex z being Element of N st x <= z & y <= z &
        for z' being Element of N st x <= z' & y <= z' holds z <= z'
    proof
     per cases by A2,ENUMSET1:def 3;
     suppose
       x=0 & y=0;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose  x=0 & y=1;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose  x=0 & y=2\1;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose  x=0 & y=3\2;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose  x=0 & y=3;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose  x=1 & y=0;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose
       x=1 & y=1;
     then reconsider z = x \/ y as Element of N;
     take z;
          x c= z & y c= z by XBOOLE_1:7;
     hence x <= z & y <= z by YELLOW_1:3;
     let w be Element of N;
     assume x <= w & y <= w;
     then x c= w & y c= w by YELLOW_1:3;
     then x \/ y c= w by XBOOLE_1:8;
     hence thesis by YELLOW_1:3;
     end;
     suppose
A3:   x=1 & y=2\1;
         3 in Z by ENUMSET1:def 3;
     then reconsider z = 3 as Element of N by YELLOW_1:1;
     take z;
          x c= z & y c= z
      proof
       thus x c= z
        proof
         let X be set;
         assume X in x; then X=0 by A3,CARD_5:1,TARSKI:def 1;
         hence X in z by Th1,ENUMSET1:def 1;
        end;
        let X be set;
        assume X in y; then X=1 by A3,Th2,TARSKI:def 1;
        hence X in z by Th1,ENUMSET1:def 1;
      end;
     hence x <= z & y <= z by YELLOW_1:3;
     let z' be Element of N;
          z' is Element of Z by YELLOW_1:1;
     then A4: z'=0 or z'=1 or z'=2\1 or z'=3\2 or z'=3 by ENUMSET1:def 3;
     assume x <= z' & y <= z';
     then A5:  1 c= z' & 2\1 c= z' by A3,YELLOW_1:3;
A6:  now
      assume A7: z'= 0;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A5,A7;
     end;
A8:  now
      assume A9: z'= 1;
           1 in 2\1 by Th2,TARSKI:def 1;
      then 1 in z' by A5;
      hence contradiction by A9;
     end;
A10: now
      assume A11: z'= 2\1;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A5,A11,Th2,TARSKI:def 1;
     end;
          now
      assume A12: z'= 3\2;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A5,A12,Th4,TARSKI:def 1;
     end;
     hence z <= z' by A4,A6,A8,A10;

     end;
     suppose
A13:   x=1 & y=3\2;
         3 in Z by ENUMSET1:def 3;
     then reconsider z = 3 as Element of N by YELLOW_1:1;
     take z;
          x c= z & y c= z
      proof
       thus x c= z
        proof
         let X be set;
         assume X in x; then X=0 by A13,CARD_5:1,TARSKI:def 1;
