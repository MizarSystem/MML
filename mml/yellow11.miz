    end;
 end;
         N is with_infima
     proof
           now
       let x,y be set;
       assume x in Z & y in Z;
       then A69: (x=0 or x=1 or x=2\1 or x=3\2 or x=3) &
       (y=0 or y=1 or y=2\1 or y=3\2 or y=3) by ENUMSET1:def 3;
                1 misses (2\1) by XBOOLE_1:79;
then A70:       1 /\ (2\1) = 0 by XBOOLE_0:def 7;
A71:       1 /\ (3\2) = 0
        proof
              now let x be set;
          assume x in 1 /\ (3\2);
          then x in 1 & x in (3\2) by XBOOLE_0:def 3;
          then x=0 & x=2 & 0<>2 by Th4,CARD_5:1,TARSKI:def 1;
          hence contradiction;
         end;
         hence thesis by XBOOLE_0:def 1;
        end;
            1 c= 3 by CARD_1:56;
then A72:       1 /\ 3 = 1 by XBOOLE_1:28;
A73:       (2\1) /\ (3\2) = 0
        proof
              now let x be set;
          assume x in (2\1) /\ (3\2);
          then x in (2\1) & x in (3\2) by XBOOLE_0:def 3;
          then x=1 & x=2 & 1<>2 by Th2,Th4,TARSKI:def 1;
          hence contradiction;
         end;
         hence thesis by XBOOLE_0:def 1;
        end;
            (2\1) c= 3
        proof
         let x be set;
         assume x in 2\1;
         then x = 1 by Th2,TARSKI:def 1;
         hence thesis by Th1,ENUMSET1:def 1;
        end;
       then A74:       (2\1) /\ 3 = 2\1 by XBOOLE_1:28;
        (3\2) /\ 3 = 3\2 by XBOOLE_1:28;
       hence x /\ y in Z by A69,A70,A71,A72,A73,A74,ENUMSET1:def 3;
      end;
      hence thesis by YELLOW_1:12;
     end;
 hence thesis by A1;
end;
end;

theorem Th9:
for L being LATTICE holds
(ex K being full Sublattice of L st N_5,K are_isomorphic) iff
(ex a,b,c,d,e being Element of L st
(a<>b & a<>c & a<>d & a<>e & b<>c & b<>d & b<>e & c <>d & c <>e & d<>e &
a"/\"b = a & a"/\"c = a & c"/\"e = c & d"/\"e = d &
  b"/\"c = a & b"/\"d = b & c"/\"d = a & b"\/"c = e & c"\/"d = e))
 proof
  let L be LATTICE;
  set cn = the carrier of N_5;
A1: cn = {0, 3 \ 1, 2, 3 \ 2, 3} by YELLOW_1:1;

  thus (ex K being full Sublattice of L st
  N_5,K are_isomorphic) implies
  (ex a,b,c,d,e being Element of L st
  (a<>b & a<>c & a<>d & a<>e & b<>c & b<>d & b<>e & c <>d & c <>e & d<>e &
  a"/\"b = a & a"/\"c = a & c"/\"e = c & d"/\"e = d &
   b"/\"c = a & b"/\"d = b & c"/\"d = a & b"\/"c = e & c"\/"d = e))
   proof
    given K being full Sublattice of L
    such that
A2:  N_5,K are_isomorphic;
    consider f being Function of N_5,K such that
A3:  f is isomorphic by A2,WAYBEL_1:def 8;
A4: K is non empty by A3,WAYBEL_0:def 38;
    reconsider K as non empty SubRelStr of L by A3,WAYBEL_0:def 38;
A5: f is one-to-one monotone by A3,A4,WAYBEL_0:def 38;
    reconsider z = 0 as Element of N_5 by A1,ENUMSET1:def 3;
    reconsider tj = 3\1 as Element of N_5 by A1,ENUMSET1:def 3;
    reconsider dw = 2 as Element of N_5 by A1,ENUMSET1:def 3;
    reconsider td = 3\2 as Element of N_5 by A1,ENUMSET1:def 3;
    reconsider t = 3 as Element of N_5 by A1,ENUMSET1:def 3;
    reconsider cl = the carrier of L as non empty set;
    reconsider ck = the carrier of K as non empty set;
A6: ck = rng f by A3,WAYBEL_0:66;
    reconsider g=f as Function of cn,ck;
    reconsider a=g.z,b=g.td,c =g.dw,d=g.tj,e=g.t as Element of K
                                   ;
    reconsider ck as non empty Subset of cl by YELLOW_0:def 13;
         a in ck & b in ck & c in ck & d in ck & e in ck;
    then reconsider A=a,B=b,C=c,D=d,E=e as Element of L;
    take A,B,C,D,E;
    thus A<>B by A5,Th4,WAYBEL_1:def 1;
    thus A<>C by A5,WAYBEL_1:def 1;
    thus A<>D by A5,Th3,WAYBEL_1:def 1;
    thus A<>E by A5,WAYBEL_1:def 1;
         now
     assume f.td = f.dw;
     then A7:   td = dw by A4,A5,WAYBEL_1:def 1;
          2 in td by Th4,TARSKI:def 1;
     hence contradiction by A7;
    end;
    hence B<>C;
         now
     assume

A8:      f.td = f.tj;
          not 1 in td & 1 in tj by Th3,Th4,TARSKI:def 1,def 2;
     hence contradiction by A4,A5,A8,WAYBEL_1:def 1;
    end;
    hence B<>D;
         now
     assume
A9:      f.td = f.t;
          not 1 in td & 1 in t by Th1,Th4,ENUMSET1:def 1,TARSKI:def 1;
     hence contradiction by A4,A5,A9,WAYBEL_1:def 1;
    end;
    hence B<>E;
         now
     assume f.dw = f.tj;
     then A10:   dw = tj by A4,A5,WAYBEL_1:def 1;
          2 in tj by Th3,TARSKI:def 2;
     hence contradiction by A10;
    end;
    hence C<>D;
    thus C<>E by A5,WAYBEL_1:def 1;
         now
     assume
A11:     f.tj = f.t;
          not 0 in tj & 0 in t by Th1,Th3,ENUMSET1:def 1,TARSKI:def 2;
     hence contradiction by A4,A5,A11,WAYBEL_1:def 1;
    end;
    hence D<>E;
         z c= td by XBOOLE_1:2;
    then z <= td by YELLOW_1:3;
    then a <= b by A3,WAYBEL_0:66;
    then A <= B by YELLOW_0:60;
    hence A "/\" B = A by YELLOW_0:25;
         z c= dw by XBOOLE_1:2;
    then z <= dw by YELLOW_1:3;
    then a <= c by A3,WAYBEL_0:66;
    then A <= C by YELLOW_0:60;
    hence A "/\" C = A by YELLOW_0:25;

         dw c= t by CARD_1:56;
    then dw <= t by YELLOW_1:3;
    then c <= e by A3,WAYBEL_0:66;
    then C <= E by YELLOW_0:60;
    hence C"/\"E = C by YELLOW_0:25;
    tj <= t by YELLOW_1:3;
    then d <= e by A3,WAYBEL_0:66;
    then D <= E by YELLOW_0:60;
    hence D"/\"E = D by YELLOW_0:25;

    thus B"/\"C = A
     proof
           B in the carrier of K & C in the carrier of K & ex_inf_of {B,C},L
                                  by YELLOW_0:21;
      then inf{B,C} in the carrier of K by YELLOW_0:def 16;
      then B"/\"C in rng f by A6,YELLOW_0:40;
      then consider x being set such that
A12:    x in dom f & B"/\"C = f.x by FUNCT_1:def 5;
      reconsider x as Element of N_5 by A12;
A13:      x = z or x = td or x = dw or x = tj or x = t by A1,ENUMSET1:def 3;
A14:    now
       assume B"/\"C = B;
       then B <= C by YELLOW_0:25;
       then b <= c by YELLOW_0:61;
       then td <= dw by A3,WAYBEL_0:66;
       then A15:    td c= dw by YELLOW_1:3;
            2 in td by Th4,TARSKI:def 1;
       then 2 in 2 by A15;
       hence contradiction;
      end;
A16:    now
       assume B"/\"C = C;
       then C <= B by YELLOW_0:25;
       then c <= b by YELLOW_0:61;
       then dw <= td by A3,WAYBEL_0:66;
       then A17:    dw c= td by YELLOW_1:3;
            0 in dw & 0 <> 2 by CARD_5:1,TARSKI:def 2;
       hence contradiction by A17,Th4,TARSKI:def 1;
      end;
A18:    now
       assume B"/\"C = D;
       then D <= C by YELLOW_0:23;
       then d <= c by YELLOW_0:61;
       then tj <= dw by A3,WAYBEL_0:66;
       then A19:    tj c= dw by YELLOW_1:3;
            2 in tj by Th3,TARSKI:def 2;
       then 2 in 2 by A19;
       hence contradiction;
      end;
           now
       assume B"/\"C = E;
       then E <= C by YELLOW_0:23;
       then e <= c by YELLOW_0:61;
       then t <= dw by A3,WAYBEL_0:66;
       then A20:    t c= dw by YELLOW_1:3;
            2 in t by Th1,ENUMSET1:def 1;
       then 2 in 2 by A20;
       hence contradiction;
      end;
      hence thesis by A12,A13,A14,A16,A18;
     end;

         td <= tj by Lm1,YELLOW_1:3;
    then b <= d by A3,WAYBEL_0:66;
    then B <= D by YELLOW_0:60;
    hence B"/\"D = B by YELLOW_0:25;

    thus C"/\"D = A
     proof
           C in the carrier of K & D in the carrier of K & ex_inf_of {C,D},L
                                  by YELLOW_0:21;
      then inf{C,D} in the carrier of K by YELLOW_0:def 16;
      then C"/\"D in rng f by A6,YELLOW_0:40;
      then consider x being set such that
A21:    x in dom f & C"/\"D = f.x by FUNCT_1:def 5;
      reconsider x as Element of N_5 by A21;
A22:      x = z or x = td or x = dw or x = tj or x = t by A1,ENUMSET1:def 3;
A23:    now
       assume C"/\"D = B;
       then B <= C by YELLOW_0:23;
       then b <= c by YELLOW_0:61;
       then td <= dw by A3,WAYBEL_0:66;
       then A24:    td c= dw by YELLOW_1:3;
            2 in td by Th4,TARSKI:def 1;
       then 2 in 2 by A24;
       hence contradiction;
      end;
A25:    now
       assume C"/\"D = C;
       then C <= D by YELLOW_0:25;
       then c <= d by YELLOW_0:61;
       then dw <= tj by A3,WAYBEL_0:66;
       then A26:    dw c= tj by YELLOW_1:3;
            0 in dw & 0 <> 2 & 0 <> 1 by CARD_5:1,TARSKI:def 2;
       hence contradiction by A26,Th3,TARSKI:def 2;
      end;
A27:    now
       assume C"/\"D = D;
       then D <= C by YELLOW_0:23;
       then d <= c by YELLOW_0:61;
       then tj <= dw by A3,WAYBEL_0:66;
       then A28:    tj c= dw by YELLOW_1:3;
            2 in tj by Th3,TARSKI:def 2;
       then 2 in 2 by A28;
       hence contradiction;
      end;
           now
       assume C"/\"D = E;
       then E <= C by YELLOW_0:23;
       then e <= c by YELLOW_0:61;
       then t <= dw by A3,WAYBEL_0:66;
       then A29:    t c= dw by YELLOW_1:3;
            2 in t by Th1,ENUMSET1:def 1;
       then 2 in 2 by A29;
       hence contradiction;
      end;
      hence thesis by A21,A22,A23,A25,A27;
     end;

    thus B"\/"C = E
     proof
           B in the carrier of K & B in the carrier of K & ex_sup_of {B,C},L
                                  by YELLOW_0:20;
      then sup{B,C} in the carrier of K by YELLOW_0:def 17;
      then B"\/"C in rng f by A6,YELLOW_0:41;
      then consider x being set such that
A30:    x in dom f & B"\/"C = f.x by FUNCT_1:def 5;
      reconsider x as Element of N_5 by A30;
A31:      x = z or x = td or x = dw or x = tj or x = t by A1,ENUMSET1:def 3;
A32:    now
       assume B"\/"C = B;
       then B >= C by YELLOW_0:24;
       then b >= c by YELLOW_0:61;
       then td >= dw by A3,WAYBEL_0:66;
       then A33:    dw c= td by YELLOW_1:3;
            0 in dw & 0 <> 2 by CARD_5:1,TARSKI:def 2;
       hence contradiction by A33,Th4,TARSKI:def 1;
      end;
A34:    now
       assume B"\/"C = C;
       then C >= B by YELLOW_0:24;
       then c >= b by YELLOW_0:61;
       then dw >= td by A3,WAYBEL_0:66;
       then A35:    td c= dw by YELLOW_1:3;
            2 in td by Th4,TARSKI:def 1;
       then 2 in 2 by A35;
       hence contradiction;
      end;
A36:    now
       assume B"\/"C = D;
       then D >= C by YELLOW_0:22;
       then d >= c by YELLOW_0:61;
       then tj >= dw by A3,WAYBEL_0:66;
       then A37:    dw c= tj by YELLOW_1:3;
            0 in dw & 0 <> 1 & 0 <>2 by CARD_5:1,TARSKI:def 2;
       hence contradiction by A37,Th3,TARSKI:def 2;
      end;
           now
       assume B"\/"C = A;
       then A >= C by YELLOW_0:22;
       then a >= c by YELLOW_0:61;
       then z >= dw by A3,WAYBEL_0:66;
       then dw c= z by YELLOW_1:3;
       hence contradiction by XBOOLE_1:3;
      end;
      hence thesis by A30,A31,A32,A34,A36;
     end;

    thus C"\/"D = E
     proof
           C in the carrier of K & D in the carrier of K & ex_sup_of {C,D},L
                                  by YELLOW_0:20;
      then sup{C,D} in the carrier of K by YELLOW_0:def 17;
      then C"\/"D in rng f by A6,YELLOW_0:41;
      then consider x being set such that
A38:    x in dom f & C"\/"D = f.x by FUNCT_1:def 5;
      reconsider x as Element of N_5 by A38;
A39:      x = z or x = td or x = dw or x = tj or x = t by A1,ENUMSET1:def 3;
A40:    now
       assume C"\/"D = B;
       then B >= C by YELLOW_0:22;
       then b >= c by YELLOW_0:61;
       then td >= dw by A3,WAYBEL_0:66;
       then A41:    dw c= td by YELLOW_1:3;
            0 in dw & 0 <> 2 by CARD_5:1,TARSKI:def 2;
       hence contradiction by A41,Th4,TARSKI:def 1;
      end;
A42:    now
       assume C"\/"D = C;
       then C >= D by YELLOW_0:24;
       then c >= d by YELLOW_0:61;
       then dw >= tj by A3,WAYBEL_0:66;
       then A43:    tj c= dw by YELLOW_1:3;
            2 in tj & 2 <> 0 & 2 <> 1 by Th3,TARSKI:def 2;
       hence contradiction by A43,CARD_5:1,TARSKI:def 2;
      end;
A44:    now
       assume C"\/"D = D;
       then D >= C by YELLOW_0:22;
       then d >= c by YELLOW_0:61;
       then tj >= dw by A3,WAYBEL_0:66;
       then A45:    dw c= tj by YELLOW_1:3;
            0 in dw & 0 <>1 & 0 <> 2 by CARD_5:1,TARSKI:def 2;
       hence contradiction by A45,Th3,TARSKI:def 2;
      end;
           now
       assume C"\/"D = A;
       then A >= C by YELLOW_0:22;
       then a >= c by YELLOW_0:61;
       then z >= dw by A3,WAYBEL_0:66;
       then dw c= z by YELLOW_1:3;
       hence contradiction by XBOOLE_1:3;
      end;
      hence thesis by A38,A39,A40,A42,A44;
     end;
   end;
  thus (ex a,b,c,d,e being Element of L st
  (a<>b & a<>c & a<>d & a<>e & b<>c & b<>d & b<>e & c <>d & c <>e & d<>e &
  a"/\"b = a & a"/\"c = a & c"/\"e = c & d"/\"e = d &
   b"/\"c = a & b"/\"d = b & c"/\"d = a & b"\/"c = e & c"\/"d = e)) implies
  (ex K being full Sublattice of L
  st N_5,K are_isomorphic)
   proof
    given a,b,c,d,e being Element of L such that
A46:  (a<>b & a<>c & a<>d & a<>e & b<>c & b<>d & b<>e & c <>d & c <>e & d<>e &
      a"/\"b = a & a"/\"c = a & c"/\"e = c & d"/\"e = d &
     b"/\"c = a & b"/\"d = b & c"/\"d = a & b"\/"c = e & c"\/"d = e);

    set ck = {a,b,c,d,e};
    reconsider ck as Subset of L;
    set K = subrelstr ck;
A47: the carrier of K = ck by YELLOW_0:def 15;
A48:  K is meet-inheriting
     proof
      let x,y be Element of L;
      assume
A49:      x in the carrier of K & y in the carrier of K & ex_inf_of {x,y},L;
      per cases by A47,A49,ENUMSET1:def 3;
      suppose x=a & y=a;
      then inf{x,y} = a"/\"a by YELLOW_0:40;
      then inf{x,y} = a by YELLOW_5:2;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=a & y=b;
      then inf{x,y} = a"/\"b by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=a & y=c;
      then inf{x,y} = a"/\"c by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose
A50:    x=a & y=d;
           a <= b & b <= d by A46,YELLOW_0:25;
      then a <= d by ORDERS_2:26;
      then a"/\"d = a by YELLOW_0:25;
      then inf {x,y} = a by A50,YELLOW_0:40;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A51:    x=a & y=e;
           a <= c & c <= e by A46,YELLOW_0:25;
      then a <= e by ORDERS_2:26;
      then a"/\"e = a by YELLOW_0:25;
      then inf {x,y} = a by A51,YELLOW_0:40;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=b & y=a;
      then inf{x,y} = a"/\"b by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=b & y=b;
      then inf{x,y} = b"/\"b by YELLOW_0:40;
      then inf{x,y} = b by YELLOW_5:2;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=b & y=c;
      then inf{x,y} = b"/\"c by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=b & y=d;
      then inf{x,y} = b"/\"d by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose
A52:    x=b & y=e;
           b <= d & d <= e by A46,YELLOW_0:25;
      then b <= e by ORDERS_2:26;
      then b"/\"e = b by YELLOW_0:25;
      then inf {x,y} = b by A52,YELLOW_0:40;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=c & y=a;
      then inf{x,y} = a"/\"c by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=c & y=b;
      then inf{x,y} = b"/\"c by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=c & y=c;
      then inf{x,y} = c"/\"c by YELLOW_0:40;
      then inf{x,y} = c by YELLOW_5:2;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=c & y=d;
      then inf{x,y} = c"/\"d by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=c & y=e;
      then inf{x,y} = c"/\"e by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose
A53:    x=d & y=a;
           a <= b & b <= d by A46,YELLOW_0:25;
      then a <= d by ORDERS_2:26;
      then a"/\"d = a by YELLOW_0:25;
      then inf {x,y} = a by A53,YELLOW_0:40;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=d & y=b;
      then inf{x,y} = b"/\"d by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=d & y=c;
      then inf{x,y} = c"/\"d by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=d & y=d;
      then inf{x,y} = d"/\"d by YELLOW_0:40;
      then inf{x,y} = d by YELLOW_5:2;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=d & y=e;
      then inf{x,y} = d"/\"e by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose
A54:    x=e & y=a;
           a <= c & c <= e by A46,YELLOW_0:25;
      then a <= e by ORDERS_2:26;
      then a"/\"e = a by YELLOW_0:25;
      then inf {x,y} = a by A54,YELLOW_0:40;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A55:    x=e & y=b;
           b <= d & d <= e by A46,YELLOW_0:25;
      then b <= e by ORDERS_2:26;
      then b"/\"e = b by YELLOW_0:25;
      then inf {x,y} = b by A55,YELLOW_0:40;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=e & y=c;
      then inf{x,y} = c"/\"e by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=e & y=d;
      then inf{x,y} = d"/\"e by YELLOW_0:40;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=e & y=e;
      then inf{x,y} = e"/\"e by YELLOW_0:40;
      then inf{x,y} = e by YELLOW_5:2;
      hence thesis by A47,ENUMSET1:def 3;
     end;
     end;
         K is join-inheriting
     proof
      let x,y be Element of L;
      assume
A56:      x in the carrier of K & y in the carrier of K & ex_sup_of {x,y},L;
      per cases by A47,A56,ENUMSET1:def 3;
      suppose x=a & y=a;
      then sup{x,y} = a"\/"a by YELLOW_0:41;
      then sup{x,y} = a by YELLOW_5:1;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=a & y=b;
      then x"\/"y = b by A46,Th5;
      then sup{x,y} = b by YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=a & y=c;
      then x"\/"y = c by A46,Th5;
      then sup{x,y} = c by YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A57:    x=a & y=d;
           a <= b & b <= d by A46,YELLOW_0:25;
      then a <= d by ORDERS_2:26;
      then a"/\"d = a by YELLOW_0:25;
      then a"\/"d = d by Th5;
      then sup {x,y} = d by A57,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A58:    x=a & y=e;
           a <= c & c <= e by A46,YELLOW_0:25;
      then a <= e by ORDERS_2:26;
      then a"/\"e = a by YELLOW_0:25;
      then a"\/"e = e by Th5;
      then sup {x,y} = e by A58,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A59:    x=b & y=a;
           a"\/"b = b by A46,Th5;
      then sup{x,y} = b by A59,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=b & y=b;
      then sup{x,y} = b"\/"b by YELLOW_0:41;
      then sup{x,y} = b by YELLOW_5:1;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=b & y=c;
      then sup{x,y} = b"\/"c by YELLOW_0:41;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose
A60:    x=b & y=d;
           b"\/"d = d by A46,Th5;
      then sup{x,y} = d by A60,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A61:    x=b & y=e;
           b <= d & d <= e by A46,YELLOW_0:25;
      then b <= e by ORDERS_2:26;
      then b"/\"e = b by YELLOW_0:25;
      then b"\/"e = e by Th5;
      then sup {x,y} = e by A61,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A62:    x=c & y=a;
           c"\/"a = c by A46,Th5;
      then sup{x,y} = c by A62,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=c & y=b;
      then sup{x,y} = b"\/"c by YELLOW_0:41;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=c & y=c;
      then sup{x,y} = c"\/"c by YELLOW_0:41;
      then sup{x,y} = c by YELLOW_5:1;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=c & y=d;
      then sup{x,y} = c"\/"d by YELLOW_0:41;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose
A63:    x=c & y=e;
           c"\/"e = e by A46,Th5;
      then sup{x,y} = e by A63,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A64:    x=d & y=a;
           a <= b & b <= d by A46,YELLOW_0:25;
      then a <= d by ORDERS_2:26;
      then a"/\"d = a by YELLOW_0:25;
      then a"\/"d = d by Th5;
      then sup {x,y} = d by A64,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A65:    x=d & y=b;
           b"\/"d = d by A46,Th5;
      then sup{x,y} = d by A65,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=d & y=c;
      then sup{x,y} = c"\/"d by YELLOW_0:41;
      hence thesis by A46,A47,ENUMSET1:def 3;
      end;
      suppose x=d & y=d;
      then sup{x,y} = d"\/"d by YELLOW_0:41;
      then sup{x,y} = d by YELLOW_5:1;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A66:    x=d & y=e;
           d"\/"e = e by A46,Th5;
      then sup{x,y} = e by A66,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A67:    x=e & y=a;
           a <= c & c <= e by A46,YELLOW_0:25;
      then a <= e by ORDERS_2:26;
      then a"/\"e = a by YELLOW_0:25;
      then a"\/"e = e by Th5;
      then sup {x,y} = e by A67,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A68:    x=e & y=b;
           b <= d & d <= e by A46,YELLOW_0:25;
      then b <= e by ORDERS_2:26;
      then b"/\"e = b by YELLOW_0:25;
      then b"\/"e = e by Th5;
      then sup {x,y} = e by A68,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A69:    x=e & y=c;
           c"\/"e = e by A46,Th5;
      then sup{x,y} = e by A69,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose
A70:    x=e & y=d;
           d"\/"e = e by A46,Th5;
      then sup{x,y} = e by A70,YELLOW_0:41;
      hence thesis by A47,ENUMSET1:def 3;
      end;
      suppose x=e & y=e;
      then sup{x,y} = e"\/"e by YELLOW_0:41;
      then sup{x,y} = e by YELLOW_5:1;
      hence thesis by A47,ENUMSET1:def 3;
     end;
     end;
    then reconsider K as non empty full Sublattice of L
           by A47,A48,STRUCT_0:def 1;
    take K;
    thus N_5,K are_isomorphic
     proof
      reconsider z = 0 as Element of N_5 by A1,ENUMSET1:def 3;
      reconsider tj = 3\1 as Element of N_5 by A1,ENUMSET1:def 3;
      reconsider dw = 2 as Element of N_5 by A1,ENUMSET1:def 3;
      reconsider td = 3\2 as Element of N_5 by A1,ENUMSET1:def 3;
      reconsider t = 3 as Element of N_5 by A1,ENUMSET1:def 3;

A71: now
       assume
    A72: tj=dw;
            2 in tj by Th3,TARSKI:def 2;
       hence contradiction by A72;
      end;
A73: now
       assume
    A74: tj=td;
            not 1 in td & 1 in tj by Th3,Th4,TARSKI:def 1,def 2;
       hence contradiction by A74;
      end;
A75:  now
       assume
    A76: tj=t;
            not 0 in tj & 0 in t by Th1,Th3,ENUMSET1:def 1,TARSKI:def 2;
       hence contradiction by A76;
      end;
A77: now
       assume
    A78: dw=td;    2 in td by Th4,TARSKI:def 1;
       hence contradiction by A78;
      end;
A79:  now
       assume
    A80: td=t;
            not 1 in td & 1 in t by Th1,Th4,ENUMSET1:def 1,TARSKI:def 1;
       hence contradiction by A80;
      end;
      defpred P[set,set] means
      for X being Element of N_5 st X=$1 holds
     ((X = z implies $2 = a) &
      (X = td implies $2 = b) &
      (X = dw implies $2 = c) &
      (X = tj implies $2 = d) &
      (X = t implies $2 = e));

A81:   for x being set st x in cn ex y being set st y in ck & P[x,y]
       proof
        let x be set;
        assume
A82:         x in cn;
        per cases by A1,A82,ENUMSET1:def 3;
        suppose
A83:      x = 0;
        take y=a;
        thus y in ck by ENUMSET1:def 3;
        let X be Element of N_5;
        thus thesis by A83,Th3,Th4;
        end;
        suppose
A84:      x=3\1;
        take y=d;
        thus y in ck by ENUMSET1:def 3;
        let X be Element of N_5;
        thus thesis by A71,A73,A75,A84,Th3;
        end;
        suppose
A85:      x = 2;
        take y=c;
        thus y in ck by ENUMSET1:def 3;
        let X be Element of N_5;
        thus thesis by A71,A77,A85;
        end;
        suppose
A86:      x = 3 \ 2;
        take y=b;
        thus y in ck by ENUMSET1:def 3;
        let X be Element of N_5;
        thus thesis by A73,A77,A79,A86,Th4;
        end;
        suppose
A87:      x = 3;
        take y=e;
        thus y in ck by ENUMSET1:def 3;
        let X be Element of N_5;
        thus thesis by A75,A79,A87;
       end;
       end;
      consider f being Function of cn,ck such that
A88:    for x being set st x in cn holds P[x,f.x] from FUNCT_2:sch 1(A81);
      reconsider f as Function of N_5,K by A47;
      take f;
A89:  f is one-to-one
       proof
             now
         let x,y be Element of N_5;
         assume
A90:      f.x = f.y;
         thus x=y
          proof
           per cases by A1,ENUMSET1:def 3;
           suppose x = z & y = z;hence thesis;
           end;
           suppose x = tj & y = tj;hence thesis;
           end;
           suppose x = td & y = td;hence thesis;
           end;
           suppose x = dw & y = dw;hence thesis;
           end;
           suppose x = t & y = t;hence thesis;
           end;
           suppose x = z & y = tj;
           then f.x=a & f.y=d by A88;
           hence thesis by A46,A90;
           end;
           suppose x = z & y = dw;
           then f.x=a & f.y=c by A88;
           hence thesis by A46,A90;
           end;
           suppose x = z & y = td;
           then f.x=a & f.y=b by A88;
           hence thesis by A46,A90;
           end;
           suppose x = z & y = t;
           then f.x=a & f.y=e by A88;
           hence thesis by A46,A90;
           end;
           suppose x = tj & y = z;
           then f.x=d & f.y=a by A88;
           hence thesis by A46,A90;
           end;
           suppose x = tj & y = dw;
           then f.x=d & f.y=c by A88;
           hence thesis by A46,A90;
           end;
           suppose x = tj & y = td;
           then f.x=d & f.y=b by A88;
           hence thesis by A46,A90;
           end;
           suppose x = tj & y = t;
           then f.x=d & f.y=e by A88;
           hence thesis by A46,A90;
           end;
           suppose x = dw & y = z;
           then f.x=c & f.y=a by A88;
           hence thesis by A46,A90;
           end;
           suppose x = dw & y = tj;
           then f.x=c & f.y=d by A88;
           hence thesis by A46,A90;
           end;
           suppose x = dw & y = td;
           then f.x=c & f.y=b by A88;
           hence thesis by A46,A90;
           end;
           suppose x = dw & y = t;
           then f.x=c & f.y=e by A88;
           hence thesis by A46,A90;
           end;
           suppose x = td & y = z;
           then f.x=b & f.y=a by A88;
           hence thesis by A46,A90;
           end;
           suppose x = td & y = tj;
           then f.x=b & f.y=d by A88;
           hence thesis by A46,A90;
           end;
           suppose x = td & y = dw;
           then f.x=b & f.y=c by A88;
           hence thesis by A46,A90;
           end;
           suppose x = td & y = t;
           then f.x=b & f.y=e by A88;
           hence thesis by A46,A90;
           end;
           suppose x = t & y = z;
           then f.x=e & f.y=a by A88;
           hence thesis by A46,A90;
           end;
           suppose x = t & y = tj;
           then f.x=e & f.y=d by A88;
           hence thesis by A46,A90;
           end;
           suppose x = t & y = dw;
           then f.x=e & f.y=c by A88;
           hence thesis by A46,A90;
           end;
           suppose x = t & y = td;
           then f.x=e & f.y=b by A88;
           hence thesis by A46,A90;
          end;
          end;
        end;
        hence thesis by WAYBEL_1:def 1;
       end;
A91: dom f = the carrier of N_5 by FUNCT_2:def 1;
A92: rng f = ck
       proof
        thus rng f c= ck
         proof
          let y be set;
          assume y in rng f;
          then consider x being set such that
A93:        x in dom f & y=f.x by FUNCT_1:def 5;
          reconsider x as Element of N_5 by A93;
               (x = z or x = tj or x = dw or x = td or x = t) by A1,ENUMSET1:
def 3
;
          then y=a or y=d or y=c or y=b or y=e by A88,A93;
          hence thesis by ENUMSET1:def 3;
         end;
        thus ck c= rng f
         proof
          let y be set;
          assume
A94:          y in ck;
          per cases by A94,ENUMSET1:def 3;
          suppose
A95:        y=a;
               z in dom f & a = f.z by A88,A91;
          hence y in rng f by A95,FUNCT_1:def 5;
          end;
          suppose
A96:        y=b;
               td in dom f & b=f.td by A88,A91;
          hence y in rng f by A96,FUNCT_1:def 5;
          end;
          suppose
A97:        y=c;
               dw in dom f & c = f.dw by A88,A91;
          hence y in rng f by A97,FUNCT_1:def 5;
          end;
          suppose
A98:        y=d;
               tj in dom f & d=f.tj by A88,A91;
          hence y in rng f by A98,FUNCT_1:def 5;
          end;
          suppose
A99:        y=e;
               t in dom f & e=f.t by A88,A91;
          hence y in rng f by A99,FUNCT_1:def 5;
         end;
         end;
       end;
           for x,y being Element of N_5 holds x <= y iff f.x <= f.y
       proof
        let x,y be Element of N_5;
        thus x <= y implies f.x <= f.y
         proof
          assume
A100:       x <= y;
          per cases by A1,ENUMSET1:def 3;
          suppose x=z & y=z;hence thesis;
          end;
          suppose x=z & y=td;
          then A101:       f.x = a & f.y = b by A88;
          reconsider A=a,B=b as Element of K by A47,ENUMSET1:def 3;
               a <= b & A in the carrier of K & B in the carrier of K
                                          by A46,YELLOW_0:25;
          hence f.x <= f.y by A101,YELLOW_0:61;
          end;
          suppose x=z & y=dw;
          then A102:       f.x = a & f.y = c by A88;
          reconsider A=a,C=c as Element of K by A47,ENUMSET1:def 3;
               a <= c & A in the carrier of K & C in the carrier of K
                                          by A46,YELLOW_0:25;
          hence f.x <= f.y by A102,YELLOW_0:61;
          end;
          suppose x=z & y=tj;
          then A103:       f.x = a & f.y = d by A88;
          reconsider A=a,D=d as Element of K by A47,ENUMSET1:def 3;
               a <= b & b <= d by A46,YELLOW_0:25;
          then a <= d & A in the carrier of K & D in the carrier of K
                                          by ORDERS_2:26;
          hence f.x <= f.y by A103,YELLOW_0:61;
          end;
          suppose x=z & y=t;
          then A104:       f.x = a & f.y = e by A88;
          reconsider A=a,E=e as Element of K by A47,ENUMSET1:def 3;
               a <= c & c <= e by A46,YELLOW_0:25;
          then a <= e & A in the carrier of K & E in the carrier of K
                                          by ORDERS_2:26;
          hence f.x <= f.y by A104,YELLOW_0:61;
          end;
          suppose x=td & y=z;
          then td c= z by A100,YELLOW_1:3;
          hence thesis by Th4,XBOOLE_1:3;
          end;
          suppose x=td & y=td;hence thesis;
          end;
          suppose
A105:        x=td & y=dw;
               2 in td & not 2 in dw by Th4,TARSKI:def 1;
          then not td c= dw;
          hence thesis by A100,A105,YELLOW_1:3;
          end;
          suppose x=td & y=z;
          then td c= z by A100,YELLOW_1:3;
          hence thesis by Th4,XBOOLE_1:3;
          end;
          suppose x=td & y=tj;
          then A106:       f.x = b & f.y = d by A88;
          reconsider D=d,B=b as Element of K by A47,ENUMSET1:def 3;
               b <= d & B in the carrier of K & D in the carrier of K
                                          by A46,YELLOW_0:25;
          hence f.x <= f.y by A106,YELLOW_0:61;
          end;
          suppose x=td & y=t;
          then A107:       f.x = b & f.y = e by A88;
          reconsider B=b,E=e as Element of K by A47,ENUMSET1:def 3;
               b <= d & d <= e by A46,YELLOW_0:25;
          then b <= e & B in the carrier of K & E in the carrier of K
                                          by ORDERS_2:26;
          hence f.x <= f.y by A107,YELLOW_0:61;
          end;
          suppose x=dw & y=z;
          then dw c= z by A100,YELLOW_1:3;
          hence thesis by XBOOLE_1:3;
          end;
          suppose
A108:        x=dw & y=td;
               0 in dw & not 0 in td by Th4,CARD_5:1,TARSKI:def 1,def 2;
          then not dw c= td;
          hence thesis by A100,A108,YELLOW_1:3;
          end;
          suppose x=dw & y=dw;hence thesis;
          end;
          suppose
A109:        x=dw & y=tj;
               0 in dw & not 0 in tj by Th3,CARD_5:1,TARSKI:def 2;
          then not dw c= tj;
          hence thesis by A100,A109,YELLOW_1:3;
          end;
          suppose x=dw & y=t;
          then A110:       f.x = c & f.y = e by A88;
          reconsider C=c,E=e as Element of K by A47,ENUMSET1:def 3;
               c <= e & C in the carrier of K & E in the carrier of K
                                          by A46,YELLOW_0:25;
          hence f.x <= f.y by A110,YELLOW_0:61;
          end;
          suppose x=tj & y=z;
          then tj c= z by A100,YELLOW_1:3;
          hence thesis by Th3,XBOOLE_1:3;
          end;
          suppose
A111:        x=tj & y=td;
               1 in tj & not 1 in td by Th3,Th4,TARSKI:def 1,def 2;
          then not tj c= td;
          hence thesis by A100,A111,YELLOW_1:3;
          end;
          suppose
A112:        x=tj & y=dw;
               2 in tj & not 2 in dw by Th3,TARSKI:def 2;
          then not tj c= dw;
          hence thesis by A100,A112,YELLOW_1:3;
          end;
          suppose x=tj & y=tj;hence thesis;
          end;
          suppose x=tj & y=t;
          then A113:       f.x = d & f.y = e by A88;
          reconsider D=d,E=e as Element of K by A47,ENUMSET1:def 3;
               d <= e & D in the carrier of K & E in the carrier of K
                                          by A46,YELLOW_0:25;
          hence f.x <= f.y by A113,YELLOW_0:61;
          end;
          suppose x=t & y=z;
          then t c= z by A100,YELLOW_1:3;
          hence thesis by XBOOLE_1:3;
          end;
          suppose
A114:        x=t & y=td;
               0 in t & not 0 in td by Th1,Th4,ENUMSET1:def 1,TARSKI:def 1;
          then not t c= td;
          hence thesis by A100,A114,YELLOW_1:3;
          end;
          suppose
A115:        x=t & y=dw;
               2 in t & not 2 in dw by Th1,ENUMSET1:def 1;
          then not t c= dw;
          hence thesis by A100,A115,YELLOW_1:3;
          end;
          suppose
A116:        x=t & y=tj;
               0 in t & not 0 in tj by Th1,Th3,ENUMSET1:def 1,TARSKI:def 2;
          then not t c= tj;
          hence thesis by A100,A116,YELLOW_1:3;
          end;
          suppose x=t & y=t;hence thesis;
         end;
         end;
        thus f.x <= f.y implies x <= y
         proof
          assume
A117:       f.x <= f.y;
A118:          f.x in ck & f.y in ck by A91,A92,FUNCT_1:def 5;
          per cases by A118,ENUMSET1:def 3;
          suppose f.x = a & f.y = a;
          hence x <= y by A89,WAYBEL_1:def 1;
          end;
          suppose A119: f.x = a & f.y = b;
               f.z = a & f.td = b by A88;
          then z=x & td = y by A89,A119,WAYBEL_1:def 1;
          then x c= y by XBOOLE_1:2;
          hence x <= y by YELLOW_1:3;
          end;
          suppose A120: f.x = a & f.y = c;
               f.z = a & f.dw = c by A88;
          then z=x & dw = y by A89,A120,WAYBEL_1:def 1;
          then x c= y by XBOOLE_1:2;
          hence x <= y by YELLOW_1:3;
          end;
          suppose A121: f.x = a & f.y = d;
               f.z = a & f.tj = d by A88;
          then z=x & tj = y by A89,A121,WAYBEL_1:def 1;
          then x c= y by XBOOLE_1:2;
          hence x <= y by YELLOW_1:3;
          end;
          suppose A122: f.x = a & f.y = e;
               f.z = a & f.t = e by A88;
          then z=x & t = y by A89,A122,WAYBEL_1:def 1;
          then x c= y by XBOOLE_1:2;
          hence x <= y by YELLOW_1:3;
          end;
          suppose f.x = b & f.y = a;
          then b <= a by A117,YELLOW_0:60;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose f.x = b & f.y = b;
          hence x <= y by A89,WAYBEL_1:def 1;
          end;
          suppose f.x = b & f.y = c;
          then b <= c by A117,YELLOW_0:60;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose A123: f.x = b & f.y = d;
               f.td = b & f.tj = d by A88;
          then x=td & y=tj & 1 c= 2 by A89,A123,CARD_1:56,WAYBEL_1:def 1;
          then x c= y by XBOOLE_1:34;
          hence x <= y by YELLOW_1:3;
          end;
          suppose A124: f.x = b & f.y = e;
               f.td = b & f.t = e by A88;
          then td=x & t = y by A89,A124,WAYBEL_1:def 1;
          hence x <= y by YELLOW_1:3;
          end;
          suppose f.x = c & f.y = a;
          then c <= a by A117,YELLOW_0:60;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose f.x = c & f.y = b;
          then c <= b by A117,YELLOW_0:60;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose f.x = c & f.y = c;
          hence x <= y by A89,WAYBEL_1:def 1;
          end;
          suppose f.x = c & f.y = d;
          then c <= d by A117,YELLOW_0:60;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose A125: f.x = c & f.y = e;
A126:      dw c= t
           proof
            let X be set;
            assume X in dw;
            then X=0 or X=1 by CARD_5:1,TARSKI:def 2;
            hence thesis by Th1,ENUMSET1:def 1;
           end;
               f.dw = c & f.t = e by A88;
          then dw=x & t = y by A89,A125,WAYBEL_1:def 1;
          hence x <= y by A126,YELLOW_1:3;
          end;
          suppose f.x = d & f.y = a;
          then d <= a & a <= b by A46,A117,YELLOW_0:25,60;
          then d <= b by ORDERS_2:26;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose f.x = d & f.y = b;
          then d <= b by A117,YELLOW_0:60;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose f.x = d & f.y = c;
          then d <= c by A117,YELLOW_0:60;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose f.x = d & f.y = d;
          hence x <= y by A89,WAYBEL_1:def 1;
          end;
          suppose A127: f.x = d & f.y = e;
               f.tj = d & f.t = e by A88;
          then tj=x & t = y by A89,A127,WAYBEL_1:def 1;
          hence x <= y by YELLOW_1:3;
          end;
          suppose f.x = e & f.y = a;
          then A128:        e <= a by A117,YELLOW_0:60;
               a <= b & b <= d & d <= e by A46,YELLOW_0:25;
          then a <= d & d <= e by ORDERS_2:26;
          then a <= e by ORDERS_2:26;
          hence x <= y by A46,A128,ORDERS_2:25;
          end;
          suppose f.x = e & f.y = b;
          then A129:        e <= b by A117,YELLOW_0:60;
               b <= d & d <= e by A46,YELLOW_0:25;
          then b <= e by ORDERS_2:26;
          hence x <= y by A46,A129,ORDERS_2:25;
          end;
          suppose f.x = e & f.y = c;
          then e <= c by A117,YELLOW_0:60;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose f.x = e & f.y = d;
          then e <= d by A117,YELLOW_0:60;
          hence x <= y by A46,YELLOW_0:25;
          end;
          suppose f.x = e & f.y = e;
          hence x <= y by A89,WAYBEL_1:def 1;
         end;
         end;
       end;
      hence f is isomorphic by A47,A89,A92,WAYBEL_0:66;
     end;
   end;
 end;

theorem Th10:
for L being LATTICE holds
(ex K being full Sublattice of L st M_3,K are_isomorphic) iff
ex a,b,c,d,e being Element of L st
(a<>b & a<>c & a<>d & a<>e & b<>c & b<>d & b<>e & c <>d & c <>e & d<>e &
a"/\"b = a & a"/\"c = a & a"/\"d = a & b"/\"e = b & c"/\"e = c & d"/\"e = d &
  b"/\"c = a & b"/\"d = a & c"/\"d = a & b"\/"c = e & b"\/"d = e & c"\/"d = e)
 proof
  let L be LATTICE;
  set cn = the carrier of M_3;
A1: cn = {0, 1, 2 \ 1 , 3 \ 2, 3} by YELLOW_1:1;

  thus (ex K being full Sublattice of L st
  M_3,K are_isomorphic) implies
  ex a,b,c,d,e being Element of L st
(a<>b & a<>c & a<>d & a<>e & b<>c & b<>d & b<>e & c <>d & c <>e & d<>e &
a"/\"b = a & a"/\"c = a & a"/\"d = a & b"/\"e = b & c"/\"e = c & d"/\"e = d &
b"/\"c = a & b"/\"d = a & c"/\"d = a & b"\/"c = e & b"\/"d = e & c"\/"d = e)
   proof
    given K being full Sublattice of L such that
A2:  M_3,K are_isomorphic;
    consider f being Function of M_3,K such that
A3:  f is isomorphic by A2,WAYBEL_1:def 8;
A4: K is non empty by A3,WAYBEL_0:def 38;
    reconsider K as non empty SubRelStr of L by A3,WAYBEL_0:def 38;
A5: f is one-to-one monotone by A3,A4,WAYBEL_0:def 38;
    reconsider z = 0 as Element of M_3 by A1,ENUMSET1:def 3;
    reconsider j = 1 as Element of M_3 by A1,ENUMSET1:def 3;
    reconsider dj = 2\1 as Element of M_3 by A1,ENUMSET1:def 3;
    reconsider td = 3\2 as Element of M_3 by A1,ENUMSET1:def 3;
    reconsider t = 3 as Element of M_3 by A1,ENUMSET1:def 3;
    reconsider cl = the carrier of L as non empty set;
    reconsider ck = the carrier of K as non empty set;
A6: ck = rng f by A3,WAYBEL_0:66;
    reconsider g=f as Function of cn,ck;
    reconsider a=g.z,b=g.j,c =g.dj,d=g.td,e=g.t as Element of K
                                   ;
    reconsider ck as non empty Subset of cl by YELLOW_0:def 13;
         a in ck & b in ck & c in ck & d in ck & e in ck;
    then reconsider A=a,B=b,C=c,D=d,E=e as Element of L;
    take A,B,C,D,E;
    thus A<>B by A5,WAYBEL_1:def 1;
    thus A<>C by A5,Th2,WAYBEL_1:def 1;
    thus A<>D by A5,Th4,WAYBEL_1:def 1;
    thus A<>E by A5,WAYBEL_1:def 1;
         now
     assume f.j = f.dj;
     then j = dj by A4,A5,WAYBEL_1:def 1;
     then 1 in 1 by Th2,TARSKI:def 1;
     hence contradiction;
    end;
    hence B<>C;
         now
     assume f.j = f.td;
     then A7:   j = td by A4,A5,WAYBEL_1:def 1;
          0 in j & 0 <> 2 by CARD_5:1,TARSKI:def 1;
     hence contradiction by A7,Th4,TARSKI:def 1;
    end;
    hence B<>D;
    thus B<>E by A5,WAYBEL_1:def 1;
         now
     assume f.dj = f.td;
     then A8:   dj = td by A4,A5,WAYBEL_1:def 1;
          1 in dj & 1 <> 2 by Th2,TARSKI:def 1;
     hence contradiction by A8,Th4,TARSKI:def 1;
    end;
    hence C<>D;
         now
     assume f.dj = f.t;
     then A9:   dj = t by A4,A5,WAYBEL_1:def 1;
          0 in t & 0 <> 1 by Th1,ENUMSET1:def 1;
     hence contradiction by A9,Th2,TARSKI:def 1;
    end;
    hence C<>E;
         now
     assume f.td = f.t;
     then A10:   td = t by A4,A5,WAYBEL_1:def 1;
          0 in t & 0 <> 2 by Th1,ENUMSET1:def 1;
     hence contradiction by A10,Th4,TARSKI:def 1;
    end;
    hence D<>E;
         z c= j by XBOOLE_1:2;
    then z <= j by YELLOW_1:3;
    then a <= b by A3,WAYBEL_0:66;
    then A <= B by YELLOW_0:60;
    hence A "/\" B = A by YELLOW_0:25;
         z c= dj by XBOOLE_1:2;
    then z <= dj by YELLOW_1:3;
    then a <= c by A3,WAYBEL_0:66;
    then A <= C by YELLOW_0:60;
    hence A "/\" C = A by YELLOW_0:25;

         z c= td by XBOOLE_1:2;
    then z <= td by YELLOW_1:3;
    then a <= d by A3,WAYBEL_0:66;
    then A <= D by YELLOW_0:60;
    hence A "/\" D = A by YELLOW_0:25;

         j c= t by CARD_1:56;
    then j <= t by YELLOW_1:3;
    then b <= e by A3,WAYBEL_0:66;
    then B <= E by YELLOW_0:60;
    hence B "/\" E = B by YELLOW_0:25;

         dj c= t
     proof
      let x be set;
      assume x in dj; then x=1 by Th2,TARSKI:def 1;
      hence thesis by Th1,ENUMSET1:def 1;
     end;
    then dj <= t by YELLOW_1:3;
    then c <= e by A3,WAYBEL_0:66;
    then C <= E by YELLOW_0:60;
    hence C"/\"E = C by YELLOW_0:25;
    td <= t by YELLOW_1:3;
    then d <= e by A3,WAYBEL_0:66;
    then D <= E by YELLOW_0:60;
    hence D"/\"E = D by YELLOW_0:25;

    thus B"/\"C = A
     proof
           B in the carrier of K & C in the carrier of K & ex_inf_of {B,C},L
                                  by YELLOW_0:21;
      then inf{B,C} in the carrier of K by YELLOW_0:def 16;
      then B"/\"C in rng f by A6,YELLOW_0:40;
      then consider x being set such that
A11:    x in dom f & B"/\"C = f.x by FUNCT_1:def 5;
      reconsider x as Element of M_3 by A11;
A12:      x = z or x = j or x = dj or x = td or x = t by A1,ENUMSET1:def 3;
A13:    now
       assume B"/\"C = B;
       then B <= C by YELLOW_0:25;
       then b <= c by YELLOW_0:61;
       then j <= dj by A3,WAYBEL_0:66;
       then A14:    j c= dj by YELLOW_1:3;
            0 in j by CARD_5:1,TARSKI:def 1;
       hence contradiction by A14,Th2,TARSKI:def 1;
      end;
A15:    now
       assume B"/\"C = C;
       then C <= B by YELLOW_0:25;
       then c <= b by YELLOW_0:61;
       then dj <= j by A3,WAYBEL_0:66;
       then A16:    dj c= j by YELLOW_1:3;
            1 in dj & 0 <> 1 by Th2,TARSKI:def 1;
       hence contradiction by A16,CARD_5:1,TARSKI:def 1;
      end;
A17:    now
       assume B"/\"C = D;
       then D <= C by YELLOW_0:23;
       then d <= c by YELLOW_0:61;
       then td <= dj by A3,WAYBEL_0:66;
       then A18:    td c= dj by YELLOW_1:3;
            2 in td by Th4,TARSKI:def 1;
       hence contradiction by A18,Th2,TARSKI:def 1;
      end;
           now
       assume B"/\"C = E;
       then E <= C by YELLOW_0:23;
       then e <= c by YELLOW_0:61;
       then t <= dj by A3,WAYBEL_0:66;
       then A19:    t c= dj by YELLOW_1:3;
            2 in t by Th1,ENUMSET1:def 1;
       hence contradiction by A19,Th2,TARSKI:def 1;
      end;
      hence thesis by A11,A12,A13,A15,A17;
     end;

    thus B"/\"D = A
     proof
           B in the carrier of K & D in the carrier of K & ex_inf_of {B,D},L
                                  by YELLOW_0:21;
      then inf{B,D} in the carrier of K by YELLOW_0:def 16;
      then B"/\"D in rng f by A6,YELLOW_0:40;
      then consider x being set such that
A20:    x in dom f & B"/\"D = f.x by FUNCT_1:def 5;
      reconsider x as Element of M_3 by A20;
A21:      x = z or x = j or x = dj or x = td or x = t by A1,ENUMSET1:def 3;
A22:    now
       assume B"/\"D = B;
       then B <= D by YELLOW_0:25;
       then b <= d by YELLOW_0:61;
       then j <= td by A3,WAYBEL_0:66;
       then A23:    j c= td by YELLOW_1:3;
            0 in j by CARD_5:1,TARSKI:def 1;
       hence contradiction by A23,Th4,TARSKI:def 1;
      end;
A24:    now
       assume B"/\"D = C;
       then C <= B by YELLOW_0:23;
       then c <= b by YELLOW_0:61;
       then dj <= j by A3,WAYBEL_0:66;
       then A25:    dj c= j by YELLOW_1:3;
            1 in dj & 0 <> 1 by Th2,TARSKI:def 1;
       hence contradiction by A25,CARD_5:1,TARSKI:def 1;
      end;
A26:    now
       assume B"/\"D = D;
       then D <= B by YELLOW_0:23;
       then d <= b by YELLOW_0:61;
       then td <= j by A3,WAYBEL_0:66;
       then A27:    td c= j by YELLOW_1:3;
            2 in td by Th4,TARSKI:def 1;
       hence contradiction by A27,CARD_5:1,TARSKI:def 1;
      end;
           now
       assume B"/\"D = E;
       then E <= B by YELLOW_0:23;
       then e <= b by YELLOW_0:61;
       then t <= j by A3,WAYBEL_0:66;
       then A28:    t c= j by YELLOW_1:3;
            2 in t by Th1,ENUMSET1:def 1;
       hence contradiction by A28,CARD_5:1,TARSKI:def 1;
      end;
      hence thesis by A20,A21,A22,A24,A26;
     end;

    thus C"/\"D = A
     proof
           C in the carrier of K & D in the carrier of K & ex_inf_of {C,D},L
                                  by YELLOW_0:21;
      then inf{C,D} in the carrier of K by YELLOW_0:def 16;
      then C"/\"D in rng f by A6,YELLOW_0:40;
      then consider x being set such that
A29:    x in dom f & C"/\"D = f.x by FUNCT_1:def 5;
      reconsider x as Element of M_3 by A29;
A30:      x = z or x = j or x = dj or x = td or x = t by A1,ENUMSET1:def 3;
A31:    now
       assume C"/\"D = B;
       then B <= C by YELLOW_0:23;
       then b <= c by YELLOW_0:61;
       then j <= dj by A3,WAYBEL_0:66;
       then A32:    j c= dj by YELLOW_1:3;
            0 in j by CARD_5:1,TARSKI:def 1;
       hence contradiction by A32,Th2,TARSKI:def 1;
      end;
A33:    now
       assume C"/\"D = C;
       then C <= D by YELLOW_0:25;
       then c <= d by YELLOW_0:61;
       then dj <= td by A3,WAYBEL_0:66;
       then A34:    dj c= td by YELLOW_1:3;
            1 in dj & 1 <> 2 by Th2,TARSKI:def 1;
       hence contradiction by A34,Th4,TARSKI:def 1;
      end;
A35:    now
       assume C"/\"D = D;
       then D <= C by YELLOW_0:23;
       then d <= c by YELLOW_0:61;
       then td <= dj by A3,WAYBEL_0:66;
       then A36:    td c= dj by YELLOW_1:3;
            2 in td by Th4,TARSKI:def 1;
       hence contradiction by A36,Th2,TARSKI:def 1;
      end;
           now
       assume C"/\"D = E;
       then E <= C by YELLOW_0:23;
       then e <= c by YELLOW_0:61;
       then t <= dj by A3,WAYBEL_0:66;
       then A37:    t c= dj by YELLOW_1:3;
            2 in t by Th1,ENUMSET1:def 1;
       hence contradiction by A37,Th2,TARSKI:def 1;
      end;
      hence thesis by A29,A30,A31,A33,A35;
     end;

    thus B"\/"C = E
     proof
           B in the carrier of K & B in the carrier of K & ex_sup_of {B,C},L
                                  by YELLOW_0:20;
      then sup{B,C} in the carrier of K by YELLOW_0:def 17;
      then B"\/"C in rng f by A6,YELLOW_0:41;
      then consider x being set such that
A38:    x in dom f & B"\/"C = f.x by FUNCT_1:def 5;
      reconsider x as Element of M_3 by A38;
A39:      x = z or x = j or x = dj or x = td or x = t by A1,ENUMSET1:def 3;
A40:    now
       assume B"\/"C = B;
       then B >= C by YELLOW_0:24;
       then b >= c by YELLOW_0:61;
       then j >= dj by A3,WAYBEL_0:66;
       then A41:    dj c= j by YELLOW_1:3;
            1 in dj & 0 <> 1 by Th2,TARSKI:def 1;
       hence contradiction by A41,CARD_5:1,TARSKI:def 1;
      end;
A42:    now
       assume B"\/"C = C;
       then C >= B by YELLOW_0:24;
       then c >= b by YELLOW_0:61;
       then dj >= j by A3,WAYBEL_0:66;
       then A43:    j c= dj by YELLOW_1:3;
            0 in j by CARD_5:1,TARSKI:def 1;
       hence contradiction by A43,Th2,TARSKI:def 1;
      end;
A44:    now
       assume B"\/"C = D;
       then D >= C by YELLOW_0:22;
       then d >= c by YELLOW_0:61;
       then td >= dj by A3,WAYBEL_0:66;
       then A45:    dj c= td by YELLOW_1:3;
            1 in dj & 2 <> 1 by Th2,TARSKI:def 1;
       hence contradiction by A45,Th4,TARSKI:def 1;
      end;
           now
       assume B"\/"C = A;
       then A >= C by YELLOW_0:22;
       then a >= c by YELLOW_0:61;
       then z >= dj by A3,WAYBEL_0:66;
       then dj c= z by YELLOW_1:3;
       hence contradiction by Th2,XBOOLE_1:3;
      end;
      hence thesis by A38,A39,A40,A42,A44;
     end;

    thus B"\/"D = E
     proof
           B in the carrier of K & D in the carrier of K & ex_sup_of {B,D},L
                                  by YELLOW_0:20;
      then sup{B,D} in the carrier of K by YELLOW_0:def 17;
      then B"\/"D in rng f by A6,YELLOW_0:41;
      then consider x being set such that
A46:    x in dom f & B"\/"D = f.x by FUNCT_1:def 5;
      reconsider x as Element of M_3 by A46;
A47:      x = z or x = j or x = dj or x = td or x = t by A1,ENUMSET1:def 3;
A48:    now
       assume B"\/"D = B;
       then B >= D by YELLOW_0:22;
       then b >= d by YELLOW_0:61;
       then j >= td by A3,WAYBEL_0:66;
       then A49:    td c= j by YELLOW_1:3;
            2 in td & 2 <> 0 by Th4,TARSKI:def 1;
       hence contradiction by A49,CARD_5:1,TARSKI:def 1;
      end;
A50:    now
       assume B"\/"D = C;
       then C >= D by YELLOW_0:22;
       then c >= d by YELLOW_0:61;
       then dj >= td by A3,WAYBEL_0:66;
       then A51:    td c= dj by YELLOW_1:3;
            2 in td & 2 <> 1 by Th4,TARSKI:def 1;
       hence contradiction by A51,Th2,TARSKI:def 1;
      end;
A52:    now
       assume B"\/"D = D;
       then D >= B by YELLOW_0:22;
       then d >= b by YELLOW_0:61;
       then td >= j by A3,WAYBEL_0:66;
       then A53:    j c= td by YELLOW_1:3;
            0 in j & 0 <> 2 by CARD_5:1,TARSKI:def 1;
       hence contradiction by A53,Th4,TARSKI:def 1;
      end;
           now
       assume B"\/"D = A;
       then A >= B by YELLOW_0:22;
       then a >= b by YELLOW_0:61;
       then z >= j by A3,WAYBEL_0:66;
       then j c= z by YELLOW_1:3;
       hence contradiction by XBOOLE_1:3;
      end;
      hence thesis by A46,A47,A48,A50,A52;
     end;

    thus C"\/"D = E
     proof
           C in the carrier of K & D in the carrier of K & ex_sup_of {C,D},L
                                  by YELLOW_0:20;
      then sup{C,D} in the carrier of K by YELLOW_0:def 17;
      then C"\/"D in rng f by A6,YELLOW_0:41;
      then consider x being set such that
A54:    x in dom f & C"\/"D = f.x by FUNCT_1:def 5;
      reconsider x as Element of M_3 by A54;
A55:      x = z or x = j or x = dj or x = td or x = t by A1,ENUMSET1:def 3;
A56:    now
       assume C"\/"D = B;
       then B >= C by YELLOW_0:22;
       then b >= c by YELLOW_0:61;
       then j >= dj by A3,WAYBEL_0:66;
       then A57:    dj c= j by YELLOW_1:3;
            1 in dj by Th2,TARSKI:def 1;
       then 1 in 1 by A57;
       hence contradiction;
      end;
A58:    now
       assume C"\/"D = C;
       then C >= D by YELLOW_0:24;
       then c >= d by YELLOW_0:61;
       then dj >= td by A3,WAYBEL_0:66;
       then A59:    td c= dj by YELLOW_1:3;
            2 in td & 2 <> 1 by Th4,TARSKI:def 1;
       hence contradiction by A59,Th2,TARSKI:def 1;
      end;
A60:    now
       assume C"\/"D = D;
       then D >= C by YELLOW_0:22;
       then d >= c by YELLOW_0:61;
       then td >= dj by A3,WAYBEL_0:66;
       then A61:    dj c= td by YELLOW_1:3;
            1 in dj & 1 <> 2 by Th2,TARSKI:def 1;
       hence contradiction by A61,Th4,TARSKI:def 1;
      end;
           now
       assume C"\/"D = A;
       then A >= C by YELLOW_0:22;
       then a >= c by YELLOW_0:61;
       then z >= dj by A3,WAYBEL_0:66;
       then dj c= z by YELLOW_1:3;
       hence contradiction by Th2,XBOOLE_1:3;
      end;
      hence thesis by A54,A55,A56,A58,A60;
     end;

   end;
  thus (ex a,b,c,d,e being Element of L st
  (a<>b & a<>c & a<>d & a<>e & b<>c & b<>d & b<>e & c <>d & c <>e & d<>e &
  a"/\"b = a & a"/\"c = a & a"/\"d = a & b"/\"e = b & c"/\"e = c & d"/\"e = d &
  b"/\"c = a & b"/\"d = a & c"/\"d = a & b"\/"c = e & b"\/"d = e & c"\/"
d = e)) implies
  ex K being full Sublattice of L st
  M_3,K are_isomorphic
   proof
    given a,b,c,d,e being Element of L such that
A62:  a<>b & a<>c & a<>d & a<>e & b<>c & b<>d & b<>e & c <>d & c <>e & d<>e &
    a"/\"b = a & a"/\"c = a & a"/\"d = a & b"/\"e = b & c"/\"e = c & d"/\"
e = d &
    b"/\"c = a & b"/\"d = a & c"/\"d = a & b"\/"c = e & b"\/"d = e & c"\/"
d = e;

    set ck = {a,b,c,d,e};
    reconsider ck as Subset of L;
    set K = subrelstr ck;
A63: the carrier of K = ck by YELLOW_0:def 15;
A64:  K is meet-inheriting
     proof
      let x,y be Element of L;
      assume
A65:      x in the carrier of K & y in the carrier of K & ex_inf_of {x,y},L;
      per cases by A63,A65,ENUMSET1:def 3;
      suppose x=a & y=a;
      then inf{x,y} = a"/\"a by YELLOW_0:40;
      then inf{x,y} = a by YELLOW_5:2;
      hence thesis by A63,ENUMSET1:def 3;
      end;
      suppose x=a & y=b;
      then inf{x,y} = a"/\"b by YELLOW_0:40;
      hence thesis by A62,A63,ENUMSET1:def 3;
      end;
      suppose x=a & y=c;
      then inf{x,y} = a"/\"c by YELLOW_0:40;
      hence thesis by A62,A63,ENUMSET1:def 3;
      end;
      suppose x=a & y=d;
      then inf{x,y} = a"/\"d by YELLOW_0:40;
      hence thesis by A62,A63,ENUMSET1:def 3;
      end;
      suppose
A66:    x=a & y=e;
           a <= c & c <= e by A62,YELLOW_0:25;
      then a <= e by ORDERS_2:26;
      then a"/\"e = a by YELLOW_0:25;
      then inf {x,y} = a by A66,YELLOW_0:40;
      hence thesis by A63,ENUMSET1:def 3;
      end;
      suppose x=b & y=a;
      then inf{x,y} = a"/\"b by YELLOW_0:40;
      hence thesis by A62,A63,ENUMSET1:def 3;
      end;
      suppose x=b & y=b;
      then inf{x,y} = b"/\"b by YELLOW_0:40;
      then inf{x,y} = b by YELLOW_5:2;
      hence thesis by A63,ENUMSET1:def 3;
      end;
      suppose x=b & y=c;
      then inf{x,y} = b"/\"c by YELLOW_0:40;
      hence thesis by A62,A63,ENUMSET1:def 3;
      end;
