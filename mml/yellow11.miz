     then A14: z'=0 or z'=1 or z'=2\1 or z'=3\2 or z'=3 by ENUMSET1:def 3;
     assume x <= z' & y <= z';
     then A15:  1 c= z' & 3\2 c= z' by A13,YELLOW_1:3;
A16:  now
      assume A17: z'= 0;
           0 in 1 by CARD_5:1,TARSKI:def 1;
      hence contradiction by A15,A17;
     end;
A18:  now
      assume A19: z'= 1;
           2 in 3\2 by Th4,TARSKI:def 1;
