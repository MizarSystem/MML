end;

theorem Th41:
  seq is bounded_above implies (superior_realsequence seq).0 = sup
  seq
proof
  reconsider Y1 = {seq.k : 0 <= k} as Subset of REAL by Th29;
  (superior_realsequence seq).0 = sup Y1 by Def5
