    let G be VGraph, v be Vertex of G, x be set;
A1: G.labelVertex(v,x).labeledV() = G.labeledV() \/ {v} by tGLABELV02;
    v in {v} by TARSKI:def 1;
    hence thesis by A1,XBOOLE_0:def 2;
end;    
