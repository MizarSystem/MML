Lm12: o<>a & o<>b & LIN o,a,b & AFP is Desarguesian & ( not LIN o,a,x & LIN o,
x,y & a,x // b,y or (LIN o,a,x & ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p //
b,p9 & p,x // p9,y & LIN o,a,y)) & ( not LIN o,a,r & LIN o,r,y & a,r // b,y or
(LIN o,a,r & ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,r // p9,y
& LIN o,a,y)) implies x=r
proof
  assume that
A1: o<>a and
A2: o<>b and
A3: LIN o,a,b and
A4: AFP is Desarguesian and
A5: not LIN o,a,x & LIN o,x,y & a,x // b,y or (LIN o,a,x & ex p,p9 st
  not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,x // p9,y & LIN o,a,y) and
A6: not LIN o,a,r & LIN o,r,y & a,r // b,y or (LIN o,a,r & ex p,p9 st
  not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,r // p9,y & LIN o,a,y);
A7: not LIN o,b,y & LIN o,y,r & b,y // a,r or (LIN o,b,y & ex p,p9 st not
LIN o,b,p & LIN o,p,p9 & b,p // a,p9 & p,y // p9,r & LIN o,b,r) by A1,A2,A3,A6
  ,Lm6;
A8: LIN o,b,a by A3,AFF_1:15;
  not LIN o,b,y & LIN o,y,x & b,y // a,x or (LIN o,b,y & ex p,p9 st not
LIN o,b,p & LIN o,p,p9 & b,p // a,p9 & p,y // p9,x & LIN o,b,x) by A1,A2,A3,A5
  ,Lm6;
  hence thesis by A2,A4,A8,A7,Lm11,AFF_1:70;
end;

Lm13: for o,a,b st AFP is Desarguesian & o<>a & o<>b & LIN o,a,b ex f st for x
,y holds (f.x=y iff ( not LIN o,a,x & LIN o,x,y & a,x // b,y or (LIN o,a,x & ex
p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,x // p9,y & LIN o,a,y)))
proof
  let o,a,b;
  defpred P[Element of AFP,Element of AFP] means ((not LIN o,a,$1 & LIN o,$1,
$2 & a,$1 // b,$2) or (LIN o,a,$1 & ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p
  // b,p9 & p,$1 // p9,$2 & LIN o,a,$2));
  assume that
A1: AFP is Desarguesian and
A2: o<>a and
A3: o<>b and
A4: LIN o,a,b;
A5: for y ex x st P[x,y] by A2,A3,A4,Lm9;
A6: for x ex y st P[x,y] by A2,A4,Lm8;
A7: for x,y,s st P[x,y] & P[x,s] holds y=s by A1,A2,A4,Lm11,AFF_1:70;
A8: for x,y,r st P[x,y] & P[r,y] holds x=r by A1,A2,A3,A4,Lm12;
  ex f st for x,y holds (f.x=y iff P[x,y]) from TRANSGEO:sch 1(A6,A5, A8,
  A7);
  hence thesis;
end;

Lm14: AFP is Desarguesian & o<>a & LIN o,a,b & not LIN o,a,x & LIN o,x,y & a,x
// b,y & LIN o,a,z & (ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,z
// p9,t & LIN o,a,t) implies x,z // y,t
proof
  assume that
A1: AFP is Desarguesian and
A2: o<>a and
A3: LIN o,a,b and
A4: not LIN o,a,x and
A5: LIN o,x,y and
A6: a,x // b,y and
A7: LIN o,a,z and
A8: ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,z // p9,t &
  LIN o,a,t;
  consider p,p9 such that
A9: not LIN o,a,p and
A10: LIN o,p,p9 and
A11: a,p // b,p9 and
A12: p,z // p9,t and
A13: LIN o,a,t by A8;
A14: now
A15: p,a // p9,b by A11,AFF_1:13;
    assume
A16: a=z;
    not LIN o,p,a by A9,AFF_1:15;
    then z,x // t,y by A3,A6,A10,A12,A13,A16,A15,AFF_1:70;
    hence thesis by AFF_1:13;
  end;
A17: p,x // p9,y by A1,A3,A4,A5,A6,A9,A10,A11,Lm1;
A18: now
    assume that
A19: z<>o and
A20: not LIN o,p,x;
A21: not LIN o,p,z
    proof
A22:  LIN o,p,o by AFF_1:16;
      assume
A23:  LIN o,p,z;
      LIN o,z,a by A7,AFF_1:15;
      then LIN o,p,a by A19,A23,A22,AFF_1:20;
      hence contradiction by A9,AFF_1:15;
    end;
    LIN o,a,o by AFF_1:16;
    then LIN o,z,t by A2,A7,A13,AFF_1:17;
    hence thesis by A1,A5,A10,A12,A17,A20,A21,Lm1;
  end;
A24: o<>x by A4,AFF_1:16;
A25: now
    assume that
A26: z<>o and
A27: LIN o,p,x and
    a<>z;
    now
A28:  LIN o,x,o by AFF_1:16;
      LIN o,x,p by A27,AFF_1:15;
      then
A29:  LIN o,p,y by A5,A24,A28,AFF_1:17;
      assume
A30:  x<>p;
      z,p // t,p9 by A12,AFF_1:13;
      then z,x // t,y by A1,A3,A6,A7,A9,A10,A11,A13,A24,A26,A27,A30,A29,
      Th1;
      hence thesis by AFF_1:13;
    end;
    hence thesis by A3,A4,A5,A6,A10,A11,A12,AFF_1:70;
  end;
  now
A31: o,x // o,y by A5,AFF_1:def 1;
    assume
A32: z=o;
    then t=o by A2,A7,A8,Lm7;
    hence thesis by A32,A31,AFF_1:13;
  end;
  hence thesis by A18,A14,A25;
end;

Lm15: AFP is Desarguesian & o<>a & LIN o,a,b & LIN o,a,x & (ex p,p9 st not LIN
o,a,p & LIN o,p,p9 & a,p // b,p9 & p,x // p9,y & LIN o,a,y) & not LIN o,a,z &
LIN o,z,t & a,z // b,t implies x,z // y,t
proof
  assume that
A1: AFP is Desarguesian and
A2: o<>a and
A3: LIN o,a,b and
A4: LIN o,a,x and
A5: ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,x // p9,y &
  LIN o,a,y and
A6: not LIN o,a,z and
A7: LIN o,z,t and
A8: a,z // b,t;
  z,x // t,y by A1,A2,A3,A4,A5,A6,A7,A8,Lm14;
  hence thesis by AFF_1:13;
end;

Lm16: o<>a & LIN o,a,x & (ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9
& p,x // p9,y & LIN o,a,y) & LIN o,a,z & (ex p,p9 st not LIN o,a,p & LIN o,p,p9
& a,p // b,p9 & p,z // p9,t & LIN o,a,t) implies x,z // y,t
proof
  assume that
A1: o<>a and
A2: LIN o,a,x and
A3: ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,x // p9,y &
  LIN o,a,y and
A4: LIN o,a,z and
A5: ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,z // p9,t &
  LIN o,a,t;
  set A=Line(o,a);
A6: x in A by A2,AFF_1:def 2;
A7: z in A by A4,AFF_1:def 2;
A8: y in A by A3,AFF_1:def 2;
A9: t in A by A5,AFF_1:def 2;
A10: A is being_line by A1,AFF_1:def 3;
  now
    assume
A11: x<>z;
    then A=Line(x,z) by A10,A6,A7,AFF_1:38;
    hence thesis by A8,A9,A11,AFF_1:36;
  end;
  hence thesis by AFF_1:12;
end;

Lm17: o<>a & LIN o,a,b & AFP is Desarguesian & (for x,y holds (f.x=y iff ((not
LIN o,a,x & LIN o,x,y & a,x // b,y) or (LIN o,a,x & ex p,p9 st not LIN o,a,p &
LIN o,p,p9 & a,p // b,p9 & p,x // p9,y & LIN o,a,y)))) implies f is dilatation
& f.o=o & f.a=b
proof
  assume that
A1: o<>a and
A2: LIN o,a,b and
A3: AFP is Desarguesian;
  assume
A4: for x,y holds (f.x=y iff ( not LIN o,a,x & LIN o,x,y & a,x // b,y or
(LIN o,a,x & ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,x // p9,y
  & LIN o,a,y)));
  for x,r holds x,r // f.x,f.r
  proof
    let x,r;
    set y=f.x;
    set s=f.r;
A5: not LIN o,a,r & LIN o,r,s & a,r // b,s or (LIN o,a,r & ex p,p9 st not
    LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,r // p9,s & LIN o,a,s) by A4;
    not LIN o,a,x & LIN o,x,y & a,x // b,y or (LIN o,a,x & ex p,p9 st not
    LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,x // p9,y & LIN o,a,y) by A4;
    hence thesis by A1,A2,A3,A5,Lm1,Lm14,Lm15,Lm16;
  end;
  hence f is dilatation by TRANSGEO:85;
  thus f.o=o
  proof
    set y=f.o;
    not LIN o,a,o & LIN o,o,y & a,o // b,y or (LIN o,a,o & ex p,p9 st not
    LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,o // p9,y & LIN o,a,y) by A4;
    hence thesis by A1,Lm7;
  end;
  thus f.a=b
  proof
    set y=f.a;
    LIN o,a,a by AFF_1:16;
    then consider p,p9 such that
A6: not LIN o,a,p and
A7: LIN o,p,p9 and
A8: a,p // b,p9 and
A9: p,a // p9,y and
A10: LIN o,a,y by A4;
A11: p,a // p9,b by A8,AFF_1:13;
    not LIN o,p,a by A6,AFF_1:15;
    hence thesis by A2,A7,A9,A10,A11,AFF_1:70;
  end;
end;

theorem Th3:
  AFP is Desarguesian implies for o,a,b st o<>a & o<>b & LIN o,a,b
  ex f st f is dilatation & f.o=o & f.a=b
proof
  assume
A1: AFP is Desarguesian;
  let o,a,b;
  assume that
A2: o<>a and
A3: o<>b and
A4: LIN o,a,b;
  consider f such that
A5: for x,y holds (f.x=y iff ( not LIN o,a,x & LIN o,x,y & a,x // b,y or
(LIN o,a,x & ex p,p9 st not LIN o,a,p & LIN o,p,p9 & a,p // b,p9 & p,x // p9,y
  & LIN o,a,y))) by A1,A2,A3,A4,Lm13;
A6: f.a=b by A1,A2,A4,A5,Lm17;
A7: f.o=o by A1,A2,A4,A5,Lm17;
  f is dilatation by A1,A2,A4,A5,Lm17;
  hence thesis by A7,A6;
end;

theorem
  AFP is Desarguesian iff for o,a,b st o<>a & o<>b & LIN o,a,b ex f st f
  is dilatation & f.o=o & f.a=b by Th2,Th3;

definition
  let AFP,f,K;
  pred f is_Sc K means
  :Def1:
  f is collineation & K is being_line & (for x st
  x in K holds f.x = x) & for x holds x,f.x // K;
end;

theorem Th5:
  f is_Sc K & f.p=p & not p in K implies f=id the carrier of AFP
proof
  assume that
A1: f is_Sc K and
A2: f.p=p and
A3: not p in K;
A4: K is being_line by A1,Def1;
A5: for x st x in K holds f.x=x by A1,Def1;
  f is collineation by A1,Def1;
  hence thesis by A2,A3,A4,A5,TRANSGEO:117;
end;

theorem Th6:
  (for a,b,K st a,b // K & not a in K ex f st f is_Sc K & f.a=b)
  implies AFP is Moufangian
proof
  assume
A1: for a,b,K st a,b // K & not a in K ex f st f is_Sc K & f.a=b;
  now
    let o,K,c,c9,a,b,a9,b9;
    assume that
A2: o in K and
A3: c in K and
A4: c9 in K and
A5: K is being_line and
A6: LIN o,a,a9 and
A7: LIN o,b,b9 and
A8: not a in K and
A9: a,b // K and
A10: a9,b9 // K and
A11: a,c // a9,c9;
    consider f such that
A12: f is_Sc K and
A13: f.a=b by A1,A8,A9;
A14: f is collineation by A12,Def1;
A15: a,b // a9,b9 by A5,A9,A10,AFF_1:45;
A16: f.a9=b9
    proof
      set x=f.a9;
A17:  now
        f.o=o by A2,A12,Def1;
        then
A18:    LIN o,b,x by A6,A13,A14,TRANSGEO:108;
        a9,x // K by A12,Def1;
        then
A19:    a,b // a9,x by A5,A9,AFF_1:45;
        assume a<>b;
        hence thesis by A2,A6,A7,A8,A9,A15,A19,A18,AFF_1:60,70;
      end;
      now
        assume
A20:    a=b;
        then f=id the carrier of AFP by A8,A12,A13,Th5;
        then x=a9 by FUNCT_1:35;
        hence thesis by A2,A6,A7,A8,A10,A20,AFF_1:61;
      end;
      hence thesis by A17;
    end;
A21: f.c9=c9 by A4,A12,Def1;
    f.c = c by A3,A12,Def1;
    hence b,c // b9,c9 by A11,A13,A14,A21,A16,TRANSGEO:107;
  end;
  hence thesis by Lm2;
end;

Lm18: a,b // K & not a in K & ( x in K & x=y or (not x in K & ex p,p9 st p in
K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K)) implies b,a // K & not b
in K & ( y in K & y=x or (not y in K & ex p,p9 st p in K & p9 in K & p,b // p9,
y & p,a // p9,x & y,x // K))
proof
  assume that
A1: a,b // K and
A2: not a in K and
A3: x in K & x=y or (not x in K & ex p,p9 st p in K & p9 in K & p,a //
  p9,x & p,b // p9,y & x,y // K);
  now
    assume not x in K;
    then y,x // K by A3,AFF_1:48;
    hence thesis by A1,A2,A3,AFF_1:48,49;
  end;
  hence thesis by A1,A2,A3,AFF_1:48,49;
end;

Lm19: a,b // K & not a in K implies for x ex y st ( x in K & x=y or (not x in
K & ex p,p9 st p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K))
proof
  assume that
A1: a,b // K and
A2: not a in K;
A3: not b in K by A1,A2,AFF_1:49;
A4: K is being_line by A1,AFF_1:40;
  let x;
  consider p,q such that
A5: p in K and
  q in K and
  p<>q by A1,AFF_1:31,40;
A6: p<>b by A1,A2,A5,AFF_1:49;
  now
    set B9=Line(p,b);
    set A=Line(p,a);
    assume
A7: not x in K;
A8: a in A by AFF_1:26;
    A is being_line by A2,A5,AFF_1:def 3;
    then consider C such that
A9: x in C and
A10: A // C by AFF_1:63;
A11: C is being_line by A10,AFF_1:50;
A12: p in A by AFF_1:26;
    then not A // K by A2,A5,A8,AFF_1:59;
    then not C // K by A10,AFF_1:58;
    then consider p9 such that
A13: p9 in C and
A14: p9 in K by A4,A11,AFF_1:72;
    B9 is being_line by A6,AFF_1:def 3;
    then consider D such that
A15: p9 in D and
A16: B9 // D by AFF_1:63;
A17: b in B9 by AFF_1:26;
    K is being_line by A1,AFF_1:40; then
    consider M such that
A18: x in M and
A19: K // M by AFF_1:63;
A20: M is being_line by A19,AFF_1:50;
A21: p in B9 by AFF_1:26;
    then
A22: not B9 // K by A5,A3,A17,AFF_1:59;
A23: not D // M
    proof
      assume D // M;
      then B9 // M by A16,AFF_1:58;
      hence contradiction by A22,A19,AFF_1:58;
    end;
    D is being_line by A16,AFF_1:50;
    then consider y such that
A24: y in D and
A25: y in M by A20,A23,AFF_1:72;
A26: p,b // p9,y by A21,A17,A15,A16,A24,AFF_1:53;
A27: x,y // K by A18,A19,A25,AFF_1:54;
    p,a // p9,x by A12,A8,A9,A10,A13,AFF_1:53;
    hence thesis by A5,A7,A14,A27,A26;
  end;
  hence thesis;
end;

Lm20: a,b // K & not a in K implies for y ex x st ( x in K & x=y or (not x in
K & ex p,p9 st p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K))
proof
  assume that
A1: a,b // K and
A2: not a in K;
  let y;
A3: b,a // K by A1,AFF_1:48;
A4: not b in K by A1,A2,AFF_1:49;
  then consider x such that
A5: y in K & y=x or (not y in K & ex p,p9 st p in K & p9 in K & p,b //
  p9,y & p,a // p9,x & y,x // K) by A3,Lm19;
  x in K & x=y or (not x in K & ex p,p9 st p in K & p9 in K & p,a // p9,x
  & p,b // p9,y & x,y // K) by A3,A4,A5,Lm18;
  hence thesis;
end;

theorem Th7:
  K // M & p in K & q in K & p9 in K & q9 in K & AFP is Moufangian
& a in M & b in M & x in M & y in M & a<>b & q<>p & p,a // p9,x & p,b // p9,y &
  q,a // q9,x implies q,b // q9,y
proof
  assume that
A1: K // M and
A2: p in K and
A3: q in K and
A4: p9 in K and
A5: q9 in K and
A6: AFP is Moufangian and
A7: a in M and
A8: b in M and
A9: x in M and
A10: y in M and
A11: a<>b and
A12: q<>p and
A13: p,a // p9,x and
A14: p,b // p9,y and
A15: q,a // q9,x;
A16: K is being_line by A1,AFF_1:50;
A17: M is being_line by A1,AFF_1:50;
  now
    set D9=Line(q9,x);
    set B99=Line(p9,y);
    set D=Line(q,a);
    set B9=Line(p,b);
A18: q in D by AFF_1:26;
A19: a in D by AFF_1:26;
A20: x in D9 by AFF_1:26;
A21: q9 in D9 by AFF_1:26;
A22: LIN p,q,q9 by A2,A3,A5,A16,AFF_1:33;
A23: LIN p,q,p9 by A2,A3,A4,A16,AFF_1:33;
    assume
A24: K<>M;
    then
A25: q9<>x by A1,A5,A9,AFF_1:59;
A26: not a in K by A1,A7,A24,AFF_1:59;
A27: b<>p by A1,A2,A8,A24,AFF_1:59;
    then
A28: B9 is being_line by AFF_1:def 3;
A29: not q in M by A1,A3,A24,AFF_1:59;
A30: not p in M by A1,A2,A24,AFF_1:59;
A31: LIN a,b,x by A7,A8,A9,A17,AFF_1:33;
A32: now
A33:  now
        assume that
A34:    q=p9 and
A35:    q=q9;
        LIN q,a,x by A15,A35,AFF_1:def 1;
        then LIN a,x,q by AFF_1:15;
        then a=x by A7,A9,A17,A29,AFF_1:39;
        then a,q // a,p by A13,A34,AFF_1:13;
        then LIN a,q,p by AFF_1:def 1;
        then LIN p,q,a by AFF_1:15;
        hence contradiction by A2,A3,A12,A16,A26,AFF_1:39;
      end;
A36:  now
        assume that
A37:    p=p9 and
A38:    p=q9;
        LIN p,a,x by A13,A37,AFF_1:def 1;
        then LIN a,x,p by AFF_1:15;
        then a=x by A7,A9,A17,A30,AFF_1:39;
        then a,q // a,q9 by A15,AFF_1:13;
        then LIN a,q,q9 by AFF_1:def 1;
        then LIN p,q,a by A38,AFF_1:15;
        hence contradiction by A2,A3,A12,A16,A26,AFF_1:39;
      end;
      assume
A39:  not ex a,b,c st LIN a,b,c & a<>b & a<>c & b<>c;
A40:  now
        assume that
A41:    q=p9 and
A42:    p=q9;
A43:    now
          assume
A44:      b=x;
          then q,y // q,a by A14,A15,A27,A41,A42,AFF_1:14;
          then LIN q,y,a by AFF_1:def 1;
          then LIN a,y,q by AFF_1:15;
          then q9,y // q,b by A7,A10,A13,A17,A29,A41,A42,A44,AFF_1:39;
          hence thesis by AFF_1:13;
        end;
        now
          assume a=x;
          then a,p // a,q by A13,A41,AFF_1:13;
          then LIN a,p,q by AFF_1:def 1;
          then LIN p,q,a by AFF_1:15;
          hence contradiction by A2,A3,A12,A16,A26,AFF_1:39;
        end;
        hence thesis by A11,A31,A39,A43;
      end;
      now
        assume that
A45:    p=p9 and
A46:    q=q9;
        LIN p,b,y by A14,A45,AFF_1:def 1;
        then LIN b,y,p by AFF_1:15;
        then b=y by A8,A10,A17,A30,AFF_1:39;
        hence thesis by A46,AFF_1:11;
      end;
      hence thesis by A12,A23,A22,A39,A36,A33,A40;
    end;
A47: y in B99 by AFF_1:26;
A48: p9 in B99 by AFF_1:26;
A49: b in B9 by AFF_1:26;
    then
A50: B9<>K by A1,A8,A24,AFF_1:59;
    a<>q by A1,A3,A7,A24,AFF_1:59;
    then
A51: D // D9 by A15,A25,AFF_1:51;
A52: p in B9 by AFF_1:26;
    then
A53: B9<>M by A1,A2,A24,AFF_1:59;
A54: p9<>y by A1,A4,A10,A24,AFF_1:59;
    then
A55: B9 // B99 by A14,A27,AFF_1:51;
A56: B99 is being_line by A54,AFF_1:def 3;
    now
A57:  a,q // x,q9 by A18,A19,A21,A20,A51,AFF_1:53;
      assume ex a,b,c st LIN a,b,c & a<>b & a<>c & b<>c;
      then consider d such that
A58:  LIN p,b,d and
A59:  d<>p and
A60:  d<>b by TRANSLAC:2;
      consider N such that
A61:  d in N and
A62:  K // N by A16,AFF_1:63;
A63:  d in B9 by A58,AFF_1:def 2;
      then
A64:  N<>M by A8,A17,A28,A49,A53,A60,A61,AFF_1:30;
A65:  not B99 // N
      proof
        assume B99 // N;
        then B9 // N by A55,AFF_1:58;
        then B9 // K by A62,AFF_1:58;
        hence contradiction by A2,A52,A50,AFF_1:59;
      end;
      N is being_line by A62,AFF_1:50;
      then consider z such that
A66:  z in B99 and
A67:  z in N by A56,A65,AFF_1:72;
A68:  d,b // z,y by A49,A47,A55,A63,A66,AFF_1:53;
A69:  N<>K by A2,A16,A28,A52,A50,A59,A63,A61,AFF_1:30;
A70:  M // N by A1,A62,AFF_1:58;
A71:  K<>N by A2,A16,A28,A52,A50,A59,A63,A61,AFF_1:30;
A72:  N // M by A1,A62,AFF_1:58;
      p,d // p9,z by A52,A48,A55,A63,A66,AFF_1:53;
      then
A73:  a,d // x,z by A1,A2,A4,A6,A7,A9,A13,A24,A61,A62,A67,A71,Lm4;
      M<>N by A8,A17,A28,A49,A53,A60,A63,A61,AFF_1:30;
      then d,q // z,q9 by A1,A3,A5,A6,A7,A9,A24,A61,A67,A73,A57,A70,
      Lm4;
      hence thesis by A3,A5,A6,A8,A10,A61,A62,A67,A68,A72,A64,A69,Lm4;
    end;
    hence thesis by A32;
  end;
  hence thesis by A1,A3,A5,A8,A10,AFF_1:53;
end;

Lm21: a,b // K & not a in K & not x in K & AFP is Moufangian & p in K & p9 in
K & p,a // p9,x & p,b // p9,y & x,y // K & q in K & q9 in K & q,a // q9,x
implies q,b // q9,y
proof
  assume that
A1: a,b // K and
A2: not a in K and
A3: not x in K and
A4: AFP is Moufangian and
A5: p in K and
A6: p9 in K and
A7: p,a // p9,x and
A8: p,b // p9,y and
A9: x,y // K and
A10: q in K and
A11: q9 in K and
A12: q,a // q9,x;
A13: K is being_line by A1,AFF_1:40;
A14: now
A15: a,q // x,q9 by A12,AFF_1:13;
A16: b,p // y,p9 by A8,AFF_1:13;
A17: a,p // x,p9 by A7,AFF_1:13;
    set A=Line(a,x);
    assume that
A18: not a,x // K and
A19: a<>b;
    a<>x by A13,A18,AFF_1:47;
    then
A20: A is being_line by AFF_1:def 3;
A21: x in A by AFF_1:26;
A22: a in A by AFF_1:26;
    then consider o such that
A23: o in A and
A24: o in K by A13,A18,A21,A20,AFF_1:54,72;
A25: LIN o,a,x by A22,A21,A20,A23,AFF_1:33;
    K is being_line by A1,AFF_1:40; then
    a,b // x,y by A1,A9,AFF_1:45;
    then LIN o,b,y by A1,A2,A4,A5,A6,A19,A24,A25,A17,A16,Lm3,AFF_1:40;
    then b,q // y,q9 by A1,A2,A4,A9,A10,A11,A13,A24,A25,A15,Lm2;
    hence thesis by AFF_1:13;
  end;
A26: now
    set M=Line(a,b);
    assume that
A27: a,x // K and
A28: a<>b and
A29: p<>q;
A30: M is being_line by A28,AFF_1:def 3; 
    K is being_line by A1,AFF_1:40; then
    a,b // a,x by A1,A27,AFF_1:45;
    then LIN a,b,x by AFF_1:def 1;
    then
A31: x in M by AFF_1:def 2;
A32: b in M by AFF_1:26;
A33: a in M by AFF_1:26;
A34: M // K by A1,A28,AFF_1:def 5;
    then x,y // M by A9,AFF_1:57;
    then y in M by A30,A31,AFF_1:37;
    hence thesis by A4,A5,A6,A7,A8,A10,A11,A12,A28,A29,A33,A32,A34,A31, Th7;
  end;
A35: a=b implies x=y
  proof
    set M=Line(x,y);
A36: x in M by AFF_1:26;
    assume a=b;
    then p9,x // p9,y by A2,A5,A7,A8,AFF_1:14;
    then LIN p9,x,y by AFF_1:def 1;
    then LIN x,y,p9 by AFF_1:15;
    then
A37: p9 in M by AFF_1:def 2;
    assume x<>y;
    then M // K by A9,AFF_1:def 5;
    hence contradiction by A3,A6,A36,A37,AFF_1:59;
  end;
  now
    assume
A38: p=q;
    p9=q9
    proof
      p9,x // q9,x by A2,A5,A7,A12,A38,AFF_1:14;
      then x,p9 // x,q9 by AFF_1:13;
      then LIN x,p9,q9 by AFF_1:def 1;
      then
A39:  LIN p9,q9,x by AFF_1:15;
      assume p9<>q9;
      hence contradiction by A1,A3,A6,A11,A39,AFF_1:39,40;
    end;
    hence thesis by A8,A38;
  end;
  hence thesis by A12,A35,A14,A26;
end;

Lm22: a,b // K & not a in K & not x in K & AFP is Moufangian & p in K & p9 in
K & p,a // p9,x & p,b // p9,y & x,y // K & q in K & q9 in K & q,a // q9,x & x,s
// K & q,b // q9,s implies s=y
proof
  assume that
A1: a,b // K and
A2: not a in K and
A3: not x in K and
A4: AFP is Moufangian and
A5: p in K and
A6: p9 in K and
A7: p,a // p9,x and
A8: p,b // p9,y and
A9: x,y // K and
A10: q in K and
A11: q9 in K and
A12: q,a // q9,x and
A13: x,s // K and
A14: q,b // q9,s;
A15: not b in K by A1,A2,AFF_1:49;
  q,b // q9,y by A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,Lm21;
  then q9,s // q9,y by A10,A14,A15,AFF_1:14;
  then LIN q9,s,y by AFF_1:def 1;
  then
A16: LIN y,s,q9 by AFF_1:15;
  assume
A17: not thesis;
  K is being_line by AFF_1:40,A1; then
   consider M such that
A18: x in M and
A19: K // M by AFF_1:63;
A20: M is being_line by A19,AFF_1:50;
  x,s // M by A13,A19,AFF_1:57;
  then
A21: s in M by A18,A20,AFF_1:37;
  x,y // M by A9,A19,AFF_1:57;
  then y in M by A18,A20,AFF_1:37;
  then M=Line(y,s) by A17,A20,A21,AFF_1:38;
  then q9 in M by A16,AFF_1:def 2;
  hence contradiction by A3,A11,A18,A19,AFF_1:59;
end;

Lm23: a,b // K & not a in K & AFP is Moufangian & ( x in K & x=y or (not x in
K & ex p,p9 st p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K)) & ( r
in K & r=y or (not r in K & ex p,p9 st p in K & p9 in K & p,a // p9,r & p,b //
p9,y & r,y // K)) implies x=r
proof
  assume that
A1: a,b // K and
A2: not a in K and
A3: AFP is Moufangian and
A4: x in K & x=y or (not x in K & ex p,p9 st p in K & p9 in K & p,a //
  p9,x & p,b // p9,y & x,y // K) and
A5: r in K & r=y or (not r in K & ex p,p9 st p in K & p9 in K & p,a //
  p9,r & p,b // p9,y & r,y // K);
A6: y in K & y=r or (not y in K & ex p,p9 st p in K & p9 in K & p,b // p9,y
  & p,a // p9,r & y,r // K) by A1,A2,A5,Lm18;
A7: b,a // K by A1,AFF_1:48;
A8: not b in K by A1,A2,AFF_1:49;
  y in K & y=x or (not y in K & ex p,p9 st p in K & p9 in K & p,b // p9,y
  & p,a // p9,x & y,x // K) by A1,A2,A4,Lm18;
  hence thesis by A3,A7,A8,A6,Lm22;
end;

Lm24: a,b // K & not a in K & AFP is Moufangian implies ex f st for x,y holds
(f.x=y iff ( x in K & x=y or (not x in K & ex p,p9 st p in K & p9 in K & p,a //
p9,x & p,b // p9,y & x,y // K)))
proof
  defpred P[Element of AFP, Element of AFP] means (($1 in K & $1=$2) or (not
$1 in K & ex p,p9 st p in K & p9 in K & p,a // p9,$1 & p,b // p9,$2 & $1,$2 //
  K));
  assume that
A1: a,b // K and
A2: not a in K and
A3: AFP is Moufangian;
A4: for x,y,s st P[x,y] & P[x,s] holds y=s by A1,A2,A3,Lm22;
A5: for y ex x st P[x,y] by A1,A2,Lm20;
A6: for x ex y st P[x,y] by A1,A2,Lm19;
A7: for x,y,r st P[x,y] & P[r,y] holds x=r by A1,A2,A3,Lm23;
  ex f st for x,y holds (f.x=y iff P[x,y]) from TRANSGEO:sch 1(A6,A5, A7,
  A4);
  hence thesis;
end;

Lm25: (a,b // K & not a in K & for x,y holds (f.x=y iff ( x in K & x=y or (not
x in K & ex p,p9 st p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K))))
implies f.a=b
proof
  assume that
A1: a,b // K and
A2: not a in K;
  consider p,q such that
A3: p in K and
  q in K and
  p<>q by A1,AFF_1:31,40;
A4: p,b // p,b by AFF_1:11;
  assume
A5: for x,y holds (f.x=y iff ( x in K & x=y or (not x in K & ex p,p9 st
  p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K)));
  p,a // p,a by AFF_1:11;
  hence thesis by A1,A2,A5,A3,A4;
end;

Lm26: (a,b // K & for x,y holds (f.x=y iff ( x in K & x=y or (not x in K & ex
p,p9 st p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K)))) implies for
x holds x,f.x // K
proof
  assume that
A1: a,b // K and
A2: for x,y holds (f.x=y iff ( x in K & x=y or (not x in K & ex p,p9 st
  p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K)));
  let x;
  set y=f.x;
A3: K is being_line by A1,AFF_1:40;
A4: now
    assume x in K;
    then y=x by A2;
    hence thesis by A3,AFF_1:47;
  end;
  now
    assume not x in K;
    then
    ex p,p9 st p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K by
    A2;
    hence thesis;
  end;
  hence thesis by A4;
end;

Lm27: (a,b // K & not a in K & for x,y holds (f.x=y iff ( x in K & x=y or (not
x in K & ex p,p9 st p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K))))
implies f is collineation
proof
  assume that
A1: a,b // K and
A2: not a in K and
A3: for x,y holds (f.x=y iff ( x in K & x=y or (not x in K & ex p,p9 st
  p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K)));
A4: K is being_line by A1,AFF_1:40;
A5: not b in K by A1,A2,AFF_1:49;
  for A st A is being_line holds f.:A is being_line
  proof
    let A such that
A6: A is being_line;
    set B9=f.:A;
A7: now
      assume that
A8:   A // K and
A9:   A<>K;
      now
        let x such that
A10:    x in A;
        consider y such that
A11:    f.y=x by TRANSGEO:2;
        y in K & y=x or (not y in K & ex p,p9 st p in K & p9 in K & p,a
        // p9,y & p,b // p9,x & y,x // K) by A3,A11;
        then x,y // K by A8,A9,A10,AFF_1:48,59;
        then x,y // A by A8,AFF_1:57;
        then y in A by A6,A10,AFF_1:37;
        hence x in B9 by A11,TRANSGEO:111;
      end;
      then
A12:  A c= B9 by SUBSET_1:7;
      now
        let y;
        assume y in B9;
        then consider x such that
A13:    x in A and
A14:    y = f.x by TRANSGEO:111;
        not x in K by A8,A9,A13,AFF_1:59;
        then ex p,p9 st p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y //
        K by A3,A14;
        then x,y // A by A8,AFF_1:57;
        hence y in A by A6,A13,AFF_1:37;
      end;
      then B9 c= A by SUBSET_1:7;
      hence thesis by A6,A12,XBOOLE_0:def 10;
    end;
A15: now
      K is being_line by A1,AFF_1:40; then
      consider M such that
A16:  a in M and
A17:  K // M by AFF_1:63;
A18:  M is being_line by A17,AFF_1:50;
      consider A9 such that
A19:  a in A9 and
A20:  A // A9 by A6,AFF_1:63;
A21:  A9 is being_line by A20,AFF_1:50;
      assume that
A22:  not A // K and
A23:  A<>K;
      consider p such that
A24:  p in A and
A25:  p in K by A4,A6,A22,AFF_1:72;
      not A9 // K by A22,A20,AFF_1:58;
      then consider q such that
A26:  q in A9 and
A27:  q in K by A4,A21,AFF_1:72;
      set C9=Line(q,b);
A28:  q in C9 by AFF_1:26;
      a,b // M by A1,A17,AFF_1:57;
      then b in M by A16,A18,AFF_1:37;
      then q<>b by A2,A16,A17,A27,AFF_1:59;
      then C9 is being_line by AFF_1:def 3;
      then consider C such that
A29:  p in C and
A30:  C9 // C by AFF_1:63;
A31:  b in C9 by AFF_1:26;
A32:  C is being_line by A30,AFF_1:50;
      now
        let y such that
A33:    y in C;
A34:    now
A35:      q,b // p,y by A28,A31,A29,A30,A33,AFF_1:53;
          K is being_line by A1,AFF_1:40; then
          consider N such that
A36:      y in N and
A37:      K // N by AFF_1:63;
A38:      N is being_line by A37,AFF_1:50;
          not N // A by A22,A37,AFF_1:58;
          then consider x such that
A39:      x in N and
A40:      x in A by A6,A38,AFF_1:72;
A41:      x,y // K by A36,A37,A39,AFF_1:54;
          assume
A42:      y<>p;
A43:      not x in K
          proof
            assume x in K;
            then y in K by A36,A37,A39,AFF_1:59;
            then C9 // K by A4,A25,A29,A30,A32,A33,A42,AFF_1:30;
            hence contradiction by A5,A27,A28,A31,AFF_1:59;
          end;
          q,a // p,x by A24,A19,A20,A26,A40,AFF_1:53;
          then y=f.x by A3,A25,A27,A43,A35,A41;
          hence y in B9 by A40,TRANSGEO:111;
        end;
        now
          assume
A44:      y=p;
          f.p=p by A3,A25;
          hence y in B9 by A24,A44,TRANSGEO:111;
        end;
        hence y in B9 by A34;
      end;
      then
A45:  C c= B9 by SUBSET_1:7;
      now
        let y;
        assume y in B9;
        then consider x such that
A46:    x in A and
A47:    y=f.x by TRANSGEO:111;
        now
          K is being_line by A1,AFF_1:40; then
          consider N such that
A48:      x in N and
A49:      K // N by AFF_1:63;
A50:      not C // N
          proof
            assume C // N;
            then C9 // N by A30,AFF_1:58;
            then
A51:        C9 // K by A49,AFF_1:58;
            q in C9 by AFF_1:26;
            then C9=K by A27,A51,AFF_1:59;
            hence contradiction by A5,AFF_1:26;
          end;
          N is being_line by A49,AFF_1:50;
          then consider z such that
A52:      z in C and
A53:      z in N by A32,A50,AFF_1:72;
A54:      x,z // K by A48,A49,A53,AFF_1:54;
          assume x<>p;
          then
A55:      not x in K by A4,A6,A23,A24,A25,A46,AFF_1:30;
A56:      q,a // p,x by A24,A19,A20,A26,A46,AFF_1:53;
          q,b // p,z by A28,A31,A29,A30,A52,AFF_1:53;
          hence y in C by A3,A25,A27,A47,A55,A52,A56,A54;
        end;
        hence y in C by A3,A25,A29,A47;
      end;
      then B9 c= C by SUBSET_1:7;
      hence thesis by A32,A45,XBOOLE_0:def 10;
    end;
    now
      assume
A57:  A=K;
      now
        let y;
        assume y in B9;
        then ex x st x in A & y=f.x by TRANSGEO:111;
        hence y in A by A3,A57;
      end;
      then
A58:  B9 c= A by SUBSET_1:7;
      now
        let x such that
A59:    x in A;
        set y=f.x;
        y in B9 by A59,TRANSGEO:110;
        hence x in B9 by A3,A57,A59;
      end;
      then A c= B9 by SUBSET_1:7;
      hence thesis by A6,A58,XBOOLE_0:def 10;
    end;
    hence thesis by A7,A15;
  end;
  hence thesis by TRANSGEO:116;
end;

Lm28: (a,b // K & not a in K & for x,y holds (f.x=y iff ( x in K & x=y or (not
x in K & ex p,p9 st p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K))))
implies f is_Sc K
proof
  assume that
A1: a,b // K and
A2: not a in K and
A3: for x,y holds (f.x=y iff ( x in K & x=y or (not x in K & ex p,p9 st
  p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K)));
A4: for x holds x,f.x // K by A1,A3,Lm26;
A5: for x st x in K holds f.x=x by A3;
A6: K is being_line by A1,AFF_1:40;
  f is collineation by A1,A2,A3,Lm27;
  hence thesis by A6,A5,A4,Def1;
end;

theorem Th8:
  a,b // K & not a in K & AFP is Moufangian implies ex f st f is_Sc K & f.a=b
proof
  assume that
A1: a,b // K and
A2: not a in K and
A3: AFP is Moufangian;
  consider f such that
A4: for x,y holds (f.x=y iff ( x in K & x=y or (not x in K & ex p,p9 st
  p in K & p9 in K & p,a // p9,x & p,b // p9,y & x,y // K))) by A1,A2,A3,Lm24;
A5: f.a=b by A1,A2,A4,Lm25;
  f is_Sc K by A1,A2,A4,Lm28;
  hence thesis by A5;
end;

theorem
  AFP is Moufangian iff for a,b,K st a,b // K & not a in K ex f st f
  is_Sc K & f.a=b by Th6,Th8;
