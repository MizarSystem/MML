environ

 vocabularies DIRAF, SUBSET_1, FUNCT_2, STRUCT_0, AFF_2, ANALOAF, INCSP_1,
      AFF_1, TRANSGEO, FUNCT_1, RELAT_1, TARSKI, HOMOTHET;
 notations TARSKI, RELSET_1, PARTFUN1, FUNCT_2, STRUCT_0, ANALOAF, DIRAF,
      AFF_1, AFF_2, TRANSGEO;
 constructors PARTFUN1, AFF_1, AFF_2, TRANSGEO, RELSET_1;
 registrations STRUCT_0;
 requirements SUBSET, BOOLE;
 theorems TRANSGEO, AFF_1, AFF_2, TRANSLAC, SUBSET_1, DIRAF, FUNCT_1, XBOOLE_0;
 schemes TRANSGEO;

begin

reserve AFP for AffinPlane;
reserve a,a9,b,b9,c,c9,d,d9,o,p,p9,q,q9,r,s,t,x,y,z for Element of AFP;
reserve A,A9,C,D,P,B9,M,N,K for Subset of AFP;
reserve f for Permutation of the carrier of AFP;

Lm1: AFP is Desarguesian iff for o,a,a9,p,p9,q,q9 st LIN o,a,a9 & LIN o,p,p9 &
LIN o,q,q9 & not LIN o,a,p & not LIN o,a,q & a,p // a9,p9 & a,q // a9,q9 holds
p,q // p9,q9
proof
  thus AFP is Desarguesian implies for o,a,a9,p,p9,q,q9 st LIN o,a,a9 & LIN o,
p,p9 & LIN o,q,q9 & not LIN o,a,p & not LIN o,a,q & a,p // a9,p9 & a,q // a9,q9
  holds p,q // p9,q9
  proof
    assume
A1: AFP is Desarguesian;
    let o,a,a9,p,p9,q,q9;
    assume that
A2: LIN o,a,a9 and
A3: LIN o,p,p9 and
A4: LIN o,q,q9 and
A5: not LIN o,a,p and
A6: not LIN o,a,q and
A7: a,p // a9,p9 and
A8: a,q // a9,q9;
    set A=Line(o,a), P=Line(o,p), C=Line(o,q);
A9: a in A by AFF_1:26;
A10: o<>a by A5,AFF_1:16;
    then
A11: A is being_line by AFF_1:def 3;
A12: o in A by AFF_1:26;
    then
A13: a9 in A by A2,A10,A11,A9,AFF_1:39;
A14: q in C by AFF_1:26;
    then
A15: A<>C by A6,A11,A12,A9,AFF_1:33;
A16: o in P by AFF_1:26;
A17: p in P by AFF_1:26;
A18: o<>p by A5,AFF_1:16;
    then
A19: P is being_line by AFF_1:def 3;
    then
A20: p9 in P by A3,A18,A16,A17,AFF_1:39;
A21: o in C by AFF_1:26;
A22: o<>q by A6,AFF_1:16;
    then
A23: C is being_line by AFF_1:def 3;
    then
A24: q9 in C by A4,A22,A21,A14,AFF_1:39;
    A<>P by A5,A11,A12,A9,A17,AFF_1:33;
    hence thesis by A1,A7,A8,A10,A18,A22,A11,A19,A23,A12,A9,A16,A17,A21,A14,
    A13,A20,A24,A15,AFF_2:def 4;
  end;
  assume
A25: for o,a,a9,p,p9,q,q9 st LIN o,a,a9 & LIN o,p,p9 & LIN o,q,q9 & not
  LIN o,a,p & not LIN o,a,q & a,p // a9,p9 & a,q // a9,q9 holds p,q // p9,q9;
  now
    let A,P,C,o,a,b,c,a9,b9,c9;
    assume that
A26: o in A and
A27: o in P and
A28: o in C and
A29: o<>a and
A30: o<>b and
A31: o<>c and
A32: a in A and
A33: a9 in A and
A34: b in P and
A35: b9 in P and
A36: c in C and
A37: c9 in C and
A38: A is being_line and
A39: P is being_line and
A40: C is being_line and
A41: A<>P and
A42: A<>C and
A43: a,b // a9,b9 and
A44: a,c // a9,c9;
A45: LIN o,b,b9 by A27,A34,A35,A39,AFF_1:33;
A46: not LIN o,a,c
    proof
      assume LIN o,a,c;
      then c in A by A26,A29,A32,A38,AFF_1:39;
      hence contradiction by A26,A28,A31,A36,A38,A40,A42,AFF_1:30;
    end;
A47: not LIN o,a,b
    proof
      assume LIN o,a,b;
      then b in A by A26,A29,A32,A38,AFF_1:39;
      hence contradiction by A26,A27,A30,A34,A38,A39,A41,AFF_1:30;
    end;
A48: LIN o,c,c9 by A28,A36,A37,A40,AFF_1:33;
    LIN o,a,a9 by A26,A32,A33,A38,AFF_1:33;
    hence b,c // b9,c9 by A25,A43,A44,A45,A48,A47,A46;
  end;
  hence thesis by AFF_2:def 4;
end;

Lm2: AFP is Moufangian iff for o,K,c,c9,a,b,a9,b9 st o in K & c in K & c9 in K
& K is being_line & LIN o,a,a9 & LIN o,b,b9 & not a in K & a,b // K & a9,b9 //
K & a,c // a9,c9 holds b,c // b9,c9
proof
  thus AFP is Moufangian implies for o,K,c,c9,a,b,a9,b9 st o in K & c in K &
  c9 in K & K is being_line & LIN o,a,a9 & LIN o,b,b9 & not a in K & a,b // K &
  a9,b9 // K & a,c // a9,c9 holds b,c // b9,c9
  proof
    assume
A1: AFP is Moufangian;
    let o,K,c,c9,a,b,a9,b9;
    assume that
A2: o in K and
A3: c in K and
A4: c9 in K and
A5: K is being_line and
A6: LIN o,a,a9 and
A7: LIN o,b,b9 and
A8: not a in K and
A9: a,b // K and
A10: a9,b9 // K and
A11: a,c // a9,c9;
A12: a,b // a9,b9 by A5,A9,A10,AFF_1:45;
A13: now
A14:  now
        assume
A15:    a=b;
A16:    now
A17:      not o,a // K by A2,A8,AFF_1:48,49;
A18:      LIN o,b9,b by A7,AFF_1:15;
          assume that
A19:      a9,o // K and
A20:      a9<>b9;
          o,a // o,a9 by A6,AFF_1:def 1;
          then a9,o // o,a by AFF_1:13;
          then
A21:      o,a // K or a9=o by A19,AFF_1:46;
          then b9 in K by A2,A5,A8,A10,AFF_1:37;
          hence contradiction by A2,A5,A8,A15,A20,A21,A17,A18,AFF_1:39;
        end;
        LIN o,a,o by AFF_1:16;
        then LIN a9,b9,o by A2,A6,A7,A8,A15,AFF_1:17;
        then a9,b9 // a9,o by AFF_1:def 1;
        hence thesis by A10,A11,A15,A16,AFF_1:46;
      end;
      assume o<>c;
      hence thesis by A1,A2,A3,A4,A5,A6,A7,A8,A9,A11,A12,A14, AFF_2:def 7;
    end;
    now
      assume
A22:  o=c;
      then LIN b,b9,c by A7,AFF_1:15;
      then
A23:  b,b9 // b,c by AFF_1:def 1;
      LIN a,c,a9 by A6,A22,AFF_1:15;
      then LIN a,c,c9 by A3,A8,A11,AFF_1:18;
      then
A24:  LIN c,c9,a by AFF_1:15;
      then LIN c9,b,b9 by A2,A4,A5,A7,A8,A22,AFF_1:39;
      then LIN b9,b,c9 by AFF_1:15;
      then b9,b // b9,c9 by AFF_1:def 1;
      then
A25:  b,b9 // b9,c9 by AFF_1:13;
      c = c9 by A3,A4,A5,A8,A24,AFF_1:39;
      then b=b9 implies thesis by AFF_1:11;
      hence thesis by A23,A25,AFF_1:14;
    end;
    hence thesis by A13;
  end;
  assume
A26: for o,K,c,c9,a,b,a9,b9 st o in K & c in K & c9 in K & K is
being_line & LIN o,a,a9 & LIN o,b,b9 & not a in K & a,b // K & a9,b9 // K & a,c
  // a9,c9 holds b,c // b9,c9;
  now
    let K,o,a,b,c,a9,b9,c9;
    assume that
A27: K is being_line and
A28: o in K and
A29: c in K and
A30: c9 in K and
A31: not a in K and
    o<>c and
A32: a<>b and
A33: LIN o,a,a9 and
A34: LIN o,b,b9 and
A35: a,b // a9,b9 and
A36: a,c // a9,c9 and
A37: a,b // K;
    a9,b9 // K by A32,A35,A37,AFF_1:46;
    hence b,c // b9,c9 by A26,A27,A28,A29,A30,A31,A33,A34,A36,A37;
  end;
  hence thesis by AFF_2:def 7;
end;

Lm3: AFP is Moufangian implies for K,o,a,b,c,a9,b9,c9 st K is being_line & o
in K & c in K & c9 in K & not a in K & a<>b & LIN o,a,a9 & a,b // a9,b9 & b,c
// b9,c9 & a,c // a9,c9 & a,b // K holds LIN o,b,b9
proof
  assume
A1: AFP is Moufangian;
  thus for K,o,a,b,c,a9,b9,c9 st K is being_line & o in K & c in K & c9 in K &
not a in K & a<>b & LIN o,a,a9 & a,b // a9,b9 & b,c // b9,c9 & a,c // a9,c9 & a
  ,b // K holds LIN o,b,b9
  proof
    let K,o,a,b,c,a9,b9,c9;
    assume that
A2: K is being_line and
A3: o in K and
A4: c in K and
A5: c9 in K and
A6: not a in K and
A7: a<>b and
A8: LIN o,a,a9 and
A9: a,b // a9,b9 and
A10: b,c // b9,c9 and
A11: a,c // a9,c9 and
A12: a,b // K;
    consider P such that
A13: a9 in P and
A14: K // P by A2,AFF_1:63;
A15: P is being_line by A14,AFF_1:50;
    set A=Line(o,b), C=Line(o,a);
A16: o in A by AFF_1:26;
A17: b in A by AFF_1:26;
    assume
A18: not LIN o,b,b9;
    then o<>b by AFF_1:16;
    then
A19: A is being_line by AFF_1:def 3;
A20: not b in K by A6,A12,AFF_1:49;
    not A // P
    proof
      assume A // P;
      then A // K by A14,AFF_1:58;
      hence contradiction by A3,A20,A16,A17,AFF_1:59;
    end;
    then consider x such that
A21: x in A and
A22: x in P by A19,A15,AFF_1:72;
A23: o in C by AFF_1:26;
    a,b // P by A12,A14,AFF_1:57;
    then a9,b9 // P by A7,A9,AFF_1:46;
    then
A24: b9 in P by A13,A15,AFF_1:37;
    then
A25: LIN b9,x,b9 by A15,A22,AFF_1:33;
A26: a in C by AFF_1:26;
A27: LIN o,b,x by A19,A16,A17,A21,AFF_1:33;
A28: C is being_line by A3,A6,AFF_1:def 3;
    then
A29: a9 in C by A3,A6,A8,A23,A26,AFF_1:39;
A30: b9<>c9
    proof
A31:  a9,c9 // c,a by A11,AFF_1:13;
      assume
A32:  b9=c9;
      then P=K by A5,A14,A24,AFF_1:59;
      then a9=o by A2,A3,A6,A28,A23,A26,A29,A13,AFF_1:30;
      then b9=o by A2,A3,A4,A5,A6,A32,A31,AFF_1:62;
      hence contradiction by A18,AFF_1:16;
    end;
A33: a9<>b9
    proof
      assume
A34:  a9=b9;
A35:  now
        assume a9=c9;
        then b9=o by A2,A3,A5,A6,A28,A23,A26,A29,A34,AFF_1:30;
        hence contradiction by A18,AFF_1:16;
      end;
      a,c // b,c or a9=c9 by A10,A11,A34,AFF_1:14;
      then c,a // c,b by A35,AFF_1:13;
      then LIN c,a,b by AFF_1:def 1;
      then LIN a,c,b by AFF_1:15;
      then a,c // a,b by AFF_1:def 1;
      then a,b // a,c by AFF_1:13;
      then a,c // K by A7,A12,AFF_1:46;
      then c,a // K by AFF_1:48;
      hence contradiction by A2,A4,A6,AFF_1:37;
    end;
A36: b<>c by A4,A6,A12,AFF_1:49;
    a9,x // K by A13,A14,A22,AFF_1:54;
    then b,c // x,c9 by A1,A2,A3,A4,A5,A6,A8,A11,A12,A27,Lm2;
    then b9,c9 // x,c9 by A10,A36,AFF_1:14;
    then c9,b9 // c9,x by AFF_1:13;
    then LIN c9,b9,x by AFF_1:def 1;
    then
A37: LIN b9,x,c9 by AFF_1:15;
    LIN b9,x,a9 by A13,A15,A22,A24,AFF_1:33;
    then LIN b9,c9,a9 by A18,A27,A37,A25,AFF_1:17;
    then b9,c9 // b9,a9 by AFF_1:def 1;
    then b9,c9 // a9,b9 by AFF_1:13;
    then b,c // a9,b9 by A10,A30,AFF_1:14;
    then a,b // b,c by A9,A33,AFF_1:14;
    then b,c // K by A7,A12,AFF_1:46;
    then c,b // K by AFF_1:48;
    hence contradiction by A2,A4,A20,AFF_1:37;
  end;
end;

Lm4: AFP is Moufangian implies for K,A,C,p,q,a,a9,b,b9 st K // A & K // C & K
<>A & K<>C & p in K & q in K & a in A & a9 in A & b in C & b9 in C & p,a // q,
a9 & p,b // q,b9 holds a,b // a9,b9
proof
  assume AFP is Moufangian;
  then
A1: AFP is translational by AFF_2:28;
  let K,A,C,p,q,a,a9,b,b9;
  assume that
A2: K // A and
A3: K // C and
A4: K<>A and
A5: K<>C and
A6: p in K and
A7: q in K and
A8: a in A and
A9: a9 in A and
A10: b in C and
A11: b9 in C and
A12: p,a // q,a9 and
A13: p,b // q,b9;
A14: A is being_line by A2,AFF_1:50;
A15: C is being_line by A3,AFF_1:50;
  K is being_line by A2,AFF_1:50;
  hence thesis by A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,A13,A1,A14,A15,
  AFF_2:def 11;
end;

theorem Th1:
  not LIN o,a,p & LIN o,a,b & LIN o,a,x & LIN o,a,y & LIN o,p,p9 &
  LIN o,p,q & LIN o,p,q9 & p<>q & o<>q & o<>x & a,p // b,p9 & a,q // b,q9 & x,p
  // y,p9 & AFP is Desarguesian implies x,q // y,q9
proof
  assume that
A1: not LIN o,a,p and
A2: LIN o,a,b and
A3: LIN o,a,x and
A4: LIN o,a,y and
A5: LIN o,p,p9 and
A6: LIN o,p,q and
A7: LIN o,p,q9 and
A8: p<>q and
A9: o<>q and
A10: o<>x and
A11: a,p // b,p9 and
A12: a,q // b,q9 and
A13: x,p // y,p9 and
A14: AFP is Desarguesian;
  set B9=Line(o,p);
A15: o in B9 by AFF_1:26;
A16: o<>p by A1,AFF_1:16;
  then consider d such that
A17: LIN x,p,d and
A18: d<>x and
A19: d<>p by A6,A8,A9,TRANSLAC:2;
A20: B9 is being_line by A16,AFF_1:def 3;
A21: q9 in B9 by A7,AFF_1:def 2;
  q in B9 by A6,AFF_1:def 2;
  then
A22: LIN o,q,q9 by A20,A15,A21,AFF_1:33;
  set A=Line(o,a);
  o<>a by A1,AFF_1:16;
  then
A23: A is being_line by AFF_1:def 3;
A24: y in A by A4,AFF_1:def 2;
A25: p,a // p9,b by A11,AFF_1:13;
A26: not LIN o,p,a by A1,AFF_1:15;
  set K=Line(x,p);
A27: K is being_line by A1,A3,AFF_1:def 3;
  then consider M such that
A28: y in M and
A29: K // M by AFF_1:63;
A30: x in K by AFF_1:26;
A31: M is being_line by A29,AFF_1:50;
A32: p in K by AFF_1:26;
A33: d in K by A17,AFF_1:def 2;
A34: x in A by A3,AFF_1:def 2;
A35: not LIN o,a,d
  proof
    assume LIN o,a,d;
    then d in A by AFF_1:def 2;
    then A=K by A18,A27,A30,A33,A23,A34,AFF_1:30;
    hence contradiction by A1,A32,AFF_1:def 2;
  end;
A36: o in A by AFF_1:26;
  not o,d // M
  proof
    assume o,d // M;
    then o,d // K by A29,AFF_1:57;
    then d,o // K by AFF_1:48;
    then o in K by A27,A33,AFF_1:37;
    then A=K by A10,A27,A30,A23,A36,A34,AFF_1:30;
    hence contradiction by A1,A32,AFF_1:def 2;
  end;
  then consider d9 such that
A37: d9 in M and
A38: LIN o,d,d9 by A31,AFF_1:73;
A39: d,x // d9,y by A30,A33,A28,A29,A37,AFF_1:53;
A40: p in B9 by AFF_1:26;
A41: not LIN o,d,q
  proof
    assume LIN o,d,q;
    then
A42: LIN o,q,d by AFF_1:15;
A43: LIN o,q,o by AFF_1:16;
    LIN o,q,p by A6,AFF_1:15;
    then LIN o,p,d by A9,A43,A42,AFF_1:17;
    then d in B9 by AFF_1:def 2;
    then B9=K by A19,A27,A32,A33,A20,A40,AFF_1:30;
    then A=B9 by A10,A27,A30,A23,A36,A34,A15,AFF_1:30;
    hence contradiction by A1,A40,AFF_1:def 2;
  end;
A44: not LIN o,d,x
  proof
    assume LIN o,d,x;
    then LIN o,x,d by AFF_1:15;
    then d in A by A10,A23,A36,A34,AFF_1:39;
    then A=K by A18,A27,A30,A33,A23,A34,AFF_1:30;
    hence contradiction by A1,A32,AFF_1:def 2;
  end;
A45: not LIN o,p,d
  proof
    assume LIN o,p,d;
    then d in B9 by AFF_1:def 2;
    then K=B9 by A19,A27,A32,A33,A20,A40,AFF_1:30;
    hence contradiction by A27,A30,A33,A15,A44,AFF_1:33;
  end;
A46: q in B9 by A6,AFF_1:def 2;
A47: not LIN o,a,q
  proof
    assume LIN o,a,q;
    then q in A by AFF_1:def 2;
    then A=B9 by A9,A23,A36,A20,A15,A46,AFF_1:30;
    hence contradiction by A1,A40,AFF_1:def 2;
  end;
  x in A by A3,AFF_1:def 2;
  then
A48: LIN o,x,y by A23,A36,A24,AFF_1:33;
  x,p // K by A27,A30,A32,AFF_1:37;
  then x,p // M by A29,AFF_1:57;
  then y,p9 // M by A1,A3,A13,AFF_1:46;
  then p9 in M by A28,A31,AFF_1:37;
  then p,d // p9,d9 by A32,A33,A29,A37,AFF_1:53;
  then a,d // b,d9 by A2,A5,A14,A38,A45,A26,A25,Lm1;
  then d,q // d9,q9 by A2,A12,A14,A38,A47,A35,A22,Lm1;
  hence thesis by A14,A38,A39,A41,A44,A22,A48,Lm1;
end;

theorem Th2:
  (for o,a,b st o<>a & o<>b & LIN o,a,b ex f st f is dilatation & f
  .o=o & f.a=b) implies AFP is Desarguesian
proof
  assume
A1: for o,a,b st o<>a & o<>b & LIN o,a,b ex f st f is dilatation & f.o=o
  & f.a=b;
  now
    let o,a,a9,p,p9,q,q9;
    assume that
A2: LIN o,a,a9 and
A3: LIN o,p,p9 and
A4: LIN o,q,q9 and
A5: not LIN o,a,p and
A6: not LIN o,a,q and
A7: a,p // a9,p9 and
A8: a,q // a9,q9;
A9: now
      assume
A10:  o<>a9;
      o<>a by A5,AFF_1:16;
      then consider f such that
A11:  f is dilatation and
A12:  f.o=o and
A13:  f.a=a9 by A1,A2,A10;
      set s=f.q;
      o,q // o,s by A11,A12,TRANSGEO:85;
      then
A14:  LIN o,q,s by AFF_1:def 1;
      set r=f.p;
      o,p // o,r by A11,A12,TRANSGEO:85;
      then
A15:  LIN o,p,r by AFF_1:def 1;
      a,q // a9,s by A11,A13,TRANSGEO:85;
      then
A16:  s=q9 by A2,A4,A6,A8,A14,AFF_1:70;
      a,p // a9,r by A11,A13,TRANSGEO:85;
