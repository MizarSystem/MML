  end;
  now
    assume
A23: not LIN o,a,x;
    then
A24: not LIN o,a,y by A1,A2,A3,A4,Lm5;
    not LIN o,b,y
    proof
A25:  LIN o,b,o by AFF_1:16;
      assume
A26:  LIN o,b,y;
      LIN o,b,a by A3,AFF_1:15;
      hence contradiction by A2,A24,A26,A25,AFF_1:17;
    end;
    hence thesis by A1,A2,A3,A4,A23,AFF_1:13,15;
  end;
  hence thesis by A6;
