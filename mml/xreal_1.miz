environ

 vocabularies ARYTM_1, ARYTM, ARYTM_3, RELAT_1, ARYTM_2, BOOLE, COMPLEX1,
      ORDINAL2, FUNCOP_1, OPPCAT_1, ZF_LANG, ASYMPT_0, XCMPLX_0;
 notations TARSKI, XBOOLE_0, ZFMISC_1, SUBSET_1, ORDINAL1, ORDINAL2, FUNCT_4,
      ARYTM_2, ARYTM_1, ARYTM_0, XCMPLX_0, XREAL_0, NUMBERS, XXREAL_0;
 constructors XCMPLX_0, XREAL_0, SUBSET_1, NUMBERS, ARYTM_3, ARYTM_0, ORDINAL3,
      MCART_1, ENUMSET1, DOMAIN_1, ARYTM_2, FUNCT_4, ARYTM_1, XXREAL_0;
 registrations XREAL_0, ARYTM_2, ARYTM_3, ORDINAL2, NUMBERS, XCMPLX_0,
      ZFMISC_1, XBOOLE_0, XXREAL_0;
 requirements BOOLE, SUBSET, NUMERALS, ARITHM, REAL;
 definitions XXREAL_0;
 theorems TARSKI, ARYTM_2, ARYTM_1, ARYTM_0, XBOOLE_0, XCMPLX_0, XCMPLX_1,
      XREAL_0, ZFMISC_1, XXREAL_0;

begin

 reserve a,b,c,d for real number;

reserve r,s for real number;

Def2:
 r <= s implies
  (r in REAL+ & s in REAL+ implies
   ex x',y' being Element of REAL+ st r = x' & s = y' & x' <=' y') &
  (r in [:{0},REAL+:] & s in [:{0},REAL+:] implies
  ex x',y' being Element of REAL+ st r = [0,x'] & s = [0,y'] & y' <=' x') &
  (not(r in REAL+ & s in REAL+) & not (r in [:{0},REAL+:] & s in [:{0},REAL+:])
    implies s in REAL+ & r in [:{0},REAL+:])
    by XXREAL_0:def 5;

Def2':
  (r in REAL+ & s in REAL+ &
   ex x',y' being Element of REAL+ st r = x' & s = y' & x' <=' y') or
  (r in [:{0},REAL+:] & s in [:{0},REAL+:] &
  ex x',y' being Element of REAL+ st r = [0,x'] & s = [0,y'] & y' <=' x') or
   s in REAL+ & r in [:{0},REAL+:]
  implies r <= s
proof assume
Z: (r in REAL+ & s in REAL+ &
   ex x',y' being Element of REAL+ st r = x' & s = y' & x' <=' y') or
  (r in [:{0},REAL+:] & s in [:{0},REAL+:] &
  ex x',y' being Element of REAL+ st r = [0,x'] & s = [0,y'] & y' <=' x') or
  ( s in REAL+ & r in [:{0},REAL+:]);
 per cases;
 case r in REAL+ & s in REAL+;
   then not r in [:{0},REAL+:] & not s in [:{0},REAL+:]
          by ARYTM_0:5,XBOOLE_0:3;
  hence ex x',y' being Element of REAL+ st r = x' & s = y' & x' <=' y' by Z;
 end;
 case r in [:{0},REAL+:] & s in [:{0},REAL+:];
   then not r in REAL+ & not s in REAL+ by ARYTM_0:5,XBOOLE_0:3;
  hence ex x',y' being Element of REAL+ st r = [0,x'] & s = [0,y'] & y' <=' x'
   by Z;
 end;
 case not(r in REAL+ & s in REAL+) &
   not (r in [:{0},REAL+:] & s in [:{0},REAL+:]);
  hence thesis by Z;
 end;
end;

theorem Th1: :: AXIOMS:21
 a <= b & b <= a implies a = b by XXREAL_0:1;

theorem Th2: :: AXIOMS:22
 a <= b & b <= c implies a <= c by XXREAL_0:2;

Lm1: for x1,x2 being Element of REAL st a = [*x1,x2*]
       holds x2 = 0 & a = x1
 proof let x1,x2 being Element of REAL;
  assume
A1:  a = [*x1,x2*];
A2: a in REAL by XREAL_0:def 1;
  hereby assume x2 <> 0;
    then a = (0,one) --> (x1,x2) by A1,ARYTM_0:def 7;
   hence contradiction by A2,ARYTM_0:10;
  end;
  hence a = x1 by A1,ARYTM_0:def 7;
 end;

Lm2:
 for a',b' being Element of REAL, a,b st a' = a & b' = b
  holds +(a',b') = a + b
proof let a',b' be Element of REAL, a,b such that
A1: a' = a & b' = b;
  consider x1,x2,y1,y2 being Element of REAL such that
A2: a = [* x1,x2 *] and
A3: b = [*y1,y2*] and
A4: a+b = [*+(x1,y1),+(x2,y2)*] by XCMPLX_0:def 4;
A5: a = x1 & b = y1 by A2,A3,Lm1;
     x2 = 0 & y2 = 0 by A2,A3,Lm1;
  then +(x2,y2) = 0 by ARYTM_0:13;
 hence +(a',b') = a + b by A1,A4,A5,ARYTM_0:def 7;
end;

Lm3: {} in {{}} by TARSKI:def 1;

Lm4: :: AXIOMS:24
 a <= b implies a + c <= b + c
proof
 assume
A1: a <= b;
  reconsider x1=a, y1=b, z1=c as Element of REAL by XREAL_0:def 1;
A2: +(y1,z1) = b + c & +(x1,z1) = a + c by Lm2;
 per cases by A1,Def2;
 suppose that
A3: a in REAL+ and
A4: b in REAL+ and
A5: c in REAL+;
  consider a'',b'' being Element of REAL+ such that
A6: a = a'' and
A7: b = b'' and
A8: a'' <=' b'' by A1,A3,A4,Def2;
  consider a',c' being Element of REAL+ such that
A9: a = a' and
A10: c = c' and
A11: +(x1,z1) = a' + c' by A3,A5,ARYTM_0:def 2;
  consider b',c'' being Element of REAL+ such that
A12: b = b' and
A13: c = c'' and
A14: +(y1,z1) = b' + c'' by A4,A5,ARYTM_0:def 2;
      a' + c' <=' b' + c'' by A6,A7,A8,A9,A10,A12,A13,ARYTM_1:7;
 hence a + c <= b + c by A2,A11,A14,Def2';
 end;
 suppose that
A15: a in [:{0},REAL+:] and
A16: b in REAL+ and
A17: c in REAL+;
  consider a',c' being Element of REAL+ such that
       a = [0,a'] and
A18: c = c' and
A19: +(x1,z1) = c' - a' by A15,A17,ARYTM_0:def 2;
  consider b',c'' being Element of REAL+ such that
       b = b' and
A20: c = c'' and
A21: +(y1,z1) = b' + c'' by A16,A17,ARYTM_0:def 2;
     now per cases;
   suppose a' <=' c';
    then A22:   c' - a' = c' -' a' by ARYTM_1:def 2;
A23:  c' -' a' <=' c' by ARYTM_1:11;
       c' <=' b' + c'' by A18,A20,ARYTM_2:20;
    then c' -' a' <=' b' + c'' by A23,ARYTM_1:3;
   hence a + c <= b + c by A2,A19,A21,A22,Def2';
   end;
   suppose not a' <=' c';
    then c' - a' = [0,a' -' c'] by ARYTM_1:def 2;
    then c' - a' in [:{0},REAL+:] by Lm3,ZFMISC_1:106;
    then not a + c in REAL+ & not b + c in [:{0},REAL+:]
                 by A2,A19,A21,ARYTM_0:5,XBOOLE_0:3;
   hence a + c <= b + c by Def2;
  end;
  end;
 hence thesis;
 end;
 suppose that
A24: a in [:{0},REAL+:] and
A25: b in [:{0},REAL+:] and
A26: c in REAL+;
  consider a'',b'' being Element of REAL+ such that
A27: a = [0,a''] and
A28: b = [0,b''] and
A29: b'' <=' a'' by A1,A24,A25,Def2;
  consider a',c' being Element of REAL+ such that
A30: a = [0,a'] and
A31: c = c' and
A32: +(x1,z1) = c' - a' by A24,A26,ARYTM_0:def 2;
  consider b',c'' being Element of REAL+ such that
A33: b = [0,b'] and
A34: c = c'' and
A35: +(y1,z1) = c'' - b' by A25,A26,ARYTM_0:def 2;
A36: a' = a'' by A27,A30,ZFMISC_1:33;
A37: b' = b'' by A28,A33,ZFMISC_1:33;
     now per cases;
   suppose
A38:  a' <=' c';
then A39:   c' - a' = c' -' a' by ARYTM_1:def 2;
       b' <=' c' by A29,A36,A37,A38,ARYTM_1:3;
then A40:   c' - b' = c' -' b' by ARYTM_1:def 2;
       c' -' a' <=' c'' -' b' by A29,A31,A34,A36,A37,ARYTM_1:16;
   hence a + c <= b + c by A2,A31,A32,A34,A35,A39,A40,Def2';
   end;
   suppose not a' <=' c';
    then A41:  +(x1,z1) = [0,a' -' c'] by A32,ARYTM_1:def 2;
    then A42:   +(x1,z1) in [:{0},REAL+:] by Lm3,ZFMISC_1:106;
      now per cases;
    suppose b' <=' c';
     then c' - b' = c' -' b' by ARYTM_1:def 2;
     then not +(x1,z1) in REAL+ & not +(y1,z1) in [:{0},REAL+:]
                 by A31,A34,A35,A42,ARYTM_0:5,XBOOLE_0:3;
    hence a + c <= b + c by A2,Def2;
    end;
    suppose not b' <=' c';
     then A43:  +(y1,z1) = [0,b' -' c'] by A31,A34,A35,ARYTM_1:def 2;
    then A44:   +(y1,z1) in [:{0},REAL+:] by Lm3,ZFMISC_1:106;
        b' -' c' <=' a' -' c' by A29,A36,A37,ARYTM_1:17;
    hence a + c <= b + c by A2,A41,A42,A43,A44,Def2';
   end;
   end;
   hence thesis;
  end;
  end;
 hence thesis;
 end;
 suppose that
A45: a in REAL+ and
A46: b in REAL+ and
A47: c in [:{0},REAL+:];
  consider a'',b'' being Element of REAL+ such that
A48: a = a'' and
A49: b = b'' and
A50: a'' <=' b'' by A1,A45,A46,Def2;
  consider a',c' being Element of REAL+ such that
A51: a = a' and
A52: c = [0,c'] and
A53: +(x1,z1) = a' - c' by A45,A47,ARYTM_0:def 2;
  consider b',c'' being Element of REAL+ such that
A54: b = b' and
A55: c = [0,c''] and
A56: +(y1,z1) = b' - c'' by A46,A47,ARYTM_0:def 2;
A57: c' = c'' by A52,A55,ZFMISC_1:33;
     now per cases;
   suppose
A58:  c' <=' a';
then A59:   a' - c' = a' -' c' by ARYTM_1:def 2;
       c' <=' b' by A48,A49,A50,A51,A54,A58,ARYTM_1:3;
then A60:   b' - c' = b' -' c' by ARYTM_1:def 2;
       a' -' c' <=' b' -' c'' by A48,A49,A50,A51,A54,A57,ARYTM_1:17;
   hence a + c <= b + c by A2,A53,A56,A57,A59,A60,Def2';
   end;
   suppose not c' <=' a';
    then A61:  +(x1,z1) = [0,c' -' a'] by A53,ARYTM_1:def 2;
    then A62:   +(x1,z1) in [:{0},REAL+:] by Lm3,ZFMISC_1:106;
      now per cases;
    suppose c' <=' b';
     then b' - c' = b' -' c' by ARYTM_1:def 2;
     then not +(x1,z1) in REAL+ & not +(y1,z1) in [:{0},REAL+:]
                 by A56,A57,A62,ARYTM_0:5,XBOOLE_0:3;
    hence a + c <= b + c by A2,Def2;
    end;
    suppose not c' <=' b';
     then A63:  +(y1,z1) = [0,c' -' b'] by A56,A57,ARYTM_1:def 2;
    then A64:   +(y1,z1) in [:{0},REAL+:] by Lm3,ZFMISC_1:106;
    c' -' b' <=' c' -' a' by A48,A49,A50,A51,A54,ARYTM_1:16;
    hence a + c <= b + c by A2,A61,A62,A63,A64,Def2';
   end;
   end;
   hence thesis;
  end;
  end;
 hence thesis;
 end;
 suppose that
A65: a in [:{0},REAL+:] and
A66: b in REAL+ and
A67: c in [:{0},REAL+:];
     not a in REAL+ & not c in REAL+ by A65,A67,ARYTM_0:5,XBOOLE_0:3;
  then consider a',c' being Element of REAL+ such that
       a = [0,a'] and
A68: c = [0,c'] and
A69: +(x1,z1) = [0,a' + c'] by ARYTM_0:def 2;
  consider b',c'' being Element of REAL+ such that
       b = b' and
A70: c = [0,c''] and
A71: +(y1,z1) = b' - c'' by A66,A67,ARYTM_0:def 2;
A72: c' = c'' by A68,A70,ZFMISC_1:33;
A73:  +(x1,z1) in [:{0},REAL+:] by A69,Lm3,ZFMISC_1:106;
     now per cases;
   suppose c' <=' b';
    then b' - c'' = b' -' c'' by A72,ARYTM_1:def 2;
    then not +(x1,z1) in REAL+ & not +(y1,z1) in [:{0},REAL+:]
                 by A71,A73,ARYTM_0:5,XBOOLE_0:3;
   hence a + c <= b + c by A2,Def2;
   end;
   suppose
       not c' <=' b';
    then A74:   +(y1,z1) = [0,c' -' b'] by A71,A72,ARYTM_1:def 2;
then A75:  +(y1,z1) in [:{0},REAL+:] by Lm3,ZFMISC_1:106;
A76:  c' -' b' <=' c' by ARYTM_1:11;
       c' <=' c' + a' by ARYTM_2:20;
    then c' -' b' <=' c' + a' by A76,ARYTM_1:3;
   hence a + c <= b + c by A2,A69,A73,A74,A75,Def2';
  end;
  end;
 hence thesis;
 end;
 suppose that
A77: a in [:{0},REAL+:] and
A78: b in [:{0},REAL+:] and
A79: c in [:{0},REAL+:];
  consider a'',b'' being Element of REAL+ such that
A80: a = [0,a''] and
A81: b = [0,b''] and
A82: b'' <=' a'' by A1,A77,A78,Def2;
     not a in REAL+ & not c in REAL+ by A77,A79,ARYTM_0:5,XBOOLE_0:3;
  then consider a',c' being Element of REAL+ such that
A83: a = [0,a'] and
A84: c = [0,c'] and
A85: +(x1,z1) = [0,a' + c'] by ARYTM_0:def 2;
     not b in REAL+ & not c in REAL+ by A78,A79,ARYTM_0:5,XBOOLE_0:3;
  then consider b',c'' being Element of REAL+ such that
A86: b = [0,b'] and
A87: c = [0,c''] and
A88: +(y1,z1) = [0,b' + c''] by ARYTM_0:def 2;
A89: a' = a'' by A80,A83,ZFMISC_1:33;
A90: b' = b'' by A81,A86,ZFMISC_1:33;
A91: c' = c'' by A84,A87,ZFMISC_1:33;
then A92: b' + c' <=' a' + c'' by A82,A89,A90,ARYTM_1:7;
A93: +(x1,z1) in [:{0},REAL+:] by A85,Lm3,ZFMISC_1:106;
      +(y1,z1) in [:{0},REAL+:] by A88,Lm3,ZFMISC_1:106;
 hence a + c <= b + c by A2,A85,A88,A91,A92,A93,Def2';
end;
end;

Lm5:
 a <= b & c <= d implies a+c <= b+d
proof
  assume A1: a<=b;
  assume c <= d;
then A2: b+c <= b+d by Lm4;
  a+c <= b+c by A1,Lm4;
  hence thesis by A2,Th2;
end;

Lm6: a <= b implies a-c <= b-c
proof
  a <= b implies a + -c <= b + -c by Lm4;
  hence thesis;
end;

Lm7: a < b & c <= d implies a+c < b+d
  proof
    assume A1: a<b & c <= d;
   for a,b,c,d holds a<b & c <= d implies a+c < b+d
    proof
      let a,b,c,d;
      assume A2: a<b;
      assume A3: c <= d;
      then A4: a+c <= b+d by A2,Lm5;
      a+c <> b+d
      proof
        assume A5: a+c = b+d;
        b<=a
        proof
           a+c <= d+a by A3,Lm4;
           then a+c-d<=a+d-d by Lm6;
           hence thesis by A5;
         end;
         hence contradiction by A2;
       end;
       hence thesis by A4,Th1;
     end;
    hence thesis by A1;
  end;

Lm8: 0 < a implies b < b+a
proof
  assume 0<a;
  then b+0<b+a by Lm7;
  hence thesis;
end;

theorem :: REAL_1:76
 ex b st a < b
proof
   take a+1;
   a+0<a+1 by Lm7;
   hence thesis;
end;

theorem :: REAL_1:77
 ex b st b < a
proof
   take a-1;
   a+ -1<a+ -0 by Lm7;
   hence thesis;
end;

theorem :: CHAIN_1:2
 ex c st a < c & b < c
proof
 per cases;
 suppose
A1: a <= b;
 take z = b + 1;
  b < z by Lm8;
 hence thesis by Th2,A1;
 end;
 suppose
A2: b <= a;
 take z = a + 1;
  a < z by Lm8;
 hence thesis by Th2,A2;
 end;
end;

Lm9: :: REAL_1:53
 a+c <= b+c implies a <= b
proof
   assume a+c <= b+c;
   then a+c+(-c)<=b+c+(-c) by Lm4;
   hence thesis;
end;

theorem
 ex c st c < a & c < b
proof
 per cases;
 suppose
A1: a <= b;
 take z = a+-1;
  z < a+0 by Lm9;
 hence thesis by Th2,A1;
 end;
 suppose
A2: b <= a;
 take z = b+-1;
  z < b+0 by Lm9;
 hence thesis by Th2,A2;
 end;
end;

Lm10:
 for a',b' being Element of REAL, a,b st a' = a & b' = b
  holds *(a',b') = a * b
proof let a',b' be Element of REAL, a,b such that
A1: a' = a & b' = b;
  consider x1,x2,y1,y2 being Element of REAL such that
A2: a = [* x1,x2 *] and
A3: b = [*y1,y2*] and
A4: a*b = [* +(*(x1,y1),opp*(x2,y2)), +(*(x1,y2),*(x2,y1)) *]
      by XCMPLX_0:def 5;
A5: a = x1 & b = y1 by A2,A3,Lm1;
A6: x2 = 0 & y2 = 0 by A2,A3,Lm1;
  then *(x1,y2) = 0 & *(x2,y1) = 0 by ARYTM_0:14;
  then A7: +(*(x1,y2),*(x2,y1)) = 0 by ARYTM_0:13;
 thus *(a',b') = +(*(x1,y1),0) by A1,A5,ARYTM_0:13
      .= +(*(x1,y1),*(opp x2,y2)) by A6,ARYTM_0:14
      .= +(*(x1,y1),opp*(x2,y2)) by ARYTM_0:17
      .= a * b by A4,A7,ARYTM_0:def 7;
end;
 reconsider o = 0 as Element of REAL+ by ARYTM_2:21;

Lm11: a <= b & 0 <= c implies a*c <= b*c
proof assume that
A1: a <= b and
A2: 0 <= c;
  reconsider x1=a, y1=b, z1=c as Element of REAL by XREAL_0:def 1;
A3: *(y1,z1) = b * c & *(x1,z1) = a * c by Lm10;
   not o in [:{0},REAL+:] by ARYTM_0:5,XBOOLE_0:3;
then A4: c in REAL+ by A2,Def2;
assume
A5: not thesis;
 then A6: c <> 0;
 per cases by A1,Def2;
 suppose that
A7: a in REAL+ and
A8: b in REAL+;
  consider a'',b'' being Element of REAL+ such that
A9: a = a'' and
A10: b = b'' and
A11: a'' <=' b'' by A1,A7,A8,Def2;
  consider a',c' being Element of REAL+ such that
A12: a = a' and
A13: c = c' and
A14: *(x1,z1) = a' *' c' by A4,A7,ARYTM_0:def 3;
  consider b',c'' being Element of REAL+ such that
A15: b = b' and
A16: c = c'' and
A17: *(y1,z1) = b' *' c'' by A4,A8,ARYTM_0:def 3;
      a' *' c' <=' b' *' c' by A9,A10,A11,A12,A15,ARYTM_1:8;
 hence contradiction by A3,A5,A13,A14,A16,A17,Def2';
 end;
 suppose that
A18: a in [:{0},REAL+:] and
A19: b in REAL+;
  consider a',c' being Element of REAL+ such that
       a = [0,a'] and
       c = c' and
A20: *(x1,z1) = [0,c' *' a'] by A4,A6,A18,ARYTM_0:def 3;
  consider b',c'' being Element of REAL+ such that
       b = b' and
       c = c'' and
A21: *(y1,z1) = b' *' c'' by A4,A19,ARYTM_0:def 3;
      *(x1,z1) in [:{0},REAL+:] by A20,Lm3,ZFMISC_1:106;
  then not *(x1,z1) in REAL+ & not *(y1,z1) in [:{0},REAL+:]
     by A21,ARYTM_0:5,XBOOLE_0:3;
 hence contradiction by A3,A5,Def2;
 end;
 suppose that
A22: a in [:{0},REAL+:] and
A23: b in [:{0},REAL+:];
  consider a'',b'' being Element of REAL+ such that
A24: a = [0,a''] and
A25: b = [0,b''] and
A26: b'' <=' a'' by A1,A22,A23,Def2;
  consider a',c' being Element of REAL+ such that
A27: a = [0,a'] and
A28: c = c' and
A29: *(x1,z1) = [0,c' *' a'] by A4,A6,A22,ARYTM_0:def 3;
  consider b',c'' being Element of REAL+ such that
A30: b = [0,b'] and
A31: c = c'' and
A32: *(y1,z1) = [0,c'' *' b'] by A4,A6,A23,ARYTM_0:def 3;
A33: a' = a'' by A24,A27,ZFMISC_1:33;
A34: b' = b'' by A25,A30,ZFMISC_1:33;
A35: *(x1,z1) in [:{0},REAL+:] & *(y1,z1) in [:{0},REAL+:]
  by A29,A32,Lm3,ZFMISC_1:106;
      b' *' c' <=' a' *' c' by A26,A33,A34,ARYTM_1:8;
