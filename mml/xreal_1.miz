   for a,b st b<>0 ex c st a=b/c
  proof let a,b;
   assume
A1:  b<>0;
    then consider c being complex number such that
A2:   a=b/c by XCMPLX_1:234;
   per cases;
   suppose c = 0;
    hence thesis by A2;
   end;
   suppose c <> 0;
     then b/c <> 0 by A1,XCMPLX_1:50;
     then c = b/a by A2,XCMPLX_1:54;
