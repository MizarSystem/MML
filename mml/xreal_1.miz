begin :: with a universal premise

theorem Th211: :: REAL_2:183
 (for a being real number st 0 < a & a < 1 holds c <= b/a) implies c <= b
  proof
     assume A1:for a st a>0 & a<1 holds b/a>=c;
         now let d;
A2:  d" = 1/d by XCMPLX_1:217;
         assume d>1;
      then d"<1 & d">0 by A2,Lm38,Lm46;
      then b/(d")>=c by A1;
      then b*d/1>=c by A2,XCMPLX_1:78;
      hence b*d>=c;
     end;
      hence b>=c by Th169;
  end;

theorem :: REAL_2:184
  (for a being real number st 1 < a holds b/a <= c) implies b <= c
  proof
       (for a st a>1 holds b/a<=c) implies b<=c
    proof
     assume A1:for a st a>1 holds b/a<=c;
         now let d;
A2:    d" = 1/d by XCMPLX_1:217;
        assume A3:0<d & d<1;
      then d">1" by Lm39;
      then b/(d")<=c by A1;
      then b*d/1<=c by A2,XCMPLX_1:78;
      hence b<=c/d by A3,Th79;
     end;
     hence thesis by Th211;
    end;
   hence thesis;
  end;

