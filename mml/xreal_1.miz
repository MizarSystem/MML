theorem Th229:
 r < t implies ex s st r < s & s < t
 proof assume
A1: r < t;
  then A2:  r <> +infty & t <> -infty by XXREAL_0:3,5;
  per cases by A2,XXREAL_0:14;
  suppose that
A3: r = -infty and
A4: t = +infty;
    consider s being Element of REAL;
   take s;
   thus thesis by A3,A4,XXREAL_0:9,12;
  end;
  suppose that
A5: r = -infty and
A6: t in REAL;
    reconsider t as real number by A6,XREAL_0:def 1;
    consider s being real number such that
A7:   s < t by Th4;
   take s;
     s in REAL by XREAL_0:def 1;
   hence r < s by A5,XXREAL_0:12;
   thus thesis by A7;
  end;
  suppose that
A8: r in REAL and
A9: t = +infty;
    reconsider r' = r as real number by A8,XREAL_0:def 1;
    consider s being real number such that
A10:   r' < s by Th3;
   take s;
   thus r < s by A10;
     s in REAL by XREAL_0:def 1;
   hence thesis by A9,XXREAL_0:9;
  end;
  suppose that
A11: r in REAL and
A12: t in REAL;
    reconsider r,t as real number by A11,A12,XREAL_0:def 1;
    consider s being real number such that
A13:   r < s & s < t by A1,Th7;
   take s;
   thus thesis by A13;
  end;
 end;

theorem
  r < s & (for q st r < q & q < s holds t <= q) implies t <= r
 proof assume that
A1: r < s and
A2: for q st r < q & q < s holds t <= q;
    for q st r < q holds t <= q
    proof let q such that
A3:   r < q;
     per cases;
     suppose q < s;
      hence thesis by A2,A3;
     end;
     suppose
A4:     s <= q;
       consider p such that
A5:     r < p and
A6:     p < s by A1,Th229;
        t <= p by A2,A5,A6;
       then t <= s by A6,XXREAL_0:2;
      hence thesis by A4,XXREAL_0:2;
     end;
    end;
  hence thesis by Th229;
 end;

theorem
  r < s & (for q st r < q & q < s holds q <= t) implies s <= t
 proof assume that
A1: r < s and
A2: for q st r < q & q < s holds q <= t;
    for q st t < q holds s <= q
    proof let q such that
A3:   t < q and
A4:   q < s;
     per cases;
     suppose r < q;
      hence thesis by A2,A3,A4;
     end;
     suppose
A5:     q <= r;
       consider p such that
A6:     r < p and
A7:     p < s by A1,Th229;
        p <= t by A2,A6,A7;
       then r <= t by A6,XXREAL_0:2;
      hence thesis by A3,A5,XXREAL_0:2;
     end;
    end;
  hence thesis by Th229;
 end;

