A1: r < s and
A2: for q st r < q & q < s holds q <= t;
  for q st t < q holds s <= q
  proof
    let q such that
A3: t < q and
A4: q < s;
    per cases;
    suppose
      r < q;
      hence thesis by A2,A3,A4;
    end;
    suppose
A5:   q <= r;
