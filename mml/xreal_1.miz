A1: d<=1 and
A2: a<=b;
  1-d>=0 by A1,Th50;
  then (1-d)*a<=(1-d)*b by A2,Lm12;
  then (1-d)*a+d*b<=(1-d)*b+d*b by Lm6;
  hence thesis;
end;

theorem
  0 <= d & d <= 1 & a <= b & a <= c implies a <= (1-d)*b+d*c
