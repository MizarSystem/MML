       then c/b > c/(b*2) by XCMPLX_1:79;
       then a < 1 by A8,Th2;
       then A9: a*c >= b by A1,A7;
        per cases;
        suppose c >= b;
         then b = c by A5,Th1;
         then a*c = b/2 by A6,XCMPLX_1:93;
         hence contradiction by A3,A9,Th85;
        end;
        suppose A10: c < b;
         2*b > 0 by A2,Lm39;
         then 0 < c/(2*b) by A6,Lm37;
         then a*c < c/(2*b)*b by A6,A10,Th100;
