theorem Th211:
  (for a being real number st 0 < a & a < 1 holds c <= b/a)
  implies c <= b
proof
  assume
A1: for a st a>0 & a<1 holds b/a>=c;
  now
    let d;
    assume d>1;
    then
A2: b/(d")>=c by A1,Lm36;
    d" = 1/d by XCMPLX_1:217;
    then b*d/1>=c by A2,XCMPLX_1:78;
    hence b*d>=c;
  end;
  hence thesis by Th169;
end;

theorem
  (for a being real number st 1 < a holds b/a <= c) implies b <= c
proof
  (for a st a>1 holds b/a<=c) implies b<=c
  proof
    assume
A1: for a st a>1 holds b/a<=c;
    now
      let d;
A2:   d" = 1/d by XCMPLX_1:217;
      assume that
A3:   0<d and
A4:   d<1;
      d">1" by A3,A4,Lm34;
      then b/(d")<=c by A1;
      then b*d/1<=c by A2,XCMPLX_1:78;
      hence b<=c/d by A3,Th79;
    end;
    hence thesis by Th211;
  end;
  hence thesis;
end;

