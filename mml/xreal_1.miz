A4: a<=c;
  1-d>=0 by A2,Th50;
  then
A5: (1-d)*a<=(1-d)*b by A3,Lm12;
A6: (1-d)*a+d*a = a;
  d*a<=d*c by A1,A4,Lm12;
  hence thesis by A5,A6,Lm6;
