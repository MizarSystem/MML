A2: d <= 1 and
A3: a<b and
A4: a<c;
  per cases;
  suppose
    d=0;
    hence thesis by A3;
  end;
  suppose
    d=1;
    hence thesis by A4;
  end;
  suppose
A5: not(d=0 or d=1);
    then d<1 by A2,XXREAL_0:1;
    then 1-d>0 by Lm21;
    then
A6: (1-d)*a<(1-d)*b by A3,Lm13;
A7: (1-d)*a+d*a=a;
    d*a<d*c by A1,A4,A5,Lm13;
