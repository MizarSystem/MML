  .=(((a 'or' 'not' b) '&' ('not' a 'or' 'not' 'not' b))).x by BVFUNC_1:4
  .=(((a 'or' 'not' b) '&' ('not' a 'or' b))).x by BVFUNC_1:4
  .=(((a 'or' 'not' b) '&' 'not' a 'or' (a 'or' 'not'
b) '&' b)).x by BVFUNC_1:15
  .=(((a '&' 'not' a 'or' 'not' b '&' 'not' a) 'or' (a 'or' 'not'
b) '&' b)).x by BVFUNC_1:15
  .=(((O_el(Y) 'or' 'not' b '&' 'not' a) 'or' (a 'or' 'not'
b) '&' b)).x by BVFUNC_4:5
  .=((('not' b '&' 'not' a) 'or' (a 'or' 'not' b) '&' b)).x by BVFUNC_1:12
  .=((('not' b '&' 'not' a) 'or' (a '&' b 'or' 'not' b '&' b))).x
  by BVFUNC_1:15
  .=((('not' b '&' 'not' a) 'or' (a '&' b 'or' O_el(Y)))).x by BVFUNC_4:5
  .=(('not' b '&' 'not' a) 'or' (a '&' b)).x by BVFUNC_1:12
  .=(('not' a '&' 'not' b) 'or' (a '&' 'not' 'not' b)).x by BVFUNC_1:4
