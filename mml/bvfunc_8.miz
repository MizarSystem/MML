    ((b).z 'or' ('not' (b).z 'or' FALSE)) by VALUAT_1:def 5
  .=TRUE '&' ((b).z 'or' ('not' (b).z 'or' FALSE)) by BINARITH:18
  .=(b).z 'or' ('not' (b).z 'or' FALSE) by MARGREL1:50
  .=(b).z 'or' 'not' (b).z 'or' FALSE by BINARITH:20
