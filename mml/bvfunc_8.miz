       ((a 'or' b) 'eqv' (b 'or' a) 'eqv' a).z
   =((((a 'or' b) 'eqv' (a 'or' b)) 'imp' a) '&'
       (a 'imp' ((a 'or' b) 'eqv' (a 'or' b)))).z by BVFUNC_4:7
  .=(((a 'or' b) 'eqv' (a 'or' b)) 'imp' a).z '&'
    (a 'imp' ((a 'or' b) 'eqv' (a 'or' b))).z
   by VALUAT_1:def 6
  .=('not'( (a 'or' b) 'eqv' (a 'or' b)) 'or' a).z '&'
    (a 'imp' ((a 'or' b) 'eqv' (a 'or' b))).z
