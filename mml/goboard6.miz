    then 1 <= len G & j < j+1 by NAT_1:39,REAL_1:69;
then A8: s1 < s2 by A1,GOBOARD5:5;
    then s1+s1 < s1+s2 by REAL_1:53;
    then A9: 1/2*(s1+s1) < 1/2*(s1+s2) by REAL_1:70;
