  end;
 let x be set;
 assume x in { |[r,s]| : r < G*(1,1)`1 & G*(1,width G)`2 < s };
  then ex r,s st x = |[r,s]| & r < G*(1,1)`1 & G*(1,width G)`2 < s;
  then x in Int v_strip(G,0) & x in Int h_strip(G,width G) by A2,A3;
