  end;
  let x be set;
  assume
  x in { |[r,s]| : G*(i,1)`1 < r & r < G*(i+1,1)`1 & s < G*(1,1)`2 };
  then
A9: ex r,s st x = |[r,s]| & G*(i,1)`1 < r & r < G*(i+1,1)`1 & s < G*(1,1)`2;
  then
A10: x in Int h_strip(G,0) by A3;
  x in Int v_strip(G,i) by A2,A9;
  hence thesis by A1,A10,XBOOLE_0:def 4;
