theorem Th91:
  p = <*x1,x2,x3,x4*> iff len p = 4 & p.1 = x1 & p.2 = x2
  & p.3=x3 & p.4= x4
proof
   thus
         p = <*x1,x2,x3,x4*> implies len p = 4 & p.1 = x1 & p.2 = x2 & p.3 = x3
     & p.4 = x4
      proof
         assume A1: p =<*x1,x2,x3,x4*>;
           hence len p =len <*x1,x2,x3*> + len <*x4*> by FINSEQ_1:35
            .=3 + len <*x4*> by FINSEQ_1:62
            .=3+1 by FINSEQ_1:57
            .=4;
            set p3=<*x1,x2,x3*>;
         A2: dom p3 ={1,2,3} by FINSEQ_3:1,30;
             then 1 in dom p3 by ENUMSET1:def 1;
         hence p.1 = p3.1 by A1,FINSEQ_1:def 7
            .=x1 by FINSEQ_1:62;

              2 in dom p3 by A2,ENUMSET1:def 1;
         hence p.2 = p3.2 by A1,FINSEQ_1:def 7
            .=x2 by FINSEQ_1:62;

              3 in dom p3 by A2,ENUMSET1:def 1;
         hence p.3 = p3.3 by A1,FINSEQ_1:def 7
            .=x3 by FINSEQ_1:62;

            1 in {1} by TARSKI:def 1;
        then A3: 1 in dom <*x4*> by FINSEQ_1:4,def 8;
        thus p.4 = (p3^<*x4*>).(3+1) by A1
          .=(p3^<*x4*>).(len p3 + 1) by FINSEQ_1:62
          .= <*x4*>.1 by A3,FINSEQ_1:def 7
          .= x4 by FINSEQ_1:def 8;
      end;

    assume A4: len p = 4 & p.1 = x1 & p.2 = x2 & p.3 = x3 & p.4=x4;
      then A5: dom p = Seg(3+1) by FINSEQ_1:def 3
          .= Seg((len <*x1,x2,x3*>) + 1) by FINSEQ_1:62
          .= Seg((len <*x1,x2,x3*>) + len <*x4*>) by FINSEQ_1:56;
     A6: for k be Nat st k in dom <*x1,x2,x3*> holds p.k=<*x1,x2,x3*>.k
           proof let k be Nat such that A7: k in dom <*x1,x2,x3*>;
                    len <*x1,x2,x3*> = 3 by FINSEQ_1:62;
                 then A8: k in {1,2,3} by A7,FINSEQ_1:def 3,FINSEQ_3:1;
                per cases by A8,ENUMSET1:def 1;
                suppose k=1;
                   hence thesis by A4,FINSEQ_1:62;
                end;
                suppose k=2;
                   hence thesis by A4,FINSEQ_1:62;
                end;
                suppose k=3;
                   hence thesis by A4,FINSEQ_1:62;
           end;
           end;
           for k be Nat st
           k in dom <*x4*> holds p.( (len <*x1,x2,x3*>) + k) = <*x4*>.k
         proof let k be Nat; assume k in dom <*x4*>;
            then k in {1} by FINSEQ_1:4,def 8;
       then A9:  k = 1 by TARSKI:def 1;
          hence p.( (len <*x1,x2,x3*>) + k) = p.(3+1) by FINSEQ_1:62
            .=<*x4*>.k by A4,A9,FINSEQ_1:def 8;
         end;
     hence p=<*x1,x2,x3,x4*> by A5,A6,FINSEQ_1:def 7;
end;

