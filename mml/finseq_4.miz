theorem
  for D being non empty set, f being FinSequence of D, p being Element
  of D holds (f^<*p*>)/.(len f + 1) = p
proof
  let D be non empty set, f be FinSequence of D, p be Element of D;
  len(f^<*p*>) = len f + len<*p*> by FINSEQ_1:35;
  then 1 <= len f + 1 & len f + 1 <= len(f^<*p*>) by FINSEQ_1:56,NAT_1:11;
  then len f + 1 in dom(f^<*p*>) by FINSEQ_3:27;
  hence (f^<*p*>)/.(len f + 1) = (f^<*p*>).(len f + 1) by PARTFUN1:def 8
    .= p by FINSEQ_1:59;
end;

:: from GROUP_5

theorem
  for E being non empty set, p,q being FinSequence of E st k in dom p
  holds (p ^ q)/.k = p/.k
proof
  let E be non empty set, p,q be FinSequence of E;
  assume
A1: k in dom p;
  then k in dom(p ^ q) by FINSEQ_3:24;
  hence (p ^ q)/.k = (p ^ q).k by PARTFUN1:def 8
    .= p.k by A1,FINSEQ_1:def 7
    .= p/.k by A1,PARTFUN1:def 8;
end;

theorem
  for E being non empty set, p,q being FinSequence of E st k in dom q
  holds (p ^ q)/.(len p + k) = q/.k
proof
  let E be non empty set, p,q be FinSequence of E;
  assume
A1: k in dom q;
  then len p + k in dom(p ^ q) by FINSEQ_1:41;
  hence (p ^ q)/.(len p + k) = (p ^ q).(len p + k) by PARTFUN1:def 8
    .= q.k by A1,FINSEQ_1:def 7
    .= q/.k by A1,PARTFUN1:def 8;
end;

:: from TOPREAL1

theorem
  for m being Nat for D being set, p being FinSequence of D st a in dom(
  p|m) holds (p|m qua FinSequence of D)/.a = p/.a
proof
  let m be Nat;
  let D be set, p being FinSequence of D;
  assume
A1: a in dom(p|m);
  then a in dom p /\ (Seg m) by RELAT_1:90;
  then
A2: a in dom p by XBOOLE_0:def 4;
  thus (p|m)/.a = (p|Seg m).a by A1,PARTFUN1:def 8
    .= p.a by A1,FUNCT_1:70
    .= p/.a by A2,PARTFUN1:def 8;
end;

:: from GOBOARD1

theorem
  for D being set, f being FinSequence of D, n,m st n in dom f & m in
  Seg n holds m in dom f & (f|n)/.m = f/.m
proof
  let D be set, f be FinSequence of D, n,m;
  assume that
A1: n in dom f and
A2: m in Seg n;
  dom f = Seg len f & n<=len f by A1,FINSEQ_1:def 3,FINSEQ_3:27;
  then
A3: Seg n c= dom f by FINSEQ_1:7;
  hence m in dom f by A2;
A4: Seg n = dom f /\ Seg n by A3,XBOOLE_1:28
    .= dom(f|n) by RELAT_1:90;
  hence (f|n)/.m = (f|n).m by A2,PARTFUN1:def 8
    .= f.m by A2,A4,FUNCT_1:70
    .= f/.m by A2,A3,PARTFUN1:def 8;
end;

