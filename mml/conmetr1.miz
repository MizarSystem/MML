theorem Th10:
  X is Pappian iff X is satisfying_major_indirect_Scherungssatz
proof
A1: X is Pappian implies X is satisfying_major_indirect_Scherungssatz
  proof
    assume
A2: X is Pappian;
    then
A3: X is Desarguesian by AFF_2:25;
    now
      let o,a1,a2,a3,a4,b1,b2,b3,b4,M,N;
      assume that
A4:   M is being_line and
A5:   N is being_line and
A6:   o in M and
A7:   o in N and
A8:   a1 in M and
A9:   a3 in M and
A10:  b2 in M and
A11:  b4 in M and
A12:  a2 in N and
A13:  a4 in N and
A14:  b1 in N and
A15:  b3 in N and
A16:  not a4 in M and
A17:  not a2 in M and
A18:  not b1 in M and
A19:  not b3 in M and
A20:  not a1 in N and
A21:  not a3 in N and
A22:  not b2 in N and
A23:  not b4 in N and
A24:  a3,a2 // b3,b2 and
A25:  a2,a1 // b2,b1 and
A26:  a1,a4 // b1,b4;
A27:  now
        assume that
A28:    a1<>a3 and
A29:    a2<>a4;
        ex x,y,z st LIN x,y,z & x<>y & x<>z & y<>z
        proof
          o,a1 // o,a3 by A4,A6,A8,A9,AFF_1:53,55;
          then
A30:      LIN o,a1,a3 by AFF_1:def 1;
          assume not thesis;
          hence contradiction by A7,A20,A21,A28,A30;
        end;
        then consider d such that
A31:    LIN a2,a1,d and
A32:    a2<>d and
A33:    a1<>d by TRANSLAC:2;
A34:    a2,a1 // a2,d by A31,AFF_1:def 1;
A35:    ex c2 st c2 in M & a2,a3 // b1,c2
        proof
          consider e2 such that
A36:      a1,a3 // a1,e2 and
A37:      a1<>e2 by DIRAF:47;
          consider e1 such that
A38:      a2,a3 // b1,e1 and
A39:      b1<>e1 by DIRAF:47;
          not b1,e1 // a1,e2
          proof
            assume b1,e1 // a1,e2;
            then a2,a3 // a1,e2 by A38,A39,AFF_1:14;
            then a2,a3 // a1,a3 by A36,A37,AFF_1:14;
            then a3,a1 // a3,a2 by AFF_1:13;
