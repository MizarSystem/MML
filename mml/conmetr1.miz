          end;
A59:      now
            assume o=c1;
            then
A60:        o,b2 // a1,a4 by A54,AFF_1:13;
            o,b2 // a1,a3 by A4,A6,A8,A9,A10,AFF_1:53,55;
            then a1,a3 // a1,a4 by A7,A22,A60,AFF_1:14;
            then LIN a1,a3,a4 by AFF_1:def 1;
            hence contradiction by A4,A8,A9,A16,A28,AFF_1:39;
          end;
          assume not thesis;
          hence contradiction by A59,A57;
        end;
        a3,a2 // c2,b1 by A52,AFF_1:13;
        then b3,b2 // c2,b1 by A9,A17,A24,AFF_1:14;
        then b2,b3 // c2,b1 by AFF_1:13;
        then
A61:    b4,b3 // c2,c1 by A2,A4,A5,A6,A7,A10,A11,A14,A15,A18,A19,A22,A23
        ,A53,A51,A56,A55,AFF_2:def 2;
        LIN a1,a2,d by A31,AFF_1:15;
        then a1,a2 // a1,d by AFF_1:def 1;
        then a2,a1 // a1, d by AFF_1:13;
        then
A62:    b2,b1 // a1,d by A8,A17,A25,AFF_1:14;
A63:    b1<>b3
        proof
          assume b1=b3;
          then b2,b1 // a2,a3 by A24,AFF_1:13;
          then
A64:      a2,a1 // a2,a3 by A10,A18,A25,AFF_1:14;
          o,a1 // o,a3 by A4,A6,A8,A9,AFF_1:53,55;
          then LIN o,a1,a3 by AFF_1:def 1;
          hence contradiction by A5,A6,A7,A12,A17,A20,A28,A64,AFF_1:23 ,39;
        end;
A65:    c1<>c2
        proof
A66:      b1<>c1
          proof
            assume b1=c1;
            then a1,a4 // a2,a1 by A10,A18,A25,A54,AFF_1:14;
            then a1,a4 // a1,a2 by AFF_1:13;
            then LIN a1,a4,a2 by AFF_1:def 1;
            then LIN a2,a4,a1 by AFF_1:15;
            hence contradiction by A5,A12,A13,A20,A29,AFF_1:39;
          end;
          assume c1=c2;
          then a3,a2 // b1,c1 by A52,AFF_1:13;
          then
A67:      b3,b2 // b1,c1 by A9,A17,A24,AFF_1:14;
          b1,c1 // b3,b1 by A5,A14,A15,A53,AFF_1:53,55;
          then b3,b1 // b3,b2 by A67,A66,AFF_1:14;
          then LIN b3,b1,b2 by AFF_1:def 1;
          hence contradiction by A5,A14,A15,A22,A63,AFF_1:39;
        end;
        LIN o,d,d by AFF_1:16;
        then consider Y such that
A68:    Y is being_line and
A69:    o in Y and
A70:    d in Y and
        d in Y by AFF_1:33;
A71:    M<>N & M<>Y & N<>Y
        proof
A72:      now
            assume
A73:        N=Y;
            LIN a2,d,a1 by A31,AFF_1:15;
            hence contradiction by A5,A12,A20,A32,A70,A73,AFF_1:39;
          end;
A74:      now
            assume
A75:        M=Y;
            LIN a1,d,a2 by A31,AFF_1:15;
            hence contradiction by A4,A8,A17,A33,A70,A75,AFF_1:39;
          end;
          assume not thesis;
          hence contradiction by A13,A16,A74,A72;
        end;
A76:    o<>d
        proof
          assume o=d;
          then LIN o,a1,a2 by A31,AFF_1:15;
          hence contradiction by A4,A6,A7,A8,A17,A20,AFF_1:39;
        end;
        ex d1 st LIN b1,b2,d1 & d1 in Y
        proof
          consider e2 such that
A77:      o,d // o,e2 and
A78:      o<>e2 by DIRAF:47;
          consider e1 such that
A79:      b1,b2 // b1,e1 and
A80:      b1<>e1 by DIRAF:47;
          not b1,e1 // o,e2
          proof
            assume b1,e1 // o,e2;
            then b1,b2 // o,e2 by A79,A80,AFF_1:14;
            then b1,b2 // o,d by A77,A78,AFF_1:14;
            then b2,b1 // o,d by AFF_1:13;
            then
A81:        a2,a1 // o,d by A10,A18,A25,AFF_1:14;
            a2,a1 // a2,d by A31,AFF_1:def 1;
            then a2,d // o,d by A8,A17,A81,AFF_1:14;
            then d,o // d,a2 by AFF_1:13;
            then LIN d,o,a2 by AFF_1:def 1;
            then
A82:        LIN o,a2,d by AFF_1:15;
            LIN a1,a2,d by A31,AFF_1:15;
            then a1,a2 // a1,d by AFF_1:def 1;
            hence contradiction by A4,A6,A7,A8,A17,A20,A32,A82,AFF_1:23 ,39;
          end;
          then consider d1 such that
A83:      LIN b1,e1,d1 and
A84:      LIN o,e2,d1 by AFF_1:74;
          o,e2 // o,d1 by A84,AFF_1:def 1;
          then o,d // o,d1 by A77,A78,AFF_1:14;
          then
A85:      LIN o,d,d1 by AFF_1:def 1;
          take d1;
          b1,e1 // b1,d1 by A83,AFF_1:def 1;
          then b1,b2 // b1,d1 by A79,A80,AFF_1:14;
          hence thesis by A76,A68,A69,A70,A85,AFF_1:39,def 1;
        end;
        then consider d1 such that
A86:    LIN b1,b2,d1 and
A87:    d1 in Y;
        LIN b2,b1,d1 by A86,AFF_1:15;
        then b2,b1 // b2,d1 by AFF_1:def 1;
        then a1,d // b2,d1 by A10,A18,A62,AFF_1:14;
        then
A88:    d,a4 // d1,c1 by A3,A4,A5,A6,A7,A8,A10,A13,A16,A20,A76,A68,A69,
        A70,A87,A53,A54,A71,AFF_2:def 4;
        b1,b2 // b1,d1 by A86,AFF_1:def 1;
        then b2,b1 // b1,d1 by AFF_1:13;
        then a2,a1 // b1,d1 by A10,A18,A25,AFF_1:14;
        then a2,d // b1,d1 by A8,A17,A34,AFF_1:14;
        then d,a3 // d1,c2 by A3,A4,A5,A6,A7,A9,A12,A14,A17,A21,A76,A68,A69
        ,A70,A87,A51,A52,A71,AFF_2:def 4;
        then a3,a4 // c2,c1 by A3,A4,A5,A6,A7,A9,A13,A16,A21,A76,A68,A69,
        A70,A87,A53,A51,A71,A88,AFF_2:def 4;
        then b4,b3 // a3,a4 by A65,A61,AFF_1:14;
        hence a3,a4 // b3,b4 by AFF_1:13;
      end;
      now
A89:    now
          o,b2 // o,b4 by A4,A6,A10,A11,AFF_1:53,55;
          then
A90:      LIN o,b2,b4 by AFF_1:def 1;
          assume
A91:      a2=a4;
          then a1,a4 // b1,b2 by A25,AFF_1:13;
          then b1,b2 // b1,b4 by A8,A16,A26,AFF_1:14;
          hence a3,a4 // b3,b4 by A5,A6,A7,A14,A18,A22,A24,A91,A90,
          AFF_1:23,39;
        end;
A92:    now
          o,b1 // o,b3 by A5,A7,A14,A15,AFF_1:53,55;
          then
A93:      LIN o,b1,b3 by AFF_1:def 1;
          assume
A94:      a1=a3;
          then a2,a1 // b2,b3 by A24,AFF_1:13;
          then b2,b1 // b2,b3 by A8,A17,A25,AFF_1:14;
          hence a3,a4 // b3,b4 by A4,A6,A7,A10,A18,A22,A26,A94,A93,
          AFF_1:23,39;
        end;
        assume a1=a3 or a2=a4;
        hence a3,a4 // b3,b4 by A92,A89;
      end;
      hence a3,a4 // b3,b4 by A27;
    end;
    hence thesis by Def6;
  end;
  X is satisfying_major_indirect_Scherungssatz implies X is Pappian
  proof
    assume
A95: X is satisfying_major_indirect_Scherungssatz;
    now
      let M,N,o,a,b,c,a1,b1,c1;
      assume that
A96:  M is being_line and
A97:  N is being_line and
A98:  M<>N and
A99:  o in M and
A100: o in N and
A101: o<>a and
A102: o<>a1 and
A103: o<>b and
A104: o<>b1 and
A105: o<>c and
A106: o<>c1 and
A107: a in M and
A108: b in M and
A109: c in M and
A110: a1 in N and
A111: b1 in N and
A112: c1 in N and
A113: a,b1 // b,a1 and
A114: b,c1 // c,b1;
A115: not a in N & not b in N & not c in N & not a1 in M & not b1 in M &
      not c1 in M
      proof
A116:   o,c1 // o,c1 by AFF_1:11;
A117:   o,b1 // o,b1 by AFF_1:11;
A118:   o,a1 // o,a1 by AFF_1:11;
A119:   o,c // o,c by AFF_1:11;
A120:   o,b // o,b by AFF_1:11;
A121:   o,a // o,a by AFF_1:11;
        assume not thesis;
        then M // N by A96,A97,A99,A100,A101,A102,A103,A104,A105,A106,A107
        ,A108,A109,A110,A111,A112,A121,A120,A119,A118,A117,A116,AFF_1:52;
        hence contradiction by A98,A99,A100,AFF_1:59;
      end;
A122: b,c1 // b1,c by A114,AFF_1:13;
A123: b1,b // b,b1 by AFF_1:11;
      a,b1 // a1,b by A113,AFF_1:13;
      then a,c1 // a1,c by A95,A96,A97,A99,A100,A107,A108,A109,A110,A111
      ,A112,A115,A122,A123,Def6;
      hence a,c1 // c,a1 by AFF_1:13;
    end;
    hence thesis by AFF_2:def 2;
  end;
  hence thesis by A1;
end;

theorem
  X is satisfying_PPAP iff X is satisfying_indirect_Scherungssatz
proof
A1: X is satisfying_PPAP implies X is satisfying_indirect_Scherungssatz
  proof
    assume
A2: X is satisfying_PPAP;
    then X is satisfying_pap by AFF_2:24;
    then
A3: X is satisfying_minor_indirect_Scherungssatz by Th9;
    X is Pappian by A2,AFF_2:24;
    then X is satisfying_major_indirect_Scherungssatz by Th10;
    hence thesis by A3,Th1;
  end;
  X is satisfying_indirect_Scherungssatz implies X is satisfying_PPAP
  proof
    assume
A4: X is satisfying_indirect_Scherungssatz;
    then X is satisfying_major_indirect_Scherungssatz by Th1;
    then
A5: X is Pappian by Th10;
    X is satisfying_minor_indirect_Scherungssatz by A4,Th1;
    then X is satisfying_pap by Th9;
    hence thesis by A5,AFF_2:24;
  end;
  hence thesis by A1;
end;

theorem
  X is satisfying_major_indirect_Scherungssatz implies X is
  satisfying_minor_indirect_Scherungssatz
proof
  assume X is satisfying_major_indirect_Scherungssatz;
  then X is Pappian by Th10;
  then X is satisfying_pap by AFF_2:23;
  hence thesis by Th9;
end;

reserve X for OrtAfPl;
reserve o9,a9,a19,a29,a39,a49,b9,b19,b29,b39,b49,c9,c19 for Element of X;
reserve o,a,a1,a2,a3,a4,b,b1,b2,b3,b4,c,c1 for Element of Af(X);
reserve M9,N9 for Subset of X;
reserve A,M,N for Subset of Af(X);

theorem
  Af(X) is satisfying_Scherungssatz iff X is satisfying_SCH
proof
A1: X is satisfying_SCH implies Af(X) is satisfying_Scherungssatz
  proof
    assume
A2: X is satisfying_SCH;
    now
      let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
      assume that
A3:   M is being_line and
A4:   N is being_line and
A5:   a1 in M and
A6:   a3 in M and
A7:   b1 in M and
A8:   b3 in M and
A9:   a2 in N and
A10:  a4 in N and
A11:  b2 in N and
A12:  b4 in N and
A13:  not a4 in M and
A14:  not a2 in M and
A15:  not b2 in M and
A16:  not b4 in M and
A17:  not a1 in N and
A18:  not a3 in N and
A19:  not b1 in N and
A20:  not b3 in N and
A21:  a3,a2 // b3,b2 and
A22:  a2,a1 // b2,b1 and
A23:  a1,a4 // b1,b4;
      reconsider a19=a1,a29=a2,a39=a3,a49=a4,b19=b1,b29=b2,b39=b3,b49=b4 as
      Element of X by ANALMETR:47;
A24:  a39,a29 // b39,b29 by A21,ANALMETR:48;
A25:  a19,a49 // b19,b49 by A23,ANALMETR:48;
A26:  a29,a19 // b29,b19 by A22,ANALMETR:48;
      reconsider M9=M,N9=N as Subset of X by ANALMETR:57;
A27:  N9 is being_line by A4,ANALMETR:58;
      M9 is being_line by A3,ANALMETR:58;
      then
      a39,a49 // b39,b49 by A2,A5,A6,A7,A8,A9,A10,A11,A12,A13,A14,
      A15,A16,A17,A18,A19,A20,A27,A24,A26,A25,CONMETR:def 6;
      hence a3,a4 // b3,b4 by ANALMETR:48;
    end;
    hence thesis by Def3;
  end;
  Af(X) is satisfying_Scherungssatz implies X is satisfying_SCH
  proof
    assume
A28: Af(X) is satisfying_Scherungssatz;
    now
      let a19,a29,a39,a49,b19,b29,b39,b49,M9,N9;
      assume that
A29:  M9 is being_line and
A30:  N9 is being_line and
A31:  a19 in M9 and
A32:  a39 in M9 and
A33:  b19 in M9 and
A34:  b39 in M9 and
A35:  a29 in N9 and
A36:  a49 in N9 and
A37:  b29 in N9 and
A38:  b49 in N9 and
A39:  not a49 in M9 and
A40:  not a29 in M9 and
A41:  not b29 in M9 and
A42:  not b49 in M9 and
A43:  not a19 in N9 and
A44:  not a39 in N9 and
A45:  not b19 in N9 and
A46:  not b39 in N9 and
A47:  a39,a29 // b39,b29 and
A48:  a29,a19 // b29,b19 and
A49:  a19,a49 // b19,b49;
      reconsider a1=a19,a2=a29,a3=a39,a4=a49,b1=b19,b2=b29,b3=b39,b4=b49 as
      Element of Af(X) by ANALMETR:47;
A50:  a3,a2 // b3,b2 by A47,ANALMETR:48;
A51:  a1,a4 // b1,b4 by A49,ANALMETR:48;
A52:  a2,a1 // b2,b1 by A48,ANALMETR:48;
      reconsider M=M9,N=N9 as Subset of Af(X) by ANALMETR:57;
A53:  N is being_line by A30,ANALMETR:58;
      M is being_line by A29,ANALMETR:58;
      then a3,a4 // b3,b4 by A28,A31,A32,A33,A34,A35,A36,A37,A38,A39,A40,A41,
      A42,
      A43,A44,A45,A46,A53,A50,A52,A51,Def3;
      hence a39,a49 // b39,b49 by ANALMETR:48;
    end;
    hence thesis by CONMETR:def 6;
  end;
  hence thesis by A1;
end;

theorem
  X is satisfying_TDES iff Af(X) is Moufangian
proof
  Af(X) is Moufangian implies X is satisfying_TDES
  proof
    assume
A1: Af(X) is Moufangian;
    now
      let o9,a9,a19,b9,b19,c9,c19;
      assume that
      o9<>a9 and
      o9<>a19 and
A2:   o9<>b9 and
      o9<>b19 and
A3:   o9<>c9 and
      o9<>c19 and
A4:   not LIN b9,b19,a9 and
A5:   not LIN b9,b19,c9 and
A6:   LIN o9,a9,a19 and
A7:   LIN o9,b9,b19 and
A8:   LIN o9,c9,c19 and
A9:   a9,b9 // a19,b19 and
A10:  a9,b9 // o9,c9 and
A11:  b9,c9 // b19,c19;
      reconsider o=o9,a=a9,a1=a19,b=b9,b1=b19,c =c9,c1=c19 as Element
 of Af(X) by ANALMETR:47;
A12:  LIN o,b,b1 by A7,ANALMETR:55;
      LIN o,c,c1 by A8,ANALMETR:55;
      then consider M such that
A13:  M is being_line and
A14:  o in M and
A15:  c in M and
A16:  c1 in M by AFF_1:33;
A17:  not LIN b,b1,c by A5,ANALMETR:55;
A18:  not b in M
      proof
        LIN b,b1,o by A12,AFF_1:15;
        then b,b1 // b,o by AFF_1:def 1;
        then
A19:    b,b1 // o,b by AFF_1:13;
        assume b in M;
        then o,b // b,c by A13,A14,A15,AFF_1:53,55;
        then b,b1 // b,c by A2,A19,AFF_1:14;
        hence contradiction by A17,AFF_1:def 1;
      end;
      a,b // a1,b1 by A9,ANALMETR:48;
      then
A20:  b,a // b1,a1 by AFF_1:13;
      not LIN b,b1,a by A4,ANALMETR:55;
      then
A21:  b<>a by AFF_1:16;
A22:  b,c // b1,c1 by A11,ANALMETR:48;
A23:  LIN o,a,a1 by A6,ANALMETR:55;
      a,b // o,c by A10,ANALMETR:48;
      then b,a // o,c by AFF_1:13;
      then b,a // M by A3,A13,A14,A15,AFF_1:41;
      then a,c // a1,c1 by A1,A3,A23,A12,A13,A14,A15,A16,A18,A21,A20,A22,
      AFF_2:def 7;
      hence a9,c9 // a19,c19 by ANALMETR:48;
    end;
    hence thesis by CONMETR:def 5;
  end;
  hence thesis by CONMETR:7;
end;

theorem
  Af(X) is translational iff X is satisfying_des
proof
  X is satisfying_des implies Af(X) is translational
  proof
    assume
A1: X is satisfying_des;
    now
      let A,M,N,a,b,c,a1,b1,c1;
      assume that
A2:   A // M and
A3:   A // N and
A4:   a in A and
A5:   a1 in A and
A6:   b in M and
A7:   b1 in M and
A8:   c in N and
A9:   c1 in N and
A10:  A is being_line and
A11:  M is being_line and
A12:  N is being_line and
A13:  A<>M and
A14:  A<>N and
A15:  a,b // a1,b1 and
A16:  a,c // a1,c1;
      reconsider a9=a,a19=a1,b9=b,b19=b1,c9=c,c19=c1 as Element of X by
      ANALMETR:47;
      b,c // b1,c1
      proof
        assume
A17:    not b,c // b1,c1;
A18:    a<>a1
        proof
          assume
A19:      a=a1;
A20:      c =c1
          proof
            LIN a,c,c1 by A16,A19,AFF_1:def 1;
            then
A21:        LIN c,c1,a by AFF_1:15;
            assume c <>c1;
            then a in N by A8,A9,A12,A21,AFF_1:39;
            hence contradiction by A3,A4,A14,AFF_1:59;
          end;
          b=b1
          proof
            LIN a,b,b1 by A15,A19,AFF_1:def 1;
            then
A22:        LIN b,b1,a by AFF_1:15;
            assume b<>b1;
            then a in M by A6,A7,A11,A22,AFF_1:39;
            hence contradiction by A2,A4,A13,AFF_1:59;
          end;
          hence contradiction by A17,A20,AFF_1:11;
        end;
A23:    not LIN a9,a19,b9 & not LIN a9,a19,c9
        proof
          assume LIN a9,a19,b9 or LIN a9,a19,c9;
          then LIN a,a1,b or LIN a,a1,c by ANALMETR:55;
          then b in A or c in A by A4,A5,A10,A18,AFF_1:39;
          hence contradiction by A2,A3,A6,A8,A13,A14,AFF_1:59;
        end;
        a,a1 // c,c1 by A3,A4,A5,A8,A9,AFF_1:53;
        then
A24:    a9,a19 // c9,c19 by ANALMETR:48;
        a,a1 // b,b1 by A2,A4,A5,A6,A7,AFF_1:53;
        then
A25:    a9,a19 // b9,b19 by ANALMETR:48;
A26:    a9,c9 // a19,c19 by A16,ANALMETR:48;
        a9,b9 // a19,b19 by A15,ANALMETR:48;
        then b9,c9 // b19,c19 by A1,A23,A25,A24,A26,CONMETR:def 8;
        hence contradiction by A17,ANALMETR:48;
      end;
      hence b,c // b1,c1;
    end;
    hence thesis by AFF_2:def 11;
  end;
  hence thesis by CONMETR:8;
end;

theorem
  X is satisfying_PAP iff Af(X) is Pappian
proof
A1: X is satisfying_PAP implies Af(X) is Pappian
  proof
    assume
A2: X is satisfying_PAP;
    now
      let M,N,o,a,b,c,a1,b1,c1;
      assume that
A3:   M is being_line and
A4:   N is being_line and
A5:   M<>N and
A6:   o in M and
A7:   o in N and
A8:   o<>a and
A9:   o<>a1 and
A10:  o<>b and
A11:  o<>b1 and
A12:  o<>c and
A13:  o<>c1 and
A14:  a in M and
A15:  b in M and
A16:  c in M and
A17:  a1 in N and
A18:  b1 in N and
A19:  c1 in N and
A20:  a,b1 // b,a1 and
A21:  b,c1 // c,b1;
      reconsider a9=a,b9=b,c9=c,a19=a1,b19=b1,c19=c1 as Element of X by
      ANALMETR:47;
      reconsider M9=M,N9=N as Subset of X by ANALMETR:57;
A22:  N9 is being_line by A4,ANALMETR:58;
A23:  not c19 in M9 & not b9 in N9
      proof
        assume c19 in M9 or b9 in N9;
        then
A24:    o,c1 // M or o,b // N by A3,A4,A6,A7,AFF_1:66;
A25:    o,b // M by A3,A6,A15,AFF_1:66;
        o,c1 // N by A4,A7,A19,AFF_1:66;
        hence contradiction by A5,A6,A7,A10,A13,A24,A25,AFF_1:59,67;
      end;
      b,a1 // a,b1 by A20,AFF_1:13;
      then
A26:  b9,a19 // a9,b19 by ANALMETR:48;
A27:  b9,c19 // c9,b19 by A21,ANALMETR:48;
      M9 is being_line by A3,ANALMETR:58;
      then
      a9,c19 // c9,a19 by A2,A6,A7,A8,A9,A11,A12,A14,A15,A16,A17,A18,A19
      ,A22,A26,A27,A23,CONMETR:def 2;
      hence a,c1 // c,a1 by ANALMETR:48;
    end;
    hence thesis by AFF_2:def 2;
  end;
  Af(X) is Pappian implies X is satisfying_PAP
  proof
    assume
A28: Af(X) is Pappian;
    now
      let o9,a19,a29,a39,b19,b29,b39,M9,N9;
      assume that
A29:  M9 is being_line and
A30:  N9 is being_line and
A31:  o9 in M9 and
A32:  a19 in M9 and
A33:  a29 in M9 and
A34:  a39 in M9 and
A35:  o9 in N9 and
A36:  b19 in N9 and
A37:  b29 in N9 and
A38:  b39 in N9 and
A39:  not b29 in M9 and
A40:  not a39 in N9 and
A41:  o9<>a19 and
A42:  o9<>a29 and
      o9<>a39 and
A43:  o9<>b19 and
      o9<>b29 and
A44:  o9<>b39 and
A45:  a39,b29 // a29,b19 and
A46:  a39,b39 // a19,b19;
      reconsider a1=a19,a2=a29,a3=a39,b1=b19,b2=b29,b3=b39 as Element of Af(X)
      by ANALMETR:47;
      reconsider M=M9,N=N9 as Subset of Af(X) by ANALMETR:57;
A47:  N is being_line by A30,ANALMETR:58;
A48:  M is being_line by A29,ANALMETR:58;
      now
        assume M<>N;
        a3,b3 // a1,b1 by A46,ANALMETR:48;
        then
A49:    a1,b1 // a3,b3 by AFF_1:13;
        a3,b2 // a2,b1 by A45,ANALMETR:48;
        then
        a1,b2 // a2,b3 by A28,A31,A32,A33,A34,A35,A36,A37,A38,A39,A40,A41
        ,A42,A43,A44,A48,A47,A49,AFF_2:def 2;
        hence a19,b29 // a29,b39 by ANALMETR:48;
      end;
      hence a19,b29 // a29,b39 by A37,A39;
    end;
    hence thesis by CONMETR:def 2;
  end;
  hence thesis by A1;
end;

theorem
  X is satisfying_DES iff Af(X) is Desarguesian
proof
A1: X is satisfying_DES implies Af(X) is Desarguesian
  proof
    assume
A2: X is satisfying_DES;
    now
      let A,M,N,o,a,b,c,a1,b1,c1;
      assume that
A3:   o in A and
A4:   o in M and
A5:   o in N and
A6:   o<>a and
A7:   o<>b and
A8:   o<>c and
A9:   a in A and
A10:  a1 in A and
A11:  b in M and
A12:  b1 in M and
A13:  c in N and
A14:  c1 in N and
A15:  A is being_line and
A16:  M is being_line and
A17:  N is being_line and
A18:  A<>M and
A19:  A<>N and
A20:  a,b // a1,b1 and
A21:  a,c // a1,c1;
      reconsider o9=o,a9=a,a19=a1,b9=b,b19=b1,c9=c,c19=c1 as Element of X by
      ANALMETR:47;
      o,b // o,b1 by A4,A11,A12,A16,AFF_1:53,55;
      then
A22:  LIN o,b,b1 by AFF_1:def 1;
      then
A23:  LIN o9,b9,b19 by ANALMETR:55;
      o,a // o,a1 by A3,A9,A10,A15,AFF_1:53,55;
      then
A24:  LIN o,a, a1 by AFF_1:def 1;
      o,c // o,c1 by A5,A13,A14,A17,AFF_1:53,55;
      then
A25:  LIN o,c,c1 by AFF_1:def 1;
      then
A26:  LIN o9,c9,c19 by ANALMETR:55;
      assume
A27:  not b,c // b1,c1;
A28:  not LIN b9,b19,a9 & not LIN a9,a19,c9
      proof
A29:    a<>a1
        proof
          assume
A30:      a=a1;
A31:      c =c1
          proof
A32:        not LIN o,a,c
            proof
              assume LIN o,a,c;
              then c in A by A3,A6,A9,A15,AFF_1:39;
              then
A33:          o,c // A by A3,A15,AFF_1:66;
              o,c // N by A5,A13,A17,AFF_1:66;
              hence contradiction by A3,A5,A8,A19,A33,AFF_1:59,67;
            end;
            assume c <>c1;
            hence contradiction by A21,A25,A30,A32,AFF_1:23;
          end;
          b=b1
          proof
A34:        not LIN o,a,b
            proof
              assume LIN o,a,b;
              then b in A by A3,A6,A9,A15,AFF_1:39;
              then
A35:          o,b // A by A3,A15,AFF_1:66;
              o,b // M by A4,A11,A16,AFF_1:66;
              hence contradiction by A3,A4,A7,A18,A35,AFF_1:59,67;
            end;
            assume b<>b1;
            hence contradiction by A20,A22,A30,A34,AFF_1:23;
          end;
          hence contradiction by A27,A31,AFF_1:11;
        end;
A36:    b<>b1
        proof
A37:      not LIN o,b,a
          proof
            assume LIN o,b,a;
            then a in M by A4,A7,A11,A16,AFF_1:39;
            then
A38:        o,a // M by A4,A16,AFF_1:66;
            o,a // A by A3,A9,A15,AFF_1:66;
            hence contradiction by A3,A4,A6,A18,A38,AFF_1:59,67;
          end;
          assume b=b1;
          then b,a // b,a1 by A20,AFF_1:13;
          hence contradiction by A24,A29,A37,AFF_1:23;
        end;
        assume LIN b9,b19,a9 or LIN a9,a19,c9;
        then LIN b,b1,a or LIN a,a1,c by ANALMETR:55;
        then a in M or c in A by A9,A10,A11,A12,A15,A16,A29,A36,
        AFF_1:39;
        then
A39:    o,a // M or o,c // A by A3,A4,A15,A16,AFF_1:66;
A40:    o,c // N by A5,A13,A17,AFF_1:66;
        o,a // A by A3,A9,A15,AFF_1:66;
        hence contradiction by A3,A4,A5,A6,A8,A18,A19,A39,A40, AFF_1:59,
        67;
      end;
A41:  a9,c9 // a19,c19 by A21,ANALMETR:48;
A42:  a9,b9 // a19,b19 by A20,ANALMETR:48;
A43:  o9<>a19 & o9<>b19 & o9<>c19
      proof
A44:    now
          assume
A45:      o9=a19;
A46:      o=c1
          proof
A47:        not LIN c,a,o
            proof
              assume LIN c,a,o;
              then LIN o,a,c by AFF_1:15;
              then c in A by A3,A6,A9,A15,AFF_1:39;
              then
A48:          o,c // A by A3,A15,AFF_1:66;
              o,c // N by A5,A13,A17,AFF_1:66;
              hence contradiction by A3,A5,A8,A19,A48,AFF_1:59,67;
            end;
            assume
A49:        o<>c1;
A50:        o,c // o,c1 by A5,A13,A14,A17,AFF_1:53,55;
            then a,c // o,c by A21,A45,A49,AFF_1:14;
            then c,a // c,o by AFF_1:13;
            then LIN c,a,o by AFF_1:def 1;
            then LIN o,a,c by AFF_1:15;
            then o,a // o,c by AFF_1:def 1;
            then o,a // o,c1 by A8,A50,AFF_1:14;
            then LIN o,a,c1 by AFF_1:def 1;
            then LIN a,o,c1 by AFF_1:15;
            then
A51:        a,o // a,c1 by AFF_1:def 1;
            LIN c,o,c1 by A25,AFF_1:15;
            hence contradiction by A49,A47,A51,AFF_1:23;
          end;
          o=b1
          proof
A52:        not LIN b,a,o
            proof
              assume LIN b,a,o;
              then LIN o,a,b by AFF_1:15;
              then b in A by A3,A6,A9,A15,AFF_1:39;
              then
A53:          o,b // A by A3,A15,AFF_1:66;
              o,b // M by A4,A11,A16,AFF_1:66;
              hence contradiction by A3,A4,A7,A18,A53,AFF_1:59,67;
            end;
            assume
A54:        o<>b1;
A55:        o,b1 // o,b by A4,A11,A12,A16,AFF_1:53,55;
            then a,b // o,b by A20,A45,A54,AFF_1:14;
            then b,a // b,o by AFF_1:13;
            then LIN b,a,o by AFF_1:def 1;
            then LIN o,a,b by AFF_1:15;
            then o,a // o,b by AFF_1:def 1;
            then o,a // o,b1 by A7,A55,AFF_1:14;
            then LIN o,a,b1 by AFF_1:def 1;
            then LIN a,o,b1 by AFF_1:15;
            then
A56:        a,o // a,b1 by AFF_1:def 1;
            LIN b,o,b1 by A22,AFF_1:15;
            hence contradiction by A54,A52,A56,AFF_1:23;
          end;
          hence contradiction by A27,A46,AFF_1:12;
        end;
A57:    now
A58:      not LIN a,b,o
          proof
            assume LIN a,b,o;
            then LIN o,a,b by AFF_1:15;
            then b in A by A3,A6,A9,A15,AFF_1:39;
            then
A59:        o,b // A by A3,A15,AFF_1:66;
            o,b // M by A4,A11,A16,AFF_1:66;
            hence contradiction by A3,A4,A7,A18,A59,AFF_1:59,67;
          end;
A60:      a1,o // a,o by A3,A9,A10,A15,AFF_1:53,55;
          assume o9=b19;
          then a,b // a,o by A20,A44,A60,AFF_1:14;
          then LIN a,b,o by AFF_1:def 1;
          then LIN o,b,a by AFF_1:15;
          then o,b // o,a by AFF_1:def 1;
          then o,b // a,o by AFF_1:13;
          then o,b // a1,o by A6,A60,AFF_1:14;
          then o,b // o,a1 by AFF_1:13;
          then LIN o,b,a1 by AFF_1:def 1;
          then LIN b,o,a1 by AFF_1:15;
          then
A61:      b,o // b,a1 by AFF_1:def 1;
          LIN a,o,a1 by A24,AFF_1:15;
          hence contradiction by A44,A58,A61,AFF_1:23;
        end;
A62:    now
A63:      not LIN a,c,o
          proof
            assume LIN a,c,o;
            then LIN o,a,c by AFF_1:15;
            then c in A by A3,A6,A9,A15,AFF_1:39;
            then
A64:        o,c // A by A3,A15,AFF_1:66;
            o,c // N by A5,A13,A17,AFF_1:66;
            hence contradiction by A3,A5,A8,A19,A64,AFF_1:59,67;
          end;
A65:      a1,o // a,o by A3,A9,A10,A15,AFF_1:53,55;
          assume o9=c19;
          then a,c // a,o by A21,A44,A65,AFF_1:14;
          then LIN a,c,o by AFF_1:def 1;
          then LIN o,c,a by AFF_1:15;
          then o,c // o,a by AFF_1:def 1;
          then o,c // a,o by AFF_1:13;
          then o,c // a1,o by A6,A65,AFF_1:14;
          then o,c // o,a1 by AFF_1:13;
          then LIN o,c,a1 by AFF_1:def 1;
          then LIN c,o,a1 by AFF_1:15;
          then
A66:      c,o // c,a1 by AFF_1:def 1;
          LIN a,o,a1 by A24,AFF_1:15;
          hence contradiction by A44,A63,A66,AFF_1:23;
        end;
        assume not thesis;
        hence contradiction by A44,A57,A62;
      end;
      LIN o9,a9,a19 by A24,ANALMETR:55;
      then b9,c9 // b19,c19 by A2,A6,A7,A8,A23,A26,A43,A28,A42,A41,
      CONAFFM:def 1;
      hence contradiction by A27,ANALMETR:48;
    end;
    hence thesis by AFF_2:def 4;
  end;
  Af(X) is Desarguesian implies X is satisfying_DES
  proof
    assume
A67: Af(X) is Desarguesian;
    now
      let o9,a9,a19,b9,b19,c9,c19;
      assume that
A68:  o9<>a9 and
      o9<>a19 and
A69:  o9<>b9 and
      o9<>b19 and
A70:  o9<>c9 and
      o9<>c19 and
A71:  not LIN b9,b19,a9 and
A72:  not LIN a9,a19,c9 and
A73:  LIN o9,a9,a19 and
A74:  LIN o9,b9,b19 and
A75:  LIN o9,c9,c19 and
A76:  a9,b9 // a19,b19 and
A77:  a9,c9 // a19,c19;
      reconsider o=o9,a=a9,a1=a19,b=b9,b1=b19,c =c9,c1=c19 as Element of Af(X)
      by ANALMETR:47;
A78:  not LIN a,a1,c by A72,ANALMETR:55;
A79:  a,c // a1,c1 by A77,ANALMETR:48;
A80:  a,b // a1,b1 by A76,ANALMETR:48;
      LIN o,b,b1 by A74,ANALMETR:55;
      then consider M such that
A81:  M is being_line and
A82:  o in M and
A83:  b in M and
A84:  b1 in M by AFF_1:33;
      LIN o,c,c1 by A75,ANALMETR:55;
      then consider N such that
A85:  N is being_line and
A86:  o in N and
A87:  c in N and
A88:  c1 in N by AFF_1:33;
      LIN o,a,a1 by A73,ANALMETR:55;
      then consider A such that
A89:  A is being_line and
A90:  o in A and
A91:  a in A and
A92:  a1 in A by AFF_1:33;
A93:  not LIN b,b1,a by A71,ANALMETR:55;
      A<>M & A<>N
      proof
        assume not thesis;
        then
        b,b1 // b,a or a,a1 // a,c by A89,A91,A92,A83,A84,A87, AFF_1:53,55;
        hence contradiction by A93,A78,AFF_1:def 1;
      end;
      then b,c // b1,c1 by A67,A68,A69,A70,A89,A90,A91,A92,A81,A82
      ,A83,A84,A85,A86,A87,A88,A80,A79,AFF_2:def 4;
      hence b9,c9 // b19,c19 by ANALMETR:48;
    end;
    hence thesis by CONAFFM:def 1;
  end;
  hence thesis by A1;
end;
