          then a1,a3 // a1,c2 by A36,A37,AFF_1:14;
          then
A42:      LIN a1,a3,c2 by AFF_1:def 1;
          take c2;
          b1,e1 // b1,c2 by A40,AFF_1:def 1;
          hence thesis by A4,A8,A9,A28,A38,A39,A42,AFF_1:14,39;
        end;
A43:    ex c1 st c1 in N & a1,a4 // b2,c1
        proof
          consider e2 such that
A44:      a2,a4 // a2,e2 and
A45:      a2<>e2 by DIRAF:47;
          consider e1 such that
A46:      a1,a4 // b2,e1 and
A47:      b2<>e1 by DIRAF:47;
          not b2,e1 // a2,e2
          proof
            assume b2,e1 // a2,e2;
            then a1,a4 // a2,e2 by A46,A47,AFF_1:14;
            then a1,a4 // a2,a4 by A44,A45,AFF_1:14;
            then a4,a2 // a4,a1 by AFF_1:13;
            then LIN a4,a2,a1 by AFF_1:def 1;
            hence contradiction by A5,A12,A13,A20,A29,AFF_1:39;
          end;
          then consider c1 such that
A48:      LIN b2,e1,c1 and
A49:      LIN a2,e2,c1 by AFF_1:74;
          a2,e2 // a2,c1 by A49,AFF_1:def 1;
          then a2,a4 // a2,c1 by A44,A45,AFF_1:14;
          then
A50:      LIN a2,a4,c1 by AFF_1:def 1;
          take c1;
          b2,e1 // b2,c1 by A48,AFF_1:def 1;
          hence thesis by A5,A12,A13,A29,A46,A47,A50,AFF_1:14,39;
        end;
        consider c2 such that
A51:    c2 in M and
A52:    a2,a3 // b1,c2 by A35;
        consider c1 such that
A53:    c1 in N and
A54:    a1,a4 // b2,c1 by A43;
        b1,b4 // b2,c1 by A8,A16,A26,A54,AFF_1:14;
        then
A55:    b4,b1 // b2,c1 by AFF_1:13;
A56:    o<>c1 & o<>c2
        proof
A57:      now
            assume o=c2;
            then
A58:        o,b1 // a2,a3 by A52,AFF_1:13;
            o,b1 // a2, a4 by A5,A7,A12,A13,A14,AFF_1:53,55;
            then a2,a4 // a2,a3 by A6,A18,A58,AFF_1:14;
            then LIN a2,a4,a3 by AFF_1:def 1;
