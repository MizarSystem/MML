  hence a3,a4 // b3,b4 by AFF_1:13;end;
 hence thesis by Def5;end;
     X satisfies_minor_SCH* implies X satisfies_pap proof
  assume A6:X satisfies_minor_SCH*;
      now let M,N,a,b,c,a1,b1,c1;
