environ

 vocabularies DIRAF, SUBSET_1, ANALOAF, AFF_1, AFF_2, ANALMETR, CONMETR,
      CONAFFM, CONMETR1;
 notations STRUCT_0, ANALOAF, DIRAF, AFF_1, ANALMETR, AFF_2, CONAFFM, CONMETR;
 constructors AFF_1, AFF_2, CONAFFM, CONMETR;
 registrations ANALMETR;
 requirements SUBSET;
 theorems AFF_1, AFF_2, ANALMETR, CONAFFM, CONMETR, TRANSLAC, DIRAF;

begin

reserve X for AffinPlane;
reserve o,a,a1,a2,a3,a4,b,b1,b2,b3,b4,c,c1,c2,d,d1,d2, d3,d4,d5,e1,e2,x,y,z
  for Element of X;
reserve Y,Z,M,N,A,K,C for Subset of X;

definition
  let X;
  attr X is satisfying_minor_Scherungssatz means
  :Def1:
  for a1,a2,a3,a4,b1,b2,
b3,b4,M,N st M // N & a1 in M & a3 in M & b1 in M & b3 in M & a2 in N & a4 in N
  & b2 in N & b4 in N & not a4 in M & not a2 in M & not b2 in M & not b4 in M &
not a1 in N & not a3 in N & not b1 in N & not b3 in N & a3,a2 // b3,b2 & a2,a1
  // b2,b1 & a1,a4 // b1,b4 holds a3,a4 // b3,b4;
end;

definition
  let X;
  attr X is satisfying_major_Scherungssatz means
  :Def2:
  for o,a1,a2,a3,a4,b1,
b2,b3,b4,M,N st M is being_line & N is being_line & o in M & o in N & a1 in M &
a3 in M & b1 in M & b3 in M & a2 in N & a4 in N & b2 in N & b4 in N & not a4 in
  M & not a2 in M & not b2 in M & not b4 in M & not a1 in N & not a3 in N & not
b1 in N & not b3 in N & a3,a2 // b3,b2 & a2,a1 // b2,b1 & a1,a4 // b1,b4 holds
  a3,a4 // b3,b4;
end;

definition
  let X;
  attr X is satisfying_Scherungssatz means
  :Def3:
  for a1,a2,a3,a4,b1,b2,b3,b4,
M,N st M is being_line & N is being_line & a1 in M & a3 in M & b1 in M & b3 in
M & a2 in N & a4 in N & b2 in N & b4 in N & not a4 in M & not a2 in M & not b2
in M & not b4 in M & not a1 in N & not a3 in N & not b1 in N & not b3 in N & a3
  ,a2 // b3,b2 & a2,a1 // b2,b1 & a1,a4 // b1,b4 holds a3,a4 // b3,b4;
end;

definition
  let X;
  attr X is satisfying_indirect_Scherungssatz means
  :Def4:
  for a1,a2,a3,a4,b1,
b2,b3,b4,M,N st M is being_line & N is being_line & a1 in M & a3 in M & b2 in M
& b4 in M & a2 in N & a4 in N & b1 in N & b3 in N & not a4 in M & not a2 in M &
not b1 in M & not b3 in M & not a1 in N & not a3 in N & not b2 in N & not b4 in
  N & a3,a2 // b3,b2 & a2,a1 // b2,b1 & a1,a4 // b1,b4 holds a3,a4 // b3,b4;
end;

definition
  let X;
  attr X is satisfying_minor_indirect_Scherungssatz means
  :Def5:
  for a1,a2,a3,
a4,b1,b2,b3,b4,M,N st M // N & a1 in M & a3 in M & b2 in M & b4 in M & a2 in N
  & a4 in N & b1 in N & b3 in N & not a4 in M & not a2 in M & not b1 in M & not
b3 in M & not a1 in N & not a3 in N & not b2 in N & not b4 in N & a3,a2 // b3,
  b2 & a2,a1 // b2,b1 & a1,a4 // b1,b4 holds a3,a4 // b3,b4;
end;

definition
  let X;
  attr X is satisfying_major_indirect_Scherungssatz means
  :Def6:
  for o,a1,a2,
a3,a4,b1,b2,b3,b4,M,N st M is being_line & N is being_line & o in M & o in N &
a1 in M & a3 in M & b2 in M & b4 in M & a2 in N & a4 in N & b1 in N & b3 in N &
not a4 in M & not a2 in M & not b1 in M & not b3 in M & not a1 in N & not a3 in
N & not b2 in N & not b4 in N & a3,a2 // b3,b2 & a2,a1 // b2,b1 & a1,a4 // b1,
  b4 holds a3,a4 // b3,b4;
end;

theorem Th1:
  X is satisfying_indirect_Scherungssatz iff X is
  satisfying_minor_indirect_Scherungssatz & X is
  satisfying_major_indirect_Scherungssatz
proof
A1: X is satisfying_indirect_Scherungssatz implies X is
  satisfying_minor_indirect_Scherungssatz
  proof
    assume
A2: X is satisfying_indirect_Scherungssatz;
    now
      let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
      assume that
A3:   M // N and
A4:   a1 in M and
A5:   a3 in M and
A6:   b2 in M and
A7:   b4 in M and
A8:   a2 in N and
A9:   a4 in N and
A10:  b1 in N and
A11:  b3 in N and
A12:  not a4 in M and
A13:  not a2 in M and
A14:  not b1 in M and
A15:  not b3 in M and
A16:  not a1 in N and
A17:  not a3 in N and
A18:  not b2 in N and
A19:  not b4 in N and
A20:  a3,a2 // b3,b2 and
A21:  a2,a1 // b2,b1 and
A22:  a1,a4 // b1,b4;
A23:  N is being_line by A3,AFF_1:50;
      M is being_line by A3,AFF_1:50;
      hence a3,a4 // b3,b4 by A2,A4,A5,A6,A7,A8,A9,A10,A11,A12,A13,A14,A15,
      A16,A17,A18,A19,A20,A21,A22,A23,Def4;
    end;
    hence thesis by Def5;
  end;
A24: X is satisfying_minor_indirect_Scherungssatz & X is
  satisfying_major_indirect_Scherungssatz implies X is
  satisfying_indirect_Scherungssatz
  proof
    assume that
A25: X is satisfying_minor_indirect_Scherungssatz and
A26: X is satisfying_major_indirect_Scherungssatz;
    now
      let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
      assume that
A27:  M is being_line and
A28:  N is being_line and
A29:  a1 in M and
A30:  a3 in M and
A31:  b2 in M and
A32:  b4 in M and
A33:  a2 in N and
A34:  a4 in N and
A35:  b1 in N and
A36:  b3 in N and
A37:  not a4 in M and
A38:  not a2 in M and
A39:  not b1 in M and
A40:  not b3 in M and
A41:  not a1 in N and
A42:  not a3 in N and
A43:  not b2 in N and
A44:  not b4 in N and
A45:  a3,a2 // b3,b2 and
A46:  a2,a1 // b2,b1 and
A47:  a1,a4 // b1,b4;
      now
        assume not M // N;
        then ex o st o in M & o in N by A27,A28,AFF_1:72;
        hence a3,a4 // b3,b4 by A26,A27,A28,A29,A30,A31,A32,A33,A34,A35,A36,
        A37,A38,A39,A40,A41,A42,A43,A44,A45,A46,A47,Def6;
      end;
      hence
      a3,a4 // b3,b4 by A25,A29,A30,A31,A32,A33,A34,A35,A36,A37,A38,A39,
      A40,A41,A42,A43,A44,A45,A46,A47,Def5;
    end;
    hence thesis by Def4;
  end;
  X is satisfying_indirect_Scherungssatz implies X is
  satisfying_major_indirect_Scherungssatz
  proof
    assume X is satisfying_indirect_Scherungssatz;
    then for o,a1,a2,a3,a4,b1,b2,b3,b4,M,N holds M is being_line & N is
being_line & o in M & o in N & a1 in M & a3 in M & b2 in M & b4 in M & a2 in N
& a4 in N & b1 in N & b3 in N & not a4 in M & not a2 in M & not b1 in M & not
b3 in M & not a1 in N & not a3 in N & not b2 in N & not b4 in N & a3,a2 // b3,
    b2 & a2,a1 // b2,b1 & a1,a4 // b1,b4 implies a3,a4 // b3,b4 by Def4;
    hence thesis by Def6;
  end;
  hence thesis by A1,A24;
end;

theorem Th2:
  X is satisfying_Scherungssatz iff X is
  satisfying_minor_Scherungssatz & X is satisfying_major_Scherungssatz
proof
A1: X is satisfying_Scherungssatz implies X is satisfying_minor_Scherungssatz
  proof
    assume
A2: X is satisfying_Scherungssatz;
    now
      let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
      assume that
A3:   M // N and
A4:   a1 in M and
A5:   a3 in M and
A6:   b1 in M and
A7:   b3 in M and
A8:   a2 in N and
A9:   a4 in N and
A10:  b2 in N and
A11:  b4 in N and
A12:  not a4 in M and
A13:  not a2 in M and
A14:  not b2 in M and
A15:  not b4 in M and
A16:  not a1 in N and
A17:  not a3 in N and
A18:  not b1 in N and
A19:  not b3 in N and
A20:  a3,a2 // b3,b2 and
A21:  a2,a1 // b2,b1 and
A22:  a1,a4 // b1,b4;
A23:  N is being_line by A3,AFF_1:50;
      M is being_line by A3,AFF_1:50;
      hence a3,a4 // b3,b4 by A2,A4,A5,A6,A7,A8,A9,A10,A11,A12,A13,A14,A15,
      A16,A17,A18,A19,A20,A21,A22,A23,Def3;
    end;
    hence thesis by Def1;
  end;
A24: X is satisfying_minor_Scherungssatz & X is
  satisfying_major_Scherungssatz implies X is satisfying_Scherungssatz
  proof
    assume that
A25: X is satisfying_minor_Scherungssatz and
A26: X is satisfying_major_Scherungssatz;
    now
      let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
      assume that
A27:  M is being_line and
A28:  N is being_line and
A29:  a1 in M and
A30:  a3 in M and
A31:  b1 in M and
A32:  b3 in M and
A33:  a2 in N and
A34:  a4 in N and
A35:  b2 in N and
A36:  b4 in N and
A37:  not a4 in M and
A38:  not a2 in M and
A39:  not b2 in M and
A40:  not b4 in M and
A41:  not a1 in N and
A42:  not a3 in N and
A43:  not b1 in N and
A44:  not b3 in N and
A45:  a3,a2 // b3,b2 and
A46:  a2,a1 // b2,b1 and
A47:  a1,a4 // b1,b4;
      now
        assume not M // N;
        then ex o st o in M & o in N by A27,A28,AFF_1:72;
        hence a3,a4 // b3,b4 by A26,A27,A28,A29,A30,A31,A32,A33,A34,A35,A36,
        A37,A38,A39,A40,A41,A42,A43,A44,A45,A46,A47,Def2;
      end;
      hence
      a3,a4 // b3,b4 by A25,A29,A30,A31,A32,A33,A34,A35,A36,A37,A38,A39,
      A40,A41,A42,A43,A44,A45,A46,A47,Def1;
    end;
    hence thesis by Def3;
  end;
  X is satisfying_Scherungssatz implies X is satisfying_major_Scherungssatz
  proof
    assume X is satisfying_Scherungssatz;
    then for o,a1,a2,a3,a4,b1,b2,b3,b4,M,N holds M is being_line & N is
being_line & o in M & o in N & a1 in M & a3 in M & b1 in M & b3 in M & a2 in N
& a4 in N & b2 in N & b4 in N & not a4 in M & not a2 in M & not b2 in M & not
b4 in M & not a1 in N & not a3 in N & not b1 in N & not b3 in N & a3,a2 // b3,
    b2 & a2,a1 // b2,b1 & a1,a4 // b1,b4 implies a3,a4 // b3,b4 by Def3;
    hence thesis by Def2;
  end;
  hence thesis by A1,A24;
end;

theorem Th3:
  X is satisfying_minor_indirect_Scherungssatz implies X is
  satisfying_minor_Scherungssatz
proof
  assume
A1: X is satisfying_minor_indirect_Scherungssatz;
  now
    let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
    assume that
A2: M // N and
A3: a1 in M and
A4: a3 in M and
A5: b1 in M and
A6: b3 in M and
A7: a2 in N and
A8: a4 in N and
A9: b2 in N and
A10: b4 in N and
A11: not a4 in M and
A12: not a2 in M and
A13: not b2 in M and
A14: not b4 in M and
A15: not a1 in N and
A16: not a3 in N and
A17: not b1 in N and
A18: not b3 in N and
A19: a3,a2 // b3,b2 and
A20: a2,a1 // b2,b1 and
A21: a1,a4 // b1,b4;
A22: N is being_line by A2,AFF_1:50;
    then consider d1 such that
    a2<>d1 and
A23: d1 in N by AFF_1:32;
A24: not d1 in M by A2,A8,A11,A23,AFF_1:59;
A25: M is being_line by A2,AFF_1:50;
    ex d2 st d2 in M & a2,a1 // d2,d1
    proof
      consider e1 such that
A26:  a1<>e1 and
A27:  e1 in M by A25,AFF_1:32;
      consider e2 such that
A28:  a2,a1 // d1,e2 and
A29:  d1<>e2 by DIRAF:47;
      not a1,e1 // d1,e2
      proof
        assume a1,e1 // d1,e2;
        then a1,e1 // a2,a1 by A28,A29,AFF_1:14;
        then a1,e1 // a1,a2 by AFF_1:13;
        then LIN a1,e1,a2 by AFF_1:def 1;
        hence contradiction by A3,A12,A25,A26,A27,AFF_1:39;
      end;
      then consider d2 such that
A30:  LIN a1,e1,d2 and
A31:  LIN d1,e2,d2 by AFF_1:74;
      take d2;
      d1,e2 // d1,d2 by A31,AFF_1:def 1;
      then a2,a1 // d1,d2 by A28,A29,AFF_1:14;
      hence thesis by A3,A25,A26,A27,A30,AFF_1:13,39;
    end;
    then consider d2 such that
A32: d2 in M and
A33: a2,a1 // d2,d1;
A34: not d2 in N by A2,A8,A11,A32,AFF_1:59;
    ex d3 st d3 in N & a3,a2 // d3,d2
    proof
      consider e1 such that
A35:  a2<>e1 and
A36:  e1 in N by A22,AFF_1:32;
      consider e2 such that
A37:  a3,a2 // d2,e2 and
A38:  d2<>e2 by DIRAF:47;
      not a2,e1 // d2,e2
      proof
        assume a2,e1 // d2,e2;
        then a2,e1 // a3,a2 by A37,A38,AFF_1:14;
        then a2,e1 // a2,a3 by AFF_1:13;
        then LIN a2,e1,a3 by AFF_1:def 1;
        hence contradiction by A7,A16,A22,A35,A36,AFF_1:39;
      end;
      then consider d3 such that
A39:  LIN a2,e1,d3 and
A40:  LIN d2,e2,d3 by AFF_1:74;
      take d3;
      d2,e2 // d2,d3 by A40,AFF_1:def 1;
      then a3,a2 // d2,d3 by A37,A38,AFF_1:14;
      hence thesis by A7,A22,A35,A36,A39,AFF_1:13,39;
    end;
    then consider d3 such that
A41: d3 in N and
A42: a3,a2 // d3,d2;
A43: not d3 in M by A2,A8,A11,A41,AFF_1:59;
    ex d4 st d4 in M & a1,a4 // d1,d4
    proof
      consider e1 such that
A44:  a1<>e1 and
A45:  e1 in M by A25,AFF_1:32;
      consider e2 such that
A46:  a1,a4 // d1,e2 and
A47:  d1<>e2 by DIRAF:47;
      not a1,e1 // d1,e2
      proof
        assume a1,e1 // d1,e2;
        then a1,e1 // a1,a4 by A46,A47,AFF_1:14;
        then LIN a1,e1,a4 by AFF_1:def 1;
        hence contradiction by A3,A11,A25,A44,A45,AFF_1:39;
      end;
      then consider d4 such that
A48:  LIN a1,e1,d4 and
A49:  LIN d1,e2,d4 by AFF_1:74;
      take d4;
      d1,e2 // d1,d4 by A49,AFF_1:def 1;
      hence thesis by A3,A25,A44,A45,A46,A47,A48,AFF_1:14,39;
    end;
    then consider d4 such that
A50: d4 in M and
A51: a1,a4 // d1,d4;
A52: not d4 in N by A2,A8,A11,A50,AFF_1:59;
A53: b2,b1 // d2,d1 by A3,A12,A20,A33,AFF_1:14;
A54: b1,b4 // d1,d4 by A3,A11,A21,A51,AFF_1:14;
    b3,b2 // d3,d2 by A4,A12,A19,A42,AFF_1:14;
    then
A55: b3,b4 // d3,d4 by A1,A2,A5,A6,A9,A10,A13,A14,A17,A18,A23,A32,A41,A50
    ,A24,A43,A34,A52,A53,A54,Def5;
    a3,a4 // d3,d4 by A1,A2,A3,A4,A7,A8,A11,A12,A15,A16,A23,A32,A33,A41,
    A42,A50,A51,A24,A43,A34,A52,Def5;
    hence a3,a4 // b3,b4 by A50,A43,A55,AFF_1:14;
  end;
  hence thesis by Def1;
end;

theorem Th4:
  X is satisfying_major_indirect_Scherungssatz implies X is
  satisfying_major_Scherungssatz
proof
  assume
A1: X is satisfying_major_indirect_Scherungssatz;
  now
    let o,a1,a2,a3,a4,b1,b2,b3,b4,M,N;
    assume that
A2: M is being_line and
A3: N is being_line and
A4: o in M and
A5: o in N and
A6: a1 in M and
A7: a3 in M and
A8: b1 in M and
A9: b3 in M and
A10: a2 in N and
A11: a4 in N and
A12: b2 in N and
A13: b4 in N and
A14: not a4 in M and
A15: not a2 in M and
A16: not b2 in M and
A17: not b4 in M and
A18: not a1 in N and
A19: not a3 in N and
A20: not b1 in N and
A21: not b3 in N and
A22: a3,a2 // b3,b2 and
A23: a2,a1 // b2,b1 and
A24: a1,a4 // b1,b4;
A25: now
      assume that
A26:  a1<>a3 and
A27:  a2<>a4;
      consider d1 such that
A28:  o<>d1 and
A29:  d1 in N by A4,A11,A14;
A30:  ex d4 st d4 in M & a1,a4 // d1,d4
      proof
        consider e1 such that
A31:    o<>e1 and
A32:    e1 in M by A5,A6,A18;
        consider e2 such that
A33:    a1,a4 // d1,e2 and
A34:    d1<>e2 by DIRAF:47;
        not o,e1 // d1,e2
        proof
          assume o,e1 // d1,e2;
          then
A35:      o,e1 // a1,a4 by A33,A34,AFF_1:14;
          o,e1 // a1,a3 by A2,A4,A6,A7,A32,AFF_1:53,55;
          then a1,a3 // a1,a4 by A31,A35,AFF_1:14;
          then LIN a1,a3,a4 by AFF_1:def 1;
          hence contradiction by A2,A6,A7,A14,A26,AFF_1:39;
        end;
        then consider d4 such that
A36:    LIN o,e1,d4 and
A37:    LIN d1,e2,d4 by AFF_1:74;
        take d4;
        d1,e2 // d1,d4 by A37,AFF_1:def 1;
        hence thesis by A2,A4,A31,A32,A33,A34,A36,AFF_1:14,39;
      end;
A38:  ex d2 st d2 in M & a2,a1 // d2,d1
      proof
        consider e1 such that
A39:    o<>e1 and
A40:    e1 in M by A5,A6,A18;
        consider e2 such that
A41:    a2,a1 // d1,e2 and
A42:    d1<>e2 by DIRAF:47;
        not o,e1 // d1,e2
        proof
          assume o,e1 // d1,e2;
          then
A43:      o,e1 // a2,a1 by A41,A42,AFF_1:14;
          o,e1 // a1,a3 by A2,A4,A6,A7,A40,AFF_1:53,55;
          then a1,a3 // a2,a1 by A39,A43,AFF_1:14;
          then a1,a3 // a1,a2 by AFF_1:13;
          then LIN a1,a3,a2 by AFF_1:def 1;
          hence contradiction by A2,A6,A7,A15,A26,AFF_1:39;
        end;
        then consider d2 such that
A44:    LIN o,e1,d2 and
A45:    LIN d1,e2,d2 by AFF_1:74;
        take d2;
        d1,e2 // d1,d2 by A45,AFF_1:def 1;
        then a2,a1 // d1,d2 by A41,A42,AFF_1:14;
        hence thesis by A2,A4,A39,A40,A44,AFF_1:13,39;
      end;
      consider d4 such that
A46:  d4 in M and
A47:  a1,a4 // d1,d4 by A30;
      consider d2 such that
A48:  d2 in M and
A49:  a2,a1 // d2,d1 by A38;
A50:  b2,b1 // d2,d1 by A6,A15,A23,A49,AFF_1:14;
      ex d3 st d3 in N & a3,a2 // d3,d2
      proof
        consider e1 such that
A51:    o<>e1 and
A52:    e1 in N by A4,A11,A14;
        consider e2 such that
A53:    a3,a2 // d2,e2 and
A54:    d2<>e2 by DIRAF:47;
        not o,e1 // d2,e2
        proof
          assume o,e1 // d2,e2;
          then
A55:      o,e1 // a3,a2 by A53,A54,AFF_1:14;
          o,e1 // a2,a4 by A3,A5,A10,A11,A52,AFF_1:53,55;
          then a2,a4 // a3,a2 by A51,A55,AFF_1:14;
          then a2,a4 // a2,a3 by AFF_1:13;
          then LIN a2,a4,a3 by AFF_1:def 1;
          hence contradiction by A3,A10,A11,A19,A27,AFF_1:39;
        end;
        then consider d3 such that
A56:    LIN o,e1,d3 and
A57:    LIN d2,e2,d3 by AFF_1:74;
        take d3;
        d2,e2 // d2,d3 by A57,AFF_1:def 1;
        then a3,a2 // d2,d3 by A53,A54,AFF_1:14;
        hence thesis by A3,A5,A51,A52,A56,AFF_1:13,39;
      end;
      then consider d3 such that
A58:  d3 in N and
A59:  a3,a2 // d3,d2;
A60:  d2<>o & d3<>o & d4<>o
      proof
A61:    now
          assume
A62:      d4=o;
          d1,o // a2,a4 by A3,A5,A10,A11,A29,AFF_1:53,55;
          then a1,a4 // a2,a4 by A28,A47,A62,AFF_1:14;
          then a4,a2 // a4,a1 by AFF_1:13;
          then LIN a4, a2,a1 by AFF_1:def 1;
          hence contradiction by A3,A10,A11,A18,A27,AFF_1:39;
        end;
A63:    now
          assume
A64:      d2=o;
          o,d1 // a2,a4 by A3,A5,A10,A11,A29,AFF_1:53,55;
          then a2,a4 // a2,a1 by A28,A49,A64,AFF_1:14;
          then LIN a2,a4,a1 by AFF_1:def 1;
          hence contradiction by A3,A10,A11,A18,A27,AFF_1:39;
        end;
A65:    now
          assume
A66:      d3=o;
          o,d2 // a3,a1 by A2,A4,A6,A7,A48,AFF_1:53,55;
          then a3,a1 // a3,a2 by A59,A63,A66,AFF_1:14;
          then LIN a3,a1,a2 by AFF_1:def 1;
          hence contradiction by A2,A6,A7,A15,A26,AFF_1:39;
        end;
        assume not thesis;
        hence contradiction by A63,A65,A61;
      end;
A67:  not d1 in M & not d3 in M & not d2 in N & not d4 in N
      proof
        assume not thesis;
        then
A68:    o,d1 // M or o,d3 // M or o,d2 // N or o,d4 // N by A2,A3,A4,A5
        ,AFF_1:66;
A69:    o,d3 // N by A3,A5,A58,AFF_1:66;
A70:    o,d4 // M by A2,A4,A46,AFF_1:66;
A71:    o,d2 // M by A2,A4,A48,AFF_1:66;
        o,d1 // N by A3,A5,A29,AFF_1:66;
        hence contradiction by A4,A5,A11,A14,A28,A60,A68,A69,A71,A70
        ,AFF_1:59,67;
      end;
A72:  b1,b4 // d1,d4 by A6,A14,A24,A47,AFF_1:14;
A73:  d1<>d2 & d2<>d3 & d3<>d4 & d4<>d1
      proof
        assume not thesis;
        then
A74:    o,d1 // M or o,d3 // M by A2,A4,A48,A46,AFF_1:66;
A75:    o,d3 // N by A3,A5,A58,AFF_1:66;
        o,d1 // N by A3,A5,A29,AFF_1:66;
        hence contradiction by A4,A5,A11,A14,A28,A60,A74,A75, AFF_1:59,
        67;
      end;
      b3,b2 // d3,d2 by A7,A15,A22,A59,AFF_1:14;
      then
A76:  b3,b4 // d3,d4 by A1,A2,A3,A4,A5,A8,A9,A12,A13,A16,A17,A20,A21,
      A29,A48,A58,A46,A67,A50,A72,Def6;
      a3,a4 // d3,d4 by A1,A2,A3,A4,A5,A6,A7,A10,A11,A14,A15,A18,A19,A29
      ,A48,A49,A58,A59,A46,A47,A67,Def6;
      hence a3,a4 // b3,b4 by A73,A76,AFF_1:14;
    end;
    now
A77:  now
        o,b2 // o,b4 by A3,A5,A12,A13,AFF_1:53,55;
        then
A78:    LIN o,b2,b4 by AFF_1:def 1;
        assume
A79:    a2=a4;
        then a1,a4 // b1,b2 by A23,AFF_1:13;
        then b1,b2 // b1,b4 by A6,A14,A24,AFF_1:14;
        hence a3,a4 // b3,b4 by A2,A4,A5,A8,A16,A20,A22,A79,A78, AFF_1:23,
        39;
      end;
A80:  now
        o,b1 // o,b3 by A2,A4,A8,A9,AFF_1:53,55;
        then
A81:    LIN o,b1,b3 by AFF_1:def 1;
        assume
A82:    a1=a3;
        then a2,a1 // b2,b3 by A22,AFF_1:13;
        then b2,b1 // b2,b3 by A6,A15,A23,AFF_1:14;
        hence a3,a4 // b3,b4 by A3,A4,A5,A12,A16,A20,A24,A82,A81, AFF_1:23,
        39;
      end;
      assume a1=a3 or a2=a4;
      hence a3,a4 // b3,b4 by A80,A77;
    end;
    hence a3,a4 // b3,b4 by A25;
  end;
  hence thesis by Def2;
end;

theorem
  X is satisfying_indirect_Scherungssatz implies X is satisfying_Scherungssatz
proof
  assume
A1: X is satisfying_indirect_Scherungssatz;
  then X is satisfying_major_indirect_Scherungssatz by Th1;
  then
A2: X is satisfying_major_Scherungssatz by Th4;
  X is satisfying_minor_indirect_Scherungssatz by A1,Th1;
  then X is satisfying_minor_Scherungssatz by Th3;
  hence thesis by A2,Th2;
end;

theorem Th6:
  X is translational implies X is satisfying_minor_Scherungssatz
proof
  assume
A1: X is translational;
  now
    let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
    assume that
A2: M // N and
A3: a1 in M and
A4: a3 in M and
A5: b1 in M and
A6: b3 in M and
A7: a2 in N and
A8: a4 in N and
A9: b2 in N and
A10: b4 in N and
A11: not a4 in M and
A12: not a2 in M and
A13: not b2 in M and
    not b4 in M and
A14: not a1 in N and
A15: not a3 in N and
A16: not b1 in N and
    not b3 in N and
A17: a3,a2 // b3,b2 and
A18: a2,a1 // b2,b1 and
A19: a1,a4 // b1,b4;
A20: M is being_line by A2,AFF_1:50;
A21: N is being_line by A2,AFF_1:50;
A22: now
      assume that
A23:  a1<>a3 and
A24:  a2<>a4;
A25:  now
        assume ex x,y,z st LIN x,y,z & x<>y & x<>z & y<>z;
        then consider d such that
A26:    LIN a1,a2,d and
A27:    a1<>d and
A28:    a2<>d by TRANSLAC:2;
A29:    ex Y st Y is being_line & d in Y & Y // M
        proof
          consider Y such that
A30:      d in Y and
A31:      M // Y by A20,AFF_1:63;
          take Y;
          thus thesis by A30,A31,AFF_1:50;
        end;
        LIN a2,a1,d by A26,AFF_1:15;
        then a2,a1 // a2,d by AFF_1:def 1;
        then
A32:    b2,b1 // a2,d by A3,A12,A18,AFF_1:14;
A33:    a2,a3 // b2,b3 by A17,AFF_1:13;
        consider Y such that
A34:    Y is being_line and
A35:    d in Y and
A36:    Y // M by A29;
A37:    Y // N by A2,A36,AFF_1:58;
A38:    N<>M & N<>Y & Y<>M
        proof
A39:      now
            LIN a2,d,a1 by A26,AFF_1:15;
            then
A40:        a2,d // a2,a1 by AFF_1:def 1;
            assume N=Y;
            then a2,d // a2,a4 by A7,A8,A34,A35,AFF_1:53,55;
            then a2,a4 // a2,a1 by A28,A40,AFF_1:14;
            then LIN a2,a4,a1 by AFF_1:def 1;
            hence contradiction by A7,A8,A14,A21,A24,AFF_1:39;
          end;
A41:      now
            assume Y=M;
            then
A42:        a1,d // a1,a3 by A3,A4,A35,A36,AFF_1:53;
            a1,a2 // a1,d by A26,AFF_1:def 1;
            then a1,a3 // a1,a2 by A27,A42,AFF_1:14;
            then LIN a1,a3,a2 by AFF_1:def 1;
            hence contradiction by A3,A4,A12,A20,A23,AFF_1:39;
          end;
          assume not thesis;
          hence contradiction by A8,A11,A39,A41;
        end;
A43:    Y // Y by A34,AFF_1:55;
        ex d1 st d1 in Y & b1,b2 // b1,d1
        proof
          consider e1 such that
A44:      d<>e1 and
A45:      e1 in Y by A34,AFF_1:32;
          consider e2 such that
A46:      b1,b2 // b1,e2 and
A47:      b1<>e2 by DIRAF:47;
          not d,e1 // b1,e2
          proof
            assume d,e1 // b1,e2;
            then d,e1 // b1,b2 by A46,A47,AFF_1:14;
            then
A48:        b1,b2 // Y by A35,A43,A44,A45,AFF_1:46,54;
            a1,a3 // Y by A3,A4,A36,AFF_1:54;
            then a1,a3 // b1,b2 by A34,A48,AFF_1:45;
            then a1,a3 // b2,b1 by AFF_1:13;
            then a1,a3 // a2,a1 by A5,A13,A18,AFF_1:14;
            then a1,a3 // a1,a2 by AFF_1:13;
            then LIN a1,a3,a2 by AFF_1:def 1;
            hence contradiction by A3,A4,A12,A20,A23,AFF_1:39;
          end;
          then consider d1 such that
A49:      LIN d,e1,d1 and
A50:      LIN b1,e2,d1 by AFF_1:74;
          take d1;
          b1,e2 // b1,d1 by A50,AFF_1:def 1;
          hence thesis by A34,A35,A44,A45,A46,A47,A49,AFF_1:14 ,39;
        end;
        then consider d1 such that
A51:    d1 in Y and
A52:    b1,b2 // b1,d1;
        a1,a2 // a1,d by A26,AFF_1:def 1;
        then a2,a1 // a1,d by AFF_1:13;
        then b2,b1 // a1,d by A3,A12,A18,AFF_1:14;
        then b1,b2 // a1,d by AFF_1:13;
        then a1,d // b1,d1 by A5,A13,A52,AFF_1:14;
        then
A53:    d,a4 // d1,b4 by A1,A2,A3,A5,A8,A10,A19,A20,A21,A34,A35,A36,
        A51,A38,AFF_2:def 11;
        LIN b1,b2,d1 by A52,AFF_1:def 1;
        then LIN b2,b1,d1 by AFF_1:15;
        then b2,b1 // b2,d1 by AFF_1:def 1;
        then
A54:    a2,d // b2,d1 by A5,A13,A32,AFF_1:14;
        N // Y by A2,A36,AFF_1:58;
        then d,a3 // d1,b3 by A1,A2,A4,A6,A7,A9,A20,A21,A34,A35,A51,
        A38,A54,A33,AFF_2:def 11;
        hence a3,a4 // b3,b4 by A1,A4,A6,A8,A10,A20,A21,A34,A35,A36,A51,
        A38,A53,A37,AFF_2:def 11;
      end;
      now
        assume
A55:    not ex x,y,z st LIN x,y,z & x<>y & x<>z & y<>z;
A56:    now
          assume
A57:      a1=b1;
          then
A58:      LIN a1,a4,b4 by A19,AFF_1:def 1;
          a1,a2 // a1,b2 by A18,A57,AFF_1:13;
          then
A59:      LIN a1,a2,b2 by AFF_1:def 1;
          a2,b2 // M by A2,A7,A9,AFF_1:54;
          then a2=b2 by A3,A12,A59,AFF_1:60;
          then a2,a3 // a2,b3 by A17,AFF_1:13;
          then
A60:      LIN a2,a3,b3 by AFF_1:def 1;
          a3,b3 // N by A2,A4,A6,AFF_1:54;
          then
A61:      a3=b3 by A7,A15,A60,AFF_1:60;
          a4,b4 // M by A2,A8,A10,AFF_1:54;
          then a4=b4 by A3,A11,A58,AFF_1:60;
          hence a3,a4 // b3,b4 by A61,AFF_1:11;
        end;
A62:    now
          assume
A63:      a3=b1;
A64:      now
A65:        a4<>b4
            proof
              assume a4=b4;
              then a4,a1 // a4,b1 by A19,AFF_1:13;
              then
A66:          LIN a4,a1,b1 by AFF_1:def 1;
              a1,b1 // N by A2,A3,A5,AFF_1:54;
              hence contradiction by A8,A14,A23,A63,A66,AFF_1:60;
            end;
            a2,a4 // a2,b4 by A7,A8,A10,A21,AFF_1:53,55;
            then LIN a2,a4,b4 by AFF_1:def 1;
            then
A67:        a2=b4 by A24,A55,A65;
            assume
A68:        a4=b2;
A69:        a3<>b3
            proof
              assume a3=b3;
              then
A70:          LIN a3,a2,b2 by A17,AFF_1:def 1;
              a2,b2 // M by A2,A7,A9,AFF_1:54;
              hence contradiction by A4,A12,A24,A68,A70,AFF_1:60;
            end;
            a1,a3 // a1,b3 by A3,A4,A6,A20,AFF_1:53,55;
            then LIN a1,a3,b3 by AFF_1:def 1;
            then
A71:        a1=b3 by A23,A55,A69;
            a3,a4 // b2,b1 by A63,A68,AFF_1:11;
            then a3,a4 // a2,a1 by A5,A13,A18,AFF_1:14;
            hence a3,a4 // b3,b4 by A71,A67,AFF_1:13;
          end;
A72:      now
            assume a2=b2;
            then
A73:        LIN a2,a1,b1 by A18,AFF_1:def 1;
            a1,b1 // N by A2,A3,A5,AFF_1:54;
            hence a3,a4 // b3,b4 by A7,A14,A56,A73,AFF_1:60;
          end;
          a2,a4 // a2,b2 by A7,A8,A9,A21,AFF_1:53,55;
          then LIN a2,a4,b2 by AFF_1:def 1;
          hence a3,a4 // b3,b4 by A24,A55,A64,A72;
        end;
        a1,a3 // a1,b1 by A3,A4,A5,A20,AFF_1:53,55;
        then LIN a1,a3,b1 by AFF_1:def 1;
        hence a3,a4 // b3,b4 by A23,A55,A56,A62;
      end;
      hence a3,a4 // b3,b4 by A25;
    end;
    now
A74:  now
        assume
A75:    a1=a3;
        b1=b3
        proof
          LIN a2,a1,a3 by A75,AFF_1:16;
          then a2,a1 // a2,a3 by AFF_1:def 1;
          then b2,b1 // a2,a3 by A3,A12,A18,AFF_1:14;
          then b2,b1 // a3,a2 by AFF_1:13;
          then b2,b1 // b3,b2 by A4,A12,A17,AFF_1:14;
          then b2,b1 // b2,b3 by AFF_1:13;
          then
A76:      LIN b2,b1,b3 by AFF_1:def 1;
          assume
A77:      b1<>b3;
          b1,b3 // N by A2,A5,A6,AFF_1:54;
          hence contradiction by A9,A16,A77,A76,AFF_1:60;
        end;
        hence a3,a4 // b3,b4 by A19,A75;
      end;
A78:  now
        assume
A79:    a2=a4;
        then LIN a1,a4,a2 by AFF_1:16;
        then a1,a4 // a1,a2 by AFF_1:def 1;
        then b1,b4 // a1,a2 by A3,A11,A19,AFF_1:14;
        then b1,b4 // a2,a1 by AFF_1:13;
        then b1,b4 // b2,b1 by A3,A12,A18,AFF_1:14;
        then b1,b2 // b1,b4 by AFF_1:13;
        then
A80:    LIN b1,b2,b4 by AFF_1:def 1;
        b2,b4 // M by A2,A9,A10,AFF_1:54;
        hence a3,a4 // b3,b4 by A5,A13,A17,A79,A80,AFF_1:60;
      end;
      assume a1=a3 or a2=a4;
      hence a3,a4 // b3,b4 by A74,A78;
    end;
    hence a3,a4 // b3,b4 by A22;
  end;
  hence thesis by Def1;
end;

theorem Th7:
  X is Desarguesian implies X is satisfying_major_Scherungssatz
proof
  assume
A1: X is Desarguesian;
  now
    let o,a1,a2,a3,a4,b1,b2,b3,b4,M,N;
    assume that
A2: M is being_line and
A3: N is being_line and
A4: o in M and
A5: o in N and
A6: a1 in M and
A7: a3 in M and
A8: b1 in M and
A9: b3 in M and
A10: a2 in N and
A11: a4 in N and
A12: b2 in N and
A13: b4 in N and
A14: not a4 in M and
A15: not a2 in M and
A16: not b2 in M and
    not b4 in M and
A17: not a1 in N and
A18: not a3 in N and
A19: not b1 in N and
    not b3 in N and
A20: a3,a2 // b3,b2 and
A21: a2,a1 // b2,b1 and
A22: a1,a4 // b1,b4;
A23: now
A24:  now
        assume ex x,y,z st LIN x,y,z & x<>y & x<>z & y<>z;
        then consider d such that
A25:    LIN a1,a2,d and
A26:    a1<>d and
A27:    a2<>d by TRANSLAC:2;
        LIN o,d,d by AFF_1:16;
        then consider Y such that
A28:    Y is being_line and
A29:    o in Y and
A30:    d in Y and
        d in Y by AFF_1:33;
        a1,a2 // a1,d by A25,AFF_1:def 1;
        then a2,a1 // a1,d by AFF_1:13;
        then
A31:    b2,b1 // a1,d by A6,A15,A21,AFF_1:14;
A32:    N<>M & N<>Y & M<>Y
        proof
A33:      now
A34:        LIN a1,d,a2 by A25,AFF_1:15;
            assume
A35:        N=Y or M=Y;
            LIN a2,d,a1 by A25,AFF_1:15;
            hence contradiction by A2,A3,A6,A10,A15,A17,A26,A27,A30,A35,
            A34,AFF_1:39;
          end;
          assume not thesis;
          hence contradiction by A11,A14,A33;
        end;
A36:    ex d1 st LIN b1,b2,d1 & d1 in Y
        proof
          not b1,b2 // o,d
          proof
A37:        o<>d
            proof
              assume o=d;
              then LIN o,a1,a2 by A25,AFF_1:15;
              hence contradiction by A2,A4,A5,A6,A15,A17,AFF_1:39;
            end;
            assume b1,b2 // o,d;
            then b2,b1 // o,d by AFF_1:13;
            then
A38:        a2,a1 // o,d by A8,A16,A21,AFF_1:14;
            a1,a2 // a1,d by A25,AFF_1:def 1;
            then a2,a1 // a1,d by AFF_1:13;
            then a1,d // o,d by A6,A15,A38,AFF_1:14;
            then d,o // d,a1 by AFF_1:13;
            then LIN d,o,a1 by AFF_1:def 1;
            then LIN o,d,a1 by AFF_1:15;
            then o,d // o,a1 by AFF_1:def 1;
            then
A39:        Y // M by A2,A4,A5,A6,A17,A28,A29,A30,A37,AFF_1:52;
            LIN a2,a1,d by A25,AFF_1:15;
            then a2,a1 // a2,d by AFF_1:def 1;
            then a2,d // o,d by A6,A15,A38,AFF_1:14;
            then d,a2 // d,o by AFF_1:13;
            then LIN d,a2,o by AFF_1:def 1;
            then LIN o,d,a2 by AFF_1:15;
            then o,d // o,a2 by AFF_1:def 1;
            then Y // N by A3,A4,A5,A10,A15,A28,A29,A30,A37,AFF_1:52;
            then M // N by A39,AFF_1:58;
            hence contradiction by A4,A5,A11,A14,AFF_1:59;
          end;
          then consider d1 such that
A40:      LIN b1,b2,d1 and
A41:      LIN o,d,d1 by AFF_1:74;
          take d1;
          o<>d
          proof
            assume o=d;
            then LIN o,a1,a2 by A25,AFF_1:15;
