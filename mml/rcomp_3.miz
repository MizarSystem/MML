      a <= x & x < b by A1,A3,A2,RCOMP_2:3;
      then a < x & x < b by A4,REAL_1:def 5;
      hence thesis by RCOMP_1:47;
    end;
    ].a,b.[ c= [.a,b.[ by TOPREALA:20;
