theorem Th4:
  r < s implies lower_bound [.r,s.[ = r
proof
  set X = [.r,s.[;
  assume
A1: r < s;
  then
A2: X is non empty by XXREAL_1:3;
A3: for a st a in X holds r <= a by XXREAL_1:3;
  reconsider r, s as Real by XREAL_0:def 1;
A4: r < (r+s)/2 & (r+s)/2 < s by A1,XREAL_1:228;
  for b st 0 < b ex a st a in X & a < r+b
  proof
    let b such that
A5: 0 < b and
A6: for a st a in X holds a >= r+b;
    reconsider b as Real by XREAL_0:def 1;
    per cases;
    suppose r + b > s;
      then (r+s)/2 in X & (r+s)/2 < r+b by A4,XXREAL_0:2,XXREAL_1:3;
      hence thesis by A6;
    end;
    suppose
A7:   r + b <= s;
A8:   r < r + b by A5,XREAL_1:31;
      then
A9:   r < (r+(r+b))/2 by XREAL_1:228;
A10:  (r+(r+b))/2 < r+b by A8,XREAL_1:228;
      then (r+(r+b))/2 < s by A7,XXREAL_0:2;
      then (r+(r+b))/2 in X by A9,XXREAL_1:3;
      hence thesis by A6,A10;
    end;
  end;
  hence thesis by A2,A3,SEQ_4:def 5;
end;

theorem Th5:
  r < s implies upper_bound [.r,s.[ = s
proof
  set X = [.r,s.[;
  assume
A1: r < s;
  then
A2: X is non empty by XXREAL_1:3;
A3: for a st a in X holds a <= s by XXREAL_1:3;
  reconsider r, s as Real by XREAL_0:def 1;
A4: r < (r+s)/2 & (r+s)/2 < s by A1,XREAL_1:228;
  for b st 0 < b ex a st a in X & s-b < a
  proof
    let b such that
A5: 0 < b and
A6: for a st a in X holds a <= s-b;
    reconsider b as Real by XREAL_0:def 1;
    per cases;
    suppose s - b <= r;
      then (r+s)/2 in X & (r+s)/2 > s-b by A4,XXREAL_0:2,XXREAL_1:3;
      hence thesis by A6;
    end;
    suppose
A7:   s - b > r;
A8:   s - b < s - 0 by A5,XREAL_1:17;
      then s-b < (s+(s-b))/2 by XREAL_1:228;
      then
A9:   r < (s+(s-b))/2 by A7,XXREAL_0:2;
A10:  (s+(s-b))/2 > s-b by A8,XREAL_1:228;
      (s+(s-b))/2 < s by A8,XREAL_1:228;
      then (s+(s-b))/2 in X by A9,XXREAL_1:3;
      hence thesis by A6,A10;
    end;
  end;
  hence thesis by A2,A3,SEQ_4:def 4;
end;

