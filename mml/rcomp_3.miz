begin :: Halflines

canceled 12;

theorem Th20:
  a <= b implies
  [.a,b.] /\ (left_closed_halfline(a) \/ right_closed_halfline(b)) = {a,b}
proof
  set A = left_closed_halfline(a);
  set B = right_closed_halfline(b);
  assume
A1: a <= b;
  thus [.a,b.] /\ (A \/ B) c= {a,b}
  proof
    let x be set;
    assume
A2: x in [.a,b.] /\ (A \/ B);
    then reconsider x as Real;
    x in A \/ B by A2,XBOOLE_0:def 4;
    then x in A or x in B by XBOOLE_0:def 3;
    then
A3: x <= a or x >= b by XXREAL_1:234,236;
    x in [.a,b.] by A2,XBOOLE_0:def 4;
    then a <= x & x <= b by XXREAL_1:1;
    then x = a or x = b by A3,XXREAL_0:1;
    hence thesis by TARSKI:def 2;
  end;
  let x be set;
  assume x in {a,b};
  then
A4: x = a or x = b by TARSKI:def 2;
A5: a in [.a,b.] & b in [.a,b.] by A1,XXREAL_1:1;
  a in A & b in B by XXREAL_1:234,236;
  then a in A \/ B & b in A \/ B by XBOOLE_0:def 3;
  hence thesis by A4,A5,XBOOLE_0:def 4;
end;

Lm2: now
  let a;
  assume left_open_halfline(a) is bounded_below;
  then consider b such that
A1: for r st r in left_open_halfline(a) holds b <= r by SEQ_4:def 2;
A2: a-1 < a-0 by XREAL_1:17;
  then a-1 in left_open_halfline(a) by XXREAL_1:233;
  then b-1 < a-1-0 by A1,XREAL_1:17;
  then b-1 < a by A2,XXREAL_0:2;
  then b-1 in left_open_halfline(a) by XXREAL_1:233;
  then b-0 <= b-1 by A1;
  hence contradiction by XREAL_1:17;
end;

Lm3: now
  let a;
  assume right_open_halfline(a) is bounded_above;
  then consider b such that
A1: for r st r in right_open_halfline(a) holds r <= b by SEQ_4:def 1;
A2: a+0 < a+1 by XREAL_1:8;
  then a+1 in right_open_halfline(a) by XXREAL_1:235;
  then a+1 <= b by A1;
  then a+1+0 <= b+1 by XREAL_1:9;
  then a < b+1 by A2,XXREAL_0:2;
  then b+1 in right_open_halfline(a) by XXREAL_1:235;
  then b+1 <= b+0 by A1;
  hence contradiction by XREAL_1:8;
end;

registration
  let a;
  cluster left_closed_halfline(a) -> non bounded_below bounded_above connected;
  coherence
  proof
    set A = left_closed_halfline(a);
X:  left_open_halfline(a) c= A by XXREAL_1:21;
    left_open_halfline a is not bounded_below by Lm2;
    hence A is non bounded_below by X,XXREAL_2:44;
    thus A is bounded_above
    proof
      take a;
      thus thesis by XXREAL_1:234;
    end;
    let r, s;
    assume r in A;
    assume s in A;
    then
A1: s <= a by XXREAL_1:234;
    let x be set;
    assume
A2: x in [.r,s.];
    then reconsider x as Real;
    x <= s by A2,XXREAL_1:1;
    then x <= a by A1,XXREAL_0:2;
    hence thesis by XXREAL_1:234;
  end;
  cluster left_open_halfline(a) -> non bounded_below bounded_above connected;
  coherence
  proof
    set A = left_open_halfline(a);
    thus A is non bounded_below by Lm2;
    thus A is bounded_above
    proof
      take a;
      thus thesis by XXREAL_1:233;
    end;
    let r, s;
    assume r in A;
    assume s in A;
    then
A3: s < a by XXREAL_1:233;
    let x be set;
    assume
A4: x in [.r,s.];
    then reconsider x as Real;
    x <= s by A4,XXREAL_1:1;
    then x < a by A3,XXREAL_0:2;
    hence thesis by XXREAL_1:233;
  end;
  cluster right_closed_halfline(a) ->
    bounded_below non bounded_above connected;
  coherence
  proof
    set A = right_closed_halfline(a);
    thus A is bounded_below
    proof
      take a;
      thus thesis by XXREAL_1:236;
    end;
X:  right_open_halfline(a) c= A by XXREAL_1:22;
    right_open_halfline a is not bounded_above by Lm3;
    hence A is non bounded_above by X,XXREAL_2:43;
    let r, s;
    assume r in A;
    then
A5: a <= r by XXREAL_1:236;
    assume s in A;
    let x be set;
    assume
A6: x in [.r,s.];
    then reconsider x as Real;
    r <= x by A6,XXREAL_1:1;
    then a <= x by A5,XXREAL_0:2;
    hence thesis by XXREAL_1:236;
  end;
  cluster right_open_halfline(a) -> bounded_below non bounded_above connected;
  coherence
  proof
    set A = right_open_halfline(a);
    thus A is bounded_below
    proof
      take a;
      thus thesis by XXREAL_1:235;
    end;
    thus A is non bounded_above by Lm3;
    let r, s;
    assume r in A;
    then
A7: a < r by XXREAL_1:235;
    assume s in A;
    let x be set;
    assume
A8: x in [.r,s.];
    then reconsider x as Real;
    r <= x by A8,XXREAL_1:1;
    then a < x by A7,XXREAL_0:2;
    hence thesis by XXREAL_1:235;
  end;
end;

theorem Th21:
  upper_bound left_closed_halfline(a) = a
proof
  set X = left_closed_halfline(a);
A1: for r st r in X holds r <= a by XXREAL_1:234;
  for s st 0 < s ex r st r in X & a-s < r
  proof
    let s such that
A2: 0 < s;
    take a;
    thus a in X by XXREAL_1:234;
    a-s < a-0 by A2,XREAL_1:17;
    hence thesis;
  end;
  hence thesis by A1,SEQ_4:def 4;
end;

theorem Th22:
  upper_bound left_open_halfline(a) = a
proof
  set X = left_open_halfline(a);
A1: for r st r in X holds r <= a by XXREAL_1:233;
  for s st 0 < s ex r st r in X & a-s < r
  proof
    let s;
    assume 0 < s;
    then a-s < a-0 by XREAL_1:17;
    then
A2: a-s < (a-s+a)/2 & (a-s+a)/2 < a by XREAL_1:228;
    take (a-s+a)/2;
    thus thesis by A2,XXREAL_1:233;
  end;
  hence thesis by A1,SEQ_4:def 4;
end;

theorem Th23:
  lower_bound right_closed_halfline(a) = a
proof
  set X = right_closed_halfline(a);
A1: for r st r in X holds a <= r by XXREAL_1:236;
  for s st 0 < s ex r st r in X & r < a+s
  proof
    let s such that
A2: 0 < s;
    take a;
    thus a in X by XXREAL_1:236;
    a+0 < a+s by A2,XREAL_1:8;
    hence thesis;
  end;
  hence thesis by A1,SEQ_4:def 5;
end;

theorem Th24:
  lower_bound right_open_halfline(a) = a
proof
  set X = right_open_halfline(a);
A1: for r st r in X holds a <= r by XXREAL_1:235;
  for s st 0 < s ex r st r in X & r < a+s
  proof
    let s;
    assume 0 < s;
    then a+0 < a+s by XREAL_1:8;
    then
A2: a < (a+(a+s))/2 & (a+(a+s))/2 < a+s by XREAL_1:228;
    take (a+a+s)/2;
    thus thesis by A2,XXREAL_1:235;
  end;
  hence thesis by A1,SEQ_4:def 5;
end;

begin :: Connectedness

registration
  cluster [#]REAL -> connected non bounded_below non bounded_above;
  coherence;
end;

theorem Th25:
  for X being bounded connected Subset of REAL st
  lower_bound X in X & upper_bound X in X
  holds X = [.lower_bound X,upper_bound X.]
proof
  let X be bounded connected Subset of REAL such that
A1: lower_bound X in X & upper_bound X in X;
  thus X c= [.lower_bound X,upper_bound X.] by TOPREAL6:24;
  thus thesis by A1,JCT_MISC:def 1;
end;

theorem Th26:
  for X being bounded Subset of REAL st not lower_bound X in X holds
  X c= ].lower_bound X,upper_bound X.]
proof
  let X be bounded Subset of REAL such that
A1: not lower_bound X in X;
  let x be set;
  assume
A2: x in X;
  then reconsider x as Real;
A3: x <= upper_bound X by A2,SEQ_4:def 4;
  lower_bound X <= x by A2,SEQ_4:def 5;
  then lower_bound X < x by A1,A2,XXREAL_0:1;
  hence thesis by A3,XXREAL_1:2;
end;

theorem Th27:
  for X being bounded connected Subset of REAL st
  not lower_bound X in X & upper_bound X in X
  holds X = ].lower_bound X,upper_bound X.]
proof
  let X be bounded connected Subset of REAL such that
A1: not lower_bound X in X and
A2: upper_bound X in X;
  thus X c= ].lower_bound X,upper_bound X.] by A1,Th26;
  let x be set;
  assume
A3: x in ].lower_bound X,upper_bound X.];
  then reconsider x as Real;
A4: x <= upper_bound X by A3,XXREAL_1:2;
  lower_bound X < x by A3,XXREAL_1:2;
  then lower_bound X - lower_bound X < x - lower_bound X by XREAL_1:16;
  then consider r such that
A5: r in X and
A6: r < lower_bound X + (x - lower_bound X) by A2,SEQ_4:def 5;
A7: [.r,upper_bound X.] c= X by A2,A5,JCT_MISC:def 1;
  x in [.r,upper_bound X.] by A4,A6,XXREAL_1:1;
  hence thesis by A7;
end;

theorem Th28:
  for X being bounded Subset of REAL st not upper_bound X in X holds
  X c= [.lower_bound X,upper_bound X.[
proof
  let X be bounded Subset of REAL such that
A1: not upper_bound X in X;
  let x be set;
  assume
A2: x in X;
  then reconsider x as Real;
A3: lower_bound X <= x by A2,SEQ_4:def 5;
  x <= upper_bound X by A2,SEQ_4:def 4;
  then x < upper_bound X by A1,A2,XXREAL_0:1;
  hence thesis by A3,XXREAL_1:3;
end;

theorem Th29:
  for X being bounded connected Subset of REAL st
  lower_bound X in X & not upper_bound X in X
  holds X = [.lower_bound X,upper_bound X.[
proof
  let X be bounded connected Subset of REAL such that
A1: lower_bound X in X and
A2: not upper_bound X in X;
  thus X c= [.lower_bound X,upper_bound X.[ by A2,Th28;
  let x be set;
  assume
A3: x in [.lower_bound X,upper_bound X.[;
  then reconsider x as Real;
A4: lower_bound X <= x by A3,XXREAL_1:3;
  x < upper_bound X by A3,XXREAL_1:3;
  then x - x < upper_bound X - x by XREAL_1:16;
  then consider r such that
A5: r in X and
A6: upper_bound X - (upper_bound X - x) < r by A1,SEQ_4:def 4;
A7: [.lower_bound X,r.] c= X by A1,A5,JCT_MISC:def 1;
  x in [.lower_bound X,r.] by A4,A6,XXREAL_1:1;
  hence thesis by A7;
end;

theorem Th30:
  for X being bounded Subset of REAL st
  not lower_bound X in X & not upper_bound X in X
  holds X c= ].lower_bound X,upper_bound X.[
proof
  let X be bounded Subset of REAL such that
A1: not lower_bound X in X and
A2: not upper_bound X in X;
  let x be set;
  assume
A3: x in X;
  then reconsider x as Real;
  lower_bound X <= x & x <= upper_bound X by A3,SEQ_4:def 4,def 5;
  then lower_bound X < x & x < upper_bound X by A1,A2,A3,XXREAL_0:1;
  hence thesis by XXREAL_1:4;
end;

theorem Th31:
  for X being non empty bounded connected Subset of REAL st
  not lower_bound X in X & not upper_bound X in X
  holds X = ].lower_bound X,upper_bound X.[
proof
  let X be non empty bounded connected Subset of REAL such that
A1: not lower_bound X in X and
A2: not upper_bound X in X;
  thus X c= ].lower_bound X,upper_bound X.[ by A1,A2,Th30;
  let x be set;
  assume
A3: x in ].lower_bound X,upper_bound X.[;
  then reconsider x as Real;
  lower_bound X < x by A3,XXREAL_1:4;
  then lower_bound X - lower_bound X < x - lower_bound X by XREAL_1:16;
  then consider s such that
A4: s in X and
A5: s < lower_bound X + (x - lower_bound X) by SEQ_4:def 5;
  x < upper_bound X by A3,XXREAL_1:4;
  then x - x < upper_bound X - x by XREAL_1:16;
  then consider r such that
A6: r in X and
A7: upper_bound X - (upper_bound X - x) < r by SEQ_4:def 4;
A8: [.s,r.] c= X by A4,A6,JCT_MISC:def 1;
  x in [.s,r.] by A5,A7,XXREAL_1:1;
  hence thesis by A8;
end;

theorem Th32:
  for X being Subset of REAL st X is bounded_above holds
  X c= left_closed_halfline(upper_bound X)
proof
  let X be Subset of REAL such that
A1: X is bounded_above;
  let x be set;
  assume
A2: x in X;
  then reconsider x as Real;
  x <= upper_bound X by A1,A2,SEQ_4:def 4;
  hence thesis by XXREAL_1:234;
end;

theorem Th33:
  for X being connected Subset of REAL st
  X is not bounded_below & X is bounded_above & upper_bound X in X holds
  X = left_closed_halfline(upper_bound X)
proof
  let X be connected Subset of REAL such that
A1: X is not bounded_below and
A2: X is bounded_above and
A3: upper_bound X in X;
  thus X c= left_closed_halfline(upper_bound X) by A2,Th32;
  let x be set;
  assume
A4: x in left_closed_halfline(upper_bound X);
  then reconsider x as Real;
  consider r such that
A5: r in X and
A6: x > r by A1,SEQ_4:def 2;
A7: x <= upper_bound X by A4,XXREAL_1:234;
A8: [.r,upper_bound X.] c= X by A3,A5,JCT_MISC:def 1;
  x in [.r,upper_bound X.] by A6,A7,XXREAL_1:1;
  hence thesis by A8;
end;

theorem Th34:
  for X being Subset of REAL
   st X is bounded_above & not upper_bound X in X holds
  X c= left_open_halfline(upper_bound X)
proof
  let X be Subset of REAL such that
A1: X is bounded_above and
A2: not upper_bound X in X;
  let x be set;
  assume
A3: x in X;
  then reconsider x as Real;
  x <= upper_bound X by A1,A3,SEQ_4:def 4;
  then x < upper_bound X by A2,A3,XXREAL_0:1;
  hence thesis by XXREAL_1:233;
end;

theorem Th35:
  for X being non empty connected Subset of REAL st
  X is not bounded_below & X is bounded_above & not upper_bound X in X holds
  X = left_open_halfline(upper_bound X)
proof
  let X be non empty connected Subset of REAL such that
A1: X is not bounded_below and
A2: X is bounded_above and
A3: not upper_bound X in X;
  thus X c= left_open_halfline(upper_bound X) by A2,A3,Th34;
  let x be set;
  assume
A4: x in left_open_halfline(upper_bound X);
  then reconsider x as Real;
  consider r such that
A5: r in X and
A6: x > r by A1,SEQ_4:def 2;
  x < upper_bound X by A4,XXREAL_1:233;
  then x - x < upper_bound X - x by XREAL_1:16;
  then consider s such that
A7: s in X and
A8: upper_bound X - (upper_bound X - x) < s by A2,SEQ_4:def 4;
A9: [.r,s.] c= X by A5,A7,JCT_MISC:def 1;
  x in [.r,s.] by A6,A8,XXREAL_1:1;
  hence thesis by A9;
end;

theorem Th36:
  for X being Subset of REAL st X is bounded_below holds
  X c= right_closed_halfline(lower_bound X)
proof
  let X be Subset of REAL such that
A1: X is bounded_below;
  let x be set;
  assume
A2: x in X;
  then reconsider x as Real;
  lower_bound X <= x by A1,A2,SEQ_4:def 5;
  hence thesis by XXREAL_1:236;
end;

theorem Th37:
  for X being connected Subset of REAL st
  X is bounded_below & X is not bounded_above & lower_bound X in X holds
  X = right_closed_halfline(lower_bound X)
proof
  let X be connected Subset of REAL such that
A1: X is bounded_below and
A2: X is not bounded_above and
A3: lower_bound X in X;
  thus X c= right_closed_halfline(lower_bound X) by A1,Th36;
  let x be set;
  assume
A4: x in right_closed_halfline(lower_bound X);
  then reconsider x as Real;
  consider r such that
A5: r in X and
A6: x < r by A2,SEQ_4:def 1;
A7: lower_bound X <= x by A4,XXREAL_1:236;
A8: [.lower_bound X,r.] c= X by A3,A5,JCT_MISC:def 1;
  x in [.lower_bound X,r.] by A6,A7,XXREAL_1:1;
  hence thesis by A8;
end;

theorem Th38:
  for X being Subset of REAL st
  X is bounded_below & not lower_bound X in X holds
  X c= right_open_halfline(lower_bound X)
proof
  let X be Subset of REAL such that
A1: X is bounded_below and
A2: not lower_bound X in X;
  let x be set;
  assume
A3: x in X;
  then reconsider x as Real;
  lower_bound X <= x by A1,A3,SEQ_4:def 5;
  then lower_bound X < x by A2,A3,XXREAL_0:1;
  hence thesis by XXREAL_1:235;
end;

theorem Th39:
  for X being non empty connected Subset of REAL st
  X is bounded_below & X is not bounded_above & not lower_bound X in X holds
  X = right_open_halfline(lower_bound X)
proof
  let X be non empty connected Subset of REAL such that
A1: X is bounded_below and
A2: X is not bounded_above and
A3: not lower_bound X in X;
  thus X c= right_open_halfline(lower_bound X) by A1,A3,Th38;
  let x be set;
  assume
A4: x in right_open_halfline(lower_bound X);
  then reconsider x as Real;
  consider r such that
A5: r in X and
A6: x < r by A2,SEQ_4:def 1;
  lower_bound X < x by A4,XXREAL_1:235;
  then lower_bound X - lower_bound X < x - lower_bound X by XREAL_1:16;
  then consider s such that
A7: s in X and
A8: s < lower_bound X + (x - lower_bound X) by A1,SEQ_4:def 5;
A9: [.s,r.] c= X by A5,A7,JCT_MISC:def 1;
  x in [.s,r.] by A6,A8,XXREAL_1:1;
  hence thesis by A9;
end;

theorem Th40:
  for X being connected Subset of REAL st
  X is not bounded_above & X is not bounded_below holds X = REAL
proof
  let X be connected Subset of REAL;
  assume that
A1: X is not bounded_above and
A2: X is not bounded_below;
  thus X c= REAL;
  let x be set;
  assume x in REAL;
  then reconsider x as Real;
  consider r such that
A3: r in X and
A4: r > x by A1,SEQ_4:def 1;
  consider s such that
A5: s in X and
A6: s < x by A2,SEQ_4:def 2;
A7: [.s,r.] c= X by A3,A5,JCT_MISC:def 1;
  x in [.s,r.] by A4,A6,XXREAL_1:1;
  hence thesis by A7;
end;

theorem Th41:
  for X being connected Subset of REAL holds X is empty or X = REAL or
  (ex a st X = left_closed_halfline(a)) or
  (ex a st X = left_open_halfline(a)) or
  (ex a st X = right_closed_halfline(a)) or
  (ex a st X = right_open_halfline(a)) or (ex a, b st a <= b & X = [.a,b.]) or
  (ex a, b st a < b & X = [.a,b.[) or (ex a, b st a < b & X = ].a,b.]) or
  (ex a, b st a < b & X = ].a,b.[)
proof
  let X be connected Subset of REAL;
  assume X is non empty;
  then reconsider X as non empty connected Subset of REAL;
  per cases;
  suppose X is bounded;
    then reconsider X as non empty bounded connected Subset of REAL;
    per cases;
    suppose X is trivial;
      then consider x being set such that
A1:   X = {x} by REALSET1:def 4;
      x in X by A1,TARSKI:def 1;
      then reconsider x as Real;
      X = [.x,x.] by A1,XXREAL_1:17;
      hence thesis;
    end;
    suppose X is non trivial;
      then consider p, q being set such that
A2:   p in X & q in X & p <> q by REALSET1:14;
A3:   lower_bound X < upper_bound X by A2,SEQ_4:25;
      per cases;
      suppose upper_bound X in X & lower_bound X in X;
        then X = [.lower_bound X,upper_bound X.] by Th25;
        hence thesis by A3;
      end;
      suppose upper_bound X in X & not lower_bound X in X;
        then X = ].lower_bound X,upper_bound X.] by Th27;
        hence thesis by A3;
      end;
      suppose not upper_bound X in X & lower_bound X in X;
        then X = [.lower_bound X,upper_bound X.[ by Th29;
        hence thesis by A3;
      end;
      suppose not upper_bound X in X & not lower_bound X in X;
        then X = ].lower_bound X,upper_bound X.[ by Th31;
        hence thesis by A3;
      end;
    end;
  end;
  suppose
A4: X is not bounded;
    per cases by A4;
    suppose
A5:   X is not bounded_below & X is bounded_above;
      per cases;
      suppose upper_bound X in X;
        then X = left_closed_halfline(upper_bound X) by A5,Th33;
        hence thesis;
      end;
      suppose not upper_bound X in X;
        then X = left_open_halfline(upper_bound X) by A5,Th35;
        hence thesis;
      end;
    end;
    suppose
A6:   X is not bounded_above & X is bounded_below;
      per cases;
      suppose lower_bound X in X;
        then X = right_closed_halfline(lower_bound X) by A6,Th37;
        hence thesis;
      end;
      suppose not lower_bound X in X;
        then X = right_open_halfline(lower_bound X) by A6,Th39;
        hence thesis;
      end;
    end;
    suppose X is not bounded_above & X is not bounded_below;
      hence thesis by Th40;
    end;
  end;
end;

theorem Th42:
  for X being non empty connected Subset of REAL st not r in X holds
  r <= lower_bound X or upper_bound X <= r
proof
  let X be non empty connected Subset of REAL such that
A1: not r in X;
  per cases by Th41;
  suppose X = REAL;
    hence thesis by A1,XREAL_0:def 1;
  end;
  suppose ex a st X = left_closed_halfline(a);
    then consider a such that
A2: X = left_closed_halfline(a);
    upper_bound X = a by A2,Th21;
    hence thesis by A1,A2,XXREAL_1:234;
  end;
  suppose ex a st X = left_open_halfline(a);
    then consider a such that
A3: X = left_open_halfline(a);
    upper_bound X = a by A3,Th22;
    hence thesis by A1,A3,XXREAL_1:233;
  end;
  suppose ex a st X = right_closed_halfline(a);
    then consider a such that
A4: X = right_closed_halfline(a);
    lower_bound X = a by A4,Th23;
    hence thesis by A1,A4,XXREAL_1:236;
  end;
  suppose ex a st X = right_open_halfline(a);
    then consider a such that
A5: X = right_open_halfline(a);
    lower_bound X = a by A5,Th24;
    hence thesis by A1,A5,XXREAL_1:235;
  end;
  suppose ex a, b st a <= b & X = [.a,b.];
    then consider a, b such that
A6: a <= b and
A7: X = [.a,b.];
    lower_bound X = a & upper_bound X = b by A6,A7,JORDAN5A:20;
    hence thesis by A1,A7,XXREAL_1:1;
  end;
  suppose ex a, b st a < b & X = [.a,b.[;
    then consider a, b such that
A8: a < b and
A9: X = [.a,b.[;
    lower_bound X = a & upper_bound X = b by A8,A9,Th4,Th5;
    hence thesis by A1,A9,XXREAL_1:3;
  end;
  suppose ex a, b st a < b & X = ].a,b.];
    then consider a, b such that
A10: a < b and
A11: X = ].a,b.];
    lower_bound X = a & upper_bound X = b by A10,A11,Th6,Th7;
    hence thesis by A1,A11,XXREAL_1:2;
  end;
  suppose ex a, b st a < b & X = ].a,b.[;
    then consider a, b such that
A12: a < b and
A13: X = ].a,b.[;
    lower_bound X = a & upper_bound X = b by A12,A13,TOPREAL6:22;
    hence thesis by A1,A13,XXREAL_1:4;
  end;
end;

theorem Th43:
  for X, Y being non empty bounded connected Subset of REAL
  st lower_bound X <= lower_bound Y & upper_bound Y <= upper_bound X &
  (lower_bound X = lower_bound Y & lower_bound Y in Y
  implies lower_bound X in X) &
  (upper_bound X = upper_bound Y &
  upper_bound Y in Y implies upper_bound X in X) holds Y c= X
proof
  let X, Y be non empty bounded connected Subset of REAL such that
A1: lower_bound X <= lower_bound Y and
A2: upper_bound Y <= upper_bound X and
A3: lower_bound X = lower_bound Y & lower_bound Y in Y
implies lower_bound X in X and
A4: upper_bound X = upper_bound Y &
upper_bound Y in Y implies upper_bound X in X;
  let x be set;
  assume
A5: x in Y;
  then reconsider x as Real;
  Y c= [.lower_bound Y,upper_bound Y.] by TOPREAL6:24;
  then
A6: lower_bound Y <= x & x <= upper_bound Y by A5,XXREAL_1:1;
  then
A7: lower_bound X <= x & x <= upper_bound X by A1,A2,XXREAL_0:2;
  per cases by Th41;
  suppose X = [#]REAL;
    hence thesis;
  end;
  suppose (ex a st X = left_closed_halfline(a)) or
    (ex a st X = left_open_halfline(a)) or
    (ex a st X = right_closed_halfline(a)) or
    ex a st X = right_open_halfline(a);
    hence thesis;
  end;
  suppose ex a, b st a <= b & X = [.a,b.];
    then consider a, b such that
A8: a <= b & X = [.a,b.];
    lower_bound X = a & upper_bound X = b by A8,JORDAN5A:20;
    hence thesis by A7,A8,XXREAL_1:1;
  end;
  suppose ex a, b st a < b & X = [.a,b.[;
    then consider a, b such that
A9: a < b & X = [.a,b.[;
A10: lower_bound X = a & upper_bound X = b by A9,Th4,Th5;
    per cases by Th41;
    suppose Y = [#]REAL;
      hence thesis;
    end;
    suppose (ex a st Y = left_closed_halfline(a)) or
      (ex a st Y = left_open_halfline(a)) or
      (ex a st Y = right_closed_halfline(a)) or
      ex a st Y = right_open_halfline(a);
      hence thesis;
    end;
    suppose ex a, b st a <= b & Y = [.a,b.];
      then consider a1, b1 being real number such that
A11:  a1 <= b1 & Y = [.a1,b1.];
A12:  lower_bound Y = a1 & upper_bound Y = b1 by A11,JORDAN5A:20;
      then b1 < b by A2,A4,A9,A10,A11,XXREAL_0:1,XXREAL_1:1,3;
      then x < b by A6,A12,XXREAL_0:2;
      hence thesis by A7,A9,A10,XXREAL_1:3;
    end;
    suppose ex a, b st a < b & Y = [.a,b.[;
      then consider a1, b1 being real number such that
A13:  a1 < b1 & Y = [.a1,b1.[;
A14:  lower_bound Y = a1 & upper_bound Y = b1 by A13,Th4,Th5;
      x < b1 by A5,A13,XXREAL_1:3;
      then x < b by A2,A10,A14,XXREAL_0:2;
      hence thesis by A7,A9,A10,XXREAL_1:3;
    end;
    suppose ex a, b st a < b & Y = ].a,b.];
      then consider a1, b1 being real number such that
A15:  a1 < b1 & Y = ].a1,b1.];
A16:  lower_bound Y = a1 & upper_bound Y = b1 by A15,Th6,Th7;
      then b1 < b by A2,A4,A9,A10,A15,XXREAL_0:1,XXREAL_1:2,3;
      then x < b by A6,A16,XXREAL_0:2;
      hence thesis by A7,A9,A10,XXREAL_1:3;
    end;
    suppose ex a, b st a < b & Y = ].a,b.[;
      then consider a1, b1 being real number such that
A17:  a1 < b1 & Y = ].a1,b1.[;
A18:  lower_bound Y = a1 & upper_bound Y = b1 by A17,TOPREAL6:22;
      x < b1 by A5,A17,XXREAL_1:4;
      then x < b by A2,A10,A18,XXREAL_0:2;
      hence thesis by A7,A9,A10,XXREAL_1:3;
    end;
  end;
  suppose ex a, b st a < b & X = ].a,b.];
    then consider a, b such that
A19: a < b & X = ].a,b.];
A20: lower_bound X = a & upper_bound X = b by A19,Th6,Th7;
    per cases by Th41;
    suppose Y = [#]REAL;
      hence thesis;
    end;
    suppose (ex a st Y = left_closed_halfline(a)) or
      (ex a st Y = left_open_halfline(a)) or
      (ex a st Y = right_closed_halfline(a)) or
      ex a st Y = right_open_halfline(a);
      hence thesis;
    end;
    suppose ex a, b st a <= b & Y = [.a,b.];
      then consider a1, b1 being real number such that
A21:  a1 <= b1 & Y = [.a1,b1.];
A22:  lower_bound Y = a1 & upper_bound Y = b1 by A21,JORDAN5A:20;
      then a < a1 by A1,A3,A19,A20,A21,XXREAL_0:1,XXREAL_1:1,2;
      then a < x by A6,A22,XXREAL_0:2;
      hence thesis by A7,A19,A20,XXREAL_1:2;
    end;
    suppose ex a, b st a < b & Y = [.a,b.[;
      then consider a1, b1 being real number such that
A23:  a1 < b1 & Y = [.a1,b1.[;
      lower_bound Y = a1 & upper_bound Y = b1 by A23,Th4,Th5;
      then
A24:  a < a1 by A1,A3,A19,A20,A23,XXREAL_0:1,XXREAL_1:2,3;
      a1 <= x by A5,A23,XXREAL_1:3;
      then a < x by A24,XXREAL_0:2;
      hence thesis by A7,A19,A20,XXREAL_1:2;
    end;
    suppose ex a, b st a < b & Y = ].a,b.];
      then consider a1, b1 being real number such that
A25:  a1 < b1 & Y = ].a1,b1.];
A26:  lower_bound Y = a1 & upper_bound Y = b1 by A25,Th6,Th7;
      a1 < x by A5,A25,XXREAL_1:2;
      then a < x by A1,A20,A26,XXREAL_0:2;
      hence thesis by A7,A19,A20,XXREAL_1:2;
    end;
    suppose ex a, b st a < b & Y = ].a,b.[;
      then consider a1, b1 being real number such that
A27:  a1 < b1 & Y = ].a1,b1.[;
A28:  lower_bound Y = a1 & upper_bound Y = b1 by A27,TOPREAL6:22;
      a1 < x by A5,A27,XXREAL_1:4;
      then a < x by A1,A20,A28,XXREAL_0:2;
      hence thesis by A7,A19,A20,XXREAL_1:2;
    end;
  end;
  suppose ex a, b st a < b & X = ].a,b.[;
    then consider a, b such that
A29: a < b & X = ].a,b.[;
A30: lower_bound X = a & upper_bound X = b by A29,TOPREAL6:22;
    per cases by Th41;
    suppose Y = [#]REAL;
      hence thesis;
    end;
    suppose (ex a st Y = left_closed_halfline(a)) or
      (ex a st Y = left_open_halfline(a)) or
      (ex a st Y = right_closed_halfline(a)) or
      ex a st Y = right_open_halfline(a);
      hence thesis;
    end;
    suppose ex a, b st a <= b & Y = [.a,b.];
      then consider a1, b1 being real number such that
A31:  a1 <= b1 & Y = [.a1,b1.];
A32:  lower_bound Y = a1 & upper_bound Y = b1 by A31,JORDAN5A:20;
A33:  x <= b1 by A5,A31,XXREAL_1:1;
      b1 < b by A2,A4,A29,A30,A31,A32,XXREAL_0:1,XXREAL_1:1,4;
