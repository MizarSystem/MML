A3: x in Int X;
    then reconsider x as Point of R^1;
A4: now
      assume
A5:   x = a;
A6:   Int X misses Fr X by TOPS_1:73;
      now
        assume a >= b;
        then X = {}R^1 by A1,XXREAL_1:27;
        hence contradiction by A3;
      end;
      then Fr X = {a,b} by A1,Th46;
      then a in Fr X by TARSKI:def 2;
      hence contradiction by A3,A5,A6,XBOOLE_0:3;
    end;
    a <= x & x < b by A1,A2,A3,XXREAL_1:3;
    then a < x & x < b by A4,XXREAL_0:1;
    hence thesis by XXREAL_1:4;
