definition
  let R,f;
  attr f is being_an_Amalgam_of_squares means
  :Def10:
  len f<>0 & for n st n<>0
& n<=len f holds f/.n is being_a_product_of_squares or ex i,j st f/.n=f/.i*f/.j
  & i<>0 & i<n & j<>0 & j<n;
end;

