A2: <*x*>/.len<*x*>=x by Lm2;
  len g=len <*x*>+len <*x+y*> by FINSEQ_1:35
    .=len <*x*>+1 by Lm2;
  hence thesis by A1,A2,Lm3,Lm6,Lm55;
end;

Lm63: f is being_a_generation_from_squares & g is
being_a_generation_from_squares implies (f^g)^<*f/.len f+g/.len g*> is
being_a_generation_from_squares
proof
  assume that
A1: f is being_a_generation_from_squares and
A2: g is being_a_generation_from_squares;
A3: len g<>0 by A2,Def14;
  then
A4: len f+len g<>0 by NAT_1:7;
  len f<>0 by A1,Def14;
  then 1<=len f by NAT_1:26;
  then len f in dom f by FINSEQ_3:27;
  then
A5: len f+len g<=len(f^g) & (f^g)/.len f=f/.len f by Lm1,FINSEQ_1:35;
  len f<=len f+len g by NAT_1:11;
  then
A6: len f<=len(f^g) by FINSEQ_1:35;
  1<=len g by A3,NAT_1:26;
  then len g in dom g by FINSEQ_3:27;
  then
A7: (f^g)/.(len f+len g)=g/.len g by Lm1;
  (f^g) is being_a_generation_from_squares & len f<>0 by A1,A2,Def14,Lm33;
  hence thesis by A6,A4,A5,A7,Lm61;
end;

Lm64: x is generated_from_squares & y is generated_from_squares implies x+y is
