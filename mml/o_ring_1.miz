    implies x is_generated_from_squares
    proof
      assume x is_a_sum_of_squares;
      then consider f such that
        A1:f is_a_Sum_of_squares & x=f.:len f
          by Def5;
         0<>len f & len f<=len f by A1,Def4;
      hence thesis by A1,Lm38;
    end;

