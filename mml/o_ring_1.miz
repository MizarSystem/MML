    A3:f is_an_Amalgam_of_squares & x=f/.len f by A1,Def11;
    consider g such that
    A4:g is_an_Amalgam_of_squares & y=g/.len g by A2,Def11;
    take h=(f^g)^<*f/.len f*g/.len g*>;
    len h=len(f^g)+len<*f/.len f*g/.len g*> by FINSEQ_1:35
