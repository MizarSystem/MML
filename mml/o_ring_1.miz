    thus thesis
      proof
        let n such that A2:n<>0 & n<=len f;
        A3:( ex i,j st f.:n=f.:i*f.:j & i<>0 & i<n & j<>0 & j<n) implies
 thesis;
            f.:n is_a_product_of_squares implies thesis by Lm20;
