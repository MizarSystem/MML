  then x is being_a_product_of_squares by Lm9;
  then
A1: <*x*>/.1 is being_a_product_of_squares by Lm2;
A2: for n st n<>0 & n<=len <*x*> holds <*x*>/.n is
being_a_product_of_squares or ex i,j st <*x*>/.n=<*x*>/.i*<*x*>/.j & i<>0 & i<n
  & j<>0 & j<n
  proof
    let n;
    assume that
A3: n<>0 and
A4: n<=len <*x*>;
    n<=1 by A4,Lm2;
    hence thesis by A1,A3,NAT_1:26;
  end;
  len<*x*>=1 by Lm2;
  hence thesis by A2,Def10;
end;

