          end;
             now assume A11: n=len g;

            then g.:n=f.:i*f.:j by A3,Lm5
                     .=g.:i*f.:j by A1,Lm6
                     .=g.:i*g.:j by A1,Lm6;
            then g.:n=g.:i*g.:j & i<n & j<n by A1,A3,A11,NAT_1:38;
