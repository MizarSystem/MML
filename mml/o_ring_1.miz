begin :: O_RING_3

Lm80: f is being_an_Amalgam_of_squares & g is being_an_Amalgam_of_squares
implies f^g is being_an_Amalgam_of_squares
proof
  assume that
A1: f is being_an_Amalgam_of_squares and
A2: g is being_an_Amalgam_of_squares;
A3: for n st n<>0 & n<=len (f^g) holds (f^g)/.n is
being_a_product_of_squares or ex i,j st (f^g)/.n=(f^g)/.i*(f^g)/.j & i<>0 & i<n
  & j<>0 & j<n
  proof
    let n;
    assume that
A4: n<>0 and
A5: n<=len (f^g);
A6: n<=len f+len g by A5,FINSEQ_1:35;
A7: now
      assume
A8:   len f<n;
      then consider m be Nat such that
A9:   n=len f+m by NAT_1:10;
      len f+m<=len f+len g by A5,A9,FINSEQ_1:35;
      then
A10:  m<=len g by XREAL_1:8;
A11:  m<>0 by A8,A9;
A12:  m<=len g by A6,A9,XREAL_1:8;
A13:  now
        given k,l be Nat such that
A14:    g/.m=g/.k*g/.l and
A15:    k<>0 and
A16:    k<m and
A17:    l<>0 and
A18:    l<m;
A19:    len f+k<>0 & len f+l<>0 by A15,A17,NAT_1:7;
A20:    len f+k<n & len f+l<n by A9,A16,A18,XREAL_1:8;
        (f^g)/.n=g/.k*g/.l by A9,A11,A10,A14,Lm5
          .=(f^g)/.(len f+k)*g/.l by A12,A15,A16,Lm5,XXREAL_0:2
          .=(f^g)/.(len f+k)*(f^g)/.(len f+l) by A12,A17,A18,Lm5,XXREAL_0:2;
        hence thesis by A19,A20;
      end;
      g/.m is being_a_product_of_squares implies thesis by A9,A11,A10,
      Lm5;
      hence thesis by A2,A11,A10,A13,Def10;
    end;
    now
      assume
A21:  n<=len f;
A22:  now
        given k,l be Nat such that
A23:    f/.n=f/.k*f/.l and
A24:    k<>0 & k<n and
A25:    l<>0 & l<n;
        (f^g)/.n=f/.k*f/.l by A4,A21,A23,Lm4
          .=(f^g)/.k*f/.l by A21,A24,Lm4,XXREAL_0:2
          .=(f^g)/.k*(f^g)/.l by A21,A25,Lm4,XXREAL_0:2;
        hence thesis by A24,A25;
      end;
      f/.n is being_a_product_of_squares implies thesis by A4,A21,Lm4;
      hence thesis by A1,A4,A21,A22,Def10;
    end;
    hence thesis by A7;
  end;
  len f<>0 by A1,Def10;
  then len f+len g<>0 by NAT_1:7;
  then len(f^g)<>0 by FINSEQ_1:35;
  hence thesis by A3,Def10;
end;

Lm81: f is being_an_Amalgam_of_squares & i<>0 & i<=len f & j<>0 & j<=len f
implies f^<*f/.i*f/.j*> is being_an_Amalgam_of_squares
proof
  assume that
A1: f is being_an_Amalgam_of_squares and
A2: i<>0 and
A3: i<=len f and
A4: j<>0 and
A5: j<=len f;
  set g=f^<*f/.i*f/.j*>;
A6: len g=len f+len<*f/.i*f/.j*> by FINSEQ_1:35
    .=len f+1 by Lm2;
A7: for n st n<>0 & n<=len g holds g/.n is being_a_product_of_squares or ex
  i,j st g/.n=g/.i*g/.j & i<>0 & i<n & j<>0 & j<n
  proof
    let n such that
A8: n<>0 and
A9: n<=len g;
A10: now
      assume n<len g;
      then
A11:  n<=len f by A6,NAT_1:13;
A12:  now
        given k,m be Nat such that
A13:    f/.n=f/.k*f/.m and
A14:    k<>0 & k<n and
A15:    m<>0 & m<n;
        g/.n=f/.k*f/.m by A8,A11,A13,Lm4
          .=g/.k*f/.m by A11,A14,Lm4,XXREAL_0:2
          .=g/.k*g/.m by A11,A15,Lm4,XXREAL_0:2;
        hence thesis by A14,A15;
      end;
      f/.n is being_a_product_of_squares implies thesis by A8,A11,Lm4;
      hence thesis by A1,A8,A11,A12,Def10;
    end;
    now
      assume
A16:  n=len g;
      then
A17:  i<n & j<n by A3,A5,A6,NAT_1:13;
      g/.n=f/.i*f/.j by A6,A16,Lm3
        .=g/.i*f/.j by A2,A3,Lm4
        .=g/.i*g/.j by A4,A5,Lm4;
      hence thesis by A2,A4,A17;
    end;
    hence thesis by A9,A10,XXREAL_0:1;
  end;
  len f<>0 by A1,Def10;
  then len f+len<*f/.i*f/.j*><>0 by NAT_1:7;
  then len (f^<*f/.i*f/.j*>)<>0 by FINSEQ_1:35;
  hence thesis by A7,Def10;
end;

Lm82: f is being_a_generation_from_squares & i<>0 & i<=len f & j<>0 & j<=len f
implies f^<*f/.i*f/.j*> is being_a_generation_from_squares
proof
  assume that
A1: f is being_a_generation_from_squares and
A2: i<>0 and
A3: i<=len f and
A4: j<>0 and
A5: j<=len f;
  set g=f^<*f/.i*f/.j*>;
A6: len g=len f+len<*f/.i*f/.j*> by FINSEQ_1:35
    .=len f+1 by Lm2;
A7: for n st n<>0 & n<=len g holds g/.n is being_an_amalgam_of_squares or
  ex i,j st (g/.n=g/.i*g/.j or g/.n=g/.i+g/. j) & i<>0 & i<n & j<>0 & j<n
  proof
    let n such that
A8: n<>0 and
A9: n<=len g;
A10: now
      assume n<len g;
      then
A11:  n<=len f by A6,NAT_1:13;
      then
A12:  g/.n=f/.n by A8,Lm4;
A13:  now
        given k,m be Nat such that
A14:    f/.n=f/.k*f/.m or f/.n=f/.k+f/.m and
A15:    k<>0 & k<n & m<>0 & m<n;
        f/.k=g/.k & f/.m=g/.m by A11,A15,Lm4,XXREAL_0:2;
        hence thesis by A12,A14,A15;
      end;
      f/.n is being_an_amalgam_of_squares implies thesis by A8,A11,Lm4;
      hence thesis by A1,A8,A11,A13,Def14;
    end;
    now
      assume
A16:  n=len g;
      then
A17:  i<n & j<n by A3,A5,A6,NAT_1:13;
      g/.n=f/.i*f/.j by A6,A16,Lm3
        .=g/.i*f/.j by A2,A3,Lm4
        .=g/.i*g/.j by A4,A5,Lm4;
      hence thesis by A2,A4,A17;
    end;
    hence thesis by A9,A10,XXREAL_0:1;
  end;
  len f<>0 by A1,Def14;
  then len f+len<*f/.i*f/.j*><>0 by NAT_1:7;
  then len (f^<*f/.i*f/.j*>)<>0 by FINSEQ_1:35;
  hence thesis by A7,Def14;
end;

Lm83: f is being_a_Product_of_squares & x is being_a_square implies f^<*f/.len
f*x*> is being_a_Product_of_squares
proof
  assume that
A1: f is being_a_Product_of_squares and
A2: x is being_a_square;
  set g=f^<*f/.len f*x*>;
A3: len g=len f+len <*f/.len f*x*> by FINSEQ_1:35
    .=len f+1 by Lm2;
A4: for n st n<>0 & n<len g holds f/.n=g/.n
  proof
    let n;
    assume n<>0 & n<len g;
    then 1<=n & n<=len f by A3,NAT_1:13,26;
    then n in dom f by FINSEQ_3:27;
    hence thesis by Lm1;
  end;
A5: for n st n<>0 & n<len g ex y st y is being_a_square & g/.(n+1)=g/.n*y
  proof
    let n;
    assume that
A6: n<>0 and
A7: n<len g;
A8: now
A9:   f/.n=g/.n by A4,A6,A7;
      assume
A10:  n<len f;
      then n+1<>0 & n+1<len g by A3,NAT_1:5,XREAL_1:8;
      then f/.(n+1)=g/.(n+1) by A4;
      hence thesis by A1,A6,A10,A9,Def6;
    end;
A11: now
      assume
A12:  n=len f;
      1<=n by A6,NAT_1:26;
      then
A13:  n in dom f by A12,FINSEQ_3:27;
      g/.(n+1)=f/.n*x by A12,Lm3;
      then g/.(n+1)=g/.n*x by A13,Lm1;
      hence thesis by A2;
    end;
    n<=len f by A3,A7,NAT_1:13;
    hence thesis by A8,A11,XXREAL_0:1;
  end;
  len f<>0 by A1,Def6;
  then 1<=len f by NAT_1:26;
  then 1<len g by A3,NAT_1:13;
  then g/.1=f/.1 by A4;
  then
A14: g/.1 is being_a_square by A1,Def6;
  len g<>0 by A3,NAT_1:5;
  hence thesis by A14,A5,Def6;
end;

Lm84: f is being_an_Amalgam_of_squares & g is being_an_Amalgam_of_squares
implies (f^g)^<*f/.len f*g/.len g*> is being_an_Amalgam_of_squares
proof
  assume that
A1: f is being_an_Amalgam_of_squares and
A2: g is being_an_Amalgam_of_squares;
  len f<>0 by A1,Def10;
  then
A3: len f+len g<=len(f^g) & (f^g)/.len f=f/.len f by Lm4,FINSEQ_1:35;
  len g<>0 by A2,Def10;
  then
A4: len f+len g<>0 & (f^g)/.(len f+len g)=g/.len g by Lm5,NAT_1:7;
  len f<=len f+len g by NAT_1:11;
  then
A5: len f<=len(f^g) by FINSEQ_1:35;
  len f<>0 & f^g is being_an_Amalgam_of_squares by A1,A2,Def10,Lm80;
  hence thesis by A5,A3,A4,Lm81;
end;

Lm85: f is being_a_generation_from_squares & g is
being_a_generation_from_squares implies (f^g)^<*f/.len f*g/.len g*> is
being_a_generation_from_squares
proof
  assume that
A1: f is being_a_generation_from_squares and
A2: g is being_a_generation_from_squares;
A3: len g<>0 by A2,Def14;
  then
A4: len f+len g<>0 by NAT_1:7;
  len f<>0 by A1,Def14;
  then 1<=len f by NAT_1:26;
  then len f in dom f by FINSEQ_3:27;
  then
A5: len f+len g<=len(f^g) & (f^g)/.len f=f/.len f by Lm1,FINSEQ_1:35;
  len f<=len f+len g by NAT_1:11;
  then
A6: len f<=len(f^g) by FINSEQ_1:35;
  1<=len g by A3,NAT_1:26;
  then len g in dom g by FINSEQ_3:27;
  then
A7: (f^g)/.(len f+len g)=g/.len g by Lm1;
  f^g is being_a_generation_from_squares & len f<>0 by A1,A2,Def14,Lm33;
  hence thesis by A6,A4,A5,A7,Lm82;
end;

theorem
  x is being_a_product_of_squares & y is being_a_square implies x*y is
  being_a_product_of_squares
proof
  assume that
A1: x is being_a_product_of_squares and
A2: y is being_a_square;
  consider f such that
A3: f is being_a_Product_of_squares & x=f/.len f by A1,Def7;
  take g=f^<*x*y*>;
  len g=len f+len <*x*y*> by FINSEQ_1:35
    .=len f+1 by Lm2;
  hence thesis by A2,A3,Lm3,Lm83;
end;

