begin :: Neither vertical nor horizontal

reserve D1 for non vertical non empty compact Subset of TOP-REAL 2,
        D2 for non horizontal non empty compact Subset of TOP-REAL 2,
        D for non vertical non horizontal non empty compact
              Subset of TOP-REAL 2;

theorem Th33:
 W-bound D1 < E-bound D1
proof
     W-bound D1 <= E-bound D1 & W-bound D1 <> E-bound D1 by Th17,Th23;
 hence thesis by REAL_1:def 5;
end;

theorem Th34:
 S-bound D2 < N-bound D2
proof
     S-bound D2 <= N-bound D2 & S-bound D2 <> N-bound D2 by Th18,Th24;
 hence thesis by REAL_1:def 5;
end;

theorem Th35:
 LSeg(SW-corner D1,NW-corner D1) misses LSeg(SE-corner D1,NE-corner D1)
  proof
   assume LSeg(SW-corner D1,NW-corner D1) /\
    LSeg(SE-corner D1,NE-corner D1) <> {};
    then consider a being set such that
A1:    a in LSeg(SW-corner D1,NW-corner D1) /\ LSeg(SE-corner D1,NE-corner D1)
        by XBOOLE_0:def 1;
        a in LSeg(NW-corner D1,SW-corner D1)by A1,XBOOLE_0:def 3;
     then a in {p : p`1 = W-bound D1 & p`2 <= N-bound D1 & p`2 >= S-bound D1}
        by Th28;
     then A2:    ex p st p = a & p`1 = W-bound D1 & p`2 <= N-bound D1 & p`2 >=
 S-bound D1;
         a in LSeg(NE-corner D1,SE-corner D1) by A1,XBOOLE_0:def 3;
      then a in {p : p`1= E-bound D1 & p`2 <= N-bound D1 & p`2 >= S-bound D1}
                                            by Th25;
      then ex p st p=a & p`1 = E-bound D1 & p`2 <= N-bound D1 & p`2 >=
       S-bound D1;
     hence contradiction by A2,Th17;
  end;

theorem Th36:
 LSeg(SW-corner D2,SE-corner D2) misses LSeg(NW-corner D2,NE-corner D2)
  proof
   assume LSeg(SW-corner D2,SE-corner D2) /\
    LSeg(NW-corner D2,NE-corner D2) <> {};
    then consider a being set such that
A1:    a in LSeg(SW-corner D2,SE-corner D2) /\ LSeg(NW-corner D2,NE-corner D2)
        by XBOOLE_0:def 1;
        a in LSeg(SE-corner D2,SW-corner D2)by A1,XBOOLE_0:def 3;
     then a in {p : p`1 <= E-bound D2 & p`1 >= W-bound D2 & p`2 = S-bound D2}
        by Th26;
     then A2:    ex p st p = a & p`1 <= E-bound D2 & p`1 >=
 W-bound D2 & p`2 = S-bound D2;
         a in LSeg(NE-corner D2,NW-corner D2) by A1,XBOOLE_0:def 3;
      then a in {p : p`1 <= E-bound D2 & p`1 >= W-bound D2 & p`2= N-bound D2}
                                            by Th27;
      then ex p st p=a & p`1 <= E-bound D2 & p`1 >= W-bound D2 & p`2 =
       N-bound D2;
     hence contradiction by A2,Th18;
  end;

begin :: SpStSeq

definition let C be Subset of TOP-REAL 2;
 func SpStSeq C -> FinSequence of TOP-REAL 2 equals
 <*NW-corner C,NE-corner C,SE-corner C*>^
      <*SW-corner C,NW-corner C*>;
 coherence;
end;

theorem Th37:
 (SpStSeq S)/.1 = NW-corner S
proof
     1 in dom<*NW-corner S,NE-corner S,SE-corner S*> by TOPREAL3:6;
 hence (SpStSeq S)/.1 = <*NW-corner S,NE-corner S,SE-corner S*>/.1
            by FINSEQ_4:83
         .= NW-corner S by FINSEQ_4:27;
end;

theorem Th38:
 (SpStSeq S)/.2 = NE-corner S
proof
     2 in dom<*NW-corner S,NE-corner S,SE-corner S*> by TOPREAL3:6;
 hence (SpStSeq S)/.2 = <*NW-corner S,NE-corner S,SE-corner S*>/.2
             by FINSEQ_4:83
         .= NE-corner S by FINSEQ_4:27;
end;

theorem Th39:
 (SpStSeq S)/.3 = SE-corner S
proof
     3 in dom<*NW-corner S,NE-corner S,SE-corner S*> by TOPREAL3:6;
 hence (SpStSeq S)/.3 = <*NW-corner S,NE-corner S,SE-corner S*>/.3
            by FINSEQ_4:83
         .= SE-corner S by FINSEQ_4:27;
end;

theorem Th40:
 (SpStSeq S)/.4 = SW-corner S
proof
 set g = <*NW-corner S,NE-corner S,SE-corner S*>;
      1 in {1,2} by TARSKI:def 2;
then A1: 1 in dom <*SW-corner S,NW-corner S*> by FINSEQ_1:4,FINSEQ_3:29;
     len g = 3 by FINSEQ_1:62;
 hence (SpStSeq S)/.4 = (SpStSeq S)/.(len g + 1)
         .= <*SW-corner S,NW-corner S*>/.1 by A1,FINSEQ_4:84
         .= SW-corner S by FINSEQ_4:26;
end;

theorem
    (SpStSeq S)/.5 = NW-corner S
proof
 set g = <*NW-corner S,NE-corner S,SE-corner S*>;
      2 in {1,2} by TARSKI:def 2;
then A1: 2 in dom <*SW-corner S,NW-corner S*> by FINSEQ_1:4,FINSEQ_3:29;
     len g = 3 by FINSEQ_1:62;
 hence (SpStSeq S)/.5 = (SpStSeq S)/.(len g + 2)
         .= <*SW-corner S,NW-corner S*>/.2 by A1,FINSEQ_4:84
         .= NW-corner S by FINSEQ_4:26;
end;

theorem Th42:
 len SpStSeq S = 5
proof
 thus len SpStSeq S
          = len<*NW-corner S,NE-corner S,SE-corner S*>
              + len<*SW-corner S,NW-corner S*> by FINSEQ_1:35
         .= len<*SW-corner S,NW-corner S*> + 3 by FINSEQ_1:62
         .= 2 + 3 by FINSEQ_1:61
         .= 5;
end;

theorem Th43:
 L~SpStSeq S =
   (LSeg(NW-corner S,NE-corner S) \/ LSeg(NE-corner S,SE-corner S)) \/
   (LSeg(SE-corner S,SW-corner S) \/ LSeg(SW-corner S,NW-corner S))
proof
      len<*NW-corner S,NE-corner S,SE-corner S*> = 3 by FINSEQ_1:62;
then A1: <*NW-corner S,NE-corner S,SE-corner S*>/.
     len<*NW-corner S,NE-corner S,SE-corner S*> = SE-corner S by FINSEQ_4:27;
A2: <*SW-corner S,NW-corner S*>/.1 = SW-corner S by FINSEQ_4:26;
 thus L~SpStSeq S
  = L~(<*NW-corner S,NE-corner S,SE-corner S*>^<*SW-corner S,NW-corner S*>)
 .= L~<*NW-corner S,NE-corner S,SE-corner S*> \/
    LSeg(SE-corner S,SW-corner S) \/ L~<*SW-corner S,NW-corner S*>
         by A1,A2,SPPOL_2:23
 .= L~<*NW-corner S,NE-corner S,SE-corner S*> \/
    LSeg(SE-corner S,SW-corner S) \/
 LSeg(SW-corner S,NW-corner S) by SPPOL_2:21
 .= LSeg(NW-corner S,NE-corner S) \/ LSeg(NE-corner S,SE-corner S) \/
    LSeg(SE-corner S,SW-corner S) \/ LSeg(SW-corner S,NW-corner S) by Th10
 .= (LSeg(NW-corner S,NE-corner S) \/ LSeg(NE-corner S,SE-corner S)) \/
    (LSeg(SE-corner S,SW-corner S) \/ LSeg(SW-corner S,NW-corner S))
       by XBOOLE_1:4;
end;

registration let D be non vertical non empty compact Subset of TOP-REAL 2;
 cluster SpStSeq D -> non constant;
  coherence
   proof
    assume SpStSeq D is constant;
     then A1:    <*NW-corner D,NE-corner D,SE-corner D*> is constant by Th3;
         |[W-bound D, N-bound D]| = NW-corner D by PSCOMP_1:def 35
        .= NE-corner D by A1,Th5
        .= |[E-bound D, N-bound D]| by PSCOMP_1:def 36;
     then W-bound D = E-bound D by SPPOL_2:1;
    hence contradiction by Th17;
   end;
end;

registration let D be non horizontal non empty compact Subset of TOP-REAL 2;
 cluster SpStSeq D -> non constant;
  coherence
   proof
    assume SpStSeq D is constant;
     then A1:    <*SW-corner D,NW-corner D*> is constant by Th3;
         |[W-bound D, N-bound D]| = NW-corner D by PSCOMP_1:def 35
        .= SW-corner D by A1,Th4
        .= |[W-bound D, S-bound D]| by PSCOMP_1:def 34;
     then N-bound D = S-bound D by SPPOL_2:1;
    hence contradiction by Th18;
   end;
end;

registration let D be non vertical non horizontal non empty compact
                    Subset of TOP-REAL 2;
 cluster SpStSeq D -> special unfolded circular s.c.c. standard;
  coherence
   proof
   set f1 = <*NW-corner D,NE-corner D,SE-corner D*>,
       f2 = <*SW-corner D,NW-corner D*>;
A1:len f1 = 3 & len f2 = 2 by FINSEQ_1:61,62;
    then A2: len(f1^f2) = 3+2 by FINSEQ_1:35;
    set f = SpStSeq D;
A3: NW-corner D = |[W-bound D,N-bound D]| by PSCOMP_1:def 35;
A4: NE-corner D = |[E-bound D,N-bound D]| by PSCOMP_1:def 36;
A5: SE-corner D = |[E-bound D,S-bound D]| by PSCOMP_1:def 37;
A6: SW-corner D = |[W-bound D,S-bound D]| by PSCOMP_1:def 34;
       1 in dom f1 by A1,FINSEQ_3:27;
then A7: f/.1 = f1/.1 by FINSEQ_4:83 .= NW-corner D by FINSEQ_4:27;
       2 in dom f1 by A1,FINSEQ_3:27;
then A8: f/.2 = f1/.2 by FINSEQ_4:83 .= NE-corner D by FINSEQ_4:27;
       3 in dom f1 by A1,FINSEQ_3:27;
then A9: f/.3 = f1/.3 by FINSEQ_4:83 .= SE-corner D by FINSEQ_4:27;
       1 in dom f2 by A1,FINSEQ_3:27;
then A10: f/.(3+1) = f2/.1 by A1,FINSEQ_4:84 .= SW-corner D by FINSEQ_4:26;
       2 in dom f2 by A1,FINSEQ_3:27;
then A11: f/.(3+2) = f2/.2 by A1,FINSEQ_4:84 .= NW-corner D by FINSEQ_4:26;
        1+1 = 2;
then A12: LSeg(f,1) = LSeg(NW-corner D,NE-corner D) by A2,A7,A8,
TOPREAL1:def 5;
        2+1 = 3;
then A13: LSeg(f,2) = LSeg(NE-corner D,SE-corner D) by A2,A8,A9,
TOPREAL1:def 5;
A14: LSeg(f,3) = LSeg(SE-corner D,SW-corner D) by A2,A9,A10,TOPREAL1:def 5;
        4+1 = 5;
then A15: LSeg(f,4) = LSeg(SW-corner D,NW-corner D) by A2,A10,A11,
TOPREAL1:def 5;
 thus f is special
    proof let i;
    assume 1 <= i;
     then 1+1 <= i+1 by XREAL_1:8;
     then A16:   1 < i+1 & 0 < i+1 by XREAL_1:2;
    assume i+1 <= len f;
      then A17:    i+1 = 1+1 or i+1 = 2+1 or i+1 = 3+1 or i+1 = 4+1
          by A2,A16,CQC_THE1:6;
     per cases by A17;
      suppose
A18:      i = 1;
           (f/.1)`2 = N-bound D by A3,A7,EUCLID:56
                .= (f/.(1+1))`2 by A4,A8,EUCLID:56;
       hence thesis by A18;
      end;
      suppose
A19:     i = 2;
          (f/.2)`1 = E-bound D by A4,A8,EUCLID:56
               .= (f/.(2+1))`1 by A5,A9,EUCLID:56;
       hence thesis by A19;
      end;
      suppose
A20:     i = 3;
          (f/.3)`2 = S-bound D by A5,A9,EUCLID:56
               .= (f/.(3+1))`2 by A6,A10,EUCLID:56;
       hence thesis by A20;
      end;
      suppose
A21:     i = 4;
          (f/.4)`1 = W-bound D by A6,A10,EUCLID:56
               .= (f/.(4+1))`1 by A3,A11,EUCLID:56;
       hence thesis by A21;
    end;
    end;
 thus f is unfolded
    proof let i;
     assume 1 <= i;
      then 1+2 <= i+2 by XREAL_1:8;
      then A22:   2 < i+2 & 1 < i+2 & 0 < i+2 by XREAL_1:2;
     assume i + 2 <= len f;
      then A23:   i+2 = 1+2 or i+2 = 2+2 or i+2 = 3+2 by A2,A22,CQC_THE1:6;
      per cases by A23;
     suppose i = 1;
      hence LSeg(f,i) /\ LSeg(f,i+1) = {f/.(i+1)} by A8,A12,A13,Th30;
     end;
     suppose i = 2;
      hence LSeg(f,i) /\ LSeg(f,i+1) = {f/.(i+1)} by A9,A13,A14,Th31;
     end;
     suppose i = 3;
      hence LSeg(f,i) /\ LSeg(f,i+1) = {f/.(i+1)} by A10,A14,A15,Th32;
    end;
    end;
      thus f is circular by A2,A7,A11,FINSEQ_6:def 1;
 thus f is s.c.c.
    proof let i,j;
     assume that
A24:  i+1 < j and
A25:  (i > 1 & j < len f or j+1 < len f);
A26:   i+1 >= 1 by NAT_1:29;
     then A27:   i+1 >= 0 by XREAL_1:2;
     A28:   j+1 = 0+1 or j+1 = 1+1 or j+1 = 2+1 or j+1 = 3+1 or j+1 = 4+1
                                                      by A2,A25,CQC_THE1:6;
A29:   i+1+1 = i+(1+1);
     then A30:  i+2 <= j by A24,NAT_1:38;
A31:   (i+2 = 2+2 implies i=2) &
     (i+2 = 1+2 implies i=1) &
     (i+2 = 0+2 implies i=0);
A32: i+2 <> 0+1 by A29;
A33:   now per cases by A24,A26,A27,A28;
       case j = 2;
        hence i = 0 by A24,A29,A31,CQC_THE1:3;
       end;
       case j = 3;
        hence i = 0 or i = 1 by A24,A29,A31,CQC_THE1:4;
       end;
       case j = 4;
        hence i = 0 or i = 2 by A2,A25,A30,A31,A32,CQC_THE1:5;
      end;
      end;
     per cases by A33;
     suppose i = 0;
      then LSeg(f,i) = {} by TOPREAL1:def 5;
     hence LSeg(f,i) /\ LSeg(f,j) = {};
     end;
     suppose i = 1 & j = 3;
     then LSeg(f,i) misses LSeg(f,j) by A12,A14,Th36;
     hence LSeg(f,i) /\ LSeg(f,j) = {} by XBOOLE_0:def 7;
     end;
     suppose i = 2 & j = 4;
     then LSeg(f,i) misses LSeg(f,j) by A13,A15,Th35;
     hence LSeg(f,i) /\ LSeg(f,j) = {} by XBOOLE_0:def 7;
    end;
    end;
    set Xf1 = <*W-bound D,E-bound D,E-bound D*>,
        Xf2 = <*W-bound D,W-bound D*>;
    reconsider Xf = Xf1^Xf2 as FinSequence of REAL;
A34:len Xf1 = 3 & len Xf2 = 2 by FINSEQ_1:61,62;
    then A35: len Xf = 3+2 by FINSEQ_1:35;
       1 in dom Xf1 by A34,FINSEQ_3:27;
then A36: Xf.1 = Xf1.1 by FINSEQ_1:def 7 .= W-bound D by FINSEQ_1:62;
       2 in dom Xf1 by A34,FINSEQ_3:27;
then A37: Xf.2 = Xf1.2 by FINSEQ_1:def 7 .= E-bound D by FINSEQ_1:62;
       3 in dom Xf1 by A34,FINSEQ_3:27;
then A38: Xf.3 = Xf1.3 by FINSEQ_1:def 7 .= E-bound D by FINSEQ_1:62;
       1 in dom Xf2 by A34,FINSEQ_3:27;
then A39: Xf.(3+1) = Xf2.1 by A34,FINSEQ_1:def 7 .= W-bound D by FINSEQ_1:61;
       2 in dom Xf2 by A34,FINSEQ_3:27;
then A40: Xf.(3+2) = Xf2.2 by A34,FINSEQ_1:def 7 .= W-bound D by FINSEQ_1:61;
        now let n be Nat;
      assume n in dom Xf;
       then A41:    n <> 0 & n <= 5 by A35,FINSEQ_3:27;
      per cases by A41,CQC_THE1:6;
      suppose n = 1;
      hence Xf.n = (f/.n)`1 by A7,A36,PSCOMP_1:74;
      end;
      suppose n = 2;
      hence Xf.n = (f/.n)`1 by A8,A37,PSCOMP_1:76;
      end;
      suppose n = 3;
      hence Xf.n = (f/.n)`1 by A9,A38,PSCOMP_1:78;
      end;
      suppose n = 4;
      hence Xf.n = (f/.n)`1 by A10,A39,PSCOMP_1:72;
      end;
      suppose n = 5;
      hence Xf.n = (f/.n)`1 by A11,A40,PSCOMP_1:74;
     end;
     end;
     then A42:   Xf = X_axis f by A2,A35,GOBOARD1:def 3;
A43:    W-bound D < E-bound D by Th33;
A44:    rng Xf1 = { W-bound D,E-bound D,E-bound D } by FINSEQ_2:148
             .= { E-bound D,E-bound D,W-bound D } by ENUMSET1:102
             .= { W-bound D,E-bound D } by ENUMSET1:70;
         rng Xf2 = { W-bound D,W-bound D } by FINSEQ_2:147
             .= { W-bound D } by ENUMSET1:69;
then A45:   rng Xf = { W-bound D,E-bound D } \/ { W-bound D } by A44,
FINSEQ_1:44
            .= { W-bound D,W-bound D,E-bound D } by ENUMSET1:42
            .= { W-bound D,E-bound D } by ENUMSET1:70;
      then A46:   rng <*W-bound D,E-bound D*> = rng Xf by FINSEQ_2:147;
A47:   len <*W-bound D,E-bound D*> = 2 by FINSEQ_1:61
               .= card rng Xf by A43,A45,CARD_2:76;
        <*W-bound D,E-bound D*> is increasing
       proof let n,m be Nat;
            len <*W-bound D,E-bound D*> = 2 by FINSEQ_1:61;
then A48:       dom <*W-bound D,E-bound D*> = { 1,2 } by FINSEQ_1:4,def 3;
        assume n in dom <*W-bound D,E-bound D*> &
               m in dom <*W-bound D,E-bound D*>;
         then A49:       (n = 1 or n = 2) & (m = 1 or m = 2) by A48,
TARSKI:def 2;
        assume n < m;
         then <*W-bound D,E-bound D*>.n = W-bound D &
              <*W-bound D,E-bound D*>.m = E-bound D by A49,FINSEQ_1:61;
        hence <*W-bound D,E-bound D*>.n < <*W-bound D,E-bound D*>.m by Th33;
       end;
     then A50: <*W-bound D,E-bound D*> = Incr X_axis f by A42,A46,A47,
GOBOARD2:def 2;
    set Yf1 = <*N-bound D,N-bound D,S-bound D*>,
        Yf2 = <*S-bound D,N-bound D*>;
A51:  S-bound D < N-bound D by Th34;
    reconsider Yf = Yf1^Yf2 as FinSequence of REAL;
A52:len Yf1 = 3 & len Yf2 = 2 by FINSEQ_1:61,62;
    then A53: len Yf = 3+2 by FINSEQ_1:35;
       1 in dom Yf1 by A52,FINSEQ_3:27;
then A54: Yf.1 = Yf1.1 by FINSEQ_1:def 7 .= N-bound D by FINSEQ_1:62;
       2 in dom Yf1 by A52,FINSEQ_3:27;
then A55: Yf.2 = Yf1.2 by FINSEQ_1:def 7 .= N-bound D by FINSEQ_1:62;
       3 in dom Yf1 by A52,FINSEQ_3:27;
then A56: Yf.3 = Yf1.3 by FINSEQ_1:def 7 .= S-bound D by FINSEQ_1:62;
       1 in dom Yf2 by A52,FINSEQ_3:27;
then A57: Yf.(3+1) = Yf2.1 by A52,FINSEQ_1:def 7 .= S-bound D by FINSEQ_1:61;
       2 in dom Yf2 by A52,FINSEQ_3:27;
then A58: Yf.(3+2) = Yf2.2 by A52,FINSEQ_1:def 7 .= N-bound D by FINSEQ_1:61;
        now let n be Nat;
      assume n in dom Yf;
       then A59:    n <> 0 & n <= 5 by A53,FINSEQ_3:27;
      per cases by A59,CQC_THE1:6;
      suppose n = 1;
      hence Yf.n = (f/.n)`2 by A7,A54,PSCOMP_1:75;
      end;
      suppose n = 2;
      hence Yf.n = (f/.n)`2 by A8,A55,PSCOMP_1:77;
      end;
      suppose n = 3;
      hence Yf.n = (f/.n)`2 by A9,A56,PSCOMP_1:79;
      end;
      suppose n = 4;
      hence Yf.n = (f/.n)`2 by A10,A57,PSCOMP_1:73;
      end;
      suppose n = 5;
      hence Yf.n = (f/.n)`2 by A11,A58,PSCOMP_1:75;
     end;
     end;
     then A60:   Yf = Y_axis f by A2,A53,GOBOARD1:def 4;
A61:    rng Yf1 = { N-bound D,N-bound D,S-bound D } by FINSEQ_2:148
           .= { S-bound D,N-bound D } by ENUMSET1:70;
         rng Yf2 = { S-bound D,N-bound D } by FINSEQ_2:147;
then A62:   rng Yf = { S-bound D,N-bound D } \/ { S-bound D,N-bound D }
                      by A61,FINSEQ_1:44
            .= { S-bound D,N-bound D };
      then A63:   rng <*S-bound D,N-bound D*> = rng Yf by FINSEQ_2:147;
A64:   len <*S-bound D,N-bound D*> = 2 by FINSEQ_1:61
              .= card rng Yf by A51,A62,CARD_2:76;
        <*S-bound D,N-bound D*> is increasing
       proof let n,m be Nat;
            len <*S-bound D,N-bound D*> = 2 by FINSEQ_1:61;
then A65:       dom <*S-bound D,N-bound D*> = { 1,2 } by FINSEQ_1:4,def 3;
        assume n in dom <*S-bound D,N-bound D*> &
               m in dom <*S-bound D,N-bound D*>;
         then A66:       (n = 1 or n = 2) & (m = 1 or m = 2) by A65,
TARSKI:def 2;
        assume n < m;
         then <*S-bound D,N-bound D*>.n = S-bound D &
              <*S-bound D,N-bound D*>.m = N-bound D
            by A66,FINSEQ_1:61;
        hence <*S-bound D,N-bound D*>.n < <*S-bound D,N-bound D*>.m by Th34;
       end;
     then A67:   <*S-bound D,N-bound D*> = Incr Y_axis f by A60,A63,A64,
GOBOARD2:def 2;
     set S = (|[W-bound D,S-bound D]|,|[W-bound D,N-bound D]|)
           ][(|[E-bound D,S-bound D]|,|[E-bound D,N-bound D]|);
A68:  len S = 2 by MATRIX_2:3 .= len Incr X_axis f by A50,FINSEQ_1:61;
A69:  width S = 2 by MATRIX_2:3 .= len Incr Y_axis f by A67,FINSEQ_1:61;
        for n,m being Nat st [n,m] in Indices S
            holds S*(n,m) = |[(Incr X_axis f).n,(Incr Y_axis f).m]|
      proof let n,m be Nat;
       assume [n,m] in Indices S;
        then [n,m] in [:{ 1,2 },{ 1,2 }:] by FINSEQ_1:4,MATRIX_2:3;
        then A70:     [n,m] in {[1,1],[1,2],[2,1],[2,2]} by MCART_1:25;
A71:    <*S-bound D,N-bound D*>.1 = S-bound D &
       <*S-bound D,N-bound D*>.2 = N-bound D by FINSEQ_1:61;
       per cases by A70,ENUMSET1:def 2;
       suppose [n,m] = [1,1];
        then A72:       n = 1 & m = 1 by ZFMISC_1:33;
       hence S*(n,m) = |[W-bound D,S-bound D]| by MATRIX_2:6
          .= |[(Incr X_axis f).n,(Incr Y_axis f).m]|
          by A50,A67,A71,A72,FINSEQ_1:61;
       end;
       suppose [n,m] = [1,2];
        then A73:       n = 1 & m = 2 by ZFMISC_1:33;
       hence S*(n,m) = |[W-bound D,N-bound D]| by MATRIX_2:6
          .= |[(Incr X_axis f).n,(Incr Y_axis f).m]|
          by A50,A67,A71,A73,FINSEQ_1:61;
       end;
       suppose [n,m] = [2,1];
        then A74:       n = 2 & m = 1 by ZFMISC_1:33;
       hence S*(n,m) = |[E-bound D,S-bound D]| by MATRIX_2:6
            .= |[(Incr X_axis f).n,(Incr Y_axis f).m]|
            by A50,A67,A71,A74,FINSEQ_1:61;
       end;
       suppose [n,m] = [2,2];
        then A75:       n = 2 & m = 2 by ZFMISC_1:33;
       hence S*(n,m) = |[E-bound D,N-bound D]| by MATRIX_2:6
            .= |[(Incr X_axis f).n,(Incr Y_axis f).m]|
            by A50,A67,A71,A75,FINSEQ_1:61;
      end;
      end;
      then A76:   S = GoB(Incr X_axis f, Incr Y_axis f) by A68,A69,
GOBOARD2:def 1
        .= GoB f by GOBOARD2:def 3;
A77: f/.1 = |[W-bound D,N-bound D]| by A7,PSCOMP_1:def 35
           .= (GoB f)*(1,2) by A76,MATRIX_2:6;
A78: f/.2 = |[E-bound D,N-bound D]| by A8,PSCOMP_1:def 36
           .= (GoB f)*(2,2) by A76,MATRIX_2:6;
A79: f/.3 = |[E-bound D,S-bound D]| by A9,PSCOMP_1:def 37
           .= (GoB f)*(2,1) by A76,MATRIX_2:6;
A80: f/.4 = |[W-bound D,S-bound D]| by A10,PSCOMP_1:def 34
           .= (GoB f)*(1,1) by A76,MATRIX_2:6;
  thus SpStSeq D is standard
   proof
   thus for k st k in dom f
    ex i,j st [i,j] in Indices GoB f & f/.k = (GoB f)*(i,j)
   proof let k;
    assume
A81:   k in dom f;
     then A82:    k >= 1 & k <= 5 by A2,FINSEQ_3:27;
A83:    k <> 0 by A81,FINSEQ_3:27;
    per cases by A82,A83,CQC_THE1:6;
    suppose
A84:    k = 1;
     take 1,2;
     thus [1,2] in Indices GoB f by A76,MATRIX_2:4;
     thus f/.k = (GoB f)*(1,2) by A77,A84;
    end;
    suppose
A85:    k = 2;
     take 2,2;
     thus [2,2] in Indices GoB f by A76,MATRIX_2:4;
     thus f/.k = (GoB f)*(2,2) by A78,A85;
    end;
    suppose
A86:    k = 3;
     take 2,1;
     thus [2,1] in Indices GoB f by A76,MATRIX_2:4;
     thus f/.k = (GoB f)*(2,1) by A79,A86;
    end;
    suppose
A87:    k = 4;
     take 1,1;
     thus [1,1] in Indices GoB f by A76,MATRIX_2:4;
     thus f/.k = (GoB f)*(1,1) by A80,A87;
    end;
    suppose
A88:    k = 5;
     take 1,2;
     thus [1,2] in Indices GoB f by A76,MATRIX_2:4;
     thus f/.k = (GoB f)*(1,2) by A7,A11,A77,A88;
   end;
   end;

   let n be Nat such that
A89: n in dom f and
A90: n+1 in dom f;
   let m,k,i,j be Nat such that
A91: [m,k] in Indices GoB f and
A92: [i,j] in Indices GoB f and
A93: f/.n = (GoB f)*(m,k) and
A94: f/.(n+1) = (GoB f)*(i,j);
   A95:  n <> 0 by A89,FINSEQ_3:27;
       n+1 <= 4+1 by A2,A90,FINSEQ_3:27;
    then A96: n <= 4 by XREAL_1:8;
   per cases by A95,A96,CQC_THE1:5;
   suppose
A97:  n = 1;
       [1,2] in Indices GoB f by A76,MATRIX_2:4;
then A98:  m = 1 & k = 2 by A77,A91,A93,A97,GOBOARD1:21;
       [2,2] in Indices GoB f by A76,MATRIX_2:4;
then A99:  i = 1+1 & j = 2 by A78,A92,A94,A97,GOBOARD1:21;
    then abs(m-i) = 1 by A98,GOBOARD1:1;
   hence abs(m-i)+abs(k-j) = 1 by A98,A99,GOBOARD1:2;
   end;
   suppose
A100:  n = 2;
       [2,2] in Indices GoB f by A76,MATRIX_2:4;
then A101:  m = 2 & k = 1+1 by A78,A91,A93,A100,GOBOARD1:21;
       [2,1] in Indices GoB f by A76,MATRIX_2:4;
then A102:  i = 2 & j = 1 by A79,A92,A94,A100,GOBOARD1:21;
    then abs(k-j) = 1 by A101,GOBOARD1:1;
   hence abs(m-i)+abs(k-j) = 1 by A101,A102,GOBOARD1:2;
   end;
   suppose
A103:  n = 3;
       [2,1] in Indices GoB f by A76,MATRIX_2:4;
then A104:  m = 1+1 & k = 1 by A79,A91,A93,A103,GOBOARD1:21;
       [1,1] in Indices GoB f by A76,MATRIX_2:4;
then A105:  i = 1 & j = 1 by A80,A92,A94,A103,GOBOARD1:21;
    then abs(m-i) = 1 by A104,GOBOARD1:1;
   hence abs(m-i)+abs(k-j) = 1 by A104,A105,GOBOARD1:2;
   end;
   suppose
A106:  n = 4;
       [1,1] in Indices GoB f by A76,MATRIX_2:4;
then A107:  m = 1 & k = 1 by A80,A91,A93,A106,GOBOARD1:21;
       [1,2] in Indices GoB f by A76,MATRIX_2:4;
then A108:  i = 1 & j = 1+1 by A7,A11,A77,A92,A94,A106,GOBOARD1:21;
    then abs(k-j) = 1 by A107,GOBOARD1:1;
   hence abs(m-i)+abs(k-j) = 1 by A107,A108,GOBOARD1:2;
   end;
   end;
   end;
end;

theorem Th44:
 L~SpStSeq D = [.W-bound D,E-bound D,S-bound D,N-bound D.]
proof
A1: L~SpStSeq D =
   (LSeg(NW-corner D,NE-corner D) \/ LSeg(NE-corner D,SE-corner D)) \/
   (LSeg(SE-corner D,SW-corner D) \/ LSeg(SW-corner D,NW-corner D)) by Th43
   .=
     LSeg(SW-corner D,NW-corner D) \/ ( LSeg(NW-corner D,NE-corner D) \/
     LSeg(NE-corner D,SE-corner D) ) \/ LSeg(SE-corner D,SW-corner D)
         by XBOOLE_1:4
   .=
     LSeg(SW-corner D,NW-corner D) \/ LSeg(NW-corner D,NE-corner D) \/
     LSeg(NE-corner D,SE-corner D) \/ LSeg(SE-corner D,SW-corner D)
      by XBOOLE_1:4
   .=
   ( LSeg(SW-corner D,NW-corner D) \/ LSeg(NW-corner D,NE-corner D) ) \/
   ( LSeg(NE-corner D,SE-corner D) \/ LSeg(SE-corner D,SW-corner D) )
                                    by XBOOLE_1:4;
 NW-corner D = |[W-bound D, N-bound D]| &
   NE-corner D = |[E-bound D, N-bound D]| &
   SE-corner D = |[E-bound D, S-bound D]| &
   SW-corner D = |[W-bound D, S-bound D]|
         by PSCOMP_1:def 34,def 35,def 36,def 37;
 hence L~SpStSeq D = [.W-bound D,E-bound D,S-bound D,N-bound D.]
                               by A1,SPPOL_2:def 3;
end;

