theorem
    z1<>0 & CPoly3(z1,z2,z3,0c,z)=0c implies
     (for s,h,t st s=-(z3/z1) & h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1) holds
           z=0 or
           z=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
                sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
               -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
               -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
                sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t)
proof
       assume
 A100: z1<>0 & CPoly3(z1,z2,z3,0c,z)=0c;
       let s,h,t;
       assume
 A102: s=-(z3/z1) & h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1);
       0c=z1*z^3 +z2*z^2+z3*z+0c by Def11,A100
       .=z1*z#N 3 +z2*z^2+z3*z+0c by Th50
       .=z1*z#N 3 +z2*z^2+z3*z by COMPLEX1:22
       .=z1*(z^2*z) +z2*z^2+z3*z by Th50
       .=z1*z^2*z +z2*z^2+z3*z by XCMPLX_1:4
       .=z1*z^2*z +z2*(z*z)+z3*z by Th4
       .=z1*z^2*z +z2*z*z+z3*z by XCMPLX_1:4
       .=(z1*z^2+z2*z)*z+z3*z by XCMPLX_1:8
       .=(z1*z^2+z2*z+z3)*z by XCMPLX_1:8
       .=CPoly2(z1,z2,z3,z)*z by Def10;then
       z=0c or CPoly2(z1,z2,z3,z)=0c by XCMPLX_1:6,COMPLEX1:def 6;
     hence thesis by A100,A102,Th43,COMPLEX1:def 6;
end;

theorem Th54:
   z=s-[*1/3,0*]*z1 implies
    z^2 =s^2+(-[*2/3,0*])*z1*s+[*1/9,0*]*z1^2
proof
    assume z=s-[*1/3,0*]*z1;then
       z^2=(s+(-[*1/3,0*]*z1))^2 by XCMPLX_0:def 8
      .=s^2+2*s*(-[*1/3,0*]*z1)+(-[*1/3,0*]*z1)^2 by Tha
