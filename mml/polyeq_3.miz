theorem
   for z be complex number holds z|^2= z*z by Lm2;

theorem Th50:
  for n be Nat st n>0 holds
    0 |^ n = 0
proof
    let n be Nat;
    assume n>0;
       then n >= 0+1 by NAT_1:13;
 then A1: n-1 >= 0 by XREAL_1:21;
     n=n-1+1
       .= n-'1+1 by A1,BINARITH:def 3;
    hence 0 |^ n = (0 |^ (n-'1))*0 by NEWTON:11
           .= 0;
end;

theorem Th51:
  for x,y be Element of COMPLEX
    for n be Element of NAT holds
     (x*y) |^ n = x |^ n * y |^ n
proof
    let x,y be Element of COMPLEX;
    defpred P[Element of NAT] means (x*y) |^ $1 = x |^ $1 * y |^ $1;
       (x*y) |^ 0 =1 by NEWTON:9
                .=(x |^ 0)*1 by NEWTON:9
                .=(x |^ 0)*(y |^ 0) by NEWTON:9;
then A1:  P[0];
 A2: now let n be Element of NAT;
     assume P[n];
      then (x*y)|^ (n+1)= x|^ n * y|^ n * (y*x) by NEWTON:11
        .= x|^ n * (y|^ n * y*x)
        .= x|^ n * (y|^ (n+1)*x) by NEWTON:11
        .= x|^ n * x *(y|^ (n+1))
        .= x|^ (n+1) * y|^ (n+1) by NEWTON:11;
     hence P[n+1];
   end;
   thus for n be Element of NAT holds P[n] from NAT_1:sch 1(A1,A2);
end;

theorem Th52:
  for x be Real st x > 0
    for n be Element of NAT holds [*x,0*]|^ n = x to_power n
proof
  let x be Real;
  assume
A1: x > 0;
 defpred P[Element of NAT] means [*x,0*]|^ $1 = x to_power $1;
    [*x,0*]|^ 0 = 1+0*<i> by NEWTON:9
       .= x to_power 0+0*<i> by POWER:29;
 then A2: P[0];
 A3: now let n be Element of NAT;
A4:  [*x to_power n,0*] = x to_power n+0*<i> &
    [*x,0*] = x+0*<i> by COMPLEX1:27;
     assume P[n];
     then [*x,0*]|^ (n+1) = (x to_power n)*x by A4,NEWTON:11
        .= (x to_power n)*(x to_power 1) by POWER:30
        .= x to_power (n+1) by A1,POWER:32;
     hence P[n+1];
    end;
   thus for n be Element of NAT holds P[n] from NAT_1:sch 1(A2,A3);
end;

theorem Th53:
  for x being Real,
      n be Element of NAT holds
    [*cos x,sin x*] |^ n = cos (n*x) + sin (n*x)*<i>
proof
  let x be Real;
 defpred P[Element of NAT] means
  [*cos x,sin x*]|^ $1 = cos ($1*x)+sin ($1*x)*<i>;
 A1: P[0] by NEWTON:9,SIN_COS:34;
 A2: now let n be Element of NAT;
    set a'=Re [*cos(n*x),sin(n*x)*];
    set b'=Im [*cos(n*x),sin(n*x)*];
    set c'=Re [*cos x,sin x*];
    set d'=Im [*cos x,sin x*];
A3: [*cos(n*x),sin(n*x)*] = cos(n*x)+sin(n*x)*<i> &
   [*cos x,sin x*] = cos x+(sin x)*<i> by COMPLEX1:27;
     assume P[n];
     then [*cos x,sin x*] |^ (n+1)
         = (cos(n*x)+sin(n*x)*<i>)*(cos x+(sin x)*<i>) by A3,NEWTON:11
        .= cos(n*x)*cos x - sin(n*x)*sin x+
           (cos(n*x)*sin x+cos x*sin(n*x))*<i>
        .= cos(n*x+x)+(cos(n*x)*sin x+cos x*sin(n*x))*<i> by SIN_COS:80
        .= cos((n+1)*x)+sin((n+1)*x)*<i> by SIN_COS:80;
      hence P[n+1];
    end;
    thus for n be Element of NAT holds P[n] from NAT_1:sch 1(A1,A2);
 end;

theorem
   for z being Element of COMPLEX
  for n being Element of NAT st z <> 0 or n > 0 holds
    z|^ n = (|.z.| to_power n)*cos (n*Arg z) +
            (|.z.| to_power n)*sin (n*Arg z)*<i>
proof
    let z be Element of COMPLEX;
    let n be Element of NAT;
    assume
 A1: z <> 0 or n > 0;
      per cases by A1;
     suppose z <> 0;
 then A2: |.z.| > 0 by COMPLEX1:133;
A3:  [*|.z.|,0*] = |.z.|+0*<i> &
    [*cos Arg z,sin Arg z*] = cos Arg z+(sin Arg z)*<i> by COMPLEX1:27;
        set x=Arg z;
A4: [*cos (n*x),sin (n*x)*] = cos (n*x)+sin (n*x)*<i> by COMPLEX1:27;
         z|^ n =[*|.z.|*cos Arg z-0*sin Arg z,|.z.|*sin Arg z+cos Arg z*0*]|^ n
                                            by COMPLEX2:19
         .=(|.z.|*cos Arg z-0*sin Arg z+(|.z.|*sin Arg z+cos Arg z*0)*<i>)|^ n
                                             by COMPLEX1:27
         .=([*|.z.|,0*]*[*cos Arg z,sin Arg z*])|^ n by A3
         .= [*|.z.|,0*]|^ n*[*cos Arg z,sin Arg z*]|^ n by Th51;
       hence z|^ n= [*|.z.|,0*]|^ n*[*cos (n*x),sin (n*x)*] by A4,Th53
       .= (|.z.| to_power n)*[*cos (n*Arg z),sin (n*Arg z)*]
                                     by A2,Th52
       .= (|.z.| to_power n+0*<i>)*(cos (n*Arg z)+(sin (n*Arg z))*<i>)
                                     by COMPLEX1:27
       .= (|.z.| to_power n)*cos(n*Arg z)+(|.z.| to_power n)*sin(n*Arg z)*<i>;
     end;
     suppose
 A5: z=0 & n > 0;
     then 0=|.z.| to_power n by COMPLEX1:130,POWER:def 2;
       hence z|^ n= (|.z.| to_power n)*cos(n*Arg z)+
        (|.z.| to_power n)*sin(n*Arg z)*<i> by A5,Th50;
      end;
end;

theorem Th55:
  for n,k be Element of NAT,x be Real st n <> 0 holds
    [*cos((x+2*PI*k)/n),sin((x+2*PI*k)/n)*]|^ n = cos x + (sin x)*<i>
  proof
    let n,k be Element of NAT,x be Real;
    assume
A1: n <> 0;
       thus [*cos((x+2*PI*k)/n),sin((x+2*PI*k)/n)*]|^ n
        = cos (n*((x+2*PI*k)/n))+sin (n*((x+2*PI*k)/n))*<i> by Th53
       .= cos(x+2*PI*k)+sin(n*((x+2*PI*k)/n))*<i> by A1,XCMPLX_1:88
       .= cos(x+2*PI*k)+sin(x+2*PI*k)*<i> by A1,XCMPLX_1:88
       .= cos.(x+2*PI*k)+sin(x+2*PI*k)*<i> by SIN_COS:def 23
       .= cos.(x+2*PI*k)+(sin.(x+2*PI*k))*<i> by SIN_COS:def 21
       .= cos.(x+2*PI*k)+(sin.x)*<i> by SIN_COS2:10
       .= cos.x + (sin.x)*<i> by SIN_COS2:11
       .= cos.x + (sin x)*<i> by SIN_COS:def 21
       .= cos x + (sin x)*<i> by SIN_COS:def 23;
   end;

theorem Th56:
  for z be complex number
  for n,k be Element of NAT st n <> 0 holds
    z = [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
          (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]|^ n
   proof
    let z be complex number;
    let n,k be Element of NAT;
    assume
A1: n <> 0;
 then A2: n > 0;
 then A3: n >= 0+1 by NAT_1:13;
    per cases;
      suppose z <> 0;
 then A4: |.z.| > 0 by COMPLEX1:133;
       then n-root |.z.| = n -Root |.z.| by A3,POWER:def 1;
 then A5: n-root |.z.| > 0 by A3,A4,PREPOWER:def 3;
     thus [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
            (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]|^ n
        =((n-root |.z.|)*cos((Arg z+2*PI*k)/n)-
          0*sin((Arg z+2*PI*k)/n)+((n-root |.z.|)*sin((Arg z+2*PI*k)/n)+
          cos((Arg z+2*PI*k)/n)*0)*<i>)|^ n by COMPLEX1:27
       .= ((n-root |.z.|+0*<i>)*
          (cos((Arg z+2*PI*k)/n)+(sin((Arg z+2*PI*k)/n))*<i>))|^ n
       .= ((n-root |.z.|+0*<i>)*
          [*cos((Arg z+2*PI*k)/n),sin((Arg z+2*PI*k)/n)*])|^ n by COMPLEX1:27
       .= ([*n-root |.z.|,0*]*
          [*cos((Arg z+2*PI*k)/n),sin((Arg z+2*PI*k)/n)*])|^ n by COMPLEX1:27
       .= ([*n-root |.z.|,0*]|^ n)
         *([*cos((Arg z+2*PI*k)/n),sin((Arg z+2*PI*k)/n)*]|^ n) by Th51
       .= ([*n-root |.z.|,0*]|^ n)*(cos Arg z+(sin Arg z)*<i>) by A1,Th55
       .= ((n-root |.z.|) to_power n)*(cos Arg z+(sin Arg z)*<i>)
                                    by A5,Th52
       .= ((n-root |.z.|) |^ n)*(cos Arg z+(sin Arg z)*<i>) by A3,POWER:47
       .= |.z.|*(cos Arg z+(sin Arg z)*<i>) by A1,A4,COMPTRIG:17
       .= |.z.|*cos Arg z-0*sin Arg z+(|.z.|*sin Arg z+0*cos Arg z)*<i>
       .= [*|.z.|*cos Arg z-0*sin Arg z,|.z.|*sin Arg z+0*cos Arg z*]
                                     by COMPLEX1:27
       .= z by COMPLEX2:19;
     end;
     suppose
 A6: z = 0;
     hence [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
            (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]|^ n
        =([*0*cos((Arg z+2*PI*k)/n),
           (n-root 0)*sin((Arg z+2*PI*k)/n)*]|^ n) by A3,COMPLEX1:130,POWER:6
       .= [*0,0*sin((Arg z+2*PI*k)/n)*]|^ n by A3,POWER:6
       .= z by A2,A6,Lm1,Th50;
     end;
   end;

theorem
   for z be Element of COMPLEX,
      n be non empty Element of NAT,
      k be Element of NAT holds
   [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
     (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*] is CRoot of n,z
proof
  let z be Element of COMPLEX,
      n be non empty Element of NAT,k be Element of NAT;
   [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
  (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]|^ n=z by Th56;
  hence thesis by COMPTRIG:def 2;
end;

theorem
   for z be complex number, v be CRoot of 1,z holds
    v = z
  proof
    let z be complex number, v be CRoot of 1,z;
     v|^ 1 = z by COMPTRIG:def 2;
    hence v = z by NEWTON:10;
  end;

theorem
   for n be non empty Nat, v be CRoot of n,0 holds v = 0
  proof
    let n be non empty Nat, v be CRoot of n,0;
    defpred P[Nat] means v|^ $1 = 0;
A1: ex n be non empty Nat st P[n]
    proof
      take n;
      thus thesis by COMPTRIG:def 2;
    end;
A2: now let k be non empty Nat;
      assume that
A3:   k <> 1 and
A4:   P[k];
      consider t be Nat such that
A5:   k = t+1 by NAT_1:6;
      reconsider t as non empty Nat by A3,A5;
      take t;
      thus t < k by A5,NAT_1:13;
       v|^ k = (v|^ t)*v by A5,NEWTON:11;
      then v|^ t = 0 or v = 0 by A4,XCMPLX_1:6;
      hence P[t] by Th50;
    end;
     P[1] from COMPTRIG:sch 1(A1,A2);
    hence thesis by NEWTON:10;
  end;

theorem
   for n be non empty Element of NAT, z be complex number
   for v be CRoot of n,z st v = 0 holds z = 0
proof
  let n be non empty Element of NAT, z be complex number;
  let v be CRoot of n,z;
  assume v = 0;
  then 0|^ n = z by COMPTRIG:def 2;
  hence thesis by Th50;
end;

theorem
   for n be non empty Element of NAT, k be Element of NAT holds
    cos(2*PI*k/n) + sin(2*PI*k/n)*<i> is CRoot of n,1
proof
   let n be non empty Element of NAT,k be Element of NAT;
A1: [*cos(2*PI*k/n),sin(2*PI*k/n)*] = cos(2*PI*k/n)+sin(2*PI*k/n)*<i>
               by COMPLEX1:27;
    [*cos(2*PI*k/n),sin(2*PI*k/n)*]|^n
       =cos(n*((2*PI*k)/n))+sin(n*((2*PI*k)/n))*<i> by Th53
      .=cos(2*PI*k)+(sin(n*((2*PI*k)/n)))*<i> by XCMPLX_1:88
      .=cos(2*PI*k+0)+(sin(2*PI*k+0))*<i> by XCMPLX_1:88
      .=cos.(2*PI*k+0)+(sin(2*PI*k+0))*<i> by SIN_COS:def 23
      .=cos.(2*PI*k+0)+(sin.(2*PI*k+0))*<i> by SIN_COS:def 21
      .=cos.(2*PI*k+0)+(sin.0)*<i> by SIN_COS2:10
      .=1 by SIN_COS:33,SIN_COS2:11;
   hence thesis by A1,COMPTRIG:def 2;
end;

canceled;

theorem
   for z,s being Element of COMPLEX, n being Element of NAT st
     s<>0 & z<>0 & n>=1 & s|^ n=z|^ n holds
     |.s.| = |.z.|
proof
     let z,s be Element of COMPLEX, n be Element of NAT;
     assume
 A1: s<>0 & z<>0 & n>=1 & s|^ n=z|^ n;
 then A2: |.s.|>0 & |.z.|>0 by COMPLEX1:133;
          z|^ n =[* |.s.|*cos Arg s, |.s.|*sin Arg s *]|^ n by A1,COMPLEX2:19
        .=([*|.s.|,0*]*[*cos Arg s,sin Arg s *])|^ n by Th46
        .=([*|.s.|,0*]|^ n)*([*cos Arg s,sin Arg s *]|^ n) by Th51
        .=(|.s.| to_power n)*([*cos Arg s,sin Arg s *]|^ n) by A2,Th52
        .=(|.s.| to_power n)*(cos (n*Arg s)+sin (n*Arg s)*<i>) by Th53
       .= (|.s.| to_power n)*cos (n*Arg s)-0*sin (n*Arg s)+
        ((|.s.| to_power n)*sin (n*Arg s)+cos (n*Arg s)*0)*<i>
       .= [*(|.s.| to_power n)*cos (n*Arg s)
           ,(|.s.| to_power n)*sin (n*Arg s)*] by COMPLEX1:27;
       then [*(|.s.| to_power n)*cos (n*Arg s),(|.s.| to_power n)*sin (n*Arg s
)*]
        =[* |.z.|*cos Arg z, |.z.|*sin Arg z *]|^ n by COMPLEX2:19
        .=([*|.z.|,0*]*[*cos Arg z,sin Arg z *])|^ n by Th46
        .=([*|.z.|,0*]|^ n)*([*cos Arg z,sin Arg z *]|^ n) by Th51
        .=(|.z.| to_power n)*([*cos Arg z,sin Arg z *]|^ n) by A2,Th52
        .=(|.z.| to_power n)*(cos (n*Arg z)+sin (n*Arg z)*<i>) by Th53
       .= (|.z.| to_power n)*cos (n*Arg z)-0*sin (n*Arg z)+
          ((|.z.| to_power n)*sin (n*Arg z)+cos (n*Arg z)*0)*<i>
       .= [*(|.z.| to_power n)*cos (n*Arg z),(|.z.| to_power n)
          *sin (n*Arg z)*] by COMPLEX1:27;
      then (|.s.| to_power n)*cos (n*Arg s)=(|.z.| to_power n)*cos (n*Arg z)
       &(|.s.| to_power n)*sin (n*Arg s)
       =(|.z.| to_power n)*sin (n*Arg z) by ARYTM_0:12;
       then ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
       ((|.s.| to_power n)*sin (n*Arg s))^2
       =((|.z.| to_power n)*cos (n*Arg z))^2+
       ((|.z.| to_power n)*sin (n*Arg z))^2 by SQUARE_1:68;
       then ((|.s.| to_power n))^2*((cos (n*Arg s))^2+(sin (n*Arg s))^2)
       =((|.z.| to_power n))^2*((cos (n*Arg z))^2+(sin (n*Arg z))^2);
       then ((|.s.| to_power n))^2*((cos (n*Arg s))^2+(sin (n*Arg s))^2)
       =((|.z.| to_power n))^2*1 by SIN_COS:32;
 then A3: ((|.s.| to_power n))^2*1=((|.z.| to_power n))^2 by SIN_COS:32;
 A4: |.s.| to_power n > 0 & |.z.| to_power n > 0 by A2,POWER:39;
      then (|.s.| to_power n)
       =sqrt ((|.z.| to_power n))^2 by A3,SQUARE_1:89;
      then |.s.| to_power n=|.z.| to_power n by A4,SQUARE_1:89;
      then |.s.| |^ n=|.z.| to_power n by A2,POWER:46;
      then |.s.| |^ n=|.z.| |^ n by A2,POWER:46;
      then |.s.| = n-root (|.z.| |^ n) by A1,A2,POWER:5;
      hence thesis by A1,A2,POWER:5;
end;
