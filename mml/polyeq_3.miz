     .=s^3+([*1/3,0 *]*z1^2+(z1^2*(-[*2/3,0*])+z2))*s+-[*1/27,0*]*z1^3
     +z1^3*[*1/9,0*]-z2*([*1/3,0*]*z1)+z3 by XCMPLX_1:8
     .=s^3+([*1/3,0 *]*z1^2+(z1^2*(-[*2/3,0*])+z2))*s+(-[*1/27,0*]*z1^3
     +z1^3*[*1/9,0*])-z2*([*1/3,0*]*z1)+z3 by XCMPLX_1:1
     .=s^3+([*1/3,0 *]*z1^2+(z1^2*(-[*2/3,0*])+z2))*s+((-[*1/27,0*])*z1^3
     +z1^3*[*1/9,0*])-z2*([*1/3,0*]*z1)+z3 by XCMPLX_1:175
     .=s^3+([*1/3,0 *]*z1^2+(z1^2*(-[*2/3,0*])+z2))*s+(-[*1/27,0*]
     +[*1/9,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by XCMPLX_1:8
     .=s^3+([*1/3,0 *]*z1^2+(z1^2*(-[*2/3,0*])+z2))*s+(
     [*1/9,0*]-[*1/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by XCMPLX_0:def 8
     .=s^3+([*1/3,0 *]*z1^2+z1^2*(-[*2/3,0*])+z2)*s+(
     [*1/9,0*]-[*1/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by XCMPLX_1:1
     .=s^3+((-[*2/3,0*]+[*1/3,0 *])*z1^2+z2)*s+(
     [*1/9,0*]-[*1/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by XCMPLX_1:8
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+(
     [*1/9,0*]-[*1/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by XCMPLX_1:162
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*Re [*1/9,0*]-
     Re [*1/27,0*],Im [*1/9,0*]-Im [*1/27,0*]*])*z1^3-z2*([*1/3,0*]*z1)+z3
                           by COMPLEX1:def 12
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*Re [*1/9,0*]-Re [*1/27
      ,0*],Im [*1/9,0*]-0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*Re [*1/9,0*]-Re [*1/27
       ,0*],0-0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*Re [*1/9,0*]-1/27
     ,0-0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*1/9-1/27,0-0*])*z1^3-
        z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-[*Re [*2/3,0 *] -Re [*1/3,0*],Im [*2/3,0 *]-Im [*1/3,0*]*])*z1^2
       +z2)*s+([*2/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:def 12
     .=s^3+((-[*Re[*2/3,0 *] -Re [*1/3,0*],Im[*2/3,0 *] -0*])*z1^2
       +z2)*s+([*2/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-[*Re[*2/3,0 *] -Re [*1/3,0*],0-0*])*z1^2
       +z2)*s+([*2/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-[*2/3-Re [*1/3,0*],0-0*])*z1^2
       +z2)*s+([*2/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-[*2/3-1/3,0-0*])*z1^2+z2)*s+([*2/27,0*])*z1^3-z2
       *([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+(-[*1/3,0*]*z1^2+z2)*s+([*2/27,0*])*z1^3-z2
       *([*1/3,0*]*z1)+z3 by XCMPLX_1:175
     .=s^3+p*s+([*2/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by A102
     .=s^3+p*s+([*2/27,0*])*z1^3-z2*[*1/3,0*]*z1+z3 by XCMPLX_1:4
     .=s^3+p*s+[*2/27,0*]*z1^3+-z2*[*1/3,0*]*z1+z3 by XCMPLX_0:def 8
     .=(s^3+p*s)+([*2/27,0*]*z1^3+-z2*[*1/3,0*]*z1)+z3 by XCMPLX_1:1
     .=(s^3+p*s)+([*2/27,0*]*z1^3+-z2*[*1/3,0*]*z1+z3) by XCMPLX_1:1
     .=(s^3+p*s)+([*2/27,0*]*z1^3-[*1/3,0*]*z2*z1+z3) by XCMPLX_0:def 8
     .=(s^3+p*s)+([*2/27,0*]*z1^3-[*1/3,0*]*z1*z2+z3) by XCMPLX_1:4
     .=s^3+p*s+q by A102
     .=s^3+0c+p*s+q by COMPLEX1:22
     .=s^3+0c*s^2+p*s+q by COMPLEX1:28
     .=1r*s^3+0c*s^2+p*s+q by COMPLEX1:29;
  hence CPoly3(1r,0c,p,q,s)=0c by Def11;
end;

Thx9:
   for z being Element of COMPLEX holds
    z = [* |.z.|*cos Arg z, |.z.|*sin Arg z *] by COMPLEX2:19;

theorem Thxa:
   for z being Element of COMPLEX holds
    [* |.z.|*cos Arg z, |.z.|*sin Arg z *]
    = [*|.z.|,0*]*[*cos Arg z,sin Arg z *]
proof
    let z be Element of COMPLEX;
    [* |.z.|*cos Arg z, |.z.|*sin Arg z *]
    =[* |.z.|*cos Arg z-0*sin Arg z, |.z.|*sin Arg z+(cos Arg z)*0 *]
    .= [*|.z.|,0*]*[*cos Arg z,sin Arg z *] by HAHNBAN1:2;
    hence thesis;
end;

theorem The1:
  for z be Element of COMPLEX,n be Nat holds
    z #N (n+1)=(z #N n) * z
proof
    let z be Element of COMPLEX,n be Nat;
    z #N (n+1) = (z GeoSeq).(n+1) by COMSEQ_3:def 2
         .= (z GeoSeq).n*z by COMSEQ_3:def 1
         .= (z #N n)*z by COMSEQ_3:def 2;
   hence thesis;
end;

theorem The2:
  for z be Element of COMPLEX holds
    z#N 1= z
proof
    let z be Element of COMPLEX;
    z #N (0+1) = (z#N 0)*z by The1
              .= 1r*z by COMSEQ_3:11
              .= z by COMPLEX1:29;
   hence thesis;
end;

theorem
  for z be Element of COMPLEX holds
    z#N 2= z*z
proof
    let z be Element of COMPLEX;
    z #N (1+1) = (z#N 1)*z by The1
              .= z*z by The2;
   hence thesis;
end;

theorem The4:
  for n be Nat st n>0 holds
    0c#N n = 0c
proof
    let n be Nat;
    assume n>0;then
       n >= 0+1 by NAT_1:38;then
 A102: n-1 >= 0 by REAL_1:84;
       n=n-(1-1)
       .=n-1+1 by XCMPLX_1:37
       .= n-'1+1 by A102,BINARITH:def 3;
       hence 0c #N n = (0c #N (n-'1))*0c by The1
              .= 0c by COMPLEX1:28;
end;

theorem The5:
   for x,y be Element of COMPLEX
    for n be Nat holds
     (x*y) #N n = x #N n * y #N n
proof
    let x,y be Element of COMPLEX;
    defpred P[Nat] means (x*y) #N $1 = x #N $1 * y #N $1;
    (x*y) #N 0 =1r by COMSEQ_3:11
                .=1r*1r by COMPLEX1:29
                .=(x #N 0)*1r by COMSEQ_3:11
                .=(x #N 0)*(y #N 0) by COMSEQ_3:11; then
A100:  P[0];
 A102: now let n be Nat;
     assume P[n];
      then (x*y)#N (n+1)= x#N n * y#N n * (y*x) by The1
        .= x#N n * (y#N n * (y*x)) by XCMPLX_1:4
        .= x#N n * (y#N n * y*x) by XCMPLX_1:4
        .= x#N n * (y#N (n+1)*x) by The1
        .= x#N n * x *(y#N (n+1)) by XCMPLX_1:4
        .= x#N (n+1) * y#N (n+1) by The1;
     hence P[n+1];
   end;
    thus for n be Nat holds P[n] from NAT_1:sch 1(A100,A102);
end;

theorem The6:
    for x be Real st x > 0
      for n be Nat holds
      [*x,0*]#N n = [*x to_power n,0*]
proof
      let x be Real;
      assume
 A101: x > 0;
 defpred P[Nat] means [*x,0*]#N $1 = [*x to_power $1,0*];
 [*x,0*]#N 0 = 1r by COMSEQ_3:11 .= 1 by COMPLEX1:def 7
       .= [*1,0*] by ARYTM_0:def 7
       .= [**1,0**] by HAHNBAN1:def 1
       .= [**x to_power 0,0**] by POWER:29
       .= [*x to_power 0,0*] by HAHNBAN1:def 1; then
 A102: P[0];
 A103: now let n be Nat;
     assume P[n]; then
     [*x,0*]#N (n+1) = [*x to_power n,0*]*[*x,0*] by The1
        .= [*(x to_power n)*x,0*] by Th7
        .= [*(x to_power n)*(x to_power 1),0*] by POWER:30
        .= [*x to_power (n+1),0*] by A101,POWER:32;
     hence P[n+1];
    end;
   thus for n be Nat holds P[n] from NAT_1:sch 1(A102,A103);
end;

theorem The7:
   for x being Real
   for n be Nat holds
    [*cos x,sin x*] #N n = [*cos (n*x),sin (n*x)*]
proof
 let x be Real;
 defpred P[Nat] means [*cos x,sin x*]#N $1 = [*cos ($1*x),sin ($1*x)*];
 [*cos x,sin x*] #N 0 = 1r by COMSEQ_3:11
       .= [*1,0*] by ARYTM_0:def 7,COMPLEX1:def 7
       .= [*cos (0*x),sin (0*x)*] by SIN_COS:34; then
 A101: P[0];
 A102: now let n be Nat;
      set a'=Re [*cos(n*x),sin(n*x)*];
      set b'=Im [*cos(n*x),sin(n*x)*];
      set c'=Re [*cos x,sin x*];
      set d'=Im [*cos x,sin x*];
 A104:a'=cos(n*x) & b'=sin(n*x) & c'=cos x & d'=sin x by COMPLEX1:7;
       assume P[n];
     then [*cos x,sin x*] #N (n+1)
         =[*cos(n*x),sin(n*x)*]*[*cos x,sin x*] by The1
        .=[*a'*c'-b'*d',a'*d'+c'*b'*] by Th5,A104
        .= [*cos(n*x)*cos x - sin(n*x)*sin x,cos(n*x)*sin x+cos x*sin(n*x)*]
                                            by A104
        .= [*cos(n*x+x),cos(n*x)*sin x+cos x*sin(n*x)*] by SIN_COS:80
        .= [*cos(n*x+1*x+0*x),sin(n*x+1*x+0*x)*] by SIN_COS:80
        .= [*cos((n+1+0)*x),sin(n*x+1*x+0*x)*] by XCMPLX_1:9
        .= [*cos((n+1)*x),sin((n+1)*x)*] by XCMPLX_1:9;
      hence P[n+1];
    end;
    thus for n be Nat holds P[n] from NAT_1:sch 1(A101,A102);
 end;

theorem
     for z being Element of COMPLEX
     for n being Nat st z <> 0c or n > 0 holds
    z#N n = [*(|.z.| to_power n)*cos (n*Arg z)
             ,(|.z.| to_power n)*sin (n*Arg z)*]
proof
    let z be Element of COMPLEX;
    let n be Nat;
    assume
 A101: z <> 0c or n > 0;
      per cases by A101;
     suppose z <> 0c;then
 A103: |.z.| > 0 by COMPLEX1:133;
        set x=Arg z;
      z#N n =[*|.z.|*cos Arg z-0*sin Arg z,|.z.|*sin Arg z+cos Arg z*0*]#N n
                                            by Thx9
         .=([*|.z.|,0*]*[*cos Arg z,sin Arg z*])#N n by HAHNBAN1:2
         .= [*|.z.|,0*]#N n*[*cos Arg z,sin Arg z*]#N n by The5;
       hence z#N n= [*|.z.|,0*]#N n*[*cos (n*x),sin (n*x)*] by The7
       .= [*|.z.| to_power n,0*]*[*cos (n*Arg z),sin (n*Arg z)*]
                                     by A103,The6
       .= [*(|.z.| to_power n)*cos(n*Arg z)-0*sin (n*Arg z),
             (|.z.| to_power n)*sin(n*Arg z)+0*cos(n*Arg z)*] by HAHNBAN1:2
       .= [*(|.z.| to_power n)*cos(n*Arg z),(|.z.| to_power n)*sin(n*Arg z)*];
     end;
     suppose
 A114: z=0c & n > 0;then
     0=|.z.| to_power n by POWER:def 2,COMPLEX1:130;
       hence z#N n=[*(|.z.| to_power n)*cos(n*Arg z),
        (|.z.| to_power n)*sin(n*Arg z)*] by The4,A114,XXX;
      end;
end;

theorem The9:
   for n,k be Nat,x be Real st n <> 0 holds
    [*cos((x+2*PI*k)/n),sin((x+2*PI*k)/n)*]#N n = [*cos x,sin x*]
   proof
    let n,k be Nat,x be Real;
    assume
 A101: n <> 0;
       thus [*cos((x+2*PI*k)/n),sin((x+2*PI*k)/n)*]#N n
        = [*cos (n*((x+2*PI*k)/n)),sin (n*((x+2*PI*k)/n))*] by The7
       .= [*cos(x+2*PI*k),sin(n*((x+2*PI*k)/n))*] by A101,XCMPLX_1:88
       .= [*cos(x+2*PI*k),sin(x+2*PI*k)*] by A101,XCMPLX_1:88
       .= [*cos.(x+2*PI*k),sin(x+2*PI*k)*] by SIN_COS:def 23
       .= [*cos.(x+2*PI*k),sin.(x+2*PI*k)*] by SIN_COS:def 21
       .= [*cos.(x+2*PI*k),sin.x*] by SIN_COS2:10
       .= [*cos.x,sin.x*] by SIN_COS2:11
       .= [*cos.x,sin x*] by SIN_COS:def 21
       .= [*cos x,sin x*] by SIN_COS:def 23;
   end;

theorem Thea:
   for z be Element of COMPLEX
   for n,k be Nat st n <> 0 holds
    z = [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
          (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]#N n
   proof
    let z be Element of COMPLEX;
    let n,k be Nat;
    assume 
 A101: n <> 0;then
 A102: n > 0 by NAT_1:19;then
 A103: n >= 0+1 by NAT_1:38;
    per cases;
      suppose 
      z <> 0c;then
 A105: |.z.| > 0 by COMPLEX1:133;then
       n-root |.z.| = n -Root |.z.| by A103,POWER:def 1;then
 A106: n-root |.z.| > 0 by A103,A105,PREPOWER:def 3;
     thus [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
            (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]#N n
       =[*(n-root |.z.|)*cos((Arg z+2*PI*k)/n)-
          0*sin((Arg z+2*PI*k)/n),(n-root |.z.|)*sin((Arg z+2*PI*k)/n)+
          cos((Arg z+2*PI*k)/n)*0*]#N n
       .= ([*n-root |.z.|,0*]*
          [*cos((Arg z+2*PI*k)/n),sin((Arg z+2*PI*k)/n)*])#N n by HAHNBAN1:2
       .= ([*n-root |.z.|,0*]#N n)
         *([*cos((Arg z+2*PI*k)/n),sin((Arg z+2*PI*k)/n)*]#N n) by The5
       .= ([*n-root |.z.|,0*]#N n)*[*cos Arg z,sin Arg z*] by A101,The9
       .= [*(n-root |.z.|) to_power n,0*]*[*cos Arg z,sin Arg z*]
                                    by A106,The6
       .= [*|.z.|,0*]*[*cos Arg z,sin Arg z*] by A101,A105,COMPTRIG:17
       .= [*|.z.|*cos Arg z-0*sin Arg z,|.z.|*sin Arg z+0*cos Arg z*]
                                     by HAHNBAN1:2
       .= z by Thx9;
     end;
     suppose
 A107: z = 0c;
      hence [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
            (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]#N n
        =([*0*cos((Arg z+2*PI*k)/n),
           (n-root 0)*sin((Arg z+2*PI*k)/n)*]#N n) by A103,POWER:6,COMPLEX1:130
       .= [*0,0*sin((Arg z+2*PI*k)/n)*]#N n by A103,POWER:6
       .= z by A102,A107,The4,XXX;
     end;
   end;

definition
   let z be Element of COMPLEX;
   let n be non empty Nat;
   mode CRoot of n,z -> Element of COMPLEX means :Def14:
         it#N n = z;
  existence
   proof
    take [*(n-root |.z.|)*cos((Arg z+2*PI*0)/n),
         (n-root |.z.|)*sin((Arg z+2*PI*0)/n)*];
    thus thesis by Thea;
   end;
end;

theorem
   for z be Element of COMPLEX
   for n be non empty Nat
   for k be Nat holds
    [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
      (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*] is CRoot of n,z
proof
   let  z be Element of COMPLEX,n be non empty Nat,k be Nat;
    [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
      (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]#N n=z by Thea;
    hence thesis by Def14;
end;

theorem 
     for z be Element of COMPLEX
     for v be CRoot of 1,z holds
       v = z
   proof
    let z be Element of COMPLEX;
    let v be CRoot of 1,z;
    v#N 1 = z by Def14;
    hence v = z by The2;
   end;

theorem
   for n be non empty Nat
   for v be CRoot of n,0c holds
    v = 0c
proof
    let n be non empty Nat;
    let v be CRoot of n,0c;
    defpred P[Nat] means v#N $1 = 0c;
 A101: ex n be non empty Nat st P[n]
    proof
       take n;
       thus thesis by Def14;
    end;
 A102: now let k be non empty Nat;
      assume that
 A103: k <> 1 and
 A104: P[k];
     consider t be Nat such that
 A105: k = t+1 by NAT_1:22;
       k >= 1 by RLVECT_1:99;then
       t+1 > 0+1 by A103,A105,REAL_1:def 5;then
 A106: t > 0 by AXIOMS:24;
     reconsider t as non empty Nat by A103,A105,CARD_1:51;
     take t;
     thus t < k by A105,NAT_1:38;
     v#N k = (v#N t)*v by A105,The1;then
     v#N t = 0c or v = 0c by A104,XCMPLX_1:6,COMPLEX1:def 6;
     hence P[t] by A106,The4;
    end;
    P[1] from COMPTRIG:sch 1(A101,A102);
    hence thesis by The2;
end;

theorem
     for n be non empty Nat
     for z be Element of COMPLEX
     for v be CRoot of n,z st v = 0c holds
       z = 0c
proof
     let n be non empty Nat;
     let z be Element of COMPLEX;
     let v be CRoot of n,z;
 A101:n>0 by NAT_1:19;
     assume v = 0c;then
     0c#N n = z by Def14;
     hence thesis by A101,The4;
end;

theorem
   for n be non empty Nat
   for k be Nat holds
    [*cos(2*PI*k/n),sin(2*PI*k/n)*] is CRoot of n,1r
proof
   let n be non empty Nat,k be Nat;
      [*cos(2*PI*k/n),sin(2*PI*k/n)*]#N n
      =[*cos(n*((2*PI*k)/n)),sin(n*((2*PI*k)/n))*] by The7
      .=[*cos(2*PI*k),sin(n*((2*PI*k)/n))*] by XCMPLX_1:88
      .=[*cos(2*PI*k+0),sin(2*PI*k+0)*] by XCMPLX_1:88
      .=[*cos.(2*PI*k+0),sin(2*PI*k+0)*] by SIN_COS:def 23
      .=[*cos.(2*PI*k+0),sin.(2*PI*k+0)*] by SIN_COS:def 21
      .=[*cos.(2*PI*k+0),sin.0*] by SIN_COS2:10
      .=[*cos.0,sin.0*] by SIN_COS2:11
      .=[*cos(0),sin.0*] by SIN_COS:def 23
      .=[*1,0*] by SIN_COS:34,def 21
      .=1 by ARYTM_0:def 7
      .=1r by COMPLEX1:def 7;
    hence thesis by Def14;
end;

theorem
   for k be Nat holds
    [*cos(2*PI*k/3),sin(2*PI*k/3)*] is CRoot of 3,1r
proof
     let k be Nat;
     [*cos(2*PI*k/3),sin(2*PI*k/3)*]#N 3
     =[*cos((2*PI*k/3)*3),sin((2*PI*k/3)*3)*] by The7
      .=[*cos(2*PI*k),sin(3*((2*PI*k)/3))*] by XCMPLX_1:88
      .=[*cos(2*PI*k+0),sin(2*PI*k+0)*] by XCMPLX_1:88
      .=[*cos.(2*PI*k+0),sin(2*PI*k+0)*] by SIN_COS:def 23
      .=[*cos.(2*PI*k+0),sin.(2*PI*k+0)*] by SIN_COS:def 21
      .=[*cos.(2*PI*k+0),sin.0*] by SIN_COS2:10
      .=[*cos.0,sin.0*] by SIN_COS2:11
      .=[*cos(0),sin.0*] by SIN_COS:def 23
      .=[*1,0*] by SIN_COS:34,def 21
      .=1 by ARYTM_0:def 7
      .=1r by COMPLEX1:def 7;
    hence thesis by Def14;
end;

theorem
     for z,s being Element of COMPLEX, n being Nat st
     s<>0 & z<>0 & n>=1 & s#N n=z#N n holds
     |.s.| = |.z.|
proof
     let z,s be Element of COMPLEX, n be Nat;
     assume
 A100: s<>0 & z<>0 & n>=1 & s#N n=z#N n;then
       s <> 0c & z <> 0c by COMPLEX1:def 6; then
 A102: |.s.|>0 & |.z.|>0 by COMPLEX1:133;
       z#N n =[* |.s.|*cos Arg s, |.s.|*sin Arg s *]#N n by Thx9,A100
        .=([*|.s.|,0*]*[*cos Arg s,sin Arg s *])#N n by Thxa
        .=([*|.s.|,0*]#N n)*([*cos Arg s,sin Arg s *]#N n) by The5
        .=[*|.s.| to_power n,0*]*([*cos Arg s,sin Arg s *]#N n) by The6,A102
        .=[*|.s.| to_power n,0*]*[*cos (n*Arg s),sin (n*Arg s)*] by The7
       .= [*(|.s.| to_power n)*cos (n*Arg s)-0*sin (n*Arg s)
        ,(|.s.| to_power n)*sin (n*Arg s)+cos (n*Arg s)*0*] by HAHNBAN1:2
       .= [*(|.s.| to_power n)*cos (n*Arg s)
           ,(|.s.| to_power n)*sin (n*Arg s)*];then
       [*(|.s.| to_power n)*cos (n*Arg s),(|.s.| to_power n)*sin (n*Arg s)*]
        =[* |.z.|*cos Arg z, |.z.|*sin Arg z *]#N n by Thx9
        .=([*|.z.|,0*]*[*cos Arg z,sin Arg z *])#N n by Thxa
        .=([*|.z.|,0*]#N n)*([*cos Arg z,sin Arg z *]#N n) by The5
        .=[*|.z.| to_power n,0*]*([*cos Arg z,sin Arg z *]#N n) by The6,A102
        .=[*|.z.| to_power n,0*]*[*cos (n*Arg z),sin (n*Arg z)*] by The7
       .= [*(|.z.| to_power n)*cos (n*Arg z)-0*sin (n*Arg z)
           ,(|.z.| to_power n)*sin (n*Arg z)+cos (n*Arg z)*0*] by HAHNBAN1:2
       .= [*(|.z.| to_power n)*cos (n*Arg z),(|.z.| to_power n)
          *sin (n*Arg z)*];then
      (|.s.| to_power n)*cos (n*Arg s)=(|.z.| to_power n)*cos (n*Arg z)
       &(|.s.| to_power n)*sin (n*Arg s)
       =(|.z.| to_power n)*sin (n*Arg z) by ARYTM_0:12;then
       ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
       ((|.s.| to_power n)*sin (n*Arg s))^2
       =((|.z.| to_power n)*cos (n*Arg z))^2+
       ((|.z.| to_power n)*sin (n*Arg z))^2 by SQUARE_1:68;then
       ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
       ((|.s.| to_power n))^2*(sin (n*Arg s))^2
       =((|.z.| to_power n)*cos (n*Arg z))^2+
       ((|.z.| to_power n)*sin (n*Arg z))^2 by SQUARE_1:68;then
       ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
       ((|.s.| to_power n))^2*(sin (n*Arg s))^2
       =((|.z.| to_power n))^2*(cos (n*Arg z))^2+
       ((|.z.| to_power n)*sin (n*Arg z))^2 by SQUARE_1:68;then
       ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
       ((|.s.| to_power n))^2*(sin (n*Arg s))^2
       =((|.z.| to_power n))^2*(cos (n*Arg z))^2+
       ((|.z.| to_power n))^2*(sin (n*Arg z))^2 by SQUARE_1:68;then
       ((|.s.| to_power n))^2*((cos (n*Arg s))^2+(sin (n*Arg s))^2)
       =((|.z.| to_power n))^2*(cos (n*Arg z))^2+
       ((|.z.| to_power n))^2*(sin (n*Arg z))^2 by XCMPLX_1:8;then
       ((|.s.| to_power n))^2*((cos (n*Arg s))^2+(sin (n*Arg s))^2)
       =((|.z.| to_power n))^2*((cos (n*Arg z))^2+(sin (n*Arg z))^2)
                          by XCMPLX_1:8;then
       ((|.s.| to_power n))^2*((cos (n*Arg s))^2+(sin (n*Arg s))^2)
       =((|.z.| to_power n))^2*1 by SIN_COS:32;then
 A112: ((|.s.| to_power n))^2*1=((|.z.| to_power n))^2 by SIN_COS:32;
 A114: |.s.| to_power n > 0 & |.z.| to_power n > 0 by POWER:39,A102;then
       (|.s.| to_power n)
       =sqrt ((|.z.| to_power n))^2 by A112,SQUARE_1:89;then
       |.s.| to_power n=|.z.| to_power n by A114,SQUARE_1:89;then
       |.s.| |^ n=|.z.| to_power n by A102,POWER:46;then
       |.s.| |^ n=|.z.| |^ n by A102,POWER:46;then
       |.s.| = n-root (|.z.| |^ n) by A100,A102,POWER:5;
      hence thesis by A100,A102,POWER:5;
end;
