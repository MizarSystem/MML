environ

 vocabularies COMPLEX1, ARYTM_3, ARYTM_1, POLYEQ_1, SQUARE_1, FUNCT_3,
      POLYEQ_3, RELAT_1, POWER, GROUP_1, FUNCT_1, SIN_COS, COMPTRIG, XCMPLX_0,
      ARYTM;
 notations SUBSET_1, NUMBERS, XBOOLE_0, ORDINAL1, XCMPLX_0, XXREAL_0, XREAL_0,
      REAL_1, COMPLEX1, POLYEQ_1, SQUARE_1, NEWTON, ARYTM_0, QUIN_1, POWER,
      NAT_1, NAT_D, SIN_COS, PREPOWER, BINARITH, SEQ_1, COMPTRIG;
 constructors FUNCT_4, ARYTM_0, REAL_1, SQUARE_1, NAT_1, SEQ_1, QUIN_1,
      PREPOWER, POWER, POLYEQ_1, BINARITH, SIN_COS, COMPTRIG, VALUED_1,
      BINOP_2, NAT_D, XREAL_0;
 registrations ORDINAL1, NUMBERS, XCMPLX_0, XXREAL_0, XREAL_0, SQUARE_1, NAT_1,
      MEMBERED, NEWTON, POLYEQ_1, XBOOLE_0, VALUED_0, RELSET_1, POWER, SIN_COS;
 requirements ARITHM, SUBSET, REAL, BOOLE, NUMERALS;
 definitions POLYEQ_1, SQUARE_1, XCMPLX_0, QUIN_1;
 theorems COMPLEX1, POLYEQ_1, SQUARE_1, NEWTON, COMPTRIG, NAT_1, POWER,
      SIN_COS, SIN_COS2, COMPLEX2, XCMPLX_0, XCMPLX_1, XREAL_1;
 schemes NAT_1, COMPTRIG;

begin

reserve a,b,c,d,a',b',c',d',x,y,x1,u,v for Real,
  s,t,h,z,z1,z2,z3,s1,s2,s3,p,q for Element of COMPLEX;

definition
  let a be Real,z;
  redefine func a*z -> Element of COMPLEX;
  correctness by XCMPLX_0:def 2;
  redefine func a + z -> Element of COMPLEX;
  correctness by XCMPLX_0:def 2;
end;

definition
  let z be Element of COMPLEX;
  redefine func z^2 -> Element of COMPLEX equals
  (Re z)^2-(Im z)^2 + 2*(Re z*Im z)*<i>;
  compatibility
  proof
    z = Re z +(Im z)*<i> by COMPLEX1:29;
    hence thesis;
  end;
  correctness by XCMPLX_0:def 2;
end;

definition
  let a,b,c,z;
  redefine func Polynom (a,b,c,z) -> Element of COMPLEX;
  coherence by XCMPLX_0:def 2;
end;

canceled 3;

theorem Th4:
  a <> 0 & delta(a,b,c) >=0 & Polynom(a,b,c,z) = 0 implies
  z= (-b+sqrt delta(a,b,c))/(2*a) or z= (-b-sqrt delta(a,b,c))/(2*a) or
  z= -b/(2*a)
proof
A1: a = a+0*<i>;
  assume
A2: a <> 0 & delta(a,b,c) >= 0;
  assume
A3: Polynom(a,b,c,z) = 0;
A4: z = Re z + (Im z)*<i> by COMPLEX1:29;
  set x=Re z;
  set y=Im z;
  (a+0*<i>)*(x^2-y^2+2*(x*y)*<i>)+b*z+c = 0 by A3,A4;
  then 0 = Re a*Re (x^2-y^2+(2*x*y)*<i>) -Im a*Im (x^2-y^2+2*x*y*<i>)+
  (Re a*Im (x^2-y^2+2*x*y*<i>) +Re (x^2-y^2+2*x*y*<i>)*Im a)*<i>
  +b*z+c by COMPLEX1:def 10
    .=(a*Re (x^2-y^2+2*x*y*<i>)-Im a*
  Im (x^2-y^2+2*x*y*<i>))+(Re a*Im (x^2-y^2+2*x*y*<i>)
  +Re (x^2-y^2+2*x*y*<i>)*Im a)*<i> +b*z+c by A1,COMPLEX1:28
    .=(a*(x^2-y^2)-Im a* Im (x^2-y^2+2*x*y*<i>))+(Re a*Im (x^2-y^2+2*x*y*<i>)
  +Re (x^2-y^2+2*x*y*<i>)*Im a)*<i> +b*z+c by COMPLEX1:28
    .=(a*(x^2-y^2)-0*Im (x^2-y^2+2*x*y*<i>)
  )+(Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
  *Im a )*<i> +b*z+c by A1,COMPLEX1:28
    .=(a*(x^2-y^2)-0)+(Re a*(2*x*y)+Re (x^2-y^2+2*x*y*<i>)
  *Im a )*<i> +b*z+c by COMPLEX1:28
    .=(a*(x^2-y^2)-0)+(a*(2*x*y)+Re (x^2-y^2+2*x*y*<i>)
  *Im a )*<i> +b*z+c by A1,COMPLEX1:28
    .=(a*(x^2-y^2)-0)+(a*(2*x*y)+(x^2-y^2)
  *Im a )*<i> +b*z+c by COMPLEX1:28
    .=(a*(x^2-y^2)-0)+(a*(2*x*y)+(x^2-y^2)*0)*<i> +b*z+c by A1,COMPLEX1:28;
  then
A6: (a*(x^2-y^2)-0*(2*x*y))+(0+a*(2*x*y))*<i>
  +(b+0*<i>)*(x+y*<i>)+c = 0 by COMPLEX1:29;
  then
A7: a*(x^2-y^2)+b*x+c+(a*(2*x*y)+b*y)*<i> = 0;
  then
A8: a*(x^2-y^2)+b*x+c = 0 & (2*a*x+b)*y = 0 by COMPLEX1:12,28;
  per cases by A8;
  suppose
A9: y = 0;
    then Polynom(a,b,c,x)=0 by A6;
    then Re z=(-b+sqrt delta(a,b,c))/(2*a) & Im z = 0 or
    Re z= (-b-sqrt delta(a,b,c))/(2*a) & Im z = 0 by A2,A9,POLYEQ_1:5;
    then z=(-b+sqrt delta(a,b,c))/(2*a)+0*<i> or
    z= (-b-sqrt delta(a,b,c))/(2*a)+0*<i> by COMPLEX1:29;
    hence thesis;
  end;
  suppose
A10: 2*a*x+b = 0;
A11: x= (-b)/(2*a) by A10,XCMPLX_1:90,A2;
    then a*((b/(2*a))^2-y^2)+b*(-b/(2*a))+c = 0 by A7,COMPLEX1:12,28;
    then (b/(2*a))^2-y^2 = (-(b*(-b/(2*a))+c))/a-0 by A2,XCMPLX_1:90;
    then (b/(2*a))^2- (-(b*(-b/(2*a))+c))/a= y^2-0;
    then y^2 = (b/(2*a))^2+c*a"-((b^2/(2*a))*a");
    then
A12: y^2 *((2*a)^2)=(b^2/(2*a)^2+c*a"-((b^2/(2*a))*a"))*((2*a)^2)
    by XCMPLX_1:77
      .=b^2/((2*a)^2)*((2*a)^2)+(c*a")*(2*a)^2
    -(b^2*(2*a)"*a")*(2*a)^2;
A15: y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2-(b^2*((2*a)"*a"))*(2*a)^2
    by A12,XCMPLX_1:88,A2
      .=b^2+(c*a")*(2*a)^2-(b^2*((2*a)*a)")*(2*a)^2
    by XCMPLX_1:205
      .=b^2+(c*a")*(2*a)^2-(b^2*(2*(a*a))")*(2*a)^2;
    set t = (b^2*(2*(a*a))")*(2*a)^2;
    t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*(1/(2*a)^2));
    then t*((2*a)^2)" = b^2*(2*(a*a))"*1 by A2,XCMPLX_1:107;
    then t*((2*a)^2)"*2" = b^2*(2*a^2)"*2";
    then t*((2*a)^2)"*2" = b^2*((2*a^2)"*2");
    then t*((2*a)^2)"*2" = b^2*((2*(a^2*2))") by XCMPLX_1:205;
    then (t*2")/(2*a)^2*(2*a)^2 = b^2/(2*a)^2*(2*a)^2;
    then t*2" = b^2/(2*a)^2*(2*a)^2 by A2,XCMPLX_1:88;
    then
A16: t/2 = b^2 by A2,XCMPLX_1:88;
    set t=(c*a")*(2*a)^2;
    t=(c/a*a)*2*(2*a);
    then
A17: t=c*2*(2*a) by A2,XCMPLX_1:88;
    -delta(a,b,c) <= 0 by A2;
    then (y *(2*a))^2 = 0 by A15,A16,A17,XREAL_1:65;
    then Re z=-b/(2*a) & Im z=0 by A11,A2;
    then z = -b/(2*a)+0*<i> by COMPLEX1:29;
    hence thesis;
  end;
end;

theorem Th5:
  a <> 0 & delta(a,b,c) < 0 & Polynom(a,b,c,z) = 0 implies
  z= -b/(2*a)+(sqrt (-delta(a,b,c))/(2*a))*<i> or
  z= -b/(2*a)+(-sqrt (-delta(a,b,c))/(2*a))*<i>
proof
A1: a= a+0*<i>;
  assume
A2: a <> 0 & delta(a,b,c) < 0;
  now
    let z;
    assume
A3: Polynom(a,b,c,z) = 0;
    set x=Re z;
    set y=Im z;
A4: z = x + y*<i> by COMPLEX1:29;
    (a+0*<i>)*(x^2-y^2+(2*x*y)*<i>)+b*z+c = 0 by A3,A4;
    then Re a*Re (x^2-y^2+2*x*y*<i>)-Im a*Im (x^2-y^2+(2*x*y)*<i>)
    +(Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
    *Im a)*<i>+b*z+c = 0 by COMPLEX1:def 10;
    then (a*Re (x^2-y^2+2*x*y*<i>)-Im a*Im (x^2-y^2+2*x*y*<i>)
    )+(Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
    *Im a)*<i>+b*z+c = 0 by A1,COMPLEX1:28;
    then (a*(x^2-y^2)-Im a*Im (x^2-y^2+2*x*y*<i>)
    )+(Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
    *Im a)*<i>+b*z+c = 0 by COMPLEX1:28;
    then (a*(x^2-y^2)-0*Im (x^2-y^2+2*x*y*<i>)
    )+(Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
    *Im a)*<i>+b*z+c = 0 by A1,COMPLEX1:28;
    then (a*(x^2-y^2)-0)+(Re a*(2*x*y)+Re (x^2-y^2+2*x*y*<i>)
    *Im a)*<i>+b*z+c = 0 by COMPLEX1:28;
    then (a*(x^2-y^2)-0)+(a*(2*x*y)+Re (x^2-y^2+2*x*y*<i>)
    *Im a)*<i>+b*z+c = 0 by A1,COMPLEX1:28;
    then (a*(x^2-y^2)-0)+(a*(2*x*y)+(x^2-y^2) *Im a)*<i>+b*z+c = 0
     by COMPLEX1:28;
    then (a*(x^2-y^2)-0)+(a*(2*x*y)+(x^2-y^2) *0)*<i>+b*z+c = 0
    by A1,COMPLEX1:28;
    then (a*(x^2-y^2)-0*(2*x*y))+((x^2-y^2)*0+a*(2*x*y))*<i>
    +(b+0*<i>)*(x+y*<i>)+c = 0 by COMPLEX1:29;
    then
A6: a*(x^2-y^2)+b*x+c+(a*(2*x*y)+b*y)*<i> = 0;
    then -a*y^2+((b*x+a*x^2)+c )+a*y^2= 0+a*y^2 by COMPLEX1:12,28;
    then
A7: a*x^2*a+b*x*a+c*a = a*y^2*a by XCMPLX_1:9;
    set t1= (x*a+b/2)^2;
    set t2=(-b^2+c*a*4)/4;
    0-delta(a,b,c)>0 by A2;
    then 0+0<t1+t2 by XREAL_1:10,65;
    then
A8: y<>0 by A7;
    (a*(2*x))*y+b*y = 0 by A6,COMPLEX1:12,28;
    then (a*2*x)*y = (-b)*y;
    then a*(2*x) = -b by A8,XCMPLX_1:5;
    then 2*x = (-b)/a by A2,XCMPLX_1:90;
    then x = 1/a*((-b)/2);
    then
A9: x = (-b)/(2*a) by XCMPLX_1:104;
    then a*((b/(2*a))^2-y^2)+b*(-b/(2*a))+c = 0 by A6,COMPLEX1:12,28;
    then (b/(2*a))^2-y^2 = (-(b*(-b/(2*a))+c))/a-0 by A2,XCMPLX_1:90;
    then (b/(2*a))^2- (-(b*(-b/(2*a))+c))/a= y^2-0;
    then y^2 = (b/(2*a))^2+c*a"-((b^2/(2*a))*a");
    then
A10: y^2 *((2*a)^2)=(b^2/(2*a)^2+c*a"-((b^2/(2*a))*a"))*((2*a)^2)
    by XCMPLX_1:77
      .=b^2/((2*a)^2)*((2*a)^2)+(c*a")*(2*a)^2
    -(b^2*(2*a)"*a")*(2*a)^2;
A13: y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2-(b^2*((2*a)"*a"))*(2*a)^2
    by A10,XCMPLX_1:88,A2
      .=b^2+(c*a")*(2*a)^2-(b^2*((2*a)*a)")*(2*a)^2
    by XCMPLX_1:205
      .=b^2+(c*a")*(2*a)^2-(b^2*(2*(a*a))")*(2*a)^2;
    set t = (b^2*(2*(a*a))")*(2*a)^2;
    t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*(1/(2*a)^2));
    then t*((2*a)^2)" = b^2*(2*(a*a))"*1 by A2,XCMPLX_1:107;
    then t*((2*a)^2)"*2" = b^2*(2*a^2)"*2";
    then t*((2*a)^2)"*2" = b^2*((2*a^2)"*2");
    then t*((2*a)^2)"*2" = b^2*((2*(a^2*2))") by XCMPLX_1:205;
    then (t*2")/(2*a)^2*(2*a)^2 = b^2/(2*a)^2*(2*a)^2;
    then t*2" = b^2/(2*a)^2*(2*a)^2 by A2,XCMPLX_1:88;
    then
A14: t/2 = b^2 by A2,XCMPLX_1:88;
    set t=(c*a")*(2*a)^2;
    t=(c/a*a)*2*(2*a);
    then
A15: t=c*2*(2*a) by A2,XCMPLX_1:88;
    (y *(2*a))^2 =(sqrt (-delta(a,b,c)))^2 by A13,A14,A15,SQUARE_1:def 4,A2;
    then ((y *(2*a))+(sqrt (-delta(a,b,c))))*
    ((y *(2*a))-(sqrt (-delta(a,b,c))))=0;
    then ((y *(2*a))+(sqrt (-delta(a,b,c))))=0 or
    ((y *(2*a))-(sqrt (-delta(a,b,c))))=0;
    then y =(-sqrt (-delta(a,b,c)))/(2*a) or
    y *(2*a)/(2*a)=sqrt (-delta(a,b,c))/(2*a) by A2,XCMPLX_1:90;
    then Re z=-b/(2*a) & Im z=sqrt (-delta(a,b,c))/(2*a)
    or Re z= -b/(2*a) & Im z= -sqrt (-delta(a,b,c))/(2*a)
    by A9,A2,XCMPLX_1:90;
    hence z= -b/(2*a)+(sqrt(-delta(a,b,c))/(2*a))*<i> or
    z= -b/(2*a)+(-sqrt (-delta(a,b,c))/(2*a))*<i> by COMPLEX1:29;
  end;
  hence thesis;
end;

theorem
  b <> 0 & (for z holds Polynom(0,b,c,z) = 0) implies z = -c/b
proof
A1: 0 = 0+0*<i>;
  assume
A2: b <> 0;
  assume
A3: (for z holds Polynom(0,b,c,z) = 0);
  now
    let z1;
    Polynom(0,b,c,z1) = 0 by A3;
    then b*(Re z1+(Im z1)*<i>)+c = 0 by COMPLEX1:29;
    then (b*Re z1-0+c+(b*Im z1+0)*<i>)= 0+0*<i>;
    then b*Re z1-0+c = Re 0 & b*Im z1= Im 0 by COMPLEX1:28;
    then b*(Re z1)-0+c = 0 & b*Im z1 = 0 by A1,COMPLEX1:28;
    then Re z1 =(-c)/b & Im z1 = 0 by A2,XCMPLX_1:90;
    then z1 = -c/b+0*<i> by COMPLEX1:29;
    hence z1 = -c/b;
  end;
  hence thesis;
end;

theorem
  for a,b,c being Real,z,z1,z2 being complex number st a <> 0 &
  for z being complex number holds Polynom(a,b,c,z) = Quard(a,z1,z2,z)
  holds b/a =-(z1+z2) & c/a = z1*z2
proof
  let a,b,c be Real,z,z1,z2 be complex number;
  assume
A1: a <> 0;
  assume
A2: for z being complex number holds Polynom(a,b,c,z) = Quard(a,z1,z2,z);
A3: Polynom(a,b,c,0) = Quard(a,z1,z2,0) by A2;
  Quard(a,z1,z2,1) = Polynom(a,b,c,1) by A2
    .=a+b+c;
  then a+b+c =a+a*(-(z1+z2))+c by A3;
  hence thesis by A1,A3,XCMPLX_1:204;
end;

definition
  let z be complex number;
  func z^3 -> Element of COMPLEX equals

  z^2*z;
  correctness by XCMPLX_0:def 2;
end;

Lm2: for z be complex number holds z|^2= z*z
proof
  let z be complex number;
  z|^(1+1) = (z|^1)*z by NEWTON:11
    .= z*z by NEWTON:10;
  hence thesis;
end;

definition
  let a,b,c,d,z be complex number;
  redefine func Polynom(a,b,c,d,z) equals

  a*z^3+b*z^2+c*z+d;
  compatibility
  proof
    let x be set;
    Polynom(a,b,c,d,z) = a*(z |^(2+1))+ b*z^2 +c*z +d
      .= a*((z |^2)*z)+ b*z^2 +c*z +d by NEWTON:11;
    hence thesis by Lm2;
  end;
end;

canceled 3;

theorem Th11:
  Re z^3 = (Re z)|^3 - 3*Re z*(Im z)^2 & Im z^3 = -(Im z)|^ 3+3*(Re z)^2*Im z
proof
A1: Re z + (Im z)*<i> = z by COMPLEX1:29;
  set x = Re z;
  set y = Im z;
  z^3 = Re(x^2-y^2+(2*(x*y))*<i>) *Re (x+y*<i>)-Im(x^2-y^2+(2*(x*y))*<i>)
  *Im (x+y*<i>)+(Re(x^2-y^2+(2*(x*y))*<i>) *Im (x+y*<i>)+
  Re (x+y*<i>)*Im (x^2-y^2+(2*(x*y))*<i>))*<i> by A1,COMPLEX1:def 10
    .= ((x^2-y^2) *Re (x+y*<i>)-Im(x^2-y^2+(2*(x*y))*<i>)
  *Im (x+y*<i>))+(Re(x^2-y^2+(2*(x*y))*<i>) *Im (x+y*<i>)+
  Re (x+y*<i>)*Im (x^2-y^2+(2*(x*y))*<i>))*<i> by COMPLEX1:28
    .= ((x^2-y^2) *Re (x+y*<i>)-(2*(x*y))
  *Im (x+y*<i>))+(Re(x^2-y^2+(2*(x*y))*<i>) *Im (x+y*<i>)+
  Re (x+y*<i>)*Im (x^2-y^2+(2*(x*y))*<i>))*<i> by COMPLEX1:28
    .= ((x^2-y^2) *Re (x+y*<i>)-(2*(x*y))
  *Im (x+y*<i>))+((x^2-y^2) *Im (x+y*<i>)+
  Re (x+y*<i>)*Im (x^2-y^2+(2*(x*y))*<i>))*<i> by COMPLEX1:28
    .= ((x^2-y^2) *Re (x+y*<i>)-(2*(x*y))*Im (x+y*<i>)
  )+((x^2-y^2) *Im (x+y*<i>)+Re (x+y*<i>)*(2*(x*y)))*<i> by COMPLEX1:28
    .= ((x^2-y^2) *x-(2*(x*y))*Im (x+y*<i>)
  )+((x^2-y^2) *Im (x+y*<i>)+Re (x+y*<i>)*(2*(x*y)))*<i> by COMPLEX1:28
    .= ((x^2-y^2) *x-(2*(x*y))*y
  )+((x^2-y^2) *Im (x+y*<i>)+Re (x+y*<i>)*(2*(x*y)))*<i> by COMPLEX1:28
    .= ((x^2-y^2) *x-(2*(x*y))*y
  )+((x^2-y^2) *y+Re (x+y*<i>)*(2*(x*y)))*<i> by COMPLEX1:28
    .= ((x^2*x-y^2*x+0*x)-2*(x*y)*y)+((x^2-y^2+0)*y+x*(2*(x*y)))*<i>
    by COMPLEX1:28
    .= ((x|^1*x*x-y^2*x)-2*(x*y)*y)+((x^2*y-y^2*y)+x*(2*(x*y)))*<i>
    by NEWTON:10
    .= ((x|^ (1+1)*x-y^2*x)-2*(x*y)*y)+((x^2*y-y^2*y)+x*(2*(x*y)))*<i>
  by NEWTON:11
    .= ((x|^(2+1)-y^2*x)-2*(x*y)*y)+((x^2*y-y^2*y)+x*(2*(x*y)))*<i>
  by NEWTON:11
    .= ((x|^ 3-y^2*x)-2*(x*y)*y)+((x^2*y-y|^ 1*y*y)+x*(2*(x*y)))*<i>
  by NEWTON:10
    .= ((x|^ 3-y^2*x)-2*(x*y)*y)+((x^2*y-y|^ (1+1)*y)+x*(2*(x*y)))*<i>
  by NEWTON:11
    .= ((x|^ 3-y^2*x)-2*(x*(y*y)))+((x^2*y-y|^ (2+1))+x*(2*(x*y)))*<i>
     by NEWTON:11
    .= (x|^ 3-3*x*y^2 )+(-y|^ 3+3*x^2*y)*<i>;
  hence thesis by COMPLEX1:28;
end;

theorem
  (for z being complex number holds Polynom(a,b,c,d,z) = Polynom(a',b',c',d',z
  ) ) implies a = a' & b = b' & c = c' & d = d'
proof
  assume
A1: for z being complex number holds
  Polynom(a,b,c,d,z) = Polynom(a',b',c',d',z);
  then
A2: Polynom(a,b,c,d,0) = Polynom(a',b',c',d',0);
A3: Polynom(a,b,c,d,1) = Polynom(a',b',c',d',1) by A1;
A4: Polynom(a,b,c,d,-1) = Polynom(a',b',c',d',-1) by A1;
A5: Polynom(a,b,c,d,2) = Polynom(a',b',c',d',2) by A1;
  hence a = a' by A2,A3,A4;
  thus b = b' by A2,A3,A4;
  thus c = c' by A2,A3,A4,A5;
  thus thesis by A2;
end;

