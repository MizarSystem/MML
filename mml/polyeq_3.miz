theorem
    CPoly2(0c,0c,0c,z) = 0
proof
    CPoly2(0c,0c,0c,z) = 0c*z^2+0c*z+0c by Def6
     .=0c*z^2+0c+0c by COMPLEX1:28
     .=0c+0c+0c by COMPLEX1:28
     .=0c+0c by COMPLEX1:22;
  hence thesis by COMPLEX1:14;
end;

theorem Th34:
  z1<>0 & CPoly2(z1,0c,0c,z) = 0 implies z=0
proof
    assume
 A1: z1<>0 & CPoly2(z1,0c,0c,z) = 0;
      then 0= z1*z^2+0c*z+0c by Def6
       .=z1*z^2+0c+0c by COMPLEX1:28
       .=z1*z^2+0c by COMPLEX1:22
       .=z1*z^2 by COMPLEX1:22;
       then z^2=0 by A1,XCMPLX_1:6;
       then z*z=0 by SQUARE_1:def 3;
       hence z=0 by XCMPLX_1:6;
end;

theorem
      z1<>0 & CPoly2(z1,z2,0c,z)=0 implies z=-(z2/z1) or z=0
proof
       assume
 A1: z1<>0 & CPoly2(z1,z2,0c,z)=0;
       then 0=z1*z^2+z2*z+0 by Def6,COMPLEX1:def 6
       .=z1*(z*z)+z2*z by SQUARE_1:def 3
       .=(z1*z+z2)*z;
       then z1*z+z2=0 or z=0 by XCMPLX_1:6;
       then Poly1(z1,z2,z) = 0 or z=0 by POLYEQ_1:def 1;
    hence z=-(z2/z1) or z=0 by A1,Th24;
end;

theorem Th36:
    z1<>0c & CPoly2(z1,0c,z3,z)=0c implies
    (for s st s=-(z3/z1) holds
       z=[* sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
            sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
       z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
           -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
       z=[* sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
           -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
       z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
            sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*])
proof
       assume
 A1: z1<>0c & CPoly2(z1,0c,z3,z)=0c;
       let s;
       assume
 A2: s=-(z3/z1);
         0= z1*z^2+0c*z+z3 by A1,Def6,COMPLEX1:def 6
       .=z1*z^2+0c+z3 by COMPLEX1:28
       .=z1*z^2+z3 by COMPLEX1:22;
       then z1*z^2=-z3 by XCMPLX_0:def 6;
       then z^2=(-z3)/z1 by A1,COMPLEX1:def 6,XCMPLX_1:90;
       then z^2=s by A2,XCMPLX_1:188;
      hence thesis by Th32;
end;

theorem Th37:
    z1<>0 & CPoly2(z1,z2,z3,z)=0c implies
          (for h,t st h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1) holds
           z=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
                sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
               -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
               -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
                sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t)
proof
       assume
 A1: z1<>0 & CPoly2(z1,z2,z3,z)=0c;
       let h,t;
       assume
 A2: h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1);
        z1*z^2+z2*z+z3=0c by A1,Def6;
      then (z1*z^2+z2*z+z3)/z1=0c by COMPLEX1:87;
      then (z1*z^2+z2*z)/z1+z3/z1=0c by XCMPLX_1:63;
      then (z^2*z1)/z1+(z2*z)/z1+z3/z1=0c by XCMPLX_1:63;
      then z^2+(z*z2)/z1+z3/z1=0c by A1,XCMPLX_1:90;
      then z^2+(z*z2)*z1"+z3/z1=0c by XCMPLX_0:def 9;
      then z^2+z*(z2*z1")+z3/z1=0c;
 then A3:z^2+(z2/z1)*z+z3/z1=0c by XCMPLX_0:def 9;
        [*2,0*]<>0 by ARYTM_0:def 7;
 then z^2+[*2,0*]*z2/([*2,0*]*z1)*z+z3/z1=0c by A3,XCMPLX_1:92;
      then z^2+[*2,0*]*(z2/([*2,0*]*z1))*z+z3/z1=0c
                  by XCMPLX_1:75;
      then z^2+[*2,0*]*z*(z2/([*2,0*]*z1))+z3/z1=0c;
      then z^2+2*z*(z2/([*2,0*]*z1))+z3/z1=0c by Def1;
      then z^2+2*z*(z2/(2*z1))+(z2/(2*z1))^2-(z2/(2*z1))^2+z3/z1=0c
                                   by Def1;
      then (z+z2/(2*z1))^2-(z2/(2*z1))^2+z3/z1=0c by SQUARE_1:63;
      then (z+(z2/(2*z1)))^2-((z2/(2*z1))^2-z3/z1)=0c by XCMPLX_1:37;
        then (z+t)^2=h by A2,COMPLEX1:def 6,XCMPLX_1:15;
      then z+t-t=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
             sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
      z+t-t=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
            -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
      z+t-t=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
            -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
      z+t-t=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
        sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t by Th32;
    hence thesis by XCMPLX_1:26;
end;

definition let z1,z2,z3,z4,z;
  func CPoly3(z1,z2,z3,z4,z) -> Element of COMPLEX equals
     :Def7:   z1*z^3 +z2*z^2+z3*z+z4;
  coherence;
end;

theorem
    z^2=1 implies z=1 or z=-1
proof
  assume z^2=1;
  then z*z = 1 by SQUARE_1:def 3;
  hence thesis by XCMPLX_1:183;
end;

theorem Th39:
  z#N 3=z*z*z & z#N 3=z^2*z & z#N 3 = z^3
proof
  A1: z#N 3=z GeoSeq.(2+1) by COMSEQ_3:def 2
         .=z GeoSeq.(1+1)*z by COMSEQ_3:def 1
         .=z GeoSeq.(0+1)*z*z by COMSEQ_3:def 1
         .=z GeoSeq.0*z*z*z by COMSEQ_3:def 1
         .=1r*z*z*z by COMSEQ_3:def 1
         .=z*z*z by COMPLEX1:29;
         then z#N 3=z^2*z by SQUARE_1:def 3;
  hence thesis by A1,Def4;
end;

theorem
      z1<>0 & CPoly3(z1,z2,0c,0c,z)=0c implies z=-z2/z1 or z=0
proof
    assume
 A1: z1<>0 & CPoly3(z1,z2,0c,0c,z)=0c;
       then 0c=z1*z^3 +z2*z^2+0c*z+0c by Def7
        .=z1*z^3 +z2*z^2+0c+0c by COMPLEX1:28
        .=z1*z^3 +z2*z^2+0c by COMPLEX1:22
        .=z1*z^3 +z2*z^2 by COMPLEX1:22
        .=z1*(z^2*z) +z2*z^2 by Def4
        .=(z1*z +z2)*z^2;
        then z1*z +z2=0c or z^2=0c by COMPLEX1:def 6,XCMPLX_1:6;
        then z1*z +z2=0c or 1r*z^2=0c by COMPLEX1:29;
        then z1*z +z2=0c or 1r*z^2+0c=0c by COMPLEX1:22;
        then z1*z +z2=0c or 1r*z^2+0c+0c=0c by COMPLEX1:22;
        then Poly1(z1,z2,z)=0c or 1r*z^2+0c*z+0c=0c
          by COMPLEX1:28,POLYEQ_1:def 1;
         then z=-(z2/z1) or CPoly2(1r,0c,0c,z)=0 by A1,Def6,Th24,COMPLEX1:def 6
;
       hence z=-(z2/z1) or z=0 by Th34,COMPLEX1:def 7;
end;

theorem
      z1<>0c & CPoly3(z1,0c,z3,0c,z)=0c implies
     (for s st s=-(z3/z1) holds
     z=0c or
     z=[*sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
          sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
     z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
         -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
     z=[* sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
         -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
     z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
          sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*])
proof
       assume
 A1: z1<>0c & CPoly3(z1,0c,z3,0c,z)=0c;
       let s;
       assume
 A2: s=-(z3/z1);
        0c=z1*z^3 +0c*z^2+z3*z+0c by A1,Def7
       .=z1*z#N 3 +0c*z^2+z3*z+0c by Th39
       .=z1*z#N 3 +0c*z^2+z3*z by COMPLEX1:22
       .=z1*z#N 3 +0c+z3*z by COMPLEX1:28
       .=z1*z#N 3+z3*z by COMPLEX1:22
       .=z1*(z^2*z)+z3*z by Th39
       .=(z1*z^2+z3)*z;
       then z1*z^2+0c*z+z3=0c or z=0c
         by COMPLEX1:def 6,XCMPLX_1:6;
       then CPoly2(z1,0c,z3,z)=0c or z=0c by Def6;
     hence thesis by A1,A2,Th36;
end;

theorem
      z1<>0 & CPoly3(z1,z2,z3,0c,z)=0c implies
     (for s,h,t st s=-(z3/z1) & h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1) holds
           z=0 or
           z=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
                sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
               -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
               -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
                sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t)
proof
       assume
 A1: z1<>0 & CPoly3(z1,z2,z3,0c,z)=0c;
       let s,h,t;
       assume
 A2: s=-(z3/z1) & h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1);
         0c=z1*z^3 +z2*z^2+z3*z+0c by A1,Def7
       .=z1*z#N 3 +z2*z^2+z3*z+0c by Th39
       .=z1*z#N 3 +z2*z^2+z3*z by COMPLEX1:22
       .=z1*(z^2*z) +z2*z^2+z3*z by Th39
       .=z1*z^2*z +z2*(z*z)+z3*z by SQUARE_1:def 3
       .=(z1*z^2+z2*z+z3)*z
       .=CPoly2(z1,z2,z3,z)*z by Def6;
       then z=0c or CPoly2(z1,z2,z3,z)=0c by COMPLEX1:def 6,XCMPLX_1:6;
     hence thesis by A1,A2,Th37,COMPLEX1:def 6;
end;

theorem Th43:
   z=s-[*1/3,0*]*z1 implies
    z^2 =s^2+(-[*2/3,0*])*z1*s+[*1/9,0*]*z1^2
proof
    assume z=s-[*1/3,0*]*z1;
       then z^2=(s+(-[*1/3,0*]*z1))^2
      .=s^2+2*s*(-[*1/3,0*]*z1)+(-[*1/3,0*]*z1)^2 by SQUARE_1:63
      .=s^2+2*s*(-[*1/3,0*]*z1)+([*1/3,0*]*z1)^2 by SQUARE_1:61
      .=s^2+2*s*(-[*1/3,0*])*z1+[*1/3,0*]^2*z1^2 by SQUARE_1:68
      .=s^2+[*2,0*]*s*(-[*1/3,0*])*z1+[*1/3,0*]^2*z1^2 by Def1
      .=s^2+[*2,0*]*(-[*1/3,0*])*z1*s+[*1/3,0*]^2*z1^2
      .=s^2+[*2,0*]*((-1r)*[*1/3,0*])*z1*s+[*1/3,0*]^2*z1^2 by COMPLEX1:46
      .=s^2+(-1r)*([*2,0*]*[*1/3,0*])*z1*s+[*1/3,0*]^2*z1^2
      .=s^2+(-1r)*([*Re [*2,0*]*Re [*1/3,0*]-Im [*2,0*]*Im [*1/3,0*]
      ,Re [*2,0*]*Im [*1/3,0*]+Re [*1/3,0*]*Im [*2,0*]*])*z1*s+[*1/3,0*]^2*z1^2
                                by COMPLEX1:def 10
      .=s^2+(-1r)*([*Re [*2,0*]*Re [*1/3,0*]-Im [*2,0*]*Im [*1/3,0*]
      ,Re [*2,0*]*Im [*1/3,0*]+Re [*1/3,0*]*0*])*z1*s+[*1/3,0*]^2*z1^2
                                by COMPLEX1:7
      .=s^2+(-1r)*([*Re [*2,0*]*Re [*1/3,0*]-Im [*2,0*]*Im [*1/3,0*]
      ,Re [*2,0*]*0+0*])*z1*s+[*1/3,0*]^2*z1^2 by COMPLEX1:7
      .=s^2+(-1r)*([*Re [*2,0*]*Re [*1/3,0*]-Im [*2,0*]*0,0*])*z1*s
             +[*1/3,0*]^2*z1^2 by COMPLEX1:7
      .=s^2+(-1r)*([*Re [*2,0*]*(1/3)-0,0*])*z1*s+[*1/3,0*]^2*z1^2
                                          by COMPLEX1:7
      .=s^2+(-1r)*([*2*(1/3)-0,0*])*z1*s+[*1/3,0*]^2*z1^2 by COMPLEX1:7
      .=s^2+(-[*2/3,0*])*z1*s+[*1/3,0*]^2*z1^2 by COMPLEX1:46
      .=s^2+(-[*2/3,0*])*z1*s+[*(Re [*1/3,0*])^2-(Im [*1/3,0*])^2
         ,2*(Re [*1/3,0*]*Im [*1/3,0*])*]*z1^2 by Def3
      .=s^2+(-[*2/3,0*])*z1*s+[*(Re [*1/3,0*])^2-(Im [*1/3,0*])^2
         ,2*(Re [*1/3,0*]*0)*]*z1^2 by COMPLEX1:7
      .=s^2+(-[*2/3,0*])*z1*s+[*(Re [*1/3,0*])^2-0^2,0*]*z1^2 by COMPLEX1:7
      .=s^2+(-[*2/3,0*])*z1*s+[*(1/3)^2-0,0*]*z1^2 by COMPLEX1:7,SQUARE_1:60
      .=s^2+(-[*2/3,0*])*z1*s+[*(1/3)*(1/3)-0,0*]*z1^2 by SQUARE_1:def 3;
 hence thesis;
end;

theorem Th44:
   z=s-[*1/3,0*]*z1 implies
    z^3 =s^3-z1*s^2+[*1/3,0*]*z1^2*s-[*1/27,0*]*z1^3
proof
    assume
         z=s-[*1/3,0*]*z1;
 then A1: z^3=(s+(-[*1/3,0*]*z1))^3
      .=s^3+3*(-[*1/3,0*]*z1)*s^2+3*(-[*1/3,0*]*z1)^2*s+(-[*1/3,0*]*z1)^3
                                    by Th13;
      set s1=-[*1/3,0*]*z1;
 A2:3*s1=[*3,0*]*((-[*1/3,0*])*z1) by Def1
         .=[*3,0*]*(-[*1/3,0*])*z1
         .=[*3,0*]*((-1r)*[*1/3,0*])*z1 by COMPLEX1:46
         .=(-1r)*([*3,0*]*[*1/3,0*])*z1
         .=(-1r)* [* Re [*3,0*]*Re [*1/3,0*]
            -Im [*3,0*]*Im [*1/3,0*],Re [*3,0*]*Im [*1/3,0*]
            +Re [*1/3,0*]*Im [*3,0*] *]*z1 by COMPLEX1:def 10
         .=(-1r)* [* Re [*3,0*]*Re [*1/3,0*]
            -Im [*3,0*]*0,Re [*3,0*]*Im [*1/3,0*]
            +Re [*1/3,0*]*Im [*3,0*] *]*z1 by COMPLEX1:7
         .=(-1r)* [* Re [*3,0*]*Re [*1/3,0*]-0,
             Re [*3,0*]*0 +Re [*1/3,0*]*Im [*3,0*] *]*z1 by COMPLEX1:7
         .=(-1r)* [* Re [*3,0*]*Re [*1/3,0*]-0,
             0+Re [*1/3,0*]*0 *]*z1 by COMPLEX1:7
         .=(-1r)* [* 3*Re [*1/3,0*]-0,Re [*1/3,0*]*0 *]*z1 by COMPLEX1:7
         .=(-1r)* [* 3*(1/3)-0,Re [*1/3,0*]*0 *]*z1 by COMPLEX1:7
         .=(-1r)*1r*z1 by ARYTM_0:def 7,COMPLEX1:def 7
         .=(-1r)*z1 by COMPLEX1:29
         .=-z1 by COMPLEX1:46;
 A3: s1^2=((-[*1/3,0*])*z1)*((-[*1/3,0*])*z1) by SQUARE_1:def 3
       .=(-[*1/3,0*])*(-[*1/3,0*])*(z1*z1)
       .=[*1/3,0*]*[*1/3,0*]*z1^2 by SQUARE_1:def 3
       .=[*1/3,0*]^2*z1^2 by SQUARE_1:def 3
       .=[*(Re [*1/3,0*])^2-(Im [*1/3,0*])^2,2*(Re [*1/3,0*]*Im [*1/3,0*])*]
               *z1^2 by Def3
       .=[*(Re [*1/3,0*])^2-(Im [*1/3,0*])^2,2*(Re [*1/3,0*]*0)*]*z1^2
                              by COMPLEX1:7
       .=[*(Re [*1/3,0*])^2-0^2,2*(Re [*1/3,0*]*0)*]*z1^2 by COMPLEX1:7
       .=[*(1/3)^2-0,2*(1/3)*0*]*z1^2 by COMPLEX1:7,SQUARE_1:60
       .=[*(1/3)*(1/3)-0,2*(1/3)*0*]*z1^2 by SQUARE_1:def 3
       .=[*1/9,0*]*z1^2;
 then A4: 3*s1^2=[*3,0*]*([*1/9,0*]*z1^2) by Def1
            .=[*3,0*]*[*1/9,0*]*z1^2
            .=[*Re [*3,0*]*Re [*1/9,0*]-Im[*3,0*] *Im [*1/9,0*]
              ,Re[*3,0*] *Im [*1/9,0*]+Re [*1/9,0*]*Im[*3,0*] *]*z1^2
                          by COMPLEX1:def 10
            .=[*Re [*3,0*]*Re [*1/9,0*]-0*Im [*1/9,0*]
              ,Re[*3,0*] *Im [*1/9,0*]+Re [*1/9,0*]*Im[*3,0*] *]*z1^2
                          by COMPLEX1:7
            .=[*Re [*3,0*]*Re [*1/9,0*]-0,Re[*3,0*] *Im [*1/9,0*]
              +Re [*1/9,0*]*0*]*z1^2 by COMPLEX1:7
            .=[*Re [*3,0*]*Re [*1/9,0*]-0,Re[*3,0*] *0
              +0 *]*z1^2 by COMPLEX1:7
            .=[*Re [*3,0*]*(1/9)-0,0 *]*z1^2 by COMPLEX1:7
            .=[*3*(1/9)-0,0 *]*z1^2 by COMPLEX1:7
            .=[*1/3,0 *]*z1^2;
       s1^3=([*1/9,0*]*z1^2)*((-[*1/3,0*])*z1) by A3,Def4
       .=[*1/9,0*]*(-[*1/3,0*])*(z1^2*z1)
       .=[*1/9,0*]*(-[*1/3,0*])*z1^3 by Def4
       .=[*1/9,0*]*((-1r)*[*1/3,0*])*z1^3 by COMPLEX1:46
       .=[*1/9,0*]*[*1/3,0*]*(-1r)*z1^3
       .=[* Re [*1/9,0*]*Re [*1/3,0*]-Im [*1/9,0*]*Im [*1/3,0*],
        Re [*1/9,0*]*Im [*1/3,0*]+Re [*1/3,0*]*Im [*1/9,0*] *]*(-1r)*z1^3
                    by COMPLEX1:def 10
       .=[* Re [*1/9,0*]*Re [*1/3,0*]-Im [*1/9,0*]*Im [*1/3,0*],
        Re [*1/9,0*]*Im [*1/3,0*]+Re [*1/3,0*]*0 *]*(-1r)*z1^3
                    by COMPLEX1:7
       .=[* Re [*1/9,0*]*Re [*1/3,0*]-Im [*1/9,0*]*Im [*1/3,0*],
        Re [*1/9,0*]*0 *]*(-1r)*z1^3 by COMPLEX1:7
       .=[* Re [*1/9,0*]*Re [*1/3,0*]-0*Im [*1/3,0*],0 *]*(-1r)*z1^3
                           by COMPLEX1:7
       .=[* 1/9*Re [*1/3,0*]-0*Im [*1/3,0*],0 *]*(-1r)*z1^3 by COMPLEX1:7
       .=[* 1/9*(1/3)-0,0 *]*(-1r)*z1^3 by COMPLEX1:7
       .=(-[*1/27,0*])*z1^3 by COMPLEX1:46;
     hence thesis by A1,A2,A4;
end;

theorem
      CPoly3(1r,z1,z2,z3,z)=0c implies
    (for p,q,s st z=s-[*1/3,0*]*z1 & p=-[*1/3,0*]*z1^2+z2
                & q=[*2/27,0*]*z1^3-[*1/3,0*]*z1*z2+z3 holds
      CPoly3(1r,0c,p,q,s)=0c )
proof
    assume
 A1:CPoly3(1r,z1,z2,z3,z) = 0c;
    let p,q,s;
     assume
 A2: z=s-[*1/3,0*]*z1 & p=-[*1/3,0*]*z1^2+z2
       & q=[*2/27,0*]*z1^3-[*1/3,0*]*z1*z2+z3;
         0c=1r*z^3+ z1*z^2+z2*z+z3 by A1,Def7
       .=1r*(s^3-z1*s^2+[*1/3,0 *]*z1^2*s-[*1/27,0*]*z1^3)+ z1*z^2+z2*z+z3
               by A2,Th44
       .=s^3-z1*s^2+[*1/3,0 *]*z1^2*s-[*1/27,0*]*z1^3+z1*z^2+z2*z+z3
               by COMPLEX1:29
       .=s^3-z1*s^2+[*1/3,0 *]*z1^2*s-[*1/27,0*]*z1^3+z1*(s^2+(-[*2/3,0*])
        *z1*s+[*1/9,0*]*z1^2)+z2*(s-[*1/3,0*]*z1)+z3 by A2,Th43
       .=s^3-z1*s^2+[*1/3,0 *]*z1^2*s-[*1/27,0*]*z1^3+(z1*s^2+z1*((-[*2/3,0*])
        *z1*s)+z1*z1^2*[*1/9,0*])+z2*(s-[*1/3,0*]*z1)+z3
       .=s^3-z1*s^2+[*1/3,0 *]*z1^2*s-[*1/27,0*]*z1^3+(z1*s^2+z1*z1*
      (-[*2/3,0*])*s+z1^3*[*1/9,0*])+z2*(s-[*1/3,0*]*z1)+z3 by Def4
      .=s^3-z1*s^2+[*1/3,0 *]*z1^2*s-[*1/27,0*]*z1^3+(z1*s^2+z1^2*(-[*2/3,0*])
       *s+z1^3*[*1/9,0*])+(z2*s-z2*([*1/3,0*]*z1))+z3 by SQUARE_1:def 3
      .=s^3+((-[*2/3,0*]+[*1/3,0 *])*z1^2+z2)*s+(
     [*1/9,0*]-[*1/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*Re [*1/9,0*]-
     Re [*1/27,0*],Im [*1/9,0*]-Im [*1/27,0*]*])*z1^3-z2*([*1/3,0*]*z1)+z3
                           by COMPLEX1:def 12
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*Re [*1/9,0*]-Re [*1/27
      ,0*],Im [*1/9,0*]-0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*Re [*1/9,0*]-Re [*1/27
       ,0*],0-0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*Re [*1/9,0*]-1/27
     ,0-0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-([*2/3,0*]-[*1/3,0 *]))*z1^2+z2)*s+([*1/9-1/27,0-0*])*z1^3-
        z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-[*Re [*2/3,0 *] -Re [*1/3,0*],Im [*2/3,0 *]-Im [*1/3,0*]*])*z1^2
       +z2)*s+([*2/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:def 12
     .=s^3+((-[*Re[*2/3,0 *] -Re [*1/3,0*],Im[*2/3,0 *] -0*])*z1^2
       +z2)*s+([*2/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-[*Re[*2/3,0 *] -Re [*1/3,0*],0-0*])*z1^2
       +z2)*s+([*2/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-[*2/3-Re [*1/3,0*],0-0*])*z1^2
       +z2)*s+([*2/27,0*])*z1^3-z2*([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+((-[*2/3-1/3,0-0*])*z1^2+z2)*s+([*2/27,0*])*z1^3-z2
       *([*1/3,0*]*z1)+z3 by COMPLEX1:7
     .=s^3+p*s+([*2/27,0*])*z1^3-z2*[*1/3,0*]*z1+z3 by A2,XCMPLX_1:175
     .=(s^3+p*s)+([*2/27,0*]*z1^3-[*1/3,0*]*z2*z1+z3)
     .=s^3+0c+p*s+q by A2,COMPLEX1:22
     .=s^3+0c*s^2+p*s+q by COMPLEX1:28
     .=1r*s^3+0c*s^2+p*s+q by COMPLEX1:29;
  hence CPoly3(1r,0c,p,q,s)=0c by Def7;
end;

theorem Th46:
   for z being Element of COMPLEX holds
    [* |.z.|*cos Arg z, |.z.|*sin Arg z *]
    = [*|.z.|,0*]*[*cos Arg z,sin Arg z *]
proof
    let z be Element of COMPLEX;
      [* |.z.|*cos Arg z, |.z.|*sin Arg z *]
    =[* |.z.|*cos Arg z-0*sin Arg z, |.z.|*sin Arg z+(cos Arg z)*0 *]
    .= [*|.z.|,0*]*[*cos Arg z,sin Arg z *] by HAHNBAN1:2;
    hence thesis;
end;

theorem Th47:
  for z be Element of COMPLEX,n be Nat holds
    z #N (n+1)=(z #N n) * z
proof
    let z be Element of COMPLEX,n be Nat;
      z #N (n+1) = (z GeoSeq).(n+1) by COMSEQ_3:def 2
         .= (z GeoSeq).n*z by COMSEQ_3:def 1
         .= (z #N n)*z by COMSEQ_3:def 2;
   hence thesis;
end;

theorem Th48:
  for z be Element of COMPLEX holds
    z#N 1= z
proof
    let z be Element of COMPLEX;
      z #N (0+1) = (z#N 0)*z by Th47
              .= 1r*z by COMSEQ_3:11
              .= z by COMPLEX1:29;
   hence thesis;
end;

theorem
    for z be Element of COMPLEX holds
    z#N 2= z*z
proof
    let z be Element of COMPLEX;
      z #N (1+1) = (z#N 1)*z by Th47
              .= z*z by Th48;
   hence thesis;
end;

theorem Th50:
  for n be Nat st n>0 holds
    0c#N n = 0c
proof
    let n be Nat;
    assume n>0;
       then n >= 0+1 by NAT_1:38;
 then A1: n-1 >= 0 by REAL_1:84;
         n=n-1+1
       .= n-'1+1 by A1,BINARITH:def 3;
       hence 0c #N n = (0c #N (n-'1))*0c by Th47
              .= 0c by COMPLEX1:28;
end;

theorem Th51:
   for x,y be Element of COMPLEX
    for n be Nat holds
     (x*y) #N n = x #N n * y #N n
proof
    let x,y be Element of COMPLEX;
    defpred P[Nat] means (x*y) #N $1 = x #N $1 * y #N $1;
      (x*y) #N 0 =1r by COMSEQ_3:11
                .=1r*1r by COMPLEX1:29
                .=(x #N 0)*1r by COMSEQ_3:11
                .=(x #N 0)*(y #N 0) by COMSEQ_3:11;
then A1:  P[0];
 A2: now let n be Nat;
     assume P[n];
      then (x*y)#N (n+1)= x#N n * y#N n * (y*x) by Th47
        .= x#N n * (y#N n * y*x)
        .= x#N n * (y#N (n+1)*x) by Th47
        .= x#N n * x *(y#N (n+1))
        .= x#N (n+1) * y#N (n+1) by Th47;
     hence P[n+1];
   end;
    thus for n be Nat holds P[n] from NAT_1:sch 1(A1,A2);
end;

theorem Th52:
    for x be Real st x > 0
      for n be Nat holds
      [*x,0*]#N n = [*x to_power n,0*]
proof
      let x be Real;
      assume
 A1: x > 0;
 defpred P[Nat] means [*x,0*]#N $1 = [*x to_power $1,0*];
   [*x,0*]#N 0 = 1 by COMPLEX1:def 7,COMSEQ_3:11
       .= [*1,0*] by ARYTM_0:def 7
       .= [**1,0**] by HAHNBAN1:def 1
       .= [**x to_power 0,0**] by POWER:29
       .= [*x to_power 0,0*] by HAHNBAN1:def 1;
 then A2: P[0];
 A3: now let n be Nat;
     assume P[n];
     then [*x,0*]#N (n+1) = [*x to_power n,0*]*[*x,0*] by Th47
        .= [*(x to_power n)*x,0*] by Th3
        .= [*(x to_power n)*(x to_power 1),0*] by POWER:30
        .= [*x to_power (n+1),0*] by A1,POWER:32;
     hence P[n+1];
    end;
   thus for n be Nat holds P[n] from NAT_1:sch 1(A2,A3);
end;

theorem Th53:
   for x being Real
   for n be Nat holds
    [*cos x,sin x*] #N n = [*cos (n*x),sin (n*x)*]
proof
 let x be Real;
 defpred P[Nat] means [*cos x,sin x*]#N $1 = [*cos ($1*x),sin ($1*x)*];
   [*cos x,sin x*] #N 0 = 1r by COMSEQ_3:11
       .= [*cos (0*x),sin (0*x)*] by ARYTM_0:def 7,COMPLEX1:def 7,SIN_COS:34;
 then A1: P[0];
 A2: now let n be Nat;
      set a'=Re [*cos(n*x),sin(n*x)*];
      set b'=Im [*cos(n*x),sin(n*x)*];
      set c'=Re [*cos x,sin x*];
      set d'=Im [*cos x,sin x*];
       assume P[n];
     then [*cos x,sin x*] #N (n+1)
         =[*cos(n*x),sin(n*x)*]*[*cos x,sin x*] by Th47
        .= [*cos(n*x)*cos x - sin(n*x)*sin x,cos(n*x)*sin x+cos x*sin(n*x)*]
                                            by Th1
        .= [*cos(n*x+x),cos(n*x)*sin x+cos x*sin(n*x)*] by SIN_COS:80
        .= [*cos((n+1)*x),sin((n+1)*x)*] by SIN_COS:80;
      hence P[n+1];
    end;
    thus for n be Nat holds P[n] from NAT_1:sch 1(A1,A2);
 end;

theorem
       for z being Element of COMPLEX
     for n being Nat st z <> 0c or n > 0 holds
    z#N n = [*(|.z.| to_power n)*cos (n*Arg z)
             ,(|.z.| to_power n)*sin (n*Arg z)*]
proof
    let z be Element of COMPLEX;
    let n be Nat;
    assume
 A1: z <> 0c or n > 0;
      per cases by A1;
     suppose z <> 0c;
 then A2: |.z.| > 0 by COMPLEX1:133;
        set x=Arg z;
        z#N n =[*|.z.|*cos Arg z-0*sin Arg z,|.z.|*sin Arg z+cos Arg z*0*]#N n
                                            by COMPLEX2:19
         .=([*|.z.|,0*]*[*cos Arg z,sin Arg z*])#N n by HAHNBAN1:2
         .= [*|.z.|,0*]#N n*[*cos Arg z,sin Arg z*]#N n by Th51;
       hence z#N n= [*|.z.|,0*]#N n*[*cos (n*x),sin (n*x)*] by Th53
       .= [*|.z.| to_power n,0*]*[*cos (n*Arg z),sin (n*Arg z)*]
                                     by A2,Th52
       .= [*(|.z.| to_power n)*cos(n*Arg z)-0*sin (n*Arg z),
             (|.z.| to_power n)*sin(n*Arg z)+0*cos(n*Arg z)*] by HAHNBAN1:2
       .= [*(|.z.| to_power n)*cos(n*Arg z),(|.z.| to_power n)*sin(n*Arg z)*];
     end;
     suppose
 A3: z=0c & n > 0;
     then 0=|.z.| to_power n by COMPLEX1:130,POWER:def 2;
       hence z#N n=[*(|.z.| to_power n)*cos(n*Arg z),
        (|.z.| to_power n)*sin(n*Arg z)*] by A3,Lm1,Th50;
      end;
end;

theorem Th55:
   for n,k be Nat,x be Real st n <> 0 holds
    [*cos((x+2*PI*k)/n),sin((x+2*PI*k)/n)*]#N n = [*cos x,sin x*]
   proof
    let n,k be Nat,x be Real;
    assume
 A1: n <> 0;
       thus [*cos((x+2*PI*k)/n),sin((x+2*PI*k)/n)*]#N n
        = [*cos (n*((x+2*PI*k)/n)),sin (n*((x+2*PI*k)/n))*] by Th53
       .= [*cos(x+2*PI*k),sin(n*((x+2*PI*k)/n))*] by A1,XCMPLX_1:88
       .= [*cos(x+2*PI*k),sin(x+2*PI*k)*] by A1,XCMPLX_1:88
       .= [*cos.(x+2*PI*k),sin(x+2*PI*k)*] by SIN_COS:def 23
       .= [*cos.(x+2*PI*k),sin.(x+2*PI*k)*] by SIN_COS:def 21
       .= [*cos.(x+2*PI*k),sin.x*] by SIN_COS2:10
       .= [*cos.x,sin.x*] by SIN_COS2:11
       .= [*cos.x,sin x*] by SIN_COS:def 21
       .= [*cos x,sin x*] by SIN_COS:def 23;
   end;

theorem Th56:
   for z be Element of COMPLEX
   for n,k be Nat st n <> 0 holds
    z = [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
          (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]#N n
   proof
    let z be Element of COMPLEX;
    let n,k be Nat;
    assume
 A1: n <> 0;
 then A2: n > 0 by NAT_1:19;
 then A3: n >= 0+1 by NAT_1:38;
    per cases;
      suppose
        z <> 0c;
 then A4: |.z.| > 0 by COMPLEX1:133;
       then n-root |.z.| = n -Root |.z.| by A3,POWER:def 1;
 then A5: n-root |.z.| > 0 by A3,A4,PREPOWER:def 3;
     thus [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
            (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]#N n
       =[*(n-root |.z.|)*cos((Arg z+2*PI*k)/n)-
          0*sin((Arg z+2*PI*k)/n),(n-root |.z.|)*sin((Arg z+2*PI*k)/n)+
          cos((Arg z+2*PI*k)/n)*0*]#N n
       .= ([*n-root |.z.|,0*]*
          [*cos((Arg z+2*PI*k)/n),sin((Arg z+2*PI*k)/n)*])#N n by HAHNBAN1:2
       .= ([*n-root |.z.|,0*]#N n)
         *([*cos((Arg z+2*PI*k)/n),sin((Arg z+2*PI*k)/n)*]#N n) by Th51
       .= ([*n-root |.z.|,0*]#N n)*[*cos Arg z,sin Arg z*] by A1,Th55
       .= [*(n-root |.z.|) to_power n,0*]*[*cos Arg z,sin Arg z*]
                                    by A5,Th52
       .= [*|.z.|,0*]*[*cos Arg z,sin Arg z*] by A1,A4,COMPTRIG:17
       .= [*|.z.|*cos Arg z-0*sin Arg z,|.z.|*sin Arg z+0*cos Arg z*]
                                     by HAHNBAN1:2
       .= z by COMPLEX2:19;
     end;
     suppose
 A6: z = 0c;
      hence [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
            (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]#N n
        =([*0*cos((Arg z+2*PI*k)/n),
           (n-root 0)*sin((Arg z+2*PI*k)/n)*]#N n) by A3,COMPLEX1:130,POWER:6
       .= [*0,0*sin((Arg z+2*PI*k)/n)*]#N n by A3,POWER:6
       .= z by A2,A6,Lm1,Th50;
     end;
   end;

definition
   let z be Element of COMPLEX;
   let n be non empty Nat;
   mode CRoot of n,z -> Element of COMPLEX means :Def8:
         it#N n = z;
  existence
   proof
    take [*(n-root |.z.|)*cos((Arg z+2*PI*0)/n),
         (n-root |.z.|)*sin((Arg z+2*PI*0)/n)*];
    thus thesis by Th56;
   end;
end;

theorem
     for z be Element of COMPLEX
   for n be non empty Nat
   for k be Nat holds
    [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
      (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*] is CRoot of n,z
proof
   let z be Element of COMPLEX,n be non empty Nat,k be Nat;
      [*(n-root |.z.|)*cos((Arg z+2*PI*k)/n),
      (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*]#N n=z by Th56;
    hence thesis by Def8;
end;

theorem
       for z be Element of COMPLEX
     for v be CRoot of 1,z holds
       v = z
   proof
    let z be Element of COMPLEX;
    let v be CRoot of 1,z;
      v#N 1 = z by Def8;
    hence v = z by Th48;
   end;

theorem
     for n be non empty Nat
   for v be CRoot of n,0c holds
    v = 0c
proof
    let n be non empty Nat;
    let v be CRoot of n,0c;
    defpred P[Nat] means v#N $1 = 0c;
 A1: ex n be non empty Nat st P[n]
    proof
       take n;
       thus thesis by Def8;
    end;
 A2: now let k be non empty Nat;
      assume that
 A3: k <> 1 and
 A4: P[k];
     consider t be Nat such that
 A5: k = t+1 by NAT_1:22;
         k >= 1 by NAT_1:39;
       then t+1 > 0+1 by A3,A5,REAL_1:def 5;
 then A6: t > 0 by AXIOMS:24;
     reconsider t as non empty Nat by A3,A5,CARD_1:51;
     take t;
     thus t < k by A5,NAT_1:38;
       v#N k = (v#N t)*v by A5,Th47;
     then v#N t = 0c or v = 0c by A4,COMPLEX1:def 6,XCMPLX_1:6;
     hence P[t] by A6,Th50;
    end;
      P[1] from COMPTRIG:sch 1(A1,A2);
    hence thesis by Th48;
end;

theorem
       for n be non empty Nat
     for z be Element of COMPLEX
     for v be CRoot of n,z st v = 0c holds
       z = 0c
proof
     let n be non empty Nat;
     let z be Element of COMPLEX;
     let v be CRoot of n,z;
 A1:n>0 by NAT_1:19;
     assume v = 0c;
     then 0c#N n = z by Def8;
     hence thesis by A1,Th50;
end;

theorem
     for n be non empty Nat
   for k be Nat holds
    [*cos(2*PI*k/n),sin(2*PI*k/n)*] is CRoot of n,1r
proof
   let n be non empty Nat,k be Nat;
        [*cos(2*PI*k/n),sin(2*PI*k/n)*]#N n
      =[*cos(n*((2*PI*k)/n)),sin(n*((2*PI*k)/n))*] by Th53
      .=[*cos(2*PI*k),sin(n*((2*PI*k)/n))*] by XCMPLX_1:88
      .=[*cos(2*PI*k+0),sin(2*PI*k+0)*] by XCMPLX_1:88
      .=[*cos.(2*PI*k+0),sin(2*PI*k+0)*] by SIN_COS:def 23
      .=[*cos.(2*PI*k+0),sin.(2*PI*k+0)*] by SIN_COS:def 21
      .=[*cos.(2*PI*k+0),sin.0*] by SIN_COS2:10
      .=[*cos.0,sin.0*] by SIN_COS2:11
      .=[*cos(0),sin.0*] by SIN_COS:def 23
      .=[*1,0*] by SIN_COS:34,def 21
      .=1r by ARYTM_0:def 7,COMPLEX1:def 7;
    hence thesis by Def8;
end;

theorem
     for k be Nat holds
    [*cos(2*PI*k/3),sin(2*PI*k/3)*] is CRoot of 3,1r
proof
     let k be Nat;
       [*cos(2*PI*k/3),sin(2*PI*k/3)*]#N 3
     =[*cos(2*PI*k),sin(3*((2*PI*k)/3))*] by Th53
      .=[*cos.(2*PI*k+0),sin(2*PI*k+0)*] by SIN_COS:def 23
      .=[*cos.(2*PI*k+0),sin.(2*PI*k+0)*] by SIN_COS:def 21
      .=[*cos.(2*PI*k+0),sin.0*] by SIN_COS2:10
      .=[*cos.0,sin.0*] by SIN_COS2:11
      .=[*cos(0),sin.0*] by SIN_COS:def 23
      .=[*1,0*] by SIN_COS:34,def 21
      .=1r by ARYTM_0:def 7,COMPLEX1:def 7;
    hence thesis by Def8;
end;

theorem
       for z,s being Element of COMPLEX, n being Nat st
     s<>0 & z<>0 & n>=1 & s#N n=z#N n holds
     |.s.| = |.z.|
proof
     let z,s be Element of COMPLEX, n be Nat;
     assume
 A1: s<>0 & z<>0 & n>=1 & s#N n=z#N n;
 then A2: |.s.|>0 & |.z.|>0 by COMPLEX1:133,def 6;
         z#N n =[* |.s.|*cos Arg s, |.s.|*sin Arg s *]#N n by A1,COMPLEX2:19
        .=([*|.s.|,0*]*[*cos Arg s,sin Arg s *])#N n by Th46
        .=([*|.s.|,0*]#N n)*([*cos Arg s,sin Arg s *]#N n) by Th51
        .=[*|.s.| to_power n,0*]*([*cos Arg s,sin Arg s *]#N n) by A2,Th52
        .=[*|.s.| to_power n,0*]*[*cos (n*Arg s),sin (n*Arg s)*] by Th53
       .= [*(|.s.| to_power n)*cos (n*Arg s)-0*sin (n*Arg s)
        ,(|.s.| to_power n)*sin (n*Arg s)+cos (n*Arg s)*0*] by HAHNBAN1:2
       .= [*(|.s.| to_power n)*cos (n*Arg s)
           ,(|.s.| to_power n)*sin (n*Arg s)*];
       then [*(|.s.| to_power n)*cos (n*Arg s),(|.s.| to_power n)*sin (n*Arg s
)*]
        =[* |.z.|*cos Arg z, |.z.|*sin Arg z *]#N n by COMPLEX2:19
        .=([*|.z.|,0*]*[*cos Arg z,sin Arg z *])#N n by Th46
        .=([*|.z.|,0*]#N n)*([*cos Arg z,sin Arg z *]#N n) by Th51
        .=[*|.z.| to_power n,0*]*([*cos Arg z,sin Arg z *]#N n) by A2,Th52
        .=[*|.z.| to_power n,0*]*[*cos (n*Arg z),sin (n*Arg z)*] by Th53
       .= [*(|.z.| to_power n)*cos (n*Arg z)-0*sin (n*Arg z)
           ,(|.z.| to_power n)*sin (n*Arg z)+cos (n*Arg z)*0*] by HAHNBAN1:2
       .= [*(|.z.| to_power n)*cos (n*Arg z),(|.z.| to_power n)
          *sin (n*Arg z)*];
      then (|.s.| to_power n)*cos (n*Arg s)=(|.z.| to_power n)*cos (n*Arg z)
       &(|.s.| to_power n)*sin (n*Arg s)
       =(|.z.| to_power n)*sin (n*Arg z) by ARYTM_0:12;
       then ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
       ((|.s.| to_power n)*sin (n*Arg s))^2
       =((|.z.| to_power n)*cos (n*Arg z))^2+
       ((|.z.| to_power n)*sin (n*Arg z))^2 by SQUARE_1:68;
       then ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
       ((|.s.| to_power n))^2*(sin (n*Arg s))^2
       =((|.z.| to_power n)*cos (n*Arg z))^2+
       ((|.z.| to_power n)*sin (n*Arg z))^2 by SQUARE_1:68;
       then ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
       ((|.s.| to_power n))^2*(sin (n*Arg s))^2
       =((|.z.| to_power n))^2*(cos (n*Arg z))^2+
       ((|.z.| to_power n)*sin (n*Arg z))^2 by SQUARE_1:68;
       then ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
       ((|.s.| to_power n))^2*(sin (n*Arg s))^2
       =((|.z.| to_power n))^2*(cos (n*Arg z))^2+
       ((|.z.| to_power n))^2*(sin (n*Arg z))^2 by SQUARE_1:68;
       then ((|.s.| to_power n))^2*((cos (n*Arg s))^2+(sin (n*Arg s))^2)
       =((|.z.| to_power n))^2*((cos (n*Arg z))^2+(sin (n*Arg z))^2)
                          by XCMPLX_1:8;
       then ((|.s.| to_power n))^2*((cos (n*Arg s))^2+(sin (n*Arg s))^2)
       =((|.z.| to_power n))^2*1 by SIN_COS:32;
 then A3: ((|.s.| to_power n))^2*1=((|.z.| to_power n))^2 by SIN_COS:32;
 A4: |.s.| to_power n > 0 & |.z.| to_power n > 0 by A2,POWER:39;
       then (|.s.| to_power n)
       =sqrt ((|.z.| to_power n))^2 by A3,SQUARE_1:89;
       then |.s.| to_power n=|.z.| to_power n by A4,SQUARE_1:89;
       then |.s.| |^ n=|.z.| to_power n by A2,POWER:46;
       then |.s.| |^ n=|.z.| |^ n by A2,POWER:46;
       then |.s.| = n-root (|.z.| |^ n) by A1,A2,POWER:5;
      hence thesis by A1,A2,POWER:5;
end;
