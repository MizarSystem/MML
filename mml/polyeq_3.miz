         .= [*b*(x^2-y^2)-0,b*Im [*x^2-y^2,2*x*y*]+0*] by COMPLEX1:7
         .= [*b*(x^2-y^2)-0,b*(2*x*y)*] by COMPLEX1:7;then
         0c= [*a*(x|^ 3 - 3*x*y^2),a*(-y|^ 3+3*x^2*y)*]+[*b*(x^2-y^2)
            ,b*(2*x*y)*]+[*c*x,c*y*]+[*d,0*] by A111
         .= [*a*(x|^ 3 - 3*x*y^2)+b*(x^2-y^2),a*(-y|^ 3+3*x^2*y)+b*(2*x*y)*]
            +[*c*x,c*y*]+[*d,0*] by COMPLFLD:2
         .= [*a*(x|^ 3 - 3*x*y^2)+b*(x^2-y^2)+c*x,a*(-y|^ 3+3*x^2*y)
            +b*(2*x*y)+c*y*]+[*d,0*] by COMPLFLD:2
         .= [*a*(x|^ 3 - 3*x*y^2)+b*(x^2-y^2)+c*x+d,a*(-y|^ 3+3*x^2*y)
            +b*(2*x*y)+c*y+0*] by COMPLFLD:2
         .= [*a*(x|^ 3 - 3*x*0)+b*(x^2-0)+c*x+d
                 ,a*(-0|^ 3+0)+b*0+0*] by SQUARE_1:60,A105
         .= [*a*x|^ 3+b*x^2+c*x+d,a*(-0)+0*] by NEWTON:16
         .= [*a*x|^ 3+b*x^2+c*x+d,a*0+0*]; then
         a*x|^ 3+b*x^2+c*x+d = 0 by XXX,ARYTM_0:12;then
 A120:      Poly3(a,b,c,d,x) = 0 by POLYEQ_1:def 4;
            set a'=b/a;
            set b'=c/a;
            set c'=d/a;
            set h=-b/(3*a);
            a'=b/a & b'=c/a & c'=d/a & h=-b/(3*a);then
 A122:       x1 |^ 3 + 0*x1^2 + ((3*a*c-b^2)/(3*a^2))*x1
                  + (2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2)) = 0
            by POLYEQ_1:16,A100,A120,A104;
             set  p = (3*a*c-b^2)/(3*a^2);
             set  q = 2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2);
             p/3=1/3*((3*a*c-b^2)/(3*a^2)) by XCMPLX_1:106;then
             p/3=(3*a*c-b^2)/(a^2*3*3) by XCMPLX_1:104;then
 A124:       p/3=(3*a*c-b^2)/(a^2*(3*3)) by XCMPLX_1:4;
 A126:       -q/2=-(((b/(3*a)) |^ 3)*2+(3*a*d-b*c)/(3*a^2)+0)/2
             .=-(((b/(3*a)) |^ 3)*2/2+(3*a*d-b*c)/(3*a^2)/2+0/2)
       by XCMPLX_1:64
            .=-(((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2)/2+0/2) by XCMPLX_1:90
            .=-((b/(3*a)) |^ 3+1/2*((3*a*d-b*c)/(3*a^2))) by XCMPLX_1:106
            .=-((b/(3*a)) |^ 3+(3*a*d-b*c)/(a^2*3*2)) by XCMPLX_1:104
            .=-((b/(3*a)) |^ 3+(3*a*d-b*c)/(a^2*(3*2))) by XCMPLX_1:4
            .=0-((b/(3*a)) |^ 3+(3*a*d-b*c)/(6*a^2)) by XCMPLX_1:150
            .=0-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2) by XCMPLX_1:36
            .=-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2) by XCMPLX_1:150;
 A128:      Poly3(1,0,p,q,x1) = 0 by A122,POLYEQ_1:17;
  A132:      x1 = 3-root(-q/2+sqrt(q^2/4+(p/3) |^ 3)) +
                 3-root(-q/2-sqrt(q^2/4+(p/3) |^ 3)) or
            x1 = 3-root(-q/2+sqrt(q^2/4+(p/3) |^ 3)) +
                 3-root(-q/2+sqrt(q^2/4+(p/3) |^ 3)) or
            x1 = 3-root(-q/2-sqrt(q^2/4+(p/3) |^ 3)) +
                 3-root(-q/2-sqrt(q^2/4+(p/3) |^ 3))
                    by A128,POLYEQ_1:19,A108,A104;
            x1+0=b/(3*a)+x by A104;then
            x1-b/(3*a)=x-0 by XCMPLX_1:33;
         hence thesis by A105,A110,A124,A132,A126;
end;

theorem Th32:
  z1 <> 0 & Poly1(z1,z2,z) = 0 implies z = -(z2/z1)
  proof
   assume A100: z1 <> 0 & Poly1(z1,z2,z) = 0;then
    z1*z+z2 = 0 by POLYEQ_1:def 1;then
    z1*z+z2-z2 = -z2 by XCMPLX_1:150;then
    z1*z = -z2 by XCMPLX_1:26;then
    z = (-z2)*z1" by A100,XCMPLX_1:204;then
    z = -(z2*z1") by XCMPLX_1:175;
  hence z = -(z2/z1) by XCMPLX_0:def 9;
end;

theorem
   z2 <> 0 implies not ex z st Poly1(0,z2,z) = 0
proof
   assume A100: z2 <> 0;
   given s such that
   A102:Poly1(0,z2,s) = 0;
   0*s + z2 = 0 by A102,POLYEQ_1:def 1;
   hence contradiction by A100;
end;

begin

definition let z1,z2,z3,z;
  func CPoly2(z1,z2,z3,z) -> Element of COMPLEX equals
    :Def10: z1*z^2+z2*z+z3;
  coherence;
end;

theorem 
    (for z holds CPoly2(z1,z2,z3,z) = CPoly2(s1,s2,s3,z)) implies
    z1 = s1 & z2 = s2 & z3 = s3
proof
       assume 
 A1: for z holds CPoly2(z1,z2,z3,z) = CPoly2(s1,s2,s3,z);
       CPoly2(z1,z2,z3,0c) = CPoly2(s1,s2,s3,0c) by A1;then
    z1*0c^2+z2*0c+z3 = CPoly2(s1,s2,s3,0c) by Def10;then
    z1*(0c*0c)+z2*0c+z3 = CPoly2(s1,s2,s3,0c) by Th4;then
    z1*0c+z2*0c+z3 = CPoly2(s1,s2,s3,0c) by COMPLEX1:28;then
    0c+z2*0c+z3 = CPoly2(s1,s2,s3,0c) by COMPLEX1:28;then
    0c+0c+z3 = CPoly2(s1,s2,s3,0c) by COMPLEX1:28;then
    0c+z3 = CPoly2(s1,s2,s3,0c) by COMPLEX1:22;then
    z3 = CPoly2(s1,s2,s3,0c) by COMPLEX1:22;then
A2: z3 = s1*0c^2+s2*0c+s3 by Def10
    .=s1*(0c*0c)+s2*0c+s3 by Th4
    .=s1*0c+s2*0c+s3 by COMPLEX1:28
    .=0c+s2*0c+s3 by COMPLEX1:28
    .=0c+0c+s3 by COMPLEX1:28
    .=0c+s3 by COMPLEX1:22
    .=s3 by COMPLEX1:22;
    CPoly2(z1,z2,z3,1r) = CPoly2(s1,s2,s3,1r) by A1;then
    z1*1r^2+z2*1r+z3 = CPoly2(s1,s2,s3,1r) by Def10;then
    z1*(1r*1r)+z2*1r+z3 = CPoly2(s1,s2,s3,1r) by Th4;then
    z1*1r+z2*1r+z3 = CPoly2(s1,s2,s3,1r) by COMPLEX1:29;then
    z1+z2*1r+z3 = CPoly2(s1,s2,s3,1r) by COMPLEX1:29;then
    z1+z2+z3 = CPoly2(s1,s2,s3,1r) by COMPLEX1:29;then
    z1+z2+z3 = s1*1r^2+s2*1r+s3 by Def10;then
    z1+z2+z3 = s1*(1r*1r)+s2*1r+s3 by Th4;then
    z1+z2+z3 = s1*1r+s2*1r+s3 by COMPLEX1:29;then
    z1+z2+z3 = s1+s2*1r+s3 by COMPLEX1:29;then
    (z1+z2)+z3-z3 = (s1+s2)+s3-s3 by A2,COMPLEX1:29;then
    z1+z2 = (s1+s2)+s3-s3 by XCMPLX_1:26;then
 A3: z1+z2 = s1+s2 by XCMPLX_1:26;
     CPoly2(z1,z2,z3,-1r) = CPoly2(s1,s2,s3,-1r) by A1;then
     z1*(-1r)^2+z2*(-1r)+z3 = CPoly2(s1,s2,s3,-1r) by Def10;then
     z1*((-1r)*(-1r))+z2*(-1r)+z3 = CPoly2(s1,s2,s3,-1r) by Th4;then
     z1*(1r*1r)+z2*(-1r)+z3 = CPoly2(s1,s2,s3,-1r) by XCMPLX_1:177;then
     z1*1r+z2*(-1r)+z3 = CPoly2(s1,s2,s3,-1r) by COMPLEX1:29;then
     z1+z2*(-1r)+z3 = CPoly2(s1,s2,s3,-1r) by COMPLEX1:29;then
     z1+-z2+z3 = CPoly2(s1,s2,s3,-1r) by COMPLEX1:46;then
     z1-z2+z3 = CPoly2(s1,s2,s3,-1r) by XCMPLX_0:def 8;then
     z1-z2+z3 = s1*(-1r)^2+s2*(-1r)+s3 by Def10;then
     z1-z2+z3 = s1*((-1r)*(-1r))+s2*(-1r)+s3 by Th4;then
     z1-z2+z3 = s1*(1r*1r)+s2*(-1r)+s3 by XCMPLX_1:177;then
     z1-z2+z3 = s1*1r+s2*(-1r)+s3 by COMPLEX1:29;then
     z1-z2+z3 = s1+s2*(-1r)+s3 by COMPLEX1:29;then
     z1-z2+z3 = s1+-s2+s3 by COMPLEX1:46;then
     (z1-z2)+z3-z3 = (s1-s2)+s3-s3 by XCMPLX_0:def 8,A2;then
     z1-z2 = (s1-s2)+s3-s3 by XCMPLX_1:26;then
     z1-z2+(z2+z1) = s1-s2+(s2+s1) by A3,XCMPLX_1:26;then
     z1-(z2-(z2+z1)) = s1-s2+(s2+s1) by XCMPLX_1:37;then
     z1-(z2-(z2+z1)) = s1-(s2-(s2+s1)) by XCMPLX_1:37;then
     z1-(z2-z2-z1) = s1-(s2-(s2+s1)) by XCMPLX_1:36;then
     z1-(z2-z2-z1) = s1-(s2-s2-s1) by XCMPLX_1:36;then
     z1-(0-z1) = s1-(s2-s2-s1) by XCMPLX_1:14;then
     z1-(0-z1) = s1-(0-s1) by XCMPLX_1:14;then
     z1--z1 = s1-(0-s1) by XCMPLX_1:150;then
     z1--z1 = s1--s1 by XCMPLX_1:150;then
     z1+z1 = s1--s1 by XCMPLX_1:151;then
     z1+z1 = s1+s1 by XCMPLX_1:151; then
     2*z1 = s1+s1 by Th20;then
     2*z1 = 2*s1 by Th20; then
 A6: z1 = s1 by XCMPLX_1:5;then
     z2+z1-z1 = s2+(z1-z1) by XCMPLX_1:29,A3;then
     z2+z1-z1 = s2+0 by XCMPLX_1:14;then
     z2+(z1-z1) = s2+0 by XCMPLX_1:29;then
     z2+0 = s2+0 by XCMPLX_1:14;
 hence thesis by A2,A6;
end;

theorem
    (-a+sqrt (a^2+b^2))/2 >=0 & (a+sqrt (a^2+b^2))/2>=0
proof
 A119: a^2>=0 & b^2>=0 & a^2>=a^2 by SQUARE_1:72;then
 A120: a^2+b^2>=0+0 & a^2+b^2>=0+a^2 by REAL_1:55;then
 A124: sqrt(a^2+b^2)>=sqrt(a^2) by SQUARE_1:94,A119;
          B001:  sqrt (a^2+b^2)>=0 by SQUARE_1:def 4,A120;
          per cases;
          suppose a>=0;then
          B102:  sqrt(a^2+b^2)>=a & a+sqrt (a^2+b^2)>=0+0
         by REAL_1:55,SQUARE_1:89,B001,A124;then
                sqrt(a^2+b^2)-a>=a-a by REAL_1:49;then
                sqrt(a^2+b^2)-a>=0 by XCMPLX_1:14;then
                -(-sqrt(a^2+b^2)+a)>=0 by XCMPLX_1:163;then
                sqrt(a^2+b^2)+-a>=0 by XCMPLX_1:141;
               hence thesis by B102,REAL_2:125;
          end;
          suppose
          C100: a<0;then
                -a > 0 by REAL_1:66;then
          C101:  sqrt(a^2+b^2) >= -a & -a+sqrt (a^2+b^2)>=0+0
  by SQUARE_1:90,C100,REAL_1:67,B001,A124;then
                sqrt(a^2+b^2)-(-a) >= -a-(-a) by REAL_1:49;then
                a+sqrt(a^2+b^2) >= -a-(-a) by XCMPLX_1:151;then
                a+sqrt(a^2+b^2) >= 0 by XCMPLX_1:14;
               hence thesis by REAL_2:125,C101;
              end;
end;

theorem Th420:
   z^2 = s & Im s >= 0 implies
   z=[*sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
       sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
   z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*]   
proof
      assume
 A100: z^2=s;
       assume
 A102: Im s >=0;
       set a=Re s;
       set b=Im s;
       set u=Re z;
       set v=Im z;
 A104: z=[*u,v*] & s=[*a,b*] & b>=0 by COMPLEX1:8,A102;then
       [*u^2-v^2,2*u*v*]=[*a,b*] by Th6,A100;then
 A108: u^2-v^2=a & 2*u*v=b by ARYTM_0:12;then
       (u^2-v^2)^2+(2*(u*v))^2=a^2+b^2 by XCMPLX_1:4;then
       (u^2)^2-2*u^2*v^2+(v^2)^2+(2*(u*v))^2=a^2+b^2 by SQUARE_1:64;then
       (u^2)^2+(v^2)^2-2*u^2*v^2+(2*(u*v))^2=a^2+b^2 by XCMPLX_1:29;then
    ((u^2)^2+(v^2)^2)-2*u^2*v^2+2^2*(u*v)^2=a^2+b^2 by SQUARE_1:68;then
    ((u^2)^2+(v^2)^2)-2*u^2*v^2+2^2*(u^2*v^2)=a^2+b^2 by SQUARE_1:68;then
    ((u^2)^2+(v^2)^2)-2*u^2*v^2+(2*2)*(u^2*v^2)=a^2+b^2
                              by SQUARE_1:def 3;then
    4*(u^2*v^2)-2*u^2*v^2+((u^2)^2+(v^2)^2)=a^2+b^2 by XCMPLX_1:30;then
    4*(u^2*v^2)-2*(u^2*v^2)+((u^2)^2+(v^2)^2)=a^2+b^2 by XCMPLX_1:4;then
    (4-2)*(u^2*v^2)+((u^2)^2+(v^2)^2)=a^2+b^2 by XCMPLX_1:40;then
    (u^2)^2+2*(u^2*v^2)+(v^2)^2=a^2+b^2 by XCMPLX_1:1;then
    (u^2)^2+2*u^2*v^2+(v^2)^2=a^2+b^2 by XCMPLX_1:4;then
 A110:   (u^2+v^2)^2=a^2+b^2 by SQUARE_1:63;
       u^2>=0 & v^2>=0 by SQUARE_1:72;then
       u^2+v^2>=0+0 by REAL_1:55;then
 A114: u^2+v^2=sqrt (a^2+b^2) by A110,SQUARE_1:89;then
       u^2-v^2+v^2+u^2=a+sqrt (a^2+b^2) by XCMPLX_1:1,A108;then
       u^2-(v^2-v^2)+u^2=a+sqrt (a^2+b^2) by XCMPLX_1:37;then
       u^2+u^2=a+sqrt (a^2+b^2) by XCMPLX_1:17;then
       2*u^2=a+sqrt (a^2+b^2) by XCMPLX_1:11;then
 A116: u^2=(a+sqrt (a^2+b^2))/2 by XCMPLX_1:90;
       v^2+u^2-u^2+v^2=sqrt (a^2+b^2)-a by XCMPLX_1:37,A108,A114;then
       v^2+v^2=sqrt (a^2+b^2)-a by XCMPLX_1:26;then
       v^2*2=sqrt (a^2+b^2)-a by XCMPLX_1:11;then
       v^2=(sqrt (a^2+b^2)-a)/2 by XCMPLX_1:90;then
       v^2=(-(-sqrt (a^2+b^2)+a))/2 by XCMPLX_1:163;then
 A118: v^2=(-a+sqrt (a^2+b^2))/2 by XCMPLX_1:141;
 A140: sqrt u^2=sqrt ((a+sqrt (a^2+b^2))/2) &
       sqrt v^2=sqrt ((-a+sqrt (a^2+b^2))/2) by A116,A118;
                2*(u*v)>=0 by A108,A104,XCMPLX_1:4;then
                -2*(u*v)<=-0 by REAL_1:50;then
                (-2)*(u*v)<=0 by XCMPLX_1:175;then
                u*v>=0 by REAL_2:129;then
                u<=0 & v<=0 or u>=0 & v>=0 by SQUARE_1:25;then
           -u=sqrt ((a+sqrt (a^2+b^2))/2) & -v=sqrt ((-a+sqrt (a^2+b^2))/2)  or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                    by A140,SQUARE_1:89,SQUARE_1:90;then
                0-u=0+sqrt ((a+sqrt (a^2+b^2))/2)
                & 0-v=0+sqrt ((-a+sqrt (a^2+b^2))/2) or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                             by XCMPLX_1:150;then
                0+u=0-sqrt ((a+sqrt (a^2+b^2))/2)
                & 0+v=0-sqrt ((-a+sqrt (a^2+b^2))/2) or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                             by XCMPLX_1:35;then
           u=-sqrt ((a+sqrt (a^2+b^2))/2) & v=-sqrt ((-a+sqrt (a^2+b^2))/2) or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                             by XCMPLX_1:150;
           hence thesis by A104;
end;

theorem
   z^2 = s & Im s = 0 & Re s > 0 implies z=sqrt(Re s) or z=-sqrt(Re s)
proof
      assume
 A100: z^2=s;
       assume
 A102: Im s =0 & Re s >0;
    z=[*sqrt (( Re s+sqrt ((Re s)^2+0))/2),
       sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*]   or
    z=[*-sqrt (( Re s+sqrt ((Re s)^2+0))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*]
             by SQUARE_1:60,Th420,A102,A100;then
    z=[*sqrt (( Re s+Re s)/2),
       sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*]   or
    z=[*-sqrt (( Re s+sqrt ((Re s)^2+0))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*] by SQUARE_1:89,A102;then
    z=[*sqrt (( Re s+Re s)/2), sqrt ((-Re s+Re s)/2)*]   or
    z=[*-sqrt (( Re s+Re s)/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*] by SQUARE_1:89,A102;then
    z=[*sqrt (( Re s+Re s)/2), sqrt ((-Re s+Re s)/2)*] or
    z=[*-sqrt (( Re s+Re s)/2),-sqrt ((-Re s+Re s)/2)*]
                           by SQUARE_1:89,A102;then
    z=[*sqrt (Re s*2/2), sqrt ((-Re s+Re s)/2)*] or
    z=[*-sqrt (Re s*2/2),-sqrt ((-Re s+Re s)/2)*] by XCMPLX_1:11;then
    z=[*sqrt (Re s), sqrt ((0+(Re s+-Re s))/2)*] or
    z=[*-sqrt (Re s),-sqrt ((0+(Re s+-Re s))/2)*] by XCMPLX_1:90;then
    z=[*sqrt (Re s), sqrt ((0+(Re s-Re s))/2)*] or
    z=[*-sqrt (Re s),-sqrt ((0+(Re s-Re s))/2)*] by XCMPLX_0:def 8;then
    z=[*sqrt (Re s), sqrt (0/2)*] or
    z=[*-sqrt (Re s),-sqrt (0/2)*] by XCMPLX_1:25;
    hence thesis by SQUARE_1:82,ARYTM_0:def 7;
end;

theorem
   z^2 = s & Im s = 0 & Re s < 0 implies
    z=[*0, sqrt (-Re s)*] or z=[*0,-sqrt (-Re s)*]
proof
      assume
 A100: z^2=s;
       assume
 A102: Im s =0 & Re s < 0;
    z=[*sqrt (( Re s+sqrt ((Re s)^2+0))/2),
       sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*]   or
    z=[*-sqrt (( Re s+sqrt ((Re s)^2+0))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*] 
 by SQUARE_1:60,Th420,A102,A100;then
    z=[*sqrt (( Re s+-Re s)/2),
       sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*]   or
    z=[*-sqrt (( Re s+sqrt ((Re s)^2+0))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*] by SQUARE_1:90,A102;then
    z=[*sqrt (( Re s+-Re s)/2), sqrt ((-Re s+-Re s)/2)*]   or
    z=[*-sqrt (( Re s+-Re s)/2),-sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*]
                       by SQUARE_1:90,A102;then
    z=[*sqrt (( Re s+-Re s)/2), sqrt ((-Re s+-Re s)/2)*]   or
    z=[*-sqrt (( Re s+-Re s)/2),-sqrt ((-Re s+-Re s)/2)*]
                           by SQUARE_1:90,A102;then
    z=[*sqrt (( Re s+-Re s)/2), sqrt ((-Re s)*2/2)*]   or
    z=[*-sqrt (( Re s+-Re s)/2),-sqrt ((-Re s)*2/2)*] by XCMPLX_1:11;then
    z=[*sqrt (( 0+(Re s+-Re s))/2), sqrt (-Re s)*]   or
    z=[*-sqrt (( 0+(Re s+-Re s))/2),-sqrt (-Re s)*]
  by XCMPLX_1:90;then
    z=[*sqrt (( 0+(Re s-Re s))/2), sqrt (-Re s)*]   or
    z=[*-sqrt (( 0+(Re s-Re s))/2),-sqrt (-Re s)*]
  by XCMPLX_0:def 8;then
    z=[*sqrt (0/2), sqrt (-Re s)*] or
    z=[*-sqrt (0/2),-sqrt (-Re s)*] by XCMPLX_1:25;
   hence thesis by SQUARE_1:82;
end;

theorem
   z^2 = s & Im s <0 implies
   z=[* sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
   z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
        sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*]
proof
       assume
 A100: z^2=s;
       assume
 A102: Im s<0;
       set a=Re s;
       set b=Im s;
       set u=Re z;
       set v=Im z;
 A104: z=[*u,v*] & s=[*a,b*] & b <0 by A102,COMPLEX1:8;then
       [*u^2-v^2,2*u*v*]=[*a,b*] by Th6,A100;then
 A108: u^2-v^2=a & 2*u*v=b by ARYTM_0:12;then
       (u^2-v^2)^2+(2*(u*v))^2=a^2+b^2 by XCMPLX_1:4;then
       (u^2)^2-2*u^2*v^2+(v^2)^2+(2*(u*v))^2=a^2+b^2 by SQUARE_1:64;then
       (u^2)^2+(v^2)^2-2*u^2*v^2+(2*(u*v))^2=a^2+b^2 by XCMPLX_1:29;then
    ((u^2)^2+(v^2)^2)-2*u^2*v^2+2^2*(u*v)^2=a^2+b^2 by SQUARE_1:68;then
    ((u^2)^2+(v^2)^2)-2*u^2*v^2+2^2*(u^2*v^2)=a^2+b^2 by SQUARE_1:68;then
    ((u^2)^2+(v^2)^2)-2*u^2*v^2+(2*2)*(u^2*v^2)=a^2+b^2 
                              by SQUARE_1:def 3;then
    4*(u^2*v^2)-2*u^2*v^2+((u^2)^2+(v^2)^2)=a^2+b^2 by XCMPLX_1:30;then
    4*(u^2*v^2)-2*(u^2*v^2)+((u^2)^2+(v^2)^2)=a^2+b^2 by XCMPLX_1:4;then
    (4-2)*(u^2*v^2)+((u^2)^2+(v^2)^2)=a^2+b^2 by XCMPLX_1:40;then
    (u^2)^2+2*(u^2*v^2)+(v^2)^2=a^2+b^2 by XCMPLX_1:1;then
    (u^2)^2+2*u^2*v^2+(v^2)^2=a^2+b^2 by XCMPLX_1:4;then
 A110: sqrt (u^2+v^2)^2=sqrt (a^2+b^2) by SQUARE_1:63;
       u^2>=0 & v^2>=0 by SQUARE_1:72;then
       u^2+v^2>=0+0 by REAL_1:55;then
 A114: u^2+v^2=sqrt (a^2+b^2) by A110,SQUARE_1:89;then
       u^2-v^2+v^2+u^2=a+sqrt (a^2+b^2) by XCMPLX_1:1,A108;then
       u^2-(v^2-v^2)+u^2=a+sqrt (a^2+b^2) by XCMPLX_1:37;then
       u^2+u^2=a+sqrt (a^2+b^2) by XCMPLX_1:17;then
       2*u^2=a+sqrt (a^2+b^2) by XCMPLX_1:11;then
 A116: u^2=(a+sqrt (a^2+b^2))/2 by XCMPLX_1:90;
       v^2+u^2-u^2+v^2=sqrt (a^2+b^2)-a by XCMPLX_1:37,A108,A114;then
       v^2+v^2=sqrt (a^2+b^2)-a by XCMPLX_1:26;then
       v^2*2=sqrt (a^2+b^2)-a by XCMPLX_1:11;then
       v^2=(sqrt (a^2+b^2)-a)/2 by XCMPLX_1:90;then
       v^2=(-(-sqrt (a^2+b^2)+a))/2 by XCMPLX_1:163;then
 A118: v^2=(-a+sqrt (a^2+b^2))/2 by XCMPLX_1:141;
 A140: sqrt u^2=sqrt ((a+sqrt (a^2+b^2))/2) &
       sqrt v^2=sqrt ((-a+sqrt (a^2+b^2))/2) by A116,A118;
                2*(u*v)<0 by XCMPLX_1:4,A108,A104;then
                u*v<0 by REAL_2:132;then
                u<0 & v>0 or u>0 & v<0 by REAL_2:132;then
          -u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)  or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & -v=sqrt ((-a+sqrt (a^2+b^2))/2)
                    by A140,SQUARE_1:89,SQUARE_1:90;then
                0-u=0+sqrt ((a+sqrt (a^2+b^2))/2)
                & v=sqrt ((-a+sqrt (a^2+b^2))/2) or
                u=sqrt ((a+sqrt (a^2+b^2))/2) & 
                0-v=0+sqrt ((-a+sqrt (a^2+b^2))/2) by XCMPLX_1:150;then
                0+u=0-sqrt ((a+sqrt (a^2+b^2))/2) 
                & v=sqrt ((-a+sqrt (a^2+b^2))/2) or
                u=sqrt ((a+sqrt (a^2+b^2))/2)
                & 0+v=0-sqrt ((-a+sqrt (a^2+b^2))/2) by XCMPLX_1:35;
          hence thesis by A104,XCMPLX_1:150;
end;

theorem Th42:
   z^2 = s implies
   z=[*sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
       sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
   z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
   z=[* sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
   z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
        sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*]
proof
       assume
 A100: z^2=s;
       set a=Re s;
       set b=Im s;
       set u=Re z;
       set v=Im z;
 A104: z=[*u,v*] & s=[*a,b*] by COMPLEX1:8;then
       [*u^2-v^2,2*u*v*]=[*a,b*] by A100,Th6;then
 A108: u^2-v^2=a & 2*u*v=b by ARYTM_0:12;then
       (u^2-v^2)^2+(2*(u*v))^2=a^2+b^2 by XCMPLX_1:4;then
       (u^2)^2-2*u^2*v^2+(v^2)^2+(2*(u*v))^2=a^2+b^2 by SQUARE_1:64;then
       (u^2)^2+(v^2)^2-2*u^2*v^2+(2*(u*v))^2=a^2+b^2 by XCMPLX_1:29;then
    ((u^2)^2+(v^2)^2)-2*u^2*v^2+2^2*(u*v)^2=a^2+b^2 by SQUARE_1:68;then
    ((u^2)^2+(v^2)^2)-2*u^2*v^2+2^2*(u^2*v^2)=a^2+b^2 by SQUARE_1:68;then
    ((u^2)^2+(v^2)^2)-2*u^2*v^2+(2*2)*(u^2*v^2)=a^2+b^2
                              by SQUARE_1:def 3;then
    4*(u^2*v^2)-2*u^2*v^2+((u^2)^2+(v^2)^2)=a^2+b^2 by XCMPLX_1:30;then
    4*(u^2*v^2)-2*(u^2*v^2)+((u^2)^2+(v^2)^2)=a^2+b^2 by XCMPLX_1:4;then
    (4-2)*(u^2*v^2)+((u^2)^2+(v^2)^2)=a^2+b^2 by XCMPLX_1:40;then
    (u^2)^2+2*(u^2*v^2)+(v^2)^2=a^2+b^2 by XCMPLX_1:1;then
    (u^2)^2+2*u^2*v^2+(v^2)^2=a^2+b^2 by XCMPLX_1:4;then
 A110: sqrt (u^2+v^2)^2=sqrt (a^2+b^2) by SQUARE_1:63;
       u^2>=0 & v^2>=0 by SQUARE_1:72;then
       u^2+v^2>=0+0 by REAL_1:55;then
 A114: u^2+v^2=sqrt (a^2+b^2) by A110,SQUARE_1:89;then
       u^2-v^2+v^2+u^2=a+sqrt (a^2+b^2) by XCMPLX_1:1,A108;then
       u^2-(v^2-v^2)+u^2=a+sqrt (a^2+b^2) by XCMPLX_1:37;then
       u^2+u^2=a+sqrt (a^2+b^2) by XCMPLX_1:17;then
       2*u^2=a+sqrt (a^2+b^2) by XCMPLX_1:11;then
 A116: u^2=(a+sqrt (a^2+b^2))/2 by XCMPLX_1:90;
       v^2+u^2-u^2+v^2=sqrt (a^2+b^2)-a by XCMPLX_1:37,A108,A114;then
       v^2+v^2=sqrt (a^2+b^2)-a by XCMPLX_1:26;then
       v^2*2=sqrt (a^2+b^2)-a by XCMPLX_1:11;then
       v^2=(sqrt (a^2+b^2)-a)/2 by XCMPLX_1:90;then
       v^2=(-(-sqrt (a^2+b^2)+a))/2 by XCMPLX_1:163;then
 A118: v^2=(-a+sqrt (a^2+b^2))/2 by XCMPLX_1:141;
 A140: sqrt u^2=sqrt ((a+sqrt (a^2+b^2))/2) &
       sqrt v^2=sqrt ((-a+sqrt (a^2+b^2))/2) by A116,A118;
       per cases;
          suppose b>=0;then
                2*(u*v)>=0 by A108,XCMPLX_1:4;then
                -2*(u*v)<=-0 by REAL_1:50;then
                (-2)*(u*v)<=0 by XCMPLX_1:175;then
                u*v>=0 by REAL_2:129;then
                u<=0 & v<=0 or u>=0 & v>=0 by SQUARE_1:25;then
           -u=sqrt ((a+sqrt (a^2+b^2))/2) & -v=sqrt ((-a+sqrt (a^2+b^2))/2)  or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                    by A140,SQUARE_1:89,SQUARE_1:90;then
                0-u=0+sqrt ((a+sqrt (a^2+b^2))/2)
                & 0-v=0+sqrt ((-a+sqrt (a^2+b^2))/2) or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                             by XCMPLX_1:150;then
                0+u=0-sqrt ((a+sqrt (a^2+b^2))/2)
                & 0+v=0-sqrt ((-a+sqrt (a^2+b^2))/2) or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                             by XCMPLX_1:35;then
           u=-sqrt ((a+sqrt (a^2+b^2))/2) & v=-sqrt ((-a+sqrt (a^2+b^2))/2) or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                             by XCMPLX_1:150;
           hence thesis by A104;
           end;
           suppose b<0;then
                2*(u*v)<0 by A108,XCMPLX_1:4;then
                u*v<0 by REAL_2:132;then
                u<0 & v>0 or u>0 & v<0 by REAL_2:132;then
          -u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)  or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & -v=sqrt ((-a+sqrt (a^2+b^2))/2)
                    by A140,SQUARE_1:89,SQUARE_1:90;then
                0-u=0+sqrt ((a+sqrt (a^2+b^2))/2) 
                & v=sqrt ((-a+sqrt (a^2+b^2))/2) or
                u=sqrt ((a+sqrt (a^2+b^2))/2) &
                0-v=0+sqrt ((-a+sqrt (a^2+b^2))/2) by XCMPLX_1:150;then
                0+u=0-sqrt ((a+sqrt (a^2+b^2))/2)
                & v=sqrt ((-a+sqrt (a^2+b^2))/2) or
                u=sqrt ((a+sqrt (a^2+b^2))/2)
                & 0+v=0-sqrt ((-a+sqrt (a^2+b^2))/2) by XCMPLX_1:35;
          hence thesis by A104,XCMPLX_1:150;
          end;
end;

theorem
  CPoly2(0c,0c,0c,z) = 0
proof
  CPoly2(0c,0c,0c,z) = 0c*z^2+0c*z+0c by Def10
     .=0c*z^2+0c+0c by COMPLEX1:28
     .=0c+0c+0c by COMPLEX1:28
     .=0c+0c by COMPLEX1:22;
  hence thesis by COMPLEX1:14;
end;

theorem Th431:
  z1<>0 & CPoly2(z1,0c,0c,z) = 0 implies z=0
proof
    assume
 A100: z1<>0 & CPoly2(z1,0c,0c,z) = 0;then
      0= z1*z^2+0c*z+0c by Def10
       .=z1*z^2+0c+0c by COMPLEX1:28
       .=z1*z^2+0c by COMPLEX1:22
       .=z1*z^2 by COMPLEX1:22;then
       z^2=0 by XCMPLX_1:6,A100;then
       z*z=0 by Th4;
       hence z=0 by XCMPLX_1:6;
end;

theorem
    z1<>0 & CPoly2(z1,z2,0c,z)=0 implies z=-(z2/z1) or z=0
proof
       assume
 A100: z1<>0 & CPoly2(z1,z2,0c,z)=0;then
       0=z1*z^2+z2*z+0 by Def10,COMPLEX1:def 6
       .=z1*(z*z)+z2*z by Th4
       .=z1*z*z+z2*z by XCMPLX_1:4
       .=(z1*z+z2)*z by XCMPLX_1:8;then
       z1*z+z2=0 or z=0 by XCMPLX_1:6;then
       Poly1(z1,z2,z) = 0 or z=0 by POLYEQ_1:def 1;
    hence z=-(z2/z1) or z=0 by Th32,A100;
end;

theorem Th45:
    z1<>0c & CPoly2(z1,0c,z3,z)=0c implies
    (for s st s=-(z3/z1) holds
       z=[* sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
            sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
       z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
           -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
       z=[* sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
           -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
       z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
            sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*])
proof
       assume
 A100: z1<>0c & CPoly2(z1,0c,z3,z)=0c; then K: z1 <> 0 by COMPLEX1:def 6;
       let s;
       assume
 A101: s=-(z3/z1);
       0=0c by COMPLEX1:def 6 .= z1*z^2+0c*z+z3 by Def10,A100
       .=z1*z^2+0c+z3 by COMPLEX1:28
       .=z1*z^2+z3 by COMPLEX1:22;then
       z1*z^2=-z3 by XCMPLX_0:def 6;then
       z^2=(-z3)/z1 by XCMPLX_1:90,K;then
       z^2=s by A101,XCMPLX_1:188;
      hence thesis by Th42;
end;

theorem Th43:
    z1<>0 & CPoly2(z1,z2,z3,z)=0c implies
          (for h,t st h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1) holds
           z=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
                sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
               -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
               -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
           z=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
                sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t)
proof
       assume
 A100: z1<>0 & CPoly2(z1,z2,z3,z)=0c;
       let h,t;
       assume
 A101: h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1);
      z1*z^2+z2*z+z3=0c by Def10,A100;then
      (z1*z^2+z2*z+z3)/z1=0c by COMPLEX1:87;then
      (z1*z^2+z2*z)/z1+z3/z1=0c by XCMPLX_1:63;then
      (z^2*z1)/z1+(z2*z)/z1+z3/z1=0c by XCMPLX_1:63;then
      z^2+(z*z2)/z1+z3/z1=0c by XCMPLX_1:90,A100;then
      z^2+(z*z2)*z1"+z3/z1=0c by XCMPLX_0:def 9;then
      z^2+z*(z2*z1")+z3/z1=0c by XCMPLX_1:4;then
 A104:z^2+(z2/z1)*z+z3/z1=0c by XCMPLX_0:def 9;
      [*2,0*]<>0 by ARYTM_0:def 7;then
 A106:z^2+[*2,0*]*z2/([*2,0*]*z1)*z+z3/z1=0c by XCMPLX_1:92,A104;
      z^2+[*2,0*]*(z2/([*2,0*]*z1))*z+z3/z1=0c
                  by XCMPLX_1:75,A106;then
      z^2+[*2,0*]*z*(z2/([*2,0*]*z1))+z3/z1=0c by XCMPLX_1:4;then
      z^2+2*z*(z2/([*2,0*]*z1))+z3/z1=0c by Lem1;then
      z^2+2*z*(z2/(2*z1))+z3/z1=0c by Lem1;then
      z^2+2*z*(z2/(2*z1))+(z2/(2*z1))^2-(z2/(2*z1))^2+z3/z1=0c
                                   by XCMPLX_1:26;then
      (z+z2/(2*z1))^2-(z2/(2*z1))^2+z3/z1=0c by Tha;then
      (z+(z2/(2*z1)))^2-((z2/(2*z1))^2-z3/z1)=0c by XCMPLX_1:37;then
      (z+(z2/(2*z1)))^2-((z2/(2*z1))^2-z3/z1)=0 by COMPLEX1:def 6;then
       (z+t)^2=h by A101,XCMPLX_1:15;then
      z+t-t=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
             sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
      z+t-t=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
            -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
      z+t-t=[* sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
            -sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t   or
      z+t-t=[*-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2),
        sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2)*]-t by Th42;
    hence thesis by XCMPLX_1:26;
end;

definition let z1,z2,z3,z4,z;
  func CPoly3(z1,z2,z3,z4,z) -> Element of COMPLEX equals
     :Def11:   z1*z^3 +z2*z^2+z3*z+z4;
  coherence;
end;

theorem
  z^2=1 implies z=1 or z=-1
proof
  assume z^2=1; then
  z*z = 1 by Th4;
  hence thesis by XCMPLX_1:183;
end;

theorem Th50:
  z#N 3=z*z*z & z#N 3=z^2*z & z#N 3 = z^3
proof
  A100: z#N 3=z GeoSeq.(2+1) by COMSEQ_3:def 2
         .=z GeoSeq.(1+1)*z by COMSEQ_3:def 1
         .=z GeoSeq.(0+1)*z*z by COMSEQ_3:def 1
         .=z GeoSeq.0*z*z*z by COMSEQ_3:def 1
         .=1r*z*z*z by COMSEQ_3:def 1
         .=z*z*z by COMPLEX1:29;then
         z#N 3=z^2*z by Th4;
  hence thesis by Def7,A100;
end;

theorem
    z1<>0 & CPoly3(z1,z2,0c,0c,z)=0c implies z=-z2/z1 or z=0
proof
    assume
 A100: z1<>0 & CPoly3(z1,z2,0c,0c,z)=0c;then
       0c=z1*z^3 +z2*z^2+0c*z+0c by Def11
        .=z1*z^3 +z2*z^2+0c+0c by COMPLEX1:28
        .=z1*z^3 +z2*z^2+0c by COMPLEX1:22
        .=z1*z^3 +z2*z^2 by COMPLEX1:22
        .=z1*(z^2*z) +z2*z^2 by Def7
        .=z1*z*z^2 +z2*z^2 by XCMPLX_1:4
        .=(z1*z +z2)*z^2 by XCMPLX_1:8;then
        z1*z +z2=0c or z^2=0c by XCMPLX_1:6,COMPLEX1:def 6;then
        z1*z +z2=0c or 1r*z^2=0c by COMPLEX1:29;then
        z1*z +z2=0c or 1r*z^2+0c=0c by COMPLEX1:22;then
        z1*z +z2=0c or 1r*z^2+0c+0c=0c by COMPLEX1:22;then
        Poly1(z1,z2,z)=0c or 1r*z^2+0c*z+0c=0c
          by POLYEQ_1:def 1,COMPLEX1:28;then
        Poly1(z1,z2,z)=0 or 1r*z^2+0c*z+0c=0 by COMPLEX1:def 6;then
        z=-(z2/z1) or CPoly2(1r,0c,0c,z)=0 by Th32,A100,Def10;
       hence z=-(z2/z1) or z=0 by Th431,COMPLEX1:def 7;
end;

theorem 
    z1<>0c & CPoly3(z1,0c,z3,0c,z)=0c implies
     (for s st s=-(z3/z1) holds
     z=0c or
     z=[*sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
          sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
     z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
         -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
     z=[* sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
         -sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*] or
     z=[*-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2),
          sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2)*])
proof
       assume
 A100: z1<>0c & CPoly3(z1,0c,z3,0c,z)=0c;
       let s;
       assume
 A102: s=-(z3/z1);
       0c=z1*z^3 +0c*z^2+z3*z+0c by Def11,A100
       .=z1*z#N 3 +0c*z^2+z3*z+0c by Th50
       .=z1*z#N 3 +0c*z^2+z3*z by COMPLEX1:22
       .=z1*z#N 3 +0c+z3*z by COMPLEX1:28
       .=z1*z#N 3+z3*z by COMPLEX1:22
       .=z1*(z^2*z)+z3*z by Th50
       .=z1*z^2*z+z3*z by XCMPLX_1:4
       .=(z1*z^2+z3)*z by XCMPLX_1:8;then
       z1*z^2+z3=0c or z=0c by XCMPLX_1:6,COMPLEX1:def 6;then
       z1*z^2+0c+z3=0c or z=0c by COMPLEX1:22;then
       z1*z^2+0c*z+z3=0c or z=0c by COMPLEX1:28;then
       CPoly2(z1,0c,z3,z)=0c or z=0c by Def10;
     hence thesis by Th45,A100,A102;
