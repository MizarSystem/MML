:: Solving Complex Roots of Polynomial Equation of Degree 2 and 3 with
:: Complex Coefficients
::  by Yuzhong Ding and Xiquan Liang
::
:: Received January 26, 2004
:: Copyright (c) 2004 Association of Mizar Users

environ

 vocabulary COMPLEX1, ARYTM_3, ARYTM_1, POLYEQ_1, SQUARE_1, FUNCT_3, POLYEQ_3,
      RELAT_1, POWER, GROUP_1, PREPOWER, FUNCT_1, SIN_COS, HAHNBAN1, COMPTRIG;
 notation SUBSET_1, NUMBERS, XBOOLE_0, XCMPLX_0, XREAL_0, REAL_1, COMPLEX1,
      POLYEQ_1, SQUARE_1, COMSEQ_1, ARYTM_0, QUIN_1, POWER, NAT_1, COMSEQ_3,
      SIN_COS, HAHNBAN1, PREPOWER, BINARITH, SEQ_1, COMPTRIG;
 constructors COMPLEX1, REAL_1, POLYEQ_1, QUIN_1, NEWTON, GROUP_1, COMSEQ_1,
      ARYTM_0, SQUARE_1, SUBSET_1, POWER, INT_1, NAT_1, COMSEQ_3, COMPLFLD,
      COMPLSP1, RLVECT_1, HAHNBAN1, COMPTRIG, SIN_COS, PREPOWER, BINARITH,
      VECTSP_1, NUMBERS, SEQ_1, COMPLEX2, ARYTM_3, FUNCT_4, BINOP_2;
 clusters ARYTM_3, REAL_1, NEWTON, SQUARE_1, XREAL_0, RELAT_1, QUIN_1, INT_1,
      SIN_COS, COMPLFLD, COMPLEX1, BINARITH, VECTSP_1, XCMPLX_0, NUMBERS,
      POLYEQ_1;
 requirements ARITHM, SUBSET, REAL, BOOLE, NUMERALS;
 theorems COMPLEX1, POLYEQ_1, REAL_1, REAL_2, AXIOMS, SQUARE_1, QUIN_1,
      COMPLFLD, NEWTON, COMSEQ_3, COMPTRIG, HAHNBAN1, NAT_1, BINARITH, POWER,
      SIN_COS, SIN_COS2, PREPOWER, CARD_1, COMPLEX2, XCMPLX_0, XCMPLX_1,
      ARYTM_0;
 schemes NAT_1, COMPTRIG;

begin

Lm1: 0c = [*0,0*] by ARYTM_0:def 7,COMPLEX1:def 6;

 reserve a,b,c,d,e,a',b',c',d',x,y,x1,y1,x2,y2,u,v for Real,
         s,t,h,z,z1,z2,z3,z4,s1,s2,s3,p,q for Element of COMPLEX,
         m,n for Nat;

definition let a be Real,z;
  redefine func a*z -> Element of COMPLEX equals
     :Def1:   [*a,0*]*z;
  compatibility by ARYTM_0:def 7;
  correctness by XCMPLX_0:def 2;
  redefine func a + z -> Element of COMPLEX equals :Def2:
    z+[*a,0*];
  compatibility by ARYTM_0:def 7;
  correctness by XCMPLX_0:def 2;
end;

definition let z;
  redefine func z^2 -> Element of COMPLEX equals
    :Def3:  [*(Re z)^2-(Im z)^2,2*(Re z*Im z)*];
  compatibility
  proof
      z^2 = z * z by SQUARE_1:def 3
       .= [* Re z * Re z - Im z * Im z, Re z * Im z + Re z * Im z *]
            by COMPLEX1:def 10
       .= [* (Re z)^2 - Im z * Im z, Re z * Im z + Re z * Im z *]
            by SQUARE_1:def 3
       .= [*(Re z)^2-(Im z)^2,2*(Re z*Im z)*] by SQUARE_1:def 3;
    hence thesis;
  end;
  correctness by XCMPLX_0:def 2;
end;

definition let a,b,c,z;
  redefine func Poly2 (a,b,c,z) -> Element of COMPLEX;
  coherence by XCMPLX_0:def 2;
end;

theorem Th1:
    [*a,c*]*[*b,d*]=[*a*b-c*d,a*d+b*c*]
 proof
          [*a,c*]*[*b,d*]=[* Re [*a,c*]*Re [*b,d*]
               -Im [*a,c*]*Im [*b,d*],Re [*a,c*]*Im [*b,d*]
               +Re [*b,d*]*Im [*a,c*] *] by COMPLEX1:def 10
            .=[* a*Re [*b,d*]-Im [*a,c*]*Im [*b,d*],
              Re [*a,c*]*Im [*b,d*]+Re [*b,d*]*Im [*a,c*] *]
                         by COMPLEX1:7
            .=[* a*b-Im [*a,c*]*Im [*b,d*],
              Re [*a,c*]*Im [*b,d*]+Re [*b,d*]*Im [*a,c*] *]
                         by COMPLEX1:7
            .=[* a*b-c*Im [*b,d*],
              Re [*a,c*]*Im [*b,d*]+Re [*b,d*]*Im [*a,c*] *]
                         by COMPLEX1:7
            .=[* a*b-c*d,Re [*a,c*]*Im [*b,d*]
               +Re [*b,d*]*Im [*a,c*] *] by COMPLEX1:7
            .=[* a*b-c*d,a*Im [*b,d*]+Re [*b,d*]*Im [*a,c*] *]
                           by COMPLEX1:7
            .=[* a*b-c*d,a*d+Re [*b,d*]*Im [*a,c*] *]
                           by COMPLEX1:7
            .=[* a*b-c*d,a*d+b*Im [*a,c*] *] by COMPLEX1:7
            .=[* a*b-c*d,a*d+b*c *] by COMPLEX1:7;
       hence [*a,c*]*[*b,d*]=[*a*b-c*d,a*d+b*c*];
end;

theorem Th2:
  z=[*x,y*] implies z^2=[*x^2-y^2,2*x*y*]
proof
  assume A1:z=[*x,y*];
     z^2=[*(Re z)^2-(Im z)^2,2*(Re z*Im z)*] by Def3
     .=[*x^2-(Im [*x,y*])^2,2*(Re [*x,y*]*Im [*x,y*])*]
                           by A1,COMPLEX1:7
     .=[*x^2-y^2,2*(Re [*x,y*]*Im [*x,y*])*] by COMPLEX1:7
     .=[*x^2-y^2,2*(x*Im [*x,y*])*] by COMPLEX1:7
     .=[*x^2-y^2,2*(x*y)*] by COMPLEX1:7
     .=[*x^2-y^2,2*x*y*];
  hence z^2=[*x^2-y^2,2*x*y*];
end;

theorem Th3:
  for a,b holds [*a,0*]*[*b,0*]=[*a*b,0*]
proof
  let a,b;
    [*a,0*]*[*b,0*] =[*a*b-0*0,a*0+0*b*] by Th1
           .=[*a*b,0*];
  hence [*a,0*]*[*b,0*]=[*a*b,0*];
end;

theorem Th4:
    a <> 0 & delta(a,b,c) >=0 & Poly2(a,b,c,z) = 0 implies
         z= (-b+sqrt delta(a,b,c))/(2*a) or
         z= (-b-sqrt delta(a,b,c))/(2*a) or
         z= -b/(2*a)
proof
      assume
  A1:  a <> 0 & delta(a,b,c) >= 0;
         assume
  A2:  Poly2(a,b,c,z) = 0;
         set x=Re z;
         set y=Im z;
           a*z^2+b*z+c = 0 by A2,POLYEQ_1:def 2;
         then [*a,0*]*z^2+b*z+c = 0 by Def1;
         then [*a,0*]*z^2+[*b,0*]*z+c = 0 by Def1;
         then [*a,0*]*z^2+[*b,0*]*z+[*c,0*] = 0 by Def2;
         then [*a,0*]*([*x^2-y^2,2*(x*y)*])+[*b,0*]*z+[*c,0*] = 0
                            by Def3;
         then 0=[* Re [*a,0*]*Re [*x^2-y^2,2*x*y*]-Im [*a,0*]*
         Im [*x^2-y^2,2*x*y*],Re [*a,0*]*Im [*x^2-y^2,2*x*y*]
         +Re [*x^2-y^2,2*x*y*]*Im [*a,0*]  *]
           +[*b,0*]*z+[*c,0*] by COMPLEX1:def 10
         .=[*a*Re [*x^2-y^2,2*x*y*]-Im [*a,0*]*
         Im [*x^2-y^2,2*x*y*],Re [*a,0*]*Im [*x^2-y^2,2*x*y*]
         +Re [*x^2-y^2,2*x*y*]*Im [*a,0*]  *]
           +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-Im [*a,0*]*
         Im [*x^2-y^2,2*x*y*],Re [*a,0*]*Im [*x^2-y^2,2*x*y*]
         +Re [*x^2-y^2,2*x*y*]*Im [*a,0*]  *]
           +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0*Im [*x^2-y^2,2*x*y*]
         ,Re [*a,0*]*Im [*x^2-y^2,2*x*y*]+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]  *] +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0,Re [*a,0*]*(2*x*y)+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]  *] +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0,a*(2*x*y)+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*] *] +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)
         *Im [*a,0*] *] +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)
         *0*] +[*b,0*]*z+[*c,0*] by COMPLEX1:7;
         then [*a*(x^2-y^2)-0*(2*x*y),0+a*(2*x*y)*]
           +[*b,0*]*[*x,y*]+[*c,0*] = 0 by COMPLEX1:8;
         then [*a*(x^2-y^2)-0,0+a*(2*x*y)*]
           +[*b*x-0*y,x*0+b*y*]+[*c,0*] = 0 by Th1;
         then [*a*(x^2-y^2)+b*x,a*(2*x*y)+b*y*]+[*c,0*] = 0
                      by COMPLFLD:2;
         then A3: [*a*(x^2-y^2)+b*x+c,a*(2*x*y)+b*y+0*] = 0 by COMPLFLD:2
                   .= [*0,0*] by ARYTM_0:def 7;
         then A4:a*(x^2-y^2)+b*x+c = 0 & a*(2*x*y)+b*y = 0 by ARYTM_0:12;
  A5:  a*(x^2-y^2)+b*x+c = 0 & (2*a*x+b+0)*y = 0 by A3,ARYTM_0:12;
         per cases by A5,XCMPLX_1:6;
          suppose
  A6:      y = 0;
  then Poly2(a,b,c,x)=0 by A4,POLYEQ_1:def 2,SQUARE_1:60;
             then Re z=(-b+sqrt delta(a,b,c))/(2*a) & Im z = 0 or
             Re z= (-b-sqrt delta(a,b,c))/(2*a) & Im z = 0
           by A1,A6,POLYEQ_1:5;
             then z=[*(-b+sqrt delta(a,b,c))/(2*a),0*] or
             z= [*(-b-sqrt delta(a,b,c))/(2*a),0*] by COMPLEX1:8;
         hence thesis by ARYTM_0:def 7;
         end;
         suppose 2*a*x+b = 0;
             then A7: (2*a)*x+0= 0-b;
               a"<>0 by A1,XCMPLX_1:203;
             then 2/a" <> 0 by XCMPLX_1:50;
             then 2/(1/a) <> 0 by XCMPLX_1:217;
             then (2*a)/1 <> 0 by XCMPLX_1:78;
             then A8: x= (-b)/(2*a) by A7,XCMPLX_1:90;
  then x = -b/(2*a) by XCMPLX_1:188;
             then a*((b/(2*a))^2-y^2)+b*(-b/(2*a))+c = 0 by A4,SQUARE_1:61;
             then a*((b/(2*a))^2-y^2)+((b*(-b/(2*a))+c)-(b*(-b/(2*a))+c))
                          = 0-(b*(-b/(2*a))+c);
             then (b/(2*a))^2-y^2 = (-(b*(-b/(2*a))+c))/a-0
   by A1,XCMPLX_1:90;
             then (b/(2*a))^2- (-(b*(-b/(2*a))+c))/a= y^2-0 by XCMPLX_1:24;
             then y^2 = (b/(2*a))^2- (-(-b*(b/(2*a))+c))/a;
             then y^2 = (b/(2*a))^2- (-(-(b*b)/(2*a)+c))/a by XCMPLX_1:75;
            then y^2 = (b/(2*a))^2- (-((-(b^2/(2*a)))+c))/a by SQUARE_1:def 3;
             then y^2 = (b/(2*a))^2- (-c+b^2/(2*a)+0)/a;
             then y^2 = (b/(2*a))^2- ((-c)/a+(b^2/(2*a))/a+0/a)
                                by XCMPLX_1:64;
             then y^2 = (b/(2*a))^2- ((-c)*a"+(b^2/(2*a))/a) by XCMPLX_0:def 9
;
             then y^2 = (b/(2*a))^2- (-c*a"+(b^2/(2*a))*a") by XCMPLX_0:def 9;
             then y^2 = (b/(2*a))^2+c*a"-((b^2/(2*a))*a");
  then A9:      y^2 *((2*a)^2)=(b^2/(2*a)^2+c*a"-((b^2/(2*a))*a"))*((2*a)^2)
                                by SQUARE_1:69
              .=(b^2/(2*a)^2)*(2*a)^2+(c*a")*(2*a)^2
                             -((b^2/(2*a))*a")*(2*a)^2
              .=(b^2*((2*a)^2)")*(2*a)^2+(c*a")*(2*a)^2
                          -((b^2/(2*a))*a")*(2*a)^2 by XCMPLX_0:def 9
              .=(b^2*((2*a)^2)")*(2*a)^2+(c*a")*(2*a)^2
                          -((b^2*(2*a)")*a")*(2*a)^2 by XCMPLX_0:def 9
              .=b^2/((2*a)^2)*((2*a)^2)+(c*a")*(2*a)^2
                          -(b^2*(2*a)"*a")*(2*a)^2 by XCMPLX_0:def 9;
              a"<>0 by A1,XCMPLX_1:203;
            then 2/a" <> 0 by XCMPLX_1:50;
            then 2/(1/a) <> 0 by XCMPLX_1:217;
  then A10:    (2*a)/1 <> 0 by XCMPLX_1:78;
  then A11:     (2*a)^2 > 0 by SQUARE_1:74;
  then A12:     y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2-(b^2*((2*a)"*a"))*(2*a)^2 by
A9,XCMPLX_1:88
              .=b^2+(c*a")*(2*a)^2-(b^2*((2*a)*a)")*(2*a)^2
                          by XCMPLX_1:205
              .=b^2+(c*a")*(2*a)^2-(b^2*(2*(a*a))")*(2*a)^2;
            set t = (b^2*(2*(a*a))")*(2*a)^2;
              t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*((2*a)^2)");
            then t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*(1/(2*a)^2))
                             by XCMPLX_1:217;
            then t*((2*a)^2)" = b^2*(2*(a*a))"*1 by A11,XCMPLX_1:107;
            then t*((2*a)^2)" = b^2*(2*a^2)" by SQUARE_1:def 3;
            then t*((2*a)^2)"*2" = b^2*((2*a^2)"*2");
            then t*((2*a)^2)"*2" = b^2*((2*(a^2*2))") by XCMPLX_1:205;
            then t*((2*a)^2)"*2" = b^2*((2*2*a^2)");
            then t*((2*a)^2)"*2" = b^2*((2^2*a^2)") by SQUARE_1:def 3;
            then t*((2*a)^2)"*2" = b^2*((2*a)^2)" by SQUARE_1:68;
            then (t*2")*((2*a)^2)"*(2*a)^2 = b^2*((2*a)^2)"*(2*a)^2
                               by XCMPLX_1:4;
            then (t*2")/(2*a)^2*(2*a)^2 = b^2*((2*a)^2)"*(2*a)^2
                            by XCMPLX_0:def 9;
            then (t*2")/(2*a)^2*(2*a)^2 = b^2/(2*a)^2*(2*a)^2
                       by XCMPLX_0:def 9;
            then t*2" = b^2/(2*a)^2*(2*a)^2 by A11,XCMPLX_1:88;
            then t*2" = b^2 by A11,XCMPLX_1:88;
            then t/2 *2 = b^2 *2 by XCMPLX_0:def 9;
  then A13:     y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2-2*b^2 by A12,XCMPLX_1:88;
            set t=(c*a")*(2*a)^2;
              t=(c*a")*((2*a)*(2*a)) by SQUARE_1:def 3;
            then t=c*a"*a*2*(2*a);
            then t=(c/a*a)*2*(2*a) by XCMPLX_0:def 9;
            then t=c*2*(2*a) by A1,XCMPLX_1:88;
 then A14:  (y *(2*a))^2=4*a*c-b^2 by A13,SQUARE_1:68;
 A15:      -delta(a,b,c) <= 0 by A1,REAL_1:66;
              -delta(a,b,c)=-(b^2-4*a*c) by QUIN_1:def 1
                                 .=4*a*c-b^2;
            then (y *(2*a))^2 = 0 by A14,A15,SQUARE_1:72;
            then y *(2*a) = 0 by SQUARE_1:73;
            then Re z=-b/(2*a) & Im z=0 by A8,A10,XCMPLX_1:6,188;
            then z = [*-b/(2*a),0*] by COMPLEX1:8;
          hence thesis by ARYTM_0:def 7;
           end;
end;

theorem Th5:
    a <> 0 & delta(a,b,c) < 0 & Poly2(a,b,c,z) = 0c implies
        z= [*-b/(2*a),sqrt (-delta(a,b,c))/(2*a)*] or
        z= [*-b/(2*a),-sqrt (-delta(a,b,c))/(2*a)*]
proof
      assume
  A1:  a <> 0 & delta(a,b,c) < 0;
           now let z;
         assume
  A2:  Poly2(a,b,c,z) = 0c;
         set x=Re z;
         set y=Im z;
           a*z^2+b*z+c = 0c by A2,POLYEQ_1:def 2;
         then [*a,0*]*z^2+b*z+c = 0c by Def1;
         then [*a,0*]*z^2+[*b,0*]*z+c = 0c by Def1;
         then [*a,0*]*z^2+[*b,0*]*z+[*c,0*] = 0c by Def2;
         then [*a,0*]*([*x^2-y^2,2*(x*y)*])+[*b,0*]*z+[*c,0*] = 0c by Def3;
         then [*Re [*a,0*]*Re [*x^2-y^2,2*x*y*]-Im [*a,0*]*Im [*x^2-y^2,2*x*y
*]
         ,Re [*a,0*]*Im [*x^2-y^2,2*x*y*]+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]*]+[*b,0*]*z+[*c,0*]= 0c by COMPLEX1:def 10;
         then [*a*Re [*x^2-y^2,2*x*y*]-Im [*a,0*]*Im [*x^2-y^2,2*x*y*]
         ,Re [*a,0*]*Im [*x^2-y^2,2*x*y*]+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]*]+[*b,0*]*z+[*c,0*]= 0c by COMPLEX1:7;
         then [*a*(x^2-y^2)-Im [*a,0*]*Im [*x^2-y^2,2*x*y*]
         ,Re [*a,0*]*Im [*x^2-y^2,2*x*y*]+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]*]+[*b,0*]*z+[*c,0*]= 0c by COMPLEX1:7;
         then [*a*(x^2-y^2)-0*Im [*x^2-y^2,2*x*y*]
         ,Re [*a,0*]*Im [*x^2-y^2,2*x*y*]+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]*]+[*b,0*]*z+[*c,0*]= 0c by COMPLEX1:7;
         then [*a*(x^2-y^2)-0,Re [*a,0*]*(2*x*y)+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]*]+[*b,0*]*z+[*c,0*]= 0c by COMPLEX1:7;
         then [*a*(x^2-y^2)-0,a*(2*x*y)+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]*]+[*b,0*]*z+[*c,0*]= 0c by COMPLEX1:7;
         then [*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)
         *Im [*a,0*]*]+[*b,0*]*z+[*c,0*]= 0c by COMPLEX1:7;
         then [*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)
         *0*]+[*b,0*]*z+[*c,0*]= 0c by COMPLEX1:7;
         then [*a*(x^2-y^2)-0*(2*x*y),(x^2-y^2)*0+a*(2*x*y)*]
           +[*b,0*]*[*x,y*]+[*c,0*] = 0c  by COMPLEX1:8;
         then [*a*(x^2-y^2)-0,0+a*(2*x*y)*]+[*b*x-0*y,x*0+b*y*]+[*c,0*] = 0c
                                  by Th1;
         then [*a*(x^2-y^2)+b*x,a*(2*x*y)+b*y*]+[*c,0*] = 0c by COMPLFLD:2;
         then A3: [*a*(x^2-y^2)+b*x+c,a*(2*x*y)+b*y+0*] = [*0,0*]
              by Lm1,COMPLFLD:2;
  then A4:  a*(x^2-y^2+0)+b*x+c = 0 & a*(2*x*y)+b*y = 0 by ARYTM_0:12;
          -a*y^2+((b*x+a*x^2)+c )+a*y^2= 0+a*y^2 by A3,ARYTM_0:12;
         then ((b*x+a*x^2)+c )+a*y^2-a*y^2= a*y^2 by XCMPLX_1:155;
         then ((b*x+a*x^2)+c )+(a*y^2-a*y^2)= a*y^2 by XCMPLX_1:29;
         then ((b*x+a*x^2)+c)+0 = a*y^2;
          then a*x^2*a+b*x*a+c*a = a*y^2*a by XCMPLX_1:9;
          then x^2*(a*a)+x*b*a+c*a = y^2*a*a;
          then x^2*a^2+x*b*a+c*a = y^2*(a*a) by SQUARE_1:def 3;
          then x^2*a^2+x*b*a+c*a = y^2*a^2 by SQUARE_1:def 3;
          then ((x*a)^2+x*b*a)+0+c*a = y^2*a^2 by SQUARE_1:68;
          then ((x*a)^2+2*(x*a)*(b/2)+(b/2)^2)-(b/2)^2+c*a = (y*a)^2
                                     by SQUARE_1:68;
          then (x*a+b/2)^2-(b/2)^2+c*a = (y*a)^2 by SQUARE_1:63;
          then (x*a+b/2)^2-b^2/2^2+c*a*4/4 = (y*a)^2
  by SQUARE_1:69;
          then (x*a+b/2)^2+(-b^2/2^2)+c*a*4/4 = (y*a)^2;
          then (x*a+b/2)^2+(-b^2)/2^2+c*a*4/4 = (y*a)^2 by XCMPLX_1:188;
          then (x*a+b/2)^2+(-b^2)/(2*2)+c*a*4/4 = (y*a)^2 by SQUARE_1:def 3;
          then A5: (x*a+b/2)^2+((-b^2)/4+c*a*4/4+0/4) = (y*a)^2 by XCMPLX_1:1;
          set t1= (x*a+b/2)^2;
          set t2=(-b^2+c*a*4)/4;
 A6:    t1>=0 by SQUARE_1:72;
           t2=(-(b^2-4*a*c))/4;
 then A7:    t2=(-delta(a,b,c))/4 by QUIN_1:def 1;
            0-delta(a,b,c)>0 by A1,REAL_2:173;
          then t2>0 by A7,REAL_2:127;
          then 0+0<t1+t2 by A6,REAL_1:67;
          then y^2*a^2>0 by A5,SQUARE_1:68;
          then y^2 <> 0;
          then y*y <> 0 by SQUARE_1:def 3;
 then A8:    y<>0;
           (a*(2*x))*y+b*y = 0 by A3,ARYTM_0:12;
         then (a*2*x)*y+(b*y-b*y) = 0-b*y;
         then (a*2*x)*y = (-b)*y;
         then a*(2*x) = -b by A8,XCMPLX_1:5;
         then 2*x = (-b)/a by A1,XCMPLX_1:90;
         then x = (-b)/a/2 by XCMPLX_1:90;
         then x = 1/a*((-b)/2) by XCMPLX_1:105;
         then A9: x = (-b)/(2*a) by XCMPLX_1:104;
 then x = -b/(2*a) by XCMPLX_1:188;
         then a*((b/(2*a))^2-y^2)+b*(-b/(2*a))+c = 0 by A4,SQUARE_1:61;
         then a*((b/(2*a))^2-y^2)+((b*(-b/(2*a))+c)-(b*(-b/(2*a))+c))
                  = 0-(b*(-b/(2*a))+c);
         then (b/(2*a))^2-y^2 = (-(b*(-b/(2*a))+c))/a-0
 by A1,XCMPLX_1:90;
         then (b/(2*a))^2- (-(b*(-b/(2*a))+c))/a= y^2-0 by XCMPLX_1:24;
         then y^2 = (b/(2*a))^2- (-(-b*(b/(2*a))+c))/a;
         then y^2 = (b/(2*a))^2- (-(-(b*b)/(2*a)+c))/a by XCMPLX_1:75;
         then y^2 = (b/(2*a))^2- (-((-(b^2/(2*a)))+c))/a by SQUARE_1:def 3;
         then y^2 = (b/(2*a))^2- (-c+b^2/(2*a)+0)/a;
         then y^2 = (b/(2*a))^2- ((-c)/a+(b^2/(2*a))/a+0/a)
           by XCMPLX_1:64;
         then y^2 = (b/(2*a))^2- ((-c)*a"+(b^2/(2*a))/a) by XCMPLX_0:def 9;
         then y^2 = (b/(2*a))^2- (-c*a"+(b^2/(2*a))*a") by XCMPLX_0:def 9;
         then y^2 = (b/(2*a))^2+c*a"-((b^2/(2*a))*a");
  then A10:   y^2 *((2*a)^2)=(b^2/(2*a)^2+c*a"-((b^2/(2*a))*a"))*((2*a)^2)
                      by SQUARE_1:69
              .=(b^2/(2*a)^2)*(2*a)^2+(c*a")*(2*a)^2
                           -((b^2/(2*a))*a")*(2*a)^2
              .=(b^2*((2*a)^2)")*(2*a)^2+(c*a")*(2*a)^2
                        -((b^2/(2*a))*a")*(2*a)^2 by XCMPLX_0:def 9
              .=(b^2*((2*a)^2)")*(2*a)^2+(c*a")*(2*a)^2
                        -((b^2*(2*a)")*a")*(2*a)^2 by XCMPLX_0:def 9
              .=b^2/((2*a)^2)*((2*a)^2)+(c*a")*(2*a)^2
                        -(b^2*(2*a)"*a")*(2*a)^2 by XCMPLX_0:def 9;
            a"<>0 by A1,XCMPLX_1:203;
          then 2/a" <> 0 by XCMPLX_1:50;
          then 2/(1/a) <> 0 by XCMPLX_1:217;
  then A11:  (2*a)/1 <> 0 by XCMPLX_1:78;
  then A12:  (2*a)^2 > 0 by SQUARE_1:74;
  then A13:   y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2-(b^2*((2*a)"*a"))*(2*a)^2 by
A10,XCMPLX_1:88
            .=b^2+(c*a")*(2*a)^2-(b^2*((2*a)*a)")*(2*a)^2
                         by XCMPLX_1:205
            .=b^2+(c*a")*(2*a)^2-(b^2*(2*(a*a))")*(2*a)^2;
           set t = (b^2*(2*(a*a))")*(2*a)^2;
             t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*((2*a)^2)");
           then t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*(1/(2*a)^2))
                           by XCMPLX_1:217;
           then t*((2*a)^2)" = b^2*(2*(a*a))"*1 by A12,XCMPLX_1:107;
           then t*((2*a)^2)" = b^2*(2*a^2)" by SQUARE_1:def 3;
           then t*((2*a)^2)"*2" = b^2*((2*a^2)"*2");
           then t*((2*a)^2)"*2" = b^2*((2*(a^2*2))") by XCMPLX_1:205;
           then t*((2*a)^2)"*2" = b^2*((2*2*a^2)");
           then t*((2*a)^2)"*2" = b^2*((2^2*a^2)") by SQUARE_1:def 3;
           then t*((2*a)^2)"*2" = b^2*((2*a)^2)" by SQUARE_1:68;
           then (t*2")*((2*a)^2)"*(2*a)^2 = b^2*((2*a)^2)"*(2*a)^2
                           by XCMPLX_1:4;
           then (t*2")/(2*a)^2*(2*a)^2 = b^2*((2*a)^2)"*(2*a)^2
                           by XCMPLX_0:def 9;
           then (t*2")/(2*a)^2*(2*a)^2 = b^2/(2*a)^2*(2*a)^2
                       by XCMPLX_0:def 9;
            then t*2" = b^2/(2*a)^2*(2*a)^2 by A12,XCMPLX_1:88;
            then t*2" = b^2 by A12,XCMPLX_1:88;
            then t/2 *2 = b^2 *2 by XCMPLX_0:def 9;
  then A14:     y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2-2*b^2 by A13,XCMPLX_1:88;
            set t=(c*a")*(2*a)^2;
              t=(c*a")*((2*a)*(2*a)) by SQUARE_1:def 3;
            then t=c*a"*a*2*(2*a);
            then t=(c/a*a)*2*(2*a) by XCMPLX_0:def 9;
            then t=c*2*(2*a) by A1,XCMPLX_1:88;
  then A15:    (y *(2*a))^2=4*a*c-b^2 by A14,SQUARE_1:68;
  A16:     -delta(a,b,c) > 0 by A1,REAL_1:66;
               -delta(a,b,c)=-(b^2-4*a*c) by QUIN_1:def 1
                               .=4*a*c-b^2;
           then (y *(2*a))^2  =(sqrt (-delta(a,b,c)))^2
        by A15,A16,SQUARE_1:def 4;
           then (y *(2*a))^2  -(sqrt (-delta(a,b,c)))^2=0;
           then ((y *(2*a))+(sqrt (-delta(a,b,c))))*
           ((y *(2*a))-(sqrt (-delta(a,b,c))))=0 by SQUARE_1:67;
           then ((y *(2*a))+(sqrt (-delta(a,b,c))))=0 or
           ((y *(2*a))-(sqrt (-delta(a,b,c))))=0 by XCMPLX_1:6;
           then (y *(2*a))=0-sqrt (-delta(a,b,c)) or
           (y *(2*a))-sqrt (-delta(a,b,c))=0;
           then y =(-sqrt (-delta(a,b,c)))/(2*a) or
           y *(2*a)/(2*a)=sqrt (-delta(a,b,c))/(2*a)
 by A11,XCMPLX_1:15,90;
           then Re z=-b/(2*a) & Im z=sqrt (-delta(a,b,c))/(2*a)
           or  Re z= -b/(2*a) &  Im z= -sqrt (-delta(a,b,c))/(2*a)
 by A9,A11,XCMPLX_1:90,188;
           hence z= [* -b/(2*a),sqrt (-delta(a,b,c))/(2*a)*] or
          z= [* -b/(2*a),-sqrt (-delta(a,b,c))/(2*a)*] by COMPLEX1:8;
     end;
    hence thesis;
end;

theorem
      b <> 0 & (for z holds Poly2(0,b,c,z) = 0c) implies z = -c/b
proof
        assume
 A1:   b <> 0;
        assume
 A2:  (for z holds Poly2(0,b,c,z) = 0c);
          now let z1;
          Poly2(0,b,c,z1) = 0c by A2;
        then 0*z1^2+b*z1+c = 0c by POLYEQ_1:def 2;
         then [*0,0*]*z1^2+b*z1+c = 0c by Def1;
         then [*0,0*]*z1^2+[*b,0*]*z1+c = 0c by Def1;
           then [*b,0*]*z1+[*c,0*] = 0c by Def2,Lm1,COMPLEX1:def 6;
          then [*b,0*]*[*Re z1,Im z1*]+[*c,0*] = 0c by COMPLEX1:8;
          then [*b*Re z1-0*Im z1,b*Im z1+0*Re z1*]+[*c,0*] = 0c by Th1;
          then Re [*b*Re z1-0+c,b*Im z1+0*] = Re [*0,0*] &
          Im [*b*Re z1-0+c,b*Im z1+0*]= Im [*0,0*]
           by Lm1,COMPLFLD:2;
           then b*Re z1-0+c = Re [*0,0*] & b*Im z1= Im [*0,0*] by COMPLEX1:7;
          then b*(Re z1)-0+c = 0 & b*Im z1 = 0 by COMPLEX1:7;
          then b*(Re z1)+(c-c) =0-c & Im z1 = 0 by A1,XCMPLX_1:6,29;
          then b*(Re z1)+(c-c) =-c  & Im z1 = 0 by XCMPLX_1:150;
          then b*(Re z1)+0 =-c  & Im z1 = 0 by XCMPLX_1:14;
          then Re z1 =(-c)/b & Im z1 = 0 by A1,XCMPLX_1:90;
          then Re z1 =-c/b & Im z1 = 0 by XCMPLX_1:188;
       then z1 = [*-c/b,0*] by COMPLEX1:8;
       hence z1 = -c/b by ARYTM_0:def 7;
    end;hence thesis;
end;

theorem
      for a,b,c being Real,z,z1,z2 being Element of COMPLEX
      st a <> 0 holds
      (for z being Element of COMPLEX holds
          Poly2(a,b,c,z) = Quard(a,z1,z2,z)) implies
           [*b/a,0*] =-(z1+z2) & [*c/a,0*]= z1*z2
proof
        let a,b,c be Real,z,z1,z2 be Element of COMPLEX;
         assume
  A1:  a <> 0;
           now assume
  A2:  for z being Element of COMPLEX holds
               Poly2(a,b,c,z) = Quard(a,z1,z2,z);
           now let z be Element of COMPLEX;
           Poly2(a,b,c,0c) = Quard(a,z1,z2,0c) by A2;
       then a*0c^2+b*0c+c = Quard(a,z1,z2,0c) by POLYEQ_1:def 2;
       then a*0c^2+[*b,0*]*0c+c = Quard(a,z1,z2,0c) by Def1;
       then a*0c^2+[*b,0*]*0c+[*c,0*] = Quard(a,z1,z2,0c) by Def2
            .= a*((0c-z1)*(0c-z2)) by POLYEQ_1:def 3
            .= a*((0c+(-z1))*(0c+(-z2)))
            .= a*((-z1)*(0c+(-z2))) by COMPLEX1:22
            .= a*((-z1)*(-z2)) by COMPLEX1:22
            .= a*(z1*z2);
  then A3:  a*(z1*z2)=a*(0c*0c)+[*b,0*]*0c+[*c,0*] by SQUARE_1:def 3
            .=[*c,0*] by COMPLEX1:def 6;
then A4:     z1*z2=[*c,0*]*a" by A1,XCMPLX_1:204
            .= c * a" by ARYTM_0:def 7
            .= c/a by XCMPLX_0:def 9
            .= [* c/a,0*] by ARYTM_0:def 7;
     A5:   [*1,0*] = 1 by ARYTM_0:def 7;
            Quard(a,z1,z2,[*1,0*]) = Poly2(a,b,c,[*1,0*]) by A2
            .=a*[*1,0*]^2+b*[*1,0*]+c by POLYEQ_1:def 2
            .=a*[*1,0*]^2+[*b,0*]*[*1,0*]+c by Def1
            .=a*[*1,0*]^2+[*b,0*]*[*1,0*]+[*c,0*] by Def2
            .=a*(1*1)+[*b,0*]*1r+[*c,0*] by A5,COMPLEX1:def 7,SQUARE_1:def 3
            .=a+[*b,0*]+[*c,0*] by COMPLEX1:29;
          then a+[*b,0*]+[*c,0*]=a*(([*1,0*]-z1)*([*1,0*]-z2)) by
POLYEQ_1:def 3
            .=a*((1*1r-z1*1r)-(1-z1)*z2) by A5,COMPLEX1:def 7
            .=a*((1r-z1)-(1*z2-z1*z2)) by COMPLEX1:29
            .=a*1r+a*(-(z1+z2))+a*(z1*z2)
            .=a+a*(-(z1+z2))+[*c,0*] by A3,COMPLEX1:def 7;
         then a+[*b,0*]=a+a*(-(z1+z2)) by XCMPLX_1:2;
         then [*b,0*]=a*(-(z1+z2)) by XCMPLX_1:2;
         then [*b,0*]*a"=-(z1+z2) by A1,XCMPLX_1:204;
         then b*a"=-(z1+z2) by ARYTM_0:def 7;
         then -(z1+z2) = b/a by XCMPLX_0:def 9;
         hence [*b/a,0*] =-(z1+z2) & [*c/a,0*]= z1*z2 by A4,ARYTM_0:def 7;
         end;
         hence thesis;
      end;
   hence thesis;
end;

definition let z be Element of COMPLEX;
    func z^3 -> Element of COMPLEX equals
    :Def4: z^2*z;
 correctness;
end;

definition let a,b,c,d be Real,z be Element of COMPLEX;
    func Poly_3(a,b,c,d,z) -> Element of COMPLEX equals
    :Def5:   a*z^3+b*z^2+c*z+d;
  coherence by XCMPLX_0:def 2;
end;

theorem Th8:
    0c^3 = 0c
proof
    0c^3=0c^2*0c by Def4
     .=(0c*0c)*0c by SQUARE_1:def 3
     .=0c*0c by COMPLEX1:28;
  hence 0c^3 = 0c by COMPLEX1:28;
end;

theorem Th9:
  1r^3 = 1r
proof
    1r^3=1r^2*1r by Def4
  .=1r*1r*1r by SQUARE_1:def 3
  .=1r*1r by COMPLEX1:29;
  hence 1r^3 = 1r by COMPLEX1:29;
end;

theorem Th10:
    (-1r)^3=-1r
proof
    (-1r)^3=(-1r)^2*(-1r) by Def4
   .=((-1r)*(-1r))*(-1r) by SQUARE_1:def 3
   .=(1r*1r)*(-1r)
   .=1r*(-1r) by COMPLEX1:29;
   hence (-1r)^3 = -1r by COMPLEX1:29;
end;

theorem Th11:
    Re z^3 = (Re z)|^3 - 3*Re z*(Im z)^2
    & Im z^3 = -(Im z)|^ 3+3*(Re z)^2*Im z
proof
 A1:   z^3 = z^2*z by Def4
         .=z*z*z by SQUARE_1:def 3
         .=[*Re z,Im z*]*z*z by COMPLEX1:8
         .=[*Re z,Im z*]*[*Re z,Im z*]*z by COMPLEX1:8
         .=[*Re z,Im z*]*[*Re z,Im z*]*[*Re z,Im z*] by COMPLEX1:8;
         set x = Re z;
         set y = Im z;
           z^3 = [*x*x-y*y,x*y+x*y*]*[*x,y*] by A1,Th1
         .= [*x^2-y*y,x*y+x*y*]*[*x,y*] by SQUARE_1:def 3
         .= [*x^2-y^2,2*(x*y)*]*[*x,y*] by SQUARE_1:def 3
         .= [*Re[*x^2-y^2,2*(x*y)*] *Re [*x,y*]-Im[*x^2-y^2,2*(x*y)*]
            *Im [*x,y*],Re[*x^2-y^2,2*(x*y)*] *Im [*x,y*]+
            Re [*x,y*]*Im [*x^2-y^2,2*(x*y)*]*] by COMPLEX1:def 10
         .= [*(x^2-y^2) *Re [*x,y*]-Im[*x^2-y^2,2*(x*y)*]
            *Im [*x,y*],Re[*x^2-y^2,2*(x*y)*] *Im [*x,y*]+
            Re [*x,y*]*Im [*x^2-y^2,2*(x*y)*]*] by COMPLEX1:7
         .= [*(x^2-y^2) *Re [*x,y*]-(2*(x*y))
            *Im [*x,y*],Re[*x^2-y^2,2*(x*y)*] *Im [*x,y*]+
            Re [*x,y*]*Im [*x^2-y^2,2*(x*y)*]*] by COMPLEX1:7
         .= [*(x^2-y^2) *Re [*x,y*]-(2*(x*y))
            *Im [*x,y*],(x^2-y^2) *Im [*x,y*]+
            Re [*x,y*]*Im [*x^2-y^2,2*(x*y)*]*] by COMPLEX1:7
         .= [*(x^2-y^2) *Re [*x,y*]-(2*(x*y))*Im [*x,y*]
           ,(x^2-y^2) *Im [*x,y*]+Re [*x,y*]*(2*(x*y))*] by COMPLEX1:7
         .= [*(x^2-y^2) *x-(2*(x*y))*Im [*x,y*]
           ,(x^2-y^2) *Im [*x,y*]+Re [*x,y*]*(2*(x*y))*] by COMPLEX1:7
         .= [*(x^2-y^2) *x-(2*(x*y))*y
           ,(x^2-y^2) *Im [*x,y*]+Re [*x,y*]*(2*(x*y))*] by COMPLEX1:7
         .= [*(x^2-y^2) *x-(2*(x*y))*y
           ,(x^2-y^2) *y+Re [*x,y*]*(2*(x*y))*] by COMPLEX1:7
         .= [*(x^2*x-y^2*x+0*x)-2*(x*y)*y,(x^2-y^2+0)*y+x*(2*(x*y))*] by
COMPLEX1:7
         .= [*(x*x*x-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y+0*y)+x*(2*(x*y))*]
                                by SQUARE_1:def 3
         .= [*(x|^1*x*x-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y)+x*(2*(x*y))*]
                                by NEWTON:10
         .= [*(x|^ (1+1)*x-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y)+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^(2+1)-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y)+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^ 3-y^2*x)-2*(x*y)*y,(x^2*y-y*y*y)+x*(2*(x*y))*]
                                by SQUARE_1:def 3
         .= [*(x|^ 3-y^2*x)-2*(x*y)*y,(x^2*y-y|^ 1*y*y)+x*(2*(x*y))*]
                                by NEWTON:10
         .= [*(x|^ 3-y^2*x)-2*(x*y)*y,(x^2*y-y|^ (1+1)*y)+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^ 3-y^2*x)-2*(x*(y*y)),(x^2*y-y|^ (2+1))+x*(2*(x*y))*] by
NEWTON:11
         .= [*x|^ 3-y^2*x-2*(x*y^2),(x^2*y-y|^ 3)+x*(2*(x*y))*]
                                by SQUARE_1:def 3
         .= [*x|^ 3-x*y^2-2*x*y^2,x^2*y-y|^ 3+2*(x*x*y)*]
         .= [*x|^ 3-1*(x*y^2)-2*(x*y^2),x^2*y-y|^ 3+2*(x^2*y)*] by
SQUARE_1:def 3
         .= [*x|^ 3-3*x*y^2 ,-y|^ 3+3*x^2*y*];
      hence thesis by COMPLEX1:7;
end;

theorem
      (for z holds Poly_3(a,b,c,d,z) = Poly_3(a',b',c',d',z)) implies
    a = a' & b = b' & c = c' & d = d'
proof
        assume
  A1:  for z holds
         Poly_3(a,b,c,d,z) = Poly_3(a',b',c',d',z);
         then Poly_3(a,b,c,d,0c) = Poly_3(a',b',c',d',0c);
         then a*0c^3+b*0c^2+c*0c+d = Poly_3(a',b',c',d',0c) by Def5;
          then Poly_3(a',b',c',d',0c)=[*a,0*]*0c^3+b*0c^2+c*0c+d by Def1
         .=[*a,0*]*0c^3+[*b,0*]*0c^2+c*0c+d by Def1
         .=[*a,0*]*0c^3+[*b,0*]*0c^2+[*c,0*]*0c+d by Def1
         .=[*a,0*]*0c^3+[*b,0*]*0c^2+[*c,0*]*0c+[*d,0*] by Def2;
         then [*a,0*]*0c^3+[*b,0*]*0c^2+[*c,0*]*0c+[*d,0*]
         =a'*0c^3+b'*0c^2+c'*0c+d' by Def5
         .=[*a',0*]*0c^3+b'*0c^2+c'*0c+d' by Def1
         .=[*a',0*]*0c^3+[*b',0*]*0c^2+c'*0c+d' by Def1
         .=[*a',0*]*0c^3+[*b',0*]*0c^2+[*c',0*]*0c+d' by Def1
         .=[*a',0*]*0c+[*b',0*]*0c^2+[*c',0*]*0c+[*d',0*] by Def2,Th8
         .=0c+[*b',0*]*0c^2+[*c',0*]*0c+[*d',0*] by COMPLEX1:28
         .=[*b',0*]*0c^2+[*c',0*]*0c+[*d',0*] by COMPLEX1:22
         .=[*b',0*]*0c^2+0c+[*d',0*] by COMPLEX1:28
         .=[*b',0*]*0c^2+[*d',0*] by COMPLEX1:22
         .=[*b',0*]*(0c*0c)+[*d',0*] by SQUARE_1:def 3
         .=[*b',0*]*0c+[*d',0*] by COMPLEX1:28
         .=0c+[*d',0*] by COMPLEX1:28
         .=[*d',0*] by COMPLEX1:22;
         then A2: [*d',0*]= 0c+[*b,0*]*0c^2+[*c,0*]*0c+[*d,0*] by Th8,
COMPLEX1:28
         .= [*b,0*]*0c^2+[*c,0*]*0c+[*d,0*] by COMPLEX1:22
         .= [*b,0*]*0c^2+0c+ [*d,0*] by COMPLEX1:28
         .= [*b,0*]*0c^2+ [*d,0*] by COMPLEX1:22
         .= [*b,0*]*(0c*0c)+ [*d,0*] by SQUARE_1:def 3
         .= [*b,0*]*0c+ [*d,0*] by COMPLEX1:28
         .= 0c+ [*d,0*] by COMPLEX1:28
         .=[*d,0*] by COMPLEX1:22;
 then A3:   d = d' by ARYTM_0:12;
           Poly_3(a,b,c,d,1r) =  Poly_3(a',b',c',d',1r) by A1;
         then Poly_3(a',b',c',d',1r)=a*1r^3+b*1r^2+c*1r+d by Def5
         .=[*a,0*]*1r^3+b*1r^2+c*1r+d by Def1
         .=[*a,0*]*1r^3+[*b,0*]*1r^2+c*1r+d by Def1
         .=[*a,0*]*1r^3+[*b,0*]*1r^2+[*c,0*]*1r+d by Def1
         .=[*a,0*]*1r^3+[*b,0*]*1r^2+[*c,0*]*1r+[*d,0*] by Def2;
         then [*a,0*]*1r^3+[*b,0*]*1r^2+[*c,0*]*1r+[*d,0*]
         = a'*1r^3+b'*1r^2+c'*1r+d' by Def5
         .= [*a',0*]*1r^3+b'*1r^2+c'*1r+d' by Def1
         .= [*a',0*]*1r^3+[*b',0*]*1r^2+c'*1r+d' by Def1
         .= [*a',0*]*1r^3+[*b',0*]*1r^2+[*c',0*]*1r+d' by Def1
         .=[*a',0*]*1r+[*b',0*]*1r^2+[*c',0*]*1r+[*d',0*] by Def2,Th9
         .=[*a',0*]+[*b',0*]*1r^2+[*c',0*]*1r+[*d',0*] by COMPLEX1:29
         .=[*a',0*]+[*b',0*]*1r^2+[*c',0*]+[*d',0*] by COMPLEX1:29
         .=[*a',0*]+[*b',0*]*(1r*1r)+[*c',0*]+[*d',0*] by SQUARE_1:def 3
         .=[*a',0*]+[*b',0*]*1r+[*c',0*]+[*d',0*] by COMPLEX1:29
         .=[*a',0*]+[*b',0*]+[*c',0*]+[*d',0*] by COMPLEX1:29
         .=[*a'+b',0+0*]+[*c',0*]+[*d',0*] by COMPLFLD:2
         .=[*a'+b'+c',0+0*]+[*d',0*] by COMPLFLD:2
         .=[*a'+b'+c'+d',0+0*] by COMPLFLD:2;
         then [*a'+b'+c'+d',0*]
         = [*a,0*]+[*b,0*]*1r^2+[*c,0*]*1r+[*d,0*] by Th9,COMPLEX1:29
         .= [*a,0*]+[*b,0*]*1r^2+[*c,0*]+[*d,0*] by COMPLEX1:29
         .= [*a,0*]+[*b,0*]*(1r*1r)+[*c,0*]+[*d,0*] by SQUARE_1:def 3
         .= [*a,0*]+[*b,0*]*1r+[*c,0*]+[*d,0*] by COMPLEX1:29
         .= [*a,0*]+[*b,0*]+[*c,0*]+[*d,0*] by COMPLEX1:29
         .= [*a+b,0+0*]+[*c,0*]+[*d,0*] by COMPLFLD:2
         .= [*a+b+c,0+0*]+[*d,0*] by COMPLFLD:2
         .= [*a+b+c+d,0+0*] by COMPLFLD:2;
 then A4:   a+b+c+d = a'+b'+c'+d' by ARYTM_0:12;
         set r1 = a+b+c;
         set r2 = a'+b'+c';
           r1-r2 = d - d by A3,A4,XCMPLX_1:33
              .=  0;
 then A5:   r1 = r2 by XCMPLX_1:15;
           Poly_3(a,b,c,d,-1r) =  Poly_3(a',b',c',d',-1r) by A1;
         then Poly_3(a',b',c',d',-1r)=a*(-1r)^3+b*(-1r)^2+c*(-1r)+d by Def5
         .=[*a,0*]*(-1r)^3+b*(-1r)^2+c*(-1r)+d by Def1
         .=[*a,0*]*(-1r)^3+[*b,0*]*(-1r)^2+c*(-1r)+d by Def1
         .=[*a,0*]*(-1r)^3+[*b,0*]*(-1r)^2+[*c,0*]*(-1r)+d by Def1
         .=[*a,0*]*(-1r)^3+[*b,0*]*(-1r)^2+[*c,0*]*(-1r)+[*d,0*] by Def2;
         then [*a,0*]*(-1r)^3+[*b,0*]*(-1r)^2+[*c,0*]*(-1r)+[*d,0*]
         = a'*(-1r)^3+b'*(-1r)^2+c'*(-1r)+d' by Def5
         .= [*a',0*]*(-1r)^3+b'*(-1r)^2+c'*(-1r)+d' by Def1
         .= [*a',0*]*(-1r)^3+[*b',0*]*(-1r)^2+c'*(-1r)+d' by Def1
         .= [*a',0*]*(-1r)^3+[*b',0*]*(-1r)^2+[*c',0*]*(-1r)+d' by Def1
         .=[*a',0*]*(-1r)^3+[*b',0*]*(-1r)^2+[*c',0*]*(-1r)+[*d',0*] by Def2
         .=[*a',0*]*(-1r)+[*b',0*]*((-1r)*(-1r))+[*c',0*]*(-1r)+[*d',0*]
                                        by Th10,SQUARE_1:def 3
         .=-[*a',0*]+[*b',0*]+[*c',0*]*(-1)+[*d',0*] by COMPLEX1:def 7
,XCMPLX_1:180
         .=[*-Re [*a',0*],-Im [*a',0*]*]+[*b',0*]+-[*c',0*]+[*d',0*]
                               by COMPLEX1:def 11
         .=[*-a',-Im [*a',0*]*]+[*b',0*]+-[*c',0*]+[*d',0*] by COMPLEX1:7
         .=[*-a',-0*]+[*b',0*]+-[*c',0*]+[*d',0*] by COMPLEX1:7
         .=[*-a',0*]+[*b',0*]+[*-Re [*c',0*],-Im [*c',0*]*]+[*d',0*]
                                 by COMPLEX1:def 11
         .=[*-a',0*]+[*b',0*]+[*-c',-Im [*c',0*]*]+[*d',0*] by COMPLEX1:7
         .=[*-a',0*]+[*b',0*]+[*-c',-0*]+[*d',0*] by COMPLEX1:7
         .=[*-a'+b',0+0*]+[*-c',0*]+[*d',0*] by COMPLFLD:2
         .=[*-a'+b'+(-c'),0+0*]+[*d',0*] by COMPLFLD:2
         .=[*-a'+b'+(-c')+d',0+0*] by COMPLFLD:2;
         then [*-a'+b'+(-c')+d',0+0*]
         =[*a,0*]*(-1r)+[*b,0*]*((-1r)*(-1r))+[*c,0*]*(-1r)+[*d,0*]
                                             by Th10,SQUARE_1:def 3
         .=-[*a,0*]+[*b,0*]+[*c,0*]*(-1)+[*d,0*] by COMPLEX1:def 7,XCMPLX_1:180
         .=[*-Re [*a,0*],-Im [*a,0*]*]+[*b,0*]+-[*c,0*]+[*d,0*]
                               by COMPLEX1:def 11
         .=[*-a,-Im [*a,0*]*]+[*b,0*]+-[*c,0*]+[*d,0*] by COMPLEX1:7
         .=[*-a,-0*]+[*b,0*]+-[*c,0*]+[*d,0*] by COMPLEX1:7
         .=[*-a,0*]+[*b,0*]+[*-Re [*c,0*],-Im [*c,0*]*]+[*d,0*]
                                 by COMPLEX1:def 11
         .=[*-a,0*]+[*b,0*]+[*-c,-Im [*c,0*]*]+[*d,0*] by COMPLEX1:7
         .=[*-a,0*]+[*b,0*]+[*-c,-0*]+[*d,0*] by COMPLEX1:7
         .=[*-a+b,0+0*]+[*-c,0*]+[*d,0*] by COMPLFLD:2
         .=[*-a+b+(-c),0+0*]+[*d,0*] by COMPLFLD:2
         .=[*-a+b+(-c)+d,0+0*] by COMPLFLD:2;
         then A6: -a+b-c+d = -a'+b'+(-c')+d' by ARYTM_0:12;
         set s1 = -a+b-c;
         set s2 = -a'+b'-c';
 A7:   s1-s2 = d - d by A3,A6,XCMPLX_1:33
              .= 0;
         then r1+s1 =b'+0+b' by A5
          .=2*b';
          then A8: 2*b' = ( b+a+c)+(-(c+a)+b)
          .=2*b;
            r1-s1 = a'+b'+c'-(b'-a'-c') by A5,A7,XCMPLX_1:15
          .= a'+c'+(a'+c');
         then 2*(a'+c') = a+b+c-(b-a-c)
            .= 2*(a+c);
           then a'+c' = a+c by XCMPLX_1:5;
 then A9:     a-a' = c'-c by XCMPLX_1:33;
           Poly_3(a,b,c,d,[*2,0*]) =  Poly_3(a',b',c',d',[*2,0*]) by A1;
         then Poly_3(a',b',c',d',[*2,0*])
         =a*[*2,0*]^3+b*[*2,0*]^2+c*[*2,0*]+d by Def5
         .=[*a,0*]*[*2,0*]^3+b*[*2,0*]^2+c*[*2,0*]+d by Def1
         .=[*a,0*]*[*2,0*]^3+[*b,0*]*[*2,0*]^2+c*[*2,0*]+d by Def1
         .=[*a,0*]*[*2,0*]^3+[*b,0*]*[*2,0*]^2+[*c,0*]*[*2,0*]+d by Def1;
         then [*a,0*]*[*2,0*]^3+[*b,0*]*[*2,0*]^2+[*c,0*]*[*2,0*]+[*d,0*]
         = Poly_3(a',b',c',d',[*2,0*]) by Def2
         .=a'*[*2,0*]^3+b'*[*2,0*]^2+c'*[*2,0*]+d' by Def5
         .=[*a',0*]*[*2,0*]^3+b'*[*2,0*]^2+c'*[*2,0*]+d' by Def1
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*2,0*]^2+c'*[*2,0*]+d' by Def1
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*2,0*]^2+[*c',0*]*[*2,0*]+d' by Def1
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*2,0*]^2+[*c',0*]*[*2,0*]+[*d',0*]
                                      by Def2
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*([*2,0*]*[*2,0*])
                   +[*c',0*]*[*2,0*]+[*d',0*] by SQUARE_1:def 3
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*2*2,0*]
                   +[*c',0*]*[*2,0*]+[*d',0*] by Th3
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*4,0*]+[*c'*2,0*]+[*d',0*] by Th3
         .=[*a',0*]*[*2,0*]^3+[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Th3
         .=[*a',0*]*([*2,0*]^2*[*2,0*])
                                +[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Def4
         .=[*a',0*]*([*2,0*]*[*2,0*]*[*2,0*])
                                +[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by
SQUARE_1:def 3
         .=[*a',0*]*([*2*2,0*]*[*2,0*])
                                +[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Th3
         .=[*a',0*]*[*4*2,0*]+[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Th3
         .=[*a'*8,0*]+[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Th3
         .=[*a'*8+b'*4,0+0*]+[*c'*2,0*]+[*d',0*] by COMPLFLD:2
         .=[*a'*8+b'*4+c'*2,0+0*]+[*d',0*] by COMPLFLD:2
         .=[*a'*8+b'*4+c'*2+d',0+0*] by COMPLFLD:2;
        then [*a'*8+b'*4+c'*2+d',0+0*]
          =[*a,0*]*[*2,0*]^3+[*b,0*]*[*2,0*]^2+[*c*2,0*]+[*d,0*] by Th3
         .=[*a,0*]*[*2,0*]^3+[*b,0*]*([*2,0*]*[*2,0*])
                                           +[*c*2,0*]+[*d,0*] by SQUARE_1:def 3
         .=[*a,0*]*[*2,0*]^3+[*b,0*]*[*2*2,0*]+[*c*2,0*]+[*d,0*] by Th3
         .=[*a,0*]*[*2,0*]^3+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Th3
         .=[*a,0*]*([*2,0*]^2*[*2,0*])+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Def4
         .=[*a,0*]*([*2,0*]*[*2,0*]*[*2,0*])
                         +[*b*4,0*]+[*c*2,0*]+[*d,0*] by SQUARE_1:def 3
         .=[*a,0*]*([*2*2,0*]*[*2,0*])+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Th3
         .=[*a,0*]*[*4*2,0*]+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Th3
         .=[*a*8,0*]+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Th3
         .=[*a*8+b*4,0+0*]+[*c*2,0*]+[*d,0*] by COMPLFLD:2
         .=[*a*8+b*4+c*2,0+0*]+[*d,0*] by COMPLFLD:2
         .=[*a*8+b*4+c*2+d,0+0*] by COMPLFLD:2;
  then A10:   a*8+b*4+c*2+d = a'*8+b'*4+c'*2+d' by ARYTM_0:12;
         set t1=a*8+b*4+c*2;
         set t2=a'*8+b'*4+c'*2;
  A11:  t1 - t2 =d - d by A3,A10,XCMPLX_1:33
                .=0;
    (a - a')*8 = (c' -c)*2
         proof
           a*8+ (b*4 +c*2) = a'*8 + (b'*4 +c'*2) by A11,XCMPLX_1:15;
          then a*8 - a'*8 = b'*4 +c'*2 -( b*4 +c*2) by XCMPLX_1:33
                  .= b'*4 - b*4+c'*2 -c*2
                  .= (b*4 - b*4)+c'*2 -c*2 by A8,XCMPLX_1:5
                  .=  0 +c'*2 -c*2;hence thesis by XCMPLX_1:40;
         end;
          then (c' - c)*8 - (c' -c)*2 = 0 by A9;
         then (c' - c)*6 = 0;
          then c' - c = 0 or 6 = 0 by XCMPLX_1:6;
   hence thesis by A2,A8,A9,ARYTM_0:12,XCMPLX_1:5,15;
end;

theorem  Th13:
    (z+h)^3 = z^3+(3*h)*z^2+(3*h^2)*z+h^3
proof
  A1:  (z+h)^3 = (z+h)^2*(z+h) by Def4
          .=((z+h)*(z+h))*(z+h) by SQUARE_1:def 3
          .=(((z*z)*z+(z*z)*h)+(h*z)*(z+h))+(z*h+h*h)*(z+h)
          .=((z^2*z+(z*z)*h)+(h*z)*(z+h))+(z*h+h*h)*(z+h) by SQUARE_1:def 3
          .=((z^2*z+z^2*h)+(h*z)*(z+h))+(z*h+h*h)*(z+h) by SQUARE_1:def 3
          .=((z^3+z^2*h)+(h*(z*z)+z*(h*h)))+(z*h+h*h)*(z+h) by Def4
          .=((z^3+z^2*h)+(h*(z^2)+z*(h*h)))+(z*h+h*h)*(z+h) by SQUARE_1:def 3
          .=((z^3+z^2*h)+(h*(z^2)+z*(h^2)))+(z*h+h*h)*(z+h) by SQUARE_1:def 3
          .=(z^3+z^2*h+h*(z^2)+z*(h^2))+(z*h+h*h)*(z+h);
          set t = (z*h+h*h)*(z+h);
            t = ((z*h)*z+(z*h)*h)+((h*h)*z+(h*h)*h)
           .= (h*z*z+z*h*h)+((h^2)*z+(h*h)*h) by SQUARE_1:def 3
           .= (h*(z*z)+z*h*h)+((h^2)*z+h^2*h) by SQUARE_1:def 3
           .= (h*(z^2)+z*(h*h))+((h^2)*z+h^2*h) by SQUARE_1:def 3
           .= (h*(z^2)+z*(h^2))+((h^2)*z+h^2*h) by SQUARE_1:def 3
           .= (h*(z^2)+z*(h^2))+((h^2)*z+h^3) by Def4
           .= (h*(z^2)+z*(h^2))+(h^2)*z+h^3;
         then (z+h)^3 =(z^3+(3*h)*(z^2))+3*z*(h^2)+h^3 by A1
           .=(z^3+(3*h)*(z^2))+[*3,0*]*z*(h^2)+h^3 by Def1
           .=(z^3+(3*h)*(z^2))+[*3,0*]*(h^2)*z+h^3
           .=(z^3+(3*h)*(z^2))+3*(h^2)*z+h^3 by Def1;
      hence thesis;
end;

theorem
      (z*h)^3 = z^3*h^3
proof
     (z*h)^3 = (z*h)^2*(z*h) by Def4
           .= (z*h)*(z*h)*(z*h) by SQUARE_1:def 3
           .= z*(h*h)*z*(z*h)
           .= z*h^2*z*(z*h) by SQUARE_1:def 3
           .= (z*z*h^2)*h*z
           .= (z^2*h^2)*h*z by SQUARE_1:def 3
           .= z^2*(h^2*h)*z
           .= z^2*h^3*z by Def4
           .= z^2*z*h^3;
   hence thesis by Def4;
end;

theorem
      b<>0 & Poly_3(0,b,c,d,z)=0c & delta(b,c,d)>=0 implies
       z = (-c+sqrt delta(b,c,d))/(2*b) or
       z = (-c-sqrt delta(b,c,d))/(2*b) or
       z = -c/(2*b)
proof
         assume
 A1:   b<>0 & Poly_3(0,b,c,d,z) = 0c & delta(b,c,d)>=0;
         then 0c=0*(z^3)+b*z^2+c*z+d by Def5
         .=[*0,0*]*(z^3)+b*z^2+c*z+d by Def1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+c*z+d by Def1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+d by Def1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+[*d,0*] by Def2;
         then b*z^2+[*c,0*]*z+[*d,0*] = 0c by Def1,Lm1,COMPLEX1:def 6;
         then b*z^2+c*z+[*d,0*] = 0c by Def1;
         then b*z^2+c*z+d = 0c by Def2;
         then Poly2(b,c,d,z)=0c by POLYEQ_1:def 2;
         hence thesis by A1,Th4,COMPLEX1:14;
end;

theorem
      b<>0 & Poly_3(0,b,c,d,z)=0c & delta(b,c,d)<0 implies
      z = [*-c/(2*b),sqrt (-delta(b,c,d))/(2*b)*] or
      z = [*-c/(2*b),-sqrt (-delta(b,c,d))/(2*b)*]
proof
         assume
 A1:   b<>0 & Poly_3(0,b,c,d,z) = 0c & delta(b,c,d)<0;
         then 0c=0*(z^3)+b*z^2+c*z+d by Def5
         .=[*0,0*]*(z^3)+b*z^2+c*z+d by Def1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+c*z+d by Def1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+d by Def1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+[*d,0*] by Def2;
         then b*z^2+[*c,0*]*z+[*d,0*] = 0c by Def1,Lm1,COMPLEX1:def 6;
         then b*z^2+c*z+[*d,0*] = 0c by Def1;
         then b*z^2+c*z+d = 0c by Def2;
         then Poly2(b,c,d,z)=0c by POLYEQ_1:def 2;
         hence z = [*-c/(2*b),sqrt (-delta(b,c,d))/(2*b)*] or
             z = [*-c/(2*b),-sqrt (-delta(b,c,d))/(2*b)*] by A1,Th5;
end;

theorem
      a<>0 & Poly_3(a,0,c,0,z)=0 & 4*a*c <= 0 implies
       z = (sqrt -4*a*c)/(2*a) or
       z = (-sqrt -4*a*c)/(2*a) or
       z = 0
proof
         assume
 A1:   a<>0 & Poly_3(a,0,c,0,z) = 0 & 4*a*c <= 0;
         then 0=a*(z^3)+0*z^2+c*z+0 by Def5
         .=[*a,0*]*(z^3)+0*z^2+c*z+0 by Def1
         .=[*a,0*]*(z^3)+0*z^2+[*c,0*]*z+0 by Def1;
         then [*a,0*]*(z^2*z)+[*c,0*]*z = 0 by Def4;
         then ([*a,0*]*z^2+[*c,0*])*z = 0;
         then [*a,0*]*z^2+[*c,0*] = 0 or z = 0 by XCMPLX_1:6;
         then (a*z^2+0*z)+[*c,0*]=0 or z = 0 by Def1;
         then (a*z^2+0*z) + c = 0 or z = 0 by Def2;
 then A2:   Poly2(a,0,c,z) = 0 or z = 0 by POLYEQ_1:def 2;
           -4*a*c >= -0 by A1,REAL_1:50;
         then 0^2-4*a*c >= 0 by SQUARE_1:60,XCMPLX_1:150;
         then delta(a,0,c) >= 0 by QUIN_1:def 1;
            then z = (-0+sqrt delta(a,0,c))/(2*a)
            or z = (-0-sqrt delta(a,0,c))/(2*a)
            or z= 0
            or z = -0/(2*a) by A1,A2,Th4;
            then z = (sqrt delta(a,0,c))/(2*a)
            or z = (-sqrt delta(a,0,c))/(2*a)
            or z = 0;
            then z = (sqrt (0-4*a*c))/(2*a)
            or z = (-sqrt (0-4*a*c))/(2*a)
            or z= 0 by QUIN_1:def 1,SQUARE_1:60;
    hence thesis;
end;

theorem
      a<>0 & Poly_3(a,b,c,0,z)=0 & delta(a,b,c)>=0 implies
       z = (-b+sqrt delta(a,b,c))/(2*a) or
       z = (-b-sqrt delta(a,b,c))/(2*a) or
       z = -b/(2*a) or
       z = 0
proof
         assume
 A1:   a<>0 & Poly_3(a,b,c,0,z) = 0 & delta(a,b,c)>=0;
         then 0=a*(z^3)+b*z^2+c*z+0 by Def5
         .=[*a,0*]*(z^3)+b*z^2+c*z+0 by Def1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+c*z+0 by Def1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+0 by Def1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+[*0,0*] by Def2;
          then [*a,0*]*(z^2*z)+[*b,0*]*z^2+[*c,0*]*z = 0 by Def4,Lm1,
COMPLEX1:def 6;
         then [*a,0*]*(z^2*z)+[*b,0*]*(z*z)+[*c,0*]*z = 0 by SQUARE_1:def 3;
         then ([*a,0*]*z^2+[*b,0*]*z+[*c,0*])*z = 0;
         then [*a,0*]*z^2+[*b,0*]*z+[*c,0*] = 0 or z = 0
           by XCMPLX_1:6;
         then a*z^2+[*b,0*]*z+[*c,0*] = 0 or z = 0 by Def1;
         then a*z^2+b*z+[*c,0*] = 0 or z = 0 by Def1;
         then a*z^2+b*z+c = 0 or z = 0 by Def2;
         then Poly2(a,b,c,z) = 0 or z = 0 by POLYEQ_1:def 2;
     hence thesis by A1,Th4;
end;

theorem
      a<>0 & Poly_3(a,b,c,0,z)=0c & delta(a,b,c)<0 implies
      z= [*-b/(2*a),sqrt (-delta(a,b,c))/(2*a)*] or
      z= [*-b/(2*a),-sqrt (-delta(a,b,c))/(2*a)*] or
      z = 0c
proof
         assume
 A1:   a<>0 & Poly_3(a,b,c,0,z) = 0c & delta(a,b,c)<0;
         then 0c=a*(z^3)+b*z^2+c*z+0 by Def5
         .=[*a,0*]*(z^3)+b*z^2+c*z+0 by Def1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+c*z+0 by Def1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+0 by Def1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+[*0,0*] by Def2;
         then [*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+0 = 0c by ARYTM_0:def 7;
         then [*a,0*]*(z^2*z)+[*b,0*]*z^2+[*c,0*]*z = 0c by Def4;
         then [*a,0*]*z^2*z+[*b,0*]*(z*z)+[*c,0*]*z = 0c by SQUARE_1:def 3;
         then ([*a,0*]*z^2+[*b,0*]*z)*z+[*c,0*]*z = 0c;
         then ([*a,0*]*z^2+[*b,0*]*z+[*c,0*])*z = 0c by XCMPLX_1:8;
         then [*a,0*]*z^2+[*b,0*]*z+[*c,0*] = 0c or z = 0c
           by COMPLEX1:def 6,XCMPLX_1:6;
         then a*z^2+[*b,0*]*z+[*c,0*] = 0c or z = 0c by Def1;
         then a*z^2+b*z+[*c,0*] = 0c or z = 0c by Def1;
         then a*z^2+b*z+c = 0c or z = 0c by Def2;
         then Poly2(a,b,c,z) = 0c or z = 0c by POLYEQ_1:def 2;
     hence thesis by A1,Th5;
end;

theorem Th20:
    a>=0 & y^2 = a implies y = sqrt a or y = -sqrt a
proof
         assume
 A1:   a>=0 & y^2 = a;
         then 1*y^2 +0*y+(-a) = a-a;
 then A2:   Poly2(1,0,-a,y) =0 by POLYEQ_1:def 2;
           delta(1,0,-a) = 0^2 - 4*1*(-a) by QUIN_1:def 1;
         then delta(1,0,-a) = -4*(-a) by SQUARE_1:60,XCMPLX_1:150;
 then A3:   delta(1,0,-a) = 4*a;
         then delta(1,0,-a) >= 0 by A1,REAL_2:121;
         then y=(-0+sqrt delta(1,0,-a))/(2*1) or
         y=(-0-sqrt delta(1,0,-a))/(2*1) by A2,POLYEQ_1:5;
         then y=(sqrt (4*a))/2 or
         y=(-sqrt (4*a))/2 by A3;
         then y= sqrt a * 2 /2 or
         y=(-(2 * sqrt a))/2 by A1,SQUARE_1:85,97;
    hence y= sqrt a or y=-sqrt a;
end;

theorem
      a<>0 & Poly_3(a,0,c,d,z)=0c & Im z = 0
       implies
       (for u,v st Re z = u+v & 3*v*u+c/a=0 holds
          z = 3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
             +3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
       or z = 3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
             +3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
       or z = 3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
             +3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3)) )
proof
       assume
 A1:   a <> 0;
       assume
 A2:   Poly_3(a,0,c,d,z) = 0c & Im z = 0;
        let u,v;
       assume
 A3:   Re z=u+v & 3*v*u+c/a=0;
         set x=Re z;
         set y=Im z;
         set p=c/a;
 A4:   z = [*Re z,Im z*] by COMPLEX1:8
           .= x by A2,ARYTM_0:def 7;
           0 =a*(z^3)+0*z^2+c*z+d by A2,Def5,COMPLEX1:def 6
         .=[*a,0*]*(z^3)+0*z^2+c*z+d by Def1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+c*z+d by Def1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+[*c,0*]*z+d by Def1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+[*c,0*]*z+[*d,0*] by Def2;
         then 0 = [*a,0*]*([*Re z^3,Im z^3*])+[*0,0*]*z^2+[*c,0*]*z+[*d,0*]
                        by COMPLEX1:8
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +[*0,0*]*z^2+[*c,0*]*z+[*d,0*] by Th11
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +0c*z^2+[*c,0*]*z+[*d,0*] by ARYTM_0:def 7,COMPLEX1:def 6
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +0c+[*c,0*]*z+[*d,0*] by COMPLEX1:28
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
            +[*c,0*]*z+[*d,0*] by COMPLEX1:22
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,-(Im z)|^ 3+3*(Re z)^2
            *Im z*]+[*c,0*]*z+[*d,0*] by Th11
         .= [*a,0*]*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:8
         .=[*Re [*a,0*] *Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]-Im [*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:def 10
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0*(-y|^ 3+3*x^2*y),
          Re[*a,0*]*(-y|^ 3+3*x^2*y)+(x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*0 *]+[*c,0*]*[*Re z,Im z*]+[*d,0*]
                              by COMPLEX1:7
         .= [*a*(x|^ 3 - 3*x*y^2),a*(-y|^ 3+3*x^2*y)*]
            +[*c*Re z-0*Im z,c*Im z+Re z*0*]+[*d,0*] by Th1
         .= [*a*(x|^ 3 - 3*x*y^2)+c*x,a*(-y|^ 3+3*x^2*y)
            +c*y*]+[*d,0*] by COMPLFLD:2
         .= [*a*(x|^ 3 - 3*x*0)+c*x+d,a*(-0|^ 3+0)
            +0+0*] by A2,COMPLFLD:2,SQUARE_1:60
         .= [*a*(x|^ 3 - 0)+c*x+d,a*(-0)*] by NEWTON:16
         .= [*a*x|^ 3 +c*x+d,0*];
            then a"*(a*x|^ 3+c*x+d) = a"*0 by ARYTM_0:def 7;
            then a"*(x|^ 3*a)+a"*(c*x)+a"*d = 0 by XCMPLX_1:9;
            then x|^ 3*(a*a")+a"*c*x+a"*d = 0;
            then x|^ 3*(a/a)+a"*c*x+a"*d = 0 by XCMPLX_0:def 9;
            then x|^ 3+c*a"*x+d*a" = 0
  by A1,XCMPLX_1:89;
            then x|^ 3+(c/a)*x+d*a" = 0 by XCMPLX_0:def 9;
            then 1*x|^ 3+0*x^2+(c/a)*x+d/a = 0 by XCMPLX_0:def 9;
 then A5:      Poly3(1,0,c/a,d/a,x) = 0 by POLYEQ_1:def 4;
 A6:   -(d/a)/2 = -1/2*(d/a)
                 .= -d/(a*2) by XCMPLX_1:104;
 A7:   (d/a)^2/4=d^2/a^2/4 by SQUARE_1:69
                 .=1/4*(d^2/a^2)
                 .=d^2/(a^2*4) by XCMPLX_1:104;
           (c/a)/3 =1/3*(c/a)
                .=c/(a*3) by XCMPLX_1:104;
      hence thesis by A3,A4,A5,A6,A7,POLYEQ_1:19;
end;

theorem
      a<>0 & Poly_3(a,0,c,d,z)=0c & Im z <> 0 implies
    (for u,v st  Re z = u+v & 3*v*u+c/(4*a)=0 & c/a>=0 holds
          z=[*3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)),
             sqrt(3*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             + 3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))^2+c/a)*]
       or z=[*3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)),
             -sqrt(3*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))^2+c/a)*]
       or z=[*2*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))),
             sqrt(3*(2*(3-root(d/(16*a)+sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*]
       or z=[*2*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))),
             -sqrt(3*(2*(3-root(d/(16*a)+sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*]
       or z=[*2*(3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))),
             sqrt(3*(2*(3-root(d/(16*a)-sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*]
       or z=[*2*(3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))),
             -sqrt(3*(2*(3-root(d/(16*a)-sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*]
          )
proof
        assume
 A1:   a <> 0;
        assume
 A2:   Poly_3(a,0,c,d,z) = 0c  & Im z <> 0;
        let u,v;
        assume
 A3:   Re z=u+v & 3*v*u+c/(4*a)=0 &  c/a>=0;
         set x=Re z;
         set y=Im z;
         set p=c/(4*a);
 A4:   z = [*x,y*] by COMPLEX1:8;
        0c=a*(z^3)+0*z^2+c*z+d by A2,Def5
         .=[*a,0*]*(z^3)+0*z^2+c*z+d by Def1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+c*z+d by Def1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+[*c,0*]*z+d by Def1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+[*c,0*]*z+[*d,0*] by Def2;
 then A5:   0 = [*a,0*]*([*Re z^3,Im z^3*])+[*0,0*]*z^2+[*c,0*]*z+[*d,0*]
                        by COMPLEX1:8,def 6
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +[*0,0*]*z^2+[*c,0*]*z+[*d,0*] by Th11
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +0c+[*c,0*]*z+[*d,0*] by Lm1,COMPLEX1:28
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
            +[*c,0*]*z+[*d,0*] by COMPLEX1:22
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,-(Im z)|^ 3+3*(Re z)^2
            *Im z*]+[*c,0*]*z+[*d,0*] by Th11
         .= [*a,0*]*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:8
         .=[*Re [*a,0*] *Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]-Im [*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:def 10
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0*(-y|^ 3+3*x^2*y),
          Re[*a,0*]*(-y|^ 3+3*x^2*y)+(x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*0 *]+[*c,0*]*[*Re z,Im z*]+[*d,0*]
                              by COMPLEX1:7
         .= [*a*(x|^ 3 - 3*x*y^2),a*(-y|^ 3+3*x^2*y)*]
            +[*c*Re z-0*Im z,c*Im z+Re z*0*]+[*d,0*] by Th1
         .= [*a*(x|^ 3 - 3*x*y^2)+c*x,a*(-y|^ 3+3*x^2*y)
            +c*y*]+[*d,0*] by COMPLFLD:2
         .= [*a*(x|^ 3 - 3*x*y^2)+c*x+d,a*(-y|^ 3+3*x^2*y)
            +c*y+0*] by COMPLFLD:2;
then A6:        a*(-y|^ 3+3*x^2*y) +c*y+0 = 0 by ARYTM_0:26;
            a*(-y|^(2+1)+3*x^2*y)+c*y = 0 by A5,ARYTM_0:26;
          then a*(-y|^2*y+3*x^2*y)+c*y = 0 by NEWTON:11;
          then a*((-y|^2)*y+(3*x^2)*y+0*y)+c*y = 0;
          then a*((-y|^2+3*x^2+0)*y)+c*y = 0 by XCMPLX_1:9;
          then (a*(-y|^2+3*x^2+0))*y+c*y+0*y = 0;
          then (a*(-y|^2+3*x^2)+c+0)*y = 0 by XCMPLX_1:9;
            then (a*(y|^2))+(-a*(y|^2)+(a*(3*x^2)+c))=(a*(y|^2))+0
                      by A2,XCMPLX_1:6;
            then a*(y|^2)+-a*(y|^2)+(a*(3*x^2)+c) = a*(y|^2)
                           by XCMPLX_1:1;
            then y|^(1+1) = (a*(3*x^2)+c)/a
  by A1,XCMPLX_1:90;
            then y|^1*y = (a*(3*x^2)+c)/a by NEWTON:11;
            then y*y = (a*(3*x^2)+c)/a by NEWTON:10;
            then y^2 = ((3*x^2)*a+c+0)/a by SQUARE_1:def 3;
            then y^2 = (3*x^2)*a/a+c/a+0/a by XCMPLX_1:64;
 then A7:      y^2 = 3*x^2+c/a+0*a" by A1,XCMPLX_1:90;
            then a*(x|^ 3 - 3*x*(3*x^2+c/a)+0)+c*x+d = 0
 by A5,A6,ARYTM_0:def 7;
            then a*x|^ 3 - a*(3*x*(3*x^2+c/a+0))+a*0+c*x+d = 0 by XCMPLX_1:44;
            then 0 =a*x|^ 3 - a*((3*x)*(3*x^2)+(3*x)*(c/a)+(3*x)*0)+c*x+d
                                 by XCMPLX_1:9
            .=a*x|^ 3 - a*(9*(x*x^2)+(3*x)*(c/a))+c*x+d
            .=a*x|^ 3 - a*(9*(x*(x*x))+(3*x)*(c/a))+c*x+d by SQUARE_1:def 3
            .=a*x|^ 3 - a*(9*(x|^1*x*x)+(3*x)*(c/a))+c*x+d by NEWTON:10
