theorem
    b <> 0 & (for z holds Poly2(0,b,c,z) = 0c) implies z = -c/b
proof
        assume
 A100:   b <> 0;
        assume
 A101:  (for z holds Poly2(0,b,c,z) = 0c);
        now let z1;
        Poly2(0,b,c,z1) = 0c by A101;then
        0*z1^2+b*z1+c = 0c by POLYEQ_1:def 2;then
         [*0,0*]*z1^2+b*z1+c = 0c by Lem1;then
         [*0,0*]*z1^2+[*b,0*]*z1+c = 0c by Lem1;then
          0c+[*b,0*]*z1+[*c,0*] = 0c by Lem2,XXX,COMPLEX1:def 6;then
          [*b,0*]*z1+[*c,0*] = 0c by COMPLEX1:22;then
          [*b,0*]*[*Re z1,Im z1*]+[*c,0*] = 0c by COMPLEX1:8;then
          [*b*Re z1-0*Im z1,b*Im z1+0*Re z1*]+[*c,0*] = 0c by Th5;then
          Re [*b*Re z1-0+c,b*Im z1+0*] = Re [*0,0*] &
          Im [*b*Re z1-0+c,b*Im z1+0*]= Im [*0,0*]
           by XXX,COMPLFLD:2;then
           b*Re z1-0+c = Re [*0,0*] & b*Im z1= Im [*0,0*] by COMPLEX1:7;then
          b*(Re z1)-0+c = 0 & b*Im z1 = 0 by COMPLEX1:7;then
          b*(Re z1)+(c-c) =0-c & Im z1 = 0 by XCMPLX_1:29,A100,XCMPLX_1:6;then
          b*(Re z1)+(c-c) =-c  & Im z1 = 0 by XCMPLX_1:150;then
          b*(Re z1)+0 =-c  & Im z1 = 0 by XCMPLX_1:14;then
          Re z1 =(-c)/b & Im z1 = 0 by A100,XCMPLX_1:90;then
          Re z1 =-c/b & Im z1 = 0 by XCMPLX_1:188;
       then z1 = [*-c/b,0*] by COMPLEX1:8;
       hence z1 = -c/b by ARYTM_0:def 7;
    end;hence thesis;
end;

