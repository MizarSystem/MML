environ

 vocabulary COMPLEX1, ARYTM_3, ARYTM_1, POLYEQ_1, SQUARE_1, FUNCT_3, POLYEQ_3,
      RELAT_1, POWER, GROUP_1, PREPOWER, FUNCT_1, SIN_COS, HAHNBAN1, COMPTRIG;
 notation SUBSET_1, NUMBERS, XBOOLE_0, ORDINAL1, XCMPLX_0, XREAL_0, REAL_1,
      COMPLEX1, POLYEQ_1, SQUARE_1, COMSEQ_1, TARSKI, ARYTM_0, QUIN_1, POWER,
      GROUP_1, NEWTON, INT_1, NAT_1, COMSEQ_3, COMPTRIG, SIN_COS, COMPLFLD,
      COMPLSP1, STRUCT_0, RLVECT_1, HAHNBAN1, PREPOWER, BINARITH, VECTSP_1,
      SEQ_1, COMPLEX2;
 constructors COMPLEX1, REAL_1, POLYEQ_1, QUIN_1, NEWTON, GROUP_1, COMSEQ_1,
      ARYTM_0, SQUARE_1, SUBSET_1, POWER, INT_1, NAT_1, COMSEQ_3, COMPLFLD,
      COMPLSP1, RLVECT_1, HAHNBAN1, COMPTRIG, SIN_COS, PREPOWER, BINARITH,
      VECTSP_1, NUMBERS, SEQ_1, COMPLEX2, ARYTM_3, FUNCT_4;
 clusters ARYTM_3, REAL_1, NEWTON, SQUARE_1, XREAL_0, RELAT_1, QUIN_1, INT_1,
      SIN_COS, COMPLFLD, COMPLEX1, BINARITH, VECTSP_1, XCMPLX_0, NUMBERS,
      POLYEQ_1;
 requirements ARITHM, SUBSET, REAL, BOOLE, NUMERALS;
 theorems COMPLEX1, POLYEQ_1, REAL_1, REAL_2, AXIOMS, SQUARE_1, QUIN_1,
      COMPLFLD, NEWTON, COMSEQ_3, COMPTRIG, HAHNBAN1, NAT_1, BINARITH, POWER,
      SIN_COS, SIN_COS2, PREPOWER, RLVECT_1, CARD_1, COMPLEX2, XCMPLX_0,
      XCMPLX_1, ARYTM_0;
 schemes NAT_1, COMPTRIG;

begin

XXX: 0c = [*0,0*] by ARYTM_0:def 7,COMPLEX1:def 6;

 reserve a,b,c,d,e,a',b',c',d',x,y,x1,y1,x2,y2,u,v for Real,
         s,t,h,z,z1,z2,z3,z4,s1,s2,s3,p,q for Element of COMPLEX,
         m,n for Nat;

definition let a be Real,z;
  redefine func a*z -> Element of COMPLEX equals
     :Lem1:   [*a,0*]*z;
  compatibility by ARYTM_0:def 7;
  correctness by XCMPLX_0:def 2;
  redefine func a + z -> Element of COMPLEX equals :Lem2:
    z+[*a,0*];
  compatibility by ARYTM_0:def 7;
  correctness by XCMPLX_0:def 2;
end;

definition let z;
  redefine func z^2 -> Element of COMPLEX equals
    :Def4:  [*(Re z)^2-(Im z)^2,2*(Re z*Im z)*];
  compatibility
  proof
    z^2 = z * z by SQUARE_1:def 3
       .= [* Re z * Re z - Im z * Im z, Re z * Im z + Re z * Im z *]
            by COMPLEX1:def 10
       .= [* (Re z)^2 - Im z * Im z, Re z * Im z + Re z * Im z *]
            by SQUARE_1:def 3
       .= [* (Re z)^2 - (Im z)^2, Re z * Im z + Re z * Im z *]
            by SQUARE_1:def 3
       .= [*(Re z)^2-(Im z)^2,2*(Re z*Im z)*] by XCMPLX_1:11;
    hence thesis;
  end;
  correctness by XCMPLX_0:def 2;
end;

definition let a,b,c,z;
  redefine func Poly2 (a,b,c,z) -> Element of COMPLEX;
  coherence by XCMPLX_0:def 2;
end;

Th4: z^2 = z*z by SQUARE_1:def 3;

theorem Th5:
    [*a,c*]*[*b,d*]=[*a*b-c*d,a*d+b*c*]
 proof
        [*a,c*]*[*b,d*]=[* Re [*a,c*]*Re [*b,d*]
               -Im [*a,c*]*Im [*b,d*],Re [*a,c*]*Im [*b,d*]
               +Re [*b,d*]*Im [*a,c*] *] by COMPLEX1:def 10
            .=[* a*Re [*b,d*]-Im [*a,c*]*Im [*b,d*],
              Re [*a,c*]*Im [*b,d*]+Re [*b,d*]*Im [*a,c*] *]
                         by COMPLEX1:7
            .=[* a*b-Im [*a,c*]*Im [*b,d*],
              Re [*a,c*]*Im [*b,d*]+Re [*b,d*]*Im [*a,c*] *]
                         by COMPLEX1:7
            .=[* a*b-c*Im [*b,d*],
              Re [*a,c*]*Im [*b,d*]+Re [*b,d*]*Im [*a,c*] *]
                         by COMPLEX1:7
            .=[* a*b-c*d,Re [*a,c*]*Im [*b,d*]
               +Re [*b,d*]*Im [*a,c*] *] by COMPLEX1:7
            .=[* a*b-c*d,a*Im [*b,d*]+Re [*b,d*]*Im [*a,c*] *]
                           by COMPLEX1:7
            .=[* a*b-c*d,a*d+Re [*b,d*]*Im [*a,c*] *]
                           by COMPLEX1:7
            .=[* a*b-c*d,a*d+b*Im [*a,c*] *] by COMPLEX1:7
            .=[* a*b-c*d,a*d+b*c *] by COMPLEX1:7;
       hence [*a,c*]*[*b,d*]=[*a*b-c*d,a*d+b*c*];
end;

Th20:
    z+z = 2*z by XCMPLX_1:11;

Tha:(z+h)^2 = z^2+2*z*h+h^2 by SQUARE_1:63;

Thb:(z*h)^2 = z^2*h^2 by SQUARE_1:68;

Thc:(-z)^2 = z^2 by SQUARE_1:61;

theorem Th6:
  z=[*x,y*] implies z^2=[*x^2-y^2,2*x*y*]
proof
  assume A1:z=[*x,y*];
   z^2=[*(Re z)^2-(Im z)^2,2*(Re z*Im z)*] by Def4
     .=[*x^2-(Im [*x,y*])^2,2*(Re [*x,y*]*Im [*x,y*])*]
                           by COMPLEX1:7,A1
     .=[*x^2-y^2,2*(Re [*x,y*]*Im [*x,y*])*] by COMPLEX1:7
     .=[*x^2-y^2,2*(x*Im [*x,y*])*] by COMPLEX1:7
     .=[*x^2-y^2,2*(x*y)*] by COMPLEX1:7
     .=[*x^2-y^2,2*x*y*] by XCMPLX_1:4;
  hence z^2=[*x^2-y^2,2*x*y*];
end;

theorem Th7:
  for a,b holds [*a,0*]*[*b,0*]=[*a*b,0*]
proof
  let a,b;
  [*a,0*]*[*b,0*] =[*a*b-0*0,a*0+0*b*] by Th5
           .=[*a*b,0*];
  hence [*a,0*]*[*b,0*]=[*a*b,0*];
end;

theorem Th9:
    a <> 0 & delta(a,b,c) >=0 & Poly2(a,b,c,z) = 0 implies
         z= (-b+sqrt delta(a,b,c))/(2*a) or
         z= (-b-sqrt delta(a,b,c))/(2*a) or
         z= -b/(2*a)
proof
      assume
  A100:  a <> 0 & delta(a,b,c) >= 0;
         assume
  A101:  Poly2(a,b,c,z) = 0;
         set x=Re z;
         set y=Im z;
         a*z^2+b*z+c = 0 by A101,POLYEQ_1:def 2;then
         [*a,0*]*z^2+b*z+c = 0 by Lem1;then
         [*a,0*]*z^2+[*b,0*]*z+c = 0 by Lem1;then
         [*a,0*]*z^2+[*b,0*]*z+[*c,0*] = 0 by Lem2;then
         [*a,0*]*([*x^2-y^2,2*(x*y)*])+[*b,0*]*z+[*c,0*] = 0
                            by Def4;then
         [*a,0*]*[*x^2-y^2,2*x*y*]+[*b,0*]*z+[*c,0*] = 0
                        by XCMPLX_1:4;then
         0=[* Re [*a,0*]*Re [*x^2-y^2,2*x*y*]-Im [*a,0*]*
         Im [*x^2-y^2,2*x*y*],Re [*a,0*]*Im [*x^2-y^2,2*x*y*]
         +Re [*x^2-y^2,2*x*y*]*Im [*a,0*]  *]
           +[*b,0*]*z+[*c,0*] by COMPLEX1:def 10
         .=[*a*Re [*x^2-y^2,2*x*y*]-Im [*a,0*]*
         Im [*x^2-y^2,2*x*y*],Re [*a,0*]*Im [*x^2-y^2,2*x*y*]
         +Re [*x^2-y^2,2*x*y*]*Im [*a,0*]  *]
           +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-Im [*a,0*]*
         Im [*x^2-y^2,2*x*y*],Re [*a,0*]*Im [*x^2-y^2,2*x*y*]
         +Re [*x^2-y^2,2*x*y*]*Im [*a,0*]  *]
           +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0*Im [*x^2-y^2,2*x*y*]
         ,Re [*a,0*]*Im [*x^2-y^2,2*x*y*]+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]  *] +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0,Re [*a,0*]*(2*x*y)+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*]  *] +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0,a*(2*x*y)+Re [*x^2-y^2,2*x*y*]
         *Im [*a,0*] *] +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)
         *Im [*a,0*] *] +[*b,0*]*z+[*c,0*] by COMPLEX1:7
         .=[*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)
         *0*] +[*b,0*]*z+[*c,0*] by COMPLEX1:7;then
         [*a*(x^2-y^2)-0*(2*x*y),0+a*(2*x*y)*]
           +[*b,0*]*[*x,y*]+[*c,0*] = 0 by COMPLEX1:8;then
         [*a*(x^2-y^2)-0,0+a*(2*x*y)*]
           +[*b*x-0*y,x*0+b*y*]+[*c,0*] = 0 by Th5;then
         [*a*(x^2-y^2)+b*x,a*(2*x*y)+b*y*]+[*c,0*] = 0
                      by COMPLFLD:2;then
         [*a*(x^2-y^2)+b*x+c,a*(2*x*y)+b*y+0*] = 0 by COMPLFLD:2
                   .= [*0,0*] by ARYTM_0:def 7; then
         a*(x^2-y^2)+b*x+c = 0 & a*(2*x*y)+b*y = 0 by ARYTM_0:12; then
         a*(x^2-y^2)+b*x+c = 0 & a*(2*x)*y+b*y = 0 by XCMPLX_1:4; then
         a*(x^2-y^2)+b*x+c = 0 & (2*a*x)*y+b*y+0*y = 0 by XCMPLX_1:4; then
  A103:  a*(x^2-y^2)+b*x+c = 0 & (2*a*x+b+0)*y = 0 by XCMPLX_1:9; then
  A104:  (2*a*x+b + 0 = 0 or y = 0) by XCMPLX_1:6;
         per cases by A104;
          suppose
  A105:      y = 0;then
  A108:      Poly2(a,b,c,x)=0 by A103,POLYEQ_1:def 2,SQUARE_1:60;
             Re z=(-b+sqrt delta(a,b,c))/(2*a) & Im z = 0 or
             Re z= (-b-sqrt delta(a,b,c))/(2*a) & Im z = 0
           by POLYEQ_1:5,A108,A100,A105; then
             z=[*(-b+sqrt delta(a,b,c))/(2*a),0*] or
             z= [*(-b-sqrt delta(a,b,c))/(2*a),0*] by COMPLEX1:8;
         hence thesis by ARYTM_0:def 7;
         end;
         suppose 2*a*x+b = 0;then
             2*a*x+(b -b)= 0-b by XCMPLX_1:29;then
             (2*a)*x+0= 0-b by XCMPLX_1:14;then
  A109:      (2*a)*x= -b by XCMPLX_1:150;
             a"<>0 by A100,XCMPLX_1:203;then
             2/a" <> 0 by XCMPLX_1:50;then
             2/(1/a) <> 0 by XCMPLX_1:217;then
             (2*a)/1 <> 0 by XCMPLX_1:78;then
             x= (-b)/(2*a) by XCMPLX_1:90,A109;then
  A111:      x = -b/(2*a) by XCMPLX_1:188;then
             a*((b/(2*a))^2-y^2)+b*(-b/(2*a))+c = 0 by SQUARE_1:61,A103;then
             a*((b/(2*a))^2-y^2)+(b*(-b/(2*a))+c)-(b*(-b/(2*a))+c)
                           = 0-(b*(-b/(2*a))+c) by XCMPLX_1:1;then
             a*((b/(2*a))^2-y^2)+((b*(-b/(2*a))+c)-(b*(-b/(2*a))+c))
                          = 0-(b*(-b/(2*a))+c) by XCMPLX_1:29;then
             a*((b/(2*a))^2-y^2)+0 = 0-(b*(-b/(2*a))+c) by XCMPLX_1:14;then
             a*((b/(2*a))^2-y^2) = -(b*(-b/(2*a))+c) by XCMPLX_1:150;then
             (b/(2*a))^2-y^2 = (-(b*(-b/(2*a))+c))/a-0
   by XCMPLX_1:90,A100;then
             (b/(2*a))^2- (-(b*(-b/(2*a))+c))/a= y^2-0 by XCMPLX_1:24;then
             y^2 = (b/(2*a))^2- (-(b*((-1)*(b/(2*a)))+c))/a
               by XCMPLX_1:180;then
             y^2 = (b/(2*a))^2- (-((-(-b))*(-1)*(b/(2*a))+c))/a
                                     by XCMPLX_1:4;then
             y^2 = (b/(2*a))^2- (-((-b)*(b/(2*a))+c))/a
               by XCMPLX_1:180;then
             y^2 = (b/(2*a))^2- (-(-b*(b/(2*a))+c))/a by XCMPLX_1:175;then
             y^2 = (b/(2*a))^2- (-(-(b*b)/(2*a)+c))/a by XCMPLX_1:75;then
            y^2 = (b/(2*a))^2- (-((-(b^2/(2*a)))+c))/a by SQUARE_1:def 3;then
             y^2 = (b/(2*a))^2- (-c-(-(b^2/(2*a))))/a by XCMPLX_1:161;then
             y^2 = (b/(2*a))^2- (-c+b^2/(2*a)+0)/a by XCMPLX_1:151;then
             y^2 = (b/(2*a))^2- ((-c)/a+(b^2/(2*a))/a+0/a)
                                by XCMPLX_1:64;then
             y^2 = (b/(2*a))^2- ((-c)*a"+(b^2/(2*a))/a) by XCMPLX_0:def 9;then
             y^2 = (b/(2*a))^2- ((-c)*a"+(b^2/(2*a))*a") by XCMPLX_0:def 9;then
             y^2 = (b/(2*a))^2- (-c*a"+(b^2/(2*a))*a") by XCMPLX_1:175;then
             y^2 = (b/(2*a))^2- (-c*a")-((b^2/(2*a))*a") by XCMPLX_1:36;then
             y^2 = (b/(2*a))^2+c*a"-((b^2/(2*a))*a") by XCMPLX_1:151;then
  A112:      y^2 *((2*a)^2)=(b^2/(2*a)^2+c*a"-((b^2/(2*a))*a"))*((2*a)^2)
                                by SQUARE_1:69
              .=(b^2/(2*a)^2)*(2*a)^2+(c*a")*(2*a)^2
                             -((b^2/(2*a))*a")*(2*a)^2 by XCMPLX_1:43
              .=(b^2*((2*a)^2)")*(2*a)^2+(c*a")*(2*a)^2
                          -((b^2/(2*a))*a")*(2*a)^2 by XCMPLX_0:def 9
              .=(b^2*((2*a)^2)")*(2*a)^2+(c*a")*(2*a)^2
                          -((b^2*(2*a)")*a")*(2*a)^2 by XCMPLX_0:def 9
              .=b^2/((2*a)^2)*((2*a)^2)+(c*a")*(2*a)^2
                          -(b^2*(2*a)"*a")*(2*a)^2 by XCMPLX_0:def 9;
            a"<>0 by A100,XCMPLX_1:203;then
            2/a" <> 0 by XCMPLX_1:50;then
            2/(1/a) <> 0 by XCMPLX_1:217;then
  A112a:    (2*a)/1 <> 0 by XCMPLX_1:78;then
  A113:     (2*a)^2 > 0 by SQUARE_1:74;then
  A114:     y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2
                          -(b^2*(2*a)"*a")*(2*a)^2 by A112,XCMPLX_1:88
              .=b^2+(c*a")*(2*a)^2-(b^2*((2*a)"*a"))*(2*a)^2
                                   by XCMPLX_1:4
              .=b^2+(c*a")*(2*a)^2-(b^2*((2*a)*a)")*(2*a)^2 
                          by XCMPLX_1:205
              .=b^2+(c*a")*(2*a)^2-(b^2*(2*(a*a))")*(2*a)^2
                          by XCMPLX_1:4;
            set t = (b^2*(2*(a*a))")*(2*a)^2;
            t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*((2*a)^2)")
                             by XCMPLX_1:4;then
            t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*(1/(2*a)^2))
                             by XCMPLX_1:217;then
            t*((2*a)^2)" = b^2*(2*(a*a))"*1 by A113,XCMPLX_1:107;then
            t*((2*a)^2)" = b^2*(2*a^2)" by SQUARE_1:def 3;then
            t*((2*a)^2)"*2" = b^2*((2*a^2)"*2") by XCMPLX_1:4;then
            t*((2*a)^2)"*2" = b^2*((2*(a^2*2))") by XCMPLX_1:205;then
            t*((2*a)^2)"*2" = b^2*((2*2*a^2)") by XCMPLX_1:4;then
            t*((2*a)^2)"*2" = b^2*((2^2*a^2)") by SQUARE_1:def 3;then
            t*((2*a)^2)"*2" = b^2*((2*a)^2)" by SQUARE_1:68;then
            (t*2")*((2*a)^2)"*(2*a)^2 = b^2*((2*a)^2)"*(2*a)^2
                               by XCMPLX_1:4;then
            (t*2")/(2*a)^2*(2*a)^2 = b^2*((2*a)^2)"*(2*a)^2
                            by XCMPLX_0:def 9;then
            (t*2")/(2*a)^2*(2*a)^2 = b^2/(2*a)^2*(2*a)^2
                       by XCMPLX_0:def 9;then
            t*2" = b^2/(2*a)^2*(2*a)^2 by A113,XCMPLX_1:88;then
            t*2" = b^2 by A113,XCMPLX_1:88;then
            t/2 *2 = b^2 *2 by XCMPLX_0:def 9;then
  A115:     y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2-2*b^2 by XCMPLX_1:88,A114;
            set t=(c*a")*(2*a)^2;
            t=(c*a")*((2*a)*(2*a)) by SQUARE_1:def 3;then
            t=(c*a")*(a*2)*(2*a) by XCMPLX_1:4;then
            t=c*a"*a*2*(2*a) by XCMPLX_1:4;then
            t=(c/a*a)*2*(2*a) by XCMPLX_0:def 9;then
            t=c*2*(2*a) by XCMPLX_1:88,A100;then
            t=c*(2*(2*a)) by XCMPLX_1:4;then
            t=c*(2*2*a) by XCMPLX_1:4;then
            y^2 *((2*a)^2)=-2*b^2+1*b^2+0*b^2+4*a*c
              by A115,XCMPLX_1:155;then
            y^2 *((2*a)^2)=(-2)*b^2+1*b^2+0*b^2+4*a *c by XCMPLX_1:175;then
            y^2 *((2*a)^2)=((-2+1+0)*b^2)+4*a*c by XCMPLX_1:9;then
            y^2 *((2*a)^2)=-b^2+4*a*c by XCMPLX_1:180;then
            y^2 *((2*a)^2)=-b^2-(-4*a*c) by XCMPLX_1:151;then
            y^2 *(2*a)^2=(-(-4*a*c))-b^2 by XCMPLX_1:144;then
 A116:      (y *(2*a))^2=4*a*c-b^2 by SQUARE_1:68;
 A118:      -delta(a,b,c) <= 0 by A100,REAL_1:66;
            -delta(a,b,c)=-(b^2-4*a*c) by QUIN_1:def 1
                                 .=-b^2+4*a*c by XCMPLX_1:162
                                 .=-b^2-(-4*a*c) by XCMPLX_1:151
                                 .=-(-4*a*c)-b^2 by XCMPLX_1:144
                                 .=4*a*c-b^2;then
            (y *(2*a))^2 = 0 by SQUARE_1:72,A116,A118;then
            y *(2*a) = 0 by SQUARE_1:73;then
            Re z=-b/(2*a) & Im z=0 by A112a,XCMPLX_1:6,A111; then
            z = [*-b/(2*a),0*] by COMPLEX1:8;
          hence thesis by ARYTM_0:def 7;
           end;
end;

theorem Th10:
    a <> 0 & delta(a,b,c) < 0 & Poly2(a,b,c,z) = 0c implies
        z= [*-b/(2*a),sqrt (-delta(a,b,c))/(2*a)*] or
        z= [*-b/(2*a),-sqrt (-delta(a,b,c))/(2*a)*]
proof
      assume
  A100:  a <> 0 & delta(a,b,c) < 0;
         now let z;
         assume
  A101:  Poly2(a,b,c,z) = 0c;
         set x=Re z;
         set y=Im z;
         a*z^2+b*z+c = 0c by A101,POLYEQ_1:def 2;then
         [*a,0*]*z^2+b*z+c = 0c by Lem1;then
         [*a,0*]*z^2+[*b,0*]*z+c = 0c by Lem1;then
         [*a,0*]*z^2+[*b,0*]*z+[*c,0*] = 0c by Lem2;then
         [*a,0*]*([*x^2-y^2,2*(x*y)*])+[*b,0*]*z+[*c,0*] = 0c by Def4;then
         [*a,0*]*[*x^2-y^2,2*x*y*]+[*b,0*]*z+[*c,0*] = 0c by XCMPLX_1:4;then
         [*Re [*a,0*]*Re [*x^2-y^2,2*x*y*]-Im [*a,0*]*Im [*x^2-y^2,2*x*y*]
         ,Re [*a,0*]*Im [*x^2-y^2,2*x*y*]+Re [*x^2-y^2,2*x*y*]
