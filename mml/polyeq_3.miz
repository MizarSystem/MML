       a*0c^2+b*0c+c = Quard(a,z1,z2,0c) by POLYEQ_1:def 2;then
       a*0c^2+[*b,0*]*0c+c = Quard(a,z1,z2,0c) by Lem1;then
       a*0c^2+[*b,0*]*0c+[*c,0*] = Quard(a,z1,z2,0c) by Lem2
            .= a*((0c-z1)*(0c-z2)) by POLYEQ_1:def 3
            .= a*((0c+(-z1))*(0c-z2)) by XCMPLX_0:def 8
            .= a*((0c+(-z1))*(0c+(-z2))) by XCMPLX_0:def 8
            .= a*((-z1)*(0c+(-z2))) by COMPLEX1:22
            .= a*((-z1)*(-z2)) by COMPLEX1:22
            .= a*(z1*z2) by XCMPLX_1:177;then
  A108:  a*(z1*z2)=a*(0c*0c)+[*b,0*]*0c+[*c,0*] by Th4
            .=[*c,0*] by COMPLEX1:def 6;then
A114:     z1*z2=[*c,0*]*a" by XCMPLX_1:204,A100
            .= c * a" by ARYTM_0:def 7
            .= c/a by XCMPLX_0:def 9
            .= [* c/a,0*] by ARYTM_0:def 7;
     Q:   [*1,0*] = 1 by ARYTM_0:def 7;
          Quard(a,z1,z2,[*1,0*]) = Poly2(a,b,c,[*1,0*]) by A102
            .=a*[*1,0*]^2+b*[*1,0*]+c by POLYEQ_1:def 2
            .=a*[*1,0*]^2+[*b,0*]*[*1,0*]+c by Lem1
            .=a*[*1,0*]^2+[*b,0*]*[*1,0*]+[*c,0*] by Lem2
            .=a*(1*1)+[*b,0*]*1r+[*c,0*] by COMPLEX1:def 7,Q,Th4
            .=a+[*b,0*]+[*c,0*] by COMPLEX1:29; then
          a+[*b,0*]+[*c,0*]=a*(([*1,0*]-z1)*([*1,0*]-z2)) by POLYEQ_1:def 3
            .=a*((1-z1)*1r-(1-z1)*z2) by XCMPLX_1:40,Q,COMPLEX1:def 7
            .=a*((1*1r-z1*1r)-(1-z1)*z2) by XCMPLX_1:40
            .=a*((1*1r-z1*1r)-(1*z2-z1*z2)) by XCMPLX_1:40
            .=a*((1r-z1)-(1*z2-z1*z2)) by COMPLEX1:29
            .=a*(1r-z1-z2+z1*z2) by XCMPLX_1:37
            .=a*(1r-(z1+z2)+z1*z2) by XCMPLX_1:36
            .=a*((1r+(-(z1+z2)))+z1*z2) by XCMPLX_0:def 8
            .=a*(1r+(-(z1+z2)))+a*(z1*z2) by XCMPLX_1:8
            .=a*1r+a*(-(z1+z2))+a*(z1*z2) by XCMPLX_1:8
            .=a+a*(-(z1+z2))+[*c,0*] by A108,COMPLEX1:def 7;then
         a+[*b,0*]=a+a*(-(z1+z2)) by XCMPLX_1:2;then
         [*b,0*]=a*(-(z1+z2)) by XCMPLX_1:2;then
         [*b,0*]*a"=-(z1+z2) by XCMPLX_1:204,A100;then
         b*a"=-(z1+z2) by ARYTM_0:def 7;then
         -(z1+z2) = b/a by XCMPLX_0:def 9;
         hence [*b/a,0*] =-(z1+z2) & [*c/a,0*]= z1*z2 by A114,ARYTM_0:def 7;
         end;
         hence thesis;
      end;
   hence thesis;
end;

definition let z be Element of COMPLEX;
    func z^3 -> Element of COMPLEX equals
    :Def7: z^2*z;
 correctness;
end;

definition let a,b,c,d be Real,z be Element of COMPLEX;
    func Poly_3(a,b,c,d,z) -> Element of COMPLEX equals
    :Def8:   a*z^3+b*z^2+c*z+d;
  coherence by XCMPLX_0:def 2;
end;

theorem Th15:
    0c^3 = 0c
proof
  0c^3=0c^2*0c by Def7
     .=(0c*0c)*0c by Th4
     .=0c*0c by COMPLEX1:28;
  hence 0c^3 = 0c by COMPLEX1:28;
end;

theorem Th16:
  1r^3 = 1r
proof
  1r^3=1r^2*1r by Def7
  .=1r*1r*1r by Th4
  .=1r*1r by COMPLEX1:29;
  hence 1r^3 = 1r by COMPLEX1:29;
end;

theorem Th17:
    (-1r)^3=-1r
proof
  (-1r)^3=(-1r)^2*(-1r) by Def7
   .=((-1r)*(-1r))*(-1r) by Th4
   .=(1r*1r)*(-1r) by XCMPLX_1:177
   .=1r*(-1r) by COMPLEX1:29;
   hence (-1r)^3 = -1r by COMPLEX1:29;
end;

theorem Th18:
    Re z^3 = (Re z)|^3 - 3*Re z*(Im z)^2
    & Im z^3 = -(Im z)|^ 3+3*(Re z)^2*Im z
proof
 A100:   z^3 = z^2*z by Def7
         .=z*z*z by Th4
         .=[*Re z,Im z*]*z*z by COMPLEX1:8
         .=[*Re z,Im z*]*[*Re z,Im z*]*z by COMPLEX1:8
         .=[*Re z,Im z*]*[*Re z,Im z*]*[*Re z,Im z*] by COMPLEX1:8;
         set x = Re z;
         set y = Im z;
         z^3 = [*x,y*]*[*x,y*]*[*x,y*] by A100
         .= [*x*x-y*y,x*y+x*y*]*[*x,y*] by Th5
         .= [*x^2-y*y,x*y+x*y*]*[*x,y*] by SQUARE_1:def 3
         .= [*x^2-y^2,x*y+x*y*]*[*x,y*] by SQUARE_1:def 3
         .= [*x^2-y^2,2*(x*y)*]*[*x,y*] by XCMPLX_1:11
         .= [*Re[*x^2-y^2,2*(x*y)*] *Re [*x,y*]-Im[*x^2-y^2,2*(x*y)*]
            *Im [*x,y*],Re[*x^2-y^2,2*(x*y)*] *Im [*x,y*]+
            Re [*x,y*]*Im [*x^2-y^2,2*(x*y)*]*] by COMPLEX1:def 10
         .= [*(x^2-y^2) *Re [*x,y*]-Im[*x^2-y^2,2*(x*y)*] 
            *Im [*x,y*],Re[*x^2-y^2,2*(x*y)*] *Im [*x,y*]+
            Re [*x,y*]*Im [*x^2-y^2,2*(x*y)*]*] by COMPLEX1:7
         .= [*(x^2-y^2) *Re [*x,y*]-(2*(x*y))
            *Im [*x,y*],Re[*x^2-y^2,2*(x*y)*] *Im [*x,y*]+
            Re [*x,y*]*Im [*x^2-y^2,2*(x*y)*]*] by COMPLEX1:7
         .= [*(x^2-y^2) *Re [*x,y*]-(2*(x*y))
            *Im [*x,y*],(x^2-y^2) *Im [*x,y*]+
            Re [*x,y*]*Im [*x^2-y^2,2*(x*y)*]*] by COMPLEX1:7
         .= [*(x^2-y^2) *Re [*x,y*]-(2*(x*y))*Im [*x,y*]
           ,(x^2-y^2) *Im [*x,y*]+Re [*x,y*]*(2*(x*y))*] by COMPLEX1:7
         .= [*(x^2-y^2) *x-(2*(x*y))*Im [*x,y*]
           ,(x^2-y^2) *Im [*x,y*]+Re [*x,y*]*(2*(x*y))*] by COMPLEX1:7
         .= [*(x^2-y^2) *x-(2*(x*y))*y
           ,(x^2-y^2) *Im [*x,y*]+Re [*x,y*]*(2*(x*y))*] by COMPLEX1:7
         .= [*(x^2-y^2) *x-(2*(x*y))*y
           ,(x^2-y^2) *y+Re [*x,y*]*(2*(x*y))*] by COMPLEX1:7
         .= [*(x^2-y^2) *x-(2*(x*y))*y,(x^2-y^2) *y+x*(2*(x*y))*]
                                 by COMPLEX1:7
         .= [*(x^2*x-y^2*x+0*x)-2*(x*y)*y,(x^2-y^2+0)*y+x*(2*(x*y))*]
                                 by XCMPLX_1:44
         .= [*(x^2*x-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y+0*y)+x*(2*(x*y))*] 
                                  by XCMPLX_1:44
         .= [*(x*x*x-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y+0*y)+x*(2*(x*y))*]
                                by SQUARE_1:def 3
         .= [*(x|^1*x*x-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y)+x*(2*(x*y))*]
                                by NEWTON:10
         .= [*(x|^ (1+1)*x-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y)+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^(2+1)-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y)+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^ 3-y^2*x)-2*(x*y)*y,(x^2*y-y*y*y)+x*(2*(x*y))*]
                                by SQUARE_1:def 3
         .= [*(x|^ 3-y^2*x)-2*(x*y)*y,(x^2*y-y|^ 1*y*y)+x*(2*(x*y))*]
                                by NEWTON:10
         .= [*(x|^ 3-y^2*x)-2*(x*y)*y,(x^2*y-y|^ (1+1)*y)+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^ 3-y^2*x)-2*(x*y)*y,(x^2*y-y|^ (2+1))+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^ 3-y^2*x)-2*(x*y*y),(x^2*y-y|^ (2+1))+x*(2*(x*y))*]
                                by XCMPLX_1:4
         .= [*(x|^ 3-y^2*x)-2*(x*(y*y)),(x^2*y-y|^ (2+1))+x*(2*(x*y))*]
                                by XCMPLX_1:4
         .= [*x|^ 3-y^2*x-2*(x*y^2),(x^2*y-y|^ 3)+x*(2*(x*y))*]
                                by SQUARE_1:def 3
         .= [*x|^ 3-y^2*x-2*x*y^2,(x^2*y-y|^ 3)+x*(2*(x*y))*] by XCMPLX_1:4
         .= [*x|^ 3-x*y^2-2*x*y^2,x^2*y-y|^ 3+2*(x*(x*y))*] by XCMPLX_1:4
         .= [*x|^ 3-x*y^2-2*x*y^2,x^2*y-y|^ 3+2*(x*x*y)*] by XCMPLX_1:4
         .= [*x|^ 3-x*y^2-2*x*y^2,x^2*y-y|^ 3+2*(x^2*y)*] by SQUARE_1:def 3
         .= [*x|^ 3-1*(x*y^2)-2*(x*y^2),x^2*y-y|^ 3+2*(x^2*y)*] by XCMPLX_1:4
         .= [*x|^ 3-1*(x*y^2)-2*(x*y^2),-y|^ 3+2*(x^2*y)+x^2*y*]
           by XCMPLX_1:156
         .= [*x|^ 3-1*(x*y^2)-2*(x*y^2),
                          -y|^ 3+(1*x^2*y+2*(x^2*y)+0*(x^2*y))*] by XCMPLX_1:1
         .= [*x|^ 3-1*(x*y^2)-2*(x*y^2),
                      -y|^ 3+(1*(x^2*y)+2*(x^2*y)+0*(x^2*y))*]
         .= [*x|^ 3-1*(x*y^2)-2*(x*y^2),
                                   -y|^ 3+((1+2+0)*(x^2*y))*] by XCMPLX_1:9
         .= [*x|^ 3+(-1*(x*y^2)-2*(x*y^2)),-y|^ 3+3*(x^2*y)*]
           by XCMPLX_1:158
         .= [*x|^ 3+(-(2*(x*y^2)+1*(x*y^2)+0*(x*y^2)))
                              ,-y|^ 3+3*(x^2*y)*] by XCMPLX_1:161
         .= [*x|^ 3+(-((2+1+0)*(x*y^2))) ,-y|^ 3+3*(x^2*y)*] by XCMPLX_1:9
         .= [*-(-x|^ 3+3*(x*y^2)) ,-y|^ 3+3*(x^2*y)*] by XCMPLX_1:141
         .= [*x|^ 3-3*(x*y^2) ,-y|^ 3+3*(x^2*y)*] by XCMPLX_1:163
         .= [*x|^ 3-3*x*y^2 ,-y|^ 3+3*(x^2*y)*] by XCMPLX_1:4
         .= [*x|^ 3-3*x*y^2 ,-y|^ 3+3*x^2*y*] by XCMPLX_1:4;
      hence thesis by COMPLEX1:7;
end;

theorem
    (for z holds Poly_3(a,b,c,d,z) = Poly_3(a',b',c',d',z)) implies
    a = a' & b = b' & c = c' & d = d'
proof
        assume
  A100:  for z holds
         Poly_3(a,b,c,d,z) = Poly_3(a',b',c',d',z);
         Poly_3(a,b,c,d,0c) = Poly_3(a',b',c',d',0c) by A100;then
         a*0c^3+b*0c^2+c*0c+d = Poly_3(a',b',c',d',0c) by Def8;then
          Poly_3(a',b',c',d',0c)=[*a,0*]*0c^3+b*0c^2+c*0c+d by Lem1
         .=[*a,0*]*0c^3+[*b,0*]*0c^2+c*0c+d by Lem1
         .=[*a,0*]*0c^3+[*b,0*]*0c^2+[*c,0*]*0c+d by Lem1
         .=[*a,0*]*0c^3+[*b,0*]*0c^2+[*c,0*]*0c+[*d,0*] by Lem2;then
         [*a,0*]*0c^3+[*b,0*]*0c^2+[*c,0*]*0c+[*d,0*]
         =a'*0c^3+b'*0c^2+c'*0c+d' by Def8
         .=[*a',0*]*0c^3+b'*0c^2+c'*0c+d' by Lem1
         .=[*a',0*]*0c^3+[*b',0*]*0c^2+c'*0c+d' by Lem1
         .=[*a',0*]*0c^3+[*b',0*]*0c^2+[*c',0*]*0c+d' by Lem1
         .=[*a',0*]*0c+[*b',0*]*0c^2+[*c',0*]*0c+[*d',0*] by Th15,Lem2
         .=0c+[*b',0*]*0c^2+[*c',0*]*0c+[*d',0*] by COMPLEX1:28
         .=[*b',0*]*0c^2+[*c',0*]*0c+[*d',0*] by COMPLEX1:22
         .=[*b',0*]*0c^2+0c+[*d',0*] by COMPLEX1:28
         .=[*b',0*]*0c^2+[*d',0*] by COMPLEX1:22
         .=[*b',0*]*(0c*0c)+[*d',0*] by Th4
         .=[*b',0*]*0c+[*d',0*] by COMPLEX1:28
         .=0c+[*d',0*] by COMPLEX1:28
         .=[*d',0*] by COMPLEX1:22;then
         [*d',0*]=[*a,0*]*0c+[*b,0*]*0c^2+[*c,0*]*0c+[*d,0*] by Th15
         .= 0c+[*b,0*]*0c^2+[*c,0*]*0c+[*d,0*] by COMPLEX1:28
         .= [*b,0*]*0c^2+[*c,0*]*0c+[*d,0*] by COMPLEX1:22
         .= [*b,0*]*0c^2+0c+ [*d,0*] by COMPLEX1:28
         .= [*b,0*]*0c^2+ [*d,0*] by COMPLEX1:22
         .= [*b,0*]*(0c*0c)+ [*d,0*] by Th4
         .= [*b,0*]*0c+ [*d,0*] by COMPLEX1:28
         .= 0c+ [*d,0*] by COMPLEX1:28
         .=[*d,0*] by COMPLEX1:22;then
 A102:   d = d' by ARYTM_0:12;
         Poly_3(a,b,c,d,1r) =  Poly_3(a',b',c',d',1r) by A100;then
         Poly_3(a',b',c',d',1r)=a*1r^3+b*1r^2+c*1r+d by Def8
         .=[*a,0*]*1r^3+b*1r^2+c*1r+d by Lem1
         .=[*a,0*]*1r^3+[*b,0*]*1r^2+c*1r+d by Lem1
         .=[*a,0*]*1r^3+[*b,0*]*1r^2+[*c,0*]*1r+d by Lem1
         .=[*a,0*]*1r^3+[*b,0*]*1r^2+[*c,0*]*1r+[*d,0*] by Lem2;then
         [*a,0*]*1r^3+[*b,0*]*1r^2+[*c,0*]*1r+[*d,0*]
         = a'*1r^3+b'*1r^2+c'*1r+d' by Def8
         .= [*a',0*]*1r^3+b'*1r^2+c'*1r+d' by Lem1
         .= [*a',0*]*1r^3+[*b',0*]*1r^2+c'*1r+d' by Lem1
         .= [*a',0*]*1r^3+[*b',0*]*1r^2+[*c',0*]*1r+d' by Lem1
         .=[*a',0*]*1r+[*b',0*]*1r^2+[*c',0*]*1r+[*d',0*] by Th16,Lem2
         .=[*a',0*]+[*b',0*]*1r^2+[*c',0*]*1r+[*d',0*] by COMPLEX1:29
         .=[*a',0*]+[*b',0*]*1r^2+[*c',0*]+[*d',0*] by COMPLEX1:29
         .=[*a',0*]+[*b',0*]*(1r*1r)+[*c',0*]+[*d',0*] by Th4
         .=[*a',0*]+[*b',0*]*1r+[*c',0*]+[*d',0*] by COMPLEX1:29
         .=[*a',0*]+[*b',0*]+[*c',0*]+[*d',0*] by COMPLEX1:29
         .=[*a'+b',0+0*]+[*c',0*]+[*d',0*] by COMPLFLD:2
         .=[*a'+b'+c',0+0*]+[*d',0*] by COMPLFLD:2
         .=[*a'+b'+c'+d',0+0*] by COMPLFLD:2;then
         [*a'+b'+c'+d',0*]
         = [*a,0*]*1r+[*b,0*]*1r^2+[*c,0*]*1r+[*d,0*] by Th16
         .= [*a,0*]+[*b,0*]*1r^2+[*c,0*]*1r+[*d,0*] by COMPLEX1:29
         .= [*a,0*]+[*b,0*]*1r^2+[*c,0*]+[*d,0*] by COMPLEX1:29
         .= [*a,0*]+[*b,0*]*(1r*1r)+[*c,0*]+[*d,0*] by Th4
         .= [*a,0*]+[*b,0*]*1r+[*c,0*]+[*d,0*] by COMPLEX1:29
         .= [*a,0*]+[*b,0*]+[*c,0*]+[*d,0*] by COMPLEX1:29
         .= [*a+b,0+0*]+[*c,0*]+[*d,0*] by COMPLFLD:2
         .= [*a+b+c,0+0*]+[*d,0*] by COMPLFLD:2
         .= [*a+b+c+d,0+0*] by COMPLFLD:2;then
 A104:   a+b+c+d = a'+b'+c'+d' by ARYTM_0:12;
         set r1 = a+b+c;
         set r2 = a'+b'+c';
         r1-r2 = d - d by A104,XCMPLX_1:33,A102
              .=  0 by XCMPLX_1:14;then
 A106:   r1 = r2 by XCMPLX_1:15;
         Poly_3(a,b,c,d,-1r) =  Poly_3(a',b',c',d',-1r) by A100;then     
         Poly_3(a',b',c',d',-1r)=a*(-1r)^3+b*(-1r)^2+c*(-1r)+d by Def8
         .=[*a,0*]*(-1r)^3+b*(-1r)^2+c*(-1r)+d by Lem1
         .=[*a,0*]*(-1r)^3+[*b,0*]*(-1r)^2+c*(-1r)+d by Lem1
         .=[*a,0*]*(-1r)^3+[*b,0*]*(-1r)^2+[*c,0*]*(-1r)+d by Lem1
         .=[*a,0*]*(-1r)^3+[*b,0*]*(-1r)^2+[*c,0*]*(-1r)+[*d,0*] by Lem2;then
         [*a,0*]*(-1r)^3+[*b,0*]*(-1r)^2+[*c,0*]*(-1r)+[*d,0*]
         = a'*(-1r)^3+b'*(-1r)^2+c'*(-1r)+d' by Def8
         .= [*a',0*]*(-1r)^3+b'*(-1r)^2+c'*(-1r)+d' by Lem1
         .= [*a',0*]*(-1r)^3+[*b',0*]*(-1r)^2+c'*(-1r)+d' by Lem1
         .= [*a',0*]*(-1r)^3+[*b',0*]*(-1r)^2+[*c',0*]*(-1r)+d' by Lem1
         .=[*a',0*]*(-1r)^3+[*b',0*]*(-1r)^2+[*c',0*]*(-1r)+[*d',0*] by Lem2
         .=[*a',0*]*(-1r)+[*b',0*]*((-1r)*(-1r))+[*c',0*]*(-1r)+[*d',0*]
                                        by Th4,Th17
         .=[*a',0*]*(-1r)+[*b',0*]*(1r*1r)+[*c',0*]*(-1r)+[*d',0*]
                                        by XCMPLX_1:177
         .=[*a',0*]*(-1)+[*b',0*]*1+[*c',0*]*(-1)+[*d',0*]
           by COMPLEX1:def 7
         .=-[*a',0*]+[*b',0*]+[*c',0*]*(-1)+[*d',0*] by XCMPLX_1:180
         .=-[*a',0*]+[*b',0*]+-[*c',0*]+[*d',0*] by XCMPLX_1:180
         .=[*-Re [*a',0*],-Im [*a',0*]*]+[*b',0*]+-[*c',0*]+[*d',0*]
                               by COMPLEX1:def 11
         .=[*-a',-Im [*a',0*]*]+[*b',0*]+-[*c',0*]+[*d',0*] by COMPLEX1:7
         .=[*-a',-0*]+[*b',0*]+-[*c',0*]+[*d',0*] by COMPLEX1:7
         .=[*-a',0*]+[*b',0*]+[*-Re [*c',0*],-Im [*c',0*]*]+[*d',0*]
                                 by COMPLEX1:def 11
         .=[*-a',0*]+[*b',0*]+[*-c',-Im [*c',0*]*]+[*d',0*] by COMPLEX1:7
         .=[*-a',0*]+[*b',0*]+[*-c',-0*]+[*d',0*] by COMPLEX1:7
         .=[*-a'+b',0+0*]+[*-c',0*]+[*d',0*] by COMPLFLD:2
         .=[*-a'+b'+(-c'),0+0*]+[*d',0*] by COMPLFLD:2
         .=[*-a'+b'+(-c')+d',0+0*] by COMPLFLD:2;then
         [*-a'+b'+(-c')+d',0+0*]
         =[*a,0*]*(-1r)+[*b,0*]*(-1r)^2+[*c,0*]*(-1r)+[*d,0*] by Th17
         .=[*a,0*]*(-1r)+[*b,0*]*((-1r)*(-1r))+[*c,0*]*(-1r)+[*d,0*]
                                             by Th4
         .=[*a,0*]*(-1r)+[*b,0*]*(1r*1r)+[*c,0*]*(-1r)+[*d,0*]
                                         by XCMPLX_1:177
         .=[*a,0*]*(-1)+[*b,0*]+[*c,0*]*(-1)+[*d,0*] by COMPLEX1:def 7
         .=-[*a,0*]+[*b,0*]+[*c,0*]*(-1)+[*d,0*] by XCMPLX_1:180
         .=-[*a,0*]+[*b,0*]+-[*c,0*]+[*d,0*] by XCMPLX_1:180
         .=[*-Re [*a,0*],-Im [*a,0*]*]+[*b,0*]+-[*c,0*]+[*d,0*]
                               by COMPLEX1:def 11
         .=[*-a,-Im [*a,0*]*]+[*b,0*]+-[*c,0*]+[*d,0*] by COMPLEX1:7
         .=[*-a,-0*]+[*b,0*]+-[*c,0*]+[*d,0*] by COMPLEX1:7
         .=[*-a,0*]+[*b,0*]+[*-Re [*c,0*],-Im [*c,0*]*]+[*d,0*]
                                 by COMPLEX1:def 11
         .=[*-a,0*]+[*b,0*]+[*-c,-Im [*c,0*]*]+[*d,0*] by COMPLEX1:7
         .=[*-a,0*]+[*b,0*]+[*-c,-0*]+[*d,0*] by COMPLEX1:7
         .=[*-a+b,0+0*]+[*-c,0*]+[*d,0*] by COMPLFLD:2
         .=[*-a+b+(-c),0+0*]+[*d,0*] by COMPLFLD:2
         .=[*-a+b+(-c)+d,0+0*] by COMPLFLD:2;then
         -a+b+(-c)+d = -a'+b'+(-c')+d' by ARYTM_0:12;then
         -a+b-c+d = -a'+b'+(-c')+d' by XCMPLX_0:def 8;then
 A110:   -a+b-c+d = -a'+b'-c'+d' by XCMPLX_0:def 8;
         set s1 = -a+b-c;
         set s2 = -a'+b'-c';
 A112:   s1-s2 = d - d by A110,XCMPLX_1:33,A102
              .= 0 by XCMPLX_1:14;then
         r1+s1 = r2+s2 by XCMPLX_1:15,A106
          .=(a'+b'+c')+(-c'-a'+b') by XCMPLX_1:166
          .=(b'+a'+c')+(-(c'+a')+b') by XCMPLX_1:161
          .=(b'+a'+c')+-(c'+a')+b' by XCMPLX_1:1
          .=b'+(a'+c')+-(c'+a')+b' by XCMPLX_1:1
          .=b'+0+b' by XCMPLX_1:137
          .=2*b' by XCMPLX_1:11;then
          2*b' = (a+b+c)+(-c-a+b) by XCMPLX_1:166
          .= ( b+a+c)+(-(c+a)+b) by XCMPLX_1:161
          .= ( b+a+c)+-(c+a)+b by XCMPLX_1:1
          .= b+(a+c)+-(c+a)+b by XCMPLX_1:1
          .= b+0+b by XCMPLX_1:137
          .=2*b by XCMPLX_1:11;then
 A114:    b = b' by XCMPLX_1:5;
          r1-s1 = r2-s2 by XCMPLX_1:15,A106,A112
          .= a'+b'+c'- -( a' - b' +c') by XCMPLX_1:169
          .= a'+b'+c'+( a'-b'+c')by XCMPLX_1:151
          .= a'+b'+c'+(-b'+a'+c')by XCMPLX_0:def 8
          .= a'+b'+c'+(-(b'-a'-c')) by XCMPLX_1:170
          .= a'+b'+c'-(b'-a'-c') by XCMPLX_0:def 8
          .= a'+b'+c'-(b'-(a'+c')) by XCMPLX_1:36
          .= a'+b'+c'-b'+(a'+c') by XCMPLX_1:37
          .= a'+b'-b'+c'+(a'+c') by XCMPLX_1:29
          .= a'+c'+(a'+c') by XCMPLX_1:26; then
         2*(a'+c') = a+b+c-(-a+b-c) by XCMPLX_1:11
            .= a+b+c--(a-b+c) by XCMPLX_1:169
            .= a+b+c+(a-b+c) by XCMPLX_1:151
            .= a+b+c+(-b+a+c) by XCMPLX_0:def 8
            .= a+b+c+(-(b-a-c)) by XCMPLX_1:170
            .= a+b+c-(b-a-c) by XCMPLX_0:def 8
            .= a+b+c-(b-(a+c)) by XCMPLX_1:36
            .= a+b+c-b+(a+c) by XCMPLX_1:37
            .= a+b-b+c+(a+c) by XCMPLX_1:29
            .= a +c +(a+c) by XCMPLX_1:26
            .= 2*(a+c) by XCMPLX_1:11; then
           a'+c' = a+c by XCMPLX_1:5; then
 A116:     a-a' = c'-c by XCMPLX_1:33;
         Poly_3(a,b,c,d,[*2,0*]) =  Poly_3(a',b',c',d',[*2,0*]) by A100;then
         Poly_3(a',b',c',d',[*2,0*])
         =a*[*2,0*]^3+b*[*2,0*]^2+c*[*2,0*]+d by Def8
         .=[*a,0*]*[*2,0*]^3+b*[*2,0*]^2+c*[*2,0*]+d by Lem1
         .=[*a,0*]*[*2,0*]^3+[*b,0*]*[*2,0*]^2+c*[*2,0*]+d by Lem1
         .=[*a,0*]*[*2,0*]^3+[*b,0*]*[*2,0*]^2+[*c,0*]*[*2,0*]+d by Lem1;then
         [*a,0*]*[*2,0*]^3+[*b,0*]*[*2,0*]^2+[*c,0*]*[*2,0*]+[*d,0*]
         = Poly_3(a',b',c',d',[*2,0*]) by Lem2
         .=a'*[*2,0*]^3+b'*[*2,0*]^2+c'*[*2,0*]+d' by Def8
         .=[*a',0*]*[*2,0*]^3+b'*[*2,0*]^2+c'*[*2,0*]+d' by Lem1
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*2,0*]^2+c'*[*2,0*]+d' by Lem1
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*2,0*]^2+[*c',0*]*[*2,0*]+d' by Lem1
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*2,0*]^2+[*c',0*]*[*2,0*]+[*d',0*]
                                      by Lem2
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*([*2,0*]*[*2,0*])
                   +[*c',0*]*[*2,0*]+[*d',0*] by Th4
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*2*2,0*]
                   +[*c',0*]*[*2,0*]+[*d',0*] by Th7
         .=[*a',0*]*[*2,0*]^3+[*b',0*]*[*4,0*]+[*c'*2,0*]+[*d',0*] by Th7
         .=[*a',0*]*[*2,0*]^3+[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Th7
         .=[*a',0*]*([*2,0*]^2*[*2,0*])
                                +[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Def7
         .=[*a',0*]*([*2,0*]*[*2,0*]*[*2,0*])
                                +[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Th4
         .=[*a',0*]*([*2*2,0*]*[*2,0*])
                                +[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Th7
         .=[*a',0*]*[*4*2,0*]+[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Th7
         .=[*a'*8,0*]+[*b'*4,0*]+[*c'*2,0*]+[*d',0*] by Th7
         .=[*a'*8+b'*4,0+0*]+[*c'*2,0*]+[*d',0*] by COMPLFLD:2
         .=[*a'*8+b'*4+c'*2,0+0*]+[*d',0*] by COMPLFLD:2
         .=[*a'*8+b'*4+c'*2+d',0+0*] by COMPLFLD:2;then
        [*a'*8+b'*4+c'*2+d',0+0*]
          =[*a,0*]*[*2,0*]^3+[*b,0*]*[*2,0*]^2+[*c*2,0*]+[*d,0*] by Th7
         .=[*a,0*]*[*2,0*]^3+[*b,0*]*([*2,0*]*[*2,0*])
                                           +[*c*2,0*]+[*d,0*] by Th4
         .=[*a,0*]*[*2,0*]^3+[*b,0*]*[*2*2,0*]+[*c*2,0*]+[*d,0*] by Th7
         .=[*a,0*]*[*2,0*]^3+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Th7
         .=[*a,0*]*([*2,0*]^2*[*2,0*])+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Def7
         .=[*a,0*]*([*2,0*]*[*2,0*]*[*2,0*])
                         +[*b*4,0*]+[*c*2,0*]+[*d,0*] by Th4
         .=[*a,0*]*([*2*2,0*]*[*2,0*])+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Th7
         .=[*a,0*]*[*4*2,0*]+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Th7
         .=[*a*8,0*]+[*b*4,0*]+[*c*2,0*]+[*d,0*] by Th7
         .=[*a*8+b*4,0+0*]+[*c*2,0*]+[*d,0*] by COMPLFLD:2
         .=[*a*8+b*4+c*2,0+0*]+[*d,0*] by COMPLFLD:2
         .=[*a*8+b*4+c*2+d,0+0*] by COMPLFLD:2;then
  A120:   a*8+b*4+c*2+d = a'*8+b'*4+c'*2+d' by ARYTM_0:12;
         set t1=a*8+b*4+c*2;
         set t2=a'*8+b'*4+c'*2;
  A122:  t1 - t2 =d - d by A102,XCMPLX_1:33,A120
                .=0 by XCMPLX_1:14;
  A124:  (a - a')*8 = (c' -c)*2
         proof
          a*8+ b*4 +c*2 = a'*8 + b'*4 +c'*2 by A122,XCMPLX_1:15; then
          a*8+ (b*4 +c*2) = a'*8 + b'*4 +c'*2 by XCMPLX_1:1
                        .= a'*8 + (b'*4 +c'*2) by XCMPLX_1:1; then
          a*8 - a'*8 = b'*4 +c'*2 -( b*4 +c*2) by XCMPLX_1:33
                  .= b'*4 +c'*2 - b*4 -c*2 by XCMPLX_1:36
                  .= b'*4 - b*4+c'*2 -c*2 by XCMPLX_1:29
                  .= (b*4 - b*4)+c'*2 -c*2 by A114
                  .=  0 +c'*2 -c*2 by XCMPLX_1:14; then
          (a - a')*8 =  c'*2 -c*2 by XCMPLX_1:40;hence thesis by XCMPLX_1:40;
         end;
 A126:   8-2=6;
         (c' - c)*8 - (c' -c)*2 = 0 by A116,XCMPLX_1:14,A124; then
         (c' - c)*6 = 0 by A126,XCMPLX_1:40;then
          c' - c = 0 or 6 = 0 by XCMPLX_1:6;
   hence thesis by A102,A114,A116,XCMPLX_1:15;
end;

Th21:
    z+z+z = 3*z by XCMPLX_1:12;

theorem  Th22:
    (z+h)^3 = z^3+(3*h)*z^2+(3*h^2)*z+h^3
proof
  A100:  (z+h)^3 = (z+h)^2*(z+h) by Def7
          .=((z+h)*(z+h))*(z+h) by Th4
          .=((z+h)*z+(z+h)*h)*(z+h) by XCMPLX_1:8
          .=((z*z+h*z)+(z+h)*h)*(z+h) by XCMPLX_1:8
          .=((z*z+h*z)+(z*h+h*h))*(z+h) by XCMPLX_1:8
          .=(z*z+h*z)*(z+h)+(z*h+h*h)*(z+h) by XCMPLX_1:8
          .=((z*z)*(z+h)+(h*z)*(z+h))+(z*h+h*h)*(z+h) by XCMPLX_1:8
          .=(((z*z)*z+(z*z)*h)+(h*z)*(z+h))+(z*h+h*h)*(z+h) by XCMPLX_1:8
          .=((z^2*z+(z*z)*h)+(h*z)*(z+h))+(z*h+h*h)*(z+h) by Th4
          .=((z^2*z+z^2*h)+(h*z)*(z+h))+(z*h+h*h)*(z+h) by Th4
          .=((z^3+z^2*h)+(h*z)*(z+h))+(z*h+h*h)*(z+h) by Def7
          .=((z^3+z^2*h)+(h*z*z+(h*z)*h))+(z*h+h*h)*(z+h) by XCMPLX_1:8
          .=((z^3+z^2*h)+(h*(z*z)+z*h*h))+(z*h+h*h)*(z+h) by XCMPLX_1:4
          .=((z^3+z^2*h)+(h*(z*z)+z*(h*h)))+(z*h+h*h)*(z+h) by XCMPLX_1:4
          .=((z^3+z^2*h)+(h*(z^2)+z*(h*h)))+(z*h+h*h)*(z+h) by Th4
          .=((z^3+z^2*h)+(h*(z^2)+z*(h^2)))+(z*h+h*h)*(z+h) by Th4
          .=(z^3+z^2*h+h*(z^2)+z*(h^2))+(z*h+h*h)*(z+h) by XCMPLX_1:1;
          set t = (z*h+h*h)*(z+h);
          t = (z*h)*(z+h)+(h*h)*(z+h) by XCMPLX_1:8
           .= ((z*h)*z+(z*h)*h)+(h*h)*(z+h) by XCMPLX_1:8
           .= ((z*h)*z+(z*h)*h)+((h*h)*z+(h*h)*h) by XCMPLX_1:8
           .= (h*z*z+z*h*h)+((h^2)*z+(h*h)*h) by Th4
           .= (h*z*z+z*h*h)+((h^2)*z+h^2*h) by Th4
           .= (h*(z*z)+z*h*h)+((h^2)*z+h^2*h) by XCMPLX_1:4
           .= (h*(z*z)+z*(h*h))+((h^2)*z+h^2*h) by XCMPLX_1:4
           .= (h*(z^2)+z*(h*h))+((h^2)*z+h^2*h) by Th4
           .= (h*(z^2)+z*(h^2))+((h^2)*z+h^2*h) by Th4
           .= (h*(z^2)+z*(h^2))+((h^2)*z+h^3) by Def7
           .= (h*(z^2)+z*(h^2))+(h^2)*z+h^3 by XCMPLX_1:1;then
         (z+h)^3 =(z^3+z^2*h+h*(z^2)+z*(h^2))+(h*(z^2)+z*(h^2)+(h^2)*z+h^3)
                                        by A100
           .=(z^3+z^2*h+h*(z^2)+z*(h^2))+((h*(z^2)+z*(h^2))+((h^2)*z+h^3)) 
                                        by XCMPLX_1:1
           .=(z^3+z^2*h+h*(z^2)+z*(h^2))+(h*(z^2)+z*(h^2))+((h^2)*z+h^3)
                                        by XCMPLX_1:1
           .=(z^3+z^2*h+h*(z^2)+z*(h^2))+(h*(z^2)+z*(h^2))+(h^2)*z+h^3
                                        by XCMPLX_1:1
           .=(z^3+z^2*h+h*(z^2))+z*(h^2)+h*(z^2)+z*(h^2)+(h^2)*z+h^3
                                        by XCMPLX_1:1
           .=(z^3+z^2*h+h*(z^2))+h*(z^2)+z*(h^2)+z*(h^2)+(h^2)*z+h^3 
                                        by XCMPLX_1:1
           .=z^3+(z^2*h+h*(z^2))+h*(z^2)+z*(h^2)+z*(h^2)+(h^2)*z+h^3
                                        by XCMPLX_1:1
           .=z^3+(z^2*h+h*(z^2)+h*(z^2))+z*(h^2)+z*(h^2)+(h^2)*z+h^3
                                        by XCMPLX_1:1
           .=(z^3+(z^2*h+h*(z^2)+h*(z^2)))+(z*(h^2)+z*(h^2))+(h^2)*z+h^3
                                        by XCMPLX_1:1
           .=(z^3+(h*(z^2)+h*(z^2)+h*(z^2)))+(z*(h^2)+z*(h^2)+z*(h^2))+h^3 
                                        by XCMPLX_1:1
           .=(z^3+((h+h)*(z^2)+h*(z^2)))+(z*(h^2)+z*(h^2)+z*(h^2))+h^3
                                        by XCMPLX_1:8
           .=(z^3+((h+h+h)*(z^2)))+(z*(h^2)+z*(h^2)+z*(h^2))+h^3 
                                        by XCMPLX_1:8
           .=(z^3+((h+h+h)*(z^2)))+((z+z)*(h^2)+z*(h^2))+h^3 by XCMPLX_1:8
           .=(z^3+((h+h+h)*(z^2)))+((z+z+z)*(h^2))+h^3 by XCMPLX_1:8
           .=(z^3+(3*h)*(z^2))+((z+z+z)*(h^2))+h^3 by Th21
           .=(z^3+(3*h)*(z^2))+3*z*(h^2)+h^3 by Th21
           .=(z^3+(3*h)*(z^2))+[*3,0*]*z*(h^2)+h^3 by Lem1
           .=(z^3+(3*h)*(z^2))+[*3,0*]*(h^2)*z+h^3 by XCMPLX_1:4
           .=(z^3+(3*h)*(z^2))+3*(h^2)*z+h^3 by Lem1;
      hence thesis;
end;

theorem
    (z*h)^3 = z^3*h^3
proof
   (z*h)^3 = (z*h)^2*(z*h) by Def7
           .= (z*h)*(z*h)*(z*h) by Th4
           .= (z*h)*h*z*(z*h) by XCMPLX_1:4
           .= z*(h*h)*z*(z*h) by XCMPLX_1:4
           .= z*h^2*z*(z*h) by Th4
           .= (z*(z*h^2))*h*z by XCMPLX_1:4
           .= (z*z*h^2)*h*z by XCMPLX_1:4
           .= (z^2*h^2)*h*z by Th4
           .= z^2*(h^2*h)*z by XCMPLX_1:4
           .= z^2*h^3*z by Def7
           .= z^2*z*h^3 by XCMPLX_1:4;
   hence thesis by Def7;
end;

theorem
    b<>0 & Poly_3(0,b,c,d,z)=0c & delta(b,c,d)>=0 implies
       z = (-c+sqrt delta(b,c,d))/(2*b) or
       z = (-c-sqrt delta(b,c,d))/(2*b) or
       z = -c/(2*b)
proof
         assume
 A100:   b<>0 & Poly_3(0,b,c,d,z) = 0c & delta(b,c,d)>=0;then
         0c=0*(z^3)+b*z^2+c*z+d by Def8
         .=[*0,0*]*(z^3)+b*z^2+c*z+d by Lem1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+c*z+d by Lem1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+d by Lem1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+[*d,0*] by Lem2;then
         0c+[*b,0*]*z^2+[*c,0*]*z+[*d,0*] = 0c 
                 by COMPLEX1:def 6,XXX;then
         [*b,0*]*z^2+[*c,0*]*z+[*d,0*] = 0c by COMPLEX1:22;then
         b*z^2+[*c,0*]*z+[*d,0*] = 0c by Lem1;then
         b*z^2+c*z+[*d,0*] = 0c by Lem1;then
         b*z^2+c*z+d = 0c by Lem2;then
         Poly2(b,c,d,z)=0c by POLYEQ_1:def 2;
         hence thesis by A100,Th9,COMPLEX1:14;
end;

theorem
    b<>0 & Poly_3(0,b,c,d,z)=0c & delta(b,c,d)<0 implies
      z = [*-c/(2*b),sqrt (-delta(b,c,d))/(2*b)*] or
      z = [*-c/(2*b),-sqrt (-delta(b,c,d))/(2*b)*]
proof
         assume
 A100:   b<>0 & Poly_3(0,b,c,d,z) = 0c & delta(b,c,d)<0;then
         0c=0*(z^3)+b*z^2+c*z+d by Def8
         .=[*0,0*]*(z^3)+b*z^2+c*z+d by Lem1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+c*z+d by Lem1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+d by Lem1
         .=[*0,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+[*d,0*] by Lem2;then
         0c+[*b,0*]*z^2+[*c,0*]*z+[*d,0*] = 0c
                 by COMPLEX1:def 6,XXX;then
         [*b,0*]*z^2+[*c,0*]*z+[*d,0*] = 0c by COMPLEX1:22;then
         b*z^2+[*c,0*]*z+[*d,0*] = 0c by Lem1;then
         b*z^2+c*z+[*d,0*] = 0c by Lem1;then
         b*z^2+c*z+d = 0c by Lem2;then
         Poly2(b,c,d,z)=0c by POLYEQ_1:def 2;
         hence z = [*-c/(2*b),sqrt (-delta(b,c,d))/(2*b)*] or
             z = [*-c/(2*b),-sqrt (-delta(b,c,d))/(2*b)*] by A100,Th10;
end;

theorem
    a<>0 & Poly_3(a,0,c,0,z)=0 & 4*a*c <= 0 implies
       z = (sqrt -4*a*c)/(2*a) or
       z = (-sqrt -4*a*c)/(2*a) or
       z = 0
proof
         assume
 A100:   a<>0 & Poly_3(a,0,c,0,z) = 0 & 4*a*c <= 0;then
         0=a*(z^3)+0*z^2+c*z+0 by Def8
         .=[*a,0*]*(z^3)+0*z^2+c*z+0 by Lem1
         .=[*a,0*]*(z^3)+0*z^2+[*c,0*]*z+0 by Lem1
         .=[*a,0*]*(z^3)+0*z^2+[*c,0*]*z+0;then
    [*a,0*]*z^3+0c+[*c,0*]*z+0 = 0 by COMPLEX1:def 6;then
         [*a,0*]*z^3+[*c,0*]*z+0 = 0 by COMPLEX1:22;then
         [*a,0*]*(z^2*z)+[*c,0*]*z = 0 by Def7;then
         ([*a,0*]*z^2)*z+[*c,0*]*z = 0 by XCMPLX_1:4;then
         ([*a,0*]*z^2+[*c,0*])*z = 0 by XCMPLX_1:8;then
         [*a,0*]*z^2+[*c,0*] = 0 or z = 0 by XCMPLX_1:6;then
         [*a,0*]*z^2+[*0,0*]*z+[*c,0*]=0 or z = 0
                 by COMPLEX1:def 6,XXX;then
         a*z^2+[*0,0*]*z+[*c,0*]=0 or z = 0 by Lem1;then
         (a*z^2+0*z)+[*c,0*]=0 or z = 0 by Lem1;then
         (a*z^2+0*z) + c = 0 or z = 0 by Lem2;then
 A106:   Poly2(a,0,c,z) = 0 or z = 0 by POLYEQ_1:def 2;
         -4*a*c >= -0 by A100,REAL_1:50;then
         0^2-4*a*c >= 0 by SQUARE_1:60,XCMPLX_1:150;then
         delta(a,0,c) >= 0 by QUIN_1:def 1;then
            z = (-0+sqrt delta(a,0,c))/(2*a)
            or z = (-0-sqrt delta(a,0,c))/(2*a)
            or z= 0
            or z = -0/(2*a) by A106,Th9,A100;then
            z = (sqrt delta(a,0,c))/(2*a)
            or z = (-sqrt delta(a,0,c))/(2*a)
            or z = 0 by XCMPLX_1:150;then
            z = (sqrt (0-4*a*c))/(2*a)
            or z = (-sqrt (0-4*a*c))/(2*a)
            or z= 0 by SQUARE_1:60,QUIN_1:def 1;
    hence thesis by XCMPLX_1:150;
end;

theorem
    a<>0 & Poly_3(a,b,c,0,z)=0 & delta(a,b,c)>=0 implies
       z = (-b+sqrt delta(a,b,c))/(2*a) or
       z = (-b-sqrt delta(a,b,c))/(2*a) or
       z = -b/(2*a) or
       z = 0
proof
         assume
 A100:   a<>0 & Poly_3(a,b,c,0,z) = 0 & delta(a,b,c)>=0;then
         0=a*(z^3)+b*z^2+c*z+0 by Def8
         .=[*a,0*]*(z^3)+b*z^2+c*z+0 by Lem1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+c*z+0 by Lem1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+0 by Lem1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+[*0,0*] by Lem2;then
         [*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z = 0
                      by COMPLEX1:def 6,XXX;then
         [*a,0*]*(z^2*z)+[*b,0*]*z^2+[*c,0*]*z = 0 by Def7;then
         [*a,0*]*(z^2*z)+[*b,0*]*(z*z)+[*c,0*]*z = 0 by Th4;then
         [*a,0*]*z^2*z+[*b,0*]*(z*z)+[*c,0*]*z = 0 by XCMPLX_1:4;then
         [*a,0*]*z^2*z+[*b,0*]*z*z+[*c,0*]*z = 0 by XCMPLX_1:4;then
         ([*a,0*]*z^2+[*b,0*]*z)*z+[*c,0*]*z = 0 by XCMPLX_1:8;then
         ([*a,0*]*z^2+[*b,0*]*z+[*c,0*])*z = 0 by XCMPLX_1:8;then
         [*a,0*]*z^2+[*b,0*]*z+[*c,0*] = 0 or z = 0
           by XCMPLX_1:6;then
         a*z^2+[*b,0*]*z+[*c,0*] = 0 or z = 0 by Lem1;then
         a*z^2+b*z+[*c,0*] = 0 or z = 0 by Lem1;then
         a*z^2+b*z+c = 0 or z = 0 by Lem2;then
         Poly2(a,b,c,z) = 0 or z = 0 by POLYEQ_1:def 2;
     hence thesis by A100,Th9;
end;

theorem
    a<>0 & Poly_3(a,b,c,0,z)=0c & delta(a,b,c)<0 implies
      z= [*-b/(2*a),sqrt (-delta(a,b,c))/(2*a)*] or
      z= [*-b/(2*a),-sqrt (-delta(a,b,c))/(2*a)*] or
      z = 0c
proof
         assume
 A100:   a<>0 & Poly_3(a,b,c,0,z) = 0c & delta(a,b,c)<0;then
         0c=a*(z^3)+b*z^2+c*z+0 by Def8
         .=[*a,0*]*(z^3)+b*z^2+c*z+0 by Lem1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+c*z+0 by Lem1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+0 by Lem1
         .=[*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+[*0,0*] by Lem2;then
         [*a,0*]*(z^3)+[*b,0*]*z^2+[*c,0*]*z+0 = 0c by ARYTM_0:def 7;then
         [*a,0*]*(z^2*z)+[*b,0*]*z^2+[*c,0*]*z = 0c by Def7;then
         [*a,0*]*(z^2*z)+[*b,0*]*(z*z)+[*c,0*]*z = 0c by Th4;then
         [*a,0*]*z^2*z+[*b,0*]*(z*z)+[*c,0*]*z = 0c by XCMPLX_1:4;then
         [*a,0*]*z^2*z+[*b,0*]*z*z+[*c,0*]*z = 0c by XCMPLX_1:4;then
         ([*a,0*]*z^2+[*b,0*]*z)*z+[*c,0*]*z = 0c by XCMPLX_1:8;then
         ([*a,0*]*z^2+[*b,0*]*z+[*c,0*])*z = 0c by XCMPLX_1:8;then
         [*a,0*]*z^2+[*b,0*]*z+[*c,0*] = 0c or z = 0c
           by XCMPLX_1:6,COMPLEX1:def 6;then
         a*z^2+[*b,0*]*z+[*c,0*] = 0c or z = 0c by Lem1;then
         a*z^2+b*z+[*c,0*] = 0c or z = 0c by Lem1;then
         a*z^2+b*z+c = 0c or z = 0c by Lem2;then
         Poly2(a,b,c,z) = 0c or z = 0c by POLYEQ_1:def 2;
     hence thesis by A100,Th10;
end;

theorem Th28:
    a>=0 & y^2 = a implies y = sqrt a or y = -sqrt a
proof
         assume
 A100:   a>=0 & y^2 = a;
         1*y^2 +0*y+(-a) = -(-a+a) by XCMPLX_1:141,A100;then
         1*y^2 +0*y+(-a) = a-a by XCMPLX_1:163;then
         1*y^2 +0*y+(-a) = 0 by XCMPLX_1:14;then
 A103:   Poly2(1,0,-a,y) =0 by POLYEQ_1:def 2;
         delta(1,0,-a) = 0^2 - 4*1*(-a) by QUIN_1:def 1;then
         delta(1,0,-a) = -4*(-a) by XCMPLX_1:150,SQUARE_1:60;then
 A104:   delta(1,0,-a) = 4*a by XCMPLX_1:179;
         delta(1,0,-a) >= 0 by A104,REAL_2:121,A100;then
         y=(-0+sqrt delta(1,0,-a))/(2*1) or
         y=(-0-sqrt delta(1,0,-a))/(2*1) by POLYEQ_1:5,A103;then
         y=(sqrt (4*a))/2 or
         y=(-sqrt (4*a))/2 by A104,XCMPLX_1:150;then
         y= sqrt a * 2 /2 or
         y=(-(2 * sqrt a))/2 by SQUARE_1:85,A100,SQUARE_1:97;then
         y= sqrt a * 2 /2 or
         y=-(sqrt a * 2/2) by XCMPLX_1:188;
    hence y= sqrt a or y=-sqrt a by XCMPLX_1:90;
end;

theorem
    a<>0 & Poly_3(a,0,c,d,z)=0c & Im z = 0
       implies
       (for u,v st Re z = u+v & 3*v*u+c/a=0 holds
          z = 3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
             +3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
       or z = 3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
             +3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
       or z = 3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
             +3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3)) )
proof
       assume
 A100:   a <> 0;
       assume
 A102:   Poly_3(a,0,c,d,z) = 0c & Im z = 0;
        let u,v;
       assume
 A104:   Re z=u+v & 3*v*u+c/a=0;
         set x=Re z;
         set y=Im z;
         set p=c/a;
 A108:   z = [*Re z,Im z*] by COMPLEX1:8
           .= x by ARYTM_0:def 7,A102;
         0 = 0c by COMPLEX1:def 6 .=a*(z^3)+0*z^2+c*z+d by Def8,A102
         .=[*a,0*]*(z^3)+0*z^2+c*z+d by Lem1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+c*z+d by Lem1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+[*c,0*]*z+d by Lem1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+[*c,0*]*z+[*d,0*] by Lem2;then
         0 = [*a,0*]*([*Re z^3,Im z^3*])+[*0,0*]*z^2+[*c,0*]*z+[*d,0*]
                        by COMPLEX1:8
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +[*0,0*]*z^2+[*c,0*]*z+[*d,0*] by Th18
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +0c*z^2+[*c,0*]*z+[*d,0*] by COMPLEX1:def 6,ARYTM_0:def 7
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +0c+[*c,0*]*z+[*d,0*] by COMPLEX1:28
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
            +[*c,0*]*z+[*d,0*] by COMPLEX1:22
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,-(Im z)|^ 3+3*(Re z)^2
            *Im z*]+[*c,0*]*z+[*d,0*] by Th18
         .= [*a,0*]*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:8
         .=[*Re [*a,0*] *Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]-Im [*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:def 10
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0*(-y|^ 3+3*x^2*y),
          Re[*a,0*]*(-y|^ 3+3*x^2*y)+(x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*0 *]+[*c,0*]*[*Re z,Im z*]+[*d,0*]
                              by COMPLEX1:7
         .= [*a*(x|^ 3 - 3*x*y^2),a*(-y|^ 3+3*x^2*y)*]
            +[*c*Re z-0*Im z,c*Im z+Re z*0*]+[*d,0*] by Th5
         .= [*a*(x|^ 3 - 3*x*y^2)+c*x,a*(-y|^ 3+3*x^2*y)
            +c*y*]+[*d,0*] by COMPLFLD:2
         .= [*a*(x|^ 3 - 3*x*y^2)+c*x+d,a*(-y|^ 3+3*x^2*y)
            +c*y+0*] by COMPLFLD:2
         .= [*a*(x|^ 3 - 3*x*0)+c*x+d,a*(-0|^ 3+0)
            +0+0*] by SQUARE_1:60,A102
         .= [*a*(x|^ 3 - 0)+c*x+d,a*(-0)*] by NEWTON:16
         .= [*a*x|^ 3 +c*x+d,0*];then
            a"*(a*x|^ 3+c*x+d) = a"*0 by ARYTM_0:def 7;then
            a"*(x|^ 3*a)+a"*(c*x)+a"*d = 0 by XCMPLX_1:9;then
            x|^ 3*a"*a+a"*(c*x)+a"*d = 0 by XCMPLX_1:4;then
            x|^ 3*a"*a+a"*c*x+a"*d = 0 by XCMPLX_1:4;then
            x|^ 3*(a*a")+a"*c*x+a"*d = 0 by XCMPLX_1:4;then
            x|^ 3*(a/a)+a"*c*x+a"*d = 0 by XCMPLX_0:def 9;then
            x|^ 3+c*a"*x+d*a" = 0
  by XCMPLX_1:89,A100;then
            x|^ 3+(c/a)*x+d*a" = 0 by XCMPLX_0:def 9;then
            1*x|^ 3+0*x^2+(c/a)*x+d/a = 0 by XCMPLX_0:def 9;then
 A120:      Poly3(1,0,c/a,d/a,x) = 0 by POLYEQ_1:def 4;
 A121:    x = 3-root(-(d/a)/2+sqrt((d/a)^2/4+((c/a)/3) |^ 3))
             +3-root(-(d/a)/2-sqrt((d/a)^2/4+((c/a)/3) |^ 3)) or
          x = 3-root(-(d/a)/2+sqrt((d/a)^2/4+((c/a)/3) |^ 3))
             +3-root(-(d/a)/2+sqrt((d/a)^2/4+((c/a)/3) |^ 3)) or
          x = 3-root(-(d/a)/2-sqrt((d/a)^2/4+((c/a)/3) |^ 3))
             +3-root(-(d/a)/2-sqrt((d/a)^2/4+((c/a)/3) |^ 3))
           by A120,A104,POLYEQ_1:19;
 A122:   -(d/a)/2 = -1/2*(d/a) by XCMPLX_1:106
                 .= -d/(a*2) by XCMPLX_1:104;
 A123:   (d/a)^2/4=d^2/a^2/4 by SQUARE_1:69
                 .=1/4*(d^2/a^2) by XCMPLX_1:106
                 .=d^2/(a^2*4) by XCMPLX_1:104;
         (c/a)/3 =1/3*(c/a) by XCMPLX_1:106
                .=c/(a*3) by XCMPLX_1:104;
      hence thesis by A121,A122,A123,A108;
end;

theorem
    a<>0 & Poly_3(a,0,c,d,z)=0c & Im z <> 0 implies 
    (for u,v st  Re z = u+v & 3*v*u+c/(4*a)=0 & c/a>=0 holds
          z=[*3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)),
             sqrt(3*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             + 3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))^2+c/a)*]
       or z=[*3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)),
             -sqrt(3*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))^2+c/a)*]
       or z=[*2*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))),
             sqrt(3*(2*(3-root(d/(16*a)+sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*]
       or z=[*2*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))),
             -sqrt(3*(2*(3-root(d/(16*a)+sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*]
       or z=[*2*(3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))),
             sqrt(3*(2*(3-root(d/(16*a)-sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*]
       or z=[*2*(3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))),
             -sqrt(3*(2*(3-root(d/(16*a)-sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*]
          )
proof
        assume
 A100:   a <> 0;
        assume
 A102:   Poly_3(a,0,c,d,z) = 0c  & Im z <> 0;
        let u,v;
        assume
 A104:   Re z=u+v & 3*v*u+c/(4*a)=0 &  c/a>=0;
         set x=Re z;
         set y=Im z;
         set p=c/(4*a);
         1/4*(c/a)>=0 by REAL_2:121,A104;then
 A106:   x = u+v & 3*v*u+p = 0 &  p >= 0 by A104,XCMPLX_1:104;
 A105:   y <>0 by A102;
 A108:   z = [*x,y*] by COMPLEX1:8;
  P:     0c=a*(z^3)+0*z^2+c*z+d by Def8,A102
         .=[*a,0*]*(z^3)+0*z^2+c*z+d by Lem1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+c*z+d by Lem1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+[*c,0*]*z+d by Lem1
         .=[*a,0*]*(z^3)+[*0,0*]*z^2+[*c,0*]*z+[*d,0*] by Lem2;
 A114:   0 = 0c by COMPLEX1:def 6
          .= [*a,0*]*([*Re z^3,Im z^3*])+[*0,0*]*z^2+[*c,0*]*z+[*d,0*]
                        by COMPLEX1:8,P
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +[*0,0*]*z^2+[*c,0*]*z+[*d,0*] by Th18
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +0c+[*c,0*]*z+[*d,0*] by COMPLEX1:28,XXX
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
            +[*c,0*]*z+[*d,0*] by COMPLEX1:22
         .= [*a,0*]*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,-(Im z)|^ 3+3*(Re z)^2
            *Im z*]+[*c,0*]*z+[*d,0*] by Th18
         .= [*a,0*]*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:8
         .=[*Re [*a,0*] *Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]-Im [*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:def 10
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*Re [*a,0*] *(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-Im [*a,0*]
           *(-y|^ 3+3*x^2*y),Re[*a,0*]*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0*(-y|^ 3+3*x^2*y),
          Re[*a,0*]*(-y|^ 3+3*x^2*y)+(x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*Im[*a,0*] *]
               +[*c,0*]*[*Re z,Im z*]+[*d,0*] by COMPLEX1:7
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*0 *]+[*c,0*]*[*Re z,Im z*]+[*d,0*]
                              by COMPLEX1:7
         .= [*a*(x|^ 3 - 3*x*y^2),a*(-y|^ 3+3*x^2*y)*]
            +[*c*Re z-0*Im z,c*Im z+Re z*0*]+[*d,0*] by Th5
         .= [*a*(x|^ 3 - 3*x*y^2)+c*x,a*(-y|^ 3+3*x^2*y)
            +c*y*]+[*d,0*] by COMPLFLD:2
         .= [*a*(x|^ 3 - 3*x*y^2)+c*x+d,a*(-y|^ 3+3*x^2*y)
            +c*y+0*] by COMPLFLD:2;then
I:        a*(-y|^ 3+3*x^2*y) +c*y+0 = 0 by ARYTM_0:26; then
          a*(-y|^(2+1)+3*x^2*y)+c*y = 0;then
          a*(-y|^2*y+3*x^2*y)+c*y = 0 by NEWTON:11;then
          a*((-y|^2)*y+(3*x^2)*y+0*y)+c*y = 0 by XCMPLX_1:175;then
          a*((-y|^2+3*x^2+0)*y)+c*y = 0 by XCMPLX_1:9;then
          (a*(-y|^2+3*x^2+0))*y+c*y+0*y = 0 by XCMPLX_1:4;then
