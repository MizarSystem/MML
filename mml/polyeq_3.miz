Th50:
  for n be Nat st n>0 holds 0 |^ n = 0
proof
  let n be Nat; assume n>0; then
  n >= 0+1 by NAT_1:13;
  hence thesis by NEWTON:16;
end;

theorem Th53:
  for x being real number, n be Element of NAT holds
  (cos x + sin x*<i>) |^ n = cos (n*x) + sin (n*x)*<i>
proof
  let x be real number;
  defpred P[Element of NAT] means
   (cos x +sin x*<i>)|^ $1 = cos ($1*x)+sin ($1*x)*<i>;
A1: P[0] by NEWTON:9,SIN_COS:34;
A2: now
    let n be Element of NAT;
    assume P[n];
    then (cos x + sin x*<i>) |^ (n+1)
    = (cos(n*x)+sin(n*x)*<i>)*(cos x+(sin x)*<i>) by NEWTON:11
      .= cos(n*x)*cos x - sin(n*x)*sin x+ (cos(n*x)*sin x+cos x*sin(n*x))*<i>
      .= cos(n*x+x)+(cos(n*x)*sin x+cos x*sin(n*x))*<i> by SIN_COS:80
      .= cos((n+1)*x)+sin((n+1)*x)*<i> by SIN_COS:80;
    hence P[n+1];
  end;
  thus for n be Element of NAT holds P[n] from NAT_1:sch 1(A1,A2);
end;

theorem
  for z being Element of COMPLEX
  for n being Element of NAT st z <> 0 or n > 0 holds
  z|^ n = (|.z.| to_power n)*cos (n*Arg z) +
  (|.z.| to_power n)*sin (n*Arg z)*<i>
proof
  let z be Element of COMPLEX;
  let n be Element of NAT;
  assume
A1: z <> 0 or n > 0;
  per cases by A1;
  suppose z <> 0;
    set x=Arg z;
    z|^ n
      =(|.z.|*cos Arg z-0*sin Arg z+(|.z.|*sin Arg z+cos Arg z*0)*<i>)|^ n
    by COMPLEX2:19
      .=(|.z.|*(cos Arg z+sin Arg z*<i>))|^ n
      .= |.z.||^ n*(cos Arg z+sin Arg z*<i>)|^ n by NEWTON:12;
    hence z|^ n
      = (|.z.| to_power n)*(cos (n*Arg z)+sin (n*Arg z)*<i>) by Th53
      .= (|.z.| to_power n)*cos(n*Arg z)+(|.z.| to_power n)*sin(n*Arg z)*<i>;
  end;
  suppose
A5: z=0 & n > 0;
    then 0=|.z.| to_power n by COMPLEX1:130,POWER:def 2;
    hence thesis by A5,Th50;
  end;
end;

theorem Th55:
  for n,k be Element of NAT,x be Real st n <> 0 holds
  (cos((x+2*PI*k)/n) + sin((x+2*PI*k)/n)*<i>)|^ n = cos x + (sin x)*<i>
proof
  let n,k be Element of NAT,x be Real;
  assume
A1: n <> 0;
  thus (cos((x+2*PI*k)/n)+ sin((x+2*PI*k)/n)*<i>)|^ n
  = cos (n*((x+2*PI*k)/n))+sin (n*((x+2*PI*k)/n))*<i> by Th53
    .= cos(x+2*PI*k)+sin(n*((x+2*PI*k)/n))*<i> by A1,XCMPLX_1:88
    .= cos(x+2*PI*k)+sin(x+2*PI*k)*<i> by A1,XCMPLX_1:88
    .= cos.(x+2*PI*k)+sin(x+2*PI*k)*<i> by SIN_COS:def 23
    .= cos.(x+2*PI*k)+(sin.(x+2*PI*k))*<i> by SIN_COS:def 21
    .= cos.(x+2*PI*k)+(sin.x)*<i> by SIN_COS2:10
    .= cos.x + (sin.x)*<i> by SIN_COS2:11
    .= cos.x + (sin x)*<i> by SIN_COS:def 21
    .= cos x + (sin x)*<i> by SIN_COS:def 23;
end;

theorem Th56:
  for z be complex number for n,k be Element of NAT st n <> 0 holds
  z = ((n-root |.z.|)*cos((Arg z+2*PI*k)/n) +
  (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*<i>)|^ n
proof
  let z be complex number;
  let n,k be Element of NAT;
  assume
A1: n <> 0;
A3: n >= 0+1 by NAT_1:13,A1;
  per cases;
  suppose z <> 0;
    then
A4: |.z.| > 0 by COMPLEX1:133;
    thus ((n-root |.z.|)*cos((Arg z+2*PI*k)/n)+
    (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*<i>)|^ n
      = ((n-root |.z.|)*
    (cos((Arg z+2*PI*k)/n)+sin((Arg z+2*PI*k)/n)*<i>))|^ n
      .= ((n-root |.z.|)|^ n)
    *((cos((Arg z+2*PI*k)/n)+sin((Arg z+2*PI*k)/n)*<i>)|^ n) by NEWTON:12
      .= ((n-root |.z.|)|^ n)*(cos Arg z+(sin Arg z)*<i>) by A1,Th55
      .= |.z.|*(cos Arg z+(sin Arg z)*<i>) by A1,A4,COMPTRIG:17
      .= |.z.|*cos Arg z-0*sin Arg z+(|.z.|*sin Arg z+0*cos Arg z)*<i>
      .= z by COMPLEX2:19;
  end;
  suppose
A6: z = 0;
    hence ((n-root |.z.|)*cos((Arg z+2*PI*k)/n)+
    (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*<i>)|^ n
       =((0*cos((Arg z+2*PI*k)/n)+
    (n-root 0)*sin((Arg z+2*PI*k)/n)*<i>)|^ n) by A3,COMPLEX1:130,POWER:6
      .= (0*sin((Arg z+2*PI*k)/n)*<i>)|^ n by A3,POWER:6
      .= z by A1,A6,Th50;
  end;
end;

theorem
  for z be Element of COMPLEX, n be non empty Element of NAT,
  k be Element of NAT holds (n-root |.z.|)*cos((Arg z+2*PI*k)/n)+
  (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*<i> is CRoot of n,z
proof
  let z be Element of COMPLEX,
  n be non empty Element of NAT,k be Element of NAT;
  ((n-root |.z.|)*cos((Arg z+2*PI*k)/n)+
  (n-root |.z.|)*sin((Arg z+2*PI*k)/n)*<i>)|^ n=z by Th56;
  hence thesis by COMPTRIG:def 2;
end;

theorem
  for z be complex number, v be CRoot of 1,z holds v = z
proof
  let z be complex number, v be CRoot of 1,z;
  v|^ 1 = z by COMPTRIG:def 2;
  hence thesis by NEWTON:10;
end;

theorem
  for n be non empty Nat, v be CRoot of n,0 holds v = 0
proof
  let n be non empty Nat, v be CRoot of n,0;
  defpred P[Nat] means v|^ $1 = 0;
A1: ex n be non empty Nat st P[n]
  proof
    take n;
    thus thesis by COMPTRIG:def 2;
  end;
A2: now
    let k be non empty Nat;
    assume that
A3: k <> 1 and
A4: P[k];
    consider t be Nat such that
A5: k = t+1 by NAT_1:6;
    reconsider t as non empty Nat by A3,A5;
    take t;
    thus t < k by A5,NAT_1:13;
    v|^ k = (v|^ t)*v by A5,NEWTON:11;
    then v|^ t = 0 or v = 0 by A4;
    hence P[t] by Th50;
  end;
  P[1] from COMPTRIG:sch 1(A1,A2);
  hence thesis by NEWTON:10;
end;

theorem
  for n be non empty Element of NAT, z be complex number
  for v be CRoot of n,z st v = 0 holds z = 0
proof
  let n be non empty Element of NAT, z be complex number;
  let v be CRoot of n,z;
  assume v = 0;
  then 0|^ n = z by COMPTRIG:def 2;
  hence thesis by Th50;
end;

theorem
  for n be non empty Element of NAT, k be Element of NAT holds
  cos(2*PI*k/n) + sin(2*PI*k/n)*<i> is CRoot of n,1
proof
  let n be non empty Element of NAT,k be Element of NAT;
  (cos(2*PI*k/n)+sin(2*PI*k/n)*<i>)|^n
  =cos(n*((2*PI*k)/n))+sin(n*((2*PI*k)/n))*<i> by Th53
    .=cos(2*PI*k)+(sin(n*((2*PI*k)/n)))*<i> by XCMPLX_1:88
    .=cos(2*PI*k+0)+(sin(2*PI*k+0))*<i> by XCMPLX_1:88
    .=cos.(2*PI*k+0)+(sin(2*PI*k+0))*<i> by SIN_COS:def 23
    .=cos.(2*PI*k+0)+(sin.(2*PI*k+0))*<i> by SIN_COS:def 21
    .=cos.(2*PI*k+0)+(sin.0)*<i> by SIN_COS2:10
    .=1 by SIN_COS:33,SIN_COS2:11;
  hence thesis by COMPTRIG:def 2;
end;

canceled;

theorem
  for z,s being Element of COMPLEX, n being Element of NAT st
  s<>0 & z<>0 & n>=1 & s|^ n=z|^ n holds |.s.| = |.z.|
proof
  let z,s be Element of COMPLEX, n be Element of NAT;
  assume
A1: s<>0 & z<>0 & n>=1 & s|^ n=z|^ n;
  then
A2: |.s.|>0 & |.z.|>0 by COMPLEX1:133;
  z|^ n = (|.s.|*cos Arg s + |.s.|*sin Arg s *<i>)|^ n by A1,COMPLEX2:19
    .=(|.s.|*(cos Arg s+sin Arg s *<i>))|^ n
    .=(|.s.||^ n)*((cos Arg s+sin Arg s *<i>)|^ n) by NEWTON:12
    .=(|.s.| to_power n)*(cos (n*Arg s)+sin (n*Arg s)*<i>) by Th53
    .= ((|.s.| to_power n)*cos (n*Arg s)
  )+((|.s.| to_power n)*sin (n*Arg s))*<i>;
  then ((|.s.| to_power n)*cos (n*Arg s))
  +((|.s.| to_power n)*sin (n*Arg s ))*<i>
  = (|.z.|*cos Arg z + |.z.|*sin Arg z *<i>)|^ n by COMPLEX2:19
    .=(|.z.|*(cos Arg z+sin Arg z *<i>))|^ n
    .=(|.z.||^ n)*((cos Arg z+sin Arg z *<i>)|^ n) by NEWTON:12
    .=(|.z.| to_power n)*(cos (n*Arg z)+sin (n*Arg z)*<i>) by Th53
    .= ((|.z.| to_power n)*cos (n*Arg z))+((|.z.| to_power n)
  *sin (n*Arg z))*<i>;
  then (|.s.| to_power n)*cos (n*Arg s)=(|.z.| to_power n)*cos (n*Arg z)
  &(|.s.| to_power n)*sin (n*Arg s)
  =(|.z.| to_power n)*sin (n*Arg z) by COMPLEX1:163;
  then ((|.s.| to_power n))^2*(cos (n*Arg s))^2+
  ((|.s.| to_power n)*sin (n*Arg s))^2 =((|.z.| to_power n)*cos (n*Arg z))^2+
  ((|.z.| to_power n)*sin (n*Arg z))^2 by SQUARE_1:68;
  then ((|.s.| to_power n))^2*((cos (n*Arg s))^2+(sin (n*Arg s))^2)
  =((|.z.| to_power n))^2*((cos (n*Arg z))^2+(sin (n*Arg z))^2);
  then ((|.s.| to_power n))^2*((cos (n*Arg s))^2+(sin (n*Arg s))^2)
  =((|.z.| to_power n))^2*1 by SIN_COS:32;
  then
A3: ((|.s.| to_power n))^2*1=((|.z.| to_power n))^2 by SIN_COS:32;
A4: |.s.| to_power n > 0 & |.z.| to_power n > 0 by A2,POWER:39;
  then (|.s.| to_power n) =sqrt ((|.z.| to_power n))^2 by A3,SQUARE_1:89;
  then |.s.| |^ n=|.z.| |^ n by A4,SQUARE_1:89;
  then |.s.| = n-root (|.z.| |^ n) by A1,A2,POWER:5;
  hence thesis by A1,A2,POWER:5;
end;

