theorem
  b<>0 & delta(b,c,d)>=0 & Polynom(0,b,c,d,z)=0 implies
  z = (-c+sqrt delta(b,c,d))/(2*b) or z = (-c-sqrt delta(b,c,d))/(2*b) or
  z = -c/(2*b)
proof
  assume
A1: b<>0 & delta(b,c,d)>=0 & Polynom(0,b,c,d,z)=0;
  then Polynom(b,c,d,z)=0;
  hence thesis by A1,Th4;
end;

theorem
  b<>0 & delta(b,c,d)<0 & Polynom(0,b,c,d,z)=0 implies
  z = -c/(2*b)+(sqrt (-delta(b,c,d))/(2*b))*<i> or
  z = -c/(2*b)+(-sqrt (-delta(b,c,d))/(2*b))*<i>
proof
  assume
A1: b<>0 & delta(b,c,d)<0 & Polynom(0,b,c,d,z)=0;
  then Polynom(b,c,d,z)=0;
  hence thesis by A1,Th5;
end;

theorem
  a<>0 & 4*a*c <= 0 & Polynom(a,0,c,0,z)=0 implies z = (sqrt -4*a*c)/(2*a) or
  z = (-sqrt -4*a*c)/(2*a) or z = 0
proof
  assume
A1: a<>0 & 4*a*c <= 0 & Polynom(a,0,c,0,z)=0;
  then (a*z^2+c)*z = 0;
  then
A2: Polynom(a,0,c,z) = 0 or z = 0;
  z = (-0+sqrt delta(a,0,c))/(2*a)
  or z = (-0-sqrt delta(a,0,c))/(2*a) or z = 0 or z = 0/(2*a) by A1,A2,Th4;
  hence thesis;
end;

theorem
  a<>0 & delta(a,b,c)>=0 & Polynom(a,b,c,0,z)=0 implies
  z = (-b+sqrt delta(a,b,c))/(2*a) or z = (-b-sqrt delta(a,b,c))/(2*a) or
  z = -b/(2*a) or z = 0
proof
  assume
A1: a<>0 & delta(a,b,c)>=0 & Polynom(a,b,c,0,z)=0;
  then (a*z^2+b*z+c)*z = 0;
  then Polynom(a,b,c,z) = 0 or z = 0;
  hence thesis by A1,Th4;
end;

theorem
  a<>0 & delta(a,b,c)<0 & Polynom(a,b,c,0,z)=0 implies
  z= -b/(2*a)+(sqrt (-delta(a,b,c))/(2*a))*<i> or
  z= -b/(2*a)+(-sqrt (-delta(a,b,c))/(2*a))*<i> or z = 0
proof
  assume
A1: a<>0 & delta(a,b,c)<0 & Polynom(a,b,c,0,z)=0;
  then (a*z^2+b*z+c)*z = 0;
  then Polynom(a,b,c,z) = 0 or z = 0;
  hence thesis by A1,Th5;
end;

theorem Th20:
  y^2 = a implies y = sqrt a or y = -sqrt a
proof
  assume
A1: y^2 = a;
  then
A2: Polynom(1,0,-a,y) =0;
A3: a >= 0 by A1,XREAL_1:65;
  y=(-0+sqrt delta(1,0,-a))/(2*1) or
  y=(-0-sqrt delta(1,0,-a))/(2*1) by A2,POLYEQ_1:5,A3;
  then y=(sqrt (4*a))/2 or y=(0-sqrt (4*a))/2;
  then y= sqrt a * 2 /2 or y=(-(2 * sqrt a))/2 by A3,SQUARE_1:85,97;
  hence thesis;
end;

theorem
  a<>0 & Im z = 0 & Polynom(a,0,c,d,z)=0 implies
  (for u,v st Re z = u+v & 3*v*u+c/a=0 holds
  z = 3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
  +3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
  or z = 3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
  +3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
  or z = 3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
  +3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3)) )
proof
  assume
A1: a <> 0;
  assume
A2: Im z = 0 & Polynom(a,0,c,d,z)=0;
  let u,v;
  assume
A3: Re z=u+v & 3*v*u+c/a=0;
  set x=Re z;
  set y=Im z;
A5: a = a+0*<i>;
A7: z = Re z+(Im z)*<i> by COMPLEX1:29
    .= x by A2;
  0 = a*(Re z^3+(Im z^3)*<i>)+0*z^2+c*z+d by A2,COMPLEX1:29
    .= a*(((Re z)|^ 3 - 3*Re z*(Im z)^2)+(Im z^3)*<i>) +0*z^2+c*z+d by Th11
    .= a*(((Re z)|^ 3 - 3*Re z*(Im z)^2)+(-(Im z)|^ 3+3*(Re z)^2
  *Im z)*<i>)+c*z+d by Th11
    .= (a+0*<i>)*((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)
  +c*(Re z+Im z*<i>)+d by COMPLEX1:29
    .=Re a *Re((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)-Im a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+(Re a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+
  Re((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)*Im a)*<i>
  +c*(Re z+Im z*<i>)+d by COMPLEX1:def 10
    .=(Re a *(x|^ 3 - 3*x*y^2)-Im a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>))+(Re a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+
  Re((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)*Im a )*<i>
  +c*(Re z+Im z *<i>)+d by COMPLEX1:28
    .=(Re a *(x|^ 3 - 3*x*y^2)-Im a*(-y|^ 3+3*x^2*y))+(Re a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+
  Re((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)*Im a )*<i>
  +c*(Re z+Im z *<i>)+d by COMPLEX1:28
    .=(Re a *(x|^ 3 - 3*x*y^2)-Im a*(-y|^ 3+3*x^2*y))+(Re a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+
  (x|^ 3 - 3*x*y^2)*Im a )*<i> +c*(Re z+Im z *<i>)+d by COMPLEX1:28
    .=(Re a *(x|^ 3 - 3*x*y^2)-Im a *(-y|^ 3+3*x^2*y))+(Re a*(-y|^ 3+3*x^2*y)+
  (x|^ 3 - 3*x*y^2)*Im a )*<i> +c*(Re z+Im z *<i>)+d by COMPLEX1:28
    .=(a*(x|^ 3 - 3*x*y^2)-Im a *(-y|^ 3+3*x^2*y))+(Re a*(-y|^ 3+3*x^2*y)+
  (x|^ 3 - 3*x*y^2)*Im a )*<i> +c*(Re z+Im z *<i>)+d by A5,COMPLEX1:28
    .=(a*(x|^ 3 - 3*x*y^2)-0*(-y|^ 3+3*x^2*y))+(
  Re a*(-y|^ 3+3*x^2*y)+(x|^ 3 - 3*x*y^2)*Im a )*<i>
  +c*(Re z+Im z *<i>)+d by A5,COMPLEX1:28
    .=(a*(x|^ 3 - 3*x*y^2)-0)+(a*(-y|^ 3+3*x^2*y) +(x|^ 3 - 3*x*y^2)*Im a )*<i>
  +c*(Re z+Im z *<i>)+d by A5,COMPLEX1:28
    .=(a*(x|^ 3 - 3*x*y^2)-0)+(a*(-y|^ 3+3*x^2*y)
  +(x|^ 3 - 3*x*y^2)*0 )*<i>+c*(Re z+Im z *<i>)+d by A5,COMPLEX1:28
    .= a*(x|^ 3 - 0)+c*x+d+(a*(-0))*<i> by NEWTON:16,A2
    .= a*x|^ 3 +c*x+d;
  then a"*(a*x|^ 3+c*x+d) = a"*0;
  then x|^ 3*(a/a)+a"*c*x+a"*d = 0;
  then
A8: Polynom(1,0,c/a,d/a,x) = 0 by A1,XCMPLX_1:89;
A9: -(d/a)/2 = -1/2*(d/a)
    .= -d/(a*2) by XCMPLX_1:104;
A10: (d/a)^2/4 =1/4*(d^2/a^2) by XCMPLX_1:77
    .=d^2/(a^2*4) by XCMPLX_1:104;
  (c/a)/3 =1/3*(c/a)
    .=c/(a*3) by XCMPLX_1:104;
  hence thesis by A3,A7,A8,A9,A10,POLYEQ_1:19;
end;

theorem
  a<>0 & Im z <> 0 & Polynom(a,0,c,d,z)=0 implies
  (for u,v st Re z = u+v & 3*v*u+c/(4*a)=0 & c/a>=0 holds
  z=3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
  +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))+
  sqrt(3*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
  + 3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))^2+c/a)*<i>
  or z=3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
  +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
  -sqrt(3*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
  +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))^2+c/a)*<i>
  or z=2*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))+
  sqrt(3*(2*(3-root(d/(16*a)+sqrt((d/(16*a))^2 +(c/(12*a)) |^ 3))))^2+c/a)*<i>
  or z=2*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))
  -sqrt(3*(2*(3-root(d/(16*a)+sqrt((d/(16*a))^2
  +(c/(12*a)) |^ 3))))^2+c/a)*<i>
  or z=2*(3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))+
  sqrt(3*(2*(3-root(d/(16*a)-sqrt((d/(16*a))^2 +(c/(12*a)) |^ 3))))^2+c/a)*<i>
  or z=2*(3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))
  -sqrt(3*(2*(3-root(d/(16*a)-sqrt((d/(16*a))^2
  +(c/(12*a)) |^ 3))))^2+c/a)*<i>)
proof
  assume
A1: a <> 0;
  assume
A2: Im z <> 0 & Polynom(a,0,c,d,z)=0;
  let u,v;
  assume
A3: Re z=u+v & 3*v*u+c/(4*a)=0 & c/a>=0;
  set x=Re z;
  set y=Im z;
A5: a = a+0*<i>;
A7: z = x+y*<i> by COMPLEX1:29;
A8: 0 = a*(Re z^3+(Im z^3)*<i>)+0*z^2+c*z+d by A2,COMPLEX1:29
    .= a*(((Re z)|^ 3 - 3*Re z*(Im z)^2)+(Im z^3)*<i>) +c*z+d by Th11
    .= a*(((Re z)|^ 3 - 3*Re z*(Im z)^2)+(-(Im z)|^ 3+3*(Re z)^2
  *Im z)*<i>)+c*z+d by Th11
    .= (a+0*<i>)*((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)
  +c*(Re z+(Im z)*<i>)+d by COMPLEX1:29
    .=Re a *Re((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)-Im a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+(Re a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+
  Re((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)*Im a)*<i>
  +c*(Re z+Im z *<i>)+d by COMPLEX1:def 10
    .=(Re a *(x|^ 3 - 3*x*y^2)-Im a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>))+(Re a
  *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+
  Re((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)*Im a )*<i>
  +c*(Re z+Im z *<i>)+d by COMPLEX1:28
    .=(Re a *(x|^ 3 - 3*x*y^2)-Im a
  *(-y|^ 3+3*x^2*y))+(Re a *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+
  Re((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)*Im a )*<i>
  +c*(Re z+Im z *<i>)+d by COMPLEX1:28
    .=(Re a *(x|^ 3 - 3*x*y^2)-Im a
  *(-y|^ 3+3*x^2*y))+(Re a *Im((x|^ 3 - 3*x*y^2)+(-y|^ 3+3*x^2*y)*<i>)+
  (x|^ 3 - 3*x*y^2)*Im a )*<i> +c*(Re z+Im z *<i>)+d by COMPLEX1:28
    .=(Re a *(x|^ 3 - 3*x*y^2)-Im a *(-y|^ 3+3*x^2*y))+(Re a*(-y|^ 3+3*x^2*y)+
  (x|^ 3 - 3*x*y^2)*Im a )*<i> +c*(Re z+Im z *<i>)+d by COMPLEX1:28
    .=(a*(x|^ 3 - 3*x*y^2)-Im a *(-y|^ 3+3*x^2*y))+(Re a*(-y|^ 3+3*x^2*y)+
  (x|^ 3 - 3*x*y^2)*Im a)*<i> +c*(Re z+Im z *<i>)+d by A5,COMPLEX1:28
    .=(a*(x|^ 3 - 3*x*y^2)-0*(-y|^ 3+3*x^2*y))+(
  Re a*(-y|^ 3+3*x^2*y)+(x|^ 3 - 3*x*y^2)*Im a )*<i>
  +c*(Re z+Im z *<i>)+d by A5,COMPLEX1:28
    .=(a*(x|^ 3 - 3*x*y^2)-0)+(a*(-y|^ 3+3*x^2*y) +(x|^ 3 - 3*x*y^2)*Im a )*<i>
  +c*(Re z+Im z *<i>)+d by A5,COMPLEX1:28
    .=(a*(x|^ 3 - 3*x*y^2)-0)+(a*(-y|^ 3+3*x^2*y)
  +(x|^ 3 - 3*x*y^2)*0 )*<i>+c*(Re z+Im z *<i>)+d by A5,COMPLEX1:28
    .= a*(x|^ 3 - 3*x*y^2)+c*x+d+(a*(-y|^ 3+3*x^2*y) +c*y+0)*<i>;
  then
A9: a*(-y|^ 3+3*x^2*y) +c*y+0 = 0 by COMPLEX1:12,28;
  a*(-y|^(2+1)+3*x^2*y)+c*y = 0 by A8,COMPLEX1:12,28;
  then a*(-y|^2*y+3*x^2*y)+c*y = 0 by NEWTON:11;
  then (a*(-y|^2+3*x^2)+c+0)*y = 0;
  then (a*(y|^2))+(-a*(y|^2)+(a*(3*x^2)+c))=(a*(y|^2))+0 by A2,XCMPLX_1:6;
  then y|^(1+1) = (a*(3*x^2)+c)/a by A1,XCMPLX_1:90;
  then y|^1*y = (a*(3*x^2)+c)/a by NEWTON:11;
  then
A10: y^2 = (3*x^2)*a/a+c/a+0/a by NEWTON:10;
  then
A11: y^2 = 3*x^2+c/a+0*a" by A1,XCMPLX_1:90;
  a*(x|^ 3 - 3*x*(3*x^2+c/a)+0)+c*x+d = 0 by A1,A8,A9,A10,XCMPLX_1:90;
  then 0 =a*x|^ 3 - a*(9*(x*x^2)+(3*x)*(c/a))+c*x+d
    .=a*x|^ 3 - a*(9*(x|^1*x*x)+(3*x)*(c/a))+c*x+d by NEWTON:10
    .=a*x|^ 3 - a*(9*(x|^(1+1)*x)+(3*x)*(c/a))+c*x+d by NEWTON:11
    .=a*x|^ 3 - a*(9*(x|^(2+1))+(3*x)*(c/a)+0)+c*x+d by NEWTON:11
    .=a*x|^ 3 - (a*(9*(x|^3))+(3*x)*(c*(a/a)))+c*x+d
    .=a*x|^ 3 - (a*(9*(x|^3))+(3*x)*c)+c*x+d by A1,XCMPLX_1:89
    .=(-8*a)*x|^3+(-2*c)*x+d;
  then (-1)*0=(8*a)*x|^3+(2*c)*x+(-d);
  then
A12: 0=x|^3*((8*a)/(8*a))+(8*a)"*((2*c)*x)+(8*a)"*(-d);
  0=1*x|^3+0*x^2+((2*c)/(8*a))*x+((8*a)"*(-d)) by A12,XCMPLX_1:89,A1;
  then 0=1*x|^3+0*x^2+((2/8*c)/a)*x+((-d)/(8*a)) by XCMPLX_1:84;
  then 0=1*x|^3+0*x^2+(1/a*(c/4))*x+((-d)/(8*a));
  then
A13: Polynom(1,0,c/(4*a),-d/(8*a),x) = 0 by XCMPLX_1:104;
  set pp = c/(4*a);
  set q = -d/(8*a);
  -q/2=1/2*(d/(8*a));
  then
A14: -q/2=d/(a*8*2) by XCMPLX_1:104;
A15: q^2/4=(1/2*(d/(8*a)))^2;
A16: x = 3-root(-q/2+sqrt(q^2/4+(pp/3) |^ 3)) +
  3-root(-q/2-sqrt(q^2/4+(pp/3) |^ 3)) or
  x = 3-root(-q/2+sqrt(q^2/4+(pp/3) |^ 3)) +
  3-root(-q/2+sqrt(q^2/4+(pp/3) |^ 3)) or
  x = 3-root(-q/2-sqrt(q^2/4+(pp/3) |^ 3)) +
  3-root(-q/2-sqrt(q^2/4+(pp/3) |^ 3)) by A3,A13,POLYEQ_1:19;
  set m = 3-root(-q/2+sqrt(q^2/4+(pp/3) |^ 3));
  set n = 3-root(-q/2-sqrt(q^2/4+(pp/3) |^ 3));
A17: z=m+n+sqrt(3*(m + n)^2+c/a)*<i> or z=m+n-sqrt(3*(m + n)^2+c/a)*<i>
  or z=2*m+sqrt(3*(2*m)^2+c/a)*<i> or z=2*m-sqrt(3*(2*m)^2+c/a)*<i>
  or z=2*n+sqrt(3*(2*n )^2+c/a)*<i> or z=2*n-sqrt(3*(2*n )^2+c/a)*<i>
  proof
    now per cases by A16;
      case x = m + n;
        hence z=m+n+sqrt(3*(m + n )^2+c/a)*<i>
        or z=m+n+(-sqrt(3*(m + n )^2+c/a))*<i> by A7,A11,Th20;
      end;
      case x = 2*m;
        hence z=2*m+sqrt(3*(2*m)^2+c/a)*<i>
        or z=2*m+(-sqrt(3*(2*m)^2+c/a))*<i> by A7,A11,Th20;
      end;
      case x = 2*n;
        hence z=2*n+sqrt(3*(2*n )^2+c/a)*<i>
        or z=2*n+(-sqrt(3*(2*n )^2+c/a))*<i> by A7,A11,Th20;
      end;
    end;
    hence thesis;
  end;
  (c/(4*a))/3=1/3*(c/(4*a))
    .=c/(a*4*3) by XCMPLX_1:104
    .=c/(a*(4*3));
  hence thesis by A14,A15,A17;
end;

theorem
  a<>0 & Polynom(a,b,c,d,z)=0 & Im z=0 implies
  (for u,v,x1 st x1=Re z + b/(3*a) &
  Re z=u+v-b/(3*a) & 3*u*v+(3*a*c-b^2)/(3*a^2)=0 holds
  z =3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
  +sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
  +((3*a*c-b^2)/(9*a^2)) |^ 3)) + 3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
  -sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
  +((3*a*c-b^2)/(9*a^2)) |^ 3))-b/(3*a)+0*<i> or
  z =3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
  +sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
  +((3*a*c-b^2)/(9*a^2)) |^ 3)) + 3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
  +sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
  +((3*a*c-b^2)/(9*a^2)) |^ 3))-b/(3*a)+0*<i> or
  z =3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
  -sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
  +((3*a*c-b^2)/(9*a^2)) |^ 3)) + 3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
  -sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
  +((3*a*c-b^2)/(9*a^2)) |^ 3))-b/(3*a)+0*<i> )
proof
  assume
A1: a<>0;
  assume
A2: Polynom(a,b,c,d,z)=0 & Im z=0;
