environ

 vocabularies COMPLEX1, ARYTM_3, ARYTM_1, POLYEQ_1, SQUARE_1, FUNCT_3,
      POLYEQ_3, RELAT_1, POWER, GROUP_1, PREPOWER, FUNCT_1, SIN_COS, COMPTRIG,
      XCMPLX_0, ARYTM;
 notations SUBSET_1, NUMBERS, XBOOLE_0, ORDINAL1, XCMPLX_0, XXREAL_0, REAL_1,
      COMPLEX1, POLYEQ_1, SQUARE_1, NEWTON, ARYTM_0, QUIN_1, POWER, NAT_1,
      SIN_COS, PREPOWER, BINARITH, SEQ_1, COMPTRIG;
 constructors FUNCT_4, ARYTM_0, REAL_1, SQUARE_1, NAT_1, SEQ_1, QUIN_1,
      PREPOWER, POWER, POLYEQ_1, BINARITH, SIN_COS, COMPTRIG;
 registrations ORDINAL1, NUMBERS, XCMPLX_0, XXREAL_0, XREAL_0, SQUARE_1, NAT_1,
      MEMBERED, NEWTON, POLYEQ_1;
 requirements ARITHM, SUBSET, REAL, BOOLE, NUMERALS;
 definitions POLYEQ_1, SQUARE_1, XCMPLX_0, QUIN_1;
 theorems COMPLEX1, POLYEQ_1, REAL_2, SQUARE_1, NEWTON, COMPTRIG, NAT_1,
      BINARITH, POWER, SIN_COS, SIN_COS2, PREPOWER, COMPLEX2, XCMPLX_0,
      XCMPLX_1, ARYTM_0, XREAL_1;
 schemes NAT_1, COMPTRIG;

begin

Lm1: 0 = [*0,0*] by ARYTM_0:def 7;

 reserve a,b,c,d,a',b',c',d',x,y,x1,u,v for Real,
         s,t,h,z,z1,z2,z3,s1,s2,s3,p,q for Element of COMPLEX;

definition let a be Real,z;
  redefine func a*z -> Element of COMPLEX;
  correctness by XCMPLX_0:def 2;
  redefine func a + z -> Element of COMPLEX;
  correctness by XCMPLX_0:def 2;
end;

definition let z be Element of COMPLEX;
  redefine func z^2 -> Element of COMPLEX equals
      (Re z)^2-(Im z)^2 + 2*(Re z*Im z)*<i>;
  compatibility
  proof
     z = Re z +(Im z)*<i> by COMPLEX1:29;
    hence thesis;
  end;
  correctness by XCMPLX_0:def 2;
end;

definition let a,b,c,z;
  redefine func Polynom (a,b,c,z) -> Element of COMPLEX;
  coherence by XCMPLX_0:def 2;
end;

theorem
   (a+c*<i>)*(b+d*<i>)=a*b-c*d+(a*d+b*c)*<i>;

theorem
   (x+y*<i>)^2= x^2-y^2 + 2*x*y*<i>;

canceled;

theorem Th4:
  a <> 0 & delta(a,b,c) >=0 & Polynom(a,b,c,z) = 0 implies
    z= (-b+sqrt delta(a,b,c))/(2*a) or
    z= (-b-sqrt delta(a,b,c))/(2*a) or
    z= -b/(2*a)
proof
A1: a = a+0*<i>;
   assume
A2:a <> 0 & delta(a,b,c) >= 0;
   assume
A3: Polynom(a,b,c,z) = 0;
A4:  z = Re z + (Im z)*<i> by COMPLEX1:29;
   set x=Re z;
   set y=Im z;
A5:[*x^2-y^2,2*x*y*] = x^2-y^2+(2*x*y)*<i> by COMPLEX1:27;
   then (a+0*<i>)*([*x^2-y^2,2*(x*y)*])+b*z+c = 0 by A3,A4;
   then 0 = Re a*Re (x^2-y^2+(2*x*y)*<i>) -Im a*Im (x^2-y^2+2*x*y*<i>)+
       (Re a*Im (x^2-y^2+2*x*y*<i>) +Re (x^2-y^2+2*x*y*<i>)*Im a)*<i>
           +b*z+c by A5,COMPLEX1:def 10
      .=[* Re a*Re (x^2-y^2+2*x*y*<i>)-Im a*
        Im (x^2-y^2+2*x*y*<i>),Re a*Im (x^2-y^2+2*x*y*<i>)
       +Re (x^2-y^2+2*x*y*<i>)*Im a *] +b*z+c by COMPLEX1:27
      .=[*a*Re (x^2-y^2+2*x*y*<i>)-Im a*
         Im (x^2-y^2+2*x*y*<i>),Re a*Im (x^2-y^2+2*x*y*<i>)
         +Re (x^2-y^2+2*x*y*<i>)*Im a *] +b*z+c by A1,COMPLEX1:28
      .=[*a*(x^2-y^2)-Im a*
         Im (x^2-y^2+2*x*y*<i>),Re a*Im (x^2-y^2+2*x*y*<i>)
         +Re (x^2-y^2+2*x*y*<i>)*Im a *] +b*z+c by COMPLEX1:28
      .=[*a*(x^2-y^2)-0*Im (x^2-y^2+2*x*y*<i>)
         ,Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
         *Im a *] +b*z+c by A1,COMPLEX1:28
      .=[*a*(x^2-y^2)-0,Re a*(2*x*y)+Re (x^2-y^2+2*x*y*<i>)
         *Im a *] +b*z+c by COMPLEX1:28
      .=[*a*(x^2-y^2)-0,a*(2*x*y)+Re (x^2-y^2+2*x*y*<i>)
         *Im a *] +b*z+c by A1,COMPLEX1:28
      .=[*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)
         *Im a *] +b*z+c by COMPLEX1:28
      .=[*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)*0*] +b*z+c by A1,COMPLEX1:28;
   then A6: [*a*(x^2-y^2)-0*(2*x*y),0+a*(2*x*y)*]
           +(b+0*<i>)*(x+y*<i>)+c = 0 by COMPLEX1:29;
   then (a*(x^2-y^2)+(a*(2*x*y))*<i>)
           +(b*x+(b*y)*<i>)+(c+0*<i>) = 0 by COMPLEX1:27;
   then A7: a*(x^2-y^2)+b*x+c+(a*(2*x*y)+b*y)*<i> = 0;
then A8:a*(x^2-y^2)+b*x+c = 0 & (2*a*x+b)*y = 0 by COMPLEX1:12,28;
   per cases by A8,XCMPLX_1:6;
     suppose
A9:  y = 0;
     then Polynom(a,b,c,x)=0 by A6,COMPLEX1:27;
     then Re z=(-b+sqrt delta(a,b,c))/(2*a) & Im z = 0 or
     Re z= (-b-sqrt delta(a,b,c))/(2*a) & Im z = 0 by A2,A9,POLYEQ_1:5;
     then z=(-b+sqrt delta(a,b,c))/(2*a)+0*<i> or
        z= (-b-sqrt delta(a,b,c))/(2*a)+0*<i> by COMPLEX1:29;
     hence thesis;
   end;
   suppose
A10:    2*a*x+b = 0;
      2/(1/a) <> 0 by A2;
     then A11: x= (-b)/(2*a) by A10,XCMPLX_1:90;
     then a*((b/(2*a))^2-y^2)+b*(-b/(2*a))+c = 0 by A7,COMPLEX1:12,28;
     then (b/(2*a))^2-y^2 = (-(b*(-b/(2*a))+c))/a-0 by A2,XCMPLX_1:90;
     then (b/(2*a))^2- (-(b*(-b/(2*a))+c))/a= y^2-0;
     then y^2 = (b/(2*a))^2+c*a"-((b^2/(2*a))*a");
     then A12: y^2 *((2*a)^2)=(b^2/(2*a)^2+c*a"-((b^2/(2*a))*a"))*((2*a)^2)
                                by XCMPLX_1:77
              .=b^2/((2*a)^2)*((2*a)^2)+(c*a")*(2*a)^2
                          -(b^2*(2*a)"*a")*(2*a)^2;
  A13:    2*a <> 0 by A2;
  then A14:     (2*a)^2 > 0 by SQUARE_1:74;
 then A15:     y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2-(b^2*((2*a)"*a"))*(2*a)^2
          by A12,XCMPLX_1:88
              .=b^2+(c*a")*(2*a)^2-(b^2*((2*a)*a)")*(2*a)^2
                          by XCMPLX_1:205
              .=b^2+(c*a")*(2*a)^2-(b^2*(2*(a*a))")*(2*a)^2;
     set t = (b^2*(2*(a*a))")*(2*a)^2;
      t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*(1/(2*a)^2));
     then t*((2*a)^2)" = b^2*(2*(a*a))"*1 by A14,XCMPLX_1:107;
     then t*((2*a)^2)"*2" = b^2*(2*a^2)"*2";
     then t*((2*a)^2)"*2" = b^2*((2*a^2)"*2");
     then t*((2*a)^2)"*2" = b^2*((2*(a^2*2))") by XCMPLX_1:205;
     then (t*2")/(2*a)^2*(2*a)^2 = b^2/(2*a)^2*(2*a)^2;
     then t*2" = b^2/(2*a)^2*(2*a)^2 by A14,XCMPLX_1:88;
     then A16:  t/2 = b^2 by A14,XCMPLX_1:88;
     set t=(c*a")*(2*a)^2;
      t=(c/a*a)*2*(2*a);
     then A17:   t=c*2*(2*a) by A2,XCMPLX_1:88;
       -delta(a,b,c) <= 0 by A2,XREAL_1:60;
     then (y *(2*a))^2 = 0 by A15,A16,A17,XREAL_1:65;
     then y *(2*a) = 0 by XCMPLX_1:6;
     then Re z=-b/(2*a) & Im z=0 by A11,A13,XCMPLX_1:6;
     then z = -b/(2*a)+0*<i> by COMPLEX1:29;
     hence thesis;
   end;
end;

theorem Th5:
  a <> 0 & delta(a,b,c) < 0 & Polynom(a,b,c,z) = 0 implies
    z= -b/(2*a)+(sqrt (-delta(a,b,c))/(2*a))*<i> or
    z= -b/(2*a)+(-sqrt (-delta(a,b,c))/(2*a))*<i>
proof
A1: [*a,0*]= a+0*<i> by COMPLEX1:27;
   assume
A2:a <> 0 & delta(a,b,c) < 0;
    now let z;
     assume
A3:  Polynom(a,b,c,z) = 0;
     set x=Re z;
     set y=Im z;
A4:   z = x + y*<i> by COMPLEX1:29;
A5:  [*x^2-y^2,2*x*y*] = x^2-y^2+(2*x*y)*<i> by COMPLEX1:27;
     then (a+0*<i>)*([*x^2-y^2,2*(x*y)*])+b*z+c = 0 by A3,A4;
         then Re a*Re (x^2-y^2+2*x*y*<i>)-Im a*Im [*x^2-y^2,2*x*y*]
         +(Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
         *Im a)*<i>+b*z+c = 0 by A5,COMPLEX1:def 10;
         then [*Re a*Re (x^2-y^2+2*x*y*<i>)-Im a*Im [*x^2-y^2,2*x*y*]
         ,Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
         *Im a*]+b*z+c = 0 by COMPLEX1:27;
         then [*a*Re (x^2-y^2+2*x*y*<i>)-Im a*Im (x^2-y^2+2*x*y*<i>)
         ,Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
         *Im a*]+b*z+c = 0 by A1,A5,COMPLEX1:28;
         then [*a*(x^2-y^2)-Im a*Im (x^2-y^2+2*x*y*<i>)
         ,Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
         *Im a*]+b*z+c = 0 by COMPLEX1:28;
         then [*a*(x^2-y^2)-0*Im (x^2-y^2+2*x*y*<i>)
         ,Re a*Im (x^2-y^2+2*x*y*<i>)+Re (x^2-y^2+2*x*y*<i>)
         *Im a*]+b*z+c = 0 by A1,COMPLEX1:28;
         then [*a*(x^2-y^2)-0,Re a*(2*x*y)+Re (x^2-y^2+2*x*y*<i>)
         *Im a*]+b*z+c = 0 by COMPLEX1:28;
         then [*a*(x^2-y^2)-0,a*(2*x*y)+Re (x^2-y^2+2*x*y*<i>)
         *Im a*]+b*z+c = 0 by A1,COMPLEX1:28;
         then [*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)
         *Im a*]+b*z+c = 0 by COMPLEX1:28;
         then [*a*(x^2-y^2)-0,a*(2*x*y)+(x^2-y^2)
         *0*]+b*z+c = 0 by A1,COMPLEX1:28;
         then [*a*(x^2-y^2)-0*(2*x*y),(x^2-y^2)*0+a*(2*x*y)*]
           +(b+0*<i>)*(x+y*<i>)+c = 0  by COMPLEX1:29;
         then a*(x^2-y^2)+(a*(2*x*y))*<i>+(b*x+b*y*<i>)+c = 0
                                  by COMPLEX1:27;
         then A6: a*(x^2-y^2)+b*x+c+(a*(2*x*y)+b*y)*<i> = 0;
          then -a*y^2+((b*x+a*x^2)+c )+a*y^2= 0+a*y^2 by COMPLEX1:12,28;
         then A7:       a*x^2*a+b*x*a+c*a = a*y^2*a by XCMPLX_1:9;
          set t1= (x*a+b/2)^2;
          set t2=(-b^2+c*a*4)/4;
 A8:    t1>=0 by XREAL_1:65;
             0-delta(a,b,c)>0 by A2,REAL_2:173;
          then t2>0 by REAL_2:127;
          then 0+0<t1+t2 by A8,XREAL_1:10;
 then A9:    y<>0 by A7;
            (a*(2*x))*y+b*y = 0 by A6,COMPLEX1:12,28;
         then (a*2*x)*y = (-b)*y;
         then a*(2*x) = -b by A9,XCMPLX_1:5;
         then 2*x = (-b)/a by A2,XCMPLX_1:90;
         then x = 1/a*((-b)/2);
         then A10: x = (-b)/(2*a) by XCMPLX_1:104;
         then a*((b/(2*a))^2-y^2)+b*(-b/(2*a))+c = 0 by A6,COMPLEX1:12,28;
         then (b/(2*a))^2-y^2 = (-(b*(-b/(2*a))+c))/a-0
 by A2,XCMPLX_1:90;
         then (b/(2*a))^2- (-(b*(-b/(2*a))+c))/a= y^2-0;
        then y^2 = (b/(2*a))^2+c*a"-((b^2/(2*a))*a");
  then A11:   y^2 *((2*a)^2)=(b^2/(2*a)^2+c*a"-((b^2/(2*a))*a"))*((2*a)^2)
                      by XCMPLX_1:77
              .=b^2/((2*a)^2)*((2*a)^2)+(c*a")*(2*a)^2
                        -(b^2*(2*a)"*a")*(2*a)^2;
  A12: 2*a <> 0 by A2;
  then A13:  (2*a)^2 > 0 by SQUARE_1:74;
  then A14:   y^2 *((2*a)^2)=b^2+(c*a")*(2*a)^2-(b^2*((2*a)"*a"))*(2*a)^2
      by A11,XCMPLX_1:88
            .=b^2+(c*a")*(2*a)^2-(b^2*((2*a)*a)")*(2*a)^2
                         by XCMPLX_1:205
            .=b^2+(c*a")*(2*a)^2-(b^2*(2*(a*a))")*(2*a)^2;
           set t = (b^2*(2*(a*a))")*(2*a)^2;
            t*((2*a)^2)" = b^2*(2*(a*a))"*(((2*a)^2)*(1/(2*a)^2));
           then t*((2*a)^2)" = b^2*(2*(a*a))"*1 by A13,XCMPLX_1:107;
           then t*((2*a)^2)"*2" = b^2*(2*a^2)"*2";
           then t*((2*a)^2)"*2" = b^2*((2*a^2)"*2");
           then t*((2*a)^2)"*2" = b^2*((2*(a^2*2))") by XCMPLX_1:205;
           then (t*2")/(2*a)^2*(2*a)^2 = b^2/(2*a)^2*(2*a)^2;
            then t*2" = b^2/(2*a)^2*(2*a)^2 by A13,XCMPLX_1:88;
            then A15:         t/2 = b^2 by A13,XCMPLX_1:88;
            set t=(c*a")*(2*a)^2;
             t=(c/a*a)*2*(2*a);
            then A16:         t=c*2*(2*a) by A2,XCMPLX_1:88;
            -delta(a,b,c) > 0 by A2,XREAL_1:60;
           then (y *(2*a))^2  =(sqrt (-delta(a,b,c)))^2
             by A14,A15,A16,SQUARE_1:def 4;
           then ((y *(2*a))+(sqrt (-delta(a,b,c))))*
           ((y *(2*a))-(sqrt (-delta(a,b,c))))=0;
           then ((y *(2*a))+(sqrt (-delta(a,b,c))))=0 or
           ((y *(2*a))-(sqrt (-delta(a,b,c))))=0 by XCMPLX_1:6;
           then y =(-sqrt (-delta(a,b,c)))/(2*a) or
           y *(2*a)/(2*a)=sqrt (-delta(a,b,c))/(2*a) by A12,XCMPLX_1:90;
           then Re z=-b/(2*a) & Im z=sqrt (-delta(a,b,c))/(2*a)
           or  Re z= -b/(2*a) &  Im z= -sqrt (-delta(a,b,c))/(2*a)
 by A10,A12,XCMPLX_1:90;
     hence z= -b/(2*a)+(sqrt(-delta(a,b,c))/(2*a))*<i> or
          z= -b/(2*a)+(-sqrt (-delta(a,b,c))/(2*a))*<i> by COMPLEX1:29;
     end;
    hence thesis;
end;

theorem
   b <> 0 & (for z holds Polynom(0,b,c,z) = 0) implies z = -c/b
proof
A1: 0 = 0+0*<i>;
   assume
A2:b <> 0;
   assume
A3:(for z holds Polynom(0,b,c,z) = 0);
    now let z1;
      Polynom(0,b,c,z1) = 0 by A3;
     then b*(Re z1+(Im z1)*<i>)+c = 0 by COMPLEX1:29;
     then (b*Re z1-0+c+(b*Im z1+0)*<i>)= 0+0*<i>;
     then b*Re z1-0+c = Re 0 & b*Im z1= Im 0 by COMPLEX1:28;
     then b*(Re z1)-0+c = 0 & b*Im z1 = 0 by A1,COMPLEX1:28;
     then Re z1 =(-c)/b & Im z1 = 0 by A2,XCMPLX_1:6,90;
     then z1 = -c/b+0*<i> by COMPLEX1:29;
     hence z1 = -c/b;
   end;hence thesis;
end;

theorem
   for a,b,c being Real,z,z1,z2 being complex number st a <> 0 &
  for z being complex number holds Polynom(a,b,c,z) = Quard(a,z1,z2,z)
    holds b/a =-(z1+z2) & c/a = z1*z2
proof
   let a,b,c be Real,z,z1,z2 be complex number;
   assume
A1:a <> 0;
   assume
A2:for z being complex number holds Polynom(a,b,c,z) = Quard(a,z1,z2,z);
   consider z being Element of COMPLEX;
A3: Polynom(a,b,c,0) = Quard(a,z1,z2,0) by A2;
   Quard(a,z1,z2,1) = Polynom(a,b,c,1) by A2
            .=a+b+c;
  then a+b+c =a+a*(-(z1+z2))+c by A3;
  hence -(z1+z2) = b/a & c/a = z1*z2 by A1,A3,XCMPLX_1:204;
end;

definition let z be complex number;
  func z^3 -> Element of COMPLEX equals
      z^2*z;
  correctness by XCMPLX_0:def 2;
end;

Lm2:  for z be complex number holds
    z|^2= z*z
proof
    let z be complex number;
       z|^(1+1) = (z|^1)*z by NEWTON:11
              .= z*z by NEWTON:10;
   hence thesis;
end;

definition let a,b,c,d,z be complex number;
  redefine func Polynom(a,b,c,d,z) equals
        a*z^3+b*z^2+c*z+d;
  compatibility
   proof let x be set;
      Polynom(a,b,c,d,z) = a*(z |^(2+1))+ b*z^2 +c*z +d
       .= a*((z |^2)*z)+ b*z^2 +c*z +d by NEWTON:11;
    hence x = Polynom(a,b,c,d,z) iff x = a*z^3+b*z^2+c*z+d by Lm2;
   end;
end;

theorem
   0^3 = 0;

theorem
   1^3 = 1;

theorem
   (-1)^3=-1;

theorem Th11:
  Re z^3 = (Re z)|^3 - 3*Re z*(Im z)^2 &
    Im z^3 = -(Im z)|^ 3+3*(Re z)^2*Im z
proof
A1: [*Re z,Im z*] = Re z + (Im z)*<i> by COMPLEX1:27
          .= z by COMPLEX1:29;
         set x = Re z;
         set y = Im z;
A2: [*x,y*] = x+y*<i> by COMPLEX1:27;
A3: [*x|^ 3-3*x*y^2 ,-y|^ 3+3*x^2*y*] = x|^ 3-3*x*y^2 +(-y|^ 3+3*x^2*y)*<i>
                     by COMPLEX1:27;
       z^3
         = Re(x^2-y^2+(2*(x*y))*<i>) *Re (x+y*<i>)-Im(x^2-y^2+(2*(x*y))*<i>)
            *Im (x+y*<i>)+(Re(x^2-y^2+(2*(x*y))*<i>) *Im (x+y*<i>)+
            Re (x+y*<i>)*Im (x^2-y^2+(2*(x*y))*<i>))*<i>
                    by A1,A2,COMPLEX1:def 10
         .= [*Re(x^2-y^2+(2*(x*y))*<i>) *Re (x+y*<i>)-Im(x^2-y^2+(2*(x*y))*<i>)
            *Im (x+y*<i>),Re(x^2-y^2+(2*(x*y))*<i>) *Im (x+y*<i>)+
            Re (x+y*<i>)*Im (x^2-y^2+(2*(x*y))*<i>)*] by COMPLEX1:27
         .= [*(x^2-y^2) *Re (x+y*<i>)-Im(x^2-y^2+(2*(x*y))*<i>)
            *Im (x+y*<i>),Re(x^2-y^2+(2*(x*y))*<i>) *Im (x+y*<i>)+
            Re (x+y*<i>)*Im (x^2-y^2+(2*(x*y))*<i>)*] by COMPLEX1:28
         .= [*(x^2-y^2) *Re (x+y*<i>)-(2*(x*y))
            *Im (x+y*<i>),Re(x^2-y^2+(2*(x*y))*<i>) *Im (x+y*<i>)+
            Re (x+y*<i>)*Im (x^2-y^2+(2*(x*y))*<i>)*] by COMPLEX1:28
         .= [*(x^2-y^2) *Re (x+y*<i>)-(2*(x*y))
            *Im (x+y*<i>),(x^2-y^2) *Im (x+y*<i>)+
            Re (x+y*<i>)*Im (x^2-y^2+(2*(x*y))*<i>)*] by COMPLEX1:28
         .= [*(x^2-y^2) *Re (x+y*<i>)-(2*(x*y))*Im (x+y*<i>)
           ,(x^2-y^2) *Im (x+y*<i>)+Re (x+y*<i>)*(2*(x*y))*] by COMPLEX1:28
         .= [*(x^2-y^2) *x-(2*(x*y))*Im (x+y*<i>)
           ,(x^2-y^2) *Im (x+y*<i>)+Re (x+y*<i>)*(2*(x*y))*] by COMPLEX1:28
         .= [*(x^2-y^2) *x-(2*(x*y))*y
           ,(x^2-y^2) *Im (x+y*<i>)+Re (x+y*<i>)*(2*(x*y))*] by COMPLEX1:28
         .= [*(x^2-y^2) *x-(2*(x*y))*y
           ,(x^2-y^2) *y+Re (x+y*<i>)*(2*(x*y))*] by COMPLEX1:28
         .= [*(x^2*x-y^2*x+0*x)-2*(x*y)*y,(x^2-y^2+0)*y+x*(2*(x*y))*] by
COMPLEX1:28
         .= [*(x|^1*x*x-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y)+x*(2*(x*y))*]
                                by NEWTON:10
         .= [*(x|^ (1+1)*x-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y)+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^(2+1)-y^2*x)-2*(x*y)*y,(x^2*y-y^2*y)+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^ 3-y^2*x)-2*(x*y)*y,(x^2*y-y|^ 1*y*y)+x*(2*(x*y))*]
                                by NEWTON:10
         .= [*(x|^ 3-y^2*x)-2*(x*y)*y,(x^2*y-y|^ (1+1)*y)+x*(2*(x*y))*]
                                by NEWTON:11
         .= [*(x|^ 3-y^2*x)-2*(x*(y*y)),(x^2*y-y|^ (2+1))+x*(2*(x*y))*] by
NEWTON:11
         .= [*x|^ 3-3*x*y^2 ,-y|^ 3+3*x^2*y*];
    hence thesis by A3,COMPLEX1:28;
end;

theorem
  (for z being complex number holds Polynom(a,b,c,d,z) = Polynom(a',b',c',d',z)
)
   implies a = a' & b = b' & c = c' & d = d'
proof
    assume
A1: for z being complex number holds
    Polynom(a,b,c,d,z) = Polynom(a',b',c',d',z);
    then A2: Polynom(a,b,c,d,0) = Polynom(a',b',c',d',0);
A3: Polynom(a,b,c,d,1) =  Polynom(a',b',c',d',1) by A1;
A4: Polynom(a,b,c,d,-1) =  Polynom(a',b',c',d',-1) by A1;
A5: Polynom(a,b,c,d,2) =  Polynom(a',b',c',d',2) by A1;
   hence a = a' by A2,A3,A4;
   thus b = b' by A2,A3,A4;
   thus c = c' by A2,A3,A4,A5;
   thus thesis by A2;
end;

theorem
   (z+h)^3 = z^3+(3*h)*z^2+(3*h^2)*z+h^3;

theorem
   (z*h)^3 = z^3*h^3;

theorem
   b<>0 & delta(b,c,d)>=0 & Polynom(0,b,c,d,z)=0 implies
   z = (-c+sqrt delta(b,c,d))/(2*b) or
   z = (-c-sqrt delta(b,c,d))/(2*b) or
   z = -c/(2*b)
proof
    assume
A1: b<>0 & delta(b,c,d)>=0 & Polynom(0,b,c,d,z)=0;
    then Polynom(b,c,d,z)=0;
    hence thesis by A1,Th4;
end;

theorem
   b<>0 & delta(b,c,d)<0 & Polynom(0,b,c,d,z)=0 implies
    z = -c/(2*b)+(sqrt (-delta(b,c,d))/(2*b))*<i> or
    z = -c/(2*b)+(-sqrt (-delta(b,c,d))/(2*b))*<i>
proof
    assume
A1: b<>0 & delta(b,c,d)<0 & Polynom(0,b,c,d,z)=0;
    then Polynom(b,c,d,z)=0;
    hence thesis by A1,Th5;
end;

theorem
   a<>0 & 4*a*c <= 0 & Polynom(a,0,c,0,z)=0 implies
   z = (sqrt -4*a*c)/(2*a) or
   z = (-sqrt -4*a*c)/(2*a) or
   z = 0
proof
    assume
A1: a<>0 & 4*a*c <= 0 & Polynom(a,0,c,0,z)=0;
    then (a*z^2+c)*z = 0;
then A2:   Polynom(a,0,c,z) = 0 or z = 0 by XCMPLX_1:6;
     -4*a*c >= -0 by A1,XREAL_1:26;
    then z = (-0+sqrt delta(a,0,c))/(2*a)
    or z = (-0-sqrt delta(a,0,c))/(2*a) or z = 0
    or z = 0/(2*a) by A1,A2,Th4;
    hence thesis;
end;

theorem
   a<>0 & delta(a,b,c)>=0 & Polynom(a,b,c,0,z)=0 implies
   z = (-b+sqrt delta(a,b,c))/(2*a) or
   z = (-b-sqrt delta(a,b,c))/(2*a) or
   z = -b/(2*a) or
   z = 0
proof
    assume
A1: a<>0 & delta(a,b,c)>=0 & Polynom(a,b,c,0,z)=0;
    then (a*z^2+b*z+c)*z = 0;
    then Polynom(a,b,c,z) = 0 or z = 0 by XCMPLX_1:6;
    hence thesis by A1,Th4;
end;

theorem
   a<>0 & delta(a,b,c)<0 & Polynom(a,b,c,0,z)=0 implies
  z= -b/(2*a)+(sqrt (-delta(a,b,c))/(2*a))*<i> or
  z= -b/(2*a)+(-sqrt (-delta(a,b,c))/(2*a))*<i> or
  z = 0
proof
    assume
A1: a<>0 & delta(a,b,c)<0 & Polynom(a,b,c,0,z)=0;
    then (a*z^2+b*z+c)*z = 0;
    then Polynom(a,b,c,z) = 0 or z = 0 by XCMPLX_1:6;
    hence thesis by A1,Th5;
end;

theorem Th20:
  y^2 = a implies y = sqrt a or y = -sqrt a
proof
    assume
A1: y^2 = a;
then A2:   Polynom(1,0,-a,y) =0;
A3:  a >= 0 by A1,XREAL_1:65;
     delta(1,0,-a) = 4*a;
    then delta(1,0,-a) >= 0 by A3,REAL_2:121;
    then y=(-0+sqrt delta(1,0,-a))/(2*1) or
    y=(-0-sqrt delta(1,0,-a))/(2*1) by A2,POLYEQ_1:5;
    then y=(sqrt (4*a))/2 or y=(0-sqrt (4*a))/2;
    then y= sqrt a * 2 /2 or
    y=(-(2 * sqrt a))/2 by A3,SQUARE_1:85,97;
    hence y= sqrt a or y=-sqrt a;
end;

theorem
   a<>0 & Im z = 0 & Polynom(a,0,c,d,z)=0 implies
    (for u,v st Re z = u+v & 3*v*u+c/a=0 holds
       z = 3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
          +3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
    or z = 3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
          +3-root(-d/(2*a)+sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
    or z = 3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3))
          +3-root(-d/(2*a)-sqrt(d^2/(4*a^2)+(c/(3*a)) |^ 3)) )
proof
   assume
A1:a <> 0;
   assume
A2: Im z = 0 & Polynom(a,0,c,d,z)=0;
   let u,v;
   assume
A3:Re z=u+v & 3*v*u+c/a=0;
   set x=Re z;
   set y=Im z;
   set p=c/a;
A4:[*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
       = x|^ 3 - 3*x*y^2+(-y|^ 3+3*x^2*y)*<i> by COMPLEX1:27;
A5: a = a+0*<i>;
A6: Re z+(Im z)*<i> = [*Re z,Im z*] by COMPLEX1:27;
A7:z  = Re z+(Im z)*<i> by COMPLEX1:29
           .= x by A2;
    0 = a*(Re z^3+(Im z^3)*<i>)+0*z^2+c*z+d by A2,COMPLEX1:29
         .= a*([*Re z^3,Im z^3*])+0*z^2+c*z+d by COMPLEX1:27
         .= a*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +0*z^2+c*z+d by Th11
         .= a*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,-(Im z)|^ 3+3*(Re z)^2
            *Im z*]+c*z+d by Th11
         .= a*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +c*(Re z+(Im z)*<i>)+d by COMPLEX1:29
         .= (a+0*<i>)*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +c*[*Re z,Im z*]+d by COMPLEX1:27
         .=Re a *Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]-Im a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+(Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a)*<i>
               +c*[*Re z,Im z*]+d by COMPLEX1:def 10
         .=[*Re a *Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]-Im a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a *]
               +c*[*Re z,Im z*]+d by COMPLEX1:27
         .=[*Re a *(x|^ 3 - 3*x*y^2)-Im a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a *]
               +c*[*Re z,Im z*]+d by A4,COMPLEX1:28
         .=[*Re a *(x|^ 3 - 3*x*y^2)-Im a*(-y|^ 3+3*x^2*y),Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a *]
               +c*[*Re z,Im z*]+d by A4,COMPLEX1:28
         .=[*Re a *(x|^ 3 - 3*x*y^2)-Im a*(-y|^ 3+3*x^2*y),Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          (x|^ 3 - 3*x*y^2)*Im a *] +c*[*Re z,Im z*]+d by A4,COMPLEX1:28
         .=[*Re a *(x|^ 3 - 3*x*y^2)-Im a
           *(-y|^ 3+3*x^2*y),Re a*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im a *]
               +c*[*Re z,Im z*]+d by A4,COMPLEX1:28
         .=[*a*(x|^ 3 - 3*x*y^2)-Im a
           *(-y|^ 3+3*x^2*y),Re a*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im a *]
               +c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .=[*a*(x|^ 3 - 3*x*y^2)-0*(-y|^ 3+3*x^2*y),
          Re a*(-y|^ 3+3*x^2*y)+(x|^ 3 - 3*x*y^2)*Im a *]
               +c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*Im a *]
               +c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*0 *]+c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .= a*(x|^ 3 - 3*x*0)+(a*(-0|^ 3+3*x^2*0))*<i> +c*x+d
                      by A2,A6,COMPLEX1:27
         .= a*(x|^ 3 - 3*x*0)+c*x+d+(a*(-0|^ 3+0) +0+0)*<i>
         .= a*(x|^ 3 - 0)+c*x+d+(a*(-0))*<i> by NEWTON:16
         .= [*a*x|^ 3 +c*x+d,0*] by COMPLEX1:27;
            then a"*(a*x|^ 3+c*x+d) = a"*0 by ARYTM_0:def 7;
            then x|^ 3*(a/a)+a"*c*x+a"*d = 0;
 then A8:      Polynom(1,0,c/a,d/a,x) = 0 by A1,XCMPLX_1:89;
A9:   -(d/a)/2 = -1/2*(d/a)
                 .= -d/(a*2) by XCMPLX_1:104;
A10:   (d/a)^2/4 =1/4*(d^2/a^2) by XCMPLX_1:77
                 .=d^2/(a^2*4) by XCMPLX_1:104;
   (c/a)/3 =1/3*(c/a) .=c/(a*3) by XCMPLX_1:104;
  hence thesis by A3,A7,A8,A9,A10,POLYEQ_1:19;
end;

theorem
   a<>0 & Im z <> 0 & Polynom(a,0,c,d,z)=0 implies
    (for u,v st Re z = u+v & 3*v*u+c/(4*a)=0 & c/a>=0 holds
          z=3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))+
             sqrt(3*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             + 3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))^2+c/a)*<i>
       or z=3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             -sqrt(3*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3))
             +3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))^2+c/a)*<i>
       or z=2*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))+
             sqrt(3*(2*(3-root(d/(16*a)+sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*<i>
       or z=2*(3-root(d/(16*a)+sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))
             -sqrt(3*(2*(3-root(d/(16*a)+sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*<i>
       or z=2*(3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))+
             sqrt(3*(2*(3-root(d/(16*a)-sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*<i>
       or z=2*(3-root(d/(16*a)-sqrt((d/(16*a))^2+(c/(12*a)) |^ 3)))
             -sqrt(3*(2*(3-root(d/(16*a)-sqrt((d/(16*a))^2
             +(c/(12*a)) |^ 3))))^2+c/a)*<i>)
proof
   assume
A1:a <> 0;
   assume
A2: Im z <> 0 & Polynom(a,0,c,d,z)=0;
   let u,v;
   assume
A3:Re z=u+v & 3*v*u+c/(4*a)=0 &  c/a>=0;
   set x=Re z;
   set y=Im z;
   set p=c/(4*a);
A4:[*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
       = x|^ 3 - 3*x*y^2+(-y|^ 3+3*x^2*y)*<i> by COMPLEX1:27;
A5: a = a+0*<i>;
A6: Re z+(Im z)*<i> = [*Re z,Im z*] by COMPLEX1:27;
 A7:   z = x+y*<i> by COMPLEX1:29;
 A8:   0 = a*(Re z^3+(Im z^3)*<i>)+0*z^2+c*z+d by A2,COMPLEX1:29
         .= a*([*Re z^3,Im z^3*])+0*z^2+c*z+d
                        by COMPLEX1:27
         .= a*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
            +c*z+d by Th11
         .= a*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,-(Im z)|^ 3+3*(Re z)^2
            *Im z*]+c*z+d by Th11
         .= a*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +c*(Re z+(Im z)*<i>)+d by COMPLEX1:29
         .= (a+0*<i>)*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +c*[*Re z,Im z*]+d by COMPLEX1:27
         .=Re a *Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]-Im a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+(Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a)*<i>
               +c*[*Re z,Im z*]+d by COMPLEX1:def 10
         .=[*Re a *Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]-Im a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a *]
               +c*[*Re z,Im z*]+d by COMPLEX1:27
         .=[*Re a *(x|^ 3 - 3*x*y^2)-Im a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a *]
               +c*[*Re z,Im z*]+d by A4,COMPLEX1:28
         .=[*Re a *(x|^ 3 - 3*x*y^2)-Im a
           *(-y|^ 3+3*x^2*y),Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a *]
               +c*[*Re z,Im z*]+d by A4,COMPLEX1:28
         .=[*Re a *(x|^ 3 - 3*x*y^2)-Im a
           *(-y|^ 3+3*x^2*y),Re a
           *Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
          (x|^ 3 - 3*x*y^2)*Im a *]
               +c*[*Re z,Im z*]+d by A4,COMPLEX1:28
         .=[*Re a *(x|^ 3 - 3*x*y^2)-Im a
           *(-y|^ 3+3*x^2*y),Re a*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im a *]
               +c*[*Re z,Im z*]+d by A4,COMPLEX1:28
         .=[*a*(x|^ 3 - 3*x*y^2)-Im a
           *(-y|^ 3+3*x^2*y),Re a*(-y|^ 3+3*x^2*y)+
          (x|^ 3 - 3*x*y^2)*Im a *]
               +c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .=[*a*(x|^ 3 - 3*x*y^2)-0*(-y|^ 3+3*x^2*y),
          Re a*(-y|^ 3+3*x^2*y)+(x|^ 3 - 3*x*y^2)*Im a *]
               +c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*Im a *]
               +c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .=[*a*(x|^ 3 - 3*x*y^2)-0,a*(-y|^ 3+3*x^2*y)
           +(x|^ 3 - 3*x*y^2)*0 *]+c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .= a*(x|^ 3 - 3*x*y^2)+(a*(-y|^ 3+3*x^2*y))*<i>
            +(c*Re z-0*Im z+(c*Im z+Re z*0)*<i>)+d by A6,COMPLEX1:27
         .= a*(x|^ 3 - 3*x*y^2)+c*x+d+(a*(-y|^ 3+3*x^2*y) +c*y+0)*<i>;
then A9:        a*(-y|^ 3+3*x^2*y) +c*y+0 = 0 by COMPLEX1:12,28;
             a*(-y|^(2+1)+3*x^2*y)+c*y = 0 by A8,COMPLEX1:12,28;
          then a*(-y|^2*y+3*x^2*y)+c*y = 0 by NEWTON:11;
          then (a*(-y|^2+3*x^2)+c+0)*y = 0;
            then (a*(y|^2))+(-a*(y|^2)+(a*(3*x^2)+c))=(a*(y|^2))+0
                      by A2,XCMPLX_1:6;
            then y|^(1+1) = (a*(3*x^2)+c)/a by A1,XCMPLX_1:90;
            then y|^1*y = (a*(3*x^2)+c)/a by NEWTON:11;
            then A10:          y^2 = (3*x^2)*a/a+c/a+0/a by NEWTON:10;
 then A11:      y^2 = 3*x^2+c/a+0*a" by A1,XCMPLX_1:90;
             a*(x|^ 3 - 3*x*(3*x^2+c/a)+0)+c*x+d = 0
                             by A1,A8,A9,A10,XCMPLX_1:90;
            then 0 =a*x|^ 3 - a*(9*(x*x^2)+(3*x)*(c/a))+c*x+d
            .=a*x|^ 3 - a*(9*(x|^1*x*x)+(3*x)*(c/a))+c*x+d by NEWTON:10
            .=a*x|^ 3 - a*(9*(x|^(1+1)*x)+(3*x)*(c/a))+c*x+d by NEWTON:11
            .=a*x|^ 3 - a*(9*(x|^(2+1))+(3*x)*(c/a)+0)+c*x+d by NEWTON:11
            .=a*x|^ 3 - (a*(9*(x|^3))+(3*x)*(c*(a/a)))+c*x+d
            .=a*x|^ 3 - (a*(9*(x|^3))+(3*x)*c)+c*x+d by A1,XCMPLX_1:89
            .=(-8*a)*x|^3+(-2*c)*x+d;
             then (-1)*0=(8*a)*x|^3+(2*c)*x+(-d);
 then A12:       0=x|^3*((8*a)/(8*a))+(8*a)"*((2*c)*x)+(8*a)"*(-d);
                8*a <> 0 by A1;
         then 0=1*x|^3+0*x^2+((2*c)/(8*a))*x+((8*a)"*(-d)) by A12,XCMPLX_1:89;
         then 0=1*x|^3+0*x^2+((2/8*c)/a)*x+((-d)/(8*a)) by XCMPLX_1:84;
         then 0=1*x|^3+0*x^2+(1/a*(c/4))*x+((-d)/(8*a));
 then A13:      Polynom(1,0,c/(4*a),-d/(8*a),x) = 0 by XCMPLX_1:104;
             set pp = c/(4*a);
             set q = -d/(8*a);
               -q/2=1/2*(d/(8*a));
             then A14: -q/2=d/(a*8*2) by XCMPLX_1:104;
A15:         q^2/4=(1/2*(d/(8*a)))^2;
 A16:      x = 3-root(-q/2+sqrt(q^2/4+(pp/3) |^ 3)) +
                3-root(-q/2-sqrt(q^2/4+(pp/3) |^ 3)) or
            x = 3-root(-q/2+sqrt(q^2/4+(pp/3) |^ 3)) +
                3-root(-q/2+sqrt(q^2/4+(pp/3) |^ 3)) or
            x = 3-root(-q/2-sqrt(q^2/4+(pp/3) |^ 3)) +
                3-root(-q/2-sqrt(q^2/4+(pp/3) |^ 3)) by A3,A13,POLYEQ_1:19;
            set m = 3-root(-q/2+sqrt(q^2/4+(pp/3) |^ 3));
            set n = 3-root(-q/2-sqrt(q^2/4+(pp/3) |^ 3));
            A17: z=m+n+sqrt(3*(m + n)^2+c/a)*<i>
             or z=m+n-sqrt(3*(m + n)^2+c/a)*<i>
             or z=2*m+sqrt(3*(2*m)^2+c/a)*<i>
             or z=2*m-sqrt(3*(2*m)^2+c/a)*<i>
             or z=2*n+sqrt(3*(2*n )^2+c/a)*<i>
             or z=2*n-sqrt(3*(2*n )^2+c/a)*<i>
          proof
            now per cases by A16;
             case x = m + n;
              hence z=m+n+sqrt(3*(m + n )^2+c/a)*<i>
               or z=m+n+(-sqrt(3*(m + n )^2+c/a))*<i> by A7,A11,Th20;
            end;
            case x = 2*m;
             hence z=2*m+sqrt(3*(2*m)^2+c/a)*<i>
             or z=2*m+(-sqrt(3*(2*m)^2+c/a))*<i> by A7,A11,Th20;
            end;
            case x = 2*n;
             hence z=2*n+sqrt(3*(2*n )^2+c/a)*<i>
             or z=2*n+(-sqrt(3*(2*n )^2+c/a))*<i> by A7,A11,Th20;
            end;
             end;
             hence thesis;
           end;
     (c/(4*a))/3=1/3*(c/(4*a))
          .=c/(a*4*3) by XCMPLX_1:104
          .=c/(a*(4*3));
    hence thesis by A14,A15,A17;
 end;

theorem
   a<>0 & Polynom(a,b,c,d,z)=0 & Im z=0 implies
    (for u,v,x1 st x1=Re z + b/(3*a) &
       Re z=u+v-b/(3*a) & 3*u*v+(3*a*c-b^2)/(3*a^2)=0 holds
              z =3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
                 +sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
                 +((3*a*c-b^2)/(9*a^2)) |^ 3)) +
                3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
                 -sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
                 +((3*a*c-b^2)/(9*a^2)) |^ 3))-b/(3*a)+0*<i> or
            z =3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
                 +sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
                 +((3*a*c-b^2)/(9*a^2)) |^ 3)) +
                3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
                 +sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
                 +((3*a*c-b^2)/(9*a^2)) |^ 3))-b/(3*a)+0*<i> or
            z =3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
                 -sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
                 +((3*a*c-b^2)/(9*a^2)) |^ 3)) +
                3-root(-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2)
                 -sqrt((2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2))^2/4
                 +((3*a*c-b^2)/(9*a^2)) |^ 3))-b/(3*a)+0*<i> )
proof
   assume
A1:a<>0;
   assume
A2:Polynom(a,b,c,d,z)=0 & Im z=0;
   let u,v,x1;
   assume
A3:x1=Re z +b/(3*a) & Re z=u+v-b/(3*a) & 3*u*v+(3*a*c-b^2)/(3*a^2)=0;
   set x=Re z;
   set y=Im z;
A4: [*Re z,Im z*] = x+y*<i> by COMPLEX1:27;
A5:[*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
       = x|^ 3 - 3*x*y^2+(-y|^ 3+3*x^2*y)*<i> by COMPLEX1:27;
A6: a = a+0*<i>;
A7:  z = x + y*<i> by COMPLEX1:29;
 0 = a*(Re z^3+(Im z^3)*<i>)+b*z^2+c*z+d by A2,COMPLEX1:29
         .= a*([*Re z^3,Im z^3*])+b*z^2+c*z+d by COMPLEX1:27
         .= a*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,Im z^3*]
               +b*z^2+c*z+d by Th11
         .= a*[*(Re z)|^ 3 - 3*Re z*(Im z)^2,-(Im z)|^ 3+3*(Re z)^2
            *Im z*]+b*z^2+c*z+d by Th11
         .= a*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +b*z^2+c*(Re z+(Im z)*<i>)+d by COMPLEX1:29
         .= (a+0*<i>)*([*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*])
               +b*z^2+c*[*Re z,Im z*]+d by COMPLEX1:27
         .= Re a*Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
            -Im a*Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]+
           (Re a*Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
           +Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a)*<i>
               +b*z^2+c*[*Re z,Im z*]+d by COMPLEX1:def 10
         .= [*Re a*Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
            -Im a*Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],
           Re a*Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
           +Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a *]
               +b*z^2+c*[*Re z,Im z*]+d by COMPLEX1:27
         .= [*Re a*Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
            -0*Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*],
           Re a*Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
           +Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*Im a *]
               +b*z^2+c*[*Re z,Im z*]+d by A6,COMPLEX1:28
         .= [*Re a*Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
            -0,Re a*Im [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
           +Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]*0 *]
               +b*z^2+c*[*Re z,Im z*]+d by A6,COMPLEX1:28
         .= [*Re a*Re [*x|^ 3 - 3*x*y^2,-y|^ 3+3*x^2*y*]
            -0,Re a*(-y|^ 3+3*x^2*y)+0 *]
               +b*z^2+c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .= [*Re a*(x|^ 3 - 3*x*y^2)-0,Re a*(-y|^ 3+3*x^2*y)*]
               +b*z^2+c*[*Re z,Im z*]+d by A5,COMPLEX1:28
         .= [*a*(x|^ 3 - 3*x*y^2)-0,Re a*(-y|^ 3+3*x^2*y)*]
               +b*z^2+c*[*Re z,Im z*]+d by A6,COMPLEX1:28
         .= [*a*(x|^ 3 - 3*x*y^2),a*(-y|^ 3+3*x^2*y)*]
           +b*z^2+(c*Re z-0*Im z+(c*Im z+Re z*0)*<i>)+d by A4,A6,COMPLEX1:28
         .= [*a*(x|^ 3 - 3*x*y^2),a*(-y|^ 3+3*x^2*y)*]
           +b*(x^2-y^2+2*x*y*<i>)+[*c*x,c*y*]+d by A7,COMPLEX1:27
         .= a*(x|^ 3 - 3*x*y^2)+(a*(-y|^ 3+3*x^2*y))*<i>
           +b*(x^2-y^2+2*x*y*<i>)+[*c*x,c*y*]+d by COMPLEX1:27
         .= a*(x|^ 3 - 3*x*y^2)+(a*(-y|^ 3+3*x^2*y))*<i>
           +b*(x^2-y^2+(2*x*y)*<i>)+(c*x+(c*y)*<i>)+d by COMPLEX1:27;
         then 0 = a*(x|^ 3 - 3*x*0)+b*(x^2-0)+c*x+d
              +(a*(-0|^ 3+0)+b*0+0)*<i> by A2
         .= [*a*(x|^ 3 - 3*x*0)+b*(x^2-0)+c*x+d
                 ,a*(-0|^ 3+0)+b*0+0*] by COMPLEX1:27
         .= [*a*x|^ 3+b*x^2+c*x+d,a*0+0*] by NEWTON:16;
 then A8:      Polynom(a,b,c,d,x) = 0 by ARYTM_0:def 7;
            set a'=b/a;
            set b'=c/a;
            set c'=d/a;
            set h=-b/(3*a);
A9:        a'=b/a & b'=c/a & c'=d/a & h=-b/(3*a);
             set  p = (3*a*c-b^2)/(3*a^2);
             set  q = 2*((b/(3*a)) |^ 3)+(3*a*d-b*c)/(3*a^2);
                p/3=1/3*((3*a*c-b^2)/(3*a^2));
             then A10: p/3=(3*a*c-b^2)/(a^2*3*3) by XCMPLX_1:104;
 A11:       -q/2=-((b/(3*a)) |^ 3+1/2*((3*a*d-b*c)/(3*a^2)))
            .=-((b/(3*a)) |^ 3+(3*a*d-b*c)/(a^2*3*2)) by XCMPLX_1:104
            .=-(b/(3*a)) |^ 3-(3*a*d-b*c)/(6*a^2);
         Polynom(1,0,p,q,x1) = 0 by A1,A3,A8,A9,POLYEQ_1:16;
  then x1 = 3-root(-q/2+sqrt(q^2/4+(p/3) |^ 3)) +
                 3-root(-q/2-sqrt(q^2/4+(p/3) |^ 3)) or
            x1 = 3-root(-q/2+sqrt(q^2/4+(p/3) |^ 3)) +
                 3-root(-q/2+sqrt(q^2/4+(p/3) |^ 3)) or
            x1 = 3-root(-q/2-sqrt(q^2/4+(p/3) |^ 3)) +
                 3-root(-q/2-sqrt(q^2/4+(p/3) |^ 3)) by A3,POLYEQ_1:19;
  hence thesis by A2,A3,A10,A11,COMPLEX1:29;
end;

theorem Th24:
  z1 <> 0 & Polynom(z1,z2,z) = 0 implies z = -(z2/z1)
proof
  assume z1 <> 0 & Polynom(z1,z2,z) = 0;
   then z = (-z2)*z1" by XCMPLX_1:204;
  hence z = -(z2/z1);
end;

theorem
   z2 <> 0 implies not ex z st Polynom(0,z2,z) = 0;

begin

theorem
   (for z holds Polynom(z1,z2,z3,z) = Polynom(s1,s2,s3,z)) implies
    z1 = s1 & z2 = s2 & z3 = s3
proof
   assume
A1:for z holds Polynom(z1,z2,z3,z) = Polynom(s1,s2,s3,z);
   then A2:  Polynom(z1,z2,z3,0c) = Polynom(s1,s2,s3,0c);
A3:  Polynom(z1,z2,z3,1r) = Polynom(s1,s2,s3,1r) by A1;
        Polynom(z1,z2,z3,-1r) = Polynom(s1,s2,s3,-1r) by A1;
 hence thesis by A2,A3,COMPLEX1:def 6,def 7;
end;

theorem
   (-a+sqrt (a^2+b^2))/2 >=0 & (a+sqrt (a^2+b^2))/2>=0
proof
 A1: a^2>=0 & b^2>=0 & a^2>=a^2 by XREAL_1:65;
 then A2: a^2+b^2>=0+0 & a^2+b^2>=0+a^2 by XREAL_1:9;
 then A3: sqrt(a^2+b^2)>=sqrt(a^2) by A1,SQUARE_1:94;
          A4:  sqrt (a^2+b^2)>=0 by A2,SQUARE_1:def 4;
          per cases;
          suppose a>=0;
          then A5:  sqrt(a^2+b^2)>=a & a+sqrt (a^2+b^2)>=0+0
         by A3,A4,SQUARE_1:89,XREAL_1:9;
           then sqrt(a^2+b^2)-a>=a-a by XREAL_1:11;
           hence thesis by A5,REAL_2:125;
          end;
          suppose
          A6: a<0;
          then -a > 0 by XREAL_1:60;
          then A7:  sqrt(a^2+b^2) >= -a & -a+sqrt (a^2+b^2)>=0+0
  by A3,A4,A6,SQUARE_1:90,XREAL_1:10;
         then sqrt(a^2+b^2)-(-a) >= -a-(-a) by XREAL_1:11;
         hence thesis by A7,REAL_2:125;
       end;
end;

theorem Th28:
  z^2 = s & Im s >= 0 implies
   z=sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
       (sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i> or
   z=-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
       (-sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i>
proof
      assume
 A1: z^2=s;
       assume
 A2: Im s >=0;
       set a=Re s;
       set b=Im s;
       set u=Re z;
       set v=Im z;
A3: [*u,v*] = u+v*<i> & [*a,b*] = a+b*<i> by COMPLEX1:27;
     z = u+v*<i> by COMPLEX1:29;
    then u^2-v^2+2*u*v*<i> =[*a,b*] by A1,A3,COMPLEX1:29;
 then A4: u^2-v^2=a & 2*u*v=b by A3,COMPLEX1:163;
 then A5:   (u^2+v^2)^2=a^2+b^2;
          u^2>=0 & v^2>=0 by XREAL_1:65;
       then u^2+v^2>=0+0 by XREAL_1:9;
 then A6: u^2+v^2=sqrt (a^2+b^2) by A5,SQUARE_1:89;
                  -2*(u*v)<=-0 by A2,A4,XREAL_1:26;
                then (-2)*(u*v)<=0;
                then u*v>=0 by REAL_2:129;
                then u<=0 & v<=0 or u>=0 & v>=0 by SQUARE_1:25;
           then -u=sqrt ((a+sqrt (a^2+b^2))/2) & -v=sqrt ((-a+sqrt (a^2+b^2))/
2)  or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                    by A4,A6,SQUARE_1:89,90;
           hence thesis by COMPLEX1:29;
end;

theorem
   z^2 = s & Im s = 0 & Re s > 0 implies z=sqrt(Re s) or z=-sqrt(Re s)
proof
   assume
A1:z^2=s;
   assume
A2:Im s =0 & Re s >0;
   then z=sqrt (( Re s+sqrt ((Re s)^2+0))/2)+
       (sqrt ((-Re s+sqrt ((Re s)^2+0))/2))*<i> or
    z=-sqrt (( Re s+sqrt ((Re s)^2+0))/2)+
       (-sqrt ((-Re s+sqrt ((Re s)^2+0))/2))*<i> by A1,Th28,SQUARE_1:60;
    then z=[*sqrt (( Re s+sqrt ((Re s)^2+0))/2),
       sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*] or
    z=[*-sqrt (( Re s+sqrt ((Re s)^2+0))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*] by COMPLEX1:27;
    then z=[*sqrt (( Re s+Re s)/2),
       sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*] or
    z=[*-sqrt (( Re s+sqrt ((Re s)^2+0))/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*] by A2,SQUARE_1:89;
    then z=[*sqrt (( Re s+Re s)/2), sqrt ((-Re s+Re s)/2)*] or
    z=[*-sqrt (( Re s+Re s)/2),
       -sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*] by A2,SQUARE_1:89;
    then z=[*sqrt (Re s), sqrt ((0+(Re s-Re s))/2)*] or
    z=[*-sqrt (Re s),-sqrt ((0+(Re s-Re s))/2)*] by A2,SQUARE_1:89;
    hence thesis by ARYTM_0:def 7,SQUARE_1:82;
end;

theorem
   z^2 = s & Im s = 0 & Re s < 0 implies
    z= sqrt (-Re s)*<i> or z= -sqrt (-Re s)*<i>
proof
    assume
A1: z^2=s;
    assume
A2: Im s =0 & Re s < 0;
    then z=sqrt (( Re s+sqrt ((Re s)^2+0))/2)+
       (sqrt ((-Re s+sqrt ((Re s)^2+0))/2))*<i> or
    z=-sqrt (( Re s+sqrt ((Re s)^2+0))/2)+
       (-sqrt ((-Re s+sqrt ((Re s)^2+0))/2))*<i> by A1,Th28,SQUARE_1:60;
    then z=sqrt (( Re s+-Re s)/2)+
       sqrt ((-Re s+sqrt ((Re s)^2+0))/2)*<i> or
    z=-sqrt (( Re s+sqrt ((Re s)^2+0))/2)+
       (-sqrt ((-Re s+sqrt ((Re s)^2+0))/2))*<i> by A2,SQUARE_1:90;
    then z=sqrt (( Re s+-Re s)/2)+ sqrt ((-Re s+-Re s)/2)*<i>   or
    z=-sqrt (( Re s+-Re s)/2)+(-sqrt ((-Re s+sqrt ((Re s)^2+0))/2))*<i>
                       by A2,SQUARE_1:90;
    then z=sqrt (( Re s+-Re s)/2)+ sqrt ((-Re s+-Re s)/2)*<i>   or
    z=-sqrt (( Re s+-Re s)/2)+(-sqrt ((-Re s+-Re s)/2))*<i>
                           by A2,SQUARE_1:90;
   hence thesis by SQUARE_1:82;
end;

theorem
   z^2 = s & Im s <0 implies
   z= sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
       (-sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i> or
   z=-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
        (sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i>
proof
    assume
A1: z^2=s;
    assume
A2: Im s<0;
    set a=Re s;
       set b=Im s;
       set u=Re z;
       set v=Im z;
A3: [*u,v*] = u+v*<i> & [*a,b*] = a+b*<i> by COMPLEX1:27;
   then A4:  z=[*u,v*] & s=[*a,b*] & b <0 by A2,COMPLEX1:29;
    z = u + v*<i> by COMPLEX1:29;
   then A5:  z^2 = u^2-v^2 + 2*u*v*<i>;
 then A6: u^2-v^2=a & 2*u*v=b by A1,A3,A4,COMPLEX1:163;
 then A7: sqrt (u^2+v^2)^2=sqrt (a^2+b^2);
          u^2>=0 & v^2>=0 by XREAL_1:65;
       then u^2+v^2>=0+0 by XREAL_1:9;
 then A8: u^2+v^2=sqrt (a^2+b^2) by A7,SQUARE_1:89;
                 2*(u*v)<0 by A1,A3,A4,A5,COMPLEX1:163;
                then u*v<0 by XREAL_1:135;
                then u<0 & v>0 or u>0 & v<0 by XREAL_1:135;
          then -u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & -v=sqrt ((-a+sqrt (a^2+b^2))/2)
                    by A6,A8,SQUARE_1:89,90;
  hence thesis by COMPLEX1:29;
end;

theorem Th32:
  z^2 = s implies
   z=sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
       (sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i> or
   z=-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
       (-sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i> or
   z= sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
       (-sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i> or
   z=-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
        (sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i>
proof
       assume
 A1: z^2=s;
       set a=Re s;
       set b=Im s;
       set u=Re z;
       set v=Im z;
A2: [*u,v*] = u+v*<i> & [*a,b*] = a+b*<i> by COMPLEX1:27;
   then A3:  z=[*u,v*] & s=[*a,b*] by COMPLEX1:29;
  z = u + v*<i> by COMPLEX1:29;
 then A4: z^2 = u^2-v^2 + 2*u*v*<i>;
 then A5: u^2-v^2=a & 2*u*v=b by A1,A2,A3,COMPLEX1:163;
 then A6: sqrt (u^2+v^2)^2=sqrt (a^2+b^2);
          u^2>=0 & v^2>=0 by XREAL_1:65;
       then u^2+v^2>=0+0 by XREAL_1:9;
 then A7: u^2+v^2=sqrt (a^2+b^2) by A6,SQUARE_1:89;
       per cases;
        suppose b>=0;
        then -2*(u*v)<=-0 by A5,XREAL_1:26;
        then (-2)*(u*v)<=0;
        then u*v>=0 by REAL_2:129;
        then u<=0 & v<=0 or u>=0 & v>=0 by SQUARE_1:25;
        then -u=sqrt ((a+sqrt (a^2+b^2))/2) & -v=sqrt ((-a+sqrt (a^2+b^2))/
2)  or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2)
                    by A5,A7,SQUARE_1:89,90;
           hence thesis by COMPLEX1:29;
           end;
           suppose b<0;
            then 2*(u*v)<0 by A1,A2,A3,A4,COMPLEX1:163;
            then u*v<0 by XREAL_1:135;
            then u<0 & v>0 or u>0 & v<0 by XREAL_1:135;
    then -u=sqrt ((a+sqrt (a^2+b^2))/2) & v=sqrt ((-a+sqrt (a^2+b^2))/2) or
           u=sqrt ((a+sqrt (a^2+b^2))/2) & -v=sqrt ((-a+sqrt (a^2+b^2))/2)
                    by A5,A7,SQUARE_1:89,90;
          hence thesis by COMPLEX1:29;
          end;
end;

theorem
   Polynom(0,0,0,z) = 0;

theorem Th34:
  z1<>0 & Polynom(z1,0,0,z) = 0 implies z=0
proof
   assume z1<>0 & Polynom(z1,0,0,z) = 0;
   then z^2=0 by XCMPLX_1:6;
   hence z=0 by XCMPLX_1:6;
end;

theorem
   z1<>0 & Polynom(z1,z2,0,z)=0 implies z=-(z2/z1) or z=0
proof
   assume
A1:z1<>0 & Polynom(z1,z2,0,z)=0;
   then 0 =(z1*z+z2)*z;
   then Polynom(z1,z2,z) = 0 or z=0 by XCMPLX_1:6;
   hence z=-(z2/z1) or z=0 by A1,Th24;
end;

theorem Th36:
  z1<>0 & Polynom(z1,0,z3,z)=0 implies
    (for s st s=-(z3/z1) holds
       z= sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
            (sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i> or
       z=-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
           (-sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i> or
       z= sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
           (-sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i> or
       z=-sqrt (( Re s+sqrt ((Re s)^2+(Im s)^2))/2)+
            (sqrt ((-Re s+sqrt ((Re s)^2+(Im s)^2))/2))*<i>)
proof
    assume
A1: z1<>0 & Polynom(z1,0,z3,z)=0;
    let s;
    assume
A2: s=-(z3/z1);
     z^2=(-z3)/z1 by A1,XCMPLX_1:90;
    hence thesis by A2,Th32;
end;

theorem Th37:
  z1<>0 & Polynom(z1,z2,z3,z)=0 implies
    (for h,t st h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1) holds
       z= sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2)+
                (sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2))*<i>-t or
       z=-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2)+
               (-sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2))*<i>-t or
       z= sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2)+
               (-sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2))*<i>-t or
       z=-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2)+
                (sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2))*<i>-t)
proof
       assume
 A1: z1<>0 & Polynom(z1,z2,z3,z)=0;
       let h,t;
       assume
 A2: h=(z2/(2*z1))^2-z3/z1 & t=z2/(2*z1);
       (z1*z^2+z2*z+z3)/z1=0 by A1;
      then (z^2*z1)/z1+(z2*z)/z1+z3/z1=0;
 then z^2+(z2/z1)*z+z3/z1=0 by A1,XCMPLX_1:90;
      then z^2+2*z2/(2*z1)*z+z3/z1=0 by XCMPLX_1:92;
      then (z+(z2/(2*z1)))^2-((z2/(2*z1))^2-z3/z1)=0;
      then z+t-t= sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2)+
             (sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2))*<i>-t   or
      z+t-t=-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2)+
            (-sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2))*<i>-t   or
      z+t-t= sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2)+
            (-sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2))*<i>-t   or
      z+t-t=-sqrt (( Re h+sqrt ((Re h)^2+(Im h)^2))/2)+
        (sqrt ((-Re h+sqrt ((Re h)^2+(Im h)^2))/2))*<i>-t by A2,Th32;
    hence thesis;
end;

