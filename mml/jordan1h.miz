theorem Th41:
 m <= n & 1 <= i & i+1 <= len Gauge(C,n)
  implies [\ (i-2)/2|^(n-'m)+2 /] is Element of NAT
proof assume that
   m <= n and
A1: 1 <= i & i+1 <= len Gauge(C,n);
  set i1 = [\ (i-2)/2|^(n-'m)+2 /];
        (i-2)/2|^(n-'m)+2-1 = (i-2)/2|^(n-'m)+(2-1);
   then A2:  (i-2)/2|^(n-'m)+1 < i1 by INT_1:def 4;
   A3:   n -' m + 1 >= 0 + 1 by XREAL_1:8;
    A4: n-'m +1 <= 2|^(n-'m) by NEWTON:104;
   then A5:   0 +1 <= 2|^(n-'m) by A3,XXREAL_0:2;
        1-2 <= i-2 by A1,XREAL_1:11;
   then A6:   (-1)/2|^(n-'m) <= (i-2)/2|^(n-'m) by A4,REAL_1:73;
        (-1)/1 <= (-1)/2|^(n-'m) by A5,REAL_2:203;
   then -1 <= (i-2)/2|^(n-'m) by A6,XXREAL_0:2;
   then -1 + 1 <= (i-2)/2|^(n-'m) + 1 by XREAL_1:8;
 hence i1 is Element of NAT by A2,INT_1:16;
end;

theorem Th42:
 m <= n & 1 <= i & i+1 <= len Gauge(C,n)
 implies
  1 <= [\ (i-2)/2|^(n-'m)+2 /] & [\ (i-2)/2|^(n-'m)+2 /]+1 <= len Gauge(C,m)
proof assume that
A1: m <= n and
A2: 1 <= i & i+1 <= len Gauge(C,n);
  set i1 = [\ (i-2)/2|^(n-'m)+2 /];
        (i-2)/2|^(n-'m)+2-1 = (i-2)/2|^(n-'m)+(2-1);
   then A3:  (i-2)/2|^(n-'m)+1 < i1 by INT_1:def 4;
   A4:   n -' m + 1 >= 0 + 1 by XREAL_1:8;
    A5: n-'m +1 <= 2|^(n-'m) by NEWTON:104;
   then A6:   0 +1 <= 2|^(n-'m) by A4,XXREAL_0:2;
        1-2 <= i-2 by A2,XREAL_1:11;
   then A7:   (-1)/2|^(n-'m) <= (i-2)/2|^(n-'m) by A5,REAL_1:73;
        (-1)/1 <= (-1)/2|^(n-'m) by A6,REAL_2:203;
   then -1 <= (i-2)/2|^(n-'m) by A7,XXREAL_0:2;
   then -1 + 1 <= (i-2)/2|^(n-'m) + 1 by XREAL_1:8;
  then i1 >= 1+0 by A3,INT_1:20;
 hence 1 <= i1;
       i1 <= (i-2)/2|^(n-'m)+2 by INT_1:def 4;
  then A8: i1+1 <= (i-2)/2|^(n-'m)+2+1 by XREAL_1:8;
       i+1 <= 2|^n+ (2+1) by A2,JORDAN8:def 1;
  then i+1 <= 2|^n+ 2+1;
  then i <= 2|^n+ 2 by XREAL_1:8;
  then i-2 <= 2|^n by XREAL_1:22;
  then (i-2) <= 2|^(m+(n-'m)) by A1,BINARITH:53;
  then (i-2)*1 <= 2|^m*2|^(n-'m) by NEWTON:13;
  then (i-2)/2|^(n-'m) <= 2|^m/1 by A5,REAL_2:187;
  then (i-2)/2|^(n-'m) + 3 <= 2|^m + 3 by XREAL_1:8;
  then (i-2)/2|^(n-'m) + 3 <= len Gauge(C,m) by JORDAN8:def 1;
 hence i1+1 <= len Gauge(C,m) by A8,XXREAL_0:2;
end;

theorem Th43:
 m <= n & 1 <= i & i+1 <= len Gauge(C,n) & 1 <= j & j+1 <= width Gauge(C,n)
 implies
  ex i1,j1 st
   i1 = [\ (i-2)/2|^(n-'m)+2 /] & j1 = [\ (j-2)/2|^(n-'m)+2 /] &
   cell(Gauge(C,n),i,j) c= cell(Gauge(C,m),i1,j1)
proof assume that
A1: m <= n and
A2: 1 <= i & i+1 <= len Gauge(C,n) and
A3: 1 <= j & j+1 <= width Gauge(C,n);
A4: len Gauge(C,m) = width Gauge(C,m) & len Gauge(C,n) = width Gauge(C,n)
    by JORDAN8:def 1;
  then reconsider i1 = [\ (i-2)/2|^(n-'m)+2 /],
                  j1 = [\ (j-2)/2|^(n-'m)+2 /] as Element of NAT
                   by A1,A2,A3,Th41;
  take i1,j1;
  thus i1 = [\ (i-2)/2|^(n-'m)+2 /];
  thus j1 = [\ (j-2)/2|^(n-'m)+2 /];
        (i-2)/2|^(n-'m)+2-1 = (i-2)/2|^(n-'m)+(2-1);
   then A5:  (i-2)/2|^(n-'m)+1 < i1 by INT_1:def 4;
   A6:   n -' m + 1 >= 0 + 1 by XREAL_1:8;
    A7: n-'m +1 <= 2|^(n-'m) by NEWTON:104;
   then A8:   0 +1 <= 2|^(n-'m) by A6,XXREAL_0:2;
        1-2 <= i-2 by A2,XREAL_1:11;
   then A9:   (-1)/2|^(n-'m) <= (i-2)/2|^(n-'m) by A7,REAL_1:73;
A10: (-1)/1 <= (-1)/2|^(n-'m) by A8,REAL_2:203;
   then -1 <= (i-2)/2|^(n-'m) by A9,XXREAL_0:2;
   then -1 + 1 <= (i-2)/2|^(n-'m) + 1 by XREAL_1:8;
   then A11:  i1 >= 1+0 by A5,INT_1:20;
        (j-2)/2|^(n-'m)+2-1 = (j-2)/2|^(n-'m)+(2-1);
   then A12:  (j-2)/2|^(n-'m)+1 < j1 by INT_1:def 4;
        1-2 <= j-2 by A3,XREAL_1:11;
   then (-1)/2|^(n-'m) <= (j-2)/2|^(n-'m) by A7,REAL_1:73;
   then -1 <= (j-2)/2|^(n-'m) by A10,XXREAL_0:2;
   then -1 + 1 <= (j-2)/2|^(n-'m) + 1 by XREAL_1:8;
   then A13: j1 >= 1+0 by A12,INT_1:20;
A14:i1 <= (i-2)/2|^(n-'m)+2 by INT_1:def 4;
A15: i1+1 <= len Gauge(C,m) by A1,A2,Th42;
A16: j1 <= (j-2)/2|^(n-'m)+2 by INT_1:def 4;
A17: j1+1 <= width Gauge(C,m) by A1,A3,A4,Th42;
 let e be set;
  set Gm = Gauge(C,m), Gn = Gauge(C,n);
 assume
A18: e in cell(Gauge(C,n),i,j);
  then reconsider p = e as Point of TOP-REAL 2;
A19: 2|^m >m by NEWTON:105;
        i <= len Gn & j <= width Gn by A2,A3,NAT_1:13;
   then [i,j] in Indices Gn by A2,A3,GOBOARD7:10;
   then A20: Gn*(i,j) =|[(W-bound C)+(((E-bound C)-(W-bound C))/(2|^n))*(i-2),
                     (S-bound C)+(((N-bound C)-(S-bound C))/(2|^n))*(j-2)]|
                   by JORDAN8:def 1;
   then A21:  Gn*(i,j)`1 =(W-bound C)+(((E-bound C)-(W-bound C))/(2|^n))*((i-2)
/1)
                 by EUCLID:56
       .= (W-bound C)+((E-bound C)-(W-bound C))/1 *((i-2)/(2|^n)) by
XCMPLX_1:86;
A22:  Gn*(i,j)`2 =(S-bound C)+(((N-bound C)-(S-bound C))/(2|^n))*((j-2)/1)
                 by A20,EUCLID:56
     .= (S-bound C)+(((N-bound C)-(S-bound C))/1)*((j-2)/(2|^n)) by XCMPLX_1:86
;

        i1 <= len Gauge(C,m) & j1 <= width Gauge(C,m) by A15,A17,NAT_1:13;
   then [i1,j1] in Indices Gm by A11,A13,GOBOARD7:10;
   then A23:  Gm*(i1,j1) =|[(W-bound C)+(((E-bound C)-(W-bound C))/(2|^m))*(i1-
2),
                     (S-bound C)+(((N-bound C)-(S-bound C))/(2|^m))*(j1-2)]|
                   by JORDAN8:def 1;
   then A24:  Gm*(i1,j1)`1 =(W-bound C)+(((E-bound C)-(W-bound C))/(2|^m))*((i1
-2)/1)
                 by EUCLID:56
     .= (W-bound C)+(((E-bound C)-(W-bound C))/1)*((i1-2)/(2|^m)) by
XCMPLX_1:86;
A25:  Gm*(i1,j1)`2 =(S-bound C)+(((N-bound C)-(S-bound C))/(2|^m))*((j1-2)/1)
                 by A23,EUCLID:56
     .= (S-bound C)+(((N-bound C)-(S-bound C))/1)*((j1-2)/(2|^m)) by
XCMPLX_1:86;
         (E-bound C) >= (W-bound C)+0 by SPRECT_1:23;
    then A26: (E-bound C)-(W-bound C) >= 0 by XREAL_1:21;
         (N-bound C) >= (S-bound C)+0 by SPRECT_1:24;
    then A27: (N-bound C)-(S-bound C) >= 0 by XREAL_1:21;
A28: Gn*(i,j)`1 <= p`1 by A2,A3,A18,JORDAN9:19;
       i1-2 <= (i-2)/2|^(n-'m) by A14,XREAL_1:22;
  then (i1-2)/2|^m <= (i-2)/2|^(n-'m)/2|^m by A19,REAL_1:73;
  then (i1-2)/2|^m <= (i-2)/(2|^(n-'m)*2|^m) by XCMPLX_1:79;
  then (i1-2)/2|^m <= (i-2)/2|^(n-'m+m) by NEWTON:13;
  then (i1-2)/2|^m <= (i-2)/2|^n by A1,BINARITH:53;
  then (((E-bound C)-(W-bound C)))*((i1-2)/(2|^m))
        <= (((E-bound C)-(W-bound C)))*((i-2)/(2|^n)) by A26,XREAL_1:66;
  then Gm*(i1,j1)`1 <= Gn*(i,j)`1 by A21,A24,XREAL_1:8;
    then A29: Gm*(i1,j1)`1 <= p`1 by A28,XXREAL_0:2;
        1 <= i1+1 & j1 <= width Gauge(C,m) by A17,NAT_1:11,13;
   then [i1+1,j1] in Indices Gm by A13,A15,GOBOARD7:10;
   then Gm*(i1+1,j1)
      =|[(W-bound C)+(((E-bound C)-(W-bound C))/(2|^m))*(i1+1-2),
         (S-bound C)+(((N-bound C)-(S-bound C))/(2|^m))*(j1-2)]|
                   by JORDAN8:def 1;
   then A30: Gm*(i1+1,j1)`1
        =(W-bound C)+(((E-bound C)-(W-bound C))/(2|^m))*((i1+1-2)/1)
                 by EUCLID:56
       .= (W-bound C)+(((E-bound C)-(W-bound C))/1)*((i1+1-2)/(2|^m))
            by XCMPLX_1:86;
        1 <= i+1 & j <= width Gn by A3,NAT_1:11,13;
   then [i+1,j] in Indices Gn by A2,A3,GOBOARD7:10;
   then Gn*(i+1,j) =|[(W-bound C)+(((E-bound C)-(W-bound C))/(2|^n))*(i+1-2),
                     (S-bound C)+(((N-bound C)-(S-bound C))/(2|^n))*(j-2)]|
                   by JORDAN8:def 1;
   then A31: Gn*(i+1,j)`1
        =(W-bound C)+(((E-bound C)-(W-bound C))/(2|^n))*((i+1-2)/1)
                 by EUCLID:56
       .= (W-bound C)+(((E-bound C)-(W-bound C))/1)*((i+1-2)/(2|^n))
            by XCMPLX_1:86;
A32: p`1 <= Gn*(i+1,j)`1 by A2,A3,A18,JORDAN9:19;
     A33: 2|^(n-'m) >n-'m by NEWTON:105;
       (i-2)/2|^(n-'m)+2-1 < i1 by INT_1:def 4;
  then (i-2)/2|^(n-'m)+2 < i1+1 by XREAL_1:21;
  then (i-2)/2|^(n-'m) < i1+1-2 by XREAL_1:22;
  then (i-2) < (i1+1-2)*2|^(n-'m) by A33,REAL_2:177;
  then (i-2) + 1 <= (i1+1-2)*2|^(n-'m) by INT_1:20;
  then (i+1-2)/2|^(n-'m) <= i1+1-2 by A33,REAL_2:177;
  then (i+1-2)/2|^(n-'m)/2|^m <= (i1+1-2)/2|^m by A19,REAL_1:73;
  then (i+1-2)/(2|^(n-'m)*2|^m) <= (i1+1-2)/2|^m by XCMPLX_1:79;
  then (i+1-2)/2|^(n-'m+m) <= (i1+1-2)/2|^m by NEWTON:13;
  then (i+1-2)/2|^n <= (i1+1-2)/2|^m by A1,BINARITH:53;
  then (((E-bound C)-(W-bound C)))*((i+1-2)/(2|^n))
        <= (((E-bound C)-(W-bound C)))*((i1+1-2)/(2|^m)) by A26,XREAL_1:66;
  then Gn*(i+1,j)`1 <= Gm*(i1+1,j1)`1 by A30,A31,XREAL_1:8;
    then A34: p`1 <= Gm*(i1+1,j1)`1 by A32,XXREAL_0:2;
A35: Gn*(i,j)`2 <= p`2 by A2,A3,A18,JORDAN9:19;
       j1-2 <= (j-2)/2|^(n-'m) by A16,XREAL_1:22;
  then (j1-2)/2|^m <= (j-2)/2|^(n-'m)/2|^m by A19,REAL_1:73;
  then (j1-2)/2|^m <= (j-2)/(2|^(n-'m)*2|^m) by XCMPLX_1:79;
  then (j1-2)/2|^m <= (j-2)/2|^(n-'m+m) by NEWTON:13;
  then (j1-2)/2|^m <= (j-2)/2|^n by A1,BINARITH:53;
  then (((N-bound C)-(S-bound C)))*((j1-2)/(2|^m))
        <= (((N-bound C)-(S-bound C)))*((j-2)/(2|^n)) by A27,XREAL_1:66;
  then Gm*(i1,j1)`2 <= Gn*(i,j)`2 by A22,A25,XREAL_1:8;
    then A36: Gm*(i1,j1)`2 <= p`2 by A35,XXREAL_0:2;
        1 <= j1+1 & i1 <= len Gauge(C,m) by A15,NAT_1:11,13;
   then [i1,j1+1] in Indices Gm by A11,A17,GOBOARD7:10;
   then Gm*(i1,j1+1) =|[(W-bound C)+(((E-bound C)-(W-bound C))/(2|^m))*(i1-2),
                     (S-bound C)+(((N-bound C)-(S-bound C))/(2|^m))*(j1+1-2)]|
                   by JORDAN8:def 1;
   then A37: Gm*(i1,j1+1)`2
        =(S-bound C)+(((N-bound C)-(S-bound C))/(2|^m))*((j1+1-2)/1)
                 by EUCLID:56
       .= (S-bound C)+(((N-bound C)-(S-bound C))/1)*((j1+1-2)/(2|^m))
            by XCMPLX_1:86;
        1 <= j+1 & i <= len Gn by A2,NAT_1:11,13;
   then [i,j+1] in Indices Gn by A2,A3,GOBOARD7:10;
   then Gn*(i,j+1) =|[(W-bound C)+(((E-bound C)-(W-bound C))/(2|^n))*(i-2),
                     (S-bound C)+(((N-bound C)-(S-bound C))/(2|^n))*(j+1-2)]|
                   by JORDAN8:def 1;
   then A38: Gn*(i,j+1)`2
        =(S-bound C)+(((N-bound C)-(S-bound C))/(2|^n))*((j+1-2)/1)
                 by EUCLID:56
       .= (S-bound C)+(((N-bound C)-(S-bound C))/1)*((j+1-2)/(2|^n))
            by XCMPLX_1:86;
A39: p`2 <= Gn*(i,j+1)`2 by A2,A3,A18,JORDAN9:19;
       (j-2)/2|^(n-'m)+2-1 < j1 by INT_1:def 4;
  then (j-2)/2|^(n-'m)+2 < j1+1 by XREAL_1:21;
  then (j-2)/2|^(n-'m) < j1+1-2 by XREAL_1:22;
  then (j-2) < (j1+1-2)*2|^(n-'m) by A33,REAL_2:177;
  then (j-2) + 1 <= (j1+1-2)*2|^(n-'m) by INT_1:20;
  then (j+1-2)/2|^(n-'m) <= j1+1-2 by A33,REAL_2:177;
  then (j+1-2)/2|^(n-'m)/2|^m <= (j1+1-2)/2|^m by A19,REAL_1:73;
  then (j+1-2)/(2|^(n-'m)*2|^m) <= (j1+1-2)/2|^m by XCMPLX_1:79;
  then (j+1-2)/2|^(n-'m+m) <= (j1+1-2)/2|^m by NEWTON:13;
  then (j+1-2)/2|^n <= (j1+1-2)/2|^m by A1,BINARITH:53;
  then (((N-bound C)-(S-bound C)))*((j+1-2)/(2|^n))
        <= (((N-bound C)-(S-bound C)))*((j1+1-2)/(2|^m)) by A27,XREAL_1:66;
  then Gn*(i,j+1)`2 <= Gm*(i1,j1+1)`2 by A37,A38,XREAL_1:8;
  then p`2 <= Gm*(i1,j1+1)`2 by A39,XXREAL_0:2;
 hence e in cell(Gauge(C,m),i1,j1) by A11,A13,A15,A17,A29,A34,A36,JORDAN9:19;
end;

theorem Th44:
 m <= n & 1 <= i & i+1 <= len Gauge(C,n) & 1 <= j & j+1 <= width Gauge(C,n)
 implies
  ex i1,j1 st
   1 <= i1 & i1+1 <= len Gauge(C,m) & 1 <= j1 & j1+1 <= width Gauge(C,m) &
   cell(Gauge(C,n),i,j) c= cell(Gauge(C,m),i1,j1)
proof assume that
A1: m <= n and
A2: 1 <= i & i+1 <= len Gauge(C,n) and
A3: 1 <= j & j+1 <= width Gauge(C,n);
  consider i1,j1 such that
A4: i1 = [\ (i-2)/2|^(n-'m)+2 /] and
A5: j1 = [\ (j-2)/2|^(n-'m)+2 /] and
A6: cell(Gauge(C,n),i,j) c= cell(Gauge(C,m),i1,j1) by A1,A2,A3,Th43;
  take i1,j1;
 thus 1 <= i1 & i1+1 <= len Gauge(C,m) by A1,A2,A4,Th42;
       len Gauge(C,m) = width Gauge(C,m) & len Gauge(C,n) = width Gauge(C,n)
    by JORDAN8:def 1;
 hence 1 <= j1 & j1+1 <= width Gauge(C,m) by A1,A3,A5,Th42;
 thus thesis by A6;
end;

