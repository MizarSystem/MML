theorem Th36:
 for f being non empty FinSequence of TOP-REAL 2, G being Go-board
  st f is_sequence_on G &
   (ex i st [1,i] in Indices G & G*(1,i) in rng f) &
   (ex i st [len G,i] in Indices G & G*(len G,i) in rng f)
  holds proj1.:rng f = proj1.:Values G
proof let f be non empty FinSequence of TOP-REAL 2, G be Go-board such that
A1: f is_sequence_on G;
 given i1 being Element of NAT such that
A2: [1,i1] in Indices G and
A3: G*(1,i1) in rng f;
 given i2 being Element of NAT such that
A4: [len G,i2] in Indices G and
A5: G*(len G,i2) in rng f;
       rng f c= Values G by A1,GOBRD13:9;
 hence proj1.:rng f c= proj1.:Values G by RELAT_1:156;
A6: Values G = { G*(i,j): [i,j] in Indices G } by GOBRD13:7;
  consider k1 being set such that
A7: k1 in dom f and
A8: G*(1,i1) = f.k1 by A3,FUNCT_1:def 5;
  reconsider k1 as Element of NAT by A7;
A9: 1 <= k1 & k1 <= len f by A7,FINSEQ_3:27;
  consider k2 being set such that
A10: k2 in dom f and
A11: G*(len G,i2) = f.k2 by A5,FUNCT_1:def 5;
  reconsider k2 as Element of NAT by A10;
A12: 1 <= k2 & k2 <= len f by A10,FINSEQ_3:27;
  set g = mid(f,k1,k2);
A13: g is_sequence_on G by A1,Th33;
A14: now per cases;
   suppose k1 <= k2;
    then len g = k2-'k1+1 by A9,A12,JORDAN4:20;
    hence len g >= 0+1 by XREAL_1:8;
   end;
   suppose k1 > k2;
    then len g=k1-'k2+1 by A9,A12,JORDAN4:21;
    hence len g >= 0+1 by XREAL_1:8;
  end;
  end;
A15: proj1.:Values G c= proj1.:rng g
   proof assume not thesis;
     then consider x being Element of REAL such that
A16:   x in proj1.:Values G and
A17:   not x in proj1.:rng g by SUBSET_1:7;
     consider p being Element of TOP-REAL 2 such that
A18:   p in Values G and
A19:   x = proj1.p by A16,FUNCT_2:116;
     consider i0,j0 being Element of NAT such that
A20:   p = G*(i0,j0) and
A21:   [i0,j0] in Indices G by A6,A18;
A22: 1 <= i0 & i0 <= len G by A21,GOBOARD5:1;
A23: 1 <= j0 & j0 <= width G by A21,GOBOARD5:1;
defpred P[Nat] means 1 <= $1 & $1 <= len g implies
    for i,j st [i,j] in Indices G & G*(i,j) = g.$1 holds i < i0;
A24:   P[0];
A25:   for n st P[n] holds P[n+1]
   proof let n such that
A26: 1 <= n & n <= len g implies
    for i,j st [i,j] in Indices G & G*(i,j) = g.n
     holds i < i0 and
A27: 1 <= n+1 & n+1 <= len g;
    let i,j such that
A28:  [i,j] in Indices G and
A29:  G*(i,j) = g.(n+1);
A30: now assume
A31:    i = i0;
A32:     n+1 in dom g by A27,FINSEQ_3:27;
       then A33:    G*(i,j) = g/.(n+1) by A29,PARTFUN1:def 8;
A34:    1 <= j & j <= width G by A28,GOBOARD5:1;
A35:     x = p`1 by A19,PSCOMP_1:def 28
          .= G*(i0,1)`1 by A20,A22,A23,GOBOARD5:3
          .= G*(i,j)`1 by A22,A31,A34,GOBOARD5:3
          .= proj1.(g/.(n+1)) by A33,PSCOMP_1:def 28;
A36:     dom proj1 = the carrier of TOP-REAL 2 by FUNCT_2:def 1;
            g/.(n+1) in rng g by A29,A32,A33,FUNCT_1:12;
      hence contradiction by A17,A35,A36,FUNCT_1:def 12;
     end;
    per cases;
    suppose n = 0;
     then G*(i,j) = G*(1,i1) by A8,A9,A12,A29,JORDAN3:27;
     then i = 1 by A2,A28,Th32;
    hence thesis by A22,A30,XXREAL_0:1;
    end;
    suppose A37: n <> 0;
     then A38:   1 <= n by NAT_1:14;
     A39: n <= n + 1 by NAT_1:11;
     then n <= len g by A27,XXREAL_0:2;
     then A40:   n in dom g by A38,FINSEQ_3:27;
     then consider i1,j1 being Element of NAT such that
A41:   [i1,j1] in Indices G and
A42:   g/.n = G*(i1,j1) by A13,GOBOARD1:def 11;
A43:   n+1 in dom g by A27,FINSEQ_3:27;
     then g/.(n+1) = G*(i,j) by A29,PARTFUN1:def 8;
     then abs(i1-i)+abs(j1-j) = 1 by A13,A28,A40,A41,A42,A43,GOBOARD1:def 11;
     then A44:   abs(i1-i)=1 & j1=j or abs(j1-j)=1 & i1=i by GOBOARD1:2;
          now
           g.n = g/.n by A40,PARTFUN1:def 8;
      then A45:    i1 < i0 by A26,A27,A37,A39,A41,A42,NAT_1:14,XXREAL_0:2;
      per cases by A44,GOBOARD1:1;
      suppose i1 = i or i < i1;
       hence i < i0 by A45,XXREAL_0:2;
      end;
      suppose i = i1 + 1;
        then i <= i0 by A45,NAT_1:13;
      hence thesis by A30,XXREAL_0:1;
     end;
     end;
    hence i < i0;
   end;
   end;
A46: for n holds P[n] from NAT_1:sch 1(A24,A25);
         G*(len G,i2) = g.len g by A9,A11,A12,JORDAN4:23;
    then len G < i0 by A4,A14,A46;
   hence contradiction by A21,GOBOARD5:1;
  end;
       rng g c= rng f by JORDAN3:28;
  then proj1.:rng g c= proj1.:rng f by RELAT_1:156;
 hence thesis by A15,XBOOLE_1:1;
end;

theorem Th37:
 for f being non empty FinSequence of TOP-REAL 2, G being Go-board
  st f is_sequence_on G &
