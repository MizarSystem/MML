    .= (S-bound C)+(((N-bound C)-(S-bound C))/1)*((j1-2)/(2|^m)) by XCMPLX_1:86
;
  then Gm*(i1,j1)`2 <= Gn*(i,j)`2 by A12,A24,XREAL_1:8;
