  i1 <= len G by A6,MATRIX_1:39;
  then
A11: i1-'1 <= len G by NAT_D:44;
  1 <= j2 by A6,MATRIX_1:39;
  hence thesis by A8,A11,A10,MATRIX_1:37;
end;
