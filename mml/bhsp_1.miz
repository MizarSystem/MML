theorem
  (a + b) * seq = a * seq + b * seq
proof
  let n;
  thus ((a + b) * seq).n = (a + b) * seq.n by NORMSP_1:def 8
    .= a * seq.n + b * seq.n by RLVECT_1:def 9
    .= (a * seq).n + b * seq.n by NORMSP_1:def 8
    .= (a * seq).n + (b * seq).n by NORMSP_1:def 8
    .= (a * seq + b * seq).n by NORMSP_1:def 5;
end;

theorem
  (a * b) * seq = a * (b * seq)
proof
