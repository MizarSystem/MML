  thus (seq1 - seq2).n = seq1.n - seq2.n by NORMSP_1:def 6
    .= - (seq2.n - seq1.n) by RLVECT_1:47
    .= - (seq2 - seq1).n by NORMSP_1:def 6
    .= (- (seq2 - seq1)).n by ThDef10;
