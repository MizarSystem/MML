  thus ((a * b) * seq).n = (a * b) * seq.n by NORMSP_1:def 8
    .= a * (b * seq.n) by RLVECT_1:def 10
    .= a * (b * seq).n by NORMSP_1:def 8
    .= (a * (b * seq)).n by NORMSP_1:def 8;
