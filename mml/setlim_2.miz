      .= A1.(n+k) \ A by Def8
      .= (A1 ^\k).n \ A by NAT_1:def 3
      .= ((A1 ^\k) (\) A).n by Def8;
  end;
  hence thesis by FUNCT_2:113;
end;

theorem Th20:
  (A (\+\) A1) ^\k = A (\+\) (A1 ^\k)
