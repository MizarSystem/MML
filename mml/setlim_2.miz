    then A1.m c= A1.n by A1,PROB_1:def 6;
    then A \/ A1.m c= A \/ A1.n by XBOOLE_1:9;
    then (A (\/) A1).m c= A \/ A1.n by Def6;
    hence thesis by Def6;
  end;
  hence thesis by PROB_1:def 6;
end;

