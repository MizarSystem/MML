 vocabularies SUBSET_1, NUMBERS, PROB_1, SETLIM_1, FUNCT_1, EQREL_1, NAT_1,
      XXREAL_0, SETFAM_1, RELAT_1, CARD_3, TARSKI, XBOOLE_0, ARYTM_3, CARD_1,
      SEQM_3, ORDINAL2, SEQ_2, SETLIM_2;
 notations TARSKI, SUBSET_1, FUNCT_1, NUMBERS, NAT_1, SETFAM_1, KURATO_2,
      PROB_1, SETLIM_1, XXREAL_0;
 constructors SETFAM_1, NAT_1, KURATO_2, SETLIM_1, XREAL_0, RELSET_1;
 registrations SUBSET_1, RELSET_1, XREAL_0, ORDINAL1;
 requirements NUMERALS, SUBSET, ARITHM;
