   A1 (\) A is non-increasing by Th30;
   hence A1 (\) A is monotone by SETLIM_1:def 1;
  end;
  suppose A1 is non-decreasing;then
   A1 (\) A is non-decreasing by Th31;
   hence A1 (\) A is monotone by SETLIM_1:def 1;
