      NAT_1;
 schemes FUNCT_2;

begin

reserve n,m,k for Element of NAT,
  x,X for set,
  A for Subset of X,
  A1,A2 for SetSequence of X;

theorem Th1:
  (inferior_setsequence(A1)).n = Intersection (A1 ^\n)
proof
  reconsider Y = {A1.k: n <= k} as Subset-Family of X by SETLIM_1:28;
  (inferior_setsequence(A1)).n = meet Y by SETLIM_1:def 2
    .= meet rng (A1 ^\ n) by SETLIM_1:6;
  hence thesis by SETLIM_1:8;
end;

theorem Th2:
  (superior_setsequence(A1)).n = Union (A1 ^\n)
proof
  reconsider Y = {A1.k: n <= k} as Subset-Family of X by SETLIM_1:28;
  (superior_setsequence(A1)).n = union Y by SETLIM_1:def 3
    .= union rng (A1 ^\ n) by SETLIM_1:6;
  hence thesis by CARD_3:def 4;
end;

definition
  let X;
  let A1,A2 be SetSequence of X;
  func A1 (/\) A2 -> SetSequence of X means
  :Def1:
  for n holds it.n = A1.n /\ A2.n;
  existence
  proof
    deffunc F(Element of NAT) = A1.$1 /\ A2.$1;
    ex f being SetSequence of X st for n holds f.n = F(n) from FUNCT_2:sch
    4;
    hence thesis;
  end;
  uniqueness
  proof
    let A3 be SetSequence of X,A4 be SetSequence of X such that
A1: for n holds A3.n = A1.n /\ A2.n and
A2: for n holds A4.n = A1.n /\ A2.n;
    now
      let n;
      thus A3.n = A1.n /\ A2.n by A1
        .= A4.n by A2;
    end;
    hence thesis by FUNCT_2:113;
  end;
  commutativity;
  func A1 (\/) A2 -> SetSequence of X means
  :Def2:
  for n holds it.n = A1.n \/ A2.n;
  existence
  proof
    deffunc F(Element of NAT) = A1.$1 \/ A2.$1;
    ex f being SetSequence of X st for n holds f.n = F(n) from FUNCT_2:sch
    4;
    hence thesis;
  end;
  uniqueness
  proof
    let A3 be SetSequence of X,A4 be SetSequence of X such that
A3: for n holds A3.n = A1.n \/ A2.n and
A4: for n holds A4.n = A1.n \/ A2.n;
    now
      let n;
      thus A3.n = A1.n \/ A2.n by A3
        .= A4.n by A4;
    end;
    hence thesis by FUNCT_2:113;
  end;
  commutativity;
  func A1 (\) A2 -> SetSequence of X means
  :Def3:
  for n holds it.n = A1.n \ A2 .n;
  existence
  proof
    deffunc F(Element of NAT) = A1.$1 \ A2.$1;
    ex f being SetSequence of X st for n holds f.n = F(n) from FUNCT_2:sch
    4;
    hence thesis;
  end;
  uniqueness
  proof
    let A3 be SetSequence of X,A4 be SetSequence of X such that
A5: for n holds A3.n = A1.n \ A2.n and
A6: for n holds A4.n = A1.n \ A2.n;
    now
      let n;
      thus A3.n = A1.n \ A2.n by A5
        .= A4.n by A6;
    end;
    hence thesis by FUNCT_2:113;
  end;
  func A1 (\+\) A2 -> SetSequence of X means
  :Def4:
  for n holds it.n = A1.n \+\ A2.n;
  existence
  proof
    deffunc F(Element of NAT) = A1.$1 \+\ A2.$1;
    ex f being SetSequence of X st for n holds f.n = F(n) from FUNCT_2:sch
    4;
    hence thesis;
  end;
  uniqueness
  proof
    let A3 be SetSequence of X,A4 be SetSequence of X such that
A7: for n holds A3.n = A1.n \+\ A2.n and
A8: for n holds A4.n = A1.n \+\ A2.n;
    now
      let n;
      thus A3.n = A1.n \+\ A2.n by A7
        .= A4.n by A8;
    end;
    hence thesis by FUNCT_2:113;
  end;
  commutativity;
end;

theorem Th3:
  A1 (\+\) A2 = (A1 (\) A2) (\/) (A2 (\) A1)
proof
  now
    let n;
    thus (A1 (\+\) A2).n = A1.n \+\ A2.n by Def4
      .= (A1 (\) A2).n \/ (A2.n \ A1.n) by Def3
      .= (A1 (\) A2).n \/ (A2 (\) A1).n by Def3
      .= ((A1 (\) A2) (\/) (A2 (\) A1)).n by Def2;
  end;
  hence thesis by FUNCT_2:113;
end;

theorem Th4:
  (A1 (/\) A2) ^\k = (A1 ^\k) (/\) (A2 ^\k)
proof
  now
    let n;
    thus ((A1 (/\) A2) ^\k).n = (A1 (/\) A2).(n+k) by NAT_1:def 3
      .= A1.(n+k) /\ A2.(n+k) by Def1
      .= (A1 ^\k).n /\ A2.(n+k) by NAT_1:def 3
      .= (A1 ^\k).n /\ (A2 ^\k).n by NAT_1:def 3
      .= ((A1 ^\k) (/\) (A2 ^\k)).n by Def1;
  end;
  hence thesis by FUNCT_2:113;
end;

theorem Th5:
  (A1 (\/) A2) ^\k = (A1 ^\k) (\/) (A2 ^\k)
proof
  now
    let n;
    thus ((A1 (\/) A2) ^\k).n = (A1 (\/) A2).(n+k) by NAT_1:def 3
      .= A1.(n+k) \/ A2.(n+k) by Def2
      .= (A1 ^\k).n \/ A2.(n+k) by NAT_1:def 3
      .= (A1 ^\k).n \/ (A2 ^\k).n by NAT_1:def 3
      .= ((A1 ^\k) (\/) (A2 ^\k)).n by Def2;
  end;
  hence thesis by FUNCT_2:113;
end;

theorem Th6:
  (A1 (\) A2) ^\k = (A1 ^\k) (\) (A2 ^\k)
proof
  now
    let n;
    thus ((A1 (\) A2) ^\k).n = (A1 (\) A2).(n+k) by NAT_1:def 3
      .= A1.(n+k) \ A2.(n+k) by Def3
      .= (A1 ^\k).n \ A2.(n+k) by NAT_1:def 3
      .= (A1 ^\k).n \ (A2 ^\k).n by NAT_1:def 3
      .= ((A1 ^\k) (\) (A2 ^\k)).n by Def3;
  end;
  hence thesis by FUNCT_2:113;
end;

theorem Th7:
  (A1 (\+\) A2) ^\k = (A1 ^\k) (\+\) (A2 ^\k)
proof
  now
    let n;
    thus ((A1 (\+\) A2) ^\k).n = (A1 (\+\) A2).(n+k) by NAT_1:def 3
      .= A1.(n+k) \+\ A2.(n+k) by Def4
      .= (A1 ^\k).n \+\ A2.(n+k) by NAT_1:def 3
      .= (A1 ^\k).n \+\ (A2 ^\k).n by NAT_1:def 3
      .= ((A1 ^\k) (\+\) (A2 ^\k)).n by Def4;
  end;
  hence thesis by FUNCT_2:113;
end;

theorem Th8:
  Union (A1 (/\) A2) c= Union A1 /\ Union A2
proof
  let x;
  assume x in Union (A1 (/\) A2);
  then consider n such that
A1: x in (A1 (/\) A2).n by PROB_1:25;
A2: x in A1.n /\ A2.n by A1,Def1;
  then x in A2.n by XBOOLE_0:def 4;
  then
A3: x in Union A2 by PROB_1:25;
  x in A1.n by A2,XBOOLE_0:def 4;
  then x in Union A1 by PROB_1:25;
  hence thesis by A3,XBOOLE_0:def 4;
end;

theorem Th9:
  Union (A1 (\/) A2) = Union A1 \/ Union A2
proof
  thus Union (A1 (\/) A2) c= Union A1 \/ Union A2
  proof
    let x;
    assume x in Union (A1 (\/) A2);
    then consider n1 being Element of NAT such that
A1: x in (A1 (\/) A2).n1 by PROB_1:25;
A2: x in A1.n1 \/ A2.n1 by A1,Def2;
    per cases by A2,XBOOLE_0:def 3;
    suppose
      x in A1.n1;
      then x in Union A1 by PROB_1:25;
      hence thesis by XBOOLE_0:def 3;
    end;
    suppose
      x in A2.n1;
      then x in Union A2 by PROB_1:25;
      hence thesis by XBOOLE_0:def 3;
    end;
  end;
  let x;
  assume
A3: x in Union A1 \/ Union A2;
  per cases by A3,XBOOLE_0:def 3;
  suppose
    x in Union A1;
    then consider n2 being Element of NAT such that
A4: x in A1.n2 by PROB_1:25;
    x in A1.n2 \/ A2.n2 by A4,XBOOLE_0:def 3;
    then x in (A1 (\/) A2).n2 by Def2;
    hence thesis by PROB_1:25;
  end;
  suppose
    x in Union A2;
    then consider n3 being Element of NAT such that
A5: x in A2.n3 by PROB_1:25;
    x in A1.n3 \/ A2.n3 by A5,XBOOLE_0:def 3;
    then x in (A1 (\/) A2).n3 by Def2;
    hence thesis by PROB_1:25;
  end;
end;

theorem Th10:
  Union A1 \ Union A2 c= Union (A1 (\) A2)
proof
  let x;
  assume
A1: x in Union A1 \ Union A2;
  then x in Union A1 by XBOOLE_0:def 5;
  then consider n1 being Element of NAT such that
A2: x in A1.n1 by PROB_1:25;
  not x in Union A2 by A1,XBOOLE_0:def 5;
  then not x in A2.n1 by PROB_1:25;
  then x in A1.n1 \ A2.n1 by A2,XBOOLE_0:def 5;
  then x in (A1 (\) A2).n1 by Def3;
  hence thesis by PROB_1:25;
end;

theorem Th11:
  Union A1 \+\ Union A2 c= Union (A1 (\+\) A2)
proof
A1: (Union A1 \ Union A2) c= Union (A1 (\) A2) & (Union A2 \ Union A1) c=
  Union (A2 (\) A1) by Th10;
  Union (A1 (\) A2) \/ Union (A2 (\) A1) = Union ((A1 (\) A2) (\/) (A2 (\)
  A1)) by Th9
    .= Union (A1 (\+\) A2) by Th3;
  hence thesis by A1,XBOOLE_1:13;
end;

theorem Th12:
  Intersection (A1 (/\) A2) = Intersection A1 /\ Intersection A2
proof
  thus Intersection (A1 (/\) A2) c= Intersection A1 /\ Intersection A2
  proof
    let x;
    assume
A1: x in Intersection (A1 (/\) A2);
    now
      let k;
      x in (A1 (/\) A2).k by A1,PROB_1:29;
      then x in (A1.k /\ A2.k) by Def1;
      hence x in A1.k & x in A2.k by XBOOLE_0:def 4;
    end;
    then x in Intersection A1 & x in Intersection A2 by PROB_1:29;
    hence thesis by XBOOLE_0:def 4;
  end;
  let x;
  assume x in Intersection A1 /\ Intersection A2;
  then
A2: x in Intersection A1 & x in Intersection A2 by XBOOLE_0:def 4;
  now
    let k;
    x in A1.k & x in A2.k by A2,PROB_1:29;
    then x in A1.k /\ A2.k by XBOOLE_0:def 4;
    hence x in (A1 (/\) A2).k by Def1;
  end;
  hence thesis by PROB_1:29;
end;

theorem Th13:
  Intersection A1 \/ Intersection A2 c= Intersection (A1 (\/) A2)
proof
  let x;
  assume
A1: x in Intersection A1 \/ Intersection A2;
  per cases by A1,XBOOLE_0:def 3;
  suppose
A2: x in Intersection A1;
    now
      let k;
      x in A1.k by A2,PROB_1:29;
      then x in (A1.k \/ A2.k) by XBOOLE_0:def 3;
      hence x in (A1 (\/) A2).k by Def2;
    end;
    hence thesis by PROB_1:29;
  end;
  suppose
A3: x in Intersection A2;
    now
      let k;
      x in A2.k by A3,PROB_1:29;
      then x in A1.k \/ A2.k by XBOOLE_0:def 3;
      hence x in (A1 (\/) A2).k by Def2;
    end;
    hence thesis by PROB_1:29;
  end;
end;

theorem Th14:
  Intersection (A1 (\) A2) c= Intersection A1 \ Intersection A2
proof
  let x;
  assume
A1: x in Intersection (A1 (\) A2);
A2: now
    let k;
    x in (A1 (\) A2).k by A1,PROB_1:29;
    then x in (A1.k \ A2.k) by Def3;
    hence x in A1.k & not x in A2.k by XBOOLE_0:def 5;
  end;
  then not x in A2.0;
  then
A3: not x in Intersection A2 by PROB_1:29;
  x in Intersection A1 by A2,PROB_1:29;
  hence thesis by A3,XBOOLE_0:def 5;
end;

definition
  let X;
  let A1 be SetSequence of X, A be Subset of X;
  func A (/\) A1 -> SetSequence of X means
  :Def5:
  for n holds it.n = A /\ A1.n;
  existence
  proof
    deffunc F(Element of NAT) = A /\ A1.$1;
    ex f being SetSequence of X st for n holds f.n = F(n) from FUNCT_2:sch
    4;
    hence thesis;
  end;
  uniqueness
  proof
    let A3 be SetSequence of X,A4 be SetSequence of X such that
A1: for n holds A3.n = A /\ A1.n and
A2: for n holds A4.n = A /\ A1.n;
    now
      let n;
      thus A3.n = A /\ A1.n by A1
        .= A4.n by A2;
    end;
    hence thesis by FUNCT_2:113;
  end;
  func A (\/) A1 -> SetSequence of X means
  :Def6:
  for n holds it.n = A \/ A1.n;
  existence
  proof
    deffunc F(Element of NAT) = A \/ A1.$1;
    ex f being SetSequence of X st for n holds f.n = F(n) from FUNCT_2:sch
    4;
    hence thesis;
  end;
  uniqueness
  proof
    let A3 be SetSequence of X,A4 be SetSequence of X such that
A3: for n holds A3.n = A \/ A1.n and
A4: for n holds A4.n = A \/ A1.n;
    now
      let n;
      thus A3.n = A \/ A1.n by A3
        .= A4.n by A4;
    end;
    hence thesis by FUNCT_2:113;
  end;
  func A (\) A1 -> SetSequence of X means
  :Def7:
  for n holds it.n = A \ A1.n;
  existence
  proof
    deffunc F(Element of NAT) = A \ A1.$1;
    ex f being SetSequence of X st for n holds f.n = F(n) from FUNCT_2:sch
    4;
    hence thesis;
  end;
  uniqueness
  proof
    let A3 be SetSequence of X,A4 be SetSequence of X such that
A5: for n holds A3.n = A \ A1.n and
A6: for n holds A4.n = A \ A1.n;
    now
      let n;
      thus A3.n = A \ A1.n by A5
        .= A4.n by A6;
    end;
    hence thesis by FUNCT_2:113;
  end;
  func A1 (\) A -> SetSequence of X means
  :Def8:
  for n holds it.n = A1.n \ A;
  existence
  proof
    deffunc F(Element of NAT) = A1.$1 \ A;
    ex f being SetSequence of X st for n holds f.n = F(n) from FUNCT_2:sch
    4;
    hence thesis;
  end;
  uniqueness
  proof
    let A3 be SetSequence of X,A4 be SetSequence of X such that
A7: for n holds A3.n = A1.n \ A and
A8: for n holds A4.n = A1.n \ A;
    now
      let n;
      thus A3.n = A1.n \ A by A7
        .= A4.n by A8;
    end;
    hence thesis by FUNCT_2:113;
  end;
  func A (\+\) A1 -> SetSequence of X means
  :Def9:
  for n holds it.n = A \+\ A1 .n;
  existence
  proof
    deffunc F(Element of NAT) = A \+\ A1.$1;
    ex f being SetSequence of X st for n holds f.n = F(n) from FUNCT_2:sch
    4;
    hence thesis;
  end;
  uniqueness
  proof
    let A3 be SetSequence of X,A4 be SetSequence of X such that
A9: for n holds A3.n = A \+\ A1.n and
A10: for n holds A4.n = A \+\ A1.n;
    now
      let n;
      thus A3.n = A \+\ A1.n by A9
        .= A4.n by A10;
    end;
    hence thesis by FUNCT_2:113;
  end;
end;

theorem
  A (\+\) A1 = (A (\) A1) (\/) (A1 (\) A)
proof
  now
    let n;
    thus (A (\+\) A1).n = A \+\ A1.n by Def9
      .= (A (\) A1).n \/ (A1.n \ A) by Def7
      .= (A (\) A1).n \/ (A1 (\) A).n by Def8
      .= ((A (\) A1) (\/) (A1 (\) A)).n by Def2;
  end;
  hence thesis by FUNCT_2:113;
end;

theorem Th16:
  (A (/\) A1) ^\k = A (/\) (A1 ^\k)
proof
  now
    let n;
    thus ((A (/\) A1) ^\k).n = (A (/\) A1).(n+k) by NAT_1:def 3
      .= A /\ A1.(n+k) by Def5
      .= A /\ (A1 ^\k).n by NAT_1:def 3
      .= (A (/\) (A1 ^\k)).n by Def5;
  end;
  hence thesis by FUNCT_2:113;
end;

