  suppose A1 is non-increasing;then
    A (\) A1 is non-decreasing by Th27;
    hence A (\) A1 is monotone by SETLIM_1:def 1;
  end;
  suppose A1 is non-decreasing;then
    A (\) A1 is non-increasing by Th28;
    hence A (\) A1 is monotone by SETLIM_1:def 1;
  end;
end;

theorem Th30:
  A1 is non-increasing implies A1 (\) A is non-increasing
proof assume
A0:A1 is non-increasing;
   for n,m st n <= m holds (A1 (\) A).m c= (A1 (\) A).n
   proof let n,m such that
B1:  n <= m;
     A1.m c= A1.n by B1,A0,PROB_1:def 6;then
     A1.m \ A c= A1.n \ A by XBOOLE_1:33;then
     (A1 (\) A).m c= A1.n \ A by def8;
     hence thesis by def8;
    end;
  hence thesis by PROB_1:def 6;
end;

theorem Th31:
  A1 is non-decreasing implies A1 (\) A is non-decreasing
proof assume
B0: A1 is non-decreasing;
    for n,m st n <= m holds (A1 (\) A).n c= (A1 (\) A).m
    proof let n,m such that
    B1: n <= m;
       A1.n c= A1.m by B1,B0,PROB_1:def 7;then
       A1.n \ A c= A1.m \ A by XBOOLE_1:33;then
       (A1 (\) A).n c= A1.m \ A by def8;
     hence thesis by def8;
    end;
  hence thesis by PROB_1:def 7;
end;

theorem
  A1 is monotone implies A1 (\) A is monotone
proof assume
