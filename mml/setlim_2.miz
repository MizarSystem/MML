begin

 reserve n,m,k for Nat,
         x,y,z,X for set,
         A for Subset of X,
         A1,A2,A3 for SetSequence of X;

theorem Th1:
  (inferior_setsequence(A1)).n = Intersection (A1 ^\n)
proof reconsider Y = {A1.k: n <= k} as Subset-Family of X by SETLIM_1:33;
  (inferior_setsequence(A1)).n = meet Y by SETLIM_1:def 2
                                .= meet rng (A1 ^\ n) by SETLIM_1:10;
  hence thesis by SETLIM_1:13;
end;

theorem Th2:
  (superior_setsequence(A1)).n = Union (A1 ^\n)
proof reconsider Y = {A1.k: n <= k} as Subset-Family of X by SETLIM_1:33;
  (superior_setsequence(A1)).n = union Y by SETLIM_1:def 3
                                .= union rng (A1 ^\ n) by SETLIM_1:10;
  hence thesis by CARD_3:def 4;
end;

definition let X;
           let A1,A2 be SetSequence of X;
  func A1 (/\) A2 -> SetSequence of X means :def1:
    for n holds it.n = A1.n /\ A2.n;
  existence
  proof
    deffunc F(Nat) = A1.$1 /\ A2.$1;
    consider f being SetSequence of X such that
A1: for n holds f.n = F(n) from KURATO_2:sch 1;
    thus thesis by A1;
  end;
  uniqueness
  proof
  let A3 be SetSequence of X,A4 be SetSequence of X such that
B1: for n holds A3.n = A1.n /\ A2.n and
B2: for n holds A4.n = A1.n /\ A2.n;
   now let n;
     thus A3.n = A1.n /\ A2.n by B1 .= A4.n by B2;
    end;
    hence A3 = A4 by FUNCT_2:113;
  end;
  commutativity;
  func A1 (\/) A2 -> SetSequence of X means :def2:
  for n holds it.n = A1.n \/ A2.n;
  existence
  proof
    deffunc F(Nat) = A1.$1 \/ A2.$1;
    consider f being SetSequence of X such that
A1: for n holds f.n = F(n) from KURATO_2:sch 1;
    thus thesis by A1;
  end;
  uniqueness
  proof
  let A3 be SetSequence of X,A4 be SetSequence of X such that
B1: for n holds A3.n = A1.n \/ A2.n and
B2: for n holds A4.n = A1.n \/ A2.n;
   now let n;
     thus A3.n = A1.n \/ A2.n by B1 .= A4.n by B2;
    end;
    hence A3 = A4 by FUNCT_2:113;
  end;
  commutativity;
  func A1 (\) A2 -> SetSequence of X means :def3:
  for n holds it.n = A1.n \ A2.n;
  existence
  proof
    deffunc F(Nat) = A1.$1 \ A2.$1;
    consider f being SetSequence of X such that
A1: for n holds f.n = F(n) from KURATO_2:sch 1;
    thus thesis by A1;
  end;
  uniqueness
  proof
  let A3 be SetSequence of X,A4 be SetSequence of X such that
B1: for n holds A3.n = A1.n \ A2.n and
B2: for n holds A4.n = A1.n \ A2.n;
   now let n;
     thus A3.n = A1.n \ A2.n by B1 .= A4.n by B2;
    end;
    hence A3 = A4 by FUNCT_2:113;
  end;
  func A1 (\+\) A2 -> SetSequence of X means :def4:
  for n holds it.n = A1.n \+\ A2.n;
  existence
  proof
    deffunc F(Nat) = A1.$1 \+\ A2.$1;
    consider f being SetSequence of X such that
A1: for n holds f.n = F(n) from KURATO_2:sch 1;
    thus thesis by A1;
  end;
  uniqueness
  proof
  let A3 be SetSequence of X,A4 be SetSequence of X such that
B1: for n holds A3.n = A1.n \+\ A2.n and
B2: for n holds A4.n = A1.n \+\ A2.n;
   now let n;
     thus A3.n = A1.n \+\ A2.n by B1 .= A4.n by B2;
    end;
    hence A3 = A4 by FUNCT_2:113;
  end;
  commutativity;
end;

theorem Th3:
  A1 (\+\) A2 = (A1 (\) A2) (\/) (A2 (\) A1)
proof
 now let n;
 thus (A1 (\+\) A2).n = A1.n \+\ A2.n by def4
   .= (A1.n \ A2.n) \/ (A2.n \ A1.n) by XBOOLE_0:def 6
   .= (A1 (\) A2).n \/ (A2.n \ A1.n) by def3
   .= (A1 (\) A2).n \/ (A2 (\) A1).n by def3
   .= ((A1 (\) A2) (\/) (A2 (\) A1)).n by def2;
 end;
 hence thesis by FUNCT_2:113;
end;

theorem Th4:
  (A1 (/\) A2) ^\k = (A1 ^\k) (/\) (A2 ^\k)
proof
 now let n;
 thus ((A1 (/\) A2) ^\k).n = (A1 (/\) A2).(n+k) by KURATO_2:def 2
   .= A1.(n+k) /\ A2.(n+k) by def1
   .= (A1 ^\k).n /\ A2.(n+k) by KURATO_2:def 2
   .= (A1 ^\k).n /\ (A2 ^\k).n by KURATO_2:def 2
   .= ((A1 ^\k) (/\) (A2 ^\k)).n by def1;
 end;
 hence thesis by FUNCT_2:113;
end;

theorem Th5:
  (A1 (\/) A2) ^\k = (A1 ^\k) (\/) (A2 ^\k)
proof
 now let n;
 thus ((A1 (\/) A2) ^\k).n = (A1 (\/) A2).(n+k) by KURATO_2:def 2
   .= A1.(n+k) \/ A2.(n+k) by def2
   .= (A1 ^\k).n \/ A2.(n+k) by KURATO_2:def 2
   .= (A1 ^\k).n \/ (A2 ^\k).n by KURATO_2:def 2
   .= ((A1 ^\k) (\/) (A2 ^\k)).n by def2;
 end;
hence thesis by FUNCT_2:113;
end;

theorem Th6:
  (A1 (\) A2) ^\k = (A1 ^\k) (\) (A2 ^\k)
proof
 now let n;
  thus ((A1 (\) A2) ^\k).n = (A1 (\) A2).(n+k) by KURATO_2:def 2
   .= A1.(n+k) \ A2.(n+k) by def3
   .= (A1 ^\k).n \ A2.(n+k) by KURATO_2:def 2
   .= (A1 ^\k).n \ (A2 ^\k).n by KURATO_2:def 2
   .= ((A1 ^\k) (\) (A2 ^\k)).n by def3;
 end;
 hence thesis by FUNCT_2:113;
end;

theorem Th7:
  (A1 (\+\) A2) ^\k = (A1 ^\k) (\+\) (A2 ^\k)
proof
 now let n;
  thus ((A1 (\+\) A2) ^\k).n = (A1 (\+\) A2).(n+k) by KURATO_2:def 2
   .= A1.(n+k) \+\ A2.(n+k) by def4
   .= (A1 ^\k).n \+\ A2.(n+k) by KURATO_2:def 2
   .= (A1 ^\k).n \+\ (A2 ^\k).n by KURATO_2:def 2
   .= ((A1 ^\k) (\+\) (A2 ^\k)).n by def4;
 end;
 hence thesis by FUNCT_2:113;
end;

theorem Th8:
  Union (A1 (/\) A2) c= Union A1 /\ Union A2
proof
   let x;assume x in Union (A1 (/\) A2);then
   consider n such that
B1: x in (A1 (/\) A2).n by PROB_1:25;
   x in A1.n /\ A2.n by B1,def1;then
   x in A1.n & x in A2.n by XBOOLE_0:def 3;then
   x in Union A1 & x in Union A2 by PROB_1:25;
   hence thesis by XBOOLE_0:def 3;
end;

theorem Th9:
  Union (A1 (\/) A2) = Union A1 \/ Union A2
proof
  thus Union (A1 (\/) A2) c= Union A1 \/ Union A2
  proof let x;assume x in Union (A1 (\/) A2);then
    consider n1 being Nat such that
B2: x in (A1 (\/) A2).n1 by PROB_1:25;
B3: x in A1.n1 \/ A2.n1 by B2,def2;
    per cases by B3,XBOOLE_0:def 2;
     suppose x in A1.n1;then
      x in Union A1 by PROB_1:25;
      hence x in Union A1 \/ Union A2 by XBOOLE_0:def 2;
     end;
     suppose x in A2.n1;then
      x in Union A2 by PROB_1:25;
      hence x in Union A1 \/ Union A2 by XBOOLE_0:def 2;
     end;
   end;
   let x; assume
C1: x in Union A1 \/ Union A2;
  per cases by C1,XBOOLE_0:def 2;
   suppose x in Union A1;then
    consider n2 being Nat such that
    CD1: x in A1.n2 by PROB_1:25;
     x in A1.n2 \/ A2.n2 by CD1,XBOOLE_0:def 2;then
     x in (A1 (\/) A2).n2 by def2;
   hence x in Union (A1 (\/) A2) by PROB_1:25;
   end;
   suppose x in Union A2;then
    consider n3 being Nat such that
CE1: x in A2.n3 by PROB_1:25;
     x in A1.n3 \/ A2.n3 by CE1,XBOOLE_0:def 2;then
     x in (A1 (\/) A2).n3 by def2;
   hence x in Union (A1 (\/) A2) by PROB_1:25;
   end;
end;

theorem Th10:
  Union A1 \ Union A2 c= Union (A1 (\) A2)
proof
 let x;assume x in Union A1 \ Union A2;then
 B2: x in Union A1 & not x in Union A2 by XBOOLE_0:def 4;then
     consider n1 being Nat such that
 B3: x in A1.n1 by PROB_1:25;
     not x in A2.n1 by B2,PROB_1:25;then
     x in A1.n1 \ A2.n1 by B3,XBOOLE_0:def 4;then
     x in (A1 (\) A2).n1 by def3;
  hence x in Union (A1 (\) A2) by PROB_1:25;
end;

theorem Th11:
  Union A1 \+\ Union A2 c= Union (A1 (\+\) A2)
proof
A2:(Union A1 \ Union A2) c= Union (A1 (\) A2) by Th10;
   (Union A2 \ Union A1) c= Union (A2 (\) A1) by Th10;then
A3:(Union A1 \ Union A2) \/ (Union A2 \ Union A1)
    c= Union (A1 (\) A2) \/ Union (A2 (\) A1) by A2,XBOOLE_1:13;
   Union (A1 (\) A2) \/ Union (A2 (\) A1)
    = Union ((A1 (\) A2) (\/) (A2 (\) A1)) by Th9
   .= Union (A1 (\+\) A2) by Th3;
   hence thesis by XBOOLE_0:def 6,A3;
end;

theorem Th12:
  Intersection (A1 (/\) A2) = Intersection A1 /\ Intersection A2
proof
 thus Intersection (A1 (/\) A2) c= Intersection A1 /\ Intersection A2
 proof let x;assume
 B1: x in Intersection (A1 (/\) A2);
 B2: now let k;
      x in (A1 (/\) A2).k by B1,PROB_1:29;then
      x in (A1.k /\ A2.k) by def1;
     hence x in A1.k & x in A2.k by XBOOLE_0:def 3;
     end;then
 B3: x in Intersection A1 by PROB_1:29;
     x in Intersection A2 by B2,PROB_1:29;
  hence x in Intersection A1 /\ Intersection A2 by B3,XBOOLE_0:def 3;
 end;
 let x;assume x in Intersection A1 /\ Intersection A2;then
 C1: x in Intersection A1 & x in Intersection A2 by XBOOLE_0:def 3;
     now let k;
     C11: x in A1.k by C1,PROB_1:29;
          x in A2.k by C1,PROB_1:29;then
      x in A1.k /\ A2.k by C11,XBOOLE_0:def 3;
     hence x in (A1 (/\) A2).k by def1;
   end;
  hence x in Intersection (A1 (/\) A2) by PROB_1:29;
end;

theorem Th13:
  Intersection A1 \/ Intersection A2 c= Intersection (A1 (\/) A2)
proof let x;assume
A1: x in Intersection A1 \/ Intersection A2;
  per cases by A1,XBOOLE_0:def 2;
   suppose AA1: x in Intersection A1;
    now let k;
     x in A1.k by AA1,PROB_1:29;then
     x in (A1.k \/ A2.k) by XBOOLE_0:def 2;
    hence x in (A1 (\/) A2).k by def2;
    end;
   hence thesis by PROB_1:29;
   end;
   suppose CC2: x in Intersection A2;
    now let k;
     x in A2.k by CC2,PROB_1:29;then
     x in A1.k \/ A2.k by XBOOLE_0:def 2;
    hence x in (A1 (\/) A2).k by def2;
    end;
   hence thesis by PROB_1:29;
  end;
end;

theorem Th14:
  Intersection (A1 (\) A2) c= Intersection A1 \ Intersection A2
proof let x;assume
B1: x in Intersection (A1 (\) A2);
B2: now let k;
      x in (A1 (\) A2).k by B1,PROB_1:29;then
      x in (A1.k \ A2.k) by def3;
      hence x in A1.k & not x in A2.k by XBOOLE_0:def 4;
    end;then
B3: x in Intersection A1 by PROB_1:29;
    not x in A2.0 by B2;then
    not x in Intersection A2 by PROB_1:29;
   hence thesis by B3,XBOOLE_0:def 4;
end;

definition let X;
           let A1 be SetSequence of X, A be Subset of X;
  func A (/\) A1 -> SetSequence of X means :def5:
  for n holds it.n = A /\ A1.n;
  existence
  proof
    deffunc F(Nat) = A /\ A1.$1;
    consider f being SetSequence of X such that
A1: for n holds f.n = F(n) from KURATO_2:sch 1;
    thus thesis by A1;
  end;
  uniqueness
  proof
  let A3 be SetSequence of X,A4 be SetSequence of X such that
B1: for n holds A3.n = A /\ A1.n and
B2: for n holds A4.n = A /\ A1.n;
   now let n;
     thus A3.n = A /\ A1.n by B1 .= A4.n by B2;
    end;
    hence A3 = A4 by FUNCT_2:113;
  end;
  func A (\/) A1 -> SetSequence of X means :def6:
  for n holds it.n = A \/ A1.n;
  existence
  proof
    deffunc F(Nat) = A \/ A1.$1;
    consider f being SetSequence of X such that
A1: for n holds f.n = F(n) from KURATO_2:sch 1;
    thus thesis by A1;
  end;
  uniqueness
  proof
  let A3 be SetSequence of X,A4 be SetSequence of X such that
B1: for n holds A3.n = A \/ A1.n and
B2: for n holds A4.n = A \/ A1.n;
   now let n;
    thus A3.n = A \/ A1.n by B1 .= A4.n by B2;
   end;
   hence A3 = A4 by FUNCT_2:113;
  end;
  func A (\) A1 -> SetSequence of X means :def7:
  for n holds it.n = A \ A1.n;
  existence
  proof
    deffunc F(Nat) = A \ A1.$1;
    consider f being SetSequence of X such that
A1: for n holds f.n = F(n) from KURATO_2:sch 1;
   thus thesis by A1;
  end;
  uniqueness
  proof
  let A3 be SetSequence of X,A4 be SetSequence of X such that
B1: for n holds A3.n = A \ A1.n and
B2: for n holds A4.n = A \ A1.n;
   now let n;
     thus A3.n = A \ A1.n by B1 .= A4.n by B2;
    end;
    hence A3 = A4 by FUNCT_2:113;
  end;
  func A1 (\) A -> SetSequence of X means :def8:
  for n holds it.n = A1.n \ A;
  existence
  proof
    deffunc F(Nat) = A1.$1 \ A;
    consider f being SetSequence of X such that
A1: for n holds f.n = F(n) from KURATO_2:sch 1;
    thus thesis by A1;
  end;
  uniqueness
  proof
  let A3 be SetSequence of X,A4 be SetSequence of X such that
B1: for n holds A3.n = A1.n \ A and
B2: for n holds A4.n = A1.n \ A;
   now let n;
     thus A3.n = A1.n \ A by B1 .= A4.n by B2;
    end;
    hence A3 = A4 by FUNCT_2:113;
  end;
  func A (\+\) A1 -> SetSequence of X means :def9:
  for n holds it.n = A \+\ A1.n;
  existence
  proof
    deffunc F(Nat) = A \+\ A1.$1;
    consider f being SetSequence of X such that
A1: for n holds f.n = F(n) from KURATO_2:sch 1;
    thus thesis by A1;
  end;
  uniqueness
  proof
  let A3 be SetSequence of X,A4 be SetSequence of X such that
B1: for n holds A3.n = A \+\ A1.n and
B2: for n holds A4.n = A \+\ A1.n;
   now let n;
     thus A3.n = A \+\ A1.n by B1 .= A4.n by B2;
    end;
    hence A3 = A4 by FUNCT_2:113;
  end;
end;

theorem
  A (\+\) A1 = (A (\) A1) (\/) (A1 (\) A)
proof
  now let n;
    thus (A (\+\) A1).n = A \+\ A1.n by def9
     .= (A \ A1.n) \/ (A1.n \ A) by XBOOLE_0:def 6
     .= (A (\) A1).n \/ (A1.n \ A) by def7
     .= (A (\) A1).n \/ (A1 (\) A).n by def8
     .= ((A (\) A1) (\/) (A1 (\) A)).n by def2;
  end;
  hence thesis by FUNCT_2:113;
end;

theorem Th16:
  (A (/\) A1) ^\k = A (/\) (A1 ^\k)
proof
 now let n;
 thus ((A (/\) A1) ^\k).n = (A (/\) A1).(n+k) by KURATO_2:def 2
   .= A /\ A1.(n+k) by def5
   .= A /\ (A1 ^\k).n by KURATO_2:def 2
   .= (A (/\) (A1 ^\k)).n by def5;
 end;
hence thesis by FUNCT_2:113;
end;

