    D23: L~BCut1 c= Lf by A18,D12,XBOOLE_1:1;
    D15: now assume b in LSeg(BCut1,len BCut1 -' 1);
     then b in LSeg(BCut1/.(len BCut1-'1),BCut1/.len BCut1)
