    take BCut1;
    thus BCut1 is_sequence_on
     GoB(BCut^'<*North-Bound(p,L~Cage(C,n)),South-Bound(p,L~Cage(C,n))*>) &
     L~BCut = L~BCut1 &
     BCut/.1 = BCut1/.1 &
     BCut/.len BCut = BCut1/.len BCut1 &
     len BCut <= len BCut1 by C1,C3,C4,C5,C6;
    take g;
    thus g = BCut1^'pion1;
   end;

