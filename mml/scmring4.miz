T: ProgramPart s1 = ProgramPart Cs1i
by AMI_1:123;
  Cs1i1 = Following(ProgramPart s1,Cs1i) by AMI_1:14
    .= Exec (CurInstr(ProgramPart Cs1i,Cs1i), Cs1i) by T;
