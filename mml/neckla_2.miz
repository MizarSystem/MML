                hence thesis by A188,A198,ZFMISC_1:33;
              end;
              suppose
A199:           [x,y] = [2,1];
                then x = 2 by ZFMISC_1:33;
                hence thesis by A191,A199,ZFMISC_1:33;
              end;
              suppose
A200:           [x,y] = [2,3];
                then x = 2 by ZFMISC_1:33;
                hence thesis by A190,A200,ZFMISC_1:33;
              end;
              suppose
A201:           [x,y] = [3,2];
                then x = 3 by ZFMISC_1:33;
                hence thesis by A193,A201,ZFMISC_1:33;
              end;
            end;
            thus [f.x,f.y] in the InternalRel of H1 implies [x,y] in the
            InternalRel of N4
            proof
              [f.x,f.y] in the InternalRel of S implies [x,y] in the
              InternalRel of N4 by A157;
              then
A202:         [f.x,f.y] in (the InternalRel of H1) \/ (the InternalRel
              of H2) implies [x,y] in the InternalRel of N4 by A158,Def2;
              assume
              [f.x,f.y] in the InternalRel of H1;
              hence thesis by A202,XBOOLE_0:def 3;
            end;
          end;
A203:     f.3 in rng (the InternalRel of H1) by A190,RELAT_1:20;
          rng f c= the carrier of H1
          proof
            let y be set;
            assume
            y in rng f;
            then consider x being set such that
A204:       x in dom f and
A205:       y = f.x by FUNCT_1:def 5;
            per cases by A168,A204,Lm1,ENUMSET1:def 2;
            suppose
              x = 0;
              hence thesis by A167,A205;
            end;
            suppose
              x = 1;
              hence thesis by A176,A178,A205;
            end;
            suppose
              x = 2;
              hence thesis by A176,A189,A205;
            end;
            suppose
              x = 3;
              hence thesis by A176,A203,A205;
            end;
          end;
          then f is Function of the carrier of N4,the carrier of H1 by
          FUNCT_2:8;
          then H1 embeds N4 by A156,A194,NECKLACE:def 2;
          hence thesis by A5,A17,A175,A185;
        end;
        suppose
A206:     f.0 in the carrier of H2;
          set cS = the carrier of S, cH1 = the carrier of H1, cH2 = the
          carrier of H2;
A207:     dom f = the carrier of N4 by FUNCT_2:def 1;
          H2 is finite by A18,Th4;
          then reconsider cH2 as finite set;
          H1 is finite by A17,Th4;
          then reconsider cH1 as finite set;
A208:     cS = cH1 \/ cH2 by A158,Def2;
A209:     dom (the InternalRel of H1) c= (the carrier of H1) by RELAT_1:
          def 18;
A210:     now
            assume
            [f.0,f.1] in the InternalRel of H1;
            then f.0 in dom (the InternalRel of H1) by RELAT_1:20;
            hence contradiction by A16,A206,A209,XBOOLE_0:3;
          end;
A211:     rng (the InternalRel of H1) c= (the carrier of H1) by RELAT_1:
          def 19;
A212:     now
            assume
            [f.1,f.0] in the InternalRel of H1;
            then f.0 in rng (the InternalRel of H1) by RELAT_1:20;
            hence contradiction by A16,A206,A211,XBOOLE_0:3;
          end;
          [f.1,f.0] in (the InternalRel of H1) \/ (the InternalRel of H2
          ) by A158,A166,Def2;
          then
A213:     [f.1,f.0] in the InternalRel of H2 by A212,XBOOLE_0:def 3;
A214:     H2 is strict non empty RelStr by A18,Th4;
          reconsider cS as finite set by A208;
A215:     rng (the InternalRel of H2) c= (the carrier of H2) by RELAT_1:
          def 19;
          [f.0,f.1] in (the InternalRel of H1) \/ (the InternalRel of H2
          ) by A158,A159,Def2;
          then
A216:     [f.0,f.1] in the InternalRel of H2 by A210,XBOOLE_0:def 3;
          then
A217:     f.1 in rng (the InternalRel of H2) by RELAT_1:20;
A218:     now
            assume
            [f.2,f.1] in the InternalRel of H1;
            then f.1 in rng (the InternalRel of H1) by RELAT_1:20;
            hence contradiction by A16,A215,A211,A217,XBOOLE_0:3;
          end;
A219:     H1 is non empty by A17,Th4;
A220:     cH2 <> cS
          proof
A221:       cS = cH1 \/ cH2 by A158,Def2;
            assume
A222:       cH2 = cS;
            consider x being set such that
A223:       x in cH1 by A219,XBOOLE_0:def 1;
            cH1 /\ cH2 = {} by A16,XBOOLE_0:def 7;
            then not x in cH2 by A223,XBOOLE_0:def 4;
            hence contradiction by A222,A221,A223,XBOOLE_0:def 3;
          end;
          cS = cH1 \/ cH2 by A158,Def2;
          then cH2 c= cS by XBOOLE_1:7;
          then cH2 c< cS by A220,XBOOLE_0:def 8;
          then
A224:     card cH2 < card cS by CARD_2:67;
A225:     [f.2,f.3] in (the InternalRel of H1) \/ (the InternalRel of H2
          ) by A158,A163,Def2;
A226:     [f.1,f.2] in (the InternalRel of H1) \/ (the InternalRel of H2
          ) by A158,A165,Def2;
          now
            assume
            [f.1,f.2] in the InternalRel of H1;
            then f.1 in dom (the InternalRel of H1) by RELAT_1:20;
            hence contradiction by A16,A215,A209,A217,XBOOLE_0:3;
          end;
          then
A227:     [f.1,f.2] in the InternalRel of H2 by A226,XBOOLE_0:def 3;
          then
A228:     f.2 in rng (the InternalRel of H2) by RELAT_1:20;
          now
            assume
            [f.2,f.3] in the InternalRel of H1;
            then f.2 in dom (the InternalRel of H1) by RELAT_1:20;
            hence contradiction by A16,A215,A209,A228,XBOOLE_0:3;
          end;
          then
A229:     [f.2,f.3] in the InternalRel of H2 by A225,XBOOLE_0:def 3;
          [f.2,f.1] in (the InternalRel of H1) \/ (the InternalRel of H2
          ) by A158,A164,Def2;
          then
A230:     [f.2,f.1] in the InternalRel of H2 by A218,XBOOLE_0:def 3;
A231:     now
            assume
            [f.3,f.2] in the InternalRel of H1;
            then f.2 in rng (the InternalRel of H1) by RELAT_1:20;
            hence contradiction by A16,A215,A211,A228,XBOOLE_0:3;
          end;
          [f.3,f.2] in (the InternalRel of H1) \/ (the InternalRel of H2
          ) by A158,A162,Def2;
          then
A232:     [f.3,f.2] in the InternalRel of H2 by A231,XBOOLE_0:def 3;
A233:     for x,y being Element of N4 holds [x,y] in the InternalRel of
          N4 iff [f.x,f.y] in the InternalRel of H2
          proof
            let x,y being Element of N4;
            thus [x,y] in the InternalRel of N4 implies [f.x,f.y] in the
            InternalRel of H2
            proof
              assume
A234:         [x,y] in the InternalRel of N4;
              per cases by A234,Th2,ENUMSET1:def 4;
              suppose
A235:           [x,y] = [0,1];
                then x = 0 by ZFMISC_1:33;
                hence thesis by A216,A235,ZFMISC_1:33;
              end;
              suppose
A236:           [x,y] = [1,0];
                then x = 1 by ZFMISC_1:33;
                hence thesis by A213,A236,ZFMISC_1:33;
              end;
              suppose
A237:           [x,y] = [1,2];
                then x = 1 by ZFMISC_1:33;
                hence thesis by A227,A237,ZFMISC_1:33;
              end;
              suppose
A238:           [x,y] = [2,1];
                then x = 2 by ZFMISC_1:33;
                hence thesis by A230,A238,ZFMISC_1:33;
              end;
              suppose
A239:           [x,y] = [2,3];
                then x = 2 by ZFMISC_1:33;
                hence thesis by A229,A239,ZFMISC_1:33;
              end;
              suppose
A240:           [x,y] = [3,2];
                then x = 3 by ZFMISC_1:33;
                hence thesis by A232,A240,ZFMISC_1:33;
              end;
            end;
            thus [f.x,f.y] in the InternalRel of H2 implies [x,y] in the
            InternalRel of N4
            proof
              [f.x,f.y] in the InternalRel of S implies [x,y] in the
              InternalRel of N4 by A157;
              then
A241:         [f.x,f.y] in (the InternalRel of H1) \/ (the InternalRel
              of H2) implies [x,y] in the InternalRel of N4 by A158,Def2;
              assume
              [f.x,f.y] in the InternalRel of H2;
              hence thesis by A241,XBOOLE_0:def 3;
            end;
          end;
A242:     f.3 in rng (the InternalRel of H2) by A229,RELAT_1:20;
          rng f c= the carrier of H2
          proof
            let y be set;
            assume
            y in rng f;
            then consider x being set such that
A243:       x in dom f and
A244:       y = f.x by FUNCT_1:def 5;
            per cases by A207,A243,Lm1,ENUMSET1:def 2;
            suppose
              x = 0;
              hence thesis by A206,A244;
            end;
            suppose
              x = 1;
              hence thesis by A215,A217,A244;
            end;
            suppose
              x = 2;
              hence thesis by A215,A228,A244;
            end;
            suppose
              x = 3;
              hence thesis by A215,A242,A244;
            end;
          end;
          then f is Function of the carrier of N4,the carrier of H2 by
          FUNCT_2:8;
          then H2 embeds N4 by A156,A233,NECKLACE:def 2;
          hence thesis by A5,A18,A214,A224;
        end;
      end;
      hence thesis by A19,A20;
    end;
  end;
  assume
  R embeds N4;
  then P[k] by A1;
  then
A245: ex i being Nat st P[i];
  P[0] from NAT_1:sch 7(A245,A2);
  hence thesis;
end;

