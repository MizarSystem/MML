   thus
     S. [x,y] = min(R. [x,y], (converse R). [x,y]) by FUZZY_1:def 5
       .= min(R. [x,y], R. [y,x]) by FUZZY_4:def 1
       .= min((converse R). [y,x], R. [y,x]) by FUZZY_4:def 1
       .= S. [y,x] by FUZZY_1:def 5;
