    end;
    BagOrder n linearly_orders Support(p*a) by POLYNOM2:20;
then rng(SgmX(BagOrder n, Support(p*a))) = {} by A5,TRIANG_1:def 2;
then SgmX(BagOrder n, Support(p*a)) = {} by FINSEQ_1:27;
then len y = 0 by A1,FINSEQ_1:25;
