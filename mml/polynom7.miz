end;
case A6: a <> 0.L;
A7: BagOrder n linearly_orders Support(p*a) by POLYNOM2:20;
A8: for u being set holds u in Support p implies u in Support(p*a)
   proof
   let u be set;
   assume A9: u in Support p;
   then A10: p.u <> 0.L by POLYNOM1:def 9;
