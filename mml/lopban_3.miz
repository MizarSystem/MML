theorem SEQ_4_33:
for X be RealNormSpace
  for seq,seq1 be sequence of X
    for k be Nat holds
  seq is convergent implies ((seq ^\k) is convergent & lim (seq ^\k)=lim seq)
  proof
   let X be RealNormSpace;
   let seq,seq1 be sequence of X;
   let k be Nat;
   assume A1:seq is convergent;
   A2:seq^\k is_subsequence_of seq by SEQM_3_47;
   hence seq^\k is convergent by A1, SEQ_4_30;
   thus lim (seq ^\k)=lim seq by A1,A2,SEQ_4_31;
  end;

theorem SEQ_4_35:
for X be RealNormSpace
  for seq,seq1 be sequence of X
  holds
  seq is convergent & (ex k be Nat st seq=seq1 ^\k)
     implies seq1 is convergent
  proof
  let X be RealNormSpace;
  let seq,seq1 be sequence of X;
  assume that
 A1: seq is convergent and
 A2: ex k be Nat st seq=seq1 ^\k;
     consider k be Nat such that
 A3: seq=seq1 ^\k by A2;
     consider g1 be Element of X  such that
 A4: for p be Real st 0<p ex n be Nat
       st for m be Nat st n<=m holds ||.seq.m-g1.||<p
     by A1,NORMSP_1:def 9;
     take g=g1;
     let p be Real; assume 0<p;
     then consider n1 be Nat such that
 A5: for m be Nat st n1<=m holds ||.seq.m-g1.||<p by A4;
     take n=n1+k;
     let m be Nat; assume
 A6: n<=m;
     then consider l be Nat such that
 A7: m=n1+k+l by NAT_1:28;
       m-k=n1+k+l+-k by A7,XCMPLX_0:def 8;
     then m-k=n1+l+k+-k by XCMPLX_1:1;
     then m-k=n1+l+k-k by XCMPLX_0:def 8;
     then m-k=n1+l+(k-k) by XCMPLX_1:29;
     then m-k=n1+l+0 by XCMPLX_1:14;
     then consider m1 be Nat such that
 A8: m1=m-k;
 A9:  now assume not n1<=m1;
        then m1+k<n1+k by REAL_1:53;
        then m-(k-k)<n1+k by A8,XCMPLX_1:37;
        then m-0<n1+k by XCMPLX_1:14;
        hence contradiction by A6;
      end;
 A10:  m1+k=m-(k-k) by A8,XCMPLX_1:37
      .=m-0 by XCMPLX_1:14
      .=m;
      ||.seq.m1-g1.||<p by A5,A9;
   hence ||.seq1.m-g.||<p by A3,A10,SEQM_3_def_9;
 end;

theorem SEQ_4_36:
for X be RealNormSpace
  for seq,seq1 be sequence of X
  holds
   seq is convergent & (ex k be Nat st seq=seq1 ^\k)
       implies lim seq1 =lim seq
  proof
  let X be RealNormSpace;
  let seq,seq1 be sequence of X;
     assume that
 A1: seq is convergent and
 A2: ex k be Nat st seq=seq1 ^\k;
 A3: seq1 is convergent by A1,A2,SEQ_4_35;
     consider k be Nat such that
 A4: seq=seq1 ^\k by A2;
        now let p be Real; assume 0<p;
       then consider n1 be Nat such that
 A5:   for m be Nat st n1<=m holds ||.seq.m-(lim seq).||<p
 by A1,NORMSP_1:def 11;
       take n=n1+k;
       let m be Nat; assume
 A6:   n<=m;
       then consider l be Nat such that
 A7:    m=n1+k+l by NAT_1:28;
         m-k=n1+k+l+-k by A7,XCMPLX_0:def 8;
       then m-k=n1+l+k+-k by XCMPLX_1:1;
       then m-k=n1+l+k-k by XCMPLX_0:def 8;
       then m-k=n1+l+(k-k) by XCMPLX_1:29;
       then m-k=n1+l+0 by XCMPLX_1:14;
       then consider m1 be Nat such that
 A8:     m1=m-k;
 A9:    now assume not n1<=m1;
         then m1+k<n1+k by REAL_1:53;
         then m-(k-k)<n1+k by A8,XCMPLX_1:37;
         then m-0<n1+k by XCMPLX_1:14;
         hence contradiction by A6;
        end;
 A10:   m1+k=m-(k-k) by A8,XCMPLX_1:37
       .=m-0 by XCMPLX_1:14
       .=m;
        ||.seq.m1-(lim seq) .||<p by A5,A9;
      hence ||.seq1.m-(lim seq).||<p by A4,A10,SEQM_3_def_9;
     end;
    hence thesis by A3,NORMSP_1:def 11;
  end;

theorem SEQ_4_39:
for X be RealNormSpace
  for seq be sequence of X
    holds
  seq is constant implies seq is convergent
  proof
  let X be RealNormSpace;
  let seq be sequence of X;
   assume seq is constant;
   then consider r be Element of X such that
   A1:for n be Nat holds seq.n=r by SEQM_3_def_6;
   take g=r;
   let p be Real such that A2:0<p;
   take n=0;
   let m be Nat such that n<=m;
     ||.(seq.m)-g.||=||.r-g.|| by A1
    .=||.0.X.|| by RLVECT_1:28
    .=0 by NORMSP_1:5;
   hence ||.(seq.m)-g.||<p by A2;
  end;

theorem Th5200:
for X be RealNormSpace
  for seq be sequence of X
  st (for n be Nat holds seq.n = 0.X)
  holds seq is norm_summable
  proof
  let X be RealNormSpace;
  let seq be sequence of X
  such that
A1:   for n be Nat holds seq.n = 0.X;
       take 0;
    let p be real number such that
A2:   0<p;
    take 0;
    let m be Nat such that 0<=m;
      abs((Partial_Sums ||.seq.||).m-0) = abs(0-0) by A1,Th2500
       .= 0 by ABSVALUE:def 1;
    hence abs((Partial_Sums ||.seq.||).m-0)<p by A2;
  end;

registration
let X be RealNormSpace;
 cluster norm_summable sequence of X;
 existence
  proof
  deffunc z(Nat)=0.X;
    consider C being sequence of X such that
A1:   for n be Nat holds C.n = z(n) from NORMSP_1:sch 1;
    take C;
    thus thesis by A1,Th5200;
  end;
end;

:::::::::: Norm Space versions of SERIES_1_7 - SERIES_1_16

theorem SERIES_1_7:
for X be RealNormSpace
  for s be sequence of X holds
s is summable implies s is convergent & lim s = 0.X
proof
  let X be RealNormSpace;
  let s be sequence of X;
 assume s is summable;
 then A1: Partial_Sums(s) is convergent
 by SERIES_1_def_2;
 A2: Partial_Sums(s) ^\1 is convergent &
    lim(Partial_Sums(s) ^\1) = lim(Partial_Sums(s)) by A1,SEQ_4_33;
 then A3: lim(Partial_Sums(s) ^\1 - Partial_Sums(s))
     = lim(Partial_Sums(s)) - lim(Partial_Sums(s)) by A1,NORMSP_1:43
    .= 0.X by RLVECT_1:28;
 A4: s ^\1 = Partial_Sums(s) ^\1 - Partial_Sums(s)
    proof
    now let n be Nat;
        (Partial_Sums(s)).(n+1) = (Partial_Sums(s)).n
        + s.(n+1) by SERIES_1_def_1;
      then (Partial_Sums(s)).(n+1) = (Partial_Sums(s)).n + (s ^\1).n
        by SEQM_3_def_9;
      then (Partial_Sums(s) ^\1).n = (s ^\1).n+  (Partial_Sums(s)).n
        by SEQM_3_def_9;
      then (Partial_Sums(s) ^\1).n- (Partial_Sums(s)).n
         = (s ^\1).n+ ( (Partial_Sums(s)).n- (Partial_Sums(s)).n )
                 by RLVECT_1:42;
      then (Partial_Sums(s) ^\1).n- (Partial_Sums(s)).n
         = (s ^\1).n+ ( 0.X) by RLVECT_1:28;
      hence
        (Partial_Sums(s) ^\1).n- (Partial_Sums(s)).n
         = (s ^\1).n by RLVECT_1:10;
    end;
      hence thesis by NORMSP_1:def 6;
    end;
 then s ^\1 is convergent by A1,A2,NORMSP_1:35;
 hence thesis by A3,A4,SEQ_4_35,SEQ_4_36;
end;

theorem SERIES_1_8:
for X be RealNormSpace
  for s1,s2 be sequence of X holds
Partial_Sums(s1)+Partial_Sums(s2) = Partial_Sums(s1+s2)
proof
  let X be RealNormSpace;
  let s1,s2 be sequence of X;
  A1: (Partial_Sums(s1) + Partial_Sums(s2)).0
  = Partial_Sums(s1).0 + Partial_Sums(s2).0 by NORMSP_1:def 5
 .= s1.0 + Partial_Sums(s2).0 by SERIES_1_def_1
 .= s1.0 + s2.0 by SERIES_1_def_1
 .= (s1+s2).0 by NORMSP_1:def 5;
    now let n be Nat;
    thus (Partial_Sums(s1) + Partial_Sums(s2)).(n+1)
   = Partial_Sums(s1).(n+1) + Partial_Sums(s2).(n+1) by NORMSP_1:def 5
  .= Partial_Sums(s1).n + s1.(n+1) + Partial_Sums(s2).(n+1) by SERIES_1_def_1
  .= Partial_Sums(s1).n+s1.(n+1)+(s2.(n+1)
  +Partial_Sums(s2).n) by SERIES_1_def_1
  .= Partial_Sums(s1).n+s1.(n+1)+s2.(n+1)+Partial_Sums(s2).n
  by RLVECT_1:def 6
  .= Partial_Sums(s1).n+(s1.(n+1)+s2.(n+1))
  +Partial_Sums(s2).n by RLVECT_1:def 6
  .= Partial_Sums(s1).n+(s1+s2).(n+1)+Partial_Sums(s2).n by NORMSP_1:def 5
  .= Partial_Sums(s1).n+Partial_Sums(s2).n+(s1+s2).(n+1) by RLVECT_1:def 6
  .= (Partial_Sums(s1)+Partial_Sums(s2)).n+(s1+s2).(n+1) by NORMSP_1:def 5;
  end;
  hence thesis by A1,SERIES_1_def_1;
end;

theorem SERIES_1_9:
for X be RealNormSpace
  for s1,s2 be sequence of X holds
Partial_Sums(s1)-Partial_Sums(s2) = Partial_Sums(s1-s2)
 proof
 let X be RealNormSpace;
   let s1,s2 be sequence of X;
   A1: (Partial_Sums(s1) - Partial_Sums(s2)).0
  = Partial_Sums(s1).0 - Partial_Sums(s2).0 by NORMSP_1:def 6
 .= s1.0 - Partial_Sums(s2).0 by SERIES_1_def_1
 .= s1.0 - s2.0 by SERIES_1_def_1
 .= (s1-s2).0 by NORMSP_1:def 6;
    now let n be Nat; thus
     (Partial_Sums(s1) - Partial_Sums(s2)).(n+1)
   = Partial_Sums(s1).(n+1) - Partial_Sums(s2).(n+1)
     by NORMSP_1:def 6
  .= (Partial_Sums(s1).n + s1.(n+1)) - Partial_Sums(s2).(n+1) by SERIES_1_def_1
  .= (Partial_Sums(s1).n+s1.(n+1))-(s2.(n+1)
  +Partial_Sums(s2).n) by SERIES_1_def_1
  .= (Partial_Sums(s1).n+s1.(n+1))-s2.(n+1)-Partial_Sums(s2).n
      by RLVECT_1:41
  .= Partial_Sums(s1).n+(s1.(n+1)-s2.(n+1))-Partial_Sums(s2).n
     by RLVECT_1:42
  .= (s1-s2).(n+1)+Partial_Sums(s1).n-Partial_Sums(s2).n by NORMSP_1:def 6
  .= (s1-s2).(n+1)+(Partial_Sums(s1).n-Partial_Sums(s2).n)
      by RLVECT_1:42
  .= (Partial_Sums(s1)-Partial_Sums(s2)).n+(s1-s2).(n+1) by NORMSP_1:def 6;
  end;
  hence thesis by A1,SERIES_1_def_1;
end;

registration
  let X be RealNormSpace;
  let seq be norm_summable sequence of X;
  cluster ||.seq.|| -> summable;
  coherence by SERIES_1_def_5;
end;

registration
  let X be RealNormSpace;
  cluster summable -> convergent sequence of X;
coherence by SERIES_1_7;
end;

theorem SERIES_1_10:
for X be RealNormSpace
  for seq1,seq2 be sequence of X
  st seq1 is summable & seq2 is summable holds
     seq1+seq2 is summable & Sum(seq1+seq2) = Sum(seq1)+Sum(seq2)
proof
let X be RealNormSpace;
let seq1,seq2 be sequence of X such that
AS0:    seq1 is summable and
AS1:    seq2 is summable;
A1:Partial_Sums(seq1) is convergent &
   Partial_Sums(seq2) is convergent by SERIES_1_def_2,AS0,AS1;
then A2:Partial_Sums(seq1)+Partial_Sums(seq2) is
convergent by NORMSP_1:34;
A3:Partial_Sums(seq1)+Partial_Sums(seq2)
   =Partial_Sums(seq1+seq2) by SERIES_1_8;
     Sum(seq1+seq2)=lim(Partial_Sums(seq1+seq2)) by SERIES_1_def_3
              .=lim(Partial_Sums(seq1)+Partial_Sums(seq2)) by SERIES_1_8
              .=lim(Partial_Sums(seq1))+lim(Partial_Sums(seq2))
                 by A1,NORMSP_1:42;
    then Sum(seq1+seq2)=Sum(seq1)+lim(Partial_Sums(seq2)) by SERIES_1_def_3
                .= Sum(seq1)+Sum(seq2) by SERIES_1_def_3;
hence thesis by A2,A3,SERIES_1_def_2;
end;

theorem SERIES_1_11:
for X be RealNormSpace
  for seq1,seq2 be sequence of X
    st seq1 is summable & seq2 is summable holds
seq1-seq2 is summable & Sum(seq1-seq2)= Sum(seq1)-Sum(seq2)
proof
  let X be RealNormSpace;
  let seq1,seq2 be sequence of X such that
  AS0:      seq1 is summable & seq2 is summable;
 A1:Partial_Sums(seq1) is convergent &
   Partial_Sums(seq2) is convergent by SERIES_1_def_2,AS0;
then A2:Partial_Sums(seq1)-Partial_Sums(seq2) is
convergent by NORMSP_1:35;
A3:Partial_Sums(seq1)-Partial_Sums(seq2)
   =Partial_Sums(seq1-seq2) by SERIES_1_9;
   Sum(seq1-seq2)=lim(Partial_Sums(seq1-seq2)) by SERIES_1_def_3
              .=lim(Partial_Sums(seq1)-Partial_Sums(seq2)) by SERIES_1_9
              .=lim(Partial_Sums(seq1))-lim(Partial_Sums(seq2))
                 by A1,NORMSP_1:43;
    then Sum(seq1-seq2)=Sum(seq1)-lim(Partial_Sums(seq2)) by SERIES_1_def_3
                .= Sum(seq1)-Sum(seq2) by SERIES_1_def_3;
hence thesis by A2,A3,SERIES_1_def_2;
end;

registration
  let X be RealNormSpace;
  let seq1, seq2 be summable sequence of X;
  cluster seq1 + seq2 -> summable;
coherence by SERIES_1_10;
  cluster seq1 - seq2 -> summable;
coherence by SERIES_1_11;
end;

theorem SERIES_1_12:
for X be RealNormSpace
  for seq be sequence of X
       for z be Real holds
Partial_Sums(z * seq) = z * Partial_Sums(seq)
proof
let X be RealNormSpace;
let seq be sequence of X;
let z be Real;
defpred P[Nat] means Partial_Sums(z * seq).$1 = (z * Partial_Sums(seq)).$1;
A1:P[0]
proof
 thus Partial_Sums(z * seq).0 = (z * seq).0 by SERIES_1_def_1
.=z * seq.0 by NORMSP_1:def 8
.=z * Partial_Sums(seq).0 by SERIES_1_def_1
.=(z * Partial_Sums(seq)).0 by NORMSP_1:def 8;
end;
A2:now let n be Nat;
assume
A3: P[n];
thus P[n+1]
proof
thus Partial_Sums(z * seq).(n+1)
=Partial_Sums(z * seq).n + (z * seq).(n+1) by SERIES_1_def_1
.=(z * Partial_Sums(seq).n )+ (z * seq).(n+1) by A3,NORMSP_1:def 8
.=(z * Partial_Sums(seq).n )+ (z * seq.(n+1)) by NORMSP_1:def 8
.= z * ( Partial_Sums(seq).n + seq.(n+1)) by RLVECT_1:def 9
.= z * ( Partial_Sums(seq).(n+1)) by SERIES_1_def_1
.= (z * Partial_Sums(seq)).(n+1) by NORMSP_1:def 8;
end;
end;
    for n be Nat holds P[n] from NAT_1:sch 1(A1,A2);
hence thesis by FUNCT_2:113;
end;

theorem SERIES_1_13:
  for X be RealNormSpace
  for seq be summable sequence of X
    for z be Real
       holds z *seq is summable & Sum(z *seq)= z * Sum(seq)
proof
  let X be RealNormSpace;
  let seq be summable sequence of X;
  let z be Real;
A1: Partial_Sums(seq) is convergent by SERIES_1_def_2;
then A2:(z *Partial_Sums(seq)) is convergent by NORMSP_1:37;
A3: Partial_Sums(z *seq)=(z *Partial_Sums(seq)) by SERIES_1_12;
   Sum(z *seq)=lim(Partial_Sums(z *seq)) by SERIES_1_def_3
            .=lim((z *Partial_Sums(seq))) by SERIES_1_12
            .=z * lim(Partial_Sums(seq)) by A1,NORMSP_1:45
            .=z * Sum(seq) by SERIES_1_def_3;
hence z *seq is summable
& Sum(z *seq)= z * Sum(seq) by A2,A3,SERIES_1_def_2;
end;

registration
  let X be RealNormSpace;
  let z be Real, seq be summable sequence of X;
  cluster z *seq -> summable;
  coherence by SERIES_1_13;
end;

theorem SERIES_1_14:
  for X be RealNormSpace
  for s,s1 be sequence of X
    st for n be Nat holds s1.n=s.0 holds
Partial_Sums(s^\1) = (Partial_Sums(s)^\1) - s1
proof
  let X be RealNormSpace;
  let s,s1 be sequence of X;
 assume A1: for n be Nat holds s1.n=s.0;
 A2: ((Partial_Sums(s)^\1) - s1).0 = (Partial_Sums(s)^\1).0 - s1.0
    by NORMSP_1:def 6
 .= (Partial_Sums(s)^\1).0 - s.0 by A1
 .= Partial_Sums(s).(0+1) - s.0 by SEQM_3_def_9
 .= Partial_Sums(s).0 + s.(0+1) - s.0 by SERIES_1_def_1
 .= s.(0+1) + s.0 - s.0 by SERIES_1_def_1
 .= s.(0+1) + (s.0 - s.0) by RLVECT_1:42
 .= s.(0+1) + 0.X  by RLVECT_1:28
 .=s.(0+1) by RLVECT_1:10
 .= (s^\1).0 by SEQM_3_def_9;
   now let k be Nat;
   thus ((Partial_Sums(s)^\1) - s1).(k+1)
  = (Partial_Sums(s)^\1).(k+1) - s1.(k+1) by NORMSP_1:def 6
  .= (Partial_Sums(s)^\1).(k+1) - s.0 by A1
  .= Partial_Sums(s).(k+1+1) - s.0 by SEQM_3_def_9
  .= s.(k+1+1) + Partial_Sums(s).(k+1) - s.0 by SERIES_1_def_1
  .= s.(k+1+1) + Partial_Sums(s).(k+1) - s1.k by A1
  .= s.(k+1+1) + (Partial_Sums(s).(k+1) - s1.k) by RLVECT_1:42
  .= s.(k+1+1) + ((Partial_Sums(s)^\1).k - s1.k) by SEQM_3_def_9
  .= s.(k+1+1) + ((Partial_Sums(s)^\1) - s1).k by NORMSP_1:def 6
  .= ((Partial_Sums(s)^\1) - s1).k + (s^\1).(k+1) by SEQM_3_def_9;
 end;
 hence thesis by A2,SERIES_1_def_1;
end;

theorem SERIES_1_15:
  for X be RealNormSpace
  for s be sequence of X holds
    s is summable implies for n be Nat holds s^\n is summable
proof
  let X be RealNormSpace;
  let s be sequence of X;
defpred X[Nat] means s^\$1 is summable;
 assume s is summable;
 then A1: X[0] by SEQM_3_34;
 A2: for n be Nat st X[n] holds X[n+1]
 proof
   let n be Nat;
  assume s^\n is summable; then Partial_Sums(s^\n) is convergent
  by SERIES_1_def_2;
  then A3: Partial_Sums(s^\n)^\1 is convergent by SEQ_4_33;
  A4: s^\(n+1)=(s^\n)^\1 by SEQM_3_36;
  deffunc U(Nat) = (s^\n).0;
  consider s1 be sequence of X such that
A5: for k be Nat holds s1.k = U(k) from NORMSP_1:sch 1;
    s1 is constant by A5,SEQM_3_def_6;
  then A6: s1 is convergent by SEQ_4_39;
    Partial_Sums(s^\n^\1) = (Partial_Sums(s^\n)^\1) - s1 by A5,SERIES_1_14;
  then Partial_Sums(s^\n^\1) is convergent by A3,A6,NORMSP_1:35;
  hence thesis by A4,SERIES_1_def_2;
 end;
 thus for n be Nat holds X[n] from NAT_1:sch 1(A1,A2);
end;

registration
  let X be RealNormSpace;
  let seq be summable sequence of X, n be Nat;
  cluster seq^\n -> summable;
  coherence by SERIES_1_15;
end;

theorem Th6200:
for X be RealNormSpace
for seq be sequence of X holds
Partial_Sums ||.seq.|| is bounded_above iff seq is norm_summable
proof
  let X be RealNormSpace;
  let seq be sequence of X;
  for n be Nat holds 0 <=||.seq.||.n by LM2000;
   then Partial_Sums(||.seq.||) is bounded_above
   iff ||.seq.|| is summable by SERIES_1:20;
  hence thesis by SERIES_1_def_5;
end;

registration
  let X be RealNormSpace;
  let seq be norm_summable sequence of X;
  cluster Partial_Sums ||.seq.|| -> bounded_above;
  coherence by Th6200;
end;

theorem Th6300:
for X be RealBanachSpace
for seq be sequence of X holds
seq is summable iff (for p be Real st 0 <p holds ex n be Nat st
 for m be Nat st n <= m holds ||.Partial_Sums(seq).m-Partial_Sums(seq).n.||<p)
proof
  let X be RealBanachSpace;
  let seq be sequence of X;
A1:now assume seq is summable;
    then Partial_Sums(seq) is convergent by SERIES_1_def_2;
    hence for p be Real st 0<p holds ex n be Nat st for m be Nat st n <= m
    holds ||.Partial_Sums(seq).m-Partial_Sums(seq).n.||<p by LM2100;
    end;
  now assume for p be Real st 0<p
     holds ex n be Nat st for m be Nat st n <= m
    holds ||.Partial_Sums(seq).m-Partial_Sums(seq).n.||< p;
    then Partial_Sums(seq) is Cauchy_sequence_by_Norm by LM2105;
    then Partial_Sums(seq) is convergent by LOPBAN_1:def 16;
    hence seq is summable by SERIES_1_def_2;
    end;
hence thesis by A1;
end;

theorem SERIES_1_39:
  for X be RealNormSpace
  for s be sequence of X
   for n,m be Nat st n<=m
holds ||.Partial_Sums(s).m - Partial_Sums(s).n.||
      <= abs(Partial_Sums(||.s.||).m - Partial_Sums(||.s.||).n)
proof
  let X be RealNormSpace;
  let s be sequence of X;
  let n,m be Nat such that
  A1: n<=m;
 set s1=Partial_Sums(s);
 set s2=Partial_Sums(||.s.||);
  defpred X[Nat] means ||.s1.(n+$1) - s1.n.|| <= abs(s2.(n+$1) - s2.n);
   ||.s1.(n+0) - s1.n.|| = ||.0.X.|| by RLVECT_1:28
.= 0 by NORMSP_1:5;
 then A2: X[0] by ABSVALUE:5;
   now let k be Nat;
   ||.s.k.|| >=0 by NORMSP_1:8;
  hence ||.s.||.k >= 0 by NORMSP_1:def 10;
 end;
 then A3: s2 is non-decreasing by SERIES_1:19;
 A4: for k be Nat st X[k] holds X[k+1]
  proof let k be Nat;
  assume A5: ||. s1.(n+k) - s1.n.|| <= abs(s2.(n+k) - s2.n);
      ||. s1.(n+(k+1)) - s1.n.|| = ||. s1.(n+k+1) - s1.n .|| by XCMPLX_1:1
    .= ||. s.(n+k+1) + s1.(n+k) - s1.n .|| by SERIES_1_def_1
    .= ||. s.(n+k+1) + (s1.(n+k) - s1.n).|| by RLVECT_1:42;
    then A6: ||. s1.(n+(k+1))-s1.n .|| <= ||.s.(n+k+1).||+||.s1.(n+k) - s1.n.||
      by NORMSP_1:def 2;
      ||.s.(n+k+1).|| + ||. s1.(n+k) - s1.n.|| <=
    ||.s.(n+k+1).|| + abs(s2.(n+k) - s2.n) by A5,REAL_1:55;
    then A7: ||. s1.(n+(k+1))-s1.n.||
  <= ||. s.(n+k+1).||+ abs(s2.(n+k)-s2.n) by A6,AXIOMS:22;
      s2.(n+k)>=s2.n by A3,SEQM_3:11;
    then A8: s2.(n+k) - s2.n >= 0 by SQUARE_1:12;
    then A9: ||. s1.(n+(k+1))-s1.n .||
       <= ||. s.(n+k+1).||+(s2.(n+k)-s2.n) by A7,ABSVALUE:def 1;
      ||. s .(n+k+1) .|| >= 0 by NORMSP_1:8;
    then A10: ||. s .(n+k+1).|| +(s2.(n+k)-s2.n) >= 0+0 by A8,REAL_1:55;
     abs(s2.(n+(k+1)) - s2.n) = abs(s2.(n+k+1) - s2.n) by XCMPLX_1:1
                       .= abs(s2.(n+k) + ( ||.s.||).(n+k+1) - s2.n)
                       by SERIES_1:def 1
                       .= abs(s2.(n+k) +  ||.s.(n+k+1).||-s2.n)
                       by NORMSP_1:def 10
                   .= abs(||.s.(n+k+1).|| + (s2.(n+k) - s2.n)) by XCMPLX_1:29;
  hence ||.s1.(n+(k+1))-s1.n.|| <= abs(s2.(n+(k+1))-s2.n)
  by A9,A10,ABSVALUE:def 1;
  end;
  A11: for k be Nat holds X[k] from NAT_1:sch 1(A2,A4);
     reconsider k=m-n as Nat by A1,INT_1:18;
     n+k = m by XCMPLX_1:27;
   hence thesis by A11;
  end;

theorem SERIES_1_40:
  for X be RealBanachSpace
  for seq be sequence of X holds
  seq is norm_summable implies seq is summable
proof
  let X be RealBanachSpace;
  let seq be sequence of X;
  assume seq is norm_summable;
  then ||.seq.|| is summable by SERIES_1_def_5;
  then A1:  Partial_Sums(||.seq.||) is convergent by SERIES_1:def 2;
  now let p be Real;
    assume 0<p;
    then consider n be Nat such that
    A2: for m be Nat st n <= m holds
    abs(Partial_Sums(||.seq.||).m-Partial_Sums(||.seq.||).n)< p
    by A1,SEQ_4:58;
   take n;
let m be Nat; assume GG:  n <= m;
    then A3: abs(Partial_Sums(||.seq.||).m-Partial_Sums(||.seq.||).n)< p by A2;
      ||.Partial_Sums(seq).m-Partial_Sums(seq).n.||
    <= abs(Partial_Sums(||.seq.||).m-Partial_Sums(||.seq.||).n)
    by GG,SERIES_1_39;
   hence ||.Partial_Sums(seq).m-Partial_Sums(seq).n.|| <p by A3,AXIOMS:22;
   end;
hence seq is summable by Th6300;
end;

theorem
 for X be RealNormSpace
   for rseq1 be Real_Sequence
     for seq2 be sequence of X holds
       rseq1 is summable &
       (ex m be Nat st for n be Nat st m<=n holds ||.seq2.n.|| <= rseq1.n )
       implies seq2 is norm_summable
proof
 let X be RealNormSpace;
 let rseq1 be Real_Sequence;
 let seq2 be sequence of X;
assume
A1:rseq1 is summable
& (ex m be Nat st for n be Nat st  m<=n holds ||.seq2.n.|| <= rseq1.n );
then consider m be Nat such that
A2:  for n be Nat st m<=n holds ||.seq2.n.|| <= rseq1.n;
A3:now let n be Nat; assume m <= n;
      then ||.seq2.n.|| <= rseq1.n by A2;
   hence ||.seq2.||.n <= rseq1.n by NORMSP_1:def 10;
end;
  now
   let n be Nat;
   ||.seq2.n.||=||.seq2.||.n by NORMSP_1:def 10;
   hence 0 <= ||.seq2.||.n by NORMSP_1:8;
end;
hence ||.seq2.|| is summable by A1,A3,SERIES_1:22;
end;

theorem
 for X be RealNormSpace
     for seq1,seq2 be sequence of X
holds
  (for n be Nat holds 0 <= ||.seq1.||.n & ||.seq1.||.n <= ||.seq2.||.n)
 & seq2 is norm_summable implies
  seq1 is norm_summable & Sum ||.seq1.|| <= Sum ||.seq2.||
proof
let X be RealNormSpace;
let seq1,seq2 be sequence of X;
assume A1:
 (for n be Nat holds 0 <= ||.seq1.||.n & ||.seq1.||.n <= ||.seq2.||.n)
& seq2 is norm_summable;
then ||.seq2.|| is summable by SERIES_1_def_5;
hence ||.seq1.|| is summable & Sum(||.seq1.||)
<=Sum(||.seq2.||) by A1,SERIES_1:24;
end;

theorem
 for X be RealNormSpace
     for seq be sequence of X
holds
  (for n be Nat  holds ||.seq.||.n>0) &
 (ex m be Nat st for n be Nat st n >=m
 holds ||.seq.||.(n+1)/||.seq.||.n >= 1)
  implies not seq is norm_summable
proof
 let X be RealNormSpace;
 let seq be sequence of X;
assume (for n be Nat holds ||.seq.||.n>0)
  & (ex m be Nat st for n be Nat st n >=m holds ||.seq.||.(n+1)/
||.seq.||.n >= 1);
hence ||.seq.|| is not summable by SERIES_1:31;
end;

theorem
  for X be RealNormSpace
       for seq be sequence of X
        for rseq1 be Real_Sequence holds
  (for n be Nat holds rseq1.n = n-root (||.seq.||.n)) & rseq1 is convergent &
  lim rseq1 < 1
implies seq is norm_summable
proof
