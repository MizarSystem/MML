begin :: Basic Properties of Sequences of Norm Space

definition
  let X be non empty NORMSTR;
  let seq be sequence of X;
   func Partial_Sums seq -> sequence of X means :SERIES_1_def_1:
  it.0 = seq.0 & for n be Nat
  holds it.(n+1) = it.n + seq.(n+1);
existence
proof
      deffunc G(Nat,set)= (the add of X).($2,seq.($1+1));
      consider g be Function such that
      AS2:   dom g = NAT & g.0 = seq.0
      & for n being Element of NAT holds g.(n+1) = G(n,g.n)
      from RECDEF_1:sch 3;
      defpred P[Nat] means g.$1 in the carrier of X;
       A0: P[0] by AS2;
       A1:  for k be Nat st P[k] holds P[k+1]
       proof
       let k be Nat such that
         BS1: P[k];
         reconsider gk=g.k as Element of the carrier of X by BS1;
         g.(k+1)=(the add of X).(gk,seq.(k+1)) by AS2;
         hence thesis;
       end;
       A2:  for n be Nat holds P[n] from NAT_1:sch 1(A0,A1);
       A3: for n be set st n in NAT
          holds g.n in the carrier of X by A2;
        A4: g is Function of NAT,the carrier of X by A3,AS2,FUNCT_2:5;
         reconsider g0=g as sequence of X by A4;
           take g0;
         now let n be Nat;
          thus g0.(n+1) = (the add of X).(g0.n,seq.(n+1)) by AS2
            .=g0.n+seq.(n+1) by RLVECT_1:5;
        end;
        hence thesis by AS2;
end;
uniqueness
proof
 let seq1,seq2 be sequence of X;
 assume that A2: seq1.0=seq.0 & for n be Nat holds
                seq1.(n+1)=seq1.n + seq.(n+1) and
             A3: seq2.0=seq.0 & for n be Nat holds
                seq2.(n+1)=seq2.n + seq.(n+1);
   defpred P[Nat] means seq1.$1 = seq2.$1;
 A4: P[0] by A2,A3;
 A5: for k be Nat st P[k] holds P[k+1]
     proof let k be Nat;
      assume P[k];
      hence seq1.(k+1) = seq2.k + seq.(k+1) by A2
                      .= seq2.(k+1) by A3;
      end;
   for n be Nat holds P[n] from NAT_1:sch 1(A4,A5);
 hence seq1 = seq2 by FUNCT_2:113;
end;
end;

theorem COMSEQ_3_2:
for X be add-associative right_zeroed right_complementable (non empty NORMSTR)
 for seq be sequence of X st
   ( for n be Nat holds seq.n = 0.X)
     holds for m be Nat holds (Partial_Sums seq).m = 0.X
 proof
let X be add-associative right_zeroed right_complementable (non empty NORMSTR);
   let seq be sequence of X such that
    A1:   for n be Nat holds seq.n = 0.X;
    let m be Nat;
    defpred P[Nat] means seq.$1 = (Partial_Sums seq).$1;
A2: P[0] by SERIES_1_def_1;
A3: for k be Nat st P[k] holds P[k+1]
    proof let k be Nat such that
A4:   P[k];
      thus seq.(k+1) = 0.X + seq.(k+1) by RLVECT_1:10
                    .= (Partial_Sums seq).k + seq.(k+1) by A1,A4
                    .= (Partial_Sums seq).(k+1) by SERIES_1_def_1;
    end;
      for n be Nat holds P[n] from NAT_1:sch 1(A2,A3);
    then seq = Partial_Sums seq by FUNCT_2:113;
    hence (Partial_Sums seq).m = 0.X by A1;
  end;

definition
let X be RealNormSpace;
let seq be sequence of X;
 attr seq is summable means :SERIES_1_def_2:
  Partial_Sums seq is convergent;
end;

registration
let X be RealNormSpace;
cluster summable sequence of X;
existence
  proof
  deffunc G(Nat) = 0.X;
    consider C being sequence of X such that
A1:   for n be Nat holds C.n = G(n) from NORMSP_1:sch 1;
    take C;
    take 0.X;
    let p be Real such that
A2:   0<p;
    take 0;
    let m be Nat such that 0<=m;
     ||.Partial_Sums C.m-0.X.|| = ||.0.X-0.X.|| by COMSEQ_3_2,A1
    .=0 by NORMSP_1:10;
    hence ||.Partial_Sums C.m-0.X.||<p by A2;
  end;
end;

definition
  let X be RealNormSpace;
  let seq be sequence of X;
 func Sum seq -> Element of X equals :SERIES_1_def_3:
  lim Partial_Sums seq;
correctness;
end;

definition
  let X be RealNormSpace;
  let seq be sequence of X;
attr seq is norm_summable means :SERIES_1_def_5:
 ||.seq.|| is summable;
end;

theorem LM2000:
for X be RealNormSpace
  for seq be sequence of X
      for m be Nat holds  0 <= ||.seq.||.m
  proof
  let X be RealNormSpace;
  let seq be sequence of X;
  let m be Nat;
     ||.seq.||.m = ||.seq.m.|| by NORMSP_1:def 10;
  hence thesis by NORMSP_1:8;
  end;

theorem LM2010:
for X be RealNormSpace
  for x,y,z be Element of X
     holds ||.x-y.|| = ||.(x-z)+(z-y).||
 proof
    let X be RealNormSpace;
    let x,y,z be Element of X;
 thus ||.x-y.|| = ||.x-0.X-y.|| by RLVECT_1:26
     .= ||.x-(z+-z)-y.|| by RLVECT_1:16
     .= ||.x-(z-z)-y.|| by RLVECT_1:def 11
     .= ||.(x-z)+z-y.|| by RLVECT_1:43
     .= ||.(x-z)+(z-y).|| by RLVECT_1:42;
end;

theorem LM2100:
for X be RealNormSpace
  for seq be sequence of X holds
        seq is convergent implies
         for s be Real st 0<s ex n be Nat
         st for m be Nat st n<=m holds ||.seq.m -seq.n.||<s
    proof
    let X be RealNormSpace;
    let seq be sequence of X;
     assume seq is convergent;
     then consider g1 be Element of X such that
  A1: for s be Real st 0<s
     ex n be Nat st for m be Nat st n<=m holds ||.seq.m -g1.||<s
     by NORMSP_1:def 9;
     let s be Real; assume 0<s;
     then 0<s/2 by SEQ_2:3;
     then consider n be Nat such that
  A2: for m be Nat st n<=m holds ||.seq.m -g1.||<s/2 by A1;
       now
        let m be Nat such that A3: n<=m;
          ||.seq.n -g1.||<s/2 by A2;
     then A4: ||.g1-seq.n.||<s/2 by NORMSP_1:11;
       ||.seq.m -g1.||<s/2 by A2,A3;
     then ||.seq.m -g1.||+||.g1-seq.n.||<s/2+s/2 by A4,REAL_1:67;
  then A5:  ||.seq.m -g1.||+||.g1-seq.n.||<s by XCMPLX_1:66;
  A6: ||.seq.m -g1+(g1-seq.n).||<=||.seq.m -g1.||+||.g1-seq.n.||
            by NORMSP_1:def 2;
        ||.seq.m -g1+(g1-seq.n).|| = ||.seq.m -seq.n.|| by LM2010;
        hence ||.seq.m -seq.n.|| < s by A5,A6,AXIOMS:22;
   end;
   hence thesis;
   end;

theorem LM2105:
for X be RealNormSpace
  for seq be sequence of X holds
        seq is Cauchy_sequence_by_Norm
        iff
       for p be Real st p > 0 holds
           ex n be Nat st for m be Nat st n <= m
            holds ||.seq.m-seq.n.||< p
    proof
    let X be RealNormSpace;
    let seq be sequence of X;
   D0: now assume
      D1:      seq is Cauchy_sequence_by_Norm;
      thus for p be Real st p > 0 holds
           ex n be Nat st for m be Nat st n <= m
            holds ||.seq.m-seq.n.||< p
        proof
        let p be Real such that
           H1: p >0;
           consider n be Nat such that
           H2: for m,k be Nat st n <= m & n <=k
            holds ||.seq.m-seq.k.||< p by H1,D1,RSSPACE3:10;
            for m be Nat st n <= m holds ||.seq.m-seq.n.||<p by H2;
         hence thesis;
      end;
     end;
   now assume
   A1:     for p be Real st p > 0 holds
               ex n be Nat st for m be Nat st n <= m
             holds ||.seq.m-seq.n.||< p;
     now let s be Real;
      assume 0<s;
     then 0<s/2 by SEQ_2:3;
          then consider k be Nat such that
  A2: for m be Nat st k<=m holds ||.seq.m -seq.k.||<s/2 by A1;
       now
         let m,n be Nat such
          that A3: k<=m & k<=n;
          F1:   ||.seq.m -seq.k.||<s/2 by A2,A3;
            ||.seq.n -seq.k.||<s/2 by A2,A3;
          then ||.seq.k-seq.n.||<s/2 by NORMSP_1:11;
          then ||.seq.m -seq.k.||+||.seq.k-seq.n.||<s/2+s/2
           by F1,REAL_1:67;
          then
          A5: ||.seq.m -seq.k.||+||.seq.k-seq.n.||<s by XCMPLX_1:66;
          A6: ||.seq.m -seq.k+(seq.k-seq.n).||
          <=||.seq.m -seq.k.||+||.seq.k-seq.n.|| by NORMSP_1:def 2;
              ||.seq.m -seq.k+(seq.k-seq.n).||
              =||.seq.m -seq.n.|| by LM2010;
          hence ||.seq.m -seq.n.|| < s  by A5,A6,AXIOMS:22;
       end;
       hence ex k be Nat st
        for m,n be Nat st k <= m & k <=n holds ||.seq.m-seq.n.||< s;
           end;
       hence seq is Cauchy_sequence_by_Norm by RSSPACE3:10;
    end;
hence thesis by D0;
end;

theorem Th2500:
for X be RealNormSpace
  for seq be sequence of X
  st (for n be Nat holds seq.n = 0.X)
    holds for m be Nat holds (Partial_Sums ||.seq.||).m = 0
  proof
  let X be RealNormSpace;
  let seq be sequence of X such that
A1:   for n be Nat holds seq.n = 0.X;
    let m be Nat;
    defpred P[Nat] means ||.seq.||.$1 = (Partial_Sums ||.seq.||).$1;
A2: P[0] by SERIES_1:def 1;
A3: for k be Nat st P[k] holds P[k+1]
    proof let k be Nat such that
A4:   P[k];
      thus ||.seq.||.(k+1) =0 +  ||.seq.||.(k+1)
                               .=  ||.0.X.|| + ||.seq.||.(k+1) by  NORMSP_1:5
                        .= ||.seq.k.|| + ||.seq.||.(k+1) by A1
                        .= (Partial_Sums ||.seq.||).k + ||.seq.||.(k+1) by A4,
NORMSP_1:def 10
                        .= (Partial_Sums ||.seq.||).(k+1) by SERIES_1:def 1;
    end;
      for n be Nat  holds  P[n] from NAT_1:sch 1(A2,A3);
    hence (Partial_Sums ||.seq.||).m = ||.seq.||.m
                                  .= ||.seq.m.|| by NORMSP_1:def 10
                                  .= ||.0.X.|| by A1
                                  .= 0 by NORMSP_1:5;
  end;

definition
let X be RealNormSpace;
let seq be sequence of X;
  redefine attr seq is constant means   :SEQM_3_def_6:
  ex r be Element of X st for n be Nat holds seq.n=r;
 compatibility
  proof
A1:dom seq = NAT by FUNCT_2:def 1;
   hereby assume
A2:   seq is constant;
    consider n0 being Nat;
    reconsider r = seq.n0 as Element of X;
    take r;
    let n be Nat;
    thus seq.n = r by A1,A2,SEQM_3:def 5;
   end;
   given r be Element of X such that
A3:  for n be Nat holds seq.n=r;
AA: for n1,n2 being set st n1 in dom seq & n2 in dom seq
   holds seq.n1=seq.n2
   proof
     let n1,n2 be set;
     assume
A4:  n1 in dom seq & n2 in dom seq;
      hence seq.n1 = r by A1,A3 .= seq.n2 by A1,A3,A4;
  end;
  thus seq is constant by SEQM_3:def 5,AA;
 end;
end;

definition
let X be RealNormSpace;
let seq be sequence of X;
let k be Nat;
func seq ^\k -> sequence of X means :SEQM_3_def_9:
  for n be Nat holds it.n=seq.(n+k);
existence
 proof deffunc U(Nat) = seq.($1+k);
  thus ex f being sequence of X st
   for n be Nat  holds f.n=U(n) from NORMSP_1:sch 1;
 end;
uniqueness
 proof
  let seq1,seq2 be sequence of X;
  assume that A1:for n be Nat holds seq1.n=seq.(n+k) and
              A2:for n be Nat holds seq2.n=seq.(n+k);
     now let n be Nat;
      seq1.n=seq.(n+k) & seq2.n=seq.(n+k) by A1,A2;
    hence seq1.n=seq2.n;
   end;
  hence seq1=seq2 by FUNCT_2:113;
 end;
end;

definition
let X be non empty 1-sorted;
let Nseq be increasing Seq_of_Nat;
let seq be sequence of X;
redefine func seq*Nseq -> Function of NAT, the carrier of X;
 coherence
  proof
     A1:dom Nseq=NAT by FUNCT_2:def 1;
     rng Nseq c= NAT by SEQM_3:def 8;
     then rng Nseq c= dom seq by FUNCT_2:def 1;
     then dom (seq*Nseq)=NAT by A1,RELAT_1:46;
     hence thesis by FUNCT_2:def 1;
end;
end;

definition
let X be non empty 1-sorted;
let Nseq be increasing Seq_of_Nat;
let seq be sequence of X;
redefine func seq*Nseq -> sequence of X;
 coherence
proof
thus seq*Nseq is Function of NAT, the carrier of X;
end;
end;

definition
  let X be RealNormSpace;
  let seq,seq1 be sequence of X;
  pred seq is_subsequence_of seq1 means :SEQM_3_def_10:
   ex Nseq be increasing Seq_of_Nat st seq=seq1*Nseq;
end;

theorem SEQM_3_31:
for X be non empty 1-sorted
  for seq be sequence of X
   for Nseq be increasing Seq_of_Nat holds
      for n be Nat holds (seq*Nseq).n=seq.(Nseq.n)
  proof
 let X be non empty 1-sorted;
 let seq be sequence of X;
 let Nseq be increasing Seq_of_Nat;
 let n be Nat;
    dom (seq*Nseq)=NAT by FUNCT_2:def 1;
    hence (seq*Nseq).n=seq.(Nseq.n) by FUNCT_1:22;
  end;

theorem SEQM_3_34:
for X be RealNormSpace
  for seq be sequence of X holds
   seq ^\0 =seq
  proof
  let X be RealNormSpace;
  let seq be sequence of X;
    now let n be Nat;
    thus (seq ^\0).n=seq.(n+0) by SEQM_3_def_9
                  .=seq.n;
   end;
  hence thesis by FUNCT_2:113;
 end;

theorem
for X be RealNormSpace
  for seq be sequence of X
    for k, m be Nat holds
   (seq ^\k)^\m=(seq ^\m)^\k
  proof
     let X be RealNormSpace;
     let seq be sequence of X;
     let k,m be Nat;
      now let n be Nat;
     thus ((seq ^\k)^\m).n=(seq ^\k).(n+m) by SEQM_3_def_9
      .=seq.(n+m+k) by SEQM_3_def_9
      .=seq.(n+k+m) by XCMPLX_1:1
      .=(seq ^\m).(n+k) by SEQM_3_def_9
      .=((seq ^\m)^\k).n by SEQM_3_def_9;
    end;
   hence thesis by FUNCT_2:113;
  end;

theorem SEQM_3_36:
for X be RealNormSpace
  for seq be sequence of X
   for k, m be Nat holds
   (seq ^\k)^\m=seq ^\(k+m)
  proof
     let X be RealNormSpace;
     let seq be sequence of X;
     let k,m be Nat;
      now let n be Nat;
     thus ((seq ^\k)^\m).n=(seq ^\k).(n+m) by SEQM_3_def_9
      .=seq.(n+m+k) by SEQM_3_def_9
      .=seq.(n+(k+m)) by XCMPLX_1:1
      .=(seq ^\(k+m)).n by SEQM_3_def_9;
    end;
   hence thesis by FUNCT_2:113;
  end;

theorem SEQ_4_30:
for X be RealNormSpace
  for seq,seq1 be sequence of X
  holds
    seq1 is_subsequence_of seq & seq is convergent implies
        seq1 is convergent
  proof
  let X be RealNormSpace;
  let seq,seq1 be sequence of X;
   assume that A1:seq1 is_subsequence_of seq and
               A2:seq is convergent;
   consider g1 be Element of X such that
   A3:for p be Real st 0<p ex n be Nat st for m be Nat st n<=m holds
     ||.(seq.m)-g1.||< p by A2,NORMSP_1:def 9;
   consider Nseq be increasing Seq_of_Nat such that
A4:seq1=seq*Nseq by A1,SEQM_3_def_10;
   take g=g1;
   let p be Real; assume 0<p;
   then consider n1 be Nat such that
     A5:for m be Nat st n1<=m holds ||.(seq.m)-g1.||<p by A3;
   take n=n1;
   let m be Nat such that A6:n<=m;
     m<=Nseq.m by SEQM_3:33;
   then A7:n<=Nseq.m by A6,AXIOMS:22;
     seq1.m=seq.(Nseq.m) by A4,SEQM_3_31;
   hence ||.(seq1.m)-g.||<p by A5,A7;
  end;

theorem SEQ_4_31:
for X be RealNormSpace
  for seq,seq1 be sequence of X holds
  seq1 is_subsequence_of seq & seq is convergent implies
        lim seq1=lim seq
  proof
  let X be RealNormSpace;
  let seq,seq1 be sequence of X;
   assume that A1:seq1 is_subsequence_of seq and
               A2:seq is convergent;
   A3:seq1 is convergent by A1,A2,SEQ_4_30;
   consider Nseq be increasing Seq_of_Nat such that
A4:  seq1=seq*Nseq by A1,SEQM_3_def_10;
      now let p be Real; assume 0<p;
     then consider n1 be Nat such that
      A5:for m be Nat st n1<=m
      holds ||.(seq.m)-(lim seq).||<p by A2,NORMSP_1:def 11;
     take n=n1;
     let m be Nat such
     that A6:n<=m;
       m<=Nseq.m by SEQM_3:33;
     then A7:n<=Nseq.m by A6,AXIOMS:22;
       seq1.m=seq.(Nseq.m) by A4,SEQM_3_31;
     hence ||.(seq1.m)-(lim seq).||<p by A5,A7;
    end;
   hence thesis by A3,NORMSP_1:def 11;
  end;

Lm1:
for n,m be Nat holds
n<m implies ex k be Nat st m=n+1+k
 proof
 let n,m be Nat;
  assume A1: n<m;
  then consider k1 be Nat such that A2:m=n+k1 by NAT_1:28;
  k1 <> 0 by A1,A2;
  then consider n1 be Nat such that A3:k1=n1+1 by NAT_1:22;
  take k=n1;
  thus m=n+1+k by A2,A3,XCMPLX_1:1;
 end;

Lm2:
  for seq be Real_Sequence holds
     ((for n be Nat holds seq.n<seq.(n+1)) implies
              for n,k be Nat holds seq.n<seq.(n+1+k)) &
      ((for n,k be Nat holds seq.n<seq.(n+1+k)) implies
              for n,m be Nat st n<m holds seq.n<seq.m) &
      ((for n,m be Nat st n<m holds seq.n<seq.m) implies
              for n be Nat holds seq.n<seq.(n+1))
 proof
  let seq be Real_Sequence;
  thus (for n be Nat holds seq.n<seq.(n+1)) implies
                    for n,k be Nat holds seq.n<seq.(n+1+k)
   proof
    assume A1:for n be Nat holds seq.n<seq.(n+1);
    let n be Nat;
     defpred X[Nat] means seq.n<seq.(n+1+$1);
    A2: X[0] by A1;
     A3:now let k be Nat such that A4: X[k];
        seq.(n+1+k)<seq.(n+1+k+1) by A1;
      then seq.n<seq.(n+1+k+1) by A4,AXIOMS:22;
      hence X[k+1] by XCMPLX_1:1;
     end;
    thus for k be Nat holds X[k] from NAT_1:sch 1(A2,A3);
   end;
  thus (for n,k be Nat holds seq.n<seq.(n+1+k)) implies
                     for n,m be Nat st n<m holds seq.n<seq.m
   proof
    assume A5:for n,k be Nat holds seq.n<seq.(n+1+k);
    let n,m be Nat; assume n<m;
    then ex k be Nat st m=n+1+k by Lm1;
    hence seq.n<seq.m by A5;
   end;
  assume A6:for n,m be Nat st n<m holds seq.n<seq.m;
  let n be Nat;
    n<n+1 by NAT_1:38;
  hence seq.n<seq.(n+1) by A6;
 end;

Lm6:
 for f be Real_Sequence
   holds
( f is increasing iff (for n being Nat holds f.n < f.(n+1)))
  proof
   let f be Real_Sequence;
    thus f is increasing implies for n being Nat holds f.n < f.(n+1)
    proof
      assume A1: f is increasing;
      let n be Nat;
A2:   dom f = NAT by FUNCT_2:def 1;
        n < n+1 by NAT_1:38;
      hence thesis by A1,A2,SEQM_3:def 1;
    end;
    assume A3:for n being Nat holds f.n < f.(n+1);
AB:  for m,n be Nat st m in dom f & n in dom f & m < n holds f.m < f.n
        proof
        let m,nn be Nat such that
        AA:   m in dom f & nn in dom f & m < nn;
        consider k be Nat such that
        A4: nn = m+1+k by Lm1,AA;
    thus thesis by A3,A4,Lm2;
    end;
 thus thesis by AB,SEQM_3:def 1;
  end;

theorem SEQM_3_47:
for X be RealNormSpace
  for seq be sequence of X
   for k be Nat holds
   seq ^\k is_subsequence_of seq
  proof
   let X be RealNormSpace;
   let seq be sequence of X;
   let k be Nat;
  deffunc U(Nat) = $1+k;
   consider seq1 be Real_Sequence such that
   A1:for n be Nat holds seq1.n=U(n) from SEQ_1:sch 1;
      now let n be Nat;
     A2:seq1.n=n+k & seq1.(n+1)=n+1+k by A1;
       n+k<n+k+1 by NAT_1:38;
     hence seq1.n<seq1.(n+1) by A2,XCMPLX_1:1;
    end;
   then A3:seq1 is increasing by Lm6;
      now let n be Nat;
       n+k is Nat;
     hence seq1.n is Nat by A1;
    end;
   then reconsider seq1 as increasing Seq_of_Nat by A3,SEQM_3:29;
   take Nseq=seq1;
      now let n be Nat;
     thus (seq*Nseq).n=seq.(Nseq.n) by SEQM_3_31
       .=seq.(n+k) by A1
       .=(seq ^\k).n by SEQM_3_def_9;
    end;
   hence thesis by FUNCT_2:113;
  end;

