begin :: Basic Properties of Sequences of Norm Space

definition
  canceled;
end;

theorem Th1:
for X be add-associative right_zeroed right_complementable (non empty NORMSTR)
 for seq be sequence of X st
   ( for n be Element of NAT holds seq.n = 0.X)
     holds for m be Element of NAT holds (Partial_Sums seq).m = 0.X
 proof
let X be add-associative right_zeroed right_complementable (non empty NORMSTR);
   let seq be sequence of X such that
    A1:   for n be Element of NAT holds seq.n = 0.X;
    let m be Element of NAT;
    defpred P[Element of NAT] means seq.$1 = (Partial_Sums seq).$1;
A2: P[0] by BHSP_4:def 1;
A3: for k be Element of NAT st P[k] holds P[k+1]
    proof let k be Element of NAT such that
A4:   P[k];
      thus seq.(k+1) = 0.X + seq.(k+1) by RLVECT_1:10
                    .= (Partial_Sums seq).k + seq.(k+1) by A1,A4
                    .= (Partial_Sums seq).(k+1) by BHSP_4:def 1;
    end;
        for n be Element of NAT holds P[n] from NAT_1:sch 1(A2,A3);
    then seq = Partial_Sums seq by FUNCT_2:113;
    hence (Partial_Sums seq).m = 0.X by A1;
  end;

definition
let X be RealNormSpace;
let seq be sequence of X;
 attr seq is summable means :Def2:
  Partial_Sums seq is convergent;
end;

registration
let X be RealNormSpace;
cluster summable sequence of X;
existence
  proof
    reconsider C = NAT --> 0.X as sequence of X by FUNCOP_1:57;
A1:   for n be Element of NAT holds C.n = 0.X by FUNCOP_1:13;
    take C, 0.X;
    let p be Real such that
A2:   0<p;
    take 0;
    let m be Element of NAT such that 0<=m;
       ||.Partial_Sums C.m-0.X.|| = ||.0.X-0.X.|| by A1,Th1
    .=0 by NORMSP_1:10;
    hence ||.Partial_Sums C.m-0.X.||<p by A2;
  end;
end;

definition
  let X be RealNormSpace;
  let seq be sequence of X;
 func Sum seq -> Element of X equals
   lim Partial_Sums seq;
correctness;
end;

definition
  let X be RealNormSpace;
  let seq be sequence of X;
attr seq is norm_summable means :Def4:
 ||.seq.|| is summable;
end;

theorem Th2:
for X be RealNormSpace
  for seq be sequence of X
      for m be Element of NAT holds  0 <= ||.seq.||.m
  proof
  let X be RealNormSpace;
  let seq be sequence of X;
  let m be Element of NAT;
       ||.seq.||.m = ||.seq.m.|| by NORMSP_1:def 10;
  hence thesis by NORMSP_1:8;
  end;

theorem Th3:
for X be RealNormSpace
  for x,y,z be Element of X
     holds ||.x-y.|| = ||.(x-z)+(z-y).||
 proof
    let X be RealNormSpace;
    let x,y,z be Element of X;
 thus ||.x-y.|| = ||.x-0.X-y.|| by RLVECT_1:26
     .= ||.x-(z-z)-y.|| by RLVECT_1:16
     .= ||.(x-z)+z-y.|| by RLVECT_1:43
     .= ||.(x-z)+(z-y).|| by RLVECT_1:def 6;
end;

theorem Th4:
for X be RealNormSpace
  for seq be sequence of X holds
        seq is convergent implies
         for s be Real st 0<s ex n be Element of NAT
         st for m be Element of NAT st n<=m holds ||.seq.m -seq.n.||<s
    proof
    let X be RealNormSpace;
    let seq be sequence of X;
     assume seq is convergent;
     then consider g1 be Element of X such that
  A1: for s be Real st 0<s
     ex n be Element of NAT st for m be Element of NAT st n<=m
     holds ||.seq.m -g1.||<s
     by NORMSP_1:def 9;
     let s be Real; assume 0<s;
     then 0<s/2 by SEQ_2:3;
     then consider n be Element of NAT such that
  A2: for m be Element of NAT st n<=m holds ||.seq.m -g1.||<s/2 by A1;
         now
        let m be Element of NAT such that A3: n<=m;
            ||.seq.n -g1.||<s/2 by A2;
     then A4: ||.g1-seq.n.||<s/2 by NORMSP_1:11;
         ||.seq.m -g1.||<s/2 by A2,A3;
     then A5: ||.seq.m -g1.||+||.g1-seq.n.||<s/2+s/2 by A4,XREAL_1:10;
  A6: ||.seq.m -g1+(g1-seq.n).||<=||.seq.m -g1.||+||.g1-seq.n.||
            by NORMSP_1:def 2;
          ||.seq.m -g1+(g1-seq.n).|| = ||.seq.m -seq.n.|| by Th3;
        hence ||.seq.m -seq.n.|| < s by A5,A6,XXREAL_0:2;
   end;
   hence thesis;
   end;

theorem Th5:
for X be RealNormSpace
  for seq be sequence of X holds
        seq is Cauchy_sequence_by_Norm
        iff
       for p be Real st p > 0 holds
           ex n be Element of NAT st for m be Element of NAT st n <= m
            holds ||.seq.m-seq.n.||< p
    proof
    let X be RealNormSpace;
    let seq be sequence of X;
   A1: now assume
      A2:      seq is Cauchy_sequence_by_Norm;
      thus for p be Real st p > 0 holds
           ex n be Element of NAT st for m be Element of NAT st n <= m
            holds ||.seq.m-seq.n.||< p
        proof
        let p be Real such that
           A3: p >0;
           consider n be Element of NAT such that
           A4: for m,k be Element of NAT st n <= m & n <=k
            holds ||.seq.m-seq.k.||< p by A2,A3,RSSPACE3:10;
              for m be Element of NAT st n <= m holds ||.seq.m-seq.n.||<p by A4
;
         hence thesis;
      end;
     end;
     now assume
   A5:     for p be Real st p > 0 holds
               ex n be Element of NAT st for m be Element of NAT st n <= m
             holds ||.seq.m-seq.n.||< p;
       now let s be Real;
      assume 0<s;
     then 0<s/2 by SEQ_2:3;
          then consider k be Element of NAT such that
  A6: for m be Element of NAT st k<=m holds ||.seq.m -seq.k.||<s/2 by A5;
         now
         let m,n be Element of NAT such
          that A7: k<=m & k<=n;
          A8:   ||.seq.m -seq.k.||<s/2 by A6,A7;
              ||.seq.n -seq.k.||<s/2 by A6,A7;
          then ||.seq.k-seq.n.||<s/2 by NORMSP_1:11;
          then A9: ||.seq.m -seq.k.||+||.seq.k-seq.n.||<s/2+s/2
           by A8,XREAL_1:10;
          A10: ||.seq.m -seq.k+(seq.k-seq.n).||
          <=||.seq.m -seq.k.||+||.seq.k-seq.n.|| by NORMSP_1:def 2;
                ||.seq.m -seq.k+(seq.k-seq.n).||
              =||.seq.m -seq.n.|| by Th3;
          hence ||.seq.m -seq.n.|| < s  by A9,A10,XXREAL_0:2;
       end;
       hence ex k be Element of NAT st
        for m,n be Element of NAT st k <= m & k <=n holds ||.seq.m-seq.n.||< s;
           end;
       hence seq is Cauchy_sequence_by_Norm by RSSPACE3:10;
    end;
hence thesis by A1;
end;

theorem Th6:
for X be RealNormSpace
  for seq be sequence of X
  st (for n be Element of NAT holds seq.n = 0.X)
    holds for m be Element of NAT holds (Partial_Sums ||.seq.||).m = 0
  proof
  let X be RealNormSpace;
  let seq be sequence of X such that
A1:   for n be Element of NAT holds seq.n = 0.X;
    let m be Element of NAT;
    defpred P[Element of NAT] means ||.seq.||.$1 = (Partial_Sums ||.seq.||).$1;
A2: P[0] by SERIES_1:def 1;
A3: for k be Element of NAT st P[k] holds P[k+1]
    proof let k be Element of NAT such that
A4:   P[k];
      thus ||.seq.||.(k+1) =  ||.0.X.|| + ||.seq.||.(k+1)
                        .= ||.seq.k.|| + ||.seq.||.(k+1) by A1
                        .= (Partial_Sums ||.seq.||).k + ||.seq.||.(k+1) by A4,
NORMSP_1:def 10
                        .= (Partial_Sums ||.seq.||).(k+1) by SERIES_1:def 1;
    end;
        for n be Element of NAT  holds  P[n] from NAT_1:sch 1(A2,A3);
    hence (Partial_Sums ||.seq.||).m = ||.seq.||.m
                                  .= ||.seq.m.|| by NORMSP_1:def 10
                                  .= ||.0.X.|| by A1
                                  .= 0;
  end;

definition
let X be RealNormSpace;
let seq be sequence of X;
  redefine attr seq is constant means   :Def5:
  ex r be Element of X st for n be Element of NAT holds seq.n=r;
 compatibility
  proof
A1:dom seq = NAT by FUNCT_2:def 1;
   hereby assume
A2:   seq is constant;
    consider n0 being Element of NAT;
    reconsider r = seq.n0 as Element of X;
    take r;
    let n be Element of NAT;
    thus seq.n = r by A1,A2,FUNCT_1:def 16;
   end;
   given r be Element of X such that
A3:  for n be Element of NAT holds seq.n=r;
  for n1,n2 being set st n1 in dom seq & n2 in dom seq
   holds seq.n1=seq.n2
   proof
     let n1,n2 be set;
     assume
A4:  n1 in dom seq & n2 in dom seq;
      hence seq.n1 = r by A1,A3 .= seq.n2 by A1,A3,A4;
  end;
  hence seq is constant by FUNCT_1:def 16;
 end;
end;

definition
let X be RealNormSpace;
let seq be sequence of X;
let k be Element of NAT;
func seq ^\k -> sequence of X means :Def6:
  for n be Element of NAT holds it.n=seq.(n+k);
existence
 proof deffunc U(Element of NAT) = seq.($1+k);
  thus ex f being sequence of X st
   for n be Element of NAT holds f.n=U(n) from FUNCT_2:sch 4;
 end;
uniqueness
 proof
  let seq1,seq2 be sequence of X;
  assume that A1:for n be Element of NAT holds seq1.n=seq.(n+k) and
              A2:for n be Element of NAT holds seq2.n=seq.(n+k);
       now let n be Element of NAT;
        seq1.n=seq.(n+k) & seq2.n=seq.(n+k) by A1,A2;
    hence seq1.n=seq2.n;
   end;
  hence seq1=seq2 by FUNCT_2:113;
 end;
end;

definition
let X be non empty 1-sorted;
let Nseq be increasing Seq_of_Nat;
let seq be sequence of X;
redefine func seq*Nseq -> Function of NAT, the carrier of X;
 coherence
  proof
     A1:dom Nseq=NAT by FUNCT_2:def 1;
       rng Nseq c= NAT by VALUED_0:def 6;
     then rng Nseq c= dom seq by FUNCT_2:def 1;
     then dom (seq*Nseq)=NAT by A1,RELAT_1:46;
     hence thesis by FUNCT_2:def 1;
end;
end;

definition
let X be non empty 1-sorted;
let Nseq be increasing Seq_of_Nat;
let seq be sequence of X;
redefine func seq*Nseq -> sequence of X;
 coherence
proof
thus seq*Nseq is Function of NAT, the carrier of X;
end;
end;

theorem Th7:
for X be non empty 1-sorted
  for seq be sequence of X
   for Nseq be increasing Seq_of_Nat holds
      for n be Element of NAT holds (seq*Nseq).n=seq.(Nseq.n)
  proof
 let X be non empty 1-sorted;
 let seq be sequence of X;
 let Nseq be increasing Seq_of_Nat;
 let n be Element of NAT;
      dom (seq*Nseq)=NAT by FUNCT_2:def 1;
    hence (seq*Nseq).n=seq.(Nseq.n) by FUNCT_1:22;
  end;

theorem Th8:
for X be RealNormSpace
  for seq be sequence of X holds
   seq ^\0 =seq
  proof
  let X be RealNormSpace;
  let seq be sequence of X;
      now let n be Element of NAT;
    thus (seq ^\0).n=seq.(n+0) by Def6
                  .=seq.n;
   end;
  hence thesis by FUNCT_2:113;
 end;

theorem
  for X be RealNormSpace
  for seq be sequence of X
    for k, m be Element of NAT holds
   (seq ^\k)^\m=(seq ^\m)^\k
  proof
     let X be RealNormSpace;
     let seq be sequence of X;
     let k,m be Element of NAT;
        now let n be Element of NAT;
     thus ((seq ^\k)^\m).n=(seq ^\k).(n+m) by Def6
      .=seq.(n+m+k) by Def6
      .=seq.(n+k+m)
      .=(seq ^\m).(n+k) by Def6
      .=((seq ^\m)^\k).n by Def6;
    end;
   hence thesis by FUNCT_2:113;
  end;

theorem Th10:
for X be RealNormSpace
  for seq be sequence of X
   for k, m be Element of NAT holds
   (seq ^\k)^\m=seq ^\(k+m)
  proof
     let X be RealNormSpace;
     let seq be sequence of X;
     let k,m be Element of NAT;
        now let n be Element of NAT;
     thus ((seq ^\k)^\m).n=(seq ^\k).(n+m) by Def6
      .=seq.(n+m+k) by Def6
      .=seq.(n+(k+m))
      .=(seq ^\(k+m)).n by Def6;
    end;
   hence thesis by FUNCT_2:113;
  end;

theorem Th11:
for X be RealNormSpace
  for seq,seq1 be sequence of X
  holds
    seq1 is subsequence of seq & seq is convergent implies
        seq1 is convergent
  proof
  let X be RealNormSpace;
  let seq,seq1 be sequence of X;
   assume that A1:seq1 is subsequence of seq and
               A2:seq is convergent;
   consider g1 be Element of X such that
   A3:for p be Real st 0<p ex n be Element of NAT st
   for m be Element of NAT st n<=m holds
     ||.(seq.m)-g1.||< p by A2,NORMSP_1:def 9;
   consider Nseq be increasing Seq_of_Nat such that
A4:seq1=seq*Nseq by A1,BHSP_3:def 4;
   take g=g1;
   let p be Real; assume 0<p;
   then consider n1 be Element of NAT such that
     A5:for m be Element of NAT st n1<=m holds ||.(seq.m)-g1.||<p by A3;
   take n=n1;
   let m be Element of NAT such that A6:n<=m;
       m<=Nseq.m by SEQM_3:33;
   then A7:n<=Nseq.m by A6,XXREAL_0:2;
       seq1.m=seq.(Nseq.m) by A4,Th7;
   hence ||.(seq1.m)-g.||<p by A5,A7;
  end;

theorem Th12:
for X be RealNormSpace
  for seq,seq1 be sequence of X holds
  seq1 is subsequence of seq & seq is convergent implies
        lim seq1=lim seq
  proof
  let X be RealNormSpace;
  let seq,seq1 be sequence of X;
   assume that A1:seq1 is subsequence of seq and
               A2:seq is convergent;
   A3:seq1 is convergent by A1,A2,Th11;
   consider Nseq be increasing Seq_of_Nat such that
A4:  seq1=seq*Nseq by A1,BHSP_3:def 4;
        now let p be Real; assume 0<p;
     then consider n1 be Element of NAT such that
      A5:for m be Element of NAT st n1<=m
      holds ||.(seq.m)-(lim seq).||<p by A2,NORMSP_1:def 11;
     take n=n1;
     let m be Element of NAT such
     that A6:n<=m;
         m<=Nseq.m by SEQM_3:33;
     then A7:n<=Nseq.m by A6,XXREAL_0:2;
         seq1.m=seq.(Nseq.m) by A4,Th7;
     hence ||.(seq1.m)-(lim seq).||<p by A5,A7;
    end;
   hence thesis by A3,NORMSP_1:def 11;
  end;

Lm1:
for n,m be Nat holds
n<m implies ex k be Element of NAT st m=n+1+k
 proof
 let n,m be Nat;
  assume A1: n<m;
  then consider k1 be Nat such that A2:m=n+k1 by NAT_1:10;
    k1 <> 0 by A1,A2;
  then consider n1 be Nat such that A3:k1=n1+1 by NAT_1:6;
  reconsider n1 as Element of NAT by ORDINAL1:def 13;
  take n1;
  thus m=n+1+n1 by A2,A3;
 end;

Lm2:
  for seq be Real_Sequence holds
     ((for n be Element of NAT holds seq.n<seq.(n+1)) implies
              for n,k be Element of NAT holds seq.n<seq.(n+1+k)) &
      ((for n,k be Element of NAT holds seq.n<seq.(n+1+k)) implies
              for n,m be Element of NAT st n<m holds seq.n<seq.m) &
      ((for n,m be Element of NAT st n<m holds seq.n<seq.m) implies
              for n be Element of NAT holds seq.n<seq.(n+1))
 proof
  let seq be Real_Sequence;
  thus (for n be Element of NAT holds seq.n<seq.(n+1)) implies
                    for n,k be Element of NAT holds seq.n<seq.(n+1+k)
   proof
    assume A1:for n be Element of NAT holds seq.n<seq.(n+1);
    let n be Element of NAT;
     defpred X[Element of NAT] means seq.n<seq.(n+1+$1);
    A2: X[0] by A1;
     A3:now let k be Element of NAT such that A4: X[k];
          seq.(n+1+k)<seq.(n+1+k+1) by A1;
      hence X[k+1] by A4,XXREAL_0:2;
     end;
    thus for k be Element of NAT holds X[k] from NAT_1:sch 1(A2,A3);
   end;
  thus (for n,k be Element of NAT holds seq.n<seq.(n+1+k)) implies
                     for n,m be Element of NAT st n<m holds seq.n<seq.m
   proof
    assume A5:for n,k be Element of NAT holds seq.n<seq.(n+1+k);
    let n,m be Element of NAT; assume n<m;
    then ex k be Element of NAT st m=n+1+k by Lm1;
    hence seq.n<seq.m by A5;
   end;
  assume A6:for n,m be Element of NAT st n<m holds seq.n<seq.m;
  let n be Element of NAT;
      n<n+1 by NAT_1:13;
  hence seq.n<seq.(n+1) by A6;
 end;

Lm3:
 for f be Real_Sequence
   holds
( f is increasing iff (for n being Element of NAT holds f.n < f.(n+1)))
  proof
   let f be Real_Sequence;
    thus f is increasing implies for n being Element of NAT holds f.n < f.(n+1
)
    proof
      assume A1: f is increasing;
      let n be Element of NAT;
A2:   dom f = NAT by FUNCT_2:def 1;
          n < n+1 by NAT_1:13;
      hence thesis by A1,A2,SEQM_3:def 1;
    end;
    assume A3:for n being Element of NAT holds f.n < f.(n+1);
   for m,n be Element of NAT st m in dom f & n in dom f & m < n holds f.m < f.n
        proof
        let m,nn be Element of NAT such that
        A4:   m in dom f & nn in dom f & m < nn;
        consider k be Element of NAT such that
        A5: nn = m+1+k by A4,Lm1;
    thus thesis by A3,A5,Lm2;
    end;
 hence thesis by SEQM_3:def 1;
  end;

theorem Th13:
for X be RealNormSpace
  for seq be sequence of X
   for k be Element of NAT holds
   seq ^\k is subsequence of seq
  proof
   let X be RealNormSpace;
   let seq be sequence of X;
   let k be Element of NAT;
  deffunc U(Element of NAT) = $1+k;
   consider seq1 be Real_Sequence such that
   A1:for n be Element of NAT holds seq1.n=U(n) from SEQ_1:sch 1;
        now let n be Element of NAT;
     A2:seq1.n=n+k & seq1.(n+1)=n+1+k by A1;
         n+k<n+k+1 by NAT_1:13;
     hence seq1.n<seq1.(n+1) by A2;
    end;
   then A3:seq1 is increasing by Lm3;
        now let n be Element of NAT;
         n+k is Element of NAT;
     hence seq1.n is Element of NAT by A1;
    end;
   then reconsider seq1 as increasing Seq_of_Nat by A3,SEQM_3:29;
   take Nseq=seq1;
        now let n be Element of NAT;
     thus (seq*Nseq).n=seq.(Nseq.n) by Th7
       .=seq.(n+k) by A1
       .=(seq ^\k).n by Def6;
    end;
   hence thesis by FUNCT_2:113;
  end;

:::::::::: Norm Space versions of SEQ_4_33 - SEQ_4_39

theorem Th14:
for X be RealNormSpace
  for seq,seq1 be sequence of X
    for k be Element of NAT holds
  seq is convergent implies ((seq ^\k) is convergent & lim (seq ^\k)=lim seq)
  proof
   let X be RealNormSpace;
   let seq,seq1 be sequence of X;
   let k be Element of NAT;
   assume A1:seq is convergent;
   A2:seq^\k is subsequence of seq by Th13;
   hence seq^\k is convergent by A1,Th11;
   thus lim (seq ^\k)=lim seq by A1,A2,Th12;
  end;

theorem Th15:
for X be RealNormSpace
  for seq,seq1 be sequence of X
  holds
  seq is convergent & (ex k be Element of NAT st seq=seq1 ^\k)
     implies seq1 is convergent
  proof
  let X be RealNormSpace;
  let seq,seq1 be sequence of X;
  assume that
 A1: seq is convergent and
 A2: ex k be Element of NAT st seq=seq1 ^\k;
     consider k be Element of NAT such that
 A3: seq=seq1 ^\k by A2;
     consider g1 be Element of X  such that
 A4: for p be Real st 0<p ex n be Element of NAT
       st for m be Element of NAT st n<=m holds ||.seq.m-g1.||<p
     by A1,NORMSP_1:def 9;
     take g=g1;
     let p be Real; assume 0<p;
     then consider n1 be Element of NAT such that
 A5: for m be Element of NAT st n1<=m holds ||.seq.m-g1.||<p by A4;
     take n=n1+k;
     let m be Element of NAT; assume
 A6: n<=m;
   then consider l be Nat such that
 A7: m=n1+k+l by NAT_1:10;
     reconsider l as Element of NAT by ORDINAL1:def 13;
       m-k=n1+l+k+-k by A7;
     then reconsider m1=m-k as Element of NAT;
 A8:  now assume not n1<=m1;
        then m1+k<n1+k by XREAL_1:8;
        hence contradiction by A6;
      end;
 A9:  m1+k=m;
        ||.seq.m1-g1.||<p by A5,A8;
   hence ||.seq1.m-g.||<p by A3,A9,Def6;
 end;

theorem Th16:
for X be RealNormSpace
  for seq,seq1 be sequence of X
  holds
   seq is convergent & (ex k be Element of NAT st seq=seq1 ^\k)
       implies lim seq1 =lim seq
  proof
  let X be RealNormSpace;
  let seq,seq1 be sequence of X;
     assume that
 A1: seq is convergent and
 A2: ex k be Element of NAT st seq=seq1 ^\k;
 A3: seq1 is convergent by A1,A2,Th15;
     consider k be Element of NAT such that
 A4: seq=seq1 ^\k by A2;
          now let p be Real; assume 0<p;
       then consider n1 be Element of NAT such that
 A5:   for m be Element of NAT st n1<=m holds ||.seq.m-(lim seq).||<p
 by A1,NORMSP_1:def 11;
       take n=n1+k;
       let m be Element of NAT; assume
 A6:   n<=m;
       then consider l be Nat such that
 A7:    m=n1+k+l by NAT_1:10;
 reconsider l as Element of NAT by ORDINAL1:def 13;
         m-k=n1+l+k+-k by A7;
       then reconsider m1=m-k as Element of NAT;
 A8:    now assume not n1<=m1;
         then m1+k<n1+k by XREAL_1:8;
         hence contradiction by A6;
        end;
 A9:   m1+k=m;
          ||.seq.m1-(lim seq) .||<p by A5,A8;
      hence ||.seq1.m-(lim seq).||<p by A4,A9,Def6;
     end;
    hence thesis by A3,NORMSP_1:def 11;
  end;

theorem Th17:
for X be RealNormSpace
  for seq be sequence of X
    holds
  seq is constant implies seq is convergent
  proof
  let X be RealNormSpace;
  let seq be sequence of X;
   assume seq is constant;
   then consider r be Element of X such that
   A1:for n be Element of NAT holds seq.n=r by Def5;
   take g=r;
   let p be Real such that A2:0<p;
   take n=0;
   let m be Element of NAT such that n<=m;
       ||.(seq.m)-g.||=||.r-g.|| by A1
    .=||.0.X.|| by RLVECT_1:28
    .=0;
   hence ||.(seq.m)-g.||<p by A2;
  end;

theorem Th18:
for X be RealNormSpace
  for seq be sequence of X
  st (for n be Element of NAT holds seq.n = 0.X)
  holds seq is norm_summable
  proof
  let X be RealNormSpace;
  let seq be sequence of X
  such that
A1:   for n be Element of NAT holds seq.n = 0.X;
       take 0;
    let p be real number such that
A2:   0<p;
    take 0;
    let m be Element of NAT such that 0<=m;
        abs((Partial_Sums ||.seq.||).m-0) = abs(0-0) by A1,Th6
       .= 0 by ABSVALUE:def 1;
    hence abs((Partial_Sums ||.seq.||).m-0)<p by A2;
  end;

registration
let X be RealNormSpace;
 cluster norm_summable sequence of X;
 existence
  proof
    reconsider C = NAT --> 0.X as sequence of X by FUNCOP_1:57;
A1:   for n be Element of NAT holds C.n = 0.X by FUNCOP_1:13;
    take C;
    thus thesis by A1,Th18;
  end;
end;

