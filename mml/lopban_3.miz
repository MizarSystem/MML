definition let X be non empty Normed_AlgebraStr;
  let S be sequence of X;
  let a be Element of X;
  func a * S -> sequence of X means
  for n be Nat holds it.n = a * S.n;
existence
  proof
  deffunc F(Nat) = a * S.$1;
  ex S being sequence of X st
    for n be Nat holds S.n = F(n) from ExNCBASeq;
  hence thesis;
end;
uniqueness
  proof
       let S1, S2 be sequence of X;
       assume that
  A1:  ( for n be Nat holds S1.n = a * S.n ) and
  A2:  ( for n be Nat holds S2.n = a * S.n );
         for n be Nat holds S1.n = S2.n
       proof
           let n be Nat;
           S1.n = a * S.n by A1;
           hence thesis by A2;
       end;
      hence thesis by FUNCT_2:113;
  end;
end;

definition let X be non empty Normed_AlgebraStr;
