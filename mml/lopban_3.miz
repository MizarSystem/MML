begin :: Basic Properties of Sequence of Banach_Algebra

scheme ExNCBASeq{RNS()->non empty Normed_AlgebraStr,
   F(set) -> Point of RNS() } :
       ex S be sequence of RNS() st
       for n be Nat holds S.n = F(n)
       proof
       defpred P[set,set] means ex n be Nat st n =$1 & $2 = F(n);
    A1: for d be set st d in NAT ex s be set  st P[d,s]
       proof
            let d be set; assume d in NAT;
            then consider  n be Nat such that
        A2: n = d;
            reconsider z = F(n) as set;
            take z;
            thus thesis by A2;
       end;
    A3: for d , s , t be set st d in NAT & P[d,s] & P[d,t] holds s = t;
       consider f be Function such that
    A4: dom f = NAT and
    A5: for d be set st d in NAT holds
        P[d,f.d] from FUNCT_1:sch 2(A3,A1);
         for d be set st d in NAT holds f.d is Point of RNS()
       proof
            let d be set; assume d in NAT;
            then ex n be Nat st n = d & f.d = F(n) by A5;
            hence thesis;
       end;
       then reconsider f as sequence of RNS() by A4,NORMSP_1:17;
       take S = f;
         now let n be Nat;
         P[n,S.n] by A5;
       hence S.n = F(n);
       end;
   hence for n be Nat holds S.n = F(n);
end;

theorem LM000:
 for X be Banach_Algebra holds
     for x,y,z being Element of X
        for a,b be Real holds
         x+y = y+x &
         (x+y)+z = x+(y+z) &
         x+(0.X) = x &
         (ex t being Element of
          X st  x+t=
          0.X) &
         (x*y)*z = x*(y*z) &
         1*x=x     &
         0*x=0.X  &
         a*0.X =0.X&
         (-1)*x=-x &
         x*(1_ X) = x &
         (1_ X)*x = x &
         x*(y+z) = x*y + x*z &
         (y+z)*x = y*x + z*x &
         a*(x*y) = (a*x)*y &
         a*(x+y) = a*x + a*y &
         (a+b)*x = a*x + b*x &
         (a*b)*x = a*(b*x)  &
         (a*b)*(x*y)=(a*x)*(b*y) &
         a*(x*y)=x*(a*y) &
         0.X * x  = 0.X   &
         x*0.X =0.X          &
         x*(y-z) = x*y-x*z   &
          (y-z)*x = y*x-z*x   &
          x+y-z = x+(y-z)      &
          x-y+z = x-(y-z)      &
          x-y-z = x-(y+z)      &
          x+y=(x-z)+(z+y)      &
          x-y=(x-z)+(z-y)      &
          x=(x-y)+y               &
          x=y-(y-x)               &
          ( ||.x.|| = 0 iff x = 0.X ) &
           ||.a * x.|| = abs(a) * ||.x.|| &
           ||.x + y.|| <= ||.x.|| + ||.y.|| &
          ||. x*y .|| <= ||.x.|| * ||.y.|| &
            ||. 1_ X .|| = 1  &
          X is complete
 proof
   let X be Banach_Algebra;
   let x,y,z being Element of X;
   let a,b be Real;
   thus x+y = y+x;
   thus (x+y)+z = x+(y+z) by RLVECT_1:def 6;
   thus x+(0.X) = x by RLVECT_1:def 7;
   thus ex t being Element of X st x+t=0.X by RLVECT_1:def 8;
   thus (x*y)*z = x*(y*z) by VECTSP_1:def 16;
   thus 1*x = x by RLVECT_1:def 9;
   thus 0*x = 0.X by RLVECT_1:23;
   thus a*0.X =0.X by RLVECT_1:23;
   thus (-1)*x = -x by RLVECT_1:29;
   thus x*(1_ X) = x &
          (1_ X)*x= x &
         x*(y+z) = x*y + x*z &
         (y+z)*x = y*x + z*x &
         a*(x*y) = (a*x)*y &
         a*(x+y) = a*x + a*y &
         (a+b)*x = a*x + b*x &
         (a*b)*x = a*(b*x)
 by FUNCSDOM:def 20,VECTSP_1:def 12,def 19;
 thus (a*b)*(x*y) =a*(b*(x*y))
                 by FUNCSDOM:def 20
                        .=a*(x*(b*y) )
                 by LOPBAN_2:def 11
                        .=(a*x)*(b*y)
                 by FUNCSDOM:def 20;
 thus a*(x*y)=x*(a*y) by LOPBAN_2:def 11;
 thus 0.X * x  = 0.X & x*0.X =0.X
  proof
  0.X*x =(0.X+0.X)*x  by RLVECT_1:def 7
          .=0.X*x + 0.X*x by VECTSP_1:def 12;
  then
  0.X = 0.X*x + 0.X*x-0.X * x by RLVECT_1:28;
  then
  0.X = 0.X*x + (0.X*x-0.X * x)  by RLVECT_1:42;
  then
   0.X = 0.X*x + 0.X by RLVECT_1:28;
  hence 0.X = 0.X*x by RLVECT_1:def 7;
  x*0.X =x*(0.X+0.X) by RLVECT_1:def 7
          .=x*0.X + x*0.X by FUNCSDOM:def 20;
  then 0.X = x*0.X + x*0.X-x* 0.X by RLVECT_1:28;
  then 0.X = x*0.X + (x*0.X-x* 0.X)  by RLVECT_1:42;
  then 0.X = x*0.X + 0.X by RLVECT_1:28;
  hence 0.X = x*0.X by RLVECT_1:def 7;
  thus thesis;
  end;
 P1R:    x*(y-z)+x*z
          =x*((y-z)+z) by FUNCSDOM:def 20
          .=x*(y-(z-z)) by RLVECT_1:43
          .=x*(y-0.X) by RLVECT_1:28
          .=x*y by RLVECT_1:26;
 thus x*(y-z)
         =x*(y-z)+0.X by  RLVECT_1:10
         .=x*(y-z)+(x*z-x*z) by  RLVECT_1:28
         .=x*y-x*z by P1R,RLVECT_1:42;
 P1L:    (y-z)*x+z*x
          =((y-z)+z)*x  by VECTSP_1:def 12
          .=(y-(z-z))*x by RLVECT_1:43
          .=(y-0.X)*x    by RLVECT_1:28
          .=y*x            by RLVECT_1:26;
 thus (y-z)*x
         =(y-z)*x+0.X by  RLVECT_1:10
         .=(y-z)*x+(z*x-z*x) by  RLVECT_1:28
         .=y*x-z*x by P1L,RLVECT_1:42;
 thus x+y-z = x+(y-z) by RLVECT_1:42;
 thus x-y+z = x-(y-z) by RLVECT_1:43;
 thus x-y-z = x-(y+z) by RLVECT_1:41;
 thus (x-z)+(z+y)=(x-z)+z+y by  RLVECT_1:def 6
                        .=x-(z-z)+y by  RLVECT_1:43
                        .=x-0.X + y  by  RLVECT_1:28
                        .=x+y  by RLVECT_1:26;
 thus (x-z)+(z-y) =x-z+z-y  by  RLVECT_1:42
                         .=x-(z-z)-y by RLVECT_1:43
                         .=x-0.X -y by RLVECT_1:28
                         .=x-y by RLVECT_1:26;
 thus (x-y)+y=x-(y-y) by  RLVECT_1:43
                   .=x-0.X by RLVECT_1:28
                   .=x by RLVECT_1:26;
 thus y-(y-x)=y-y + x by RLVECT_1:43
                     .=0.X + x by RLVECT_1:28
                     .=x by RLVECT_1:10;
 thus ( ||.x.|| = 0 iff x = 0.X ) &
          ||.a * x.|| = abs(a) * ||.x.|| &
          ||.x + y.|| <= ||.x.|| + ||.y.|| &
          ||. x*y .|| <= ||.x.|| * ||.y.|| by NORMSP_1:def 2, LOPBAN_2:def 9;
 thus thesis by LOPBAN_2:def 10;
 end;

definition
  let X be non empty multLoopStr;
  let v be Element of X;
  attr v is invertible means :Def01:
   ex w be Element of X st v*w = 1_ X & w*v=1_ X;
