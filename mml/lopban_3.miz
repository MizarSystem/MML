definition
  let X be Banach_Algebra;
  let x be Element of X such that
   AS0:   x is invertible;
  func x" -> Element of X means :Def02:
    x*it= 1_  X & it*x= 1_  X;
  existence by Def01,AS0;
  uniqueness
  proof
  let z,y be Element of X such that
     AS1: x*z=1_ X & z*x=1_ X and
     AS2: x*y=(1_ X)  & y*x= (1_ X);
       y= y*(1_ X) by LM000
         .=(y*x)*z by LM000,AS1
         .=z by LM000,AS2;
    hence thesis;
  end;
end;

definition
  let X be Banach_Algebra;
  let z be Element of X;
  func z GeoSeq -> sequence of X means :defGeo:
  it.0 = 1_ X & for n be Nat  holds it.(n+1) = it.n * z;
  existence
   proof
      deffunc G(set,set)=(the mult of X).[$2,z];
      consider g be Function such that
      AS2:   dom g = NAT & g.0 = 1_ X
      & for n being Element of NAT holds g.(n+1) = G(n,g.n)
      from RECDEF_1:sch 3;
      defpred P[Nat] means g.$1 in the carrier of X;
       A0: P[0] by AS2;
       A1:  for k be Nat st P[k] holds P[k+1]
       proof
       let k be Nat such that
         BS1: P[k];
         reconsider gk=g.k as Element of the carrier of X by BS1;
         g.(k+1)=(the mult of X).[gk,z] by AS2;
         hence thesis;
       end;
       A2:  for n be Nat holds P[n]  from NAT_1:sch 1(A0,A1);
       A3: for n be set st n in NAT
          holds g.n in the carrier of X by A2;
        A4: g is Function of NAT,the carrier of X
           by A3,AS2,FUNCT_2:5;
         reconsider g0=g as sequence of X by A4;
           take g0;
         now let n be Nat;
          thus g0.(n+1) = (the mult of X).[g0.n,z] by AS2
            .=(the mult of X).(g0.n,z) by BINOP_1:def 1
            .=g0.n * z by VECTSP_1:def 10;
        end;
        hence thesis by AS2;
end;
uniqueness
proof
 let seq1,seq2 be sequence of X;
 assume that A2: seq1.0=1_ X & for n be Nat holds
                seq1.(n+1)=seq1.n * z and
                    A3: seq2.0=1_ X  & for n be Nat  holds
                seq2.(n+1)=seq2.n * z;
defpred P[Nat] means seq1.$1 = seq2.$1;
 A4: P[0] by A2,A3;
 A5: for k be Nat st P[k] holds P[k+1]
     proof let k be Nat;
      assume P[k];
      hence seq1.(k+1) = seq2.k * z by A2
                    .= seq2.(k+1) by A3;
      end;
   for n be Nat holds P[n] from NAT_1:sch 1(A4,A5);
 hence seq1 = seq2 by FUNCT_2:113;
end;
end;

definition
  let X be Banach_Algebra;
  let z be Element of X, n be Nat;
  func z #N n -> Element of X equals:Def03:
  z GeoSeq.n;
  correctness;
end;

theorem
  for X be Banach_Algebra
    for z be Element of X holds
  z #N 0 = 1_ X
proof
  let X be Banach_Algebra;
  let z be Element of X;
 thus z #N 0 = (z GeoSeq).0 by Def03
                   .= 1_ X  by defGeo;
end;

theorem LM010:
for X be Banach_Algebra
 for z be Element of X holds
||.z.|| < 1 implies z GeoSeq is summable norm_summable
proof
let X be Banach_Algebra;
let z be Element of X;
assume
A0:  ||.z.|| < 1;
0<=  ||.z.|| by NORMSP_1:8; then
A1:  abs( ||.z.||)  < 1 by A0,ABSVALUE:def 1;
A2:  for n be Nat holds (   0 <= ||.z GeoSeq.||.n
                                  &   ||.z GeoSeq.||.n  <=(||.z.||  GeoSeq).n )
  proof
  defpred P[Nat] means
     0 <= ||.z GeoSeq.||.$1 & ||.z GeoSeq.||.$1 <= (  ||.z.||  GeoSeq ).$1;
   P1:  ||.(z GeoSeq).||.0 = ||.(z GeoSeq).0 .|| by NORMSP_1:def 10;
     ||.(z GeoSeq).0 .|| = ||.1_ X.|| by defGeo
                        .= 1 by LOPBAN_2:def 10
                        .= ( ||.z.||  GeoSeq ).0 by PREPOWER:4; then
    A4: P[0] by P1,NORMSP_1:8;
    A5: for k be Nat st P[k] holds P[k+1]
       proof
        let k be Nat;
        assume
        A6: P[k];
        AA: 0 <=  ||.z.|| by NORMSP_1:8;
        A7:      ||.z GeoSeq.||.(k+1) = ||. (z GeoSeq).(k+1) .||
                                           by NORMSP_1:def 10
               .= ||. (z GeoSeq).k * z .|| by defGeo;
          ||. (z GeoSeq.k)*z .|| <= ||. (z GeoSeq.k) .|| *  ||.z.||
                                                by  LOPBAN_2:def 9;
        then
         A10:   ||. (z GeoSeq.k)*z .|| <= ||. z GeoSeq .||.k * ||.z.||
                                                by NORMSP_1:def 10;
           ||. z GeoSeq.||.k  *  ||.z.||
                 <=  ( ||.z.|| GeoSeq ).k * ||.z.|| by AA,A6,AXIOMS:25;
         then
            ||. (z GeoSeq.k)*z .|| <= ( ||.z.||  GeoSeq ).k *  ||.z.||
                   by A10,AXIOMS:22;
         hence 0<= ||.z GeoSeq.||.(k+1) &
             ||.z GeoSeq.||.(k+1)  <=  ( ||.z.||  GeoSeq ).(k+1)
              by A7,PREPOWER:4,NORMSP_1:8;
        end;
     for n be Nat holds P[n] from NAT_1:sch 1(A4,A5);
     hence thesis;
   end;
  ||.z.|| GeoSeq is summable by A1,SERIES_1:28;
then ||.z GeoSeq.|| is summable by A2,SERIES_1:24;
then z GeoSeq is norm_summable by SERIES_1_def_5;
hence thesis by SERIES_1_40;
end;

theorem
  for X be Banach_Algebra
  for x be Point of X
      st ||.1_ X - x .|| < 1
  holds ( (1_ X - x) GeoSeq is summable
  & (1_ X - x) GeoSeq is norm_summable) by LM010;

LmBeZero1:
 for X be RealNormSpace
   for x be Point of X
     st (for e be Real st e>0 holds  ||.x.|| <e)
   holds x=0.X
  proof
    let X be RealNormSpace;
    let x be Point of X such that
    GS1:   for e be Real st e > 0 holds ||.x.|| <e;
    now assume
      Lb: x<>0.X;
       0<> ||.x.|| by Lb, NORMSP_1:def 2;
     then 0< ||.x.|| by NORMSP_1:8;
     hence contradiction by GS1;
    end;
    hence thesis;
  end;

  LmBeZero2:
  for X be RealNormSpace
   for x,y be Point of X
     st (for e be Real st e>0 holds ||.x-y.|| <e)
   holds x=y
  proof
    let X be RealNormSpace;
    let x,y be Point of X such that
    GS1:   for e be Real st e > 0 holds  ||.x-y.|| <e;
    x-y = 0.X by LmBeZero1,GS1;
    hence thesis by RLVECT_1:35;
  end;

LmBeZero3:
 for X be RealNormSpace
   for x,y be Point of X
     for seq be Real_Sequence st
    ( seq is convergent
      & lim seq=0
      & for n be Nat holds ||.x-y.|| <=seq.n )
    holds x=y
proof
    let X be RealNormSpace;
    let x,y be Point of X;
    let seq be Real_Sequence such that
     AS1: seq is convergent and
     AS2:lim seq=0 and
    AS3:  for n be Nat holds ||.x-y.|| <=seq.n;

now
let e be Real
  such that
  AS4: 0 < e;
  consider n be Nat such that
  B4:for m be Nat st n<=m holds
     abs( seq.m - 0  ) < e by AS1,AS2,AS4,SEQ_2:def 7;
B5:  abs( seq.n -0 ) < e by B4;
   seq.n <=abs( seq.n ) by ABSVALUE:11;
  then
  B7: seq.n < e by B5,AXIOMS:22;
      ||.x-y.|| <= seq.n by AS3;
  hence ||.x-y.|| < e by B7,AXIOMS:22;
end;
hence x=y by LmBeZero2;
thus thesis;
end;

theorem
  for X be Banach_Algebra
  for x be Point of X
      st  ||.1_ X - x .|| < 1
  holds x is invertible & x" = Sum ((1_ X - x) GeoSeq )
  proof
    let X be Banach_Algebra;
    let x be Point of X such that
    AS1:    ||.1_ X - x .|| < 1;
    set seq = (1_ X - x)GeoSeq;
    P1: seq is summable by AS1,LM010;
    reconsider  y=Sum(seq) as Element of X;
    P2:for n be Nat holds
         (Partial_Sums(seq).n)*(1_ X -x)=(Partial_Sums(seq)^\1).n-seq.0
         proof
   defpred P[Nat] means
   (Partial_Sums(seq).$1)*(1_ X -x)=(Partial_Sums(seq)^\1).$1-seq.0;
  I0L:  (Partial_Sums(seq).0)*(1_ X -x)
    =(seq.0)*(1_ X -x) by SERIES_1_def_1
   .=1_ X *(1_ X -x) by defGeo
   .=1_ X -x by LM000;
  I0R:  (Partial_Sums(seq)^\1).0-seq.0
  =(Partial_Sums(seq)).(0+1)-seq.0 by SEQM_3_def_9
  .=(Partial_Sums(seq)).(0)+seq.1-seq.0 by SERIES_1_def_1
  .=seq.0 + seq.1-seq.0 by SERIES_1_def_1
  .=seq.1 +(seq.0-seq.0) by LM000
  .=seq.1 + 0.X by RLVECT_1:28
  .=seq.(0+1) by RLVECT_1:10
  .=seq.0*(1_ X -x) by defGeo
  .=1_ X * (1_ X -x) by defGeo
  .=1_ X -x by LM000;
 I4: P[0] by I0L,I0R;
 I5: for k be Nat st P[k] holds P[k+1]
     proof let k be Nat;
      assume
    I6:  P[k];
  IKL:  (Partial_Sums(seq).(k+1))*(1_ X -x)
         = ( Partial_Sums(seq).k+seq.(k+1) )*(1_ X -x) by SERIES_1_def_1
         .=(Partial_Sums(seq).k)*(1_ X -x)+seq.(k+1)*(1_ X -x) by LM000
         .=(Partial_Sums(seq)^\1).k-seq.0+seq.((k+1)+1) by defGeo,I6;
  (Partial_Sums(seq)^\1).(k+1)-seq.0
  =(Partial_Sums(seq)).((k+1)+1)-seq.0 by SEQM_3_def_9
  .=(Partial_Sums(seq)).(k+1)+seq.((k+1)+1)-seq.0 by SERIES_1_def_1
  .=(Partial_Sums(seq)^\1).(k)+seq.((k+1)+1)-seq.0 by SEQM_3_def_9
  .=(Partial_Sums(seq)^\1).(k)+seq.((k+1)+1)+(-seq.0) by RLVECT_1:def 11
  .=(Partial_Sums(seq)^\1).(k)+(-seq.0) +seq.((k+1)+1) by RLVECT_1:def 6
  .=(Partial_Sums(seq)^\1).(k)-seq.0 +seq.((k+1)+1) by RLVECT_1:def 11;
      hence (Partial_Sums(seq).(k+1))*(1_ X -x)
      =(Partial_Sums(seq)^\1).(k+1)-seq.0 by IKL;
      end;
   for n be Nat holds P[n] from NAT_1:sch 1(I4,I5);
    hence thesis;
         end;

RdefGeo:
for n be Nat holds (1_ X -x)*seq.n=seq.(n+1)
proof
 defpred P[Nat] means (1_ X -x)*seq.$1=seq.($1+1);
       I0L:  (1_ X -x)*seq.0
             =(1_ X -x)*1_ X by defGeo
            .=(1_ X -x) by LM000;
       I0R: seq.(0+1)
             =seq.0 * (1_ X -x) by defGeo
             .=1_ X * (1_ X -x) by defGeo
             .=(1_ X -x) by LM000;
       A0: P[0] by I0R,I0L;
       A1:  for k be Nat st P[k] holds P[k+1]
       proof
       let k be Nat such that
         BS1: P[k];
        thus (1_ X -x)*seq.(k+1)
        = (1_ X -x)*(seq.(k)*(1_ X -x)) by defGeo
        .=(1_ X -x)*seq.(k)*(1_ X -x) by LM000
        .=seq.((k+1)+1) by defGeo,BS1;
       end;
        for n be Nat holds P[n] from NAT_1:sch 1(A0,A1);
       hence thesis;
end;
  P3:for n be Nat holds
        (1_ X -x)* (Partial_Sums(seq).n)=(Partial_Sums(seq)^\1).n-seq.0
         proof
   defpred P[Nat] means
  (1_ X -x)* (Partial_Sums(seq).$1)=(Partial_Sums(seq)^\1).$1-seq.0;
  I0L:  (1_ X -x)*(Partial_Sums(seq).0)
    =(1_ X -x)*(seq.0) by SERIES_1_def_1
   .=(1_ X -x)*1_ X  by defGeo
   .=1_ X -x by LM000;
  I0R:  (Partial_Sums(seq)^\1).0-seq.0
  =(Partial_Sums(seq)).(0+1)-seq.0 by SEQM_3_def_9
  .=(Partial_Sums(seq)).(0)+seq.1-seq.0 by SERIES_1_def_1
  .=seq.0 + seq.1-seq.0 by SERIES_1_def_1
  .=seq.1 +(seq.0-seq.0) by LM000
  .=seq.1 + 0.X by RLVECT_1:28
  .=seq.(0+1) by RLVECT_1:10
  .=seq.0*(1_ X -x) by defGeo
  .=1_ X * (1_ X -x) by defGeo
  .=1_ X -x by LM000;
 I4: P[0] by I0L,I0R;
 I5: for k be Nat st P[k] holds P[k+1]
     proof let k be Nat;
      assume
    I6:  P[k];
  IKL:  (1_ X -x)*(Partial_Sums(seq).(k+1))
         = (1_ X -x)*( Partial_Sums(seq).k+seq.(k+1) ) by SERIES_1_def_1
         .=(1_ X -x)*(Partial_Sums(seq).k)+(1_ X -x)*seq.(k+1) by LM000
         .=(Partial_Sums(seq)^\1).k-seq.0+seq.((k+1)+1) by RdefGeo,I6;
   (Partial_Sums(seq)^\1).(k+1)-seq.0
  =(Partial_Sums(seq)).((k+1)+1)-seq.0 by SEQM_3_def_9
  .=(Partial_Sums(seq)).(k+1)+seq.((k+1)+1)-seq.0 by SERIES_1_def_1
  .=(Partial_Sums(seq)^\1).(k)+seq.((k+1)+1)-seq.0 by SEQM_3_def_9
  .=(Partial_Sums(seq)^\1).(k)+seq.((k+1)+1)+(-seq.0) by RLVECT_1:def 11
  .=(Partial_Sums(seq)^\1).(k)+(-seq.0) +seq.((k+1)+1) by RLVECT_1:def 6
  .=(Partial_Sums(seq)^\1).(k)-seq.0 +seq.((k+1)+1) by RLVECT_1:def 11;
      hence (1_ X -x)*(Partial_Sums(seq).(k+1))
      =(Partial_Sums(seq)^\1).(k+1)-seq.0 by IKL;
      end;
   for n be Nat holds P[n] from NAT_1:sch 1(I4,I5);
    hence thesis;
         end;
    P4:    now let n be Nat;
           reconsider yn=Partial_Sums(seq).n  as Element of X;
         P20:  1_ X -y*x =1_ X -  0.X -y*x by RLVECT_1:26
          .=1_ X - (yn*x-yn*x) -y*x by RLVECT_1:28
          .=1_ X-yn*x+yn*x-y*x by RLVECT_1:43
          .=(1_ X-yn*x)+(yn*x-y*x) by RLVECT_1:42
           .=(1_ X-yn*x)+(yn-y)*x by LM000;
       P40: yn*(1_ X - (1_ X-x))
            =  yn*1_ X - yn*(1_ X -x) by LM000
           .=  yn - yn*(1_ X -x) by LM000
           .=Partial_Sums(seq).n-((Partial_Sums(seq)^\1).n-seq.0) by P2
           .=Partial_Sums(seq).n-(Partial_Sums(seq)^\1).n +seq.0 by LM000;
       (Partial_Sums(seq)).(n+1) = (Partial_Sums(seq)).n
        + seq.(n+1) by SERIES_1_def_1;
       then (Partial_Sums(seq)).(n+1) = (Partial_Sums(seq)).n + (seq ^\1).n
        by SEQM_3_def_9;
       then
       P42:  yn*(1_ X - (1_ X-x))
             =Partial_Sums(seq).n
              -( (Partial_Sums(seq)).n + (seq ^\1).n)+seq.0 by P40,SEQM_3_def_9
             .=Partial_Sums(seq).n-(Partial_Sums(seq)).n - (seq ^\1).n
              +seq.0 by LM000
            .=0.X - (seq ^\1).n +seq.0 by RLVECT_1:28
            .=0.X-((seq ^\1).n-seq.0) by RLVECT_1:43
            .=-((seq ^\1).n-seq.0) by RLVECT_1:27
            .=seq.0+(- (seq ^\1).n) by RLVECT_1:47
            .= (seq.0- (seq ^\1).n) by RLVECT_1:def 11
            .=seq.0-seq.(n+1) by SEQM_3_def_9
            .=1_ X - seq.(n+1) by defGeo;
p7:  (1_ X-yn*x) =1_ X -(1_ X - seq.(n+1) ) by LM000,P42
           .=seq.(n+1) by LM000
           .=(seq^\1).n  by SEQM_3_def_9;
      P80: ||. (seq^\1).n+(yn-y)*x .||
          <= ||. (seq^\1).n .|| + ||. (yn-y)*x .|| by NORMSP_1:def 2;
      P90:  ||. (seq^\1).n .|| + ||. (yn-y)*x .||
         =  ||. (seq^\1).||.n  + ||. (yn-y)*x .|| by NORMSP_1:def 10;
      P100:  ||. (yn-y)*x .|| <= ||. (yn-y).|| * ||. x .|| by LM000;
      P110:  ||. (seq^\1).||.n  + ||. (yn-y)*x .||
              <= ||. (seq^\1).||.n + ||. (yn-y).|| * ||. x .||
              by P100,REAL_1:55;
      ||. (seq^\1).n+(yn-y)*x .||
             <= ||. (seq^\1).||.n + ||. (yn-y).|| * ||. x .||
             by AXIOMS:22,P80,P90,P110;
      then ||. 1_ X -y*x.|| <= ||. (seq^\1).||.n
             +  ||.(Partial_Sums(seq)-y).n.|| * ||.x.||
               by p7,P20,NORMSP_1:def 7;
      then ||. 1_ X -y*x.||
             <= ||. (seq^\1).||.n
             +  ||. Partial_Sums(seq)-y.||.n * ||. x .|| by NORMSP_1:def 10;
      then ||. 1_ X -y*x.||
               <= ||. (seq^\1).||.n
               + ( ||. x .|| (#) ||. Partial_Sums(seq)-y.||).n  by SEQ_1:13;
     hence ||. 1_ X -y*x.||
               <=( ||. (seq^\1).||
               +  ||. x .|| (#) ||. Partial_Sums(seq)-y.||).n  by SEQ_1:11;
    end;
 P5:    now let n be Nat;
           reconsider yn=Partial_Sums(seq).n  as Element of X;
         P20:  1_ X -x*y
          =1_ X -  0.X -x*y by RLVECT_1:26
          .=1_ X - (x*yn-x*yn) -x*y by RLVECT_1:28
          .=1_ X-x*yn+x*yn-x*y  by RLVECT_1:43
          .=(1_ X-x*yn)+(x*yn-x*y) by RLVECT_1:42
           .=(1_ X-x*yn)+x*(yn-y) by LM000;
       P40: (1_ X - (1_ X-x))*yn
            =  1_ X*yn - (1_ X -x)*yn by LM000
           .=  yn - (1_ X -x)*yn by LM000
           .=Partial_Sums(seq).n-((Partial_Sums(seq)^\1).n-seq.0) by P3
           .=Partial_Sums(seq).n-(Partial_Sums(seq)^\1).n +seq.0 by LM000;
       (Partial_Sums(seq)).(n+1) = (Partial_Sums(seq)).n
        + seq.(n+1) by SERIES_1_def_1;
       then (Partial_Sums(seq)).(n+1) = (Partial_Sums(seq)).n + (seq ^\1).n
        by SEQM_3_def_9;
       then
       P42:  (1_ X - (1_ X-x))*yn
             =Partial_Sums(seq).n
              -( (Partial_Sums(seq)).n + (seq ^\1).n)+seq.0 by P40,SEQM_3_def_9
             .=Partial_Sums(seq).n-(Partial_Sums(seq)).n - (seq ^\1).n
              +seq.0 by LM000
            .=0.X - (seq ^\1).n +seq.0 by RLVECT_1:28
            .=0.X-((seq ^\1).n-seq.0) by RLVECT_1:43
            .=-((seq ^\1).n-seq.0) by RLVECT_1:27
            .=seq.0+(- (seq ^\1).n) by RLVECT_1:47
            .= (seq.0- (seq ^\1).n) by RLVECT_1:def 11
            .=seq.0-seq.(n+1) by SEQM_3_def_9
            .=1_ X - seq.(n+1) by defGeo;
p7:   (1_ X-x*yn)
            =1_ X -(1_ X - seq.(n+1) ) by LM000,P42
           .=seq.(n+1) by LM000
           .=(seq^\1).n  by SEQM_3_def_9;
      P80: ||. (seq^\1).n+x*(yn-y) .||
          <= ||. (seq^\1).n .|| + ||. x*(yn-y) .|| by NORMSP_1:def 2;
      P90:  ||. (seq^\1).n .|| + ||. x*(yn-y) .||
         =  ||. (seq^\1).||.n  + ||. x*(yn-y) .|| by NORMSP_1:def 10;
        ||. x*(yn-y) .|| <=   ||. x .||*||. (yn-y).||  by LM000; then
        ||. (seq^\1).||.n  + ||. x*(yn-y) .||
              <= ||. (seq^\1).||.n + ||. x .|| * ||. (yn-y).||
              by REAL_1:55; then
      ||. (seq^\1).n+x*(yn-y) .||
             <= ||. (seq^\1).||.n +  ||. x .|| * ||. (yn-y).||
             by AXIOMS:22,P80,P90;
      then ||. 1_ X -x*y.|| <= ||. (seq^\1).||.n
             +  ||. x .|| * ||. (Partial_Sums(seq)-y).n.|| by
             P20,NORMSP_1:def 7,p7;
      then ||. 1_ X -x*y.|| <= ||. (seq^\1).||.n
             +  ||. x .|| * ||. Partial_Sums(seq)-y.||.n
             by NORMSP_1:def 10;
      then ||. 1_ X -x*y.|| <= ||. (seq^\1).||.n
                + ( ||. x .|| (#) ||. Partial_Sums(seq)-y.||).n by SEQ_1:13;
      hence ||. 1_ X -x*y.||
                <=( ||. (seq^\1).||
                +  ||. x .|| (#) ||. Partial_Sums(seq)-y.||).n  by SEQ_1:11;
  end;
  P6:   Partial_Sums(seq) is convergent by P1,SERIES_1_def_2;
  lim Partial_Sums(seq) =y by SERIES_1_def_3; then
  P8:   ||. Partial_Sums(seq)-y.|| is convergent
      & lim ||. Partial_Sums(seq)-y.|| =0
   by NORMSP_1:41,P6;
  P9:  ||. x .|| (#)  ||. Partial_Sums(seq)-y.|| is convergent
       by SEQ_2:21,P8;
  P10:  lim ( ||. x .|| (#)  ||. Partial_Sums(seq)-y.|| )
            = ||. x .|| *0 by P8,SEQ_2:22
            .=0;
  P11: seq is convergent & lim seq =0.X  by P1,SERIES_1_7;
  seq^\1 is convergent & lim (seq^\1)=0.X by P11, SEQ_4_33;
  then
  P13:   ||. seq^\1.|| is convergent & lim (  ||. seq^\1.|| )= ||. 0.X.||
        by LOPBAN_1:47;
  then
  P14:   ||. seq^\1.|| +  ||. x .|| (#)  ||. Partial_Sums(seq)-y.||
       is convergent by SEQ_2:19,P9;
  P15: lim( ||. seq^\1.|| +  ||. x .|| (#)  ||. Partial_Sums(seq)-y.||)
         = lim( ||. seq^\1.||)
         + lim (  ||. x .|| (#)  ||. Partial_Sums(seq)-y.||)
          by SEQ_2:20,P9,P13
         .=0 by P10,P13,NORMSP_1:5;
  P16:  1_ X = y*x by P4,P14,P15,LmBeZero3;
  P17:  1_ X = x*y by P5,P14,P15,LmBeZero3;
 hence x is invertible by P16,Def01;
 hence thesis by P16,P17,Def02;
  end;

