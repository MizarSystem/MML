      FUNCOP_1, XREAL_0, ORDINAL1, XBOOLE_0, NDIFF_2;
 notations TARSKI, XBOOLE_0, SUBSET_1, FUNCT_1, RELSET_1, PARTFUN1, FUNCT_2,
