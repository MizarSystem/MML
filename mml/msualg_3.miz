 constructors FINSEQ_4, MSUALG_2, PRALG_2, RELAT_2, PRALG_1;
 registrations FUNCT_1, PBOOLE, MSUALG_1, MSUALG_2, RELSET_1, STRUCT_0,
      PARTFUN1, FUNCT_2, XBOOLE_0, FUNCOP_1, FRAENKEL, CARD_3;
