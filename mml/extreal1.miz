theorem Th32:
  for x,y being R_eal st y <> 0. holds
  for a,b being Real st x = a & y = b holds x / y = a / b
proof
  let x,y be R_eal;
  assume
A1: y <> 0.;
  let a,b be Real;
  assume
A2: x = a & y = b;
  then consider a1,b1 being Real such that
A3: x = a1 & y = b1 & x / y = a1 / b1 by A1,Def2;
  thus thesis by A2,A3;
end;

