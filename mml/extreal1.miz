  hence thesis;
end;

theorem
  not ((y = +infty & z = +infty) or (y = -infty & z = -infty)) &
  x <> +infty & x <> -infty implies x * (y - z) = x * y - x * z
proof
  assume not ((y = +infty & z = +infty) or (y = -infty & z = -infty)) &
  x <> +infty & x <> -infty;
  then
A1: x * (y + -z) = x * y + x * (-z) by Lm2;
  x * (y - z) = x * y + -(x * z) by A1,Th26;
  hence thesis;
end;

definition
  let x,y be R_eal;
  assume
A1: not((x=-infty or x=+infty) & (y=-infty or y=+infty)) & y <> 0.;
  redefine func x / y -> R_eal means
  :Def2:
  (ex a,b being Real st (x = a & y = b & it = a / b)) or
  (((x=+infty & 0. < y) or (x=-infty & y < 0.)) & it = +infty) or
  (((x=-infty & 0. < y) or (x=+infty & y < 0.)) & it = -infty) or
  ((y = -infty or y = +infty) & it = 0.);
 coherence by XXREAL_0:def 1;
 compatibility
  proof let z be R_eal;
   thus z = x/y implies
     (ex a,b being Real st (x = a & y = b & z = a / b)) or
     (((x=+infty & 0. < y) or (x=-infty & y < 0.)) & z = +infty) or
    (((x=-infty & 0. < y) or (x=+infty & y < 0.)) & z = -infty) or
    ((y = -infty or y = +infty) & z = 0.)
   proof assume
Z:   z = x/y;
    now per cases by A1,XXREAL_0:14;
     case
C:    x in REAL & y in REAL;
      then consider c being complex number such that
W1:    y = c & y" = c" by XXREAL_3:def 4;
      reconsider yy=y, xx=x as Real by C;
      reconsider y1=yy" as Real;
      consider a,b being complex number such that
W2:    x = a & y" = b & x*y" = a * b by C,W1,XXREAL_3:def 2;
       z = xx/ yy by W2,Z,W1;
      hence ex a,b being Real st x = a & y = b & z = a / b;
     end;
     case
pc:    x=+infty & 0. < y or x=-infty & y < 0.;
      per cases by pc;
       suppose
C:        x=+infty & 0. < y;
       then reconsider y as Real by A1,XXREAL_0:14;
       reconsider yy=y as complex number;
       x / y = +infty by C,XXREAL_3:def 2;
      hence z = +infty by Z;
      end;
       suppose
C:        x=-infty & y < 0.;
       then reconsider y as Real by A1,XXREAL_0:14;
       reconsider yy=y as complex number;
       x / y = +infty by C,XXREAL_3:def 2;
      hence z = +infty by Z;
      end;
     end;
     case
pc:    x=-infty & 0. < y or x=+infty & y < 0.;
      per cases by pc;
       suppose
C:        x=-infty & 0. < y;
       then reconsider y as Real by A1,XXREAL_0:14;
       reconsider yy=y as complex number;
       x / y = -infty by C,XXREAL_3:def 2;
      hence z = -infty by Z;
      end;
       suppose
C:        x=+infty & y < 0.;
       then reconsider y as Real by A1,XXREAL_0:14;
       reconsider yy=y as complex number;
       x / y = -infty by C,XXREAL_3:def 2;
      hence z = -infty by Z;
      end;
     end;
     case y = -infty or y = +infty;
       then y" = 0 by XXREAL_3:def 4;
      hence z = 0. by Z,XXREAL_3:1;
     end;
    end;
   hence thesis;
   end;
   assume
Z:  (ex a,b being Real st x = a & y = b & z = a / b) or
     (((x=+infty & 0. < y) or (x=-infty & y < 0.)) & z = +infty) or
    (((x=-infty & 0. < y) or (x=+infty & y < 0.)) & z = -infty) or
    ((y = -infty or y = +infty) & z = 0.);
   per cases by Z;
   suppose ex a,b being Real st x = a & y = b & z = a / b;
    then consider a,b being Real such that
W1:   x = a & y = b & z = a / b;
    reconsider xx=x, yy=y as real number by W1;
Y:   yy" = b" by W1;
    then x is real & y" is real by W1;
    then reconsider y1 = y" as real number;
     z = xx * y1 by Y,W1;
    then z = x*y";
   hence thesis;
   end;
   suppose
S:   ((x=+infty & 0. < y) or (x=-infty & y < 0.)) & z = +infty;
   per cases by S;
   suppose
S1:  x=+infty & 0. < y;
     then reconsider yy=y as positive Real by A1,XXREAL_0:14;
     y in REAL by A1,S1,XXREAL_0:14;
     then ex a being complex number st y = a & y" = a" by XXREAL_3:def 4;
     then
U:   yy" = y";
     y" is positive by U;
   hence thesis by S1,S,XXREAL_3:def 2;
   end;
   suppose
S1:  x=-infty & y < 0.;
     then reconsider yy=y as Real by A1,XXREAL_0:14;
     y in REAL by A1,S1,XXREAL_0:14;
     then ex a being complex number st y = a & y" = a" by XXREAL_3:def 4;
     then
U:   yy" = y";
     y is negative by S1;
     then y" is negative by U;
   hence thesis by S1,S,XXREAL_3:def 2;
   end;
   end;
   suppose
S:  ((x=-infty & 0. < y) or (x=+infty & y < 0.)) & z = -infty;
   per cases by S;
   suppose
S1:  x=-infty & 0. < y;
     then reconsider yy=y as positive Real by A1,XXREAL_0:14;
     y in REAL by A1,S1,XXREAL_0:14;
     then ex a being complex number st y = a & y" = a" by XXREAL_3:def 4;
     then
U:   yy" = y";
     y" is positive by U;
   hence thesis by S1,S,XXREAL_3:def 2;
   end;
   suppose
S1:  x=+infty & y < 0.;
     then reconsider yy=y as Real by A1,XXREAL_0:14;
     y in REAL by A1,S1,XXREAL_0:14;
     then ex a being complex number st y = a & y" = a" by XXREAL_3:def 4;
     then
U:   yy" = y";
     y is negative by S1;
     then y" is negative by U;
   hence thesis by S1,S,XXREAL_3:def 2;
   end;
   end;
   suppose
S:   (y = -infty or y = +infty) & z = 0.;
    then y" = 0 by XXREAL_3:def 4;
    then x*y" = 0 by XXREAL_3:1;
   hence thesis by S;
   end;

  end;
end;

