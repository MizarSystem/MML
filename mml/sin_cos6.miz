Lm11: now
  let i, r;
  let p1, p2 be real number;
  assume
A1: r in [.p1+T(i),p2+T(i).];
  then r <= p2+T(i) by XXREAL_1:1;
  then
A2: r+2*PI <= p2+T(i)+2*PI by XREAL_1:8;
  p1+T(i) <= r by A1,XXREAL_1:1;
  then p1+T(i)+2*PI <= r+2*PI by XREAL_1:8;
  hence r+2*PI in [.p1+T(i+1),p2+T(i+1).] by A2,XXREAL_1:1;
end;

Lm12: now
  let i, r;
  let p1, p2 be real number;
  assume r in [.p1+T(i),p2+T(i).] /\ REAL;
  then r in [.p1+T(i),p2+T(i).] by XBOOLE_0:def 4;
  then r+2*PI in [.p1+T(i+1),p2+T(i+1).] by Lm11;
  hence r+2*PI in [.p1+T(i+1),p2+T(i+1).] /\ REAL by XBOOLE_0:def 4;
end;

Lm13: now
  let i, r;
  let p1, p2 be real number;
  assume
A1: r in [.p1+T(i),p2+T(i).];
  then r <= p2+T(i) by XXREAL_1:1;
  then
A2: r-2*PI <= p2+T(i)-2*PI by XREAL_1:11;
  p1+T(i) <= r by A1,XXREAL_1:1;
  then p1+T(i)-2*PI <= r-2*PI by XREAL_1:11;
  hence r-2*PI in [.p1+T(i-1),p2+T(i-1).] by A2,XXREAL_1:1;
end;

Lm14: now
  let i, r;
  let p1, p2 be real number;
  assume r in [.p1+T(i),p2+T(i).] /\ REAL;
  then r in [.p1+T(i),p2+T(i).] by XBOOLE_0:def 4;
  then r-2*PI in [.p1+T(i-1),p2+T(i-1).] by Lm13;
  hence r-2*PI in [.p1+T(i-1),p2+T(i-1).] /\ REAL by XBOOLE_0:def 4;
end;

theorem Th53:
  sin|[.-PI/2+2*PI*i,PI/2+2*PI*i.] is increasing
proof
  defpred P[Integer] means sin|[.-PI/2+T($1),PI/2+T($1).] is increasing;
A1: for i holds P[i] implies P[i-1] & P[i+1]
  proof
    let i such that
A2: P[i];
    set Z = [.-PI/2+T(i-1+1),PI/2+T(i-1+1).];
    thus P[i-1]
    proof
      set Y = [.-PI/2+T(i-1),PI/2+T(i-1).];
      now
        let r1, r2 be Element of REAL;
        assume r1 in Y /\ dom sin & r2 in Y /\ dom sin;
        then
A3:     r1+2*PI in Z /\ dom sin & r2+2*PI in Z /\ dom sin by Lm12,
        SIN_COS:27;
        assume r1 < r2;
        then r1+2*PI < r2+2*PI by XREAL_1:8;
        then sin.(r1+2*PI) < sin.(r2+2*PI*1) by A2,A3,RFUNCT_2:43;
        then sin.(r1+2*PI*1) < sin.r2 by Th8;
        hence sin.r1 < sin.r2 by Th8;
      end;
      hence thesis by RFUNCT_2:43;
    end;
    set Y = [.-PI/2+T(i+1),PI/2+T(i+1).];
A4: Z = [.-PI/2+T(i+1-1),PI/2+T(i+1-1).];
    now
      let r1, r2 be Element of REAL;
      assume r1 in Y /\ dom sin & r2 in Y /\ dom sin;
      then
A5:   r1-2*PI in Z /\ dom sin & r2-2*PI in Z /\ dom sin by A4,Lm14,
      SIN_COS:27;
      assume r1 < r2;
      then r1-2*PI < r2-2*PI by XREAL_1:11;
      then sin.(r1-2*PI) < sin.(r2+2*PI*(-1)) by A2,A5,RFUNCT_2:43;
      then sin.(r1+2*PI*(-1)) < sin.r2 by Th8;
      hence sin.r1 < sin.r2 by Th8;
    end;
    hence thesis by RFUNCT_2:43;
  end;
A6: P[0] by COMPTRIG:39;
  for i holds P[i] from INT_1:sch 4(A6,A1);
  hence thesis;
end;

theorem Th54:
  sin|[.PI/2+2*PI*i,3/2*PI+2*PI*i.] is decreasing
proof
  defpred P[Integer] means sin|[.PI/2+T($1),3/2*PI+T($1).] is decreasing;
A1: for i holds P[i] implies P[i-1] & P[i+1]
  proof
    let i such that
A2: P[i];
    set Z = [.PI/2+T(i-1+1),3/2*PI+T(i-1+1).];
    thus P[i-1]
    proof
      set Y = [.PI/2+T(i-1),3/2*PI+T(i-1).];
      now
        let r1, r2 be Element of REAL;
        assume r1 in Y /\ dom sin & r2 in Y /\ dom sin;
        then
A3:     r1+2*PI in Z /\ dom sin & r2+2*PI in Z /\ dom sin by Lm12,
        SIN_COS:27;
        assume r1 < r2;
        then r1+2*PI < r2+2*PI by XREAL_1:8;
        then sin.(r1+2*PI) > sin.(r2+2*PI*1) by A2,A3,RFUNCT_2:44;
        then sin.(r1+2*PI*1) > sin.r2 by Th8;
        hence sin.r1 > sin.r2 by Th8;
      end;
      hence thesis by RFUNCT_2:44;
    end;
    set Y = [.PI/2+T(i+1),3/2*PI+T(i+1).];
A4: Z = [.PI/2+T(i+1-1),3/2*PI+T(i+1-1).];
    now
      let r1, r2 be Element of REAL;
      assume r1 in Y /\ dom sin & r2 in Y /\ dom sin;
      then
A5:   r1-2*PI in Z /\ dom sin & r2-2*PI in Z /\ dom sin by A4,Lm14,
      SIN_COS:27;
      assume r1 < r2;
      then r1-2*PI < r2-2*PI by XREAL_1:11;
      then sin.(r1-2*PI) > sin.(r2+2*PI*(-1)) by A2,A5,RFUNCT_2:44;
      then sin.(r1+2*PI*(-1)) > sin.r2 by Th8;
      hence sin.r1 > sin.r2 by Th8;
    end;
    hence thesis by RFUNCT_2:44;
  end;
A6: P[0] by COMPTRIG:40;
  for i holds P[i] from INT_1:sch 4(A6,A1);
  hence thesis;
end;

theorem Th55:
  cos|[.2*PI*i,PI+2*PI*i.] is decreasing
proof
  defpred P[Integer] means cos|[.T($1),PI+T($1).] is decreasing;
A1: for i holds P[i] implies P[i-1] & P[i+1]
  proof
    let i such that
A2: P[i];
    set Z = [.0+T(i-1+1),PI+T(i-1+1).];
    thus P[i-1]
    proof
      set Y = [.T(i-1),PI+T(i-1).];
A3:   Y = [.0+T(i-1),PI+T(i-1).];
      now
        let r1, r2 be Element of REAL;
        assume r1 in Y /\ dom cos & r2 in Y /\ dom cos;
        then
A4:     r1+2*PI in Z /\ dom cos & r2+2*PI in Z /\ dom cos by A3,
        Lm12,
        SIN_COS:27;
        assume r1 < r2;
        then r1+2*PI < r2+2*PI by XREAL_1:8;
        then cos.(r1+2*PI) > cos.(r2+2*PI*1) by A2,A4,RFUNCT_2:44;
        then cos.(r1+2*PI*1) > cos.r2 by Th10;
        hence cos.r1 > cos.r2 by Th10;
      end;
      hence thesis by RFUNCT_2:44;
    end;
    set Y = [.T(i+1),PI+T(i+1).];
A5: Y = [.0+T(i+1),PI+T(i+1).] & Z = [.T(i+1-1),PI+T(i+1-1).];
    now
      let r1, r2 be Element of REAL;
      assume r1 in Y /\ dom cos & r2 in Y /\ dom cos;
      then
A6:   r1-2*PI in Z /\ dom cos & r2-2*PI in Z /\ dom cos by A5,Lm14,
      SIN_COS:27;
      assume r1 < r2;
      then r1-2*PI < r2-2*PI by XREAL_1:11;
      then cos.(r1-2*PI) > cos.(r2+2*PI*(-1)) by A2,A6,RFUNCT_2:44;
      then cos.(r1+2*PI*(-1)) > cos.r2 by Th10;
      hence cos.r1 > cos.r2 by Th10;
    end;
    hence thesis by RFUNCT_2:44;
  end;
A7: P[0] by COMPTRIG:41;
  for i holds P[i] from INT_1:sch 4(A7,A1);
  hence thesis;
end;

theorem Th56:
  cos|[.PI+2*PI*i,2*PI+2*PI*i.] is increasing
proof
  defpred P[Integer] means cos|[.PI+T($1),2*PI+T($1).] is increasing;
A1: for i holds P[i] implies P[i-1] & P[i+1]
  proof
    let i such that
A2: P[i];
    set Z = [.PI+T(i-1+1),2*PI+T(i-1+1).];
    thus P[i-1]
    proof
      set Y = [.PI+T(i-1),2*PI+T(i-1).];
      now
        let r1, r2 be Element of REAL;
        assume r1 in Y /\ dom cos & r2 in Y /\ dom cos;
        then
A3:     r1+2*PI in Z /\ dom cos & r2+2*PI in Z /\ dom cos by Lm12,
        SIN_COS:27;
        assume r1 < r2;
        then r1+2*PI < r2+2*PI by XREAL_1:8;
        then cos.(r1+2*PI) < cos.(r2+2*PI*1) by A2,A3,RFUNCT_2:43;
        then cos.(r1+2*PI*1) < cos.r2 by Th10;
        hence cos.r1 < cos.r2 by Th10;
      end;
      hence thesis by RFUNCT_2:43;
    end;
    set Y = [.PI+T(i+1),2*PI+T(i+1).];
A4: Z = [.PI+T(i+1-1),2*PI+T(i+1-1).];
    now
      let r1, r2 be Element of REAL;
      assume r1 in Y /\ dom cos & r2 in Y /\ dom cos;
      then
A5:   r1-2*PI in Z /\ dom cos & r2-2*PI in Z /\ dom cos by A4,Lm14,
      SIN_COS:27;
      assume r1 < r2;
      then r1-2*PI < r2-2*PI by XREAL_1:11;
      then cos.(r1-2*PI) < cos.(r2+2*PI*(-1)) by A2,A5,RFUNCT_2:43;
      then cos.(r1+2*PI*(-1)) < cos.r2 by Th10;
      hence cos.r1 < cos.r2 by Th10;
    end;
    hence thesis by RFUNCT_2:43;
  end;
A6: P[0] by COMPTRIG:42;
  for i holds P[i] from INT_1:sch 4(A6,A1);
  hence thesis;
end;

