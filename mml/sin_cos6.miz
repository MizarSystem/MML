  by Th62,Th63,FUNCT_1:61;

theorem Th69:
  -1 <= r & r <= 1 implies sin arcsin r = r
proof
  assume -1 <= r & r <= 1;
  then
A1: r in [.-1,1.] by TOPREAL5:1;
  then
A2: (arcsin.r) in [.-PI/2,PI/2.] by Th63,Th64,FUNCT_1:def 5;
  thus sin arcsin r = sin.(arcsin r) by SIN_COS:def 21
    .= sin.(arcsin.r) by Def2
    .= ((sin|[.-PI/2,PI/2.]) qua Function).(arcsin.r) by A2,FUNCT_1:72
    .= (id [.-1,1.]).r by A1,Th64,Th65,FUNCT_1:23
    .= r by A1,FUNCT_1:35;
end;

theorem Th70:
  -PI/2 <= r & r <= PI/2 implies arcsin sin r = r
proof
  assume -PI/2 <= r & r <= PI/2;
  then
A1: r in [.-PI/2,PI/2.] by TOPREAL5:1;
A2: dom (sin|[.-PI/2,PI/2.]) = [.-PI/2,PI/2.] by Lm1,RELAT_1:91;
  thus arcsin sin r = arcsin(sin.r) by SIN_COS:def 21
    .= arcsin.(sin.r) by Def2
    .= arcsin.((sin|[.-PI/2,PI/2.]).r) by A1,FUNCT_1:72
    .= (id [.-PI/2,PI/2.]).r by A1,A2,Th67,FUNCT_1:23
    .= r by A1,FUNCT_1:35;
end;

theorem
  arcsin (-1) = -PI/2 by Lm9,Th7,Th70;

theorem
  arcsin 0 = 0 by Lm8,Th70,SIN_COS:34;

theorem
  arcsin 1 = PI/2 by Lm9,Th70,SIN_COS:82;

theorem
  -1 <= r & r <= 1 & arcsin r = -PI/2 implies r = -1 by Th7,Th69;

theorem
  -1 <= r & r <= 1 & arcsin r = 0 implies r = 0 by Th69,SIN_COS:34;

theorem
  -1 <= r & r <= 1 & arcsin r = PI/2 implies r = 1 by Th69,SIN_COS:82;

theorem Th77:
  -1 <= r & r <= 1 implies -PI/2 <= arcsin r & arcsin r <= PI/2
proof
  assume -1 <= r & r <= 1;
  then r in [.-1,1.] by TOPREAL5:1;
  then arcsin.r in rng arcsin by Th64,FUNCT_1:def 5;
  then arcsin r in rng arcsin by Def2;
  hence thesis by Th63,TOPREAL5:1;
end;

theorem Th78:
  -1 < r & r < 1 implies -PI/2 < arcsin r & arcsin r < PI/2
proof
  assume
A1: -1 < r & r < 1;
  then -PI/2 <= arcsin r & arcsin r <= PI/2 by Th77;
  then -PI/2 < arcsin r & arcsin r < PI/2 or
    -PI/2 = arcsin r or arcsin r = PI/2 by REAL_1:def 5;
  hence thesis by A1,Th7,Th69,SIN_COS:82;
end;

theorem Th79:
  -1 <= r & r <= 1 implies arcsin r = -arcsin(-r)
proof
  assume -1 <= r & r <= 1;
  then
A1: --1 >= -r & -r >= -1 by REAL_1:50;
A2: r = 0-1*(-r)
     .= sin 0 * cos arcsin(-r) - cos 0 * sin arcsin(-r) by A1,Th69,SIN_COS:34
     .= sin(0-arcsin(-r)) by COMPLEX2:4;
  arcsin(-r) <= PI/2 by A1,Th77;
  then
A3: -PI/2 <= -arcsin(-r) by REAL_1:50;
  -PI/2 <= arcsin(-r) by A1,Th77;
  then -arcsin(-r) <= --PI/2 by REAL_1:50;
  hence thesis by A2,A3,Th70;
end;

theorem Th80:
  0 <= s & r^2 + s^2 = 1 implies cos arcsin r = s
proof
  set x = arcsin r;
  assume that
A1: 0 <= s and
A2: r^2 + s^2 = 1;
A3: -1 <= r & r <= 1 by A2,Lm7;
  (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
  then (cos.x)^2 = 1 - (sin.x)^2
    .= 1 - (sin x)^2 by SIN_COS:def 21
    .= 1 - r^2 by A3,Th69;
  then
A4: cos.x = s or cos.x = -s by A2,JGRAPH_3:1;
  -PI/2 <= x & x <= PI/2 by A3,Th77;
  then
A5: x in [.-PI/2,PI/2.] by TOPREAL5:1;
  ---s < 0 or s = 0 by A1,REAL_1:66;
  hence cos x = s by A4,A5,COMPTRIG:28,SIN_COS:def 23;
end;

theorem
  s <= 0 & r^2 + s^2 = 1 implies cos arcsin r = -s
proof
  set x = arcsin r;
  assume that
A1: s <= 0 and
A2: r^2 + s^2 = 1;
A3: -1 <= r & r <= 1 by A2,Lm7;
  (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
  then (cos.x)^2 = 1 - (sin.x)^2
    .= 1 - (sin x)^2 by SIN_COS:def 21
    .= 1 - r^2 by A3,Th69;
  then
A4: cos.x = s or cos.x = -s by A2,JGRAPH_3:1;
  -PI/2 <= x & x <= PI/2 by A3,Th77;
  then
A5: x in [.-PI/2,PI/2.] by TOPREAL5:1;
  0 > s or s = 0 by A1;
  hence cos x = -s by A4,A5,COMPTRIG:28,SIN_COS:def 23;
end;

theorem Th82:
  -1 <= r & r <= 1 implies cos arcsin r = sqrt(1-r^2)
proof
  set s = sqrt(1-r^2);
  assume -1 <= r & r <= 1;
  then r^2+0 <= 1 by JGRAPH_2:7,SQUARE_1:59;
  then 0 <= 1-r^2 by REAL_1:84;
  then 0 <= s & r^2 + s^2 = r^2 + (1-r^2) by SQUARE_1:def 4;
  hence thesis by Th80;
end;

theorem
  arcsin is_increasing_on [.-1,1.]
proof
  set f = sin | [.-PI/2,PI/2.];
A1: f.:[.-PI/2,PI/2.] = [.-1,1.] by Th45,RFUNCT_2:5;
  f is_increasing_on [.-PI/2,PI/2.] by COMPTRIG:39,RFUNCT_2:50;
  then (f|[.-PI/2,PI/2.])" is_increasing_on f.:[.-PI/2,PI/2.] by FCONT_3:17;
  hence thesis by A1,Def1,RELAT_1:101;
end;

theorem
  arcsin is_differentiable_on ].-1,1.[ &
  (-1 < r & r < 1 implies diff(arcsin,r) = 1 / sqrt(1-r^2))
proof
  set f = sin|].-PI/2,PI/2.[;
  set h = sin|[.-PI/2,PI/2.];
  set g = arcsin|].-1,1.[;
A1: f|].-PI/2,PI/2.[ = f by RELAT_1:101;
A2: f" = (h|].-PI/2,PI/2.[)" by Lm17,RELAT_1:103
      .= h"|(h.:].-PI/2,PI/2.[) by RFUNCT_2:40
      .= g by Def1,Lm17,Th46,EXTENS_1:1;
A3: dom (f") = ].-1,1.[ by A2,Lm15,Th64,RELAT_1:91;
A4: sin is_differentiable_on ].-PI/2,PI/2.[ by FDIFF_1:34,SIN_COS:73;
  then
A5: f is_differentiable_on ].-PI/2,PI/2.[ by FDIFF_2:16;
A6: now
    let x0 be Real such that
A7:   x0 in ].-PI/2,PI/2.[;
    diff(f,x0) = (f`|].-PI/2,PI/2.[).x0 by A5,A7,FDIFF_1:def 8
      .= (sin`|].-PI/2,PI/2.[).x0 by A4,FDIFF_2:16
      .= diff(sin,x0) by A4,A7,FDIFF_1:def 8
      .= cos.x0 by SIN_COS:73;
    hence 0 < diff(f,x0) by A7,COMPTRIG:27;
  end;
  then
A8: g is_differentiable_on ].-1,1.[ by A1,A2,A3,A5,FDIFF_2:48;
A9: ].-1,1.[ c= dom arcsin by Th64,RCOMP_1:15;
    for x be Real st x in ].-1,1.[ holds
      g is_differentiable_in x by A8,FDIFF_1:16;
  hence
A11: arcsin is_differentiable_on ].-1,1.[ by A9,FDIFF_1:def 7;
  set s = sqrt(1-r^2);
  set x = arcsin.r;
  assume
A12: -1 < r & r < 1;
  then
A13: r in ].-1,1.[ by JORDAN6:45;
A14: g.r = x by A13,FUNCT_1:72;
A15: x = arcsin r by Def2;
  then -PI/2 < x & x < PI/2 by A12,Th78;
  then
A16: x in ].-PI/2,PI/2.[ by JORDAN6:45;
A17: diff(f,x) = (f`|].-PI/2,PI/2.[).x by A5,A16,FDIFF_1:def 8
    .= (sin`|].-PI/2,PI/2.[).x by A4,FDIFF_2:16
    .= diff(sin,x) by A4,A16,FDIFF_1:def 8
    .= cos.x by SIN_COS:73
    .= cos arcsin r by A15,SIN_COS:def 23
    .= s by A12,Th82;
  thus diff(arcsin,r) = (arcsin`|].-1,1.[).r by A11,A13,FDIFF_1:def 8
    .= (g`|].-1,1.[).r by A11,FDIFF_2:16
    .= diff(g,r) by A8,A13,FDIFF_1:def 8
    .= 1 / s by A1,A2,A3,A5,A6,A13,A14,A17,FDIFF_2:48;
end;

theorem
  arcsin is_continuous_on [.-1,1.]
proof
  set f = sin | [.-PI/2,PI/2.];
A1: dom f = [.-PI/2,PI/2.] by Lm1,RELAT_1:91;
A2: f|[.-PI/2,PI/2.] = f by RELAT_1:102;
  f is_increasing_on [.-PI/2,PI/2.] by COMPTRIG:39,RFUNCT_2:50;
  then (f|[.-PI/2,PI/2.])" is_continuous_on f.:[.-PI/2,PI/2.]
    by A1,Lm9,FCONT_1:54;
  hence thesis by A2,Def1,COMPTRIG:48,RELAT_1:148;
end;

begin :: arccos

definition
  func arccos -> PartFunc of REAL, REAL equals :Def3:
  (cos | [.0,PI.])";
coherence;
end;

definition
  let r be set;
  func arccos r equals :Def4:
  arccos.r;
coherence;
end;

definition
  let r be set;
  redefine func arccos r -> Real;
coherence
proof
  arccos r = arccos.r by Def4;
  hence thesis;
end;
end;

theorem Th86:
  arccos" = cos | [.0,PI.] by Def3,FUNCT_1:65;

theorem Th87:
  rng arccos = [.0,PI.]
proof
  dom (cos|[.0,PI.]) = [.0,PI.] by Lm2,RELAT_1:91;
  hence thesis by Def3,FUNCT_1:55;
end;

registration
  cluster arccos -> one-to-one;
coherence by Def3,FUNCT_1:62;
end;

theorem Th88:
  dom arccos = [.-1,1.] by Def3,COMPTRIG:50,FUNCT_1:55;

theorem Th89:
  (cos | [.0,PI.]) qua Function * arccos = id [.-1,1.]
  by Def3,COMPTRIG:50,FUNCT_1:61;

theorem
  arccos * (cos | [.0,PI.]) = id [.-1,1.]
  by Def3,COMPTRIG:50,FUNCT_1:61;

theorem Th91:
  (cos | [.0,PI.]) * arccos = id [.0,PI.] by Th86,Th87,FUNCT_1:61;

theorem
  arccos qua Function * (cos | [.0,PI.]) = id [.0,PI.]
  by Th86,Th87,FUNCT_1:61;

theorem Th93:
  -1 <= r & r <= 1 implies cos arccos r = r
proof
  assume -1 <= r & r <= 1;
  then
A1: r in [.-1,1.] by TOPREAL5:1;
  then
A2: arccos.r in [.0,PI.] by Th87,Th88,FUNCT_1:def 5;
  thus cos arccos r = cos.(arccos r) by SIN_COS:def 23
    .= cos.(arccos.r) by Def4
    .= ((cos|[.0,PI.]) qua Function).(arccos.r) by A2,FUNCT_1:72
    .= (id [.-1,1.]).r by A1,Th88,Th89,FUNCT_1:23
    .= r by A1,FUNCT_1:35;
end;

theorem Th94:
  0 <= r & r <= PI implies arccos cos r = r
proof
  assume 0 <= r & r <= PI;
  then
A1: r in [.0,PI.] by TOPREAL5:1;
A2: dom (cos|[.0,PI.]) = [.0,PI.] by Lm2,RELAT_1:91;
  thus arccos cos r = arccos(cos.r) by SIN_COS:def 23
    .= arccos.(cos.r) by Def4
    .= arccos.((cos|[.0,PI.]).r) by A1,FUNCT_1:72
    .= (id [.0,PI.]).r by A1,A2,Th91,FUNCT_1:23
    .= r by A1,FUNCT_1:35;
end;

theorem
  arccos (-1) = PI by Th94,SIN_COS:82;

theorem
  arccos 0 = PI/2 by Lm10,Th94,SIN_COS:82;

theorem
  arccos 1 = 0 by Th94,SIN_COS:34;

theorem
  -1 <= r & r <= 1 & arccos r = 0 implies r = 1 by Th93,SIN_COS:34;

theorem
  -1 <= r & r <= 1 & arccos r = PI/2 implies r = 0 by Th93,SIN_COS:82;

theorem
  -1 <= r & r <= 1 & arccos r = PI implies r = -1 by Th93,SIN_COS:82;

theorem Th101:
  -1 <= r & r <= 1 implies 0 <= arccos r & arccos r <= PI
proof
  assume -1 <= r & r <= 1;
  then r in [.-1,1.] by TOPREAL5:1;
  then arccos.r in rng arccos by Th88,FUNCT_1:def 5;
  then arccos r in rng arccos by Def4;
  hence thesis by Th87,TOPREAL5:1;
end;

theorem Th102:
  -1 < r & r < 1 implies 0 < arccos r & arccos r < PI
proof
  assume
A1: -1 < r & r < 1;
  then 0 <= arccos r & arccos r <= PI by Th101;
  then 0 < arccos r & arccos r < PI or
    0 = arccos r or arccos r = PI by REAL_1:def 5;
  hence thesis by A1,Th93,SIN_COS:34,82;
end;

theorem Th103:
  -1 <= r & r <= 1 implies arccos r = PI - arccos(-r)
proof
  assume -1 <= r & r <= 1;
  then
A1: --1 >= -r & -r >= -1 by REAL_1:50;
A2: r = (-1)*(-r)
     .= cos PI * cos arccos(-r) + sin PI * sin arccos(-r) by A1,Th93,SIN_COS:82
     .= cos(PI-arccos(-r)) by COMPLEX2:4;
  0+arccos(-r) <= PI by A1,Th101;
  then
A3: 0 <= PI-arccos(-r) by REAL_1:84;
  0 <= arccos(-r) by A1,Th101;
  then PI+0 <= PI+arccos(-r) by AXIOMS:24;
  then PI-arccos(-r) <= PI by REAL_1:86;
  hence thesis by A2,A3,Th94;
end;

theorem Th104:
  0 <= s & r^2 + s^2 = 1 implies sin arccos r = s
proof
  set x = arccos r;
  assume that
A1: 0 <= s and
A2: r^2 + s^2 = 1;
A3: -1 <= r & r <= 1 by A2,Lm7;
  (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
  then (sin.x)^2 = 1 - (cos.x)^2
    .= 1 - (cos x)^2 by SIN_COS:def 23
    .= 1 - r^2 by A3,Th93;
  then
A4: sin.x = s or sin.x = -s by A2,JGRAPH_3:1;
  0 <= x & x <= PI by A3,Th101;
  then
A5: x in [.0,PI.] by TOPREAL5:1;
  ---s < 0 or s = 0 by A1,REAL_1:66;
  hence sin x = s by A4,A5,COMPTRIG:24,SIN_COS:def 21;
end;

theorem
  s <= 0 & r^2 + s^2 = 1 implies sin arccos r = -s
proof
  set x = arccos r;
  assume that
A1: s <= 0 and
A2: r^2 + s^2 = 1;
A3: -1 <= r & r <= 1 by A2,Lm7;
  (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
  then (sin.x)^2 = 1 - (cos.x)^2
    .= 1 - (cos x)^2 by SIN_COS:def 23
    .= 1 - r^2 by A3,Th93;
  then
A4: sin.x = s or sin.x = -s by A2,JGRAPH_3:1;
  0 <= x & x <= PI by A3,Th101;
  then
A5: x in [.0,PI.] by TOPREAL5:1;
  0 > s or s = 0 by A1;
  hence sin x = -s by A4,A5,COMPTRIG:24,SIN_COS:def 21;
end;

theorem Th106:
  -1 <= r & r <= 1 implies sin arccos r = sqrt(1-r^2)
proof
  set s = sqrt(1-r^2);
  assume -1 <= r & r <= 1;
  then r^2+0 <= 1 by JGRAPH_2:7,SQUARE_1:59;
  then 0 <= 1-r^2 by REAL_1:84;
  then 0 <= s & r^2 + s^2 = r^2 + (1-r^2) by SQUARE_1:def 4;
  hence thesis by Th104;
end;

theorem
  arccos is_decreasing_on [.-1,1.]
proof
  set f = cos | [.0,PI.];
A1: f.:[.0,PI.] = [.-1,1.] by Th49,RFUNCT_2:5;
  f is_decreasing_on [.0,PI.] by COMPTRIG:41,RFUNCT_2:51;
  then (f|[.0,PI.])" is_decreasing_on f.:[.0,PI.] by FCONT_3:18;
  hence thesis by A1,Def3,RELAT_1:101;
end;

theorem
  arccos is_differentiable_on ].-1,1.[ &
  (-1 < r & r < 1 implies diff(arccos,r) = -1 / sqrt(1-r^2))
proof
  set f = cos|].0,PI.[;
  set h = cos|[.0,PI.];
  set g = arccos|].-1,1.[;
A1: f|].0,PI.[ = f by RELAT_1:101;
A2: f" = (h|].0,PI.[)" by Lm19,RELAT_1:103
      .= h"|(h.:].0,PI.[) by RFUNCT_2:40
      .= g by Def3,Lm19,Th50,EXTENS_1:1;
A3: dom (f") = ].-1,1.[ by A2,Lm15,Th88,RELAT_1:91;
A4: cos is_differentiable_on ].0,PI.[ by FDIFF_1:34,SIN_COS:72;
  then
A5: f is_differentiable_on ].0,PI.[ by FDIFF_2:16;
A6: now
    let x0 be Real such that
A7:   x0 in ].0,PI.[;
A8: diff(f,x0) = (f`|].0,PI.[).x0 by A5,A7,FDIFF_1:def 8
      .= (cos`|].0,PI.[).x0 by A4,FDIFF_2:16
      .= diff(cos,x0) by A4,A7,FDIFF_1:def 8
      .= -sin.x0 by SIN_COS:72;
    --sin.x0 > 0 by A7,COMPTRIG:23;
    hence 0 > diff(f,x0) by A8,REAL_1:66;
  end;
  then
A9: g is_differentiable_on ].-1,1.[ by A1,A2,A3,A5,FDIFF_2:48;
A10: ].-1,1.[ c= dom arccos by Th88,RCOMP_1:15;
    for x be Real st x in ].-1,1.[ holds
     g is_differentiable_in x by A9,FDIFF_1:16;
  hence
A12: arccos is_differentiable_on ].-1,1.[ by A10,FDIFF_1:def 7;
  set s = sqrt(1-r^2);
  set x = arccos.r;
  assume
A13: -1 < r & r < 1;
  then
A14: r in ].-1,1.[ by JORDAN6:45;
A15: g.r = x by A14,FUNCT_1:72;
A16: x = arccos r by Def4;
  then 0 < x & x < PI by A13,Th102;
  then
A17: x in ].0,PI.[ by JORDAN6:45;
A18: diff(f,x) = (f`|].0,PI.[).x by A5,A17,FDIFF_1:def 8
    .= (cos`|].0,PI.[).x by A4,FDIFF_2:16
    .= diff(cos,x) by A4,A17,FDIFF_1:def 8
    .= -sin.x by SIN_COS:72
    .= -sin arccos r by A16,SIN_COS:def 21
    .= -s by A13,Th106;
  thus diff(arccos,r) = (arccos`|].-1,1.[).r by A12,A14,FDIFF_1:def 8
    .= (g`|].-1,1.[).r by A12,FDIFF_2:16
    .= diff(g,r) by A9,A14,FDIFF_1:def 8
    .= 1 / -s by A1,A2,A3,A5,A6,A14,A15,A18,FDIFF_2:48
    .= (-1)/s by XCMPLX_1:193
    .= -1/s by XCMPLX_1:188;
end;

theorem
  arccos is_continuous_on [.-1,1.]
proof
  set f = cos | [.0,PI.];
A1: dom f = [.0,PI.] by Lm2,RELAT_1:91;
A2: f|[.0,PI.] = f by RELAT_1:102;
  f is_decreasing_on [.0,PI.] by COMPTRIG:41,RFUNCT_2:51;
  then (f|[.0,PI.])" is_continuous_on f.:[.0,PI.] by A1,FCONT_1:54;
  hence thesis by A2,Def3,COMPTRIG:50,RELAT_1:148;
end;

:: Correspondence between arcsin and arccos

theorem Th110:
  -1 <= r & r <= 1 implies arcsin r + arccos r = PI/2
proof
  assume
A1: -1 <= r & r <= 1;
  then -PI/2+PI/2 <= arccos r & arccos r <= PI/2+PI/2 by Th101;
  then -PI/2 <= arccos r-PI/2 & arccos r-PI/2 <= PI/2 by REAL_1:84,86;
  then
A2: --PI/2 >= -(arccos r-PI/2) & -(arccos r-PI/2) >= -PI/2 by REAL_1:50;
  r = sin(PI/2)*cos arccos r - cos(PI/2)*sin arccos r by A1,Th93,SIN_COS:82
   .= sin(PI/2-arccos r) by COMPLEX2:4;
  then arcsin r = PI/2 - arccos r by A2,Th70;
  hence thesis;
end;

theorem
  -1 <= r & r <= 1 implies arccos(-r) - arcsin r = PI/2
proof
  assume
A1: -1 <= r & r <= 1;
  then arcsin r + arccos r = PI/2 + 0 by Th110;
  then
A2: arccos r - 0 = PI/2 - arcsin r by XCMPLX_1:33;
  --1 >= -r & -r >= -1 by A1,REAL_1:50;
  then arccos(-r) = PI - arccos(--r) by Th103
    .= PI - (PI/2 - arcsin r) by A2
    .= arcsin r + PI/2;
  hence thesis;
end;

theorem
  -1 <= r & r <= 1 implies arccos r - arcsin(-r) = PI/2
proof
  assume
A1: -1 <= r & r <= 1;
  then arcsin r + arccos r = PI/2 + 0 by Th110;
  then
A2: arcsin r - 0 = PI/2 - arccos r by XCMPLX_1:33;
  --1 >= -r & -r >= -1 by A1,REAL_1:50;
  then arcsin(-r) = -arcsin(--r) by Th79
    .= -(PI/2 - arccos r) by A2
    .= arccos r - PI/2;
  hence thesis;
end;
  
