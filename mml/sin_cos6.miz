theorem Th77:
  -1 <= r & r <= 1 implies -PI/2 <= arcsin r & arcsin r <= PI/2
proof
  assume -1 <= r & r <= 1;
  then r in [.-1,1.] by XXREAL_1:1;
  then arcsin.r in rng arcsin by Th64,FUNCT_1:def 5;
  hence thesis by Th63,XXREAL_1:1;
end;

theorem Th78:
  -1 < r & r < 1 implies -PI/2 < arcsin r & arcsin r < PI/2
proof
  assume
A1: -1 < r & r < 1;
  then -PI/2 <= arcsin r & arcsin r <= PI/2 by Th77;
  then
  -PI/2 < arcsin r & arcsin r < PI/2 or -PI/2 = arcsin r or arcsin r = PI/
  2 by XXREAL_0:1;
  hence thesis by A1,Th7,Th69,SIN_COS:82;
end;

theorem Th79:
  -1 <= r & r <= 1 implies arcsin r = -arcsin(-r)
proof
  assume -1 <= r & r <= 1;
  then
A1: --1 >= -r & -r >= -1 by XREAL_1:26;
  then arcsin(-r) <= PI/2 by Th77;
  then
A2: -PI/2 <= -arcsin(-r) by XREAL_1:26;
  -PI/2 <= arcsin(-r) by A1,Th77;
  then
A3: -arcsin(-r) <= --PI/2 by XREAL_1:26;
  r = 0-1*(-r)
    .= sin 0 * cos arcsin(-r) - cos 0 * sin arcsin(-r) by A1,Th69,SIN_COS:34
    .= sin(0-arcsin(-r)) by COMPLEX2:4;
  hence thesis by A2,A3,Th70;
end;

theorem Th80:
  0 <= s & r^2 + s^2 = 1 implies cos arcsin r = s
proof
  set x = arcsin r;
  assume that
A1: 0 <= s and
A2: r^2 + s^2 = 1;
A3: -1 <= r & r <= 1 by A2,Lm5;
  then -PI/2 <= x & x <= PI/2 by Th77;
  then
A4: x in [.-PI/2,PI/2.] by XXREAL_1:1;
  (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
  then (cos.x)^2 = 1 - (sin.x)^2 .= 1 - (sin x)^2 by SIN_COS:def 21
    .= 1 - r^2 by A3,Th69;
  then
A5: cos.x = s or cos.x = -s by A2,SQUARE_1:109;
  ---s < 0 or s = 0 by A1;
  hence thesis by A5,A4,COMPTRIG:28,SIN_COS:def 23;
end;

theorem
  s <= 0 & r^2 + s^2 = 1 implies cos arcsin r = -s
proof
  set x = arcsin r;
  assume that
A1: s <= 0 and
A2: r^2 + s^2 = 1;
A3: -1 <= r & r <= 1 by A2,Lm5;
  then -PI/2 <= x & x <= PI/2 by Th77;
  then
A4: x in [.-PI/2,PI/2.] by XXREAL_1:1;
  (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
  then (cos.x)^2 = 1 - (sin.x)^2 .= 1 - (sin x)^2 by SIN_COS:def 21
    .= 1 - r^2 by A3,Th69;
  then
A5: cos.x = s or cos.x = -s by A2,SQUARE_1:109;
  0 > s or s = 0 by A1;
  hence thesis by A5,A4,COMPTRIG:28,SIN_COS:def 23;
end;

theorem Th82:
  -1 <= r & r <= 1 implies cos arcsin r = sqrt(1-r^2)
proof
  set s = sqrt(1-r^2);
  assume -1 <= r & r <= 1;
  then r^2+0 <= 1^2 by SQUARE_1:119;
  then 0 <= 1-r^2 by XREAL_1:21;
  then 0 <= s & r^2 + s^2 = r^2 + (1-r^2) by SQUARE_1:def 4;
  hence thesis by Th80;
end;

