theorem
  arcsin is_differentiable_on ].-1,1.[ & (-1 < r & r < 1 implies diff(
  arcsin,r) = 1 / sqrt(1-r^2))
proof
  set g = arcsin|].-1,1.[;
  set h = sin|[.-PI/2,PI/2.];
  set f = sin|].-PI/2,PI/2.[;
A1: dom f = ].-PI/2,PI/2.[ /\ REAL by RELAT_1:90,SIN_COS:27
    .= ].-PI/2,PI/2.[ by XBOOLE_1:28;
  set x = arcsin.r;
  set s = sqrt(1-r^2);
A2: ].-1,1.[ c= dom arcsin by Th64,XXREAL_1:25;
A3: sin is_differentiable_on ].-PI/2,PI/2.[ by FDIFF_1:34,SIN_COS:73;
  then
A4: f is_differentiable_on ].-PI/2,PI/2.[ by FDIFF_2:16;
A5: now
    let x0 be Real such that
A6: x0 in ].-PI/2,PI/2.[;
    diff(f,x0) = (f`|].-PI/2,PI/2.[).x0 by A4,A6,FDIFF_1:def 8
      .= (sin`|].-PI/2,PI/2.[).x0 by A3,FDIFF_2:16
      .= diff(sin,x0) by A3,A6,FDIFF_1:def 8
      .= cos.x0 by SIN_COS:73;
    hence 0 < diff(f,x0) by A6,COMPTRIG:27;
  end;
A7: f" = (h|].-PI/2,PI/2.[)" by RELAT_1:103,XXREAL_1:25
    .= h"|(h.:].-PI/2,PI/2.[) by RFUNCT_2:40
    .= g by Th46,RELAT_1:162,XXREAL_1:25;
  then
A8: f|].-PI/2,PI/2.[ = f & dom (f") = ].-1,1.[ by Th64,RELAT_1:91,101 ,
  XXREAL_1:25;
  then
A9: g is_differentiable_on ].-1,1.[ by A7,A4,A1,A5,FDIFF_2:48;
  then for x be Real st x in ].-1,1.[ holds g is_differentiable_in x by
  FDIFF_1:16;
  hence
A10: arcsin is_differentiable_on ].-1,1.[ by A2,FDIFF_1:def 7;
  assume
A11: -1 < r & r < 1;
  then
A12: r in ].-1,1.[ by XXREAL_1:4;
  then
A13: g.r = x by FUNCT_1:72;
  x = arcsin r;
  then -PI/2 < x & x < PI/2 by A11,Th78;
  then
A14: x in ].-PI/2,PI/2.[ by XXREAL_1:4;
  then
A15: diff(f,x) = (f`|].-PI/2,PI/2.[).x by A4,FDIFF_1:def 8
    .= (sin`|].-PI/2,PI/2.[).x by A3,FDIFF_2:16
    .= diff(sin,x) by A3,A14,FDIFF_1:def 8
    .= cos.x by SIN_COS:73
    .= cos arcsin r by SIN_COS:def 23
    .= s by A11,Th82;
  thus diff(arcsin,r) = (arcsin`|].-1,1.[).r by A10,A12,FDIFF_1:def 8
    .= (g`|].-1,1.[).r by A10,FDIFF_2:16
    .= diff(g,r) by A9,A12,FDIFF_1:def 8
    .= 1 / s by A7,A8,A4,A1,A5,A12,A13,A15,FDIFF_2:48;
end;

theorem
  arcsin|[.-1,1.] is continuous
proof
  set f = sin | [.-PI/2,PI/2.];
  dom f = [.-PI/2,PI/2.] by RELAT_1:91,SIN_COS:27;
  then f|[.-PI/2,PI/2.] = f & (f|[.-PI/2,PI/2.])"|(f.:[.-PI/2,PI/2.]) is
  continuous by COMPTRIG:39,FCONT_1:54,RELAT_1:102;
  hence thesis by COMPTRIG:48,RELAT_1:148;
end;

begin :: arccos

definition
  func arccos -> PartFunc of REAL, REAL equals
  (cos | [.0,PI.])";
  coherence;
end;

definition
  let r be set;
  func arccos r equals
  arccos.r;
  coherence;
end;

definition
  let r be set;
  redefine func arccos r -> Real;
  coherence;
end;

Lm16: arccos qua Function" = cos | [.0,PI.] by FUNCT_1:65;

canceled;

theorem Th87:
  rng arccos = [.0,PI.]
proof
  dom (cos|[.0,PI.]) = [.0,PI.] by RELAT_1:91,SIN_COS:27;
  hence thesis by FUNCT_1:55;
end;

registration
  cluster arccos -> one-to-one;
  coherence;
end;

theorem Th88:
  dom arccos = [.-1,1.] by COMPTRIG:50,FUNCT_1:55;

theorem Th89:
  (cos | [.0,PI.]) qua Function * arccos = id [.-1,1.] by COMPTRIG
  :50,FUNCT_1:61;

theorem
  arccos * (cos | [.0,PI.]) = id [.-1,1.] by COMPTRIG:50,FUNCT_1:61;

theorem Th91:
  (cos | [.0,PI.]) * arccos = id [.0,PI.] by Lm16,Th87,FUNCT_1:61;

theorem
  arccos qua Function * (cos | [.0,PI.]) = id [.0,PI.] by Lm16,Th87,
  FUNCT_1:61;

theorem Th93:
  -1 <= r & r <= 1 implies cos arccos r = r
proof
  assume -1 <= r & r <= 1;
  then
A1: r in [.-1,1.] by XXREAL_1:1;
  then
A2: arccos.r in [.0,PI.] by Th87,Th88,FUNCT_1:def 5;
  thus cos arccos r = cos.(arccos.r) by SIN_COS:def 23
    .= ((cos|[.0,PI.]) qua Function).(arccos.r) by A2,FUNCT_1:72
    .= (id [.-1,1.]).r by A1,Th88,Th89,FUNCT_1:23
    .= r by A1,FUNCT_1:35;
end;

theorem Th94:
  0 <= r & r <= PI implies arccos cos r = r
proof
A1: dom (cos|[.0,PI.]) = [.0,PI.] by RELAT_1:91,SIN_COS:27;
  assume 0 <= r & r <= PI;
  then
A2: r in [.0,PI.] by XXREAL_1:1;
  thus arccos cos r = arccos.(cos.r) by SIN_COS:def 23
    .= arccos.((cos|[.0,PI.]).r) by A2,FUNCT_1:72
    .= (id [.0,PI.]).r by A2,A1,Th91,FUNCT_1:23
    .= r by A2,FUNCT_1:35;
end;

theorem
  arccos (-1) = PI by Th94,SIN_COS:82;

theorem
  arccos 0 = PI/2 by Lm6,Th94,SIN_COS:82;

theorem
  arccos 1 = 0 by Th94,SIN_COS:34;

theorem
  -1 <= r & r <= 1 & arccos r = 0 implies r = 1 by Th93,SIN_COS:34;

theorem
  -1 <= r & r <= 1 & arccos r = PI/2 implies r = 0 by Th93,SIN_COS:82;

theorem
  -1 <= r & r <= 1 & arccos r = PI implies r = -1 by Th93,SIN_COS:82;

theorem Th101:
  -1 <= r & r <= 1 implies 0 <= arccos r & arccos r <= PI
proof
  assume -1 <= r & r <= 1;
  then r in [.-1,1.] by XXREAL_1:1;
  then arccos.r in rng arccos by Th88,FUNCT_1:def 5;
  hence thesis by Th87,XXREAL_1:1;
end;

theorem Th102:
  -1 < r & r < 1 implies 0 < arccos r & arccos r < PI
proof
  assume
A1: -1 < r & r < 1;
  then arccos r <= PI by Th101;
  then 0 < arccos r & arccos r < PI or 0 = arccos r or arccos r = PI by A1,
  Th101,XXREAL_0:1;
  hence thesis by A1,Th93,SIN_COS:34,82;
end;

theorem Th103:
  -1 <= r & r <= 1 implies arccos r = PI - arccos(-r)
proof
  assume -1 <= r & r <= 1;
  then
A1: --1 >= -r & -r >= -1 by XREAL_1:26;
  then 0+arccos(-r) <= PI by Th101;
  then
A2: 0 <= PI-arccos(-r) by XREAL_1:21;
  0 <= arccos(-r) by A1,Th101;
  then PI+0 <= PI+arccos(-r) by XREAL_1:8;
  then
A3: PI-arccos(-r) <= PI by XREAL_1:22;
  r = (-1)*(-r)
    .= cos PI * cos arccos(-r) + sin PI * sin arccos(-r) by A1,Th93,SIN_COS:
  82
    .= cos(PI-arccos(-r)) by COMPLEX2:4;
  hence thesis by A2,A3,Th94;
end;

theorem Th104:
  0 <= s & r^2 + s^2 = 1 implies sin arccos r = s
proof
  set x = arccos r;
  assume that
A1: 0 <= s and
A2: r^2 + s^2 = 1;
A3: -1 <= r & r <= 1 by A2,Lm5;
  then 0 <= x & x <= PI by Th101;
  then
A4: x in [.0,PI.] by XXREAL_1:1;
  (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
  then (sin.x)^2 = 1 - (cos.x)^2 .= 1 - (cos x)^2 by SIN_COS:def 23
    .= 1 - r^2 by A3,Th93;
  then
A5: sin.x = s or sin.x = -s by A2,SQUARE_1:109;
  ---s < 0 or s = 0 by A1;
  hence thesis by A5,A4,COMPTRIG:24,SIN_COS:def 21;
end;

theorem
  s <= 0 & r^2 + s^2 = 1 implies sin arccos r = -s
proof
  set x = arccos r;
  assume that
A1: s <= 0 and
A2: r^2 + s^2 = 1;
A3: -1 <= r & r <= 1 by A2,Lm5;
  then 0 <= x & x <= PI by Th101;
  then
A4: x in [.0,PI.] by XXREAL_1:1;
  (sin.x)^2 + (cos.x)^2 = 1 by SIN_COS:31;
  then (sin.x)^2 = 1 - (cos.x)^2 .= 1 - (cos x)^2 by SIN_COS:def 23
    .= 1 - r^2 by A3,Th93;
  then
A5: sin.x = s or sin.x = -s by A2,SQUARE_1:109;
  0 > s or s = 0 by A1;
  hence thesis by A5,A4,COMPTRIG:24,SIN_COS:def 21;
end;

theorem Th106:
  -1 <= r & r <= 1 implies sin arccos r = sqrt(1-r^2)
proof
  set s = sqrt(1-r^2);
  assume -1 <= r & r <= 1;
  then r^2+0 <= 1^2 by SQUARE_1:119;
  then 0 <= 1-r^2 by XREAL_1:21;
  then 0 <= s & r^2 + s^2 = r^2 + (1-r^2) by SQUARE_1:def 4;
  hence thesis by Th104;
end;

theorem
  arccos|[.-1,1.] is decreasing
proof
  set f = cos | [.0,PI.];
  f.:[.0,PI.] = [.-1,1.] & (f|[.0,PI.])"|(f.:[.0,PI.]) is decreasing by
  Th49, COMPTRIG:41,FCONT_3:18,RELAT_1:162;
  hence thesis by RELAT_1:101;
end;

theorem
  arccos is_differentiable_on ].-1,1.[ & (-1 < r & r < 1 implies diff(
  arccos,r) = -1 / sqrt(1-r^2))
proof
  set g = arccos|].-1,1.[;
  set h = cos|[.0,PI.];
  set f = cos|].0,PI.[;
A1: dom f = ].0,PI.[ /\ REAL by RELAT_1:90,SIN_COS:27
    .= ].0,PI.[ by XBOOLE_1:28;
  set x = arccos.r;
  set s = sqrt(1-r^2);
A2: ].-1,1.[ c= dom arccos by Th88,XXREAL_1:25;
A3: cos is_differentiable_on ].0,PI.[ by FDIFF_1:34,SIN_COS:72;
  then
A4: f is_differentiable_on ].0,PI.[ by FDIFF_2:16;
A5: now
    let x0 be Real such that
A6: x0 in ].0,PI.[;
A7: --sin.x0 > 0 by A6,COMPTRIG:23;
    diff(f,x0) = (f`|].0,PI.[).x0 by A4,A6,FDIFF_1:def 8
      .= (cos`|].0,PI.[).x0 by A3,FDIFF_2:16
      .= diff(cos,x0) by A3,A6,FDIFF_1:def 8
      .= -sin.x0 by SIN_COS:72;
    hence 0 > diff(f,x0) by A7;
  end;
A8: f" = (h|].0,PI.[)" by RELAT_1:103,XXREAL_1:25
    .= h"|(h.:].0,PI.[) by RFUNCT_2:40
    .= g by Th50,RELAT_1:162,XXREAL_1:25;
  then
A9: f|].0,PI.[ = f & dom (f") = ].-1,1.[ by
  Th88,RELAT_1:91,101,XXREAL_1:
  25;
  then
A10: g is_differentiable_on ].-1,1.[ by A8,A4,A1,A5,FDIFF_2:48;
  then for x be Real st x in ].-1,1.[ holds g is_differentiable_in x by
  FDIFF_1:16;
  hence
A11: arccos is_differentiable_on ].-1,1.[ by A2,FDIFF_1:def 7;
  assume
A12: -1 < r & r < 1;
  then
A13: r in ].-1,1.[ by XXREAL_1:4;
  then
A14: g.r = x by FUNCT_1:72;
  x = arccos r;
  then 0 < x & x < PI by A12,Th102;
  then
A15: x in ].0,PI.[ by XXREAL_1:4;
  then
A16: diff(f,x) = (f`|].0,PI.[).x by A4,FDIFF_1:def 8
    .= (cos`|].0,PI.[).x by A3,FDIFF_2:16
    .= diff(cos,x) by A3,A15,FDIFF_1:def 8
    .= -sin.x by SIN_COS:72
    .= -sin arccos r by SIN_COS:def 21
    .= -s by A12,Th106;
  thus diff(arccos,r) = (arccos`|].-1,1.[).r by A11,A13,FDIFF_1:def 8
    .= (g`|].-1,1.[).r by A11,FDIFF_2:16
    .= diff(g,r) by A10,A13,FDIFF_1:def 8
    .= 1 / -s by A8,A9,A4,A1,A5,A13,A14,A16,FDIFF_2:48
    .= (-1)/s by XCMPLX_1:193
    .= -1/s by XCMPLX_1:188;
end;

theorem
  arccos|[.-1,1.] is continuous
proof
  set f = cos | [.0,PI.];
  dom f = [.0,PI.] by RELAT_1:91,SIN_COS:27;
  then f|[.0,PI.] = f & (f|[.0,PI.])"|(f.:[.0,PI.]) is continuous by
  COMPTRIG:41,FCONT_1:54,RELAT_1:102;
  hence thesis by COMPTRIG:50,RELAT_1:148;
end;

:: Correspondence between arcsin and arccos

theorem Th110:
  -1 <= r & r <= 1 implies arcsin r + arccos r = PI/2
proof
  assume
A1: -1 <= r & r <= 1;
  then -PI/2+PI/2 <= arccos r by Th101;
  then -PI/2 <= arccos r-PI/2 by XREAL_1:21;
  then
A2: --PI/2 >= -(arccos r-PI/2) by XREAL_1:26;
  arccos r <= PI/2+PI/2 by A1,Th101;
  then arccos r-PI/2 <= PI/2 by XREAL_1:22;
  then
A3: -(arccos r-PI/2) >= -PI/2 by XREAL_1:26;
  r = sin(PI/2)*cos arccos r - cos(PI/2)*sin arccos r by A1,Th93,SIN_COS:
  82
    .= sin(PI/2-arccos r) by COMPLEX2:4;
  then arcsin r = PI/2 - arccos r by A2,A3,Th70;
  hence thesis;
end;

theorem
  -1 <= r & r <= 1 implies arccos(-r) - arcsin r = PI/2
proof
  assume
A1: -1 <= r & r <= 1;
  then
A2: arcsin r + arccos r = PI/2 + 0 by Th110;
  --1 >= -r & -r >= -1 by A1,XREAL_1:26;
  then arccos(-r) = PI - arccos(--r) by Th103
    .= arcsin r + PI/2 by A2;
  hence thesis;
end;

theorem
  -1 <= r & r <= 1 implies arccos r - arcsin(-r) = PI/2
proof
  assume
A1: -1 <= r & r <= 1;
  then
A2: arcsin r + arccos r = PI/2 + 0 by Th110;
  --1 >= -r & -r >= -1 by A1,XREAL_1:26;
  then arcsin(-r) = -arcsin(--r) by Th79
    .= arccos r - PI/2 by A2;
  hence thesis;
end;
