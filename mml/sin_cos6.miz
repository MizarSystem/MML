A1: PI < r and
A2: r <= 2*PI;
A3: cos r >= -1 by Th5;
  cos r <> -1 by A1,A2,Th29;
  hence thesis by A3,REAL_1:def 5;
end;

theorem
  2*PI*i <= r & r < PI/2+2*PI*i implies sin r < 1
proof
  assume that
A1: T(i) <= r and
A2: r < PI/2+T(i) and
A3: sin r >= 1;
A4: T(i)-T(i) <= r-T(i) & r-T(i) < PI/2+T(i)-T(i) by A1,A2,REAL_1:49,54;
A5: sin r <= 1 by Th4;
  sin(r-T(i)) = sin(r+T(-i))
    .= sin r by COMPLEX2:9
    .= 1 by A3,A5,AXIOMS:21;
  hence thesis by A4,Th30;
end;

