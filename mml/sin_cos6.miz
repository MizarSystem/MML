      then 0 < i & i < (1*PI)/(2*PI) by A9,REAL_1:73;
      then 0+1 <= i & i < 1/2 by INT_1:20,XCMPLX_1:92;
      hence contradiction by AXIOMS:22;
    end;
    now
      assume x = -1;
      then a = 3/2*PI+T(i) by A6,A7,Th23;
