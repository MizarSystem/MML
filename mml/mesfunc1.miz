    thus thesis;
   end;
   consider F being Function of NAT,S such that
A5:for n holds P[n,F.n] from FUNCT_2:sch 3(A4);
      A /\ eq_dom(f,+infty) = meet rng F by A5,Th27;
   hence thesis by Def11;
end;

