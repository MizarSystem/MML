then A5: A /\ great_dom(f,a) c= A\(A /\ less_eq_dom(f,a)) by TARSKI:def 3;
 for x being set st x in A\(A /\ less_eq_dom(f,a)) holds x in
  A /\ great_dom(f,a)
  proof
