theorem Th6:
for C being non empty set, f being PartFunc of C,ExtREAL, r being Real
st r <> 0 holds
for c being Element of C st c in dom(r(#)f) holds f.c = (r(#)f).c / R_EAL r
proof
   let C being non empty set;
   let f being PartFunc of C,ExtREAL;
   let r being Real;
   assume A1:r <> 0;
then A2:R_EAL r <> 0. by SUPINF_2:def 1;
    let c be Element of C;
    assume c in dom(r(#)f);
then A3: (r(#)f).c = (R_EAL r) * f.c by Def6;
       now per cases;
     suppose A4:f.c = +infty;
        now per cases by A2,XXREAL_0:1;
      suppose A5:0. < R_EAL r;
      then (r(#)f).c = +infty by A3,A4,EXTREAL1:def 1;
      hence thesis by A4,A5,EXTREAL1:def 2;
      end;
      suppose A6:R_EAL r < 0.;
      then (r(#)f).c = -infty by A3,A4,EXTREAL1:def 1;
      hence thesis by A4,A6,EXTREAL1:def 2;
     end;
     end;
     hence thesis;
     end;
     suppose A7:f.c = -infty;
        now per cases by A2,XXREAL_0:1;
      suppose A8:0. < R_EAL r;
      then (r(#)f).c = -infty by A3,A7,EXTREAL1:def 1;
      hence thesis by A7,A8,EXTREAL1:def 2;
      end;
      suppose A9:R_EAL r < 0.;
      then (r(#)f).c = +infty by A3,A7,EXTREAL1:def 1;
      hence thesis by A7,A9,EXTREAL1:def 2;
     end;
     end;
     hence thesis;
     end;
     suppose f.c <> +infty & f.c <> -infty;
     then reconsider a = f.c as Real by XXREAL_0:14;
        (r(#)f).c = r * a by A3,EXTREAL1:13;
     then (r(#)f).c / R_EAL r = r*a/r by A1,EXTREAL1:32,SUPINF_2:def 1
     .= a/(r/r) by XCMPLX_1:78 .= a / 1 by A1,XCMPLX_1:60;
     hence thesis;
    end;
    end;
    hence thesis;
end;

