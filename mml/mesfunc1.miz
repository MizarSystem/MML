     end;
     then not(x in (A /\ less_dom(f,a))) by XBOOLE_0:def 3;
     hence thesis by A4,XBOOLE_0:def 4;
    end;
    hence thesis by TARSKI:def 3;
   end;
      A\(A /\ less_dom(f,a)) c= A /\ great_eq_dom(f,a)
