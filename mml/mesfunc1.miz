      NAT_1;
 notations TARSKI, XBOOLE_0, ZFMISC_1, SUBSET_1, ORDINAL1, NUMBERS, XCMPLX_0,
      XREAL_0, MEASURE6, REAL_1, RELAT_1, FUNCT_1, PARTFUN1, FUNCT_2, NAT_1,
      INT_1, RAT_1, FINSET_1, CARD_1, CARD_4, XXREAL_0, PROB_1, SUPINF_1,
      SUPINF_2, MEASURE1, MEASURE2, MEASURE3, EXTREAL1;
 constructors WELLORD2, REAL_1, NAT_1, RAT_1, MEASURE3, MEASURE6, EXTREAL1;
