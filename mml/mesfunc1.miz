theorem Th11:
for r being Real holds ex n being Element of NAT st r <= n
proof
   let r being Real;
A1:r <= [/ r \] by INT_1:def 5;
   per cases;
    suppose [/ r \] >= 0;
    then reconsider n=[/ r \] as Element of NAT by INT_1:16;
    take n;
    thus thesis by INT_1:def 5;
    end;
    suppose A2: [/ r \] < 0;
    take 0;
    thus thesis by A1,A2;
end;
end;

theorem Th12:
for r being Real holds ex n being Element of NAT st -n <= r
proof
   let r being Real;
A1:[\ r /] <= r by INT_1:def 4;
      [\ r /] is Element of INT by INT_1:def 2;
   then consider k being Element of NAT such that
A2:[\ r /] = k or [\ r /] = -k by INT_1:def 1;
   per cases;
    suppose [\ r /] < 0;
    hence thesis by A1,A2;
    end;
    suppose A3: [\ r /] >= 0;
    take 0;
    thus thesis by A3,INT_1:def 4;
end;
end;

theorem Th13:
for r,s being real number st r < s holds
ex n being Element of NAT st 1/(n+1) < s-r
proof
   let r,s be real number;
   assume r < s;
   then s-r > 0 by XREAL_1:52;
   then consider t being real number such that
A1:0 < t & t < s-r by XREAL_1:7;
   reconsider t as Real by XREAL_0:def 1;
A2:1/t > 0 by A1,REAL_2:127;
A3:[/ 1/t \] + 1 > 1/t by INT_1:57;
   set n = [/ 1/t \];
   reconsider i0=0 as Integer;
      n + 1 > i0 by A2,A3;
   then n >= i0 by INT_1:20;
   then reconsider n as Element of NAT by INT_1:16;
      (n+1)*t >= 1 by A1,A3,REAL_2:178;
   then 1/(n+1) <= t by REAL_2:177;
then 1/(n+1) < s-r by A1,XXREAL_0:2;
   hence thesis;
end;

theorem Th14:
for r,s being real number st for n being Element of NAT
 holds r-1/(n+1) <= s holds r <= s
proof
   let r,s be real number;
   assume A1:for n being Element of NAT holds r-1/(n+1) <= s;
   assume r > s;
   then consider n being Element of NAT such that
A2:1/(n+1) < r - s by Th13;
      s + 1/(n+1) < r by A2,XREAL_1:22;
   then s < r - 1/(n+1) by XREAL_1:22;
   hence contradiction by A1;
end;

theorem Th15:
for a being R_eal st (for r being Real holds R_EAL r < a) holds a = +infty
proof
   let a being R_eal;
   assume A1:for r being Real holds R_EAL r < a;
   assume A2:not a = +infty;
      a <= +infty by XXREAL_0:4;
   then a < +infty by A2,XXREAL_0:1;
   then consider b being R_eal such that
A3:a < b & b < +infty & b in REAL by MEASURE5:17;
   reconsider b as Real by A3;
  a <= R_EAL b by A3;
   hence contradiction by A1;
end;

theorem Th16:
for a being R_eal st (for r being Real holds a < R_EAL r) holds a = -infty
proof
   let a being R_eal;
   assume A1:for r being Real holds a < R_EAL r;
   assume A2:not a = -infty;
      -infty <= a by XXREAL_0:5;
   then -infty < a by A2,XXREAL_0:1;
   then consider b being R_eal such that
A3:-infty < b & b < a & b in REAL by MEASURE5:17;
   reconsider b as Real by A3;
  R_EAL b <= a by A3;
   hence contradiction by A1;
end;

