    end;
    assume x is_>=_than proj1 D & y is_>=_than proj2 D;
then A4: [x,y] is_>=_than [:D1,D2:] by Th30;
      the carrier of [:S1,S2:] = [:the carrier of S1, the carrier of S2:]
      by Def2;
    then D c= [:D1,D2:] by Th1;
    hence [x,y] is_>=_than D by A4,YELLOW_0:9;
  end;

