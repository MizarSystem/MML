      RELAT_1, RLVECT_1, ARYTM_1, BINOP_1, FUNCSDOM, LATTICES, GROUP_1,
      FILTER_2, IDEAL_1, FILTER_0, SUBSET_1, VECTSP_2, GROUP_4, COLLSP,
      WAYBEL20, PARTFUN1, ORDERS_2, WELLORD2, WELLORD1, TARSKI;
 notations TARSKI, XBOOLE_0, SUBSET_1, SETWISEO, RELAT_1, RELSET_1, FUNCT_1,
      PARTFUN1, ALG_1, RELAT_2, EQREL_1, WELLORD1, WELLORD2, ORDERS_1, BINOP_1,
      DOMAIN_1, STRUCT_0, PRE_TOPC, GROUP_1, RLVECT_1, VECTSP_1, VECTSP_2,
      FUNCSDOM, IDEAL_1;
 constructors WELLORD1, WELLORD2, DOMAIN_1, BINOP_1, SETWISEO, ORDERS_1,
      EQREL_1, PRE_TOPC, GCD_1, IDEAL_1;
 registrations RELSET_1, ALGSTR_1, GCD_1, STRUCT_0, IDEAL_1, PRE_TOPC,
      PARTFUN1, EQREL_1, GROUP_1, FINSET_1, FINSUB_1, ORDINAL1, QUOFIELD;
 requirements BOOLE, SUBSET;
