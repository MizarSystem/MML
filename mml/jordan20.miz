           D6: p44`1>p`1 by C1,A1;
           now per cases;
           case p44<>p;then
             LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;then
             Segment(P,p1,p2,p44,p)=LSeg(p44,p) by JJ338,C2,D5,A2b;
            hence p5`1>=e by A1,Th37,D4,D6;
           end;
           case p44=p;
            hence p5`1>=e by D6;
           end;
           end;
          hence p5`1>=e;
         end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by JJ110,A2b,A1,C1,C2;
     end;
     case k<>0;then k>0 by NAT_1:19;then
        C99: k>=0+1 by NAT_1:38;
      set p44=f/.i;
        C799: i>i-'k by A79,C99,JORDAN3:14;then
        C800: i>1 by A79,AXIOMS:22;
        C801: i>=i-'k+1 by NAT_1:38,C799;
        now assume F77: (f/.i)`1<>e;
         f.i=f/.i by FINSEQ_4:def 4,A10;then
         for p9 being Point of TOP-REAL 2 st p9=f.(i-'0)
            holds p9`1<>e by F77,JORDAN3:2;then
          G66: k<=0 by A9;
          k=0 by G66,NAT_1:18;then
          i-'k=i by JORDAN3:2;
         hence contradiction by C799;
        end;then
        C1: p44`1=e;
        D7: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A6;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
        C78: len g=len f -'i+1 by JORDAN3:27,A6b,A6,A2e;
        C77: len f-i>0 by A6b,SQUARE_1:11;then
        len f-'i=len f-i by BINARITH:def 3;then
        len f-'i+1>0+1 by C77,REAL_1:67;then
        C72: 1<len g by C78;then
        C73: 1+1<=len g by NAT_1:38;
        1+(1+i)<=1+len f by A6,REAL_1:55;then
        1+1+i<=len f+1 by XCMPLX_1:1;then
        1+1+i-i<=len f+1-i by REAL_1:49;then
        C88: 1+1<=len f+1-i by XCMPLX_1:26;then
        C76: 1<=i & i<=len f & len f<=len f & 1<=1 &
         1<=len f+1-i by A6,A6b,AXIOMS:22;
        C76b: 1<=i & i<=len f & len f<=len f & 1<=1+1 &
         1+1<=len f+1-i by C88,A6,A6b;
        1 in Seg len g by C72,FINSEQ_1:3;then
        1 in dom g by FINSEQ_1:def 3;then
        C74: g/.1=g.1 by FINSEQ_4:def 4
                .=f.(1-1+i) by C76,JORDAN3:31
                .=f/.i by FINSEQ_4:def 4,A10;
        reconsider ia=i+1 as Nat;
        ia in Seg len f by A6,A11b,FINSEQ_1:3;then
        C100: i+1 in dom f by FINSEQ_1:def 3;
        1+1 in Seg len g by C73,FINSEQ_1:3;then
        1+1 in dom g by FINSEQ_1:def 3;then
        C75: g/.(1+1)=g.(1+1) by FINSEQ_4:def 4
                    .=f.(1+1-1+i) by C76b,JORDAN3:31
                    .=f/.(i+1) by C100,FINSEQ_4:def 4;
        LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A6
                .=LSeg(g,1) by TOPREAL1:def 5,C73,C74,C75;then
        p in Y &
          Y in { LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
                           by A5,A6,C73;then
        p in union{ LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
            by TARSKI:def 4;then
        p in L~mid(f,i,len f) by TOPREAL1:def 6;then
        C2: LE p44,p,P,p1,p2 by SPRECT_4:4,A2,A6,A6b;
        P1: for p5 being Point of TOP-REAL 2 st
         LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e
         proof let p5 be Point of TOP-REAL 2;
          assume D1: LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2;
           Segment(P,p1,p2,p44,p)={p8 where p8 is Point of TOP-REAL 2:
            LE p44,p8,P,p1,p2 & LE p8,p,P,p1,p2} by JORDAN6:29;then
           D4: p5 in Segment(P,p1,p2,p44,p) by D1;
           D5: p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
           D6: p in LSeg(f,i) by A5,A6;
           D10: LSeg(p44,p) c= LSeg(f,i) by D5,D6,D7,TOPREAL1:12;
           D5: LSeg(p44,p) c= P by D10,XBOOLE_1:1,A2,A6,A13;
           D6: p44`1=p`1 by C1,A1;
           now per cases;
           case p44<>p;then
             LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;then
             Segment(P,p1,p2,p44,p)=LSeg(p44,p) by JJ338,C2,D5,A2b;
            hence p5`1>=e by A1,Th37,D4,D6;
           end;
           case p44=p;then
             Segment(P,p1,p2,p44,p)={p44} by A2b,JJ20,A1;
            hence p5`1>=e by C1,D4,TARSKI:def 1;
           end;
           end;
          hence p5`1>=e;
         end;
        reconsider g=mid(f,1,i) as FinSequence of (TOP-REAL 2);
        C78: len g=i -'1+1 by A6,A6g,A6b,JORDAN3:27;
        i-1>0 by C800,SQUARE_1:11;then
        i-'1=i-1 by BINARITH:def 3;then
        C56: i-'1+1=i by XCMPLX_1:27;
        C261: i-'k <i by C799;then
        C57: i-'k+1<=len g by C56,C78,NAT_1:38;
        C258: 1<=(i-'k) by A79;
        C76b: 1<=1 & 1<=i & i<=len f & 1<=i-'k &
        i-'k<=i+1-1 by C258,C261,A6,A6b,XCMPLX_1:26;
        C76d: i-'k+1<=i by C801;
        reconsider ia=i+1 as Nat;
        i-'k in Seg len g by C258,C261,C78,C56,FINSEQ_1:3;then
        i-'k in dom g by FINSEQ_1:def 3;then
        C75: g/.(i-'k)=g.(i-'k) by FINSEQ_4:def 4
                    .=f.(i-'k-1+1) by C76b,JORDAN3:31
                    .=f.(i-'k) by XCMPLX_1:27
                    .=f/.(i-'k) by A167;
         LSeg(g,i-'k)=LSeg(g/.(i-'k),g/.(i-'k+1))
             by TOPREAL1:def 5,C57,C258;then
         f/.(i-'k) in LSeg(g,i-'k) by C75,TOPREAL1:6;then
         pk in LSeg(g,i-'k) &
          LSeg(g,i-'k) in
          { LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
                           by A167,C258,C76d,C78,C56;then
         pk in union{ LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
            by TARSKI:def 4;then
         pk in L~mid(f,1,i) by TOPREAL1:def 6;then
         A710: LE pk,p44,P,p1,p2 by C800,A6b,A2,SPRECT_3:33;then
         A711: LE pk,p,P,p1,p2 by C2,JORDAN5C:13;
         A798: p44 in P by A710,JORDAN5C:def 3;
         P2: now assume not
           for p51 being Point of TOP-REAL 2 st
            LE pk,p51,P,p1,p2 & LE p51,p44,P,p1,p2 holds p51`1>=e;then
           consider p51 being Point of TOP-REAL 2 such that
           D33: LE pk,p51,P,p1,p2 & LE p51,p44,P,p1,p2 & p51`1<e;
           p51 in P by D33,JORDAN5C:def 3;then
           consider Y3 being set such that
           A5e: p51 in Y3 & Y3 in
            { LSeg(f,i5) where i5 is Nat : 1 <= i5 & i5+1 <= len f }
                     by TARSKI:def 4,A12,A2;
           consider kk being Nat such that
           A6e: Y3=LSeg(f,kk) &  (1 <= kk & kk+1 <= len f) by A5e;
           D34: 1<=kk & kk+1<=len f & p51 in LSeg(f,kk) by A5e,A6e;
           D66: kk<len f by D34,NAT_1:38;then
           kk in Seg len f by D34,FINSEQ_1:3;then
           D788: kk in dom f by FINSEQ_1:def 3;
           D790: 1<kk+1 by D34,NAT_1:38;then
           kk+1 in Seg len f by D34,FINSEQ_1:3;then
           D789: kk+1 in dom f by FINSEQ_1:def 3;
           D35: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by TOPREAL1:def 5,D34;then
           D37: (f/.kk)`1<e or (f/.(kk+1))`1<e by D34,D33,JJ40;
           D38: LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                     by A2,A5e,A6e,JORDAN5C:26;
           now per cases by D37;
           case E0: (f/.kk)`1<e;
              set k2=i-'kk;
                LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A2,D34,JORDAN5C:25;then
                E44: LE f/.kk,p44,L~f,f/.1,f/.(len f)
                                 by D33,A2,JORDAN5C:13;
               now assume i-kk<=0;then i-kk+kk<=0+kk by REAL_1:55;then
                kk>=i by XCMPLX_1:27;then
                LE f/.i,f/.kk,L~f,f/.1,f/.(len f)
                         by A2,C800,D66,JORDAN5C:24;
               hence contradiction by E0,C1,E44,JORDAN5C:12,A2b,A2;
              end;then
              F9: i-'kk=i-kk by BINARITH:def 3;
              F13: i-k2=i-(i-kk) by F9 .=kk by XCMPLX_1:18;then
              F18: i-k2>0 by D34,AXIOMS:22;then
              F12: i-'k2>0 by BINARITH:def 3;
              F14: i-k2=i-'k2 by F18,BINARITH:def 3;
              F15: i-'k2>=0+1 by F12,NAT_1:38;
              i-'k2<=len f by A6b,JORDAN3:13;then
              P[k2] by F14,F13,E0,FINSEQ_4:24,F15;then
              k2>=k by A9;then
              i-k2<=i-k by REAL_2:106;then
              D90: LE f/.(i-'k2),f/.(i-'k),L~f,f/.1,f/.(len f)
                           by JORDAN5C:24,A78,F13,A6e,A2, F14,A122;
              D200: LE f/.(i-'k),p51,L~f,f/.1,f/.(len f) by A167,D33,A2;then
              D202: LE f/.(i-'k),f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by D38,JORDAN5C:13;then
              D203: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by D90,F13,F14,JORDAN5C:13;
              Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;then
              D99: f/.(i-'k) in Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                                            by D90,D202,F13,F14;
              D100: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                G0: z in LSeg(f/.kk,f/.(kk+1));
                LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A6e,D35;
               hence z in L~f by G0,A12,TARSKI:def 4;
              end;
              D101: f is special by A2,TOPREAL1:def 10;
              G20: f is one-to-one by A2,TOPREAL1:def 10;
              kk<kk+1 by NAT_1:38;then
              G23: f.kk <> f.(kk+1) by FUNCT_1:def 8,G20,D788,D789;
              G24: f/.kk=f.kk by FINSEQ_4:def 4,D788;
              f/.(kk+1)=f.(kk+1) by FINSEQ_4:def 4,D789;then
              LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by G23,G24,TOPREAL1:15;then
              D102: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by D203,D100,A2,A2b,JJ338;
              (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A6e,TOPREAL1:def 7,D101;then
              D199: (f/.kk)`2=(f/.(kk+1))`2 by D99,D102,E0,JJ50,A167,B1;
              (f/.(kk+1))`1>e by E0,D99,D102,JJ30,B1,A167;then
              (f/.kk)`1< (f/.(kk+1))`1 by E0,AXIOMS:22;then
              (f/.(i-'k))`1<=p51`1
                     by JJ80b,D34,D66,D99,D102,D200,A2,D35,D199;
            hence contradiction by D33,B1,A167,AXIOMS:22;
           end;
           case E0: (f/.(kk+1))`1 <e & (f/.kk)`1>=e;then
              G77: (f/.kk)`1>(f/.(kk+1))`1 by AXIOMS:22;
              set k2=i-'kk-'1;
              D100: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                G0: z in LSeg(f/.kk,f/.(kk+1));
                LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A6e,D35;
               hence z in L~f by G0,A12,TARSKI:def 4;
              end;
              D400: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              D203: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                          by JORDAN5C:23,D34,A2;
              G20: f is one-to-one by A2,TOPREAL1:def 10;
              kk<kk+1 by NAT_1:38;then
              G23: f.kk <> f.(kk+1) by FUNCT_1:def 8,G20,D788,D789;
              G24: f/.kk=f.kk by FINSEQ_4:def 4,D788;
              f/.(kk+1)=f.(kk+1) by FINSEQ_4:def 4,D789;then
              LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by G23,G24,TOPREAL1:15;then
              D102: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by D203,D100,A2,A2b,JJ338;
                E4: p51 in LSeg(f/.kk,f/.(kk+1)) by D35,A6e,A5e;
                LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A2,D34,JORDAN5C:25;then
                E44: LE f/.kk,p44,L~f,f/.1,f/.(len f)
                                 by D33,A2,JORDAN5C:13;
              now assume
               i-kk-1<=0;then
               i-(kk+1)<=0 by XCMPLX_1:36;then
               i-(kk+1)+(kk+1)<=0+(kk+1) by REAL_1:55;then
               kk+1>=i by XCMPLX_1:27;then
                LE f/.i,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A2,D34,C800,JORDAN5C:24;then
                E9: f/.i in LSeg(f/.kk,f/.(kk+1)) by D102,D400,E44;
                E30: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by TOPREAL1:def 5,A6e;
                D101: f is special by A2,TOPREAL1:def 10;
                  (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A6e,TOPREAL1:def 7,D101;
                 hence contradiction
                       by D33,C1,E30,A2,G77,E9,E4,JJ80,A6e,D66;
              end;then
              i-kk-1+1>=0+1 by REAL_1:55;then
              i-kk>=1 by XCMPLX_1:27;then
              F9b: i-kk>0 by AXIOMS:22;then
              F9: i-'kk=i-kk by BINARITH:def 3;then
              F83: i-'kk-1=i-kk-1;
              i-'kk>=0+1 by F9b,F9,NAT_1:38;then
              i-'kk-1>=0 by SQUARE_1:12;then
              F84: i-'kk-1=i-'kk-'1 by BINARITH:def 3;
              F13: i-k2=i-(i-kk-1) by F84,F83 .=i-(i-kk)+1 by XCMPLX_1:37
                      .=i-i+kk+1 by XCMPLX_1:37
                      .=0+kk+1 by XCMPLX_1:14
                      .=kk+1;then
              F18: i-k2>0 by D790,AXIOMS:22;then
              F12: i-'k2>0 by BINARITH:def 3;
              F14: i-k2=i-'k2 by F18,BINARITH:def 3;
              F15: i-'k2>=0+1 by F12,NAT_1:38;
              i-'k2<=len f by A6b,JORDAN3:13;then
              P[k2] by F14,F13,E0,FINSEQ_4:24,F15;then
              k2>=k by A9;then
              i-k2<=i-k by REAL_2:106;then
              D202b: LE f/.(kk+1),f/.(i-'k),L~f,f/.1,f/.(len f)
                           by F13,F14,JORDAN5C:24,A78,F15,A2,A122;
              LE f/.(i-'k),f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by D38,A2,JORDAN5C:13,A167,D33;
               hence contradiction by E0,B1,A167,D202b,JORDAN5C:12,A2,A2b;
              end;
              end;
            hence contradiction;
         end;
         for p5 being Point of TOP-REAL 2 st
            LE pk,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e
          proof let p5 be Point of TOP-REAL 2;
            assume F1: LE pk,p5,P,p1,p2 & LE p5,p,P,p1,p2;then
             p5 in P by JORDAN5C:def 3;then
             F3: LE p5,p44,P,p1,p2 or LE p44,p5,P,p1,p2 by A2b,JJ319,A798;
             now per cases by F3;
             case LE p5,p44,P,p1,p2;
              hence p5`1>=e by F1,P2;
             end;
             case LE p44,p5,P,p1,p2;
              hence p5`1>=e by F1,P1;
             end;
             end;
           hence p5`1>=e;
          end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A711,B1,JJ110,A2b,A1;
     end;
     end;
    hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e;
   end;
   end;
  hence thesis;
end;

theorem for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
   then
   A2b: P is_an_arc_of p1,p2 by TOPREAL4:3;
   consider f being FinSequence of (TOP-REAL 2) such that
   A2:  f is_S-Seq & P = L~f & p1=f/.1 & p2=f/.len f by TOPREAL4:def 1,A1;
   A12: L~f = union { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
                          by TOPREAL1:def 6;
   consider Y being set such that
   A5: p in Y & Y in
      { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
            by TARSKI:def 4,A12,A2,A1;
   A13: Y c= L~f
   proof let x be set;assume x in Y;
    hence x in L~f by A12,A5,TARSKI:def 4;
   end;
   consider i being Nat such that
   A6: Y=LSeg(f,i) &  (1 <= i & i+1 <= len f) by A5;
   A90: i<i+1 by NAT_1:38;then
   A90e: 1<i+1 by A6,AXIOMS:22;
   A6b: i<len f by AXIOMS:22,A6,A90;
   i+1 in Seg len f by A6,A90e,FINSEQ_1:3;then
   A10: i+1 in dom f by FINSEQ_1:def 3;
   A6g: 1<len f by A6,A6b,AXIOMS:22;
   A11b: 1<i+1 by A90,A6,AXIOMS:22;
   defpred P[Nat] means
    for p being Point of TOP-REAL 2 st p=f.(i+1+$1)
        holds p`1<>e;
   A6h: len f - (i+1)>=0 by A6,SQUARE_1:12;
   len f in Seg len f by A6g,FINSEQ_1:3;then
   len f in dom f by FINSEQ_1:def 3;then
   A85: f/.(len f)=f.(len f) by FINSEQ_4:def 4;
   i+1+((len f)-'(i+1))=i+1+(len f -(i+1)) by A6h,BINARITH:def 3
                      .=len f by XCMPLX_1:27;then
   A200: P[(len f)-'(i+1)] by A2,A85,A1;then
   A8: ex k being Nat st P[k];
   ex k being Nat st P[k] & for n being Nat st P[n] holds k <= n
                                  from NAT_1:sch 5(A8);then
   consider k being Nat such that
   A9: P[k] & for n being Nat st P[n] holds k <= n;
   A9e: k<=len f -'(i+1) by A9,A200;
   len f -'(i+1)=len f -(i+1) by BINARITH:def 3,A6h;then
