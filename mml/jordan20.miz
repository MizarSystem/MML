theorem JJ20: for P being Subset of TOP-REAL 2,
p1,p2,p being Point of TOP-REAL 2
st P is_an_arc_of p1,p2 & p in P holds Segment(P,p1,p2,p,p)={p}
proof let P be Subset of TOP-REAL 2,p1,p2,p be Point of TOP-REAL 2;
 assume A1: P is_an_arc_of p1,p2 & p in P;
  A2: Segment(P,p1,p2,p,p)={q where q is Point of TOP-REAL 2:
  LE p,q,P,p1,p2 & LE q,p,P,p1,p2} by JORDAN6:29;
  A3: Segment(P,p1,p2,p,p)c= {p}
  proof let x be set;assume x in Segment(P,p1,p2,p,p);then
    consider q being Point of TOP-REAL 2 such that
    B1: x=q & (LE p,q,P,p1,p2 & LE q,p,P,p1,p2) by A2;
    p=q by B1,A1,JORDAN5C:12;
   hence x in {p} by B1,TARSKI:def 1;
  end;
  {p} c= Segment(P,p1,p2,p,p)
  proof let x be set;assume x in {p};then
    B2: x=p by TARSKI:def 1;
    LE p,p,P,p1,p2 by A1,JORDAN5C:9;
   hence x in Segment(P,p1,p2,p,p) by A2,B2;
  end;
 hence Segment(P,p1,p2,p,p)={p} by A3,XBOOLE_0:def 10;
end;

theorem JJ30: for p1,p2,p being Point of TOP-REAL 2,a being Real
st p in LSeg(p1,p2) & p1`1<=a & p2`1<=a holds p`1<=a
proof let p1,p2,p be Point of TOP-REAL 2,a be Real;
 assume A0: p in LSeg(p1,p2) & p1`1<=a & p2`1<=a;
 then consider r being Real such that
A3:      0<=r & r<=1 and
A1: p = (1-r)*p1+r*p2 by SPPOL_1:21;
A2:  p`1 = ((1-r)*p1)`1+(r*p2)`1 by A1,TOPREAL3:7
      .= ((1-r)*p1)`1+r*p2`1 by TOPREAL3:9
      .= (1-r)*p1`1+r*p2`1 by TOPREAL3:9;
  1-r>=0 by A3,SQUARE_1:12;then
  A5: (1-r)*p1`1<=(1-r)*a by A0,AXIOMS:25;
  r*p2`1<=r*a by A0,A3,AXIOMS:25;then
  A6: p`1<=(1-r)*a+r*a by A2,A5,REAL_1:55;
  (1-r)*a+r*a=((1-r)+r)*a by XCMPLX_1:8
           .= 1*a by XCMPLX_1:27
           .= a;
 hence p`1<=a by A6;
end;

theorem JJ40: for p1,p2,p being Point of TOP-REAL 2,a being Real
st p in LSeg(p1,p2) & p1`1>=a & p2`1>=a holds p`1>=a
proof let p1,p2,p be Point of TOP-REAL 2,a be Real;
 assume A0: p in LSeg(p1,p2) & p1`1>=a & p2`1>=a;
 then consider r being Real such that
A3:      0<=r & r<=1 and
A1: p = (1-r)*p1+r*p2 by SPPOL_1:21;
A2:  p`1 = ((1-r)*p1)`1+(r*p2)`1 by A1,TOPREAL3:7
      .= ((1-r)*p1)`1+r*p2`1 by TOPREAL3:9
      .= (1-r)*p1`1+r*p2`1 by TOPREAL3:9;
  1-r>=0 by A3,SQUARE_1:12;then
  A5: (1-r)*p1`1>=(1-r)*a by A0,AXIOMS:25;
  r*p2`1>=r*a by A0,A3,AXIOMS:25;then
  A6: p`1>=(1-r)*a+r*a by A2,A5,REAL_1:55;
  (1-r)*a+r*a=((1-r)+r)*a by XCMPLX_1:8
           .= 1*a by XCMPLX_1:27
           .= a;
 hence p`1>=a by A6;
end;

theorem JJ50: for p1,p2,p being Point of TOP-REAL 2,a being Real
st p in LSeg(p1,p2) & p1`1<a & p2`1<a holds p`1<a
proof let p1,p2,p be Point of TOP-REAL 2,a be Real;
 assume A0: p in LSeg(p1,p2) & p1`1<a & p2`1<a;
 then consider r being Real such that
A3:      0<=r & r<=1 and
A1: p = (1-r)*p1+r*p2 by SPPOL_1:21;
A2:  p`1 = ((1-r)*p1)`1+(r*p2)`1 by A1,TOPREAL3:7
      .= ((1-r)*p1)`1+r*p2`1 by TOPREAL3:9
      .= (1-r)*p1`1+r*p2`1 by TOPREAL3:9;
  per cases;
  suppose 0=r;then p=p1+(0)*p2 by A1,EUCLID:33.=p1+0.REAL 2 by EUCLID:33
    .=p1 by EUCLID:31;
   hence p`1<a by A0;
  end;
  suppose 0<>r;then
    A10: r>0 by A3;
  1-r>=0 by A3,SQUARE_1:12;then
  A5: (1-r)*p1`1<=(1-r)*a by A0,AXIOMS:25;
  r*p2`1<r*a by A10,A0,REAL_1:70;then
  A6: p`1<(1-r)*a+r*a by A2,A5,REAL_1:67;
  (1-r)*a+r*a=((1-r)+r)*a by XCMPLX_1:8
           .= 1*a by XCMPLX_1:27
           .= a;
 hence p`1<a by A6;
 end;
end;

theorem JJ60: for p1,p2,p being Point of TOP-REAL 2,a being Real
st p in LSeg(p1,p2) & p1`1>a & p2`1>a holds p`1>a
proof let p1,p2,p be Point of TOP-REAL 2,a be Real;
 assume A0: p in LSeg(p1,p2) & p1`1>a & p2`1>a;
 then consider r being Real such that
A3:      0<=r & r<=1 and
A1: p = (1-r)*p1+r*p2 by SPPOL_1:21;
A2:  p`1 = ((1-r)*p1)`1+(r*p2)`1 by A1,TOPREAL3:7
      .= ((1-r)*p1)`1+r*p2`1 by TOPREAL3:9
      .= (1-r)*p1`1+r*p2`1 by TOPREAL3:9;
  per cases;
  suppose 0=r;then p=p1+(0)*p2 by A1,EUCLID:33.=p1+0.REAL 2 by EUCLID:33
     .=p1 by EUCLID:31;
   hence p`1>a by A0;
  end;
  suppose 0<>r;then
    A10: r>0 by A3;
  1-r>=0 by A3,SQUARE_1:12;then
  A5: (1-r)*p1`1>=(1-r)*a by A0,AXIOMS:25;
  r*p2`1>r*a by A10,A0,REAL_1:70;then
  A6: p`1>(1-r)*a+r*a by A2,A5,REAL_1:67;
  (1-r)*a+r*a=((1-r)+r)*a by XCMPLX_1:8
           .= 1*a by XCMPLX_1:27
           .= a;
 hence p`1>a by A6;
 end;
end;

reserve s,p,q for Point of TOP-REAL 2;
