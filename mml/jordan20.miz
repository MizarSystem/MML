                                    by A57,A58,ABSVALUE:def 1;
                    then p8`1/2+e/2<e/2+e/2;
                    then p8`1/2<e/2 by XREAL_1:9;
                    then A60: p8`1/2-e/2<e/2-e/2 by REAL_1:92;
                       (p8`1-e)/2>=0/2 by A59,REAL_1:73;
