        hence contradiction by A69,A107,A109,AXIOMS:21;
       end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e by A96;
     end;
     end;
    hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e;
   end;
