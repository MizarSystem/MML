        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume A12: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;
          then A13: p6 in P by JORDAN5C:def 3;
              now per cases by A2,A13,Th19;
          case LE q1,p6,P,p1,p2;
           hence p6`1>=e by A3,A12;
          end;
          case A14: LE p6,q1,P,p1,p2;
                LE q2,p6,P,p1,p2 by A9,A12,JORDAN5C:13;
            then p6 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1,
p2 &
            LE qq,q1,P,p1,p2} by A14;
            then p6 in LSeg(q2,q1) by A7,JORDAN6:29;
           hence p6`1>=e by A1,A2,GOBOARD7:5;
          end;
          end;
         hence p6`1>=e;
        end;
      hence p is_Rout P,p1,p2,e by A1,A2,A3,A10,A11,Def4;
     end;
     case A15: LE q1,q2,P,p1,p2;
     A16: now per cases;
     case q1<>q2;
      then LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A1,A2,A15,Th28;
     end;
     case A17: q1=q2;
      then LSeg(q2,q1)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A2,A17,Th1;
     end;
     end;
           Segment(P,p1,p2,q1,q2) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;
       then consider p3 being Point of TOP-REAL 2 such that
       A18: p=p3 & (LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2) by A1,A16;
       A19: p`1=e by A1,A2,GOBOARD7:5;
       A20: LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by A1,A2,A4,A5,Th19;
           now assume LE p4,q2,P,p1,p2;
         then p4 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,p2
&
            LE qq,q2,P,p1,p2} by A3;
            then p4 in Segment(P,p1,p2,q1,q2) by JORDAN6:29;
        hence contradiction by A1,A2,A3,A16,GOBOARD7:5;
       end;
       then A21: LE p,p4,P,p1,p2 by A18,A20,JORDAN5C:13;
           for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume A22: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;
          then LE q1,p6,P,p1,p2 by A18,JORDAN5C:13;
         hence p6`1>=e by A3,A22;
        end;
      hence p is_Rout P,p1,p2,e by A1,A2,A3,A19,A21,Def4;
     end;
     end;
  hence p is_Rout P,p1,p2,e;
end;

theorem     for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
   then A2: P is_an_arc_of p1,p2 by TOPREAL4:3;
   consider f being FinSequence of (TOP-REAL 2) such that
   A3:  f is_S-Seq & P = L~f & p1=f/.1 & p2=f/.len f by A1,TOPREAL4:def 1;
       f is one-to-one & len f >= 2 & f is unfolded s.n.c. special
                    by A3,TOPREAL1:def 10;
   then A4: len f >1 by XXREAL_0:2;
A5: L~f = union { LSeg(f,i) where i is Element of NAT :
 1 <= i & i+1 <= len f }
                          by TOPREAL1:def 6;
   then consider Y being set such that
   A6: p in Y & Y in
      { LSeg(f,i) where i is Element of NAT : 1 <= i & i+1 <= len f }
            by A1,A3,TARSKI:def 4;
   A7: Y c= L~f
   proof let x be set;assume x in Y;
    hence x in L~f by A5,A6,TARSKI:def 4;
   end;
   consider i being Element of NAT such that
   A8: Y=LSeg(f,i) & 1 <= i & i+1 <= len f by A6;
   A9: i<len f by A8,NAT_1:13;
   then A10: i in dom f by A8,FINSEQ_3:27;
   A11: 1<len f by A8,A9,XXREAL_0:2;
   A12: 1<i+1 by A8,NAT_1:13;
   defpred P[Element of NAT] means
    for p being Point of TOP-REAL 2 st p=f.(i-'$1) holds p`1<>e;
       1 in dom f by A11,FINSEQ_3:27;
   then f/.1=f.1 by FINSEQ_4:def 4;
   then A13: P[i-'1] by A1,A3,A8,JCT_MISC:7;
   then A14: ex k being Element of NAT st P[k];
       ex k being Element of NAT st P[k] & for n being Element of NAT st P[n]
       holds k <= n
                                  from NAT_1:sch 5(A14);
   then consider k being Element of NAT such that
   A15: P[k] & for n being Element of NAT st P[n] holds k <= n;
       i-'k <=i by BINARITH:52;
   then A16: i-'k <len f by A9,XXREAL_0:2;
   A17: i-1>=0 by A8,XREAL_1:50;
       k<=i-'1 by A13,A15;
   then k<=i-1 by A17,BINARITH:def 3;
   then k+1<=i-1+1 by XREAL_1:9;
   then A18: 1+k-k<=i-k by XREAL_1:11;
   then A19: i-'k>=1 by BINARITH:def 3;
   then A20: i-'k in dom f by A16,FINSEQ_3:27;
   then A21: f/.(i-'k)=f.(i-'k) by FINSEQ_4:def 4;
   then reconsider pk=f.(i-'k) as Point of TOP-REAL 2;
   A22: i-'k=i-k by A18,BINARITH:def 3;
   A23: pk`1<> e by A15;
   reconsider kn=k+1 as Element of NAT;
       now per cases by A23,XXREAL_0:1;
   case A24: pk`1<e;
         now per cases;
     case A25: k=0;
      set p44=f/.i;
        A26: pk=f.i by A25,BINARITH:58 .=p44 by A10,FINSEQ_4:def 4;
        A27: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A8,TOPREAL1:def 5;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
        A28: len g=len f -'i+1 by A4,A8,A9,JORDAN3:27;
        A29: len f-i>0 by A9,XREAL_1:52;
        then len f-'i=len f-i by BINARITH:def 3;
        then A30: len f-'i+1>0+1 by A29,XREAL_1:10;
        then A31: 1+1<=len g by A28,NAT_1:13;
            1+(1+i)<=1+len f by A8,XREAL_1:9;
        then A32: 1+1+i-i<=len f+1-i by XREAL_1:11;
        then A33: 1<=i & i<=len f & len f<=len f & 1<=1 &
         1<=len f+1-i by A8,XXREAL_0:2,NAT_1:13;
            1 in dom g by A28,A30,FINSEQ_3:27;
        then A34: g/.1=g.1 by FINSEQ_4:def 4
                .=f.(1-1+i) by A33,JORDAN3:31
                .=f/.i by A10,FINSEQ_4:def 4;
        reconsider ia=i+1 as Element of NAT;
            ia in Seg len f by A8,A12,FINSEQ_1:3;
        then A35: i+1 in dom f by FINSEQ_1:def 3;
            1+1 in Seg len g by A31,FINSEQ_1:3;
        then 1+1 in dom g by FINSEQ_1:def 3;
        then A36: g/.(1+1)=g.(1+1) by FINSEQ_4:def 4
                    .=f.(1+1-1+i) by A8,A9,A32,JORDAN3:31
                    .=f/.(i+1) by A35,FINSEQ_4:def 4;
            LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A8,TOPREAL1:def 5
                .=LSeg(g,1) by A31,A34,A36,TOPREAL1:def 5;
        then p in Y &
          Y in { LSeg(g,i2) where i2 is Element of NAT :
           1 <= i2 & i2+1 <= len g }
                           by A6,A8,A31;
        then p in union{ LSeg(g,i2) where i2 is Element of NAT :
         1 <= i2 & i2+1 <=
len g
}
            by TARSKI:def 4;
        then p in L~mid(f,i,len f) by TOPREAL1:def 6;
        then A37: LE p44,p,P,p1,p2 by A3,A8,A9,SPRECT_4:4;
            for p5 being Point of TOP-REAL 2 st
         LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume A38: LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2;
               Segment(P,p1,p2,p44,p)={p8 where p8 is Point of TOP-REAL 2:
            LE p44,p8,P,p1,p2 & LE p8,p,P,p1,p2} by JORDAN6:29;
           then A39: p5 in Segment(P,p1,p2,p44,p) by A38;
              p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p44,p) c= LSeg(f,i) by A6,A8,A27,TOPREAL1:12;
           then A40: LSeg(p44,p) c= P by A3,A7,A8,XBOOLE_1:1;
               now per cases;
           case p44<>p;
             then LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;
             then Segment(P,p1,p2,p44,p)=LSeg(p44,p) by A2,A37,A40,Th28;
            hence p5`1<=e by A1,A24,A26,A39,TOPREAL1:9;
           end;
           case p44=p;
            hence p5`1<=e by A1,A24,A26;
           end;
           end;
          hence p5`1<=e;
         end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A1,A2,A24,A26,A37,Def1
;
     end;
     case k<>0; then A41: k>0;
        then A42: k>=0+1 by NAT_1:13;
      set p44=f/.i;
        A43: i>i-'k by A19,A42,BINARITH:69;
        then A44: i>1 by A19,XXREAL_0:2;
        A45: now assume A46: (f/.i)`1<>e;
             f.i=f/.i by A10,FINSEQ_4:def 4;
         then for p9 being Point of TOP-REAL 2 st p9=f.(i-'0)
            holds p9`1<>e by A46,BINARITH:58;
         hence contradiction by A15,A41;
        end;
        A47: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A8,TOPREAL1:def 5;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
        A48: len g=len f -'i+1 by A4,A8,A9,JORDAN3:27;
        A49: len f-i>0 by A9,XREAL_1:52;
        then len f-'i=len f-i by BINARITH:def 3;
        then A50: len f-'i+1>0+1 by A49,XREAL_1:10;
        then A51: 1+1<=len g by A48,NAT_1:13;
            1+(1+i)<=1+len f by A8,XREAL_1:9;
        then A52: 1+1+i-i<=len f+1-i by XREAL_1:11;
        then A53: 1<=i & i<=len f & len f<=len f & 1<=1 &
         1<=len f+1-i by A8,XXREAL_0:2,NAT_1:13;
            1 in dom g by A48,A50,FINSEQ_3:27;
        then A54: g/.1=g.1 by FINSEQ_4:def 4
                .=f.(1-1+i) by A53,JORDAN3:31
                .=f/.i by A10,FINSEQ_4:def 4;
        reconsider ia=i+1 as Element of NAT;
            ia in Seg len f by A8,A12,FINSEQ_1:3;
        then A55: i+1 in dom f by FINSEQ_1:def 3;
            1+1 in Seg len g by A51,FINSEQ_1:3;
        then 1+1 in dom g by FINSEQ_1:def 3;
        then A56: g/.(1+1)=g.(1+1) by FINSEQ_4:def 4
                    .=f.(1+1-1+i) by A8,A9,A52,JORDAN3:31
                    .=f/.(i+1) by A55,FINSEQ_4:def 4;
            LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A8,TOPREAL1:def 5
                .=LSeg(g,1) by A51,A54,A56,TOPREAL1:def 5;
        then p in Y &
          Y in { LSeg(g,i2) where i2 is Element of NAT :
           1 <= i2 & i2+1 <= len g }
                           by A6,A8,A51;
        then p in union{ LSeg(g,i2) where i2 is Element of NAT
         : 1 <= i2 & i2+1 <=
len g}
            by TARSKI:def 4;
        then p in L~mid(f,i,len f) by TOPREAL1:def 6;
        then A57: LE p44,p,P,p1,p2 by A3,A8,A9,SPRECT_4:4;
        A58: for p5 being Point of TOP-REAL 2 st
         LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume A59: LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2;
               Segment(P,p1,p2,p44,p)={p8 where p8 is Point of TOP-REAL 2:
            LE p44,p8,P,p1,p2 & LE p8,p,P,p1,p2} by JORDAN6:29;
           then A60: p5 in Segment(P,p1,p2,p44,p) by A59;
              p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p44,p) c= LSeg(f,i) by A6,A8,A47,TOPREAL1:12;
           then A61: LSeg(p44,p) c= P by A3,A7,A8,XBOOLE_1:1;
               now per cases;
           case p44<>p;
             then LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;
             then Segment(P,p1,p2,p44,p)=LSeg(p44,p) by A2,A57,A61,Th28
;
            hence p5`1<=e by A1,A45,A60,TOPREAL1:9;
           end;
           case p44=p;
             then Segment(P,p1,p2,p44,p)={p44} by A1,A2,Th1;
            hence p5`1<=e by A45,A60,TARSKI:def 1;
           end;
           end;
          hence p5`1<=e;
         end;
        reconsider g=mid(f,1,i) as FinSequence of (TOP-REAL 2);
        A62: len g=i -'1+1 by A8,A9,A11,JORDAN3:27;
            i-1>0 by A44,XREAL_1:52;
        then A63: i-'1=i-1 by BINARITH:def 3;
        A64: i-'k <i by A19,A42,BINARITH:69;
        A65: i-'k+1<=len g by A43,A62,A63,NAT_1:13;
        A66: 1<=(i-'k) by A18,BINARITH:def 3;
        A67: 1<=1 & 1<=i & i<=len f & 1<=i-'k &
         i-'k<=i+1-1 by A8,A19,A42,BINARITH:69,NAT_1:13;
        A68: i-'k+1<=i by A43,NAT_1:13;
        reconsider ia=i+1 as Element of NAT;
            i-'k in Seg len g by A19,A62,A63,A64,FINSEQ_1:3;
        then i-'k in dom g by FINSEQ_1:def 3;
        then A69: g/.(i-'k)=g.(i-'k) by FINSEQ_4:def 4
                    .=f.(i-'k-1+1) by A67,JORDAN3:31
                    .=f/.(i-'k) by A20,FINSEQ_4:def 4;
            LSeg(g,i-'k)=LSeg(g/.(i-'k),g/.(i-'k+1))
             by A19,A65,TOPREAL1:def 5;
          then pk in LSeg(g,i-'k) &
          LSeg(g,i-'k) in
          { LSeg(g,i2) where i2 is Element of NAT : 1 <= i2 & i2+1 <= len g }
                           by A21,A62,A63,A66,A68,A69,TOPREAL1:6;
         then pk in union{ LSeg(g,i2) where i2 is Element of NAT :
          1 <= i2 & i2+1 <=
         len g } by TARSKI:def 4;
         then pk in L~mid(f,1,i) by TOPREAL1:def 6;
         then A70: LE pk,p44,P,p1,p2 by A3,A9,A44,SPRECT_3:33;
         then A71: LE pk,p,P,p1,p2 by A57,JORDAN5C:13;
         A72: p44 in P by A70,JORDAN5C:def 3;
         A73: now assume not
           for p51 being Point of TOP-REAL 2 st
            LE pk,p51,P,p1,p2 & LE p51,p44,P,p1,p2 holds p51`1<=e;
           then consider p51 being Point of TOP-REAL 2 such that
           A74: LE pk,p51,P,p1,p2 & LE p51,p44,P,p1,p2 & p51`1>e;
               p51 in P by A74,JORDAN5C:def 3;
           then consider Y3 being set such that
           A75: p51 in Y3 & Y3 in
            { LSeg(f,i5) where i5 is Element of NAT : 1 <= i5 & i5+1 <= len f }
                     by A3,A5,TARSKI:def 4;
           consider kk being Element of NAT such that
           A76: Y3=LSeg(f,kk) &  (1 <= kk & kk+1 <= len f) by A75;
           A77: kk<len f by A76,NAT_1:13;
           then kk in Seg len f by A76,FINSEQ_1:3;
           then A78: kk in dom f by FINSEQ_1:def 3;
            1<kk+1 by A76,NAT_1:13;
           then kk+1 in Seg len f by A76,FINSEQ_1:3;
           then A79: kk+1 in dom f by FINSEQ_1:def 3;
           A80: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A76,TOPREAL1:def 5;
           A81: LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                     by A3,A75,A76,JORDAN5C:26;
               now per cases by A74,A75,A76,A80,Th2;
           case A82: (f/.kk)`1>e;
              set k2=i-'kk;
                    LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A3,A75,A76,JORDAN5C:25;
                then A83: LE f/.kk,p44,L~f,f/.1,f/.(len f)
                                 by A3,A74,JORDAN5C:13;
                  now assume i-kk<=0; then i-kk+kk<=0+kk by XREAL_1:9;
                then LE f/.i,f/.kk,L~f,f/.1,f/.(len f)
                         by A3,A44,A77,JORDAN5C:24;
               hence contradiction by A2,A3,A45,A82,A83,JORDAN5C:12;
              end;
              then A84: i-'kk=i-kk by BINARITH:def 3;
              then i-k2>0 by A76;
              then A85: i-'k2>0 by BINARITH:def 3;
              A86: i-k2=i-'k2 by A84,BINARITH:def 3;
              A87: i-'k2>=0+1 by A85,NAT_1:13;
                  i-'k2<=len f by A9,BINARITH:68;
              then P[k2] by A82,A84,A86,A87,FINSEQ_4:24;
              then k2>=k by A15;
              then i-k2<=i-k by REAL_2:106;
              then A88: LE f/.(i-'k2),f/.(i-'k),L~f,f/.1,f/.(len f)
                           by A3,A16,A22,A76,A84,A86,JORDAN5C:24;
              A89: LE f/.(i-'k),p51,L~f,f/.1,f/.(len f) by A3,A20,A74,
FINSEQ_4:def 4;
              A90: LE f/.(i-'k),f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A21,A74,A81,JORDAN5C:13;
              then A91: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A84,A86,A88,JORDAN5C:13;
                  Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              then A92: f/.(i-'k) in Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+
1))
                                            by A84,A86,A88,A90;
              A93: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A94: z in LSeg(f/.kk,f/.(kk+1));
                    LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Element of NAT :
                 1 <= i7 & i7+1 <= len f }
                               by A76,A80;
               hence z in L~f by A5,A94,TARSKI:def 4;
              end;
              A95: f is special by A3,TOPREAL1:def 10;
              A96: f is one-to-one by A3,TOPREAL1:def 10;
                  kk<kk+1 by NAT_1:13;
              then A97: f.kk <> f.(kk+1) by A78,A79,A96,FUNCT_1:def 8;
              A98: f/.kk=f.kk by A78,FINSEQ_4:def 4;
                  f/.(kk+1)=f.(kk+1) by A79,FINSEQ_4:def 4;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A97,A98,TOPREAL1:15;
              then A99: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A2,A3,A91,A93,Th28;
               A100: (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A76,A95,TOPREAL1:def 7;
                  (f/.(kk+1))`1<e by A21,A24,A82,A92,A99,Th3;
              then (f/.kk)`1> (f/.(kk+1))`1 by A82,XXREAL_0:2;
              then (f/.(i-'k))`1>=p51`1
                     by A3,A75,A76,A77,A80,A89,A92,A99,A100,Th6;
            hence contradiction by A21,A24,A74,XXREAL_0:2;
           end;
           case A101: (f/.(kk+1))`1 >e & (f/.kk)`1<=e;
              then A102: (f/.kk)`1<(f/.(kk+1))`1 by XXREAL_0:2;
              set k2=i-'kk-'1;
