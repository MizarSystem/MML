         then A137: ((TOP-REAL 2)|Segment(P,p1,p2,p1,q1)) is non empty
                               by STRUCT_0:def 1;
         A138: G5 is open by A136,TOPS_2:32;
         A139: f3"G5=g3"(G4 /\ Segment(P,p1,p2,p1,q1)) by PRE_TOPC:def 10
             .=g3"(G4) /\ g3"(Segment(P,p1,p2,p1,q1)) by FUNCT_1:137;
         A140: rng g3=[#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q1))
                              by TOPS_2:def 5,A128
              .= Segment(P,p1,p2,p1,q1) by PRE_TOPC:def 10;
           g3"G=g3"G4 /\ g3"([#]((TOP-REAL 2)|Qa')) by A136,FUNCT_1:137
             .=g3"G4 /\ g3"Qa' by PRE_TOPC:def 10
             .=g3"G4 /\ g3"((rng g3) /\ Qa') by RELAT_1:168
             .=g3"G4 /\ g3"(Segment(P,p1,p2,p1,q1)) by A134,A140,XBOOLE_1:28;
        hence g3"G is open by A129,A137,A138,A139,TOPS_2:55;
       end;
      then A141: g3 is continuous by TOPS_2:55;
        then g3 is Path of p1',q1' by A128,BORSUK_2:def 1;
       hence ex G3 being Path of p1',q1' st
           G3 is continuous & G3.0=p1' & G3.1=q1' by A141,A128;
      end;
      case A142: q1=p1;
        consider g01 being map of I[01],(((TOP-REAL 2)|P)|Qa) such that
        A143: g01 is continuous & g01.0=q1' & g01.1=q1' by BORSUK_2:4;
          g01 is Path of q1',q1' by BORSUK_2:def 1,A143;
       hence ex G3 being Path of p1',q1' st
           G3 is continuous & G3.0=p1' & G3.1=q1' by A142,A143;
      end;
      end;
      then consider G3 being Path of p1',q1' such that
      A144: G3 is continuous & G3.0=p1' & G3.1=q1';
        G3+(G2+G1) is continuous & (G3+(G2+G1)).0=p1' & (G3+(G2+G1)).1=p2'
                        by Th27,A144,A126;
     hence contradiction by Th18,A1,A86,A9,A3;
    end;
    end;
   hence contradiction;
  end;
 hence thesis by Th21,A6,A1;
end;

theorem   for P being non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p being Point of TOP-REAL 2,e being Real st
  p1`1<e & p2`1>e &
  q1 is_Lin P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
  & p in LSeg(q1,q2) holds p is_Lin P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p be Point of TOP-REAL 2,e be Real;
  assume A1: p1`1<e & p2`1>e &
   q1 is_Lin P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
   & p in LSeg(q1,q2);
   then A2: P is_an_arc_of p1,p2 & q1 in P & q1`1=e &
      (ex p4 being Point of TOP-REAL 2 st p4`1<e &
       LE p4,q1,P,p1,p2 &
       (for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,q1,P,p1,p2 holds p5`1<=e)) by Def1;
    consider p4 being Point of TOP-REAL 2 such that
    A3: p4`1<e & LE p4,q1,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,q1,P,p1,p2 holds p5`1<=e) by A1,Def1;
     A4: q1 in LSeg(q1,q2) & q2 in LSeg(q1,q2) by TOPREAL1:6;
     A5: p4 in P by A3,JORDAN5C:def 3;
       now per cases by A1,A2,A4,Th19;
     case A6: LE q1,q2,P,p1,p2;
     A7: now per cases;
     case q1<>q2;
       then LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A6,A1,Th28,A2;
     end;
     case A8: q1=q2;
       then LSeg(q1,q2)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A2,A8,Th1;
     end;
     end;
         Segment(P,p1,p2,q1,q2) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;
       then consider p3 being Point of TOP-REAL 2 such that
       A9: p=p3 & (LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2) by A1,A7;
       A10: p`1=e by A2,A1,GOBOARD7:5;
       A11: LE p4,p,P,p1,p2 by A9,A3,JORDAN5C:13;
          for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2 holds p6`1<=e
        proof let p6 be Point of TOP-REAL 2;
          assume A12: LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2;
          then A13: p6 in P by JORDAN5C:def 3;
            now per cases by A2,A13,Th19;
          case LE p6,q1,P,p1,p2;
           hence p6`1<=e by A12,A3;
          end;
          case A14: LE q1,p6,P,p1,p2;
              LE p6,q2,P,p1,p2 by A9,JORDAN5C:13,A12;
            then p6 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,
p2 &
            LE qq,q2,P,p1,p2} by A14;
            then p6 in LSeg(q1,q2) by A7,JORDAN6:29;
           hence p6`1<=e by GOBOARD7:5,A2,A1;
          end;
          end;
         hence p6`1<=e;
        end;
      hence p is_Lin P,p1,p2,e by Def1,A1,A2,A10,A11,A3;
     end;
     case A15: LE q2,q1,P,p1,p2;
     A16: now per cases;
     case q1<>q2;
       then LSeg(q2,q1) is_an_arc_of q2,q1 by TOPREAL1:15;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by A15,A1,Th28,A2;
     end;
     case A17: q1=q2;
       then LSeg(q2,q1)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by A2,A17,Th1;
     end;
     end;
         Segment(P,p1,p2,q2,q1) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;
       then consider p3 being Point of TOP-REAL 2 such that
       A18: p=p3 & (LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2) by A1,A16;
       A19: p`1=e by A2,A1,GOBOARD7:5;
       A20: LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by A1,A2,A4,A5,Th19;
         now assume LE q2,p4,P,p1,p2;
         then p4 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1,p2
&
            LE qq,q1,P,p1,p2} by A3;
            then p4 in Segment(P,p1,p2,q2,q1) by JORDAN6:29;
        hence contradiction by A1,A2,A3,A16,GOBOARD7:5;
       end;
       then A21: LE p4,p,P,p1,p2 by A20,A18,JORDAN5C:13;
         for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2 holds p6`1<=e
        proof let p6 be Point of TOP-REAL 2;
          assume A22: LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2;
          then LE p6,q1,P,p1,p2 by JORDAN5C:13,A18;
         hence p6`1<=e by A22,A3;
        end;
      hence p is_Lin P,p1,p2,e by Def1,A1,A2,A19,A21,A3;
     end;
     end;
  hence p is_Lin P,p1,p2,e;
end;

theorem   for P being non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p being Point of TOP-REAL 2,e being Real st
  p1`1<e & p2`1>e &
  q1 is_Rin P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
  & p in LSeg(q1,q2) holds p is_Rin P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p be Point of TOP-REAL 2,e be Real;
  assume A1: p1`1<e & p2`1>e &
   q1 is_Rin P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
   & p in LSeg(q1,q2);
   then A2: P is_an_arc_of p1,p2 & q1 in P & q1`1=e &
      (ex p4 being Point of TOP-REAL 2 st p4`1>e &
       LE p4,q1,P,p1,p2 &
       (for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,q1,P,p1,p2 holds p5`1>=e)) by Def2;
    consider p4 being Point of TOP-REAL 2 such that
    A3: p4`1>e & LE p4,q1,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,q1,P,p1,p2 holds p5`1>=e) by A1,Def2;
     A4: q1 in LSeg(q1,q2) & q2 in LSeg(q1,q2) by TOPREAL1:6;
     A5: p4 in P by A3,JORDAN5C:def 3;
       now per cases by A1,A2,A4,Th19;
     case A6: LE q1,q2,P,p1,p2;
     A7: now per cases;
     case q1<>q2;
       then LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A6,A1,Th28,A2;
     end;
     case A8: q1=q2;
       then LSeg(q1,q2)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A2,A8,Th1;
     end;
     end;
         Segment(P,p1,p2,q1,q2) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;
       then consider p3 being Point of TOP-REAL 2 such that
       A9: p=p3 & (LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2) by A1,A7;
       A10: p`1=e by A2,A1,GOBOARD7:5;
       A11: LE p4,p,P,p1,p2 by A9,A3,JORDAN5C:13;
          for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume A12: LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2;
          then A13: p6 in P by JORDAN5C:def 3;
            now per cases by A2,A13,Th19;
          case LE p6,q1,P,p1,p2;
           hence p6`1>=e by A12,A3;
          end;
          case A14: LE q1,p6,P,p1,p2;
              LE p6,q2,P,p1,p2 by A9,JORDAN5C:13,A12;
            then p6 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,
p2 &
            LE qq,q2,P,p1,p2} by A14;
            then p6 in LSeg(q1,q2) by A7,JORDAN6:29;
           hence p6`1>=e by GOBOARD7:5,A2,A1;
          end;
          end;
         hence p6`1>=e;
        end;
      hence p is_Rin P,p1,p2,e by Def2,A1,A2,A10,A11,A3;
     end;
     case A15: LE q2,q1,P,p1,p2;
     A16: now per cases;
     case q1<>q2;
       then LSeg(q2,q1) is_an_arc_of q2,q1 by TOPREAL1:15;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by A15,A1,Th28,A2;
     end;
     case A17: q1=q2;
       then LSeg(q2,q1)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by A2,A17,Th1;
     end;
     end;
         Segment(P,p1,p2,q2,q1) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;
       then consider p3 being Point of TOP-REAL 2 such that
       A18: p=p3 & (LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2) by A1,A16;
       A19: p`1=e by A2,A1,GOBOARD7:5;
       A20: LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by A1,A2,A4,A5,Th19;
         now assume LE q2,p4,P,p1,p2;
         then p4 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1,p2
&
            LE qq,q1,P,p1,p2} by A3;
            then p4 in Segment(P,p1,p2,q2,q1) by JORDAN6:29;
        hence contradiction by A1,A2,A3,A16,GOBOARD7:5;
       end;
       then A21: LE p4,p,P,p1,p2 by A20,A18,JORDAN5C:13;
         for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume A22: LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2;
          then LE p6,q1,P,p1,p2 by JORDAN5C:13,A18;
         hence p6`1>=e by A22,A3;
        end;
      hence p is_Rin P,p1,p2,e by Def2,A1,A2,A19,A21,A3;
     end;
     end;
  hence p is_Rin P,p1,p2,e;
end;

theorem   for P being non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p being Point of TOP-REAL 2,e being Real st
  p1`1<e & p2`1>e &
  q1 is_Lout P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
  & p in LSeg(q1,q2) holds p is_Lout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p be Point of TOP-REAL 2,e be Real;
  assume A1: p1`1<e & p2`1>e &
   q1 is_Lout P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
   & p in LSeg(q1,q2);
   then A2: P is_an_arc_of p1,p2 & q1 in P & q1`1=e &
      (ex p4 being Point of TOP-REAL 2 st p4`1<e &
       LE q1,p4,P,p1,p2 &
       (for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE q1,p5,P,p1,p2 holds p5`1<=e)) by Def3;
    consider p4 being Point of TOP-REAL 2 such that
    A3: p4`1<e & LE q1,p4,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE q1,p5,P,p1,p2 holds p5`1<=e) by A1,Def3;
     A4: q1 in LSeg(q1,q2) & q2 in LSeg(q1,q2) by TOPREAL1:6;
     A5: p4 in P by A3,JORDAN5C:def 3;
       now per cases by A1,A2,A4,Th19;
     case A6: LE q2,q1,P,p1,p2;
     A7: now per cases;
     case q1<>q2;
       then LSeg(q2,q1) is_an_arc_of q2,q1 by TOPREAL1:15;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by A6,A1,Th28,A2;
     end;
     case A8: q1=q2;
       then LSeg(q1,q2)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by A2,A8,Th1;
     end;
     end;
         Segment(P,p1,p2,q2,q1) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;
       then consider p3 being Point of TOP-REAL 2 such that
       A9: p=p3 & (LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2) by A1,A7;
       A10: p`1=e by A2,A1,GOBOARD7:5;
       A11: LE p,p4,P,p1,p2 by A9,A3,JORDAN5C:13;
          for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1<=e
        proof let p6 be Point of TOP-REAL 2;
          assume A12: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;
          then A13: p6 in P by JORDAN5C:def 3;
            now per cases by A2,A13,Th19;
          case LE q1,p6,P,p1,p2;
           hence p6`1<=e by A12,A3;
          end;
          case A14: LE p6,q1,P,p1,p2;
              LE q2,p6,P,p1,p2 by A9,JORDAN5C:13,A12;
            then p6 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1,
p2 &
            LE qq,q1,P,p1,p2} by A14;
            then p6 in LSeg(q2,q1) by A7,JORDAN6:29;
           hence p6`1<=e by GOBOARD7:5,A2,A1;
          end;
          end;
         hence p6`1<=e;
        end;
      hence p is_Lout P,p1,p2,e by Def3,A1,A2,A10,A11,A3;
     end;
     case A15: LE q1,q2,P,p1,p2;
     A16: now per cases;
     case q1<>q2;
       then LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A15,A1,Th28,A2;
     end;
     case A17: q1=q2;
       then LSeg(q2,q1)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A2,A17,Th1;
     end;
     end;
         Segment(P,p1,p2,q1,q2) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;
       then consider p3 being Point of TOP-REAL 2 such that
       A18: p=p3 & (LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2) by A1,A16;
       A19: p`1=e by A2,A1,GOBOARD7:5;
       A20: LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by A1,A2,A4,A5,Th19;
         now assume LE p4,q2,P,p1,p2;
         then p4 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,p2
&
            LE qq,q2,P,p1,p2} by A3;
            then p4 in Segment(P,p1,p2,q1,q2) by JORDAN6:29;
        hence contradiction by A1,A2,A3,A16,GOBOARD7:5;
       end;
       then A21: LE p,p4,P,p1,p2 by A20,A18,JORDAN5C:13;
         for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1<=e
        proof let p6 be Point of TOP-REAL 2;
          assume A22: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;
          then LE q1,p6,P,p1,p2 by JORDAN5C:13,A18;
         hence p6`1<=e by A22,A3;
        end;
      hence p is_Lout P,p1,p2,e by Def3,A1,A2,A19,A21,A3;
     end;
     end;
  hence p is_Lout P,p1,p2,e;
end;

theorem   for P being non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p being Point of TOP-REAL 2,e being Real st
  p1`1<e & p2`1>e &
  q1 is_Rout P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
  & p in LSeg(q1,q2) holds p is_Rout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p be Point of TOP-REAL 2,e be Real;
  assume A1: p1`1<e & p2`1>e &
   q1 is_Rout P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
   & p in LSeg(q1,q2);
   then A2: P is_an_arc_of p1,p2 & q1 in P & q1`1=e &
      (ex p4 being Point of TOP-REAL 2 st p4`1>e &
       LE q1,p4,P,p1,p2 &
       (for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE q1,p5,P,p1,p2 holds p5`1>=e)) by Def4;
    consider p4 being Point of TOP-REAL 2 such that
    A3: p4`1>e & LE q1,p4,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE q1,p5,P,p1,p2 holds p5`1>=e) by A1,Def4;
     A4: q1 in LSeg(q1,q2) & q2 in LSeg(q1,q2) by TOPREAL1:6;
     A5: p4 in P by A3,JORDAN5C:def 3;
       now per cases by A1,A2,A4,Th19;
     case A6: LE q2,q1,P,p1,p2;
     A7: now per cases;
     case q1<>q2;
       then LSeg(q2,q1) is_an_arc_of q2,q1 by TOPREAL1:15;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by A6,A1,Th28,A2;
     end;
     case A8: q1=q2;
       then LSeg(q1,q2)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by A2,A8,Th1;
     end;
     end;
         Segment(P,p1,p2,q2,q1) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;
       then consider p3 being Point of TOP-REAL 2 such that
       A9: p=p3 & (LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2) by A1,A7;
       A10: p`1=e by A2,A1,GOBOARD7:5;
       A11: LE p,p4,P,p1,p2 by A9,A3,JORDAN5C:13;
          for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume A12: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;
          then A13: p6 in P by JORDAN5C:def 3;
            now per cases by A2,A13,Th19;
          case LE q1,p6,P,p1,p2;
           hence p6`1>=e by A12,A3;
          end;
          case A14: LE p6,q1,P,p1,p2;
              LE q2,p6,P,p1,p2 by A9,JORDAN5C:13,A12;
            then p6 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1,
p2 &
            LE qq,q1,P,p1,p2} by A14;
            then p6 in LSeg(q2,q1) by A7,JORDAN6:29;
           hence p6`1>=e by GOBOARD7:5,A2,A1;
          end;
          end;
         hence p6`1>=e;
        end;
      hence p is_Rout P,p1,p2,e by Def4,A1,A2,A10,A11,A3;
     end;
     case A15: LE q1,q2,P,p1,p2;
     A16: now per cases;
     case q1<>q2;
      then LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A15,A1,Th28,A2;
     end;
     case A17: q1=q2;
      then LSeg(q2,q1)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A2,A17,Th1;
     end;
     end;
         Segment(P,p1,p2,q1,q2) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;
       then consider p3 being Point of TOP-REAL 2 such that
       A18: p=p3 & (LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2) by A1,A16;
       A19: p`1=e by A2,A1,GOBOARD7:5;
       A20: LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by A1,A2,A4,A5,Th19;
         now assume LE p4,q2,P,p1,p2;
         then p4 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,p2
&
            LE qq,q2,P,p1,p2} by A3;
            then p4 in Segment(P,p1,p2,q1,q2) by JORDAN6:29;
        hence contradiction by A1,A2,A3,A16,GOBOARD7:5;
       end;
       then A21: LE p,p4,P,p1,p2 by A20,A18,JORDAN5C:13;
         for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume A22: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;
          then LE q1,p6,P,p1,p2 by JORDAN5C:13,A18;
         hence p6`1>=e by A22,A3;
        end;
      hence p is_Rout P,p1,p2,e by Def4,A1,A2,A19,A21,A3;
     end;
     end;
  hence p is_Rout P,p1,p2,e;
end;

theorem   for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
   then A2: P is_an_arc_of p1,p2 by TOPREAL4:3;
   consider f being FinSequence of (TOP-REAL 2) such that
   A3:  f is_S-Seq & P = L~f & p1=f/.1 & p2=f/.len f by TOPREAL4:def 1,A1;
     f is one-to-one & len f >= 2 & f is unfolded s.n.c. special
                    by A3,TOPREAL1:def 10;
   then A4: len f >1 by AXIOMS:22;
   A5: L~f = union { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
                          by TOPREAL1:def 6;
   then consider Y being set such that
   A6: p in Y & Y in
      { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
            by TARSKI:def 4,A3,A1;
   A7: Y c= L~f
   proof let x be set;assume x in Y;
    hence x in L~f by A5,A6,TARSKI:def 4;
   end;
   consider i being Nat such that
   A8: Y=LSeg(f,i) & 1 <= i & i+1 <= len f by A6;
   A9: i<len f by A8,NAT_1:38;
   then A10: i in dom f by A8,FINSEQ_3:27;
   A11: 1<len f by A8,A9,AXIOMS:22;
   A12: 1<i+1 by A8,NAT_1:38;
   defpred P[Nat] means
    for p being Point of TOP-REAL 2 st p=f.(i-'$1) holds p`1<>e;
     1 in dom f by A11,FINSEQ_3:27;
   then f/.1=f.1 by FINSEQ_4:def 4;
   then A13: P[i-'1] by A3,A1,JCT_MISC:7,A8;
   then A14: ex k being Nat st P[k];
     ex k being Nat st P[k] & for n being Nat st P[n] holds k <= n
                                  from NAT_1:sch 5(A14);
   then consider k being Nat such that
   A15: P[k] & for n being Nat st P[n] holds k <= n;
     i-'k <=i by GOBOARD9:2;
   then A16: i-'k <len f by AXIOMS:22,A9;
   A17: i-1>=0 by A8,SQUARE_1:12;
     k<=i-'1 by A15,A13;
   then k<=i-1 by A17,BINARITH:def 3;
   then k+1<=i-1+1 by REAL_1:55;
   then k+1<=i by XCMPLX_1:27;
   then 1+k-k<=i-k by REAL_1:49;
   then A18: 1<=i-k by XCMPLX_1:26;
   then A19: 0<i-k by AXIOMS:22;
   then A20: i-'k>=1 by A18,BINARITH:def 3;
   then A21: i-'k in dom f by A16,FINSEQ_3:27;
   then A22: f/.(i-'k)=f.(i-'k) by FINSEQ_4:def 4;
   then reconsider pk=f.(i-'k) as Point of TOP-REAL 2;
   A23: i-'k=i-k by A19,BINARITH:def 3;
   A24: pk`1<> e by A15;
   reconsider kn=k+1 as Nat;
     now per cases by A24,AXIOMS:21;
   case A25: pk`1<e;
       now per cases;
     case A26: k=0;
      set p44=f/.i;
        A27: pk=f.i by A26,JORDAN3:2 .=p44 by A10,FINSEQ_4:def 4;
        A28: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A8;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
        A29: len g=len f -'i+1 by JORDAN3:27,A9,A8,A4;
        A30: len f-i>0 by A9,SQUARE_1:11;
        then len f-'i=len f-i by BINARITH:def 3;
        then A31: len f-'i+1>0+1 by A30,REAL_1:67;
        then A32: 1+1<=len g by A29,NAT_1:38;
          1+(1+i)<=1+len f by A8,REAL_1:55;
        then 1+1+i<=len f+1 by XCMPLX_1:1;
        then 1+1+i-i<=len f+1-i by REAL_1:49;
        then A33: 1+1<=len f+1-i by XCMPLX_1:26;
        then A34: 1<=i & i<=len f & len f<=len f & 1<=1 &
         1<=len f+1-i by A8,AXIOMS:22,NAT_1:38;
          1 in dom g by A29,A31,FINSEQ_3:27;
        then A35: g/.1=g.1 by FINSEQ_4:def 4
                .=f.(1-1+i) by A34,JORDAN3:31
                .=f/.i by FINSEQ_4:def 4,A10;
        reconsider ia=i+1 as Nat;
          ia in Seg len f by A8,A12,FINSEQ_1:3;
        then A36: i+1 in dom f by FINSEQ_1:def 3;
          1+1 in Seg len g by A32,FINSEQ_1:3;
        then 1+1 in dom g by FINSEQ_1:def 3;
        then A37: g/.(1+1)=g.(1+1) by FINSEQ_4:def 4
                    .=f.(1+1-1+i) by A8,A9,A33,JORDAN3:31
                    .=f/.(i+1) by A36,FINSEQ_4:def 4;
          LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A8
                .=LSeg(g,1) by TOPREAL1:def 5,A32,A35,A37;
        then p in Y &
          Y in { LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
                           by A6,A8,A32;
        then p in union{ LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <=
len g
}
            by TARSKI:def 4;
        then p in L~mid(f,i,len f) by TOPREAL1:def 6;
        then A38: LE p44,p,P,p1,p2 by SPRECT_4:4,A3,A8,A9;
          for p5 being Point of TOP-REAL 2 st
         LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume A39: LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2;
             Segment(P,p1,p2,p44,p)={p8 where p8 is Point of TOP-REAL 2:
            LE p44,p8,P,p1,p2 & LE p8,p,P,p1,p2} by JORDAN6:29;
           then A40: p5 in Segment(P,p1,p2,p44,p) by A39;
            p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p44,p) c= LSeg(f,i) by A6,A8,A28,TOPREAL1:12;
           then A41: LSeg(p44,p) c= P by XBOOLE_1:1,A3,A8,A7;
             now per cases;
           case p44<>p;
             then LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;
             then Segment(P,p1,p2,p44,p)=LSeg(p44,p) by Th28,A38,A41,A2;
            hence p5`1<=e by A1,A25,A27,A40,TOPREAL1:9;
           end;
           case p44=p;
            hence p5`1<=e by A1,A25,A27;
           end;
           end;
          hence p5`1<=e;
         end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A1,A2,A25,A27,A38,Def1
;
     end;
     case k<>0; then A42: k>0 by NAT_1:19;
        then A43: k>=0+1 by NAT_1:38;
      set p44=f/.i;
        A44: i>i-'k by A20,A43,JORDAN3:14;
        then A45: i>1 by A20,AXIOMS:22;
        A46: now assume A47: (f/.i)`1<>e;
           f.i=f/.i by FINSEQ_4:def 4,A10;
         then for p9 being Point of TOP-REAL 2 st p9=f.(i-'0)
            holds p9`1<>e by A47,JORDAN3:2;
         hence contradiction by A42,A15;
        end;
        A48: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A8;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
        A49: len g=len f -'i+1 by JORDAN3:27,A9,A8,A4;
        A50: len f-i>0 by A9,SQUARE_1:11;
        then len f-'i=len f-i by BINARITH:def 3;
        then A51: len f-'i+1>0+1 by A50,REAL_1:67;
        then A52: 1+1<=len g by A49,NAT_1:38;
          1+(1+i)<=1+len f by A8,REAL_1:55;
        then 1+1+i<=len f+1 by XCMPLX_1:1;
        then 1+1+i-i<=len f+1-i by REAL_1:49;
        then A53: 1+1<=len f+1-i by XCMPLX_1:26;
        then A54: 1<=i & i<=len f & len f<=len f & 1<=1 &
         1<=len f+1-i by A8,AXIOMS:22,NAT_1:38;
          1 in dom g by A49,A51,FINSEQ_3:27;
        then A55: g/.1=g.1 by FINSEQ_4:def 4
                .=f.(1-1+i) by A54,JORDAN3:31
                .=f/.i by FINSEQ_4:def 4,A10;
        reconsider ia=i+1 as Nat;
          ia in Seg len f by A8,A12,FINSEQ_1:3;
        then A56: i+1 in dom f by FINSEQ_1:def 3;
          1+1 in Seg len g by A52,FINSEQ_1:3;
        then 1+1 in dom g by FINSEQ_1:def 3;
        then A57: g/.(1+1)=g.(1+1) by FINSEQ_4:def 4
                    .=f.(1+1-1+i) by A8,A9,A53,JORDAN3:31
                    .=f/.(i+1) by A56,FINSEQ_4:def 4;
          LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A8
                .=LSeg(g,1) by TOPREAL1:def 5,A52,A55,A57;
        then p in Y &
          Y in { LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
                           by A6,A8,A52;
        then p in union{ LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <=
len g
}
            by TARSKI:def 4;
        then p in L~mid(f,i,len f) by TOPREAL1:def 6;
        then A58: LE p44,p,P,p1,p2 by SPRECT_4:4,A3,A8,A9;
        A59: for p5 being Point of TOP-REAL 2 st
         LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume A60: LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2;
             Segment(P,p1,p2,p44,p)={p8 where p8 is Point of TOP-REAL 2:
            LE p44,p8,P,p1,p2 & LE p8,p,P,p1,p2} by JORDAN6:29;
           then A61: p5 in Segment(P,p1,p2,p44,p) by A60;
            p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p44,p) c= LSeg(f,i) by A6,A8,A48,TOPREAL1:12;
           then A62: LSeg(p44,p) c= P by XBOOLE_1:1,A3,A8,A7;
             now per cases;
           case p44<>p;
             then LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;
             then Segment(P,p1,p2,p44,p)=LSeg(p44,p) by Th28,A58,A62,A2
;
            hence p5`1<=e by A1,A46,A61,TOPREAL1:9;
           end;
           case p44=p;
             then Segment(P,p1,p2,p44,p)={p44} by A2,Th1,A1;
            hence p5`1<=e by A46,A61,TARSKI:def 1;
           end;
           end;
          hence p5`1<=e;
         end;
        reconsider g=mid(f,1,i) as FinSequence of (TOP-REAL 2);
        A63: len g=i -'1+1 by A8,A11,A9,JORDAN3:27;
          i-1>0 by A45,SQUARE_1:11;
        then i-'1=i-1 by BINARITH:def 3;
        then A64: i-'1+1=i by XCMPLX_1:27;
        A65: i-'k <i by A20,A43,JORDAN3:14;
        A66: i-'k+1<=len g by A44,A63,A64,NAT_1:38;
        A67: 1<=(i-'k) by A18,A19,BINARITH:def 3;
        A68: 1<=1 & 1<=i & i<=len f & 1<=i-'k &
         i-'k<=i+1-1 by A8,A19,A18,A65,NAT_1:38,
             XCMPLX_1:26,BINARITH:def 3;
        A69: i-'k+1<=i by A44,NAT_1:38;
        reconsider ia=i+1 as Nat;
          i-'k in Seg len g by A20,A63,A64,A65,FINSEQ_1:3;
        then i-'k in dom g by FINSEQ_1:def 3;
        then A70: g/.(i-'k)=g.(i-'k) by FINSEQ_4:def 4
                    .=f.(i-'k-1+1) by A68,JORDAN3:31
                    .=f/.(i-'k) by A22,XCMPLX_1:27;
          LSeg(g,i-'k)=LSeg(g/.(i-'k),g/.(i-'k+1))
             by A20,A66,TOPREAL1:def 5;
          then pk in LSeg(g,i-'k) &
          LSeg(g,i-'k) in
          { LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
                           by A22,A63,A64,A67,A69,A70,TOPREAL1:6;
         then pk in union{ LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <=
len
g }
            by TARSKI:def 4;
         then pk in L~mid(f,1,i) by TOPREAL1:def 6;
         then A71: LE pk,p44,P,p1,p2 by A45,A9,A3,SPRECT_3:33;
         then A72: LE pk,p,P,p1,p2 by A58,JORDAN5C:13;
         A73: p44 in P by A71,JORDAN5C:def 3;
         A74: now assume not
           for p51 being Point of TOP-REAL 2 st
            LE pk,p51,P,p1,p2 & LE p51,p44,P,p1,p2 holds p51`1<=e;
           then consider p51 being Point of TOP-REAL 2 such that
           A75: LE pk,p51,P,p1,p2 & LE p51,p44,P,p1,p2 & p51`1>e;
             p51 in P by A75,JORDAN5C:def 3;
           then consider Y3 being set such that
           A76: p51 in Y3 & Y3 in
            { LSeg(f,i5) where i5 is Nat : 1 <= i5 & i5+1 <= len f }
                     by TARSKI:def 4,A5,A3;
           consider kk being Nat such that
           A77: Y3=LSeg(f,kk) &  (1 <= kk & kk+1 <= len f) by A76;
           A78: kk<len f by A77,NAT_1:38;
           then kk in Seg len f by A77,FINSEQ_1:3;
           then A79: kk in dom f by FINSEQ_1:def 3;
           A80: 1<kk+1 by A77,NAT_1:38;
           then kk+1 in Seg len f by A77,FINSEQ_1:3;
           then A81: kk+1 in dom f by FINSEQ_1:def 3;
           A82: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A77,TOPREAL1:def 5;
           A83: LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                     by A3,A76,A77,JORDAN5C:26;
             now per cases by A75,A76,A77,A82,Th2;
           case A84: (f/.kk)`1>e;
              set k2=i-'kk;
                  LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A3,A76,A77,JORDAN5C:25;
                then A85: LE f/.kk,p44,L~f,f/.1,f/.(len f)
                                 by A75,A3,JORDAN5C:13;
                now assume i-kk<=0; then i-kk+kk<=0+kk by
REAL_1:55;
                then kk>=i by XCMPLX_1:27;
                then LE f/.i,f/.kk,L~f,f/.1,f/.(len f)
                         by A3,A45,A78,JORDAN5C:24;
               hence contradiction by A2,A3,A46,A84,A85,JORDAN5C:12;
              end;
              then i-'kk=i-kk by BINARITH:def 3;
              then A86: i-k2=kk by XCMPLX_1:18;
              then A87: i-k2>0 by A77,AXIOMS:22;
              then A88: i-'k2>0 by BINARITH:def 3;
              A89: i-k2=i-'k2 by A87,BINARITH:def 3;
              A90: i-'k2>=0+1 by A88,NAT_1:38;
                i-'k2<=len f by A9,JORDAN3:13;
              then P[k2] by A89,A86,A84,FINSEQ_4:24,A90;
              then k2>=k by A15;
              then i-k2<=i-k by REAL_2:106;
              then A91: LE f/.(i-'k2),f/.(i-'k),L~f,f/.1,f/.(len f)
                           by JORDAN5C:24,A16,A86,A77,A3, A89,A23;
              A92: LE f/.(i-'k),p51,L~f,f/.1,f/.(len f) by A3,A21,A75,
FINSEQ_4:
def 4;
              A93: LE f/.(i-'k),f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A22,A75,A83,JORDAN5C:13;
              then A94: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A91,A86,A89,JORDAN5C:13;
                Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              then A95: f/.(i-'k) in Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+
1))
                                            by A91,A93,A86,A89;
              A96: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A97: z in LSeg(f/.kk,f/.(kk+1));
