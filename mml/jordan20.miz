theorem Th28: for P,Q1 being non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2 being Point of TOP-REAL 2 st
  P is_an_arc_of p1,p2 & Q1 is_an_arc_of q1,q2 &
  LE q1,q2,P,p1,p2 & Q1 c= P holds Q1= Segment(P,p1,p2,q1,q2)
proof let P,Q1 be non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2 be Point of TOP-REAL 2;
  assume A1: P is_an_arc_of p1,p2 & Q1 is_an_arc_of q1,q2 &
    LE q1,q2,P,p1,p2 & Q1 c= P;
    then A2: q1 in P by JORDAN5C:def 3;
    A3: q2 in P by A1,JORDAN5C:def 3;
    A4: p1 in P & p2 in P by A1,TOPREAL1:4;
    A5: q1<>q2 by A1,JORDAN6:49;
    then A6: Segment(P,p1,p2,q1,q2) is_an_arc_of q1,q2 by A1,JORDAN16:36;
    A7: now assume A8: q1=p2;
          LE q2,p2,P,p1,p2 by A1,A3,JORDAN5C:10;
     hence contradiction by A1,A5,A8,JORDAN5C:12;
    end;
    A9: now assume A10: q2=p1;
          LE p1,q1,P,p1,p2 by A1,A2,JORDAN5C:10;
     hence contradiction by A1,A5,A10,JORDAN5C:12;
    end;
    reconsider Q0=Segment(P,p1,p2,q1,q2) as non empty Subset of TOP-REAL 2
                     by A1,JORDAN16:22;
       now assume not Q1 c= Q0;
    then consider x8 being set such that
    A11: x8 in Q1 & not x8 in Q0 by TARSKI:def 3;
        Q0 is_an_arc_of q1,q2 by A1,A5,JORDAN16:36;
    then A12: q1 in Q0 & q2 in Q0 by TOPREAL1:4;
    reconsider q=x8 as Point of TOP-REAL 2 by A11;
    A13: q<>q1 by A1,A11,JORDAN16:9;
    A14: [#]I[01]=the carrier of I[01];
    A15: [#]((TOP-REAL 2)|P)=P by PRE_TOPC:def 10;
    A16: LE p1,q1,P,p1,p2 by A1,A2,JORDAN5C:10;
    then A17: Segment(P,p1,p2,p1,q1)\/ Segment(P,p1,p2,q1,q2)
                =Segment(P,p1,p2,p1,q2) by A1,Th23;
    A18: LE p1,q2,P,p1,p2 by A1,A16,JORDAN5C:13;
        LE q2,p2,P,p1,p2 by A1,A3,JORDAN5C:10;
    then Segment(P,p1,p2,p1,q2)\/Segment(P,p1,p2,q2,p2)=Segment(P,p1,p2,p1
,p2)
                               by A1,A18,Th23 .=P by A1,Th25;
    then A19: q in Segment(P,p1,p2,p1,q2) or q in Segment(P,p1,p2,q2,p2)
                               by A1,A11,XBOOLE_0:def 2;
        now per cases by A11,A17,A19,XBOOLE_0:def 2;
    case q in Segment(P,p1,p2,p1,q1);
      then q in {p3 where p3 is Point of TOP-REAL 2:
        LE p1,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;
      then consider p3 being Point of TOP-REAL 2 such that
      A20: q=p3 & (LE p1,p3,P,p1,p2 & LE p3,q1,P,p1,p2);
       A21: not q2 in {q1} by A5,TARSKI:def 1;
      A22: P\{q1} c= P by XBOOLE_1:36;
      reconsider Qa=P\{q1} as non empty Subset of (TOP-REAL 2)|P
                   by A3,A15,A21,XBOOLE_0:def 4,XBOOLE_1:36;
      reconsider Qa'=Qa as Subset of TOP-REAL 2;
      A23: now assume A24: p1=q1;
      then q=p1 by A1,A20,JORDAN5C:12;
       hence contradiction by A6,A11,A24,TOPREAL1:4;
      end;
      A25: the carrier of ((TOP-REAL 2)|P)|Qa=Qa by PRE_TOPC:29;
      A26: not p1 in {q1} by A23,TARSKI:def 1;
      A27:  not p2 in {q1} by A7,TARSKI:def 1;
      A28: not q2 in {q1} by A5,TARSKI:def 1;
          not q in {q1} by A13,TARSKI:def 1;
      then reconsider p1'=p1,q'=q,q2'=q2,p2'=p2 as Point of ((TOP-REAL 2)|P)|Qa
                       by A1,A3,A4,A11,A25,A26,A27,A28,XBOOLE_0:def 4;
          now per cases;
      case q<>p1;
      then A29: Segment(P,p1,p2,p1,q) is_an_arc_of p1,q by A1,A11,JORDAN16:39;
      then consider f1 being Function
 of I[01],(TOP-REAL 2)|Segment(P,p1,p2,p1,q) such that
      A30: f1 is_homeomorphism & f1.0=p1 & f1.1=q by TOPREAL1:def 2;
      A31: f1 is continuous by A30,TOPS_2:def 5;
      A32: dom f1=the carrier of I[01] by A14,A30,TOPS_2:def 5;
      A33: rng f1=[#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q)) by A30,TOPS_2:def 5
         .=Segment(P,p1,p2,p1,q) by PRE_TOPC:def 10;
          {p where p is Point of TOP-REAL 2: LE p1,p,P,p1,p2 & LE p,q,P,p1,p2
}
                      c= Qa
       proof let x be set;assume x in
        {p where p is Point of TOP-REAL 2: LE p1,p,P,p1,p2 & LE p,q,P,p1,p2};
        then consider p being Point of TOP-REAL 2 such that
         A34: x=p & (LE p1,p,P,p1,p2 & LE p,q,P,p1,p2);
             x<>q1 by A1,A13,A20,A34,JORDAN5C:12;
         then x in P & not x in {q1} by A34,JORDAN5C:def 3,TARSKI:def 1;
        hence x in Qa by XBOOLE_0:def 4;
       end;
      then A35: Segment(P,p1,p2,p1,q) c= Qa by JORDAN6:29;
      then reconsider g1=f1 as Function
 of I[01],((TOP-REAL 2)|P)|Qa by A25,A32,A33,FUNCT_2:4;
A:    [#](((TOP-REAL 2)|P)|Qa) <> {};
          for G being Subset of ((TOP-REAL 2)|P)|Qa st G is open
        holds g1"G is open
       proof let G be Subset of ((TOP-REAL 2)|P)|Qa;
        assume A36: G is open;
             ((TOP-REAL 2)|P)|Qa=(TOP-REAL 2)|Qa' by A22,PRE_TOPC:28;
         then consider G4 being Subset of (TOP-REAL 2) such that
         A37: G4 is open &
         G=G4 /\ [#]((TOP-REAL 2)|Qa') by A36,TOPS_2:32;
         reconsider G5=G4 /\ [#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q))
            as Subset of ((TOP-REAL 2)|Segment(P,p1,p2,p1,q));
         p1 in Segment(P,p1,p2,p1,q) by A29,TOPREAL1:4;
         then A:[#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q)) is non empty
                        by PRE_TOPC:def 10;
         A39: G5 is open by A37,TOPS_2:32;
         A40: f1"G5=g1"(G4 /\ Segment(P,p1,p2,p1,q)) by PRE_TOPC:def 10
             .=g1"(G4) /\ g1"(Segment(P,p1,p2,p1,q)) by FUNCT_1:137;
         A41: rng g1=[#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q)) by A30,
TOPS_2:def 5
              .= Segment(P,p1,p2,p1,q) by PRE_TOPC:def 10;
        g1"G=g1"G4 /\ g1"([#]((TOP-REAL 2)|Qa')) by A37,FUNCT_1:137
             .=g1"G4 /\ g1"Qa' by PRE_TOPC:def 10
             .=g1"G4 /\ g1"((rng g1) /\ Qa') by RELAT_1:168
             .=g1"G4 /\ g1"(Segment(P,p1,p2,p1,q)) by A35,A41,XBOOLE_1:28;
        hence g1"G is open by A,A31,A39,A40,TOPS_2:55;
       end;
       then A42: g1 is continuous by A,TOPS_2:55;
       then p1',q' are_connected by A30,BORSUK_2:def 1;
       then g1 is Path of p1',q' by A30,A42,BORSUK_2:def 2;
       hence ex G1 being Path of p1',q' st
         G1 is continuous & G1.0=p1' & G1.1=q' by A30,A42;
      end;
      case A43: q=p1;
        consider g01 being Function of I[01],(((TOP-REAL 2)|P)|Qa) such that
        A44: g01 is continuous & g01.0=p1' & g01.1=p1' by BORSUK_2:4;
        p1',p1' are_connected;
       then g01 is Path of p1',p1' by A44,BORSUK_2:def 2;
       hence ex G1 being Path of p1',q' st
           G1 is continuous & G1.0=p1' & G1.1=q' by A43,A44;
      end;
      end;
      then consider G1 being Path of p1',q' such that
      A45: G1 is continuous & G1.0=p1' & G1.1=q';
      A46: Segment(Q1,q1,q2,q,q2) is_an_arc_of q,q2
                       by A1,A11,A12,Th22;
      then consider f2 being Function
 of I[01],(TOP-REAL 2)|Segment(Q1,q1,q2,q,q2)
         such that
      A47: f2 is_homeomorphism & f2.0=q & f2.1=q2 by TOPREAL1:def 2;
      A48: f2 is continuous by A47,TOPS_2:def 5;
      A49: dom f2=the carrier of I[01] by A14,A47,TOPS_2:def 5;
      A50: rng f2=[#]((TOP-REAL 2)|Segment(Q1,q1,q2,q,q2)) by A47,TOPS_2:def 5
         .=Segment(Q1,q1,q2,q,q2) by PRE_TOPC:def 10;
      A51: the carrier of ((TOP-REAL 2)|P)|Qa=Qa by PRE_TOPC:29;
          {p where p is Point of TOP-REAL 2: LE q,p,Q1,q1,q2 & LE p,q2,Q1,q1,
q2}
