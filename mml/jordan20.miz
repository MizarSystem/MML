      ,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;
      then
A16:  ex p3 being Point of TOP-REAL 2 st p=p3 & LE q2,p3,P,p1, p2 & LE p3,
      q1,P,p1,p2 by A4,A14;
      then
A17:  LE p,p4,P,p1,p2 by A9,JORDAN5C:13;
A18:  for p6 being Point of TOP-REAL 2 st LE p6,p4,P,p1,p2 & LE p,p6,P,p1
      ,p2 holds p6`1>=e
      proof
        let p6 be Point of TOP-REAL 2;
        assume that
A19:    LE p6,p4,P,p1,p2 and
A20:    LE p,p6,P,p1,p2;
A21:    p6 in P by A19,JORDAN5C:def 3;
        now
          per cases by A11,A5,A21,Th19;
          case
            LE q1,p6,P,p1,p2;
            hence thesis by A10,A19;
          end;
          case
A22:        LE p6,q1,P,p1,p2;
            LE q2,p6,P,p1,p2 by A16,A20,JORDAN5C:13;
            then p6 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1, p2
            & LE qq,q1,P,p1,p2} by A22;
            then p6 in LSeg(q2,q1) by A14,JORDAN6:29;
            hence thesis by A2,A7,GOBOARD7:5;
          end;
        end;
        hence thesis;
      end;
      p`1=e by A2,A4,A7,GOBOARD7:5;
      hence thesis by A3,A4,A11,A8,A17,A18,Def4;
    end;
    case
A23:  LE q1,q2,P,p1,p2;
A24:  now
        per cases;
        case
          q1<>q2;
          then LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
          hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A3,A11,A23,Th28;
        end;
        case
A25:      q1=q2;
          then LSeg(q2,q1)={q1} by RLTOPSP1:71;
          hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by A11,A5,A25,Th1;
        end;
      end;
A26:  now
        assume LE p4,q2,P,p1,p2;
        then p4 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,p2 & LE
        qq,q2,P,p1,p2} by A9;
        then p4 in Segment(P,p1,p2,q1,q2) by JORDAN6:29;
        hence contradiction by A2,A7,A8,A24,GOBOARD7:5;
      end;
      Segment(P,p1,p2,q1,q2) = {p3 where p3 is Point of TOP-REAL 2: LE q1
      ,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;
      then
A27:  ex p3 being Point of TOP-REAL 2 st p=p3 & LE q1,p3,P,p1, p2 & LE p3,
      q2,P,p1,p2 by A4,A24;
A28:  for p6 being Point of TOP-REAL 2 st LE p6,p4,P,p1,p2 & LE p,p6,P,p1
      ,p2 holds p6`1>=e
      proof
        let p6 be Point of TOP-REAL 2;
        assume that
A29:    LE p6,p4,P,p1,p2 and
A30:    LE p,p6,P,p1,p2;
        LE q1,p6,P,p1,p2 by A27,A30,JORDAN5C:13;
        hence thesis by A10,A29;
      end;
      LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by A3,A11,A6,A12,Th19;
      then
A31:  LE p,p4,P,p1,p2 by A27,A26,JORDAN5C:13;
      p`1=e by A2,A4,A7,GOBOARD7:5;
      hence thesis by A3,A4,A11,A8,A31,A28,Def4;
    end;
  end;
  hence thesis;
end;

theorem
  for P being non empty Subset of TOP-REAL 2, p1,p2,p being Point of
TOP-REAL 2,e being Real st P is_S-P_arc_joining p1,p2 & p1`1<e & p in P & p`1=e
  holds p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e
proof
  let P be non empty Subset of TOP-REAL 2, p1,p2,p be Point of TOP-REAL 2,e be
  Real;
  assume that
A1: P is_S-P_arc_joining p1,p2 and
A2: p1`1<e and
A3: p in P and
A4: p`1=e;
  consider f being FinSequence of (TOP-REAL 2) such that
A5: f is being_S-Seq and
A6: P = L~f and
A7: p1=f/.1 and
A8: p2=f/.len f by A1,TOPREAL4:def 1;
A9: P is_an_arc_of p1,p2 by A1,TOPREAL4:3;
  len f >= 2 by A5,TOPREAL1:def 10;
  then
A10: len f >1 by XXREAL_0:2;
A11: L~f = union { LSeg(f,i) where i is Element of NAT : 1 <= i & i+1 <= len
  f } by TOPREAL1:def 6;
  then consider Y being set such that
A12: p in Y and
A13: Y in { LSeg(f,i) where i is Element of NAT : 1 <= i & i+1 <= len f
  } by A3,A6,TARSKI:def 4;
  consider i being Element of NAT such that
A14: Y=LSeg(f,i) and
A15: 1 <= i and
A16: i+1 <= len f by A13;
A17: i-1>=0 by A15,XREAL_1:50;
A18: 1<i+1 by A15,NAT_1:13;
A19: Y c= L~f
  proof
    let x be set;
    assume x in Y;
    hence thesis by A11,A13,TARSKI:def 4;
  end;
  defpred P[Nat] means for p being Point of TOP-REAL 2 st p=f.(i-'$1) holds p
  `1<>e;
A20: i<len f by A16,NAT_1:13;
  then
A21: i in dom f by A15,FINSEQ_3:27;
A22: 1<len f by A15,A20,XXREAL_0:2;
  then 1 in dom f by FINSEQ_3:27;
  then f/.1=f.1 by PARTFUN1:def 8;
  then
A23: P[i-'1] by A2,A7,A15,NAT_D:58;
  then
A24: ex k being Nat st P[k];
  ex k being Nat st P[k] & for n being Nat st P[n] holds k <= n from
  NAT_1:sch 5(A24);
  then consider k being Nat such that
A25: P[k] and
A26: for n being Nat st P[n] holds k <= n;
  k<=i-'1 by A23,A26;
  then k<=i-1 by A17,XREAL_0:def 2;
  then k+1<=i-1+1 by XREAL_1:9;
  then
A27: 1+k-k<=i-k by XREAL_1:11;
  then
A28: i-'k>=1 by XREAL_0:def 2;
  i-'k <=i by NAT_D:35;
  then
A29: i-'k <len f by A20,XXREAL_0:2;
  then
A30: i-'k in dom f by A28,FINSEQ_3:27;
  then
A31: f/.(i-'k)=f.(i-'k) by PARTFUN1:def 8;
  then reconsider pk=f.(i-'k) as Point of TOP-REAL 2;
A32: i-'k=i-k by A27,XREAL_0:def 2;
  now
    per cases by A25,XXREAL_0:1;
    case
A33:  pk`1<e;
      now
        per cases;
        case
A34:      k=0;
          set p44=f/.i;
A35:      pk=f.i by A34,NAT_D:40
            .=p44 by A21,PARTFUN1:def 8;
          reconsider ia=i+1 as Element of NAT;
          reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
A36:      i<=len f by A16,NAT_1:13;
          ia in Seg len f by A16,A18,FINSEQ_1:3;
          then
A37:      i+1 in dom f by FINSEQ_1:def 3;
          1+(1+i)<=1+len f by A16,XREAL_1:9;
          then
A38:      1+1+i-i<=len f+1-i by XREAL_1:11;
          then
A39:      1<=len f+1-i by XXREAL_0:2;
A40:      len f-i>0 by A20,XREAL_1:52;
          then len f-'i=len f-i by XREAL_0:def 2;
          then
A41:      len f-'i+1>0+1 by A40,XREAL_1:10;
A42:      len g=len f -'i+1 by A10,A15,A20,FINSEQ_6:124;
          then
A43:      1+1<=len g by A41,NAT_1:13;
          then 1+1 in Seg len g by FINSEQ_1:3;
          then 1+1 in dom g by FINSEQ_1:def 3;
          then
A44:      g/.(1+1)=g.(1+1) by PARTFUN1:def 8
            .=f.(1+1-1+i) by A15,A20,A38,FINSEQ_6:128
            .=f/.(i+1) by A37,PARTFUN1:def 8;
          1 in dom g by A42,A41,FINSEQ_3:27;
          then
A45:      g/.1=g.1 by PARTFUN1:def 8
            .=f.(1-1+i) by A15,A36,A39,FINSEQ_6:128
            .=f/.i by A21,PARTFUN1:def 8;
          LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A15,A16,TOPREAL1:def 5
            .=LSeg(g,1) by A43,A45,A44,TOPREAL1:def 5;
          then Y in { LSeg(g,i2) where i2 is Element of NAT : 1 <= i2 & i2+1
          <= len g } by A14,A43;
          then p in union{ LSeg(g,i2) where i2 is Element of NAT : 1 <= i2 &
          i2+1 <= len g } by A12,TARSKI:def 4;
          then
A46:      p in L~mid(f,i,len f) by TOPREAL1:def 6;
A47:      LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A15,A16,TOPREAL1:def 5;
A48:      for p5 being Point of TOP-REAL 2 st LE p44,p5,P,p1,p2 & LE p5,p
          ,P,p1,p2 holds p5`1<=e
          proof
            p44 in LSeg(f/.i,f/.(i+1)) by RLTOPSP1:69;
            then LSeg(p44,p) c= LSeg(f,i) by A12,A14,A47,TOPREAL1:12;
            then
A49:        LSeg(p44,p) c= P by A6,A19,A14,XBOOLE_1:1;
            let p5 be Point of TOP-REAL 2;
A50:        Segment(P,p1,p2,p44,p)={p8 where p8 is Point of TOP-REAL 2:
            LE p44,p8,P,p1,p2 & LE p8,p,P,p1,p2} by JORDAN6:29;
            assume LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2;
            then
A51:        p5 in Segment(P,p1,p2,p44,p) by A50;
            now
              per cases;
              case
                p44<>p;
                then LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;
                then Segment(P,p1,p2,p44,p)=LSeg(p44,p) by A9,A5,A6,A7,A8,A15
,A20,A46,A49,Th28,SPRECT_4:4;
                hence thesis by A4,A33,A35,A51,TOPREAL1:9;
              end;
              case
                p44=p;
                hence thesis by A4,A33,A35;
              end;
            end;
            hence thesis;
          end;
          LE p44,p,P,p1,p2 by A5,A6,A7,A8,A15,A20,A46,SPRECT_4:4;
          hence thesis by A3,A4,A9,A33,A35,A48,Def1;
        end;
        case
A52:      k<>0;
          reconsider ia=i+1 as Element of NAT;
          reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
A53:      i<=len f by A16,NAT_1:13;
          ia in Seg len f by A16,A18,FINSEQ_1:3;
          then
A54:      i+1 in dom f by FINSEQ_1:def 3;
          1+(1+i)<=1+len f by A16,XREAL_1:9;
          then
A55:      1+1+i-i<=len f+1-i by XREAL_1:11;
          then
A56:      1<=len f+1-i by XXREAL_0:2;
A57:      len f-i>0 by A20,XREAL_1:52;
          then len f-'i=len f-i by XREAL_0:def 2;
          then
A58:      len f-'i+1>0+1 by A57,XREAL_1:10;
A59:      len g=len f -'i+1 by A10,A15,A20,FINSEQ_6:124;
          then
A60:      1+1<=len g by A58,NAT_1:13;
          then 1+1 in Seg len g by FINSEQ_1:3;
          then 1+1 in dom g by FINSEQ_1:def 3;
          then
A61:      g/.(1+1)=g.(1+1) by PARTFUN1:def 8
            .=f.(1+1-1+i) by A15,A20,A55,FINSEQ_6:128
            .=f/.(i+1) by A54,PARTFUN1:def 8;
          1 in dom g by A59,A58,FINSEQ_3:27;
          then
A62:      g/.1=g.1 by PARTFUN1:def 8
            .=f.(1-1+i) by A15,A53,A56,FINSEQ_6:128
            .=f/.i by A21,PARTFUN1:def 8;
          LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A15,A16,TOPREAL1:def 5
            .=LSeg(g,1) by A60,A62,A61,TOPREAL1:def 5;
          then Y in { LSeg(g,i2) where i2 is Element of NAT : 1 <= i2 & i2+1
          <= len g } by A14,A60;
          then p in union{ LSeg(g,i2) where i2 is Element of NAT : 1 <= i2 &
          i2+1 <= len g} by A12,TARSKI:def 4;
          then
A63:      p in L~mid(f,i,len f) by TOPREAL1:def 6;
          reconsider g=mid(f,1,i) as FinSequence of (TOP-REAL 2);
          set p44=f/.i;
A64:      i<=len f & 1<=i-'k by A16,A27,NAT_1:13,XREAL_0:def 2;
A65:      k>=0+1 by A52,NAT_1:13;
          then
A66:      i-'k<=i+1-1 by A28,NAT_D:51;
A67:      i>i-'k by A28,A65,NAT_D:51;
          then
A68:      i>1 by A28,XXREAL_0:2;
          then i-1>0 by XREAL_1:52;
          then
A69:      i-'1=i-1 by XREAL_0:def 2;
A70:      now
            assume
A71:        (f/.i)`1<>e;
            f.i=f/.i by A21,PARTFUN1:def 8;
            then
            for p9 being Point of TOP-REAL 2 st p9=f.(i-'0) holds p9`1<>e
            by A71,NAT_D:40;
            hence contradiction by A26,A52;
          end;
A72:      now
            assume not for p51 being Point of TOP-REAL 2 st LE pk,p51,P,p1,
            p2 & LE p51,p44,P,p1,p2 holds p51`1<=e;
            then consider p51 being Point of TOP-REAL 2 such that
A73:        LE pk,p51,P,p1,p2 and
A74:        LE p51,p44,P,p1,p2 and
A75:        p51`1>e;
            p51 in P by A73,JORDAN5C:def 3;
            then consider Y3 being set such that
A76:        p51 in Y3 and
A77:        Y3 in { LSeg(f,i5) where i5 is Element of NAT : 1 <= i5
            & i5+1 <= len f } by A6,A11,TARSKI:def 4;
            consider kk being Element of NAT such that
A78:        Y3=LSeg(f,kk) and
A79:        1 <= kk and
A80:        kk+1 <= len f by A77;
A81:        LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A79,A80,TOPREAL1:def 5;
            1<kk+1 by A79,NAT_1:13;
            then kk+1 in Seg len f by A80,FINSEQ_1:3;
            then
A82:        kk+1 in dom f by FINSEQ_1:def 3;
A83:        kk<len f by A80,NAT_1:13;
            then kk in Seg len f by A79,FINSEQ_1:3;
            then
A84:        kk in dom f by FINSEQ_1:def 3;
A85:        LE p51,f/.(kk+1),L~f,f/.1,f/.(len f) by A5,A76,A78,A79,A80,
JORDAN5C:26;
            now
              per cases by A75,A76,A78,A81,Th2;
              case
A86:            (f/.kk)`1>e;
A87:            LSeg(f/.kk,f/.(kk+1)) c= L~f
                proof
                  let z be set;
                  assume
A88:              z in LSeg(f/.kk,f/.(kk+1));
                  LSeg(f/.kk,f/.(kk+1)) in { LSeg(f,i7) where i7 is
                  Element of NAT : 1 <= i7 & i7+1 <= len f } by A79,A80,A81;
                  hence thesis by A11,A88,TARSKI:def 4;
                end;
                f is special by A5,TOPREAL1:def 10;
                then
A89:            (f/.kk)`1=(f/.(kk+1))`1 or (f/.kk)`2=(f/.(kk+1))`2 by A79,A80,
TOPREAL1:def 7;
                f is one-to-one & kk<kk+1 by A5,NAT_1:13,TOPREAL1:def 10;
                then
A90:            f.kk <> f.(kk+1) by A84,A82,FUNCT_1:def 8;
A91:            LE f/.(i-'k),p51,L~f,f/.1,f/.(len f) by A6,A7,A8,A30,A73,
PARTFUN1:def 8;
A92:            LE f/.(i-'k),f/.(kk+1),L~f,f/.1,f/.(len f) by A6,A7,A8,A31,A73
,A85,JORDAN5C:13;
                set k2=i-'kk;
                LE f/.kk,p51,L~f,f/.1,f/.(len f) by A5,A76,A78,A79,A80,
JORDAN5C:25;
                then
A93:            LE f/.kk,p44,L~f,f/.1,f/.(len f) by A6,A7,A8,A74,JORDAN5C:13;
                now
                  assume i-kk<=0;
                  then i-kk+kk<=0+kk by XREAL_1:9;
                  then LE f/.i,f/.kk,L~f,f/.1,f/.(len f) by A5,A68,A83,
JORDAN5C:24;
                  hence contradiction by A1,A6,A7,A8,A70,A86,A93,JORDAN5C:12
,TOPREAL4:3;
                end;
                then
A94:            i-'kk=i-kk by XREAL_0:def 2;
                then
A95:            i-k2=i-'k2 by XREAL_0:def 2;
                i-k2>0 by A79,A94;
                then i-'k2>0 by XREAL_0:def 2;
                then i-'k2>=0+1 by NAT_1:13;
                then P[k2] by A20,A86,A94,A95,FINSEQ_4:24,NAT_D:50;
                then k2>=k by A26;
                then i-k2<=i-k by XREAL_1:12;
                then
A96:            LE f/.(i-'k2),f/.(i-'k),L~f,f/.1,f/.(len f ) by A5,A29,A32,A79
,A94,A95,JORDAN5C:24;
                f/.kk=f.kk & f/.(kk+1)=f.(kk+1) by A84,A82,PARTFUN1:def 8;
                then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1) by A90,
TOPREAL1:15;
                then
A97:            Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1 )) =LSeg(f/.
kk,f/.(kk+1)) by A9,A6,A7,A8,A94,A95,A96,A92,A87,Th28,JORDAN5C:13;
                Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1)) ={p8 where
p8 is Point of TOP-REAL 2: LE f/.kk,p8,L~f,f/.1,f/.(len f) & LE p8,f/.(kk+1),L~
                f,f/.1,f/.(len f)} by JORDAN6:29;
                then
A98:            f/.(i-'k) in Segment(L~f,f/.1,f/.(len f),f /.kk,f/.(kk+
                1)) by A94,A95,A96,A92;
                then (f/.(kk+1))`1<e by A31,A33,A86,A97,Th3;
                then (f/.kk)`1> (f/.(kk+1))`1 by A86,XXREAL_0:2;
                then (f/.(i-'k))`1>=p51`1 by A5,A76,A78,A79,A83,A81,A91,A98,A97
,A89,Th6;
                hence contradiction by A31,A33,A75,XXREAL_0:2;
              end;
              case
A99:            (f/.(kk+1))`1 >e & (f/.kk)`1<=e;
                set k2=i-'kk-'1;
A100:           LSeg(f/.kk,f/.(kk+1)) c= L~f
                proof
                  let z be set;
                  assume
A101:             z in LSeg(f/.kk,f/.(kk+1));
                  LSeg(f/.kk,f/.(kk+1)) in { LSeg(f,i7) where i7 is
                  Element of NAT : 1 <= i7 & i7+1 <= len f } by A79,A80,A81;
                  hence thesis by A11,A101,TARSKI:def 4;
                end;
                f is one-to-one & kk<kk+1 by A5,NAT_1:13,TOPREAL1:def 10;
                then
A102:           f.kk <> f.(kk+1) by A84,A82,FUNCT_1:def 8;
A103:           (f/.kk)`1<(f/.(kk+1))`1 by A99,XXREAL_0:2;
                LE f/.kk,p51,L~f,f/.1,f/.(len f) by A5,A76,A78,A79,A80,
JORDAN5C:25;
                then
A104:           LE f/.kk,p44,L~f,f/.1,f/.(len f) by A6,A7,A8,A74,JORDAN5C:13;
                f/.kk=f.kk & f/.(kk+1)=f.(kk+1) by A84,A82,PARTFUN1:def 8;
                then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1) by A102
,TOPREAL1:15;
                then
A105:           Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1)) ={p8 where
p8 is Point of TOP-REAL 2: LE f/.kk,p8,L~f,f/.1,f/.(len f) & LE p8,f/.(kk+1),L~
f,f/.1,f/.(len f)} & Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1 )) =LSeg(f/.kk,
f/.(kk+1)) by A9,A5,A6,A7,A8,A79,A80,A100,Th28,JORDAN5C:23,JORDAN6:29;
A106:           now
                  assume i-kk-1<=0;
                  then i-(kk+1)+(kk+1)<=0+(kk+1) by XREAL_1:9;
                  then LE f/.i,f/.(kk+1),L~f,f/.1,f/.(len f) by A5,A68,A80,
JORDAN5C:24;
                  then
A107:             f/.i in LSeg(f/.kk,f/.(kk+1)) by A105,A104;
                  f is special by A5,TOPREAL1:def 10;
                  then
A108:             (f/.kk)`1=(f/.(kk+1))`1 or (f/.kk)`2=(f/.(kk+1))`2 by A79,A80
,TOPREAL1:def 7;
                  LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A79,A80,TOPREAL1:def 5;
                  hence contradiction by A5,A6,A7,A8,A70,A74,A75,A76,A78,A79
,A83,A103,A107,A108,Th7;
                end;
                then i-kk-1+1>=0+1 by XREAL_1:9;
                then i-'kk=i-kk by XREAL_0:def 2;
                then
