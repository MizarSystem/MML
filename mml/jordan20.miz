Th37: s in LSeg(p,q) & p`1 <= q`1 implies p`1 <= s`1 & s`1 <= q`1
     by TOPREAL1:9;

Th37b:
   s in LSeg(p,q) & p`1 >= q`1 implies p`1 >= s`1 & s`1 >= q`1 by Th37;

theorem JJ80: for f being S-Sequence_in_R2, p,q being Point of TOP-REAL 2 st
   1 <=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) &
   (f/.j)`2=(f/.(j+1))`2 & (f/.j)`1>(f/.(j+1))`1 &
   LE p,q,L~f,f/.1,f/.(len f)
  holds p`1>=q`1
proof let f be S-Sequence_in_R2, p,q be Point of TOP-REAL 2;
 assume A1: 1 <=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) &
   (f/.j)`2=(f/.(j+1))`2 & (f/.j)`1>(f/.(j+1))`1 &
   LE p,q,L~f,f/.1,f/.(len f);
  A7: L~f is_an_arc_of f/.1,f/.len f by TOPREAL1:31;
     j+1<=len f by A1,NAT_1:38;then
     B7: LSeg(f,j)=LSeg(f/.j,f/.(j+1)) by A1,TOPREAL1:def 5;
     per cases;
     suppose C0: p`1<> (f/.j)`1;
     (f/.j)`1>=p`1 by Th37,A1,B7;then
     (f/.j)`1>p`1 by C0,REAL_1:def 5;then
     B3b: (f/.j)`1-p`1>0 by SQUARE_1:11;
   now assume B1: p`1<q`1;then
     B2: q`1-p`1>0 by SQUARE_1:11;
     B8: p`2=(f/.j)`2 by A1,B7,GOBOARD7:6;
     B9: q`2=(f/.j)`2 by B7,A1,GOBOARD7:6;
     reconsider a=((f/.j)`1-q`1)/((f/.j)`1-p`1) as Real by XREAL_0:def 1;
     B33: (1-a)=((f/.j)`1-p`1)/((f/.j)`1-p`1)- ((f/.j)`1-q`1)/((f/.j)`1-p`1)
                         by B3b,XCMPLX_1:60
         .=((f/.j)`1-p`1- ((f/.j)`1-q`1))/((f/.j)`1-p`1) by XCMPLX_1:121
         .=(q`1-p`1)/((f/.j)`1-p`1) by XCMPLX_1:23;
     B10: ((1-a)*(f/.j)+a*p)`1=((1-a)*(f/.j))`1+(a*p)`1 by TOPREAL3:7
     .=(1-a)*(f/.j)`1+ (a*p)`1 by TOPREAL3:9
     .=(1-a)*(f/.j)`1+ a*(p`1) by TOPREAL3:9
     .=1* (f/.j)`1 -a*(f/.j)`1 +a*(p`1) by XCMPLX_1:40
     .= (f/.j)`1 -(a*(f/.j)`1 -a*(p`1)) by XCMPLX_1:37
     .= (f/.j)`1 -(a*((f/.j)`1 -(p`1))) by XCMPLX_1:40
     .= (f/.j)`1 -((f/.j)`1-q`1) by B3b,XCMPLX_1:88
     .= (f/.j)`1 -(f/.j)`1+q`1 by XCMPLX_1:37
     .=0+q`1 by XCMPLX_1:14 .=q`1;
     B11: ((1-a)*(f/.j)+a*p)`2=((1-a)*(f/.j))`2+(a*p)`2 by TOPREAL3:7
     .=(1-a)*(f/.j)`2+ (a*p)`2 by TOPREAL3:9
     .=(1-a)*(f/.j)`2+ a*(p`2) by TOPREAL3:9
     .=1* (f/.j)`2 -a*(f/.j)`2 +a*(p`2) by XCMPLX_1:40
     .= (f/.j)`2 -(a*(f/.j)`2 -a*(p`2)) by XCMPLX_1:37
     .= (f/.j)`2 -(a*0) by B8,XCMPLX_1:14
     .=q`2 by B9;
     (f/.j)`1>=q`1 & q`1>=(f/.(j+1))`1 by B7,A1,Th37;then
     (f/.j)`1-q`1>=0 by SQUARE_1:12;then
     B34: a>=0 by B3b,REAL_2:125;
     (1-a)>=0 by B33,B2,B3b,REAL_2:125;then
     1-a+a>=0+a by REAL_1:55;then
     B2: 0<=a & a<=1 by B34,XCMPLX_1:27;
     q=(1-a)*(f/.j)+a*p by B10,B11,TOPREAL3:11;then
     q in { (1-lambda)*(f/.j) + lambda*p where lambda is Real :
          0 <= lambda & lambda <= 1 } by B2;then
     q in LSeg(f/.j,p) by TOPREAL1:def 4;then
     LE q,p,L~f,f/.1,f/.(len f) by A1,SPRECT_3:40;
    hence contradiction by B1,JORDAN5C:12,A1,A7;
   end;
 hence p`1>=q`1;
 end;
 suppose p`1=(f/.j)`1;
 hence p`1>=q`1 by A1,Th37,B7;
 end;
end;

theorem JJ80b: for f being S-Sequence_in_R2, p,q being Point of TOP-REAL 2 st
   1 <=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) &
   (f/.j)`2=(f/.(j+1))`2 & (f/.j)`1<(f/.(j+1))`1 &
   LE p,q,L~f,f/.1,f/.(len f)
  holds p`1<=q`1
proof let f be S-Sequence_in_R2, p,q be Point of TOP-REAL 2;
 assume A1: 1 <=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) &
   (f/.j)`2=(f/.(j+1))`2 & (f/.j)`1<(f/.(j+1))`1 &
   LE p,q,L~f,f/.1,f/.(len f);
  A7: L~f is_an_arc_of f/.1,f/.len f by TOPREAL1:31;
     j+1<=len f by A1,NAT_1:38;then
     B7: LSeg(f,j)=LSeg(f/.j,f/.(j+1)) by A1,TOPREAL1:def 5;
     per cases;
     suppose C0: p`1<> (f/.(j))`1;
     (f/.j)`1<=p`1 & p`1<=(f/.(j+1))`1 by Th37,A1,B7;then
     (f/.(j))`1<p`1 by C0,REAL_1:def 5;then
     B3b: (f/.(j))`1 -p`1<0 by REAL_2:105;
   now assume B1: p`1>q`1;then
     B2: q`1-p`1<0 by REAL_2:105;
     B8: p`2=(f/.j)`2 by A1,B7,GOBOARD7:6;
     B9: q`2=(f/.j)`2 by B7,A1,GOBOARD7:6;
     reconsider a=((f/.(j))`1-q`1)/((f/.(j))`1-p`1)
                             as Real by XREAL_0:def 1;
     B33: (1-a)=((f/.(j))`1-p`1)/((f/.(j))`1-p`1)
     - ((f/.(j))`1-q`1)/((f/.(j))`1-p`1)
                         by B3b,XCMPLX_1:60
         .=((f/.(j))`1-p`1- ((f/.(j))`1-q`1))/((f/.(j))`1-p`1)
                         by XCMPLX_1:121
         .=(q`1-p`1)/((f/.(j))`1-p`1) by XCMPLX_1:23;
     B10: ((1-a)*(f/.(j))+a*p)`1=((1-a)*(f/.(j)))`1+(a*p)`1 by TOPREAL3:7
     .=(1-a)*(f/.(j))`1+ (a*p)`1 by TOPREAL3:9
     .=(1-a)*(f/.(j))`1+ a*(p`1) by TOPREAL3:9
     .=1* (f/.(j))`1 -a*(f/.(j))`1 +a*(p`1) by XCMPLX_1:40
     .= (f/.(j))`1 -(a*(f/.(j))`1 -a*(p`1)) by XCMPLX_1:37
     .= (f/.(j))`1 -(a*((f/.(j))`1 -(p`1))) by XCMPLX_1:40
     .= (f/.(j))`1 -((f/.(j))`1-q`1) by B3b,XCMPLX_1:88
     .= (f/.(j))`1 -(f/.(j))`1+q`1 by XCMPLX_1:37
     .=0+q`1 by XCMPLX_1:14 .=q`1;
     B11: ((1-a)*(f/.(j))+a*p)`2=((1-a)*(f/.(j)))`2+(a*p)`2 by TOPREAL3:7
     .=(1-a)*(f/.(j))`2+ (a*p)`2 by TOPREAL3:9
     .=(1-a)*(f/.(j))`2+ a*(p`2) by TOPREAL3:9
     .=1* (f/.(j))`2 -a*(f/.(j))`2 +a*(p`2) by XCMPLX_1:40
     .= (f/.(j))`2 -(a*(f/.(j))`2 -a*(p`2)) by XCMPLX_1:37
     .= (f/.(j))`2 -(a*0) by B8,XCMPLX_1:14
     .=q`2 by B9;
     (f/.j)`1<=q`1 & q`1<=(f/.(j+1))`1 by B7,A1,Th37b;then
     (f/.(j))`1-q`1<=0 by REAL_2:106;then
     B34: a>=0 by B3b,REAL_2:125;
     (1-a)>=0 by B33,B2,B3b,REAL_2:125;then
     1-a+a>=0+a by REAL_1:55;then
     B2: 0<=a & a<=1 by B34,XCMPLX_1:27;
     q=(1-a)*(f/.(j))+a*p by B10,B11,TOPREAL3:11;then
     q in { (1-lambda)*(f/.(j)) + lambda*p where lambda is Real :
          0 <= lambda & lambda <= 1 } by B2;then
     q in LSeg(f/.(j),p) by TOPREAL1:def 4;then
     LE q,p,L~f,f/.1,f/.(len f) by A1,SPRECT_3:40;
    hence contradiction by B1,JORDAN5C:12,A1,A7;
   end;
 hence p`1<=q`1;
 end;
 suppose p`1=(f/.(j))`1;
 hence p`1<=q`1 by A1,Th37b,B7;
 end;
end;

definition
  let P be Subset of TOP-REAL 2, p1,p2,p be Point of TOP-REAL 2, e be Real;
  pred p is_Lin P,p1,p2,e means :JJ100:
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    (ex p4 being Point of TOP-REAL 2 st p4`1<e & LE p4,p,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
    LE p4,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1<=e));
  pred p is_Rin P,p1,p2,e means :JJ110:
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    (ex p4 being Point of TOP-REAL 2 st p4`1>e & LE p4,p,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
    LE p4,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e));
  pred p is_Lout P,p1,p2,e means :JJ120:
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    (ex p4 being Point of TOP-REAL 2 st p4`1<e & LE p,p4,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
    LE p5,p4,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1<=e));
  pred p is_Rout P,p1,p2,e means :JJ130:
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    (ex p4 being Point of TOP-REAL 2 st p4`1>e & LE p,p4,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
    LE p5,p4,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1>=e));
  pred p is_OSin P,p1,p2,e means :JJ140: ::Oscilating In
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    ex p7 being Point of TOP-REAL 2 st LE p7,p,P,p1,p2 &
    (for p8 being Point of TOP-REAL 2 st LE p7,p8,P,p1,p2 & LE p8,p,P,p1,p2
    holds p8`1=e) &
    (for p4 being Point of TOP-REAL 2 st LE p4,p7,P,p1,p2 & p4<>p7 holds
    (ex p5 being Point of TOP-REAL 2 st
    LE p4,p5,P,p1,p2 & LE p5,p7,P,p1,p2 & p5`1>e) &
    (ex p6 being Point of TOP-REAL 2 st
