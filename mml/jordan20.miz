A4:  p`1 = ((1-r)*p1)`1+(r*p2)`1 by A3,TOPREAL3:7
      .= ((1-r)*p1)`1+r*p2`1 by TOPREAL3:9
      .= (1-r)*p1`1+r*p2`1 by TOPREAL3:9;
  per cases;
  suppose 0=r; then p=p1+(0)*p2 by A3,EUCLID:33.=p1+0.REAL 2 by EUCLID:33
     .=p1 by EUCLID:31;
   hence p`1>a by A1;
  end;
  suppose A5: 0<>r;
    1-r>=0 by A2,SQUARE_1:12;
  then A6: (1-r)*p1`1>=(1-r)*a by A1,AXIOMS:25;
  A7: r*p2`1>r*a by A1,A2,A5,REAL_1:70;
     (1-r)*a+r*a= a;
 hence p`1>a by A4,A6,A7,REAL_1:67;
 end;
