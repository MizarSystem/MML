      end;
          now assume A74: not ex p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2 & p51`1<e;
        A75: for p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2
        holds p51`1=e
        proof let p51 be Point of TOP-REAL 2;
          assume A76: LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2;
          then A77: p51`1>=e by A74;
             p51`1<=e by A66,A76;
