        g01 is Path of p2',p2' by BORSUK_2:def 1,E3;
       hence ex G1 being Path of q',p2' st
           G1 is continuous & G1.0=q' & G1.1=p2' by E0,E3;
      end;
      end;then
      consider G1 being Path of q',p2' such that
      B6: G1 is continuous & G1.0=q' & G1.1=p2';
      B810: Segment(Q1,q1,q2,q1,q) is_an_arc_of q1,q
                       by B1,JJ320,B30,A1;then
      consider f2 being map of I[01],(TOP-REAL 2)|Segment(Q1,q1,q2,q1,q)
         such that
      B6b: f2 is_homeomorphism & f2.0=q1 & f2.1=q by TOPREAL1:def 2;

      B81: f2 is continuous by B6b,TOPS_2:def 5;
      B82: dom f2=the carrier of I[01] by A106,B6b,TOPS_2:def 5;
      B83: rng f2=[#]((TOP-REAL 2)|Segment(Q1,q1,q2,q1,q)) by B6b,TOPS_2:def 5
         .=Segment(Q1,q1,q2,q1,q) by PRE_TOPC:def 10;
      B58: the carrier of ((TOP-REAL 2)|P)|Qa=Qa by JORDAN1:1;
      {p where p is Point of TOP-REAL 2: LE q1,p,Q1,q1,q2 & LE p,q,Q1,q1,q2}
                      c= Qa
       proof let x be set;assume x in
        {p where p is Point of TOP-REAL 2: LE q1,p,Q1,q1,q2 & LE p,q,Q1,q1,q2};
        then
         consider p being Point of TOP-REAL 2 such that
         C1: x=p & (LE q1,p,Q1,q1,q2 & LE p,q,Q1,q1,q2);
         x in Q1 by C1,JORDAN5C:def 3;then
         C2b: x in P by A1;
         now assume x=q2;then
           E1: LE q2,q,Q1,q1,q2 by C1;
           LE q,q2,Q1,q1,q2 by JORDAN5C:10,A1,B1;
          hence contradiction by B31,E1,JORDAN5C:12,A1;
         end;then
         x in P & not x in {q2} by C2b,TARSKI:def 1;
        hence x in Qa by XBOOLE_0:def 4;
       end;then
      C92: Segment(Q1,q1,q2,q1,q) c= Qa by JORDAN6:29;then
      f2 is Function of the carrier of I[01],
            the carrier of ((TOP-REAL 2)|P)|Qa by FUNCT_2:4,B82,B83,B58;then
      reconsider g2=f2 as map of I[01],((TOP-REAL 2)|P)|Qa;
      for G being Subset of ((TOP-REAL 2)|P)|Qa st G is open
        holds g2"G is open
       proof let G be Subset of ((TOP-REAL 2)|P)|Qa;
        assume D1: G is open;
         ((TOP-REAL 2)|P)|Qa=(TOP-REAL 2)|Qa' by C88,JORDAN6:47;then
         consider G4 being Subset of (TOP-REAL 2) such that
         D2: G4 is open &
         G=G4 /\ [#]((TOP-REAL 2)|Qa') by D1,TOPS_2:32;
         [#]((TOP-REAL 2)|Segment(Q1,q1,q2,q1,q))
         = the carrier of ((TOP-REAL 2)|Segment(Q1,q1,q2,q1,q))
                            by PRE_TOPC:12;then
         reconsider G5=G4 /\ [#]((TOP-REAL 2)|Segment(Q1,q1,q2,q1,q))
            as Subset of ((TOP-REAL 2)|Segment(Q1,q1,q2,q1,q)) by XBOOLE_1:17;
         q1 in Segment(Q1,q1,q2,q1,q) by B810,TOPREAL1:4;then
         [#]((TOP-REAL 2)|Segment(Q1,q1,q2,q1,q)) is non empty
                        by PRE_TOPC:def 10;then
         the carrier of ((TOP-REAL 2)|Segment(Q1,q1,q2,q1,q)) is non empty
                               by PRE_TOPC:12;
         then
         D18: ((TOP-REAL 2)|Segment(Q1,q1,q2,q1,q)) is non empty
                               by STRUCT_0:def 1;
         G5 is open by D2,TOPS_2:32;then
         D8: f2"G5 is open by D18,B81,TOPS_2:55;
         D5: f2"G5=g2"(G4 /\ Segment(Q1,q1,q2,q1,q)) by PRE_TOPC:def 10
             .=g2"(G4) /\ g2"(Segment(Q1,q1,q2,q1,q)) by FUNCT_1:137;
         D7: rng g2=[#]((TOP-REAL 2)|Segment(Q1,q1,q2,q1,q))
                                by TOPS_2:def 5,B6b
              .= Segment(Q1,q1,q2,q1,q) by PRE_TOPC:def 10;
         g2"G=g2"G4 /\ g2"([#]((TOP-REAL 2)|Qa')) by D2,FUNCT_1:137
             .=g2"G4 /\ g2"Qa' by PRE_TOPC:def 10
             .=g2"G4 /\ g2"((rng g2) /\ Qa') by RELAT_1:168
             .=g2"G4 /\ g2"(Segment(Q1,q1,q2,q1,q)) by C92,D7,XBOOLE_1:28;
        hence g2"G is open by D5,D8;
       end;then
      B128b: g2 is continuous by TOPS_2:55;then
      reconsider G2=g2 as Path of q1',q' by B6b,BORSUK_2:def 1;
      B716: (G2+G1) is continuous & (G2+G1).0=q1' & (G2+G1).1=p2'
                           by JJ338f,B128b,B6,B6b;
      now per cases;
      case q1<>p1;then
      C77d: Segment(P,p1,p2,p1,q1) is_an_arc_of p1,q1 by B88,JJ320,A1;then
      consider f3 being map of I[01],(TOP-REAL 2)|Segment(P,p1,p2,p1,q1)
         such that
      B6d: f3 is_homeomorphism & f3.0=p1 & f3.1=q1 by TOPREAL1:def 2;
      B81: f3 is continuous by B6d,TOPS_2:def 5;
      B82: dom f3=the carrier of I[01] by A106,B6d,TOPS_2:def 5;
      B83: rng f3=[#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q1))
                       by B6d,TOPS_2:def 5
         .=Segment(P,p1,p2,p1,q1) by PRE_TOPC:def 10;
      B58: the carrier of ((TOP-REAL 2)|P)|Qa=Qa by JORDAN1:1;
      {p where p is Point of TOP-REAL 2: LE p1,p,P,p1,p2 & LE p,q1,P,p1,p2}
                      c= Qa
       proof let x be set;assume x in
        {p where p is Point of TOP-REAL 2: LE p1,p,P,p1,p2 & LE p,q1,P,p1,p2};
        then
         consider p being Point of TOP-REAL 2 such that
         C1: x=p & (LE p1,p,P,p1,p2 & LE p,q1,P,p1,p2);
         C2: x in P by C1,JORDAN5C:def 3;
         x<>q2 by C1,B77,JORDAN5C:12,A1;then
         x in P & not x in {q2} by C2,TARSKI:def 1;
        hence x in Qa by XBOOLE_0:def 4;
       end;then
      C92: Segment(P,p1,p2,p1,q1) c= Qa by JORDAN6:29;then
      f3 is Function of the carrier of I[01],
            the carrier of ((TOP-REAL 2)|P)|Qa by FUNCT_2:4,B82,B83,B58;then
      reconsider g3=f3 as map of I[01],((TOP-REAL 2)|P)|Qa;
      for G being Subset of ((TOP-REAL 2)|P)|Qa st G is open
        holds g3"G is open
       proof let G be Subset of ((TOP-REAL 2)|P)|Qa;
        assume D1: G is open;
         ((TOP-REAL 2)|P)|Qa=(TOP-REAL 2)|Qa' by C88,JORDAN6:47;then
         consider G4 being Subset of (TOP-REAL 2) such that
         D2: G4 is open &
         G=G4 /\ [#]((TOP-REAL 2)|Qa') by D1,TOPS_2:32;
         [#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q1))
         = the carrier of ((TOP-REAL 2)|Segment(P,p1,p2,p1,q1))
                            by PRE_TOPC:12;then
         reconsider G5=G4 /\ [#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q1))
            as Subset of ((TOP-REAL 2)|Segment(P,p1,p2,p1,q1)) by XBOOLE_1:17;
         p1 in Segment(P,p1,p2,p1,q1) by C77d,TOPREAL1:4;then
         [#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q1)) is non empty
                        by PRE_TOPC:def 10;then
         the carrier of ((TOP-REAL 2)|Segment(P,p1,p2,p1,q1)) is non empty
                               by PRE_TOPC:12;
         then
         D18: ((TOP-REAL 2)|Segment(P,p1,p2,p1,q1)) is non empty
                               by STRUCT_0:def 1;
         G5 is open by D2,TOPS_2:32;then
         D8: f3"G5 is open by D18,B81,TOPS_2:55;
         D5: f3"G5=g3"(G4 /\ Segment(P,p1,p2,p1,q1)) by PRE_TOPC:def 10
             .=g3"(G4) /\ g3"(Segment(P,p1,p2,p1,q1)) by FUNCT_1:137;
         D7: rng g3=[#]((TOP-REAL 2)|Segment(P,p1,p2,p1,q1))
                              by TOPS_2:def 5,B6d
              .= Segment(P,p1,p2,p1,q1) by PRE_TOPC:def 10;
         g3"G=g3"G4 /\ g3"([#]((TOP-REAL 2)|Qa')) by D2,FUNCT_1:137
             .=g3"G4 /\ g3"Qa' by PRE_TOPC:def 10
             .=g3"G4 /\ g3"((rng g3) /\ Qa') by RELAT_1:168
             .=g3"G4 /\ g3"(Segment(P,p1,p2,p1,q1)) by C92,D7,XBOOLE_1:28;
        hence g3"G is open by D5,D8;
       end;then
      B128d: g3 is continuous by TOPS_2:55;then
        g3 is Path of p1',q1' by B6d,BORSUK_2:def 1;
       hence ex G3 being Path of p1',q1' st
           G3 is continuous & G3.0=p1' & G3.1=q1' by B128d,B6d;
      end;
      case E0: q1=p1;
        consider g01 being map of I[01],(((TOP-REAL 2)|P)|Qa) such that
        E3: g01 is continuous & g01.0=q1' & g01.1=q1' by BORSUK_2:4;
        g01 is Path of q1',q1' by BORSUK_2:def 1,E3;
       hence ex G3 being Path of p1',q1' st
           G3 is continuous & G3.0=p1' & G3.1=q1' by E0,E3;
      end;
      end;then
      consider G3 being Path of p1',q1' such that
      B6d: G3 is continuous & G3.0=p1' & G3.1=q1';
      G3+(G2+G1) is continuous & (G3+(G2+G1)).0=p1' & (G3+(G2+G1)).1=p2'
                        by JJ338f,B6d,B716;
     hence contradiction by JJ315,A1,B4,A350b,B89;
    end;
    end;
   hence contradiction;
  end;
 hence thesis by JJ329,A31,A1;
end;

theorem for P being non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p being Point of TOP-REAL 2,e being Real st
  p1`1<e & p2`1>e &
  q1 is_Lin P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
  & p in LSeg(q1,q2) holds p is_Lin P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p be Point of TOP-REAL 2,e be Real;
  assume A1: p1`1<e & p2`1>e &
   q1 is_Lin P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
   & p in LSeg(q1,q2);then
   C2: P is_an_arc_of p1,p2 & q1 in P & q1`1=e &
      (ex p4 being Point of TOP-REAL 2 st p4`1<e &
       LE p4,q1,P,p1,p2 &
       (for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,q1,P,p1,p2 holds p5`1<=e)) by JJ100;then
    consider p4 being Point of TOP-REAL 2 such that
    A2: p4`1<e & LE p4,q1,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,q1,P,p1,p2 holds p5`1<=e);
     q1 in LSeg(q1,q2) & q2 in LSeg(q1,q2) by TOPREAL1:6;then
     A79: q2 in P by A1;
     A10: p4 in P by A2,JORDAN5C:def 3;
     B89: LE q1,q2,P,p1,p2 or LE q2,q1,P,p1,p2 by C2,JJ319,A79;
     now per cases by B89;
     case D0: LE q1,q2,P,p1,p2;
     now per cases;
     case q1<>q2;then
       LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by D0,A1,JJ338,C2;
     end;
     case C0: q1=q2;then
       LSeg(q1,q2)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by C0,C2,JJ338h;
     end;
     end;then
     B5: Segment(P,p1,p2,q1,q2) =LSeg(q1,q2);
       Segment(P,p1,p2,q1,q2) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;then
       consider p3 being Point of TOP-REAL 2 such that
       B87: p=p3 & (LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2) by A1,B5;
       B90: p`1=e by C2,A1,GOBOARD7:5;
       B91: LE p4,p,P,p1,p2 by B87,A2,JORDAN5C:13;
        for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2 holds p6`1<=e
        proof let p6 be Point of TOP-REAL 2;
          assume D1: LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2;then
          p6 in P by JORDAN5C:def 3;then
          D2: LE p6,q1,P,p1,p2 or LE q1,p6,P,p1,p2 by JJ319,C2;
          now per cases by D2;
          case LE p6,q1,P,p1,p2;
           hence p6`1<=e by D1,A2;
          end;
          case E0: LE q1,p6,P,p1,p2;
            LE p6,q2,P,p1,p2 by B87,JORDAN5C:13,D1;then
            p6 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,p2 &
            LE qq,q2,P,p1,p2} by E0;then
            p6 in LSeg(q1,q2) by B5,JORDAN6:29;
           hence p6`1<=e by GOBOARD7:5,C2,A1;
          end;
          end;
         hence p6`1<=e;
        end;
      hence p is_Lin P,p1,p2,e by JJ100,A1,C2,B90,B91,A2;
     end;
     case D0: LE q2,q1,P,p1,p2;
     now per cases;
     case q1<>q2;then
       LSeg(q2,q1) is_an_arc_of q2,q1 by TOPREAL1:15;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by D0,A1,JJ338,C2;
     end;
     case C0: q1=q2;then
       LSeg(q2,q1)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by C0,C2,JJ338h;
     end;
     end;then
     B5: Segment(P,p1,p2,q2,q1) =LSeg(q2,q1);
       Segment(P,p1,p2,q2,q1) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;then
       consider p3 being Point of TOP-REAL 2 such that
       B87: p=p3 & (LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2) by A1,B5;
       B90: p`1=e by C2,A1,GOBOARD7:5;
       B6: LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by JJ319,C2,A10,A79;
       now assume LE q2,p4,P,p1,p2;then
         p4 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1,p2 &
            LE qq,q1,P,p1,p2} by A2;then
            p4 in Segment(P,p1,p2,q2,q1) by JORDAN6:29;
        hence contradiction by A2,GOBOARD7:5,C2,A1,B5;
       end;then
       B91: LE p4,p,P,p1,p2 by B6,B87,JORDAN5C:13;
       for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2 holds p6`1<=e
        proof let p6 be Point of TOP-REAL 2;
          assume F1: LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2;then
          LE p6,q1,P,p1,p2 by JORDAN5C:13,B87;
         hence p6`1<=e by F1,A2;
        end;
      hence p is_Lin P,p1,p2,e by JJ100,A1,C2,B90,B91,A2;
     end;
     end;
  hence p is_Lin P,p1,p2,e;
end;

theorem for P being non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p being Point of TOP-REAL 2,e being Real st
  p1`1<e & p2`1>e &
  q1 is_Rin P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
  & p in LSeg(q1,q2) holds p is_Rin P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p be Point of TOP-REAL 2,e be Real;
  assume A1: p1`1<e & p2`1>e &
   q1 is_Rin P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
   & p in LSeg(q1,q2);then
   C2: P is_an_arc_of p1,p2 & q1 in P & q1`1=e &
      (ex p4 being Point of TOP-REAL 2 st p4`1>e &
       LE p4,q1,P,p1,p2 &
       (for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,q1,P,p1,p2 holds p5`1>=e)) by JJ110;then
    consider p4 being Point of TOP-REAL 2 such that
    A2: p4`1>e & LE p4,q1,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,q1,P,p1,p2 holds p5`1>=e);
     q1 in LSeg(q1,q2) & q2 in LSeg(q1,q2) by TOPREAL1:6;then
     A79: q2 in P by A1;
     A10: p4 in P by A2,JORDAN5C:def 3;
     B89: LE q1,q2,P,p1,p2 or LE q2,q1,P,p1,p2 by C2,JJ319,A79;
     now per cases by B89;
     case D0: LE q1,q2,P,p1,p2;
     now per cases;
     case q1<>q2;then
       LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by D0,A1,JJ338,C2;
     end;
     case C0: q1=q2;then
       LSeg(q1,q2)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by C0,C2,JJ338h;
     end;
     end;then
     B5: Segment(P,p1,p2,q1,q2) =LSeg(q1,q2);
       Segment(P,p1,p2,q1,q2) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;then
       consider p3 being Point of TOP-REAL 2 such that
       B87: p=p3 & (LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2) by A1,B5;
       B90: p`1=e by C2,A1,GOBOARD7:5;
       B91: LE p4,p,P,p1,p2 by B87,A2,JORDAN5C:13;
        for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume D1: LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2;then
          p6 in P by JORDAN5C:def 3;then
          D2: LE p6,q1,P,p1,p2 or LE q1,p6,P,p1,p2 by JJ319,C2;
          now per cases by D2;
          case LE p6,q1,P,p1,p2;
           hence p6`1>=e by D1,A2;
          end;
          case E0: LE q1,p6,P,p1,p2;
            LE p6,q2,P,p1,p2 by B87,JORDAN5C:13,D1;then
            p6 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,p2 &
            LE qq,q2,P,p1,p2} by E0;then
            p6 in LSeg(q1,q2) by B5,JORDAN6:29;
           hence p6`1>=e by GOBOARD7:5,C2,A1;
          end;
          end;
         hence p6`1>=e;
        end;
      hence p is_Rin P,p1,p2,e by JJ110,A1,C2,B90,B91,A2;
     end;
     case D0: LE q2,q1,P,p1,p2;
     now per cases;
     case q1<>q2;then
       LSeg(q2,q1) is_an_arc_of q2,q1 by TOPREAL1:15;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by D0,A1,JJ338,C2;
     end;
     case C0: q1=q2;then
       LSeg(q2,q1)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by C0,C2,JJ338h;
     end;
     end;then
     B5: Segment(P,p1,p2,q2,q1) =LSeg(q2,q1);
       Segment(P,p1,p2,q2,q1) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;then
       consider p3 being Point of TOP-REAL 2 such that
       B87: p=p3 & (LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2) by A1,B5;
       B90: p`1=e by C2,A1,GOBOARD7:5;
       B6: LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by JJ319,C2,A10,A79;
       now assume LE q2,p4,P,p1,p2;then
         p4 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1,p2 &
            LE qq,q1,P,p1,p2} by A2;then
            p4 in Segment(P,p1,p2,q2,q1) by JORDAN6:29;
        hence contradiction by A2,GOBOARD7:5,C2,A1,B5;
       end;then
       B91: LE p4,p,P,p1,p2 by B6,B87,JORDAN5C:13;
       for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume F1: LE p4,p6,P,p1,p2 & LE p6,p,P,p1,p2;then
          LE p6,q1,P,p1,p2 by JORDAN5C:13,B87;
         hence p6`1>=e by F1,A2;
        end;
      hence p is_Rin P,p1,p2,e by JJ110,A1,C2,B90,B91,A2;
     end;
     end;
  hence p is_Rin P,p1,p2,e;
end;

theorem for P being non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p being Point of TOP-REAL 2,e being Real st
  p1`1<e & p2`1>e &
  q1 is_Lout P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
  & p in LSeg(q1,q2) holds p is_Lout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p be Point of TOP-REAL 2,e be Real;
  assume A1: p1`1<e & p2`1>e &
   q1 is_Lout P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
   & p in LSeg(q1,q2);then
   C2: P is_an_arc_of p1,p2 & q1 in P & q1`1=e &
      (ex p4 being Point of TOP-REAL 2 st p4`1<e &
       LE q1,p4,P,p1,p2 &
       (for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE q1,p5,P,p1,p2 holds p5`1<=e)) by JJ120;then
    consider p4 being Point of TOP-REAL 2 such that
    A2: p4`1<e & LE q1,p4,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE q1,p5,P,p1,p2 holds p5`1<=e);
     q1 in LSeg(q1,q2) & q2 in LSeg(q1,q2) by TOPREAL1:6;then
     A79: q2 in P by A1;
     A10: p4 in P by A2,JORDAN5C:def 3;
     B89: LE q1,q2,P,p1,p2 or LE q2,q1,P,p1,p2 by C2,JJ319,A79;
     now per cases by B89;
     case D0: LE q2,q1,P,p1,p2;
     now per cases;
     case q1<>q2;then
       LSeg(q2,q1) is_an_arc_of q2,q1 by TOPREAL1:15;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by D0,A1,JJ338,C2;
     end;
     case C0: q1=q2;then
       LSeg(q1,q2)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by C0,C2,JJ338h;
     end;
     end;then
     B5: Segment(P,p1,p2,q2,q1) =LSeg(q2,q1);
       Segment(P,p1,p2,q2,q1) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;then
       consider p3 being Point of TOP-REAL 2 such that
       B87: p=p3 & (LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2) by A1,B5;
       B90: p`1=e by C2,A1,GOBOARD7:5;
       B91: LE p,p4,P,p1,p2 by B87,A2,JORDAN5C:13;
        for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1<=e
        proof let p6 be Point of TOP-REAL 2;
          assume D1: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;then
          p6 in P by JORDAN5C:def 3;then
          D2: LE p6,q1,P,p1,p2 or LE q1,p6,P,p1,p2 by JJ319,C2;
          now per cases by D2;
          case LE q1,p6,P,p1,p2;
           hence p6`1<=e by D1,A2;
          end;
          case E0: LE p6,q1,P,p1,p2;
            LE q2,p6,P,p1,p2 by B87,JORDAN5C:13,D1;then
            p6 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1,p2 &
            LE qq,q1,P,p1,p2} by E0;then
            p6 in LSeg(q2,q1) by B5,JORDAN6:29;
           hence p6`1<=e by GOBOARD7:5,C2,A1;
          end;
          end;
         hence p6`1<=e;
        end;
      hence p is_Lout P,p1,p2,e by JJ120,A1,C2,B90,B91,A2;
     end;
     case D0: LE q1,q2,P,p1,p2;
     now per cases;
     case q1<>q2;then
       LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by D0,A1,JJ338,C2;
     end;
     case C0: q1=q2;then
       LSeg(q2,q1)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by C0,C2,JJ338h;
     end;
     end;then
     B5: Segment(P,p1,p2,q1,q2) =LSeg(q1,q2);
       Segment(P,p1,p2,q1,q2) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;then
       consider p3 being Point of TOP-REAL 2 such that
       B87: p=p3 & (LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2) by A1,B5;
       B90: p`1=e by C2,A1,GOBOARD7:5;
       B6: LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by JJ319,C2,A10,A79;
       now assume LE p4,q2,P,p1,p2;then
         p4 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,p2 &
            LE qq,q2,P,p1,p2} by A2;then
            p4 in Segment(P,p1,p2,q1,q2) by JORDAN6:29;
        hence contradiction by A2,GOBOARD7:5,C2,A1,B5;
       end;then
       B91: LE p,p4,P,p1,p2 by B6,B87,JORDAN5C:13;
       for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1<=e
        proof let p6 be Point of TOP-REAL 2;
          assume F1: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;then
          LE q1,p6,P,p1,p2 by JORDAN5C:13,B87;
         hence p6`1<=e by F1,A2;
        end;
      hence p is_Lout P,p1,p2,e by JJ120,A1,C2,B90,B91,A2;
     end;
     end;
  hence p is_Lout P,p1,p2,e;
end;

theorem for P being non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p being Point of TOP-REAL 2,e being Real st
  p1`1<e & p2`1>e &
  q1 is_Rout P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
  & p in LSeg(q1,q2) holds p is_Rout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,q1,q2,p be Point of TOP-REAL 2,e be Real;
  assume A1: p1`1<e & p2`1>e &
   q1 is_Rout P,p1,p2,e & q2`1=e & LSeg(q1,q2) c= P
   & p in LSeg(q1,q2);then
   C2: P is_an_arc_of p1,p2 & q1 in P & q1`1=e &
      (ex p4 being Point of TOP-REAL 2 st p4`1>e &
       LE q1,p4,P,p1,p2 &
       (for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE q1,p5,P,p1,p2 holds p5`1>=e)) by JJ130;then
    consider p4 being Point of TOP-REAL 2 such that
    A2: p4`1>e & LE q1,p4,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE q1,p5,P,p1,p2 holds p5`1>=e);
     q1 in LSeg(q1,q2) & q2 in LSeg(q1,q2) by TOPREAL1:6;then
     A79: q2 in P by A1;
     A10: p4 in P by A2,JORDAN5C:def 3;
     B89: LE q1,q2,P,p1,p2 or LE q2,q1,P,p1,p2 by C2,JJ319,A79;
     now per cases by B89;
     case D0: LE q2,q1,P,p1,p2;
     now per cases;
     case q1<>q2;then
       LSeg(q2,q1) is_an_arc_of q2,q1 by TOPREAL1:15;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by D0,A1,JJ338,C2;
     end;
     case C0: q1=q2;then
       LSeg(q1,q2)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q2,q1) =LSeg(q2,q1) by C0,C2,JJ338h;
     end;
     end;then
     B5: Segment(P,p1,p2,q2,q1) =LSeg(q2,q1);
       Segment(P,p1,p2,q2,q1) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2} by JORDAN6:29;then
       consider p3 being Point of TOP-REAL 2 such that
       B87: p=p3 & (LE q2,p3,P,p1,p2 & LE p3,q1,P,p1,p2) by A1,B5;
       B90: p`1=e by C2,A1,GOBOARD7:5;
       B91: LE p,p4,P,p1,p2 by B87,A2,JORDAN5C:13;
        for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume D1: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;then
          p6 in P by JORDAN5C:def 3;then
          D2: LE p6,q1,P,p1,p2 or LE q1,p6,P,p1,p2 by JJ319,C2;
          now per cases by D2;
          case LE q1,p6,P,p1,p2;
           hence p6`1>=e by D1,A2;
          end;
          case E0: LE p6,q1,P,p1,p2;
            LE q2,p6,P,p1,p2 by B87,JORDAN5C:13,D1;then
            p6 in {qq where qq is Point of TOP-REAL 2: LE q2,qq,P,p1,p2 &
            LE qq,q1,P,p1,p2} by E0;then
            p6 in LSeg(q2,q1) by B5,JORDAN6:29;
           hence p6`1>=e by GOBOARD7:5,C2,A1;
          end;
          end;
         hence p6`1>=e;
        end;
      hence p is_Rout P,p1,p2,e by JJ130,A1,C2,B90,B91,A2;
     end;
     case D0: LE q1,q2,P,p1,p2;
     now per cases;
     case q1<>q2;then
      LSeg(q1,q2) is_an_arc_of q1,q2 by TOPREAL1:15;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by D0,A1,JJ338,C2;
     end;
     case C0: q1=q2;then
      LSeg(q2,q1)={q1} by TOPREAL1:7;
      hence Segment(P,p1,p2,q1,q2) =LSeg(q1,q2) by C0,C2,JJ338h;
     end;
     end;then
     B5: Segment(P,p1,p2,q1,q2) =LSeg(q1,q2);
       Segment(P,p1,p2,q1,q2) =
        {p3 where p3 is Point of TOP-REAL 2:
              LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2} by JORDAN6:29;then
       consider p3 being Point of TOP-REAL 2 such that
       B87: p=p3 & (LE q1,p3,P,p1,p2 & LE p3,q2,P,p1,p2) by A1,B5;
       B90: p`1=e by C2,A1,GOBOARD7:5;
       B6: LE q2,p4,P,p1,p2 or LE p4,q2,P,p1,p2 by JJ319,C2,A10,A79;
       now assume LE p4,q2,P,p1,p2;then
         p4 in {qq where qq is Point of TOP-REAL 2: LE q1,qq,P,p1,p2 &
            LE qq,q2,P,p1,p2} by A2;then
            p4 in Segment(P,p1,p2,q1,q2) by JORDAN6:29;
        hence contradiction by A2,GOBOARD7:5,C2,A1,B5;
       end;then
       B91: LE p,p4,P,p1,p2 by B6,B87,JORDAN5C:13;
       for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2 holds p6`1>=e
        proof let p6 be Point of TOP-REAL 2;
          assume F1: LE p6,p4,P,p1,p2 & LE p,p6,P,p1,p2;then
          LE q1,p6,P,p1,p2 by JORDAN5C:13,B87;
         hence p6`1>=e by F1,A2;
        end;
      hence p is_Rout P,p1,p2,e by JJ130,A1,C2,B90,B91,A2;
     end;
     end;
  hence p is_Rout P,p1,p2,e;
end;

theorem for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
   then
   A2b: P is_an_arc_of p1,p2 by TOPREAL4:3;
   consider f being FinSequence of (TOP-REAL 2) such that
   A2:  f is_S-Seq & P = L~f & p1=f/.1 & p2=f/.len f by TOPREAL4:def 1,A1;
   f is one-to-one & len f >= 2 & f is unfolded s.n.c. special
                    by A2,TOPREAL1:def 10;then
   A2e: len f >1 by AXIOMS:22;
   A12: L~f = union { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
                          by TOPREAL1:def 6;
   consider Y being set such that
   A5: p in Y & Y in
      { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
            by TARSKI:def 4,A12,A2,A1;
   A13: Y c= L~f
   proof let x be set;assume x in Y;
    hence x in L~f by A12,A5,TARSKI:def 4;
   end;
   consider i being Nat such that
   A6: Y=LSeg(f,i) & 1 <= i & i+1 <= len f by A5;
   A90: i<i+1 by NAT_1:38;then
   A6b: i<len f by AXIOMS:22,A6;then
   A10: i in dom f by A6,FINSEQ_3:27;
   A6g: 1<len f by A6,A6b,AXIOMS:22;
   A11b: 1<i+1 by A90,A6,AXIOMS:22;
   defpred P[Nat] means
    for p being Point of TOP-REAL 2 st p=f.(i-'$1) holds p`1<>e;
   1 in dom f by A6g,FINSEQ_3:27;then
   f/.1=f.1 by FINSEQ_4:def 4;then
   A200: P[i-'1] by A2,A1,JCT_MISC:7,A6;then
   A8: ex k being Nat st P[k];
   ex k being Nat st P[k] & for n being Nat st P[n] holds k <= n
                                  from NAT_1:sch 5(A8);then
   consider k being Nat such that
   A9: P[k] & for n being Nat st P[n] holds k <= n;
   i-'k <=i by GOBOARD9:2;then
   A78: i-'k <len f by AXIOMS:22,A6b;
   A51: i-1>=0 by A6,SQUARE_1:12;
   k<=i-'1 by A9,A200;then
   k<=i-1 by A51,BINARITH:def 3;then
   k+1<=i-1+1 by REAL_1:55;then
   k+1<=i by XCMPLX_1:27;then
   1+k-k<=i-k by REAL_1:49;then
   A120: 1<=i-k by XCMPLX_1:26;then
   A121: 0<i-k by AXIOMS:22;then
   A79: i-'k>=1 by A120,BINARITH:def 3;then
   i-'k in dom f by A78,FINSEQ_3:27;then
   A167: f/.(i-'k)=f.(i-'k) by FINSEQ_4:def 4;then
   reconsider pk=f.(i-'k) as Point of TOP-REAL 2;
   A122: i-'k=i-k by A121,BINARITH:def 3;
   pk`1<> e by A9;then
   A12b: pk`1<e or pk`1>e by AXIOMS:21;
   reconsider kn=k+1 as Nat;
   now per cases by A12b;
   case B1: pk`1<e;
     now per cases;
     case C0: k=0;
      set p44=f/.i;
        pk=f.i by C0,JORDAN3:2 .=p44 by A10,FINSEQ_4:def 4;then
        C1: p44`1<e by B1;
        D7: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A6;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
        C78: len g=len f -'i+1 by JORDAN3:27,A6b,A6,A2e;
        C77: len f-i>0 by A6b,SQUARE_1:11;then
        len f-'i=len f-i by BINARITH:def 3;then
        len f-'i+1>0+1 by C77,REAL_1:67;then
        C72: 1<len g by C78;then
        C73: 1+1<=len g by NAT_1:38;
        1+(1+i)<=1+len f by A6,REAL_1:55;then
        1+1+i<=len f+1 by XCMPLX_1:1;then
        1+1+i-i<=len f+1-i by REAL_1:49;then
        C88: 1+1<=len f+1-i by XCMPLX_1:26;then
        C76: 1<=i & i<=len f & len f<=len f & 1<=1 &
         1<=len f+1-i by A6,A6b,AXIOMS:22;
        C76b: 1<=i & i<=len f & len f<=len f & 1<=1+1 &
         1+1<=len f+1-i by C88,A6,A6b;
        1 in dom g by C72,FINSEQ_3:27;then
        C74: g/.1=g.1 by FINSEQ_4:def 4
                .=f.(1-1+i) by C76,JORDAN3:31
                .=f/.i by FINSEQ_4:def 4,A10;
        reconsider ia=i+1 as Nat;
        ia in Seg len f by A6,A11b,FINSEQ_1:3;then
        C100: i+1 in dom f by FINSEQ_1:def 3;
        1+1 in Seg len g by C73,FINSEQ_1:3;then
        1+1 in dom g by FINSEQ_1:def 3;then
        C75: g/.(1+1)=g.(1+1) by FINSEQ_4:def 4
                    .=f.(1+1-1+i) by C76b,JORDAN3:31
                    .=f/.(i+1) by C100,FINSEQ_4:def 4;
        LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A6
                .=LSeg(g,1) by TOPREAL1:def 5,C73,C74,C75;then
        p in Y &
          Y in { LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
                           by A5,A6,C73;then
        p in union{ LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
            by TARSKI:def 4;then
        p in L~mid(f,i,len f) by TOPREAL1:def 6;then
        C2: LE p44,p,P,p1,p2 by SPRECT_4:4,A2,A6,A6b;
        for p5 being Point of TOP-REAL 2 st
         LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume D1: LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2;
           Segment(P,p1,p2,p44,p)={p8 where p8 is Point of TOP-REAL 2:
            LE p44,p8,P,p1,p2 & LE p8,p,P,p1,p2} by JORDAN6:29;then
           D4: p5 in Segment(P,p1,p2,p44,p) by D1;
           D5: p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
           D6: p in LSeg(f,i) by A5,A6;
           D10: LSeg(p44,p) c= LSeg(f,i) by D5,D6,D7,TOPREAL1:12;
           D5: LSeg(p44,p) c= P by D10,XBOOLE_1:1,A2,A6,A13;
           D6: p44`1<p`1 by C1,A1;
           now per cases;
           case p44<>p;then
             LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;then
             Segment(P,p1,p2,p44,p)=LSeg(p44,p) by JJ338,C2,D5,A2b;
            hence p5`1<=e by A1,Th37,D4,D6;
           end;
           case p44=p;
            hence p5`1<=e by D6;
           end;
           end;
          hence p5`1<=e;
         end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by JJ100,A2b,A1,C1,C2;
     end;
