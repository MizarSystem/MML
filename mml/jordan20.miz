     then A21: R is bounded_below by SEQ_4:def 2;
     A22: LE p,p9,P,p1,p2 by A1,A4,A9,A10,A17,A20,JORDAN5C:8;
     A23: for p8 being Point of TOP-REAL 2 st
       LE p8,p9,P,p1,p2 & LE p,p8,P,p1,p2
       holds p8`1=e
       proof let p8 be Point of TOP-REAL 2;
        assume A24: LE p8,p9,P,p1,p2 & LE p,p8,P,p1,p2;
         then A25: p8 in P by JORDAN5C:def 3;
         then consider x8 being set such that
         A26: x8 in dom f & p8=f.x8 by A8,FUNCT_1:def 5;
         reconsider s8=x8 as Real by A7,A26;
         A27: 0<=s8 & s8<=1 & f.s8=p8 by A26,BORSUK_1:83,RCOMP_1:48;
