        P1: for p5 being Point of TOP-REAL 2 st
         LE p,p5,P,p1,p2 & LE p5,p44,P,p1,p2 holds p5`1>=e
         proof let p5 be Point of TOP-REAL 2;
          assume D1: LE p,p5,P,p1,p2 & LE p5,p44,P,p1,p2;
           Segment(P,p1,p2,p,p44)={p8 where p8 is Point of TOP-REAL 2:
            LE p,p8,P,p1,p2 & LE p8,p44,P,p1,p2} by JORDAN6:29;then
           D4: p5 in Segment(P,p1,p2,p,p44) by D1;
           D5: p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
           D6: p in LSeg(f,i) by A5,A6;
           D10: LSeg(p,p44) c= LSeg(f,i) by D5,D6,D7,TOPREAL1:12;
           D5: LSeg(p,p44) c= P by D10,XBOOLE_1:1,A2,A6,A13;
           D6: p44`1=p`1 by C1,A1;
           now per cases;
           case p44<>p;then
             LSeg(p,p44) is_an_arc_of p,p44 by TOPREAL1:15;then
             Segment(P,p1,p2,p,p44)=LSeg(p,p44) by JJ338,C2,D5,A2b;
            hence p5`1>=e by A1,Th37,D4,D6;
           end;
           case p44=p;then
             Segment(P,p1,p2,p,p44)={p44} by A2b,JJ20,A1;
            hence p5`1>=e by C1,D4,TARSKI:def 1;
           end;
           end;
          hence p5`1>=e;
         end;
        reconsider g=mid(f,1,i) as FinSequence of (TOP-REAL 2);
         i+1<=i+1+k by NAT_1:29;then
         A710: LE p44,pk,P,p1,p2
                 by A90e,A9f,A2,A167,JORDAN5C:24;then
         A711: LE p,pk,P,p1,p2 by C2,JORDAN5C:13;
         A798: p44 in P by A710,JORDAN5C:def 3;
         P2: now assume not
           for p51 being Point of TOP-REAL 2 st
            LE p44,p51,P,p1,p2 & LE p51,pk,P,p1,p2 holds p51`1>=e;then
           consider p51 being Point of TOP-REAL 2 such that
           D33: LE p44,p51,P,p1,p2 & LE p51,pk,P,p1,p2 & p51`1<e;
           p51 in P by D33,JORDAN5C:def 3;then
           consider Y3 being set such that
           A5e: p51 in Y3 & Y3 in
            { LSeg(f,i5) where i5 is Nat : 1 <= i5 & i5+1 <= len f }
                     by TARSKI:def 4,A12,A2;
           consider kk being Nat such that
           A6e: Y3=LSeg(f,kk) &  (1 <= kk & kk+1 <= len f) by A5e;
           D34: 1<=kk & kk+1<=len f & p51 in LSeg(f,kk) by A5e,A6e;then
           D34e: kk-1>=0 by SQUARE_1:12;
           D66: kk<len f by D34,NAT_1:38;then
           kk in Seg len f by D34,FINSEQ_1:3;then
           D788: kk in dom f by FINSEQ_1:def 3;
           D790: 1<kk+1 by D34,NAT_1:38;then
           D789: kk+1 in dom f by D34,FINSEQ_3:27;
           D35: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by TOPREAL1:def 5,D34;then
           D37: (f/.kk)`1<e or (f/.(kk+1))`1<e by D34,D33,JJ40;
           D55: f/.kk in LSeg(f,kk) by D35,TOPREAL1:6;
           D38: LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                     by A2,A5e,A6e,JORDAN5C:26;
           E43: LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A2,D34,JORDAN5C:25;
           G20: f is one-to-one by A2,TOPREAL1:def 10;
           kk<kk+1 by NAT_1:38;then
           G23: f.kk <> f.(kk+1) by FUNCT_1:def 8,G20,D788,D789;
           G24: f/.kk=f.kk by FINSEQ_4:def 4,D788;
           now per cases by D37;
           case E0: (f/.(kk+1))`1<e;
              set k2=kk-'i;
                E44: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by D33,D38,A2,JORDAN5C:13;
              now assume kk-i<0;then kk-i+i<0+i by REAL_1:53;then
                kk<i by XCMPLX_1:27;then
                kk+1<=i+1 by REAL_1:55;then
                LE f/.(kk+1),f/.(i+1),L~f,f/.1,f/.(len f)
                         by D790,A2,A6,JORDAN5C:24;
               hence contradiction by E0,C1,E44,JORDAN5C:12,A2b,A2;
              end;then
              F9: kk-'i=kk-i by BINARITH:def 3;
              F71: 1<kk+1 by D34,NAT_1:38;
              F13: i+1+k2=1+i+(kk-i) by F9
                        .=1+(i+(kk-i)) by XCMPLX_1:1
                        .=kk+1 by XCMPLX_1:27;then
              F15: i+1+k2>=0+1 by F71;
              P[k2] by F13,E0,FINSEQ_4:24,F15,D34;then
              k2>=k by A9;then
              i+1+k2>=i+1+k by REAL_1:55;then
              D90: LE f/.(i+1+k),f/.(i+1+k2),L~f,f/.1,f/.(len f)
                           by JORDAN5C:24,A78,F13,A6e,A2;
              D200: LE p51,f/.(i+1+k),L~f,f/.1,f/.(len f) by A167,D33,A2;then
              D202: LE f/.(kk),f/.(i+1+k),L~f,f/.1,f/.(len f)
                                 by E43,JORDAN5C:13;then
              D203: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by D90,F13,JORDAN5C:13;
              Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;then
              D99: f/.(i+1+k) in Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                                            by D90,D202,F13;
              D100: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                G0: z in LSeg(f/.kk,f/.(kk+1));
                LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A6e,D35;
               hence z in L~f by G0,A12,TARSKI:def 4;
              end;
              D101: f is special by A2,TOPREAL1:def 10;
              G20: f is one-to-one by A2,TOPREAL1:def 10;
              kk<kk+1 by NAT_1:38;then
              G23: f.kk <> f.(kk+1) by FUNCT_1:def 8,G20,D788,D789;
              G24: f/.kk=f.kk by FINSEQ_4:def 4,D788;
              f/.(kk+1)=f.(kk+1) by FINSEQ_4:def 4,D789;then
              LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by G23,G24,TOPREAL1:15;then
              D102: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by D203,D100,A2,A2b,JJ338;
              (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A6e,TOPREAL1:def 7,D101;then
              D199: (f/.kk)`2=(f/.(kk+1))`2 by D99,D102,E0,JJ50,A167,B1;
              (f/.(kk))`1>e by E0,D99,D102,JJ30,B1,A167;then
              (f/.kk)`1> (f/.(kk+1))`1 by E0,AXIOMS:22;then
              (f/.(i+1+k))`1<=p51`1
                     by JJ80,D34,D66,D99,D102,D200,A2,D35,D199;
            hence contradiction by D33,B1,A167,AXIOMS:22;
           end;
           case E0: (f/.(kk))`1 <e & (f/.(kk+1))`1>=e;then
              G77: (f/.kk)`1<(f/.(kk+1))`1 by AXIOMS:22;
              set k2=kk-'(i+1);
              D100: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                G0: z in LSeg(f/.kk,f/.(kk+1));
                LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A6e,D35;
               hence z in L~f by G0,A12,TARSKI:def 4;
              end;
              D400: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              D203: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                          by JORDAN5C:23,D34,A2;
              f/.(kk+1)=f.(kk+1) by FINSEQ_4:def 4,D789;then
              LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by G23,G24,TOPREAL1:15;then
              D102: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by D203,D100,A2,A2b,JJ338;
              E4: p51 in LSeg(f/.kk,f/.(kk+1)) by D35,A6e,A5e;
              LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A2,D34,JORDAN5C:26;then
                E44: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by D33,A2,JORDAN5C:13;
              now assume kk-(i+1)<0;then
                kk-(i+1)+(i+1)<0+(i+1) by REAL_1:53;then
                kk<i+1 by XCMPLX_1:27;then
                kk<=i by NAT_1:38;then
                E34: LE f/.kk,f/.i,L~f,f/.1,f/.(len f)
                         by A2,D34,A6b,JORDAN5C:24;
                LE f/.i,p44,L~f,f/.1,f/.(len f) by A6,JORDAN5C:23,A2;then
                LE f/.i,f/.(kk+1),L~f,f/.1,f/.(len f)
                             by E44,JORDAN5C:13;then
                E9: f/.i in LSeg(f/.kk,f/.(kk+1)) by E34,D102,D400;
                f/.i in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;then
                LSeg(f/.kk,f/.(kk+1)) /\ LSeg(f/.i,f/.(i+1))<>{}
                             by E9,XBOOLE_0:def 3;then
                E8: not (LSeg(f/.kk,f/.(kk+1)) misses LSeg(f/.i,f/.(i+1)))
                             by XBOOLE_0:def 7;
                E30: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by TOPREAL1:def 5,A6e;
                E31: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A6;
                E10: f is s.n.c. by TOPREAL1:def 10,A2;then
                E81: kk+1>=i by E30,E31,E8,TOPREAL1:def 9;
                E79: kk-1=kk-'1  by BINARITH:def 3,D34e;then
                E80: kk+1=kk-'1+1+1 by XCMPLX_1:27;
                i+1>=kk by E8,E30,E31,E10,TOPREAL1:def 9;then
                i+1-1>=kk-1 by REAL_1:49;then
                kk-'1<=i & i<=kk-'1+(1+1) by E80,E81,XCMPLX_1:1,26,E79;then
                E91: i=kk-'1 or i=kk-'1+1 or i=kk-'1+2 by JORDAN2C:2;
                now assume i=kk-'1+2;then
                  i=kk-'1+(1+1);then
                  i=kk-1+1+1 by E79,XCMPLX_1:1;then
                  H2: i=kk+1 by XCMPLX_1:27;then
                  kk+1<i+1 by NAT_1:38;then
                  LE f/.(kk+1),p44,L~f,f/.1,f/.(len f)
                             by JORDAN5C:24,A2,D790,A6;then
                  p44=f/.(kk+1) by E44,JORDAN5C:12,A2,A2b;then
                  f.(i+1)=f/.(kk+1) by A10,FINSEQ_4:def 4;then
                  G19: f.(i+1)=f.(kk+1) by D789,FINSEQ_4:def 4;
                  f is one-to-one by A2,TOPREAL1:def 10;then
                  i+1=kk+1 by FUNCT_1:def 8,G19,A10,D789;
                 hence contradiction by H2,NAT_1:38;
                end;then
                D202: i=kk-1 or i=kk by E91,E79,XCMPLX_1:27;
                D102:now per cases by D202;
                 case i=kk;
                  hence p44 in LSeg(f,kk) by D7,TOPREAL1:6;
                 end;
                 case i=kk-1;
                  hence p44 in LSeg(f,kk) by D55,XCMPLX_1:27;
                 end;
                 end;
                 f is special by A2,TOPREAL1:def 10;then
                  (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A6e,TOPREAL1:def 7;
                 hence contradiction
                 by D102,D33,C1,E30,A2,G77,E4,JJ80b,D66,A6e;
              end;then
              F82b: kk-'(i+1)=kk-(i+1) by BINARITH:def 3;
              F13: i+1+k2=i+1+(kk-(i+1)) by F82b
                        .=kk by XCMPLX_1:27;then
              i+1+k2>=1 by D34;then
              P[k2] by D66,F13,E0,FINSEQ_4:24;then
              k2>=k by A9;then
              kk-(i+1)+(i+1)>=k+(i+1) by F82b,REAL_1:55;then
              kk >=i+1+k by XCMPLX_1:27;then
              D202b: LE f/.(i+1+k),f/.(kk),L~f,f/.1,f/.(len f)
                  by D66,JORDAN5C:24,A78,A2;
              LE f/.(kk),f/.(i+1+k),L~f,f/.1,f/.(len f)
                                 by E43,A2,JORDAN5C:13,A167,D33;
               hence contradiction by E0,B1,A167,D202b,JORDAN5C:12,A2,A2b;
              end;
              end;
            hence contradiction;
         end;
         for p5 being Point of TOP-REAL 2 st
            LE p5,pk,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1>=e
          proof let p5 be Point of TOP-REAL 2;
            assume F1: LE p5,pk,P,p1,p2 & LE p,p5,P,p1,p2;then
             p5 in P by JORDAN5C:def 3;then
             F3: LE p5,p44,P,p1,p2 or LE p44,p5,P,p1,p2 by A2b,JJ319,A798;
             now per cases by F3;
             case LE p44,p5,P,p1,p2;
              hence p5`1>=e by F1,P2;
             end;
             case LE p5,p44,P,p1,p2;
              hence p5`1>=e by F1,P1;
             end;
             end;
           hence p5`1>=e;
          end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
                    by A711,B1,JJ130,A2b,A1;
     end;
     end;
    hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e;
   end;
   end;
  hence thesis;
end;
