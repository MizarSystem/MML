              proof let p7 be Point of TOP-REAL 2;
               assume A54: p7=f.s70;
                   s70 in [.0,1.] by A10,A53,RCOMP_1:48;
                then A55: p7 in rng f by A7,A54,FUNCT_1:def 5;
                A56: rng f=[#]((TOP-REAL 2)|P) by A4,TOPS_2:def 5
