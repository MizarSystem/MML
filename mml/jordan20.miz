            LE p44,p8,P,p1,p2 & LE p8,p,P,p1,p2} by JORDAN6:29;
           then A142: p5 in Segment(P,p1,p2,p44,p) by A141;
             p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p44,p) c= LSeg(f,i) by A6,A8,A130,TOPREAL1:12;
           then A143: LSeg(p44,p) c= P by A3,A7,A8,XBOOLE_1:1;
              now per cases;
           case p44<>p;
             then LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;
             then Segment(P,p1,p2,p44,p)=LSeg(p44,p) by A2,A140,A143,Th28
;
            hence p5`1>=e by A1,A127,A129,A142,TOPREAL1:9;
           end;
           case p44=p;
            hence p5`1>=e by A1,A127,A129;
           end;
           end;
          hence p5`1>=e;
         end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A1,A2,A127,A129,A140,
Def2;
     end;
     case k<>0; then A144: k>0 by NAT_1:19;
        then A145: k>=0+1 by NAT_1:38;
      set p44=f/.i;
        A146: i>i-'k by A20,A145,JORDAN3:14;
        then A147: i>1 by A20,AXIOMS:22;
        A148: now assume A149: (f/.i)`1<>e;
            f.i=f/.i by A10,FINSEQ_4:def 4;
         then for p9 being Point of TOP-REAL 2 st p9=f.(i-'0)
            holds p9`1<>e by A149,JORDAN3:2;
         hence contradiction by A15,A144;
        end;
        A150: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A8,TOPREAL1:def 5;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
        A151: len g=len f -'i+1 by A4,A8,A9,JORDAN3:27;
        A152: len f-i>0 by A9,SQUARE_1:11;
        then len f-'i=len f-i by BINARITH:def 3;
        then A153: len f-'i+1>0+1 by A152,REAL_1:67;
        then A154: 1+1<=len g by A151,NAT_1:38;
           1+(1+i)<=1+len f by A8,REAL_1:55;
        then A155: 1+1+i-i<=len f+1-i by REAL_1:49;
        then A156: 1<=i & i<=len f & len f<=len f & 1<=1 &
         1<=len f+1-i by A8,AXIOMS:22,NAT_1:38;
           1 in Seg len g by A151,A153,FINSEQ_1:3;
        then 1 in dom g by FINSEQ_1:def 3;
        then A157: g/.1=g.1 by FINSEQ_4:def 4
                .=f.(1-1+i) by A156,JORDAN3:31
                .=f/.i by A10,FINSEQ_4:def 4;
        reconsider ia=i+1 as Nat;
           ia in Seg len f by A8,A12,FINSEQ_1:3;
        then A158: i+1 in dom f by FINSEQ_1:def 3;
           1+1 in Seg len g by A154,FINSEQ_1:3;
        then 1+1 in dom g by FINSEQ_1:def 3;
        then A159: g/.(1+1)=g.(1+1) by FINSEQ_4:def 4
                    .=f.(1+1-1+i) by A8,A9,A155,JORDAN3:31
                    .=f/.(i+1) by A158,FINSEQ_4:def 4;
           LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A8,TOPREAL1:def 5
                .=LSeg(g,1) by A154,A157,A159,TOPREAL1:def 5;
        then p in Y &
          Y in { LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
                           by A6,A8,A154;
        then p in union{ LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <=
len g
}
            by TARSKI:def 4;
        then p in L~mid(f,i,len f) by TOPREAL1:def 6;
        then A160: LE p44,p,P,p1,p2 by A3,A8,A9,SPRECT_4:4;
        A161: for p5 being Point of TOP-REAL 2 st
         LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e
         proof let p5 be Point of TOP-REAL 2;
          assume A162: LE p44,p5,P,p1,p2 & LE p5,p,P,p1,p2;
              Segment(P,p1,p2,p44,p)={p8 where p8 is Point of TOP-REAL 2:
            LE p44,p8,P,p1,p2 & LE p8,p,P,p1,p2} by JORDAN6:29;
           then A163: p5 in Segment(P,p1,p2,p44,p) by A162;
             p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p44,p) c= LSeg(f,i) by A6,A8,A150,TOPREAL1:12;
           then A164: LSeg(p44,p) c= P by A3,A7,A8,XBOOLE_1:1;
              now per cases;
           case p44<>p;
             then LSeg(p44,p) is_an_arc_of p44,p by TOPREAL1:15;
             then Segment(P,p1,p2,p44,p)=LSeg(p44,p) by A2,A160,A164,Th28
;
            hence p5`1>=e by A1,A148,A163,TOPREAL1:9;
           end;
           case p44=p;
             then Segment(P,p1,p2,p44,p)={p44} by A1,A2,Th1;
            hence p5`1>=e by A148,A163,TARSKI:def 1;
           end;
           end;
          hence p5`1>=e;
         end;
        reconsider g=mid(f,1,i) as FinSequence of (TOP-REAL 2);
        A165: len g=i -'1+1 by A8,A9,A11,JORDAN3:27;
           i-1>0 by A147,SQUARE_1:11;
        then A166: i-'1=i-1 by BINARITH:def 3;
        A167: i-'k <i by A20,A145,JORDAN3:14;
        A168: i-'k+1<=len g by A146,A165,A166,NAT_1:38;
        A169: 1<=(i-'k) by A18,A19,BINARITH:def 3;
        A170: 1<=1 & 1<=i & i<=len f & 1<=i-'k &
        i-'k<=i+1-1 by A8,A20,A145,JORDAN3:14,NAT_1:38;
        A171: i-'k+1<=i by A146,NAT_1:38;
        reconsider ia=i+1 as Nat;
           i-'k in Seg len g by A20,A165,A166,A167,FINSEQ_1:3;
        then i-'k in dom g by FINSEQ_1:def 3;
        then A172: g/.(i-'k)=g.(i-'k) by FINSEQ_4:def 4
                    .=f.(i-'k-1+1) by A170,JORDAN3:31
                    .=f/.(i-'k) by A21,FINSEQ_4:def 4;
           LSeg(g,i-'k)=LSeg(g/.(i-'k),g/.(i-'k+1))
             by A20,A168,TOPREAL1:def 5;
          then pk in LSeg(g,i-'k) &
          LSeg(g,i-'k) in
          { LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <= len g }
                           by A22,A165,A166,A169,A171,A172,TOPREAL1:6;
         then pk in union{ LSeg(g,i2) where i2 is Nat : 1 <= i2 & i2+1 <=
len
g }
            by TARSKI:def 4;
         then pk in L~mid(f,1,i) by TOPREAL1:def 6;
         then A173: LE pk,p44,P,p1,p2 by A3,A9,A147,SPRECT_3:33;
         then A174: LE pk,p,P,p1,p2 by A160,JORDAN5C:13;
         A175: p44 in P by A173,JORDAN5C:def 3;
         A176: now assume not
           for p51 being Point of TOP-REAL 2 st
            LE pk,p51,P,p1,p2 & LE p51,p44,P,p1,p2 holds p51`1>=e;
           then consider p51 being Point of TOP-REAL 2 such that
           A177: LE pk,p51,P,p1,p2 & LE p51,p44,P,p1,p2 & p51`1<e;
              p51 in P by A177,JORDAN5C:def 3;
           then consider Y3 being set such that
           A178: p51 in Y3 & Y3 in
            { LSeg(f,i5) where i5 is Nat : 1 <= i5 & i5+1 <= len f }
                     by A3,A5,TARSKI:def 4;
           consider kk being Nat such that
           A179: Y3=LSeg(f,kk) &  (1 <= kk & kk+1 <= len f) by A178;
           A180: kk<len f by A179,NAT_1:38;
           then kk in Seg len f by A179,FINSEQ_1:3;
           then A181: kk in dom f by FINSEQ_1:def 3;
           A182: 1<kk+1 by A179,NAT_1:38;
           then kk+1 in Seg len f by A179,FINSEQ_1:3;
           then A183: kk+1 in dom f by FINSEQ_1:def 3;
           A184: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A179,TOPREAL1:def 5;
           A185: LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                     by A3,A178,A179,JORDAN5C:26;
              now per cases by A177,A178,A179,A184,Th3;
           case A186: (f/.kk)`1<e;
              set k2=i-'kk;
                   LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A3,A178,A179,JORDAN5C:25;
                then A187: LE f/.kk,p44,L~f,f/.1,f/.(len f)
                                 by A3,A177,JORDAN5C:13;
                 now assume i-kk<=0; then i-kk+kk<=0+kk by REAL_1:55;
                then LE f/.i,f/.kk,L~f,f/.1,f/.(len f)
                         by A3,A147,A180,JORDAN5C:24;
               hence contradiction by A2,A3,A148,A186,A187,JORDAN5C:12;
              end;
              then A188: i-k2=i-(i-kk) by BINARITH:def 3 .=kk;
              then A189: i-k2>0 by A179,AXIOMS:22;
              then A190: i-'k2>0 by BINARITH:def 3;
              A191: i-k2=i-'k2 by A189,BINARITH:def 3;
              A192: i-'k2>=0+1 by A190,NAT_1:38;
                 i-'k2<=len f by A9,JORDAN3:13;
              then P[k2] by A186,A188,A191,A192,FINSEQ_4:24;
              then k2>=k by A15;
              then i-k2<=i-k by REAL_2:106;
              then A193: LE f/.(i-'k2),f/.(i-'k),L~f,f/.1,f/.(len f)
                           by A3,A16,A23,A179,A188,A191,JORDAN5C:24;
              A194: LE f/.(i-'k),p51,L~f,f/.1,f/.(len f) by A3,A21,A177,
FINSEQ_4:def 4;
              A195: LE f/.(i-'k),f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A22,A177,A185,JORDAN5C:13;
              then A196: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A188,A191,A193,JORDAN5C:13;
                 Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              then A197: f/.(i-'k) in Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+
1))
                                            by A188,A191,A193,A195;
              A198: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A199: z in LSeg(f/.kk,f/.(kk+1));
                   LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A179,A184;
               hence z in L~f by A5,A199,TARSKI:def 4;
              end;
              A200: f is special by A3,TOPREAL1:def 10;
              A201: f is one-to-one by A3,TOPREAL1:def 10;
                 kk<kk+1 by NAT_1:38;
              then A202: f.kk <> f.(kk+1) by A181,A183,A201,FUNCT_1:def 8;
              A203: f/.kk=f.kk by A181,FINSEQ_4:def 4;
                 f/.(kk+1)=f.(kk+1) by A183,FINSEQ_4:def 4;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A202,A203,TOPREAL1:15;
              then A204: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A2,A3,A196,A198,Th28;
               A205: (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A179,A200,TOPREAL1:def 7;
                 (f/.(kk+1))`1>e by A22,A127,A186,A197,A204,Th2;
              then (f/.kk)`1< (f/.(kk+1))`1 by A186,AXIOMS:22;
              then (f/.(i-'k))`1<=p51`1
                     by A3,A178,A179,A180,A184,A194,A197,A204,A205,Th7;
            hence contradiction by A22,A127,A177,AXIOMS:22;
           end;
           case A206: (f/.(kk+1))`1 <e & (f/.kk)`1>=e;
              then A207: (f/.kk)`1>(f/.(kk+1))`1 by AXIOMS:22;
              set k2=i-'kk-'1;
              A208: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A209: z in LSeg(f/.kk,f/.(kk+1));
                   LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A179,A184;
               hence z in L~f by A5,A209,TARSKI:def 4;
              end;
              A210: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              A211: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                          by A3,A179,JORDAN5C:23;
              A212: f is one-to-one by A3,TOPREAL1:def 10;
                 kk<kk+1 by NAT_1:38;
              then A213: f.kk <> f.(kk+1) by A181,A183,A212,FUNCT_1:def 8;
              A214: f/.kk=f.kk by A181,FINSEQ_4:def 4;
                 f/.(kk+1)=f.(kk+1) by A183,FINSEQ_4:def 4;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A213,A214,TOPREAL1:15;
              then A215: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A2,A3,A208,A211,Th28;
                   LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A3,A178,A179,JORDAN5C:25;
                then A216: LE f/.kk,p44,L~f,f/.1,f/.(len f)
                                 by A3,A177,JORDAN5C:13;
                A217: now assume
                  i-kk-1<=0;
               then i-(kk+1)+(kk+1)<=0+(kk+1) by REAL_1:55;
                then LE f/.i,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A3,A147,A179,JORDAN5C:24;
                then A218: f/.i in LSeg(f/.kk,f/.(kk+1)) by A210,A215,A216;
                A219: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A179,TOPREAL1:def 5;
                   f is special by A3,TOPREAL1:def 10;
                  then (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A179,TOPREAL1:def 7;
                 hence contradiction
                       by A3,A148,A177,A178,A179,A180,A207,A218,A219,Th6;
              end;
              then i-kk-1+1>=0+1 by REAL_1:55;
              then i-kk>0 by AXIOMS:22;
              then i-'kk=i-kk by BINARITH:def 3;
              then A220: i-k2=i-(i-kk-1) by A217,BINARITH:def 3 .=kk+1;
              then A221: i-k2>0 by A182,AXIOMS:22;
              then A222: i-'k2>0 by BINARITH:def 3;
              A223: i-k2=i-'k2 by A221,BINARITH:def 3;
              A224: i-'k2>=0+1 by A222,NAT_1:38;
                 i-'k2<=len f by A9,JORDAN3:13;
              then P[k2] by A206,A220,A223,A224,FINSEQ_4:24;
              then k2>=k by A15;
              then i-k2<=i-k by REAL_2:106;
              then A225: LE f/.(kk+1),f/.(i-'k),L~f,f/.1,f/.(len f)
                           by A3,A16,A23,A220,A223,A224,JORDAN5C:24;
                 LE f/.(i-'k),f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A22,A177,A185,JORDAN5C:13;
               hence contradiction by A2,A3,A22,A127,A206,A225,JORDAN5C:12;
              end;
              end;
            hence contradiction;
         end;
            for p5 being Point of TOP-REAL 2 st
            LE pk,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e
          proof let p5 be Point of TOP-REAL 2;
            assume A226: LE pk,p5,P,p1,p2 & LE p5,p,P,p1,p2;
             then A227: p5 in P by JORDAN5C:def 3;
                now per cases by A2,A175,A227,Th19;
             case LE p5,p44,P,p1,p2;
              hence p5`1>=e by A176,A226;
             end;
             case LE p44,p5,P,p1,p2;
              hence p5`1>=e by A161,A226;
             end;
             end;
           hence p5`1>=e;
          end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A1,A2,A127,A174,Def2;
     end;
     end;
    hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e;
   end;
   end;
  hence thesis;
end;

theorem    for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
   then A2: P is_an_arc_of p1,p2 by TOPREAL4:3;
   consider f being FinSequence of (TOP-REAL 2) such that
   A3:  f is_S-Seq & P = L~f & p1=f/.1 & p2=f/.len f by A1,TOPREAL4:def 1;
   A4: L~f = union { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
                          by TOPREAL1:def 6;
   then consider Y being set such that
   A5: p in Y & Y in
      { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
            by A1,A3,TARSKI:def 4;
   A6: Y c= L~f
   proof let x be set;assume x in Y;
    hence x in L~f by A4,A5,TARSKI:def 4;
   end;
   consider i being Nat such that
   A7: Y=LSeg(f,i) &  (1 <= i & i+1 <= len f) by A5;
   A8: 1<i+1 by A7,NAT_1:38;
   A9: i<len f by A7,NAT_1:38;
      i+1 in Seg len f by A7,A8,FINSEQ_1:3;
   then A10: i+1 in dom f by FINSEQ_1:def 3;
   A11: 1<len f by A7,A9,AXIOMS:22;
   A12: 1<i+1 by A7,NAT_1:38;
   defpred P[Nat] means
    for p being Point of TOP-REAL 2 st p=f.(i+1+$1)
        holds p`1<>e;
   A13: len f - (i+1)>=0 by A7,SQUARE_1:12;
      len f in Seg len f by A11,FINSEQ_1:3;
   then A14: len f in dom f by FINSEQ_1:def 3;
      i+1+((len f)-'(i+1))=i+1+(len f -(i+1)) by A13,BINARITH:def 3
                      .=len f;
   then A15: P[(len f)-'(i+1)] by A1,A3,A14,FINSEQ_4:def 4;
   then A16: ex k being Nat st P[k];
      ex k being Nat st P[k] & for n being Nat st P[n] holds k <= n
                                  from NAT_1:sch 5(A16);
   then consider k being Nat such that
   A17: P[k] & for n being Nat st P[n] holds k <= n;
   A18: k<=len f -'(i+1) by A15,A17;
      len f -'(i+1)=len f -(i+1) by A13,BINARITH:def 3;
   then A19: k+(i+1)<= len f -(i+1)+(i+1) by A18,REAL_1:55;
      i+k >=i by NAT_1:29;
   then A20: i+k+1 >=i+1 by REAL_1:55;
   then A21: k+i+1>1 by A12,AXIOMS:22;
   A22: i+1+k >1 by A12,A20,AXIOMS:22;
      1<=i+1+k by A8,NAT_1:37;
   then i+1+k in Seg len f by A19,FINSEQ_1:3;
   then A23: i+1+k in dom f by FINSEQ_1:def 3;
   then A24: f/.(i+1+k)=f.(i+1+k) by FINSEQ_4:def 4;
   then reconsider pk=f.(i+1+k) as Point of TOP-REAL 2;
   A25: pk`1<> e by A17;
   reconsider kn=k+1 as Nat;
      now per cases by A25,AXIOMS:21;
   case A26: pk`1<e;
        now per cases;
     case A27: k=0;
      set p44=f/.(i+1);
        A28: pk=p44 by A10,A27,FINSEQ_4:def 4;
        A29: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A7,TOPREAL1:def 5;
           p44 in LSeg(p,f/.(i+1)) by TOPREAL1:6;
        then A30: LE p,p44,P,p1,p2 by A3,A5,A7,A9,SPRECT_4:5;
           for p5 being Point of TOP-REAL 2 st
         LE p5,p44,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume A31: LE p5,p44,P,p1,p2 & LE p,p5,P,p1,p2;
              Segment(P,p1,p2,p,p44)={p8 where p8 is Point of TOP-REAL 2:
            LE p,p8,P,p1,p2 & LE p8,p44,P,p1,p2} by JORDAN6:29;
