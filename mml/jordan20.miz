theorem Th41a:for P being Subset of I[01],s being Real st
 P=[.0,s.[ holds P is open
proof let P be Subset of I[01],s be Real;
  assume A1:P=[.0,s.[;
   A106: [#]I[01]=the carrier of I[01] by PRE_TOPC:12;then
   A106b: [#]I[01]=[.0,1.] by TOPMETR:25,27;
  per cases;
  suppose A2: s in [.0,1.];then
   A8: 0<=s & s<=1 by TOPREAL5:1;
   reconsider Q=].-1,s.[ as Subset of R^1 by TOPMETR:24;
   A3: Q is open by JORDAN6:46;
   reconsider T=[.0,1.] as Subset of R^1 by TOPMETR:24;
   A9: [.0,s.[ c= [.0,s.] by RCOMP_2:17;
   0 in [.0,1.] by RCOMP_1:15;then
   [.0,s.] c= [.0,1.] by A2,RCOMP_1:16;then
   [.0,s.[ c= [.0,1.] by A9,XBOOLE_1:1;then
   P c= [#](R^1|T) by A1,PRE_TOPC:def 10;then
   reconsider P2=P as Subset of R^1|T by PRE_TOPC:12;
   A55: ].-1,s.[ /\ [.0,1.] c= [.0,s.[
   proof let x be set;assume x in ].-1,s.[ /\ [.0,1.];then
     B6: x in ].-1,s.[ & x in [.0,1.] by XBOOLE_0:def 3;then
     reconsider sx=x as Real;
     B7: -1<sx & sx<s by B6,JORDAN6:45;
     0<=sx & sx<=1 by B6,TOPREAL5:1;
    hence x in [.0,s.[ by RCOMP_2:3,B7;
   end;
   [.0,s.[ c= ].-1,s.[ /\ [.0,1.]
   proof let x be set;assume B1: x in [.0,s.[;then
     reconsider sx=x as Real;
     B3: 0<=sx & sx<s by B1,RCOMP_2:3;then
     B4: sx<=1 by A8,AXIOMS:22;
     -1 < sx by B3,AXIOMS:22;then
     x in ].-1,s.[ & x in [.0,1.] by B3,B4,TOPREAL5:1,JORDAN6:45;
    hence x in ].-1,s.[ /\ [.0,1.] by XBOOLE_0:def 3;
   end;then
   [.0,s.[ = ].-1,s.[ /\ [.0,1.] by A55,XBOOLE_0:def 10;then
   P2=Q /\ [#](R^1|T) by A1,PRE_TOPC:def 10;then
   A5: P2 is open by TOPS_2:32,A3;
   Closed-Interval-TSpace(0,1) =R^1|T by TOPMETR:26;
 hence P is open by A5,TOPMETR:27;
 end;
 suppose not s in [.0,1.];then
   B10: not (0<=s & s<=1) by TOPREAL5:1;
   now per cases by B10;
   case s<0;then
     [.0,s.[ ={} by RCOMP_2:9;then
     P in the topology of I[01] by A1,PRE_TOPC:5;
    hence P is open by PRE_TOPC:def 5;
   end;
   case s>1;then
     consider t being real number such that
     C3: 1<t & t<s by REAL_1:75;
     C5: [.0,s.[ c= [.0,1.] by A1,A106,A106b;
     C4: for r being Real st 0<=r & r<s holds r<=1
     proof let r be Real;assume  0<=r & r<s;then
       r in [.0,s.[ by RCOMP_2:3;
      hence r<=1 by C5,TOPREAL5:1;
     end;
     reconsider t as Real by XREAL_0:def 1;
     0<t by C3,AXIOMS:22;
    hence contradiction by C3,C4;
  end;
  end;
  hence P is open;
 end;
end;

theorem Th41b:for P being Subset of I[01],s being Real st
 P=].s,1.] holds P is open
proof let P be Subset of I[01],s be Real;
  assume A1:P=].s,1.];
   A106: [#]I[01]=the carrier of I[01] by PRE_TOPC:12;then
   A106b: [#]I[01]=[.0,1.] by TOPMETR:25,27;
  per cases;
  suppose A2: s in [.0,1.];then
   A8: 0<=s & s<=1 by TOPREAL5:1;
   reconsider Q=].s,2.[ as Subset of R^1 by TOPMETR:24;
   A3: Q is open by JORDAN6:46;
   reconsider T=[.0,1.] as Subset of R^1 by TOPMETR:24;
   A9: ].s,1.] c= [.s,1.] by RCOMP_2:17;
   1 in [.0,1.] by RCOMP_1:15;then
   [.s,1.] c= [.0,1.] by A2,RCOMP_1:16;then
   ].s,1.] c= [.0,1.] by A9,XBOOLE_1:1;then
   P c= [#](R^1|T) by A1,PRE_TOPC:def 10;then
   reconsider P2=P as Subset of R^1|T by PRE_TOPC:12;
   A55: ].s,2.[ /\ [.0,1.] c= ].s,1.]
   proof let x be set;assume x in ].s,2.[ /\ [.0,1.];then
     B6: x in ].s,2.[ & x in [.0,1.] by XBOOLE_0:def 3;then
     reconsider sx=x as Real;
     B7: s<sx & sx<=2 by B6,JORDAN6:45;
     0<=sx & sx<=1 by B6,TOPREAL5:1;
    hence x in ].s,1.] by RCOMP_2:4,B7;
   end;
   ].s,1.] c= ].s,2.[ /\ [.0,1.]
   proof let x be set;assume B1: x in ].s,1.];then
     reconsider sx=x as Real;
     B3: s<sx & sx<=1 by B1,RCOMP_2:4;then
     B4: 0<sx by A8;
     2 > sx by B3,AXIOMS:22;then
     x in ].s,2.[ & x in [.0,1.] by B3,B4,TOPREAL5:1,JORDAN6:45;
    hence x in ].s,2.[ /\ [.0,1.] by XBOOLE_0:def 3;
   end;then
   ].s,1.] = ].s,2.[ /\ [.0,1.] by A55,XBOOLE_0:def 10;then
   P2=Q /\ [#](R^1|T) by A1,PRE_TOPC:def 10;then
   A5: P2 is open by A3,TOPS_2:32;
   Closed-Interval-TSpace(0,1) =R^1|T by TOPMETR:26;
 hence P is open by A5,TOPMETR:27;
 end;
 suppose not s in [.0,1.];then
   B10: not (0<=s & s<=1) by TOPREAL5:1;
   now per cases by B10;
   case s>1;then
     ].s,1.] ={} by RCOMP_2:10;then
     P in the topology of I[01] by A1,PRE_TOPC:5;
    hence P is open by PRE_TOPC:def 5;
   end;
   case s<0;then
     consider t being real number such that
     C3: s<t & t<0 by REAL_1:75;
     C5: ].s,1.] c= [.0,1.] by A1,A106,A106b;
     C4: for r being Real st s<r & r<=1 holds r>=0
     proof let r be Real;assume s<r & r<=1;then
       r in ].s,1.] by RCOMP_2:4;
      hence r>=0 by C5,TOPREAL5:1;
     end;
     reconsider t as Real by XREAL_0:def 1;
     t<=1 by C3,AXIOMS:22;
    hence contradiction by C3,C4;
  end;
  end;
  hence P is open;
 end;
end;

theorem JJ311: for P being non empty Subset of TOP-REAL 2,
P1 being Subset of (TOP-REAL 2)|P ,Q being Subset of I[01],
 f being map of I[01],(TOP-REAL 2)|P,s being Real st s<=1 &
  P1= {q0 where q0 is Point of TOP-REAL 2:
   ex ss being Real st 0<=ss & ss<s & q0=f.ss} &
  Q=[.0,s.[ holds f.:Q=P1
proof let P be non empty Subset of TOP-REAL 2,
 P1 be Subset of (TOP-REAL 2)|P, Q be Subset of I[01],
 f be map of I[01],(TOP-REAL 2)|P,s be Real;
 assume A1: s<=1 &
  P1= {q0 where q0 is Point of TOP-REAL 2:
   ex ss being Real st 0<=ss & ss<s & q0=f.ss} & Q=[.0,s.[;
  A2: the carrier of (TOP-REAL 2)|P=P by JORDAN1:1;
  A6: P1 c= f.:Q
  proof let x be set;assume x in P1;then
    consider q0 being Point of TOP-REAL 2 such that
    B2: q0=x & ex ss being Real st 0<=ss & ss<s & q0=f.ss by A1;
    consider ss being Real such that
    B3: 0<=ss & ss<s & q0=f.ss by B2;
    B6: ss<1 by A1,B3,AXIOMS:22;
    B4: ss in Q by B3,A1,RCOMP_2:3;
    dom f= [.0,1.] by BORSUK_1:83,FUNCT_2:def 1;then
    ss in dom f by B6,B3,TOPREAL5:1;
   hence x in f.:Q by FUNCT_1:def 12,B4,B2,B3;
  end;
  f.:Q c= P1
  proof let y be set;assume y in f.:Q;then
    consider z being set such that
    B1: z in dom f & z in Q & f.z=y by FUNCT_1:def 12;
    dom f= [.0,1.] by BORSUK_1:83,FUNCT_2:def 1;then
    reconsider ss=z as Real by B1;
    B5: 0<=ss & ss<s by B1,A1,RCOMP_2:3;
    B6: y in rng f by B1,FUNCT_1:def 5;
    y in P by B6,A2;then
    reconsider q=y as Point of TOP-REAL 2;
    ex ss being Real st 0<=ss & ss<s & q=f.ss by B1,B5;
   hence y in P1 by A1;
  end;
 hence f.:Q=P1 by A6,XBOOLE_0:def 10;
end;

theorem JJ311c: for P being non empty Subset of TOP-REAL 2,
