           hence p5`1>=e by A95,A100;
          end;
          case LE p5,p9,P,p1,p2;
           hence p5`1>=e by A23,A99;
          end;
          end;
         hence p5`1>=e;
        end;
       hence p is_Rout P,p1,p2,e by Def4,A1,A97,A98;
      end;
         now assume A103: not ex p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2 & p51`1>e;
        A104: for p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2
        holds p51`1=e
        proof let p51 be Point of TOP-REAL 2;
          assume A105: LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2;
          then A106: p51`1<=e by A103;
            p51`1>=e by A105,A95;
         hence p51`1=e by A106,AXIOMS:21;
        end;
          p4 in P by JORDAN5C:def 3,A69;
        then p4 in rng f by PRE_TOPC:def 10,A6;
        then consider xs4 being set such that
        A107: xs4 in dom f & p4=f.xs4 by FUNCT_1:def 5;
        reconsider s4=xs4 as Real by A107,A7;
        A108: 0<=s4 & s4<=1 by A6,A107,BORSUK_1:83,TOPREAL5:1;
        then A109: s4>=s0 by A4,A10,A17,A20,A69,A107,JORDAN5C:def 3;
          now assume s4>s0;
         then --(s4-s0)>0 by SQUARE_1:11;
         then -(s4-s0)<0 by REAL_1:66;
         then s0-s4<0 by XCMPLX_1:143;
         then A110: (s0-s4)/2<0 by REAL_2:128;
         then A111: -((s0-s4)/2)>0 by REAL_1:66;
         A112: s4+0>s4+(s0-s4)/2 by A110,REAL_1:67;
          consider r being real number such that
          A113: r in R & r<s0+(-((s0-s4)/2)) by A111,SEQ_4:def 5,A21;
          A114: r>=s0 by A113,SEQ_4:def 5,A21;
            s0+(-((s0-s4)/2))-s4>r-s4 by A113,REAL_1:54;
          then s0-((s0-s4)/2)-s4>r-s4 by XCMPLX_0:def 8;
          then s0-s4-((s0-s4)/2)>r-s4 by XCMPLX_1:21;
          then s0-s4+-((s0-s4)/2)>r-s4 by XCMPLX_0:def 8;
          then (s0-s4)/2+(s0-s4)/2+-((s0-s4)/2)>r-s4 by XCMPLX_1:66;
          then (s0-s4)/2+(s0-s4)/2-((s0-s4)/2)>r-s4 by XCMPLX_0:def 8;
          then ((s0-s4)/2)>r-s4 by XCMPLX_1:26;
          then s4+((s0-s4)/2)>r by REAL_1:84;
          then A115: s4>r by A112,AXIOMS:22;
          then A116: 1>r by A108,AXIOMS:22;
          consider s7 being Real such that
          A117: s7=r & 1>=s7 & s7>=s2 &
          (for q being Point of TOP-REAL 2 st q=f.s7 holds q`1<>e) by A113;
          A118: r in [.0,1.] by A10,A117,TOPREAL5:1;
          reconsider rss=r as Real by XREAL_0:def 1;
            f.rss in rng f by A7,A118,FUNCT_1:def 5;
          then f.rss in P by A6,PRE_TOPC:def 10;
          then reconsider pss=f.rss as Point of TOP-REAL 2;
            LE pss,p4,P,p1,p2 & LE p9,pss,P,p1,p2
               by A1,A4,A10,A17,A20,A107,A108,A114,A115,A116,JORDAN5C:8;
          then pss`1=e by A104;
         hence contradiction by A117;
