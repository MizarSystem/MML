              then A206: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A198,A200,A3,A2,Th28;
               A207: (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A181,TOPREAL1:def 7,A202;
                (f/.(kk+1))`1>e by A188,A199,A206,Th2,A129,A22;
              then (f/.kk)`1< (f/.(kk+1))`1 by A188,AXIOMS:22;
              then (f/.(i-'k))`1<=p51`1
                     by A3,A180,A181,A182,A186,A196,A199,A206,
A207,Th7;
            hence contradiction by A179,A129,A22,AXIOMS:22;
           end;
           case A208: (f/.(kk+1))`1 <e & (f/.kk)`1>=e;
              then A209: (f/.kk)`1>(f/.(kk+1))`1 by AXIOMS:22;
              set k2=i-'kk-'1;
              A210: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A211: z in LSeg(f/.kk,f/.(kk+1));
                  LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A181,A186;
               hence z in L~f by A211,A5,TARSKI:def 4;
              end;
              A212: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              A213: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                          by A3,A181,JORDAN5C:23;
              A214: f is one-to-one by A3,TOPREAL1:def 10;
                kk<kk+1 by NAT_1:38;
              then A215: f.kk <> f.(kk+1) by FUNCT_1:def 8,A214,A183,A185;
              A216: f/.kk=f.kk by FINSEQ_4:def 4,A183;
                f/.(kk+1)=f.(kk+1) by FINSEQ_4:def 4,A185;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A215,A216,TOPREAL1:15;
              then A217: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A213,A210,A3,A2,Th28;
                  LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A3,A180,A181,JORDAN5C:25;
                then A218: LE f/.kk,p44,L~f,f/.1,f/.(len f)
                                 by A179,A3,JORDAN5C:13;
                now assume
                 i-kk-1<=0;
               then i-(kk+1)<=0 by XCMPLX_1:36;
               then i-(kk+1)+(kk+1)<=0+(kk+1) by REAL_1:55;
               then kk+1>=i by XCMPLX_1:27;
                then LE f/.i,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A3,A149,A181,JORDAN5C:24;
                then A219: f/.i in LSeg(f/.kk,f/.(kk+1)) by A217,A212,A218;
                A220: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by TOPREAL1:def 5,A181;
                  f is special by A3,TOPREAL1:def 10;
                  then (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A181,TOPREAL1:def 7;
                 hence contradiction
                       by A3,A150,A179,A180,A181,A182,A209,A219,A220,Th6;
              end;
              then i-kk-1+1>=0+1 by REAL_1:55;
              then A221: i-kk>=1 by XCMPLX_1:27;
              then A222: i-kk>0 by AXIOMS:22;
              then A223: i-'kk=i-kk by BINARITH:def 3;
              A224: i-'kk-1=i-kk-1 by A222,BINARITH:def 3;
               i-'kk-1>=0 by A221,A223,SQUARE_1:12;
              then A225: i-k2=i-(i-kk-1) by A224,BINARITH:def 3 .=i-(i-kk)+1 by
XCMPLX_1:37
                      .=i-i+kk+1 by XCMPLX_1:37
                      .=0+kk+1 by XCMPLX_1:14
                      .=kk+1;
              then A226: i-k2>0 by A184,AXIOMS:22;
              then A227: i-'k2>0 by BINARITH:def 3;
              A228: i-k2=i-'k2 by A226,BINARITH:def 3;
              A229: i-'k2>=0+1 by A227,NAT_1:38;
                i-'k2<=len f by A9,JORDAN3:13;
              then P[k2] by A228,A225,A208,FINSEQ_4:24,A229;
              then k2>=k by A15;
              then i-k2<=i-k by REAL_2:106;
              then A230: LE f/.(kk+1),f/.(i-'k),L~f,f/.1,f/.(len f)
                           by A225,A228,JORDAN5C:24,A16,A229,A3,A23;
                LE f/.(i-'k),f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A187,A3,JORDAN5C:13,A22,A179;
               hence contradiction by A208,A129,A22,A230,JORDAN5C:12,A3,A2;
              end;
              end;
            hence contradiction;
         end;
           for p5 being Point of TOP-REAL 2 st
            LE pk,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e
          proof let p5 be Point of TOP-REAL 2;
            assume A231: LE pk,p5,P,p1,p2 & LE p5,p,P,p1,p2;
             then A232: p5 in P by JORDAN5C:def 3;
               now per cases by A2,A177,A232,Th19;
             case LE p5,p44,P,p1,p2;
              hence p5`1>=e by A231,A178;
             end;
             case LE p44,p5,P,p1,p2;
              hence p5`1>=e by A231,A163;
             end;
             end;
           hence p5`1>=e;
          end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A176,A129,Def2,A2,A1;
     end;
     end;
    hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e;
   end;
   end;
  hence thesis;
end;

theorem   for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
   then A2: P is_an_arc_of p1,p2 by TOPREAL4:3;
   consider f being FinSequence of (TOP-REAL 2) such that
   A3:  f is_S-Seq & P = L~f & p1=f/.1 & p2=f/.len f by TOPREAL4:def 1,A1;
   A4: L~f = union { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
                          by TOPREAL1:def 6;
   then consider Y being set such that
   A5: p in Y & Y in
      { LSeg(f,i) where i is Nat : 1 <= i & i+1 <= len f }
            by TARSKI:def 4,A3,A1;
   A6: Y c= L~f
   proof let x be set;assume x in Y;
    hence x in L~f by A4,A5,TARSKI:def 4;
   end;
   consider i being Nat such that
   A7: Y=LSeg(f,i) &  (1 <= i & i+1 <= len f) by A5;
   A8: 1<i+1 by A7,NAT_1:38;
   A9: i<len f by A7,NAT_1:38;
     i+1 in Seg len f by A7,A8,FINSEQ_1:3;
   then A10: i+1 in dom f by FINSEQ_1:def 3;
   A11: 1<len f by A7,A9,AXIOMS:22;
   A12: 1<i+1 by A7,NAT_1:38;
   defpred P[Nat] means
    for p being Point of TOP-REAL 2 st p=f.(i+1+$1)
        holds p`1<>e;
   A13: len f - (i+1)>=0 by A7,SQUARE_1:12;
     len f in Seg len f by A11,FINSEQ_1:3;
   then A14: len f in dom f by FINSEQ_1:def 3;
     i+1+((len f)-'(i+1))=i+1+(len f -(i+1)) by A13,BINARITH:def 3
                      .=len f by XCMPLX_1:27;
   then A15: P[(len f)-'(i+1)] by A1,A3,A14,FINSEQ_4:def 4;
   then A16: ex k being Nat st P[k];
     ex k being Nat st P[k] & for n being Nat st P[n] holds k <= n
                                  from NAT_1:sch 5(A16);
   then consider k being Nat such that
   A17: P[k] & for n being Nat st P[n] holds k <= n;
   A18: k<=len f -'(i+1) by A17,A15;
     len f -'(i+1)=len f -(i+1) by BINARITH:def 3,A13;
   then k+(i+1)<= len f -(i+1)+(i+1) by REAL_1:55,A18;
   then A19: k+(i+1)<=len f by XCMPLX_1:27;
     i+k >=i by NAT_1:29;
   then i+k+1 >=i+1 by REAL_1:55;
   then k+i+1>1 by AXIOMS:22,A12;
   then A20: i+1+k >1 by XCMPLX_1:1;
     1<=i+1+k by A8,NAT_1:37;
   then i+1+k in Seg len f by A19,FINSEQ_1:3;
   then A21: i+1+k in dom f by FINSEQ_1:def 3;
   then A22: f/.(i+1+k)=f.(i+1+k) by FINSEQ_4:def 4;
   then reconsider pk=f.(i+1+k) as Point of TOP-REAL 2;
   A23: pk`1<> e by A17;
   reconsider kn=k+1 as Nat;
     now per cases by A23,AXIOMS:21;
   case A24: pk`1<e;
       now per cases;
     case A25: k=0;
      set p44=f/.(i+1);
        A26: pk=p44 by A25,A10,FINSEQ_4:def 4;
        A27: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A7;
          p44 in LSeg(p,f/.(i+1)) by TOPREAL1:6;
        then A28: LE p,p44,P,p1,p2 by SPRECT_4:5,A3,A9,A5,A7;
          for p5 being Point of TOP-REAL 2 st
         LE p5,p44,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume A29: LE p5,p44,P,p1,p2 & LE p,p5,P,p1,p2;
             Segment(P,p1,p2,p,p44)={p8 where p8 is Point of TOP-REAL 2:
            LE p,p8,P,p1,p2 & LE p8,p44,P,p1,p2} by JORDAN6:29;
           then A30: p5 in Segment(P,p1,p2,p,p44) by A29;
            p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p,p44) c= LSeg(f,i) by A5,A7,A27,TOPREAL1:12;
           then A31: LSeg(p,p44) c= P by XBOOLE_1:1,A3,A7,A6;
             now per cases;
           case p44<>p;
             then LSeg(p,p44) is_an_arc_of p,p44 by TOPREAL1:15;
             then Segment(P,p1,p2,p,p44)=LSeg(p,p44) by Th28,A28,A31,A2;
            hence p5`1<=e by A1,A24,A26,A30,TOPREAL1:9;
           end;
           case p44=p;
            hence p5`1<=e by A1,A24,A25,A10,FINSEQ_4:def 4;
           end;
           end;
          hence p5`1<=e;
         end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
                     by A1,A2,A24,A26,A28,Def3;
     end;
     case k<>0;
        then A32: k>0 by NAT_1:19;
      set p44=f/.(i+1);
        A33: now assume (f/.(i+1))`1<>e;
         then  for p9 being Point of TOP-REAL 2 st p9=f.(i+1+0)
            holds p9`1<>e by FINSEQ_4:def 4,A10;
         hence contradiction by A17,A32;
        end;
        A34: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by TOPREAL1:def 5,A7;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
          p44 in LSeg(p,f/.(i+1)) by TOPREAL1:6;
        then A35: LE p,p44,P,p1,p2 by SPRECT_4:5,A3,A7,A9,A5;
        A36: for p5 being Point of TOP-REAL 2 st
         LE p,p5,P,p1,p2 & LE p5,p44,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume A37: LE p,p5,P,p1,p2 & LE p5,p44,P,p1,p2;
             Segment(P,p1,p2,p,p44)={p8 where p8 is Point of TOP-REAL 2:
            LE p,p8,P,p1,p2 & LE p8,p44,P,p1,p2} by JORDAN6:29;
           then A38: p5 in Segment(P,p1,p2,p,p44) by A37;
            p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p,p44) c= LSeg(f,i) by A5,A7,A34,TOPREAL1:12;
           then A39: LSeg(p,p44) c= P by XBOOLE_1:1,A3,A7,A6;
             now per cases;
           case p44<>p;
             then LSeg(p,p44) is_an_arc_of p,p44 by TOPREAL1:15;
             then Segment(P,p1,p2,p,p44)=LSeg(p,p44) by Th28,A35,A39,A2;
            hence p5`1<=e by A1,A33,A38,TOPREAL1:9;
           end;
           case p44=p;
             then Segment(P,p1,p2,p,p44)={p44} by A2,Th1,A1;
            hence p5`1<=e by A33,A38,TARSKI:def 1;
           end;
           end;
          hence p5`1<=e;
         end;
        reconsider g=mid(f,1,i) as FinSequence of (TOP-REAL 2);
           i+1<=i+1+k by NAT_1:29;
         then A40: LE p44,pk,P,p1,p2
                 by A8,A19,A3,A22,JORDAN5C:24;
         then A41: LE p,pk,P,p1,p2 by A35,JORDAN5C:13;
         A42: p44 in P by A40,JORDAN5C:def 3;
         A43: now assume not
           for p51 being Point of TOP-REAL 2 st
            LE p44,p51,P,p1,p2 & LE p51,pk,P,p1,p2 holds p51`1<=e;
           then consider p51 being Point of TOP-REAL 2 such that
           A44: LE p44,p51,P,p1,p2 & LE p51,pk,P,p1,p2 & p51`1>e;
             p51 in P by A44,JORDAN5C:def 3;
           then consider Y3 being set such that
           A45: p51 in Y3 & Y3 in
            { LSeg(f,i5) where i5 is Nat : 1 <= i5 & i5+1 <= len f }
                     by TARSKI:def 4,A4,A3;
           consider kk being Nat such that
           A46: Y3=LSeg(f,kk) &  (1 <= kk & kk+1 <= len f) by A45;
           A47: kk-1>=0 by A46,SQUARE_1:12;
           A48: kk<len f by A46,NAT_1:38;
           then A49: kk in dom f by A46,FINSEQ_3:27;
           A50: 1<kk+1 by A46,NAT_1:38;
           then A51: kk+1 in dom f by A46,FINSEQ_3:27;
           A52: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A46,TOPREAL1:def 5;
           then A53: f/.kk in LSeg(f,kk) by TOPREAL1:6;
           A54: LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                     by A3,A45,A46,JORDAN5C:26;
           A55: LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A3,A45,A46,JORDAN5C:25;
           A56: f is one-to-one by A3,TOPREAL1:def 10;
             kk<kk+1 by NAT_1:38;
           then A57: f.kk <> f.(kk+1) by FUNCT_1:def 8,A56,A49,A51;
           A58: f/.kk=f.kk by FINSEQ_4:def 4,A49;
             now per cases by A44,A45,A46,A52,Th2;
           case A59: (f/.(kk+1))`1>e;
              set k2=kk-'i;
                A60: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A44,A54,A3,JORDAN5C:13;
              A61: now assume kk-i<0; then kk-i+i<0+i by REAL_1:53
;
                then kk<i by XCMPLX_1:27;
                then kk+1<=i+1 by REAL_1:55;
                then LE f/.(kk+1),f/.(i+1),L~f,f/.1,f/.(len f)
                         by A50,A3,A7,JORDAN5C:24;
               hence contradiction by A2,A3,A33,A59,A60,JORDAN5C:12;
              end;
              A62: 1<kk+1 by A46,NAT_1:38;
              A63: i+1+k2=1+i+(kk-i) by A61,BINARITH:def 3
                        .=1+(i+(kk-i)) by XCMPLX_1:1
                        .=kk+1 by XCMPLX_1:27;
               then P[k2] by A46,A59,A62,FINSEQ_4:24;
              then k2>=k by A17;
              then i+1+k2>=i+1+k by REAL_1:55;
              then A64: LE f/.(i+1+k),f/.(i+1+k2),L~f,f/.1,f/.(len f)
                           by JORDAN5C:24,A20,A63,A46,A3;
              A65: LE p51,f/.(i+1+k),L~f,f/.1,f/.(len f) by A3,A21,A44,
FINSEQ_4
:def 4;
              A66: LE f/.(kk),f/.(i+1+k),L~f,f/.1,f/.(len f)
                                 by A3,A22,A44,A55,JORDAN5C:13;
              then A67: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A64,A63,JORDAN5C:13;
                Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              then A68: f/.(i+1+k) in Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk
+
1))
                                            by A64,A66,A63;
              A69: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A70: z in LSeg(f/.kk,f/.(kk+1));
                  LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A46,A52;
               hence z in L~f by A70,A4,TARSKI:def 4;
              end;
              A71: f is special by A3,TOPREAL1:def 10;
              A72: f is one-to-one by A3,TOPREAL1:def 10;
                kk<kk+1 by NAT_1:38;
              then A73: f.kk <> f.(kk+1) by FUNCT_1:def 8,A72,A49,A51;
              A74: f/.kk=f.kk by FINSEQ_4:def 4,A49;
                f/.(kk+1)=f.(kk+1) by FINSEQ_4:def 4,A51;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A73,A74,TOPREAL1:15;
              then A75: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A67,A69,A3,A2,Th28;
               A76: (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A46,TOPREAL1:def 7,A71;
                (f/.(kk))`1<e by A59,A68,A75,Th3,A24,A22;
              then (f/.kk)`1< (f/.(kk+1))`1 by A59,AXIOMS:22;
              then (f/.(i+1+k))`1>=p51`1
                     by A3,A45,A46,A48,A52,A65,A68,A75,A76
,Th7;
            hence contradiction by A44,A24,A22,AXIOMS:22;
           end;
           case A77: (f/.(kk))`1 >e & (f/.(kk+1))`1<=e;
              then A78: (f/.kk)`1>(f/.(kk+1))`1 by AXIOMS:22;
              set k2=kk-'(i+1);
              A79: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A80: z in LSeg(f/.kk,f/.(kk+1));
                  LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A46,A52;
               hence z in L~f by A80,A4,TARSKI:def 4;
              end;
              A81: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              A82: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                          by A3,A46,JORDAN5C:23;
                f/.(kk+1)=f.(kk+1) by FINSEQ_4:def 4,A51;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A57,A58,TOPREAL1:15;
              then A83: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A82,A79,A3,A2,Th28;
                LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A45,A46,JORDAN5C:26;
                then A84: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A44,A3,JORDAN5C:13;
              A85: now assume
                  kk-(i+1)<0;
                then kk-(i+1)+(i+1)<0+(i+1) by REAL_1:53;
                then kk<i+1 by XCMPLX_1:27;
                then kk<=i by NAT_1:38;
                then A86: LE f/.kk,f/.i,L~f,f/.1,f/.(len f)
                         by A3,A9,A46,JORDAN5C:24;
