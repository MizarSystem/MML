end;

reserve j for Element of NAT;

theorem Th6:
  for f being S-Sequence_in_R2, p,q being Point of TOP-REAL 2 st 1
<=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) & (f/.j)`2=(f/.(j+1))`2 & (f
  /.j)`1>(f/.(j+1))`1 & LE p,q,L~f,f/.1,f/.(len f) holds p`1>=q`1
proof
  let f be S-Sequence_in_R2, p,q be Point of TOP-REAL 2;
  assume that
A1: 1 <=j and
A2: j < len f and
A3: p in LSeg(f,j) and
A4: q in LSeg(f,j) and
A5: (f/.j)`2=(f/.(j+1))`2 and
A6: (f/.j)`1>(f/.(j+1))`1 and
A7: LE p,q,L~f,f/.1,f/.(len f);
  j+1<=len f by A2,NAT_1:13;
  then
A8: LSeg(f,j)=LSeg(f/.j,f/.(j+1)) by A1,TOPREAL1:def 5;
  per cases;
  suppose
A9: p`1<> (f/.j)`1;
    (f/.j)`1>=p`1 by A3,A6,A8,TOPREAL1:9;
    then (f/.j)`1>p`1 by A9,XXREAL_0:1;
    then
A10: (f/.j)`1-p`1>0 by XREAL_1:52;
    now
      reconsider a=((f/.j)`1-q`1)/((f/.j)`1-p`1) as Real;
A11:  (1-a)=((f/.j)`1-p`1)/((f/.j)`1-p`1)- ((f/.j)`1-q`1)/((f/.j)`1-p`1)
      by A10,XCMPLX_1:60
        .=((f/.j)`1-p`1- ((f/.j)`1-q`1))/((f/.j)`1-p`1) by XCMPLX_1:121
        .=(q`1-p`1)/((f/.j)`1-p`1);
A12:  ((1-a)*(f/.j)+a*p)`1=((1-a)*(f/.j))`1+(a*p)`1 by TOPREAL3:7
        .=(1-a)*(f/.j)`1+ (a*p)`1 by TOPREAL3:9
        .=1* (f/.j)`1 -a*(f/.j)`1 +a*(p`1) by TOPREAL3:9
        .= (f/.j)`1 -(a*((f/.j)`1 -(p`1)))
        .= (f/.j)`1 -((f/.j)`1-q`1) by A10,XCMPLX_1:88
        .=q`1;
      (f/.j)`1>=q`1 by A4,A6,A8,TOPREAL1:9;
      then
A13:  (f/.j)`1-q`1>=0 by XREAL_1:50;
A14:  p`2=(f/.j)`2 by A3,A5,A8,GOBOARD7:6;
      ((1-a)*(f/.j)+a*p)`2=((1-a)*(f/.j))`2+(a*p)`2 by TOPREAL3:7
        .=(1-a)*(f/.j)`2+ (a*p)`2 by TOPREAL3:9
        .=1* (f/.j)`2 -a*(f/.j)`2 +a*(p`2) by TOPREAL3:9
        .=q`2 by A4,A5,A8,A14,GOBOARD7:6;
      then
A15:  q=(1-a)*(f/.j)+a*p by A12,TOPREAL3:11;
      assume
A16:  p`1<q`1;
      then q`1-p`1>0 by XREAL_1:52;
      then 1-a+a>=0+a by A10,A11,XREAL_1:9;
      then q in LSeg(f/.j,p) by A10,A13,A15;
      then LE q,p,L~f,f/.1,f/.(len f) by A1,A2,A3,SPRECT_3:40;
      hence contradiction by A7,A16,JORDAN5C:12,TOPREAL1:31;
    end;
    hence thesis;
  end;
  suppose
    p`1=(f/.j)`1;
    hence thesis by A4,A6,A8,TOPREAL1:9;
  end;
end;

theorem Th7:
  for f being S-Sequence_in_R2, p,q being Point of TOP-REAL 2 st 1
<=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) & (f/.j)`2=(f/.(j+1))`2 & (f
  /.j)`1<(f/.(j+1))`1 & LE p,q,L~f,f/.1,f/.(len f) holds p`1<=q`1
proof
  let f be S-Sequence_in_R2, p,q be Point of TOP-REAL 2;
  assume that
A1: 1 <=j and
A2: j < len f and
A3: p in LSeg(f,j) and
A4: q in LSeg(f,j) and
A5: (f/.j)`2=(f/.(j+1))`2 and
A6: (f/.j)`1<(f/.(j+1))`1 and
A7: LE p,q,L~f,f/.1,f/.(len f);
  j+1<=len f by A2,NAT_1:13;
  then
A8: LSeg(f,j)=LSeg(f/.j,f/.(j+1)) by A1,TOPREAL1:def 5;
  per cases;
  suppose
A9: p`1<> (f/.(j))`1;
    (f/.j)`1<=p`1 by A3,A6,A8,TOPREAL1:9;
    then (f/.(j))`1<p`1 by A9,XXREAL_0:1;
    then
A10: (f/.(j))`1 -p`1<0 by XREAL_1:51;
    now
      reconsider a=((f/.(j))`1-q`1)/((f/.(j))`1-p`1) as Real;
A11:  (1-a)=((f/.(j))`1-p`1)/((f/.(j))`1-p`1) - ((f/.(j))`1-q`1)/((f/.(j)
      )`1-p`1) by A10,XCMPLX_1:60
        .=((f/.(j))`1-p`1- ((f/.(j))`1-q`1))/((f/.(j))`1-p`1) by XCMPLX_1:121
        .=(q`1-p`1)/((f/.(j))`1-p`1);
A12:  ((1-a)*(f/.(j))+a*p)`1=((1-a)*(f/.(j)))`1+(a*p)`1 by TOPREAL3:7
        .=(1-a)*(f/.(j))`1+ (a*p)`1 by TOPREAL3:9
        .=1* (f/.(j))`1 -a*(f/.(j))`1 +a*(p`1) by TOPREAL3:9
        .= (f/.(j))`1 -(a*((f/.(j))`1 -(p`1)))
        .= (f/.(j))`1 -((f/.(j))`1-q`1) by A10,XCMPLX_1:88
        .=q`1;
      (f/.j)`1<=q`1 by A4,A6,A8,TOPREAL1:9;
      then
A13:  (f/.(j))`1-q`1<=0 by XREAL_1:49;
A14:  p`2=(f/.j)`2 by A3,A5,A8,GOBOARD7:6;
      ((1-a)*(f/.(j))+a*p)`2=((1-a)*(f/.(j)))`2+(a*p)`2 by TOPREAL3:7
        .=(1-a)*(f/.(j))`2+ (a*p)`2 by TOPREAL3:9
        .=1* (f/.(j))`2 -a*(f/.(j))`2 +a*(p`2) by TOPREAL3:9
        .=q`2 by A4,A5,A8,A14,GOBOARD7:6;
      then
A15:  q=(1-a)*(f/.(j))+a*p by A12,TOPREAL3:11;
      assume
A16:  p`1>q`1;
      then q`1-p`1<0 by XREAL_1:51;
      then 1-a+a>=0+a by A10,A11,XREAL_1:9;
      then q in LSeg(f/.(j),p) by A10,A13,A15;
      then LE q,p,L~f,f/.1,f/.(len f) by A1,A2,A3,SPRECT_3:40;
      hence contradiction by A7,A16,JORDAN5C:12,TOPREAL1:31;
    end;
    hence thesis;
  end;
  suppose
    p`1=(f/.(j))`1;
    hence thesis by A4,A6,A8,TOPREAL1:9;
  end;
end;

definition
  let P be Subset of TOP-REAL 2, p1,p2,p be Point of TOP-REAL 2, e be Real;
  pred p is_Lin P,p1,p2,e means
  :Def1:
  P is_an_arc_of p1,p2 & p in P & p`1=e &
  ex p4 being Point of TOP-REAL 2 st p4`1<e & LE p4,p,P,p1,p2 & for p5 being
  Point of TOP-REAL 2 st LE p4,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1<=e;
  pred p is_Rin P,p1,p2,e means
  :Def2:
  P is_an_arc_of p1,p2 & p in P & p`1=e &
  ex p4 being Point of TOP-REAL 2 st p4`1>e & LE p4,p,P,p1,p2 & for p5 being
  Point of TOP-REAL 2 st LE p4,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e;
  pred p is_Lout P,p1,p2,e means
  :Def3:
  P is_an_arc_of p1,p2 & p in P & p`1=e
  & ex p4 being Point of TOP-REAL 2 st p4`1<e & LE p,p4,P,p1,p2 & for p5 being
  Point of TOP-REAL 2 st LE p5,p4,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1<=e;
  pred p is_Rout P,p1,p2,e means
  :Def4:
  P is_an_arc_of p1,p2 & p in P & p`1=e
  & ex p4 being Point of TOP-REAL 2 st p4`1>e & LE p,p4,P,p1,p2 & for p5 being
  Point of TOP-REAL 2 st LE p5,p4,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1>=e;
  pred p is_OSin P,p1,p2,e means
  :Def5:
  P is_an_arc_of p1,p2 & p in P & p`1=e
  & ex p7 being Point of TOP-REAL 2 st LE p7,p,P,p1,p2 & (for p8 being Point of
  TOP-REAL 2 st LE p7,p8,P,p1,p2 & LE p8,p,P,p1,p2 holds p8`1=e) & for p4 being
  Point of TOP-REAL 2 st LE p4,p7,P,p1,p2 & p4<>p7 holds (ex p5 being Point of
TOP-REAL 2 st LE p4,p5,P,p1,p2 & LE p5,p7,P,p1,p2 & p5`1>e) & ex p6 being Point
  of TOP-REAL 2 st LE p4,p6,P,p1,p2 & LE p6,p7,P,p1,p2 & p6`1<e;
  pred p is_OSout P,p1,p2,e means
  :Def6:
  P is_an_arc_of p1,p2 & p in P & p`1=e
  & ex p7 being Point of TOP-REAL 2 st LE p,p7,P,p1,p2 & (for p8 being Point of
  TOP-REAL 2 st LE p8,p7,P,p1,p2 & LE p,p8,P,p1,p2 holds p8`1=e) & for p4 being
  Point of TOP-REAL 2 st LE p7,p4,P,p1,p2 & p4<>p7 holds (ex p5 being Point of
TOP-REAL 2 st LE p5,p4,P,p1,p2 & LE p7,p5,P,p1,p2 & p5`1>e)& ex p6 being Point
  of TOP-REAL 2 st LE p6,p4,P,p1,p2 & LE p7,p6,P,p1,p2 & p6`1<e;
  correctness;
end;

theorem
  for P being Subset of TOP-REAL 2, p1,p2,p being Point of TOP-REAL 2,e
being Real st P is_an_arc_of p1,p2 & p1`1<=e & p2`1>=e holds ex p3 being Point
  of TOP-REAL 2 st p3 in P & p3`1=e
proof
  let P be Subset of TOP-REAL 2, p1,p2,p be Point of TOP-REAL 2,e be Real;
  consider x being Element of P /\ Vertical_Line(e);
  assume P is_an_arc_of p1,p2 & p1`1<=e & p2`1>=e;
  then P meets Vertical_Line(e) by JORDAN6:64;
  then
A1: P /\ Vertical_Line(e) <> {} by XBOOLE_0:def 7;
  then x in Vertical_Line(e) by XBOOLE_0:def 4;
  then x in {p3 where p3 is Point of TOP-REAL 2: p3`1=e} by JORDAN6:def 6;
  then
A2: ex p4 being Point of TOP-REAL 2 st p4=x & p4`1=e;
  x in P by A1,XBOOLE_0:def 4;
  hence thesis by A2;
end;

theorem
  for P being non empty Subset of TOP-REAL 2, p1,p2,p being Point of
TOP-REAL 2,e being Real st P is_an_arc_of p1,p2 & p1`1<e & p in P & p`1=e holds
  p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e or p is_OSin P,p1,p2,e
proof
  let P be non empty Subset of TOP-REAL 2, p1,p2,p be Point of TOP-REAL 2,e be
  Real;
  assume that
A1: P is_an_arc_of p1,p2 and
A2: p1`1<e and
A3: p in P and
A4: p`1=e;
  now
    reconsider pr1a=proj1 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
    reconsider pro1=pr1a|P as Function of (TOP-REAL 2)|P,R^1 by PRE_TOPC:30;
    consider f being Function of I[01], (TOP-REAL 2)|P such that
A5: f is being_homeomorphism and
A6: f.0 = p1 and
A7: f.1 = p2 by A1,TOPREAL1:def 2;
A8: f is continuous by A5,TOPS_2:def 5;
A9: rng f=[#]((TOP-REAL 2)|P) by A5,TOPS_2:def 5;
    then p in rng f by A3,PRE_TOPC:def 10;
    then consider xs being set such that
A10: xs in dom f and
A11: p=f.xs by FUNCT_1:def 5;
A12: dom f=[#](I[01]) by A5,TOPS_2:def 5;
    then reconsider s2=xs as Real by A10,BORSUK_1:83;
A13: 0<=s2 by A10,BORSUK_1:83,XXREAL_1:1;
    for q being Point of TOP-REAL 2 st q=f.0 holds q`1<>e by A2,A6;
    then
A14: 0 in {s where s is Real: 0<=s & s<=s2 & (for q being Point of
    TOP-REAL 2 st q=f.s holds q`1<>e)} by A13;
    {s where s is Real: 0<=s & s<=s2 & for q being Point of TOP-REAL 2 st
    q=f.s holds q`1<>e } c= REAL
    proof
      let x be set;
      assume x in {s where s is Real: 0<=s & s<=s2 & (for q being Point of
      TOP-REAL 2 st q=f.s holds q`1<>e)};
      then ex s being Real st s=x & 0<=s & s<=s2 & for q being Point of
      TOP-REAL 2 st q=f.s holds q`1<>e;
      hence thesis;
    end;
    then reconsider R={s where s is Real: 0<=s & s<=s2 & (for q being Point of
    TOP-REAL 2 st q=f.s holds q`1<>e)} as non empty Subset of REAL by A14;
A15: s2<=1 by A10,BORSUK_1:83,XXREAL_1:1;
    R c= [.0,1.]
    proof
      let x be set;
      assume x in R;
      then consider s being Real such that
A16:  s=x & 0<=s and
A17:  s<=s2 and
      for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e;
      s<=1 by A15,A17,XXREAL_0:2;
      hence thesis by A16,XXREAL_1:1;
    end;
    then reconsider R99=R as Subset of I[01] by BORSUK_1:83;
    reconsider s0=upper_bound R as Real;
A18: for s being real number st s in R holds s < s2
    proof
      let s be real number;
      assume s in R;
      then
A19:  ex s3 being Real st s3=s & 0<=s3 & s3<=s2 & for q being Point of
      TOP-REAL 2 st q=f.s3 holds q`1<>e;
      then s<>s2 by A4,A11;
      hence thesis by A19,XXREAL_0:1;
    end;
    then for s being real number st s in R holds s <= s2;
    then
A20: s0 <= s2 by SEQ_4:62;
    then
A21: s0 <=1 by A15,XXREAL_0:2;
    R99=R;
    then
A22: 0 <= s0 by A14,BORSUK_4:51;
    then s0 in dom f by A12,A21,BORSUK_1:83,XXREAL_1:1;
    then f.s0 in rng f by FUNCT_1:def 5;
    then f.s0 in P by A9,PRE_TOPC:def 10;
    then reconsider p9=f.s0 as Point of TOP-REAL 2;
A23: LE p9,p,P,p1,p2 by A1,A5,A6,A7,A11,A15,A22,A20,A21,JORDAN5C:8;
    for p7 being Point of (TOP-REAL 2)|P holds pro1.p7=proj1.p7
    proof
      let p7 be Point of (TOP-REAL 2)|P;
      the carrier of ((TOP-REAL 2)|P)=P by PRE_TOPC:29;
      hence thesis by FUNCT_1:72;
    end;
    then
A24: pro1 is continuous by JGRAPH_2:39;
    reconsider h=pro1*f as Function of I[01],R^1;
A25: dom h=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
    for s being ext-real number st s in R holds s <= s2 by A18;
    then s2 is UpperBound of R by XXREAL_2:def 1;
    then
A26: R is bounded_above by XXREAL_2:def 10;
A27: rng f=P by A9,PRE_TOPC:def 10;
A28: for p8 being Point of TOP-REAL 2 st LE p9,p8,P,p1,p2 & LE p8,p,P,p1,
    p2 holds p8`1=e
    proof
      let p8 be Point of TOP-REAL 2;
      assume that
A29:  LE p9,p8,P,p1,p2 and
A30:  LE p8,p,P,p1,p2;
A31:  p8 in P by A29,JORDAN5C:def 3;
      then consider x8 being set such that
A32:  x8 in dom f and
A33:  p8=f.x8 by A27,FUNCT_1:def 5;
      reconsider s8=x8 as Real by A12,A32,BORSUK_1:83;
A34:  s8<=1 by A32,BORSUK_1:83,XXREAL_1:1;
      then
A35:  s8<=s2 by A5,A6,A7,A11,A13,A15,A30,A33,JORDAN5C:def 3;
A36:  0<=s8 by A32,BORSUK_1:83,XXREAL_1:1;
      then
A37:  s0 <=s8 by A5,A6,A7,A21,A29,A33,A34,JORDAN5C:def 3;
      now
        reconsider s8n=s8 as Point of RealSpace by METRIC_1:def 14;
        reconsider s8m=s8 as Point of Closed-Interval-MSpace(0,1) by A32,
BORSUK_1:83,TOPMETR:14;
        reconsider ee=abs(p8`1-e)/2 as Real;
        reconsider w=p8`1 as Element of RealSpace by METRIC_1:def 14;
        reconsider B=Ball(w,ee) as Subset of R^1 by METRIC_1:def 14,TOPMETR:24;
A38:    B={s7 where s7 is Real:p8`1-ee<s7 & s7<p8`1+ee} by JORDAN2B:21
          .= ].p8`1-ee,p8`1+ee.[ by RCOMP_1:def 2;
        assume
A39:    p8`1<>e;
        then p8`1-e<>0;
        then abs(p8`1-e)>0 by COMPLEX1:133;
        then
A40:    w in Ball(w,ee) by GOBOARD6:4,XREAL_1:141;
A41:    h"B is open & I[01]=TopSpaceMetr(Closed-Interval-MSpace(0,1)) by A8,A24
,TOPMETR:27,def 7,def 8,UNIFORM1:3;
        h.s8=pro1.(f.s8) by A25,A32,BORSUK_1:83,FUNCT_1:22
          .=proj1.p8 by A31,A33,FUNCT_1:72
          .=p8`1 by PSCOMP_1:def 28;
        then s8 in h"B by A25,A32,A40,BORSUK_1:83,FUNCT_1:def 13;
        then consider r0 being real number such that
A42:    r0>0 and
A43:    Ball(s8m,r0) c= h"B by A41,TOPMETR:22;
        reconsider r0 as Real by XREAL_0:def 1;
        reconsider r01=min(s2-s8,r0) as Real;
        s8<s2 by A4,A11,A33,A35,A39,XXREAL_0:1;
        then s2-s8>0 by XREAL_1:52;
        then
A44:    r01>0 by A42,XXREAL_0:21;
        then
A45:    r01-r01/2+r01/2>0+r01/2 by XREAL_1:8;
        then
A46:    s8+r01/2<s8+r01 by XREAL_1:8;
        reconsider s70=s8+r01/2 as Real;
        the carrier of Closed-Interval-MSpace(0,1)=[.0,1.] & Ball(s8n,r01
        )= (].s8- r01,s8+r01 .[) by FRECHET:7,TOPMETR:14;
        then
A47:    Ball(s8m,r01)= (].s8-r01,s8+r01 .[) /\ [.0,1.] by TOPMETR:13;
        s2-s8>=r01 by XXREAL_0:17;
        then
A48:    s2-s8+s8>=r01+s8 by XREAL_1:9;
        then
A49:    s70<=s2 by A46,XXREAL_0:2;
        s8+r01<=1 by A15,A48,XXREAL_0:2;
        then s8+r01/2<1 by A46,XXREAL_0:2;
        then
A50:    s8+r01/2 in [.0,1.] by A36,A44,XXREAL_1:1;
        Ball(s8m,r01) c= Ball(s8m,r0) by PCOMPS_1:1,XXREAL_0:17;
        then
A51:    (].s8-r01,s8+r01 .[)/\ [.0,1.] c= h"B by A43,A47,XBOOLE_1:1;
        s8+0<s8+(r01/2+r01) by A44,XREAL_1:8;
        then s8-r01+r01<s8+r01/2+r01;
        then
A52:    s8-r01<s8+r01/2 by XREAL_1:8;
        s8+r01/2<s8+r01 by A45,XREAL_1:8;
        then s8+r01/2 in ].s8-r01,s8+r01 .[ by A52,XXREAL_1:4;
        then
A53:    s8+r01/2 in (].s8-r01,s8+r01 .[)/\ [.0,1.] by A50,XBOOLE_0:def 4;
        then
A54:    h.(s8+r01/2) in B by A51,FUNCT_1:def 13;
A55:    s8+r01/2 in dom h by A51,A53,FUNCT_1:def 13;
A56:    for p7 being Point of TOP-REAL 2 st p7=f.s70 holds p7`1<>e
        proof
          let p7 be Point of TOP-REAL 2;
          assume
A57:      p7=f.s70;
          s70<=1 by A15,A49,XXREAL_0:2;
          then s70 in [.0,1.] by A36,A44,XXREAL_1:1;
          then
A58:      p7 in rng f by A12,A57,BORSUK_1:83,FUNCT_1:def 5;
A59:      rng f=[#]((TOP-REAL 2)|P) by A5,TOPS_2:def 5
            .=P by PRE_TOPC:def 10;
A60:      h.s70=pro1.(f.s70) by A55,FUNCT_1:22
            .=pr1a.(p7) by A57,A58,A59,FUNCT_1:72
            .=p7`1 by PSCOMP_1:def 28;
          then
A61:      p7`1<p8`1+ee by A38,A54,XXREAL_1:4;
A62:      p8`1-ee<p7`1 by A38,A54,A60,XXREAL_1:4;
          now
            assume
A63:        p7`1=e;
            now
              per cases;
              case
A64:            p8`1-e>=0;
                then p8`1-(p8`1-e)/2<e by A62,A63,ABSVALUE:def 1;
                then p8`1/2+e/2<e/2+e/2;
                then p8`1/2<e/2 by XREAL_1:9;
                then
A65:            p8`1/2-e/2<e/2-e/2 by XREAL_1:16;
                (p8`1-e)/2>=0/2 by A64;
                hence contradiction by A65;
              end;
              case
A66:            p8`1-e<0;
                then e< p8`1+(-(p8`1-e))/2 by A61,A63,ABSVALUE:def 1;
                then p8`1/2+e/2>e/2+e/2;
                then p8`1/2>e/2 by XREAL_1:9;
                then
A67:            p8`1/2-e/2>e/2-e/2 by XREAL_1:16;
                (p8`1-e)/2<=0/2 by A66;
                hence contradiction by A67;
              end;
            end;
            hence contradiction;
          end;
          hence thesis;
        end;
        s8<s70 by A44,XREAL_1:31,141;
        then consider s7 being Real such that
A68:    s8<s7 and
A69:    0<=s7 & s7<=s2 & for p7 being Point of TOP-REAL 2 st p7=f.s7
        holds p7`1 <>e by A36,A49,A56;
        s7 in R by A69;
        then s7 <= s0 by A26,SEQ_4:def 4;
        hence contradiction by A37,A68,XXREAL_0:2;
      end;
      hence thesis;
    end;
    assume not p is_OSin P,p1,p2,e;
    then consider p4 being Point of TOP-REAL 2 such that
A70: LE p4,p9,P,p1,p2 and
A71: p4<>p9 and
A72: (for p5 being Point of TOP-REAL 2 st LE p4,p5,P,p1,p2 & LE p5,p9
, P,p1,p2 holds p5`1<=e)or for p6 being Point of TOP-REAL 2 st LE p4,p6,P,p1,p2
    & LE p6,p9,P,p1,p2 holds p6`1>=e by A1,A3,A4,A23,A28,Def5;
A73: p9 in P by A70,JORDAN5C:def 3;
    now
      per cases by A72;
      case
A74:    for p5 being Point of TOP-REAL 2 st LE p4,p5,P,p1,p2 & LE p5
        ,p9,P,p1,p2 holds p5`1<=e;
A75:    now
          p4 in P by A70,JORDAN5C:def 3;
          then p4 in rng f by A9,PRE_TOPC:def 10;
          then consider xs4 being set such that
A76:      xs4 in dom f and
A77:      p4=f.xs4 by FUNCT_1:def 5;
          reconsider s4=xs4 as Real by A12,A76,BORSUK_1:83;
A78:      0<=s4 by A76,BORSUK_1:83,XXREAL_1:1;
A79:      s4<=1 by A76,BORSUK_1:83,XXREAL_1:1;
          assume
A80:      not ex p51 being Point of TOP-REAL 2 st LE p4,p51,P,p1,p2
          & LE p51,p9,P,p1,p2 & p51`1<e;
A81:      for p51 being Point of TOP-REAL 2 st LE p4,p51,P,p1,p2 & LE
          p51,p9,P,p1,p2 holds p51`1=e
          proof
            let p51 be Point of TOP-REAL 2;
            assume LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2;
            then p51`1>=e & p51`1<=e by A74,A80;
            hence thesis by XXREAL_0:1;
          end;
A82:      now
            assume s4< s0;
            then
A83:        s0-s4>0 by XREAL_1:52;
            then
A84:        s4<s4+(s0-s4)/2 by XREAL_1:31,141;
            (s0-s4)/2>0 by A83,XREAL_1:141;
            then consider r being real number such that
A85:        r in R and
A86:        s0-((s0-s4)/2)<r by A26,SEQ_4:def 4;
            reconsider rss=r as Real by XREAL_0:def 1;
A87:        ex s7 being Real st s7=r & 0<=s7 & s7<=s2 & for q being Point
            of TOP-REAL 2 st q=f.s7 holds q`1<>e by A85;
            then
A88:        r<=1 by A15,XXREAL_0:2;
            then r in [.0,1.] by A78,A84,A86,XXREAL_1:1;
            then f.rss in rng f by A12,BORSUK_1:83,FUNCT_1:def 5;
            then f.rss in P by A9,PRE_TOPC:def 10;
            then reconsider pss=f.rss as Point of TOP-REAL 2;
            s4<r by A84,A86,XXREAL_0:2;
            then
A89:        LE p4,pss,P,p1,p2 by A1,A5,A6,A7,A77,A78,A79,A88,JORDAN5C:8;
            r<=s0 by A26,A85,SEQ_4:def 4;
            then LE pss,p9,P,p1,p2 by A1,A5,A6,A7,A21,A78,A84,A86,A88,
JORDAN5C:8;
            then pss`1=e by A81,A89;
            hence contradiction by A87;
          end;
          s4<= s0 by A5,A6,A7,A22,A21,A70,A77,A79,JORDAN5C:def 3;
          hence contradiction by A71,A77,A82,XXREAL_0:1;
        end;
        now
          assume ex p51 being Point of TOP-REAL 2 st LE p4,p51,P,p1,p2 & LE
          p51,p9,P,p1,p2 & p51`1<e;
          then consider p51 being Point of TOP-REAL 2 such that
A90:      LE p4,p51,P,p1,p2 and
A91:      LE p51,p9,P,p1,p2 and
A92:      p51`1<e;
A93:      for p5 being Point of TOP-REAL 2 st LE p51,p5,P,p1,p2 & LE p5,
          p,P,p1,p2 holds p5`1<=e
          proof
            let p5 be Point of TOP-REAL 2;
            assume that
A94:        LE p51,p5,P,p1,p2 and
A95:        LE p5,p,P,p1,p2;
A96:        LE p4,p5,P,p1,p2 by A90,A94,JORDAN5C:13;
A97:        p5 in P by A94,JORDAN5C:def 3;
            then
A98:        p5=p9 implies LE p9,p5,P,p1,p2 by JORDAN5C:9;
            now
              per cases by A1,A73,A97,A98,JORDAN5C:14;
              case
                LE p5,p9,P,p1,p2;
                hence thesis by A74,A96;
              end;
              case
                LE p9,p5,P,p1,p2;
                hence thesis by A28,A95;
              end;
            end;
            hence thesis;
          end;
          LE p51,p,P,p1,p2 by A23,A91,JORDAN5C:13;
          hence p is_Lin P,p1,p2,e by A1,A3,A4,A92,A93,Def1;
        end;
        hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A75;
      end;
      case
A99:    for p6 being Point of TOP-REAL 2 st LE p4,p6,P,p1,p2 & LE p6
        ,p9,P,p1,p2 holds p6`1>=e;
A100:   now
          p4 in P by A70,JORDAN5C:def 3;
          then p4 in rng f by A9,PRE_TOPC:def 10;
          then consider xs4 being set such that
A101:     xs4 in dom f and
A102:     p4=f.xs4 by FUNCT_1:def 5;
          reconsider s4=xs4 as Real by A12,A101,BORSUK_1:83;
A103:     0<=s4 by A101,BORSUK_1:83,XXREAL_1:1;
A104:     s4<=1 by A101,BORSUK_1:83,XXREAL_1:1;
          assume
A105:     not ex p51 being Point of TOP-REAL 2 st LE p4,p51,P,p1,p2
          & LE p51,p9,P,p1,p2 & p51`1>e;
A106:     for p51 being Point of TOP-REAL 2 st LE p4,p51,P,p1,p2 & LE
          p51,p9,P,p1,p2 holds p51`1=e
          proof
            let p51 be Point of TOP-REAL 2;
            assume LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2;
            then p51`1<=e & p51`1>=e by A99,A105;
            hence thesis by XXREAL_0:1;
          end;
A107:     now
            assume s4<s0;
            then
A108:       s0-s4>0 by XREAL_1:52;
            then
A109:       s4<s4+(s0-s4)/2 by XREAL_1:31,141;
            (s0-s4)/2>0 by A108,XREAL_1:141;
            then consider r being real number such that
A110:       r in R and
A111:       s0-((s0-s4)/2)<r by A26,SEQ_4:def 4;
            reconsider rss=r as Real by XREAL_0:def 1;
A112:       ex s7 being Real st s7=r & 0<=s7 & s7<=s2 & for q being Point
            of TOP-REAL 2 st q=f.s7 holds q`1<>e by A110;
            then
A113:       r<=1 by A15,XXREAL_0:2;
            then r in [.0,1.] by A103,A109,A111,XXREAL_1:1;
            then f.rss in rng f by A12,BORSUK_1:83,FUNCT_1:def 5;
            then f.rss in P by A9,PRE_TOPC:def 10;
            then reconsider pss=f.rss as Point of TOP-REAL 2;
            s4<r by A109,A111,XXREAL_0:2;
            then
A114:       LE p4,pss,P,p1,p2 by A1,A5,A6,A7,A102,A103,A104,A113,JORDAN5C:8;
            r<=s0 by A26,A110,SEQ_4:def 4;
            then LE pss,p9,P,p1,p2 by A1,A5,A6,A7,A21,A103,A109,A111,A113,
JORDAN5C:8;
            then pss`1=e by A106,A114;
            hence contradiction by A112;
          end;
          s4<=s0 by A5,A6,A7,A22,A21,A70,A102,A104,JORDAN5C:def 3;
          hence contradiction by A71,A102,A107,XXREAL_0:1;
        end;
        now
          assume ex p51 being Point of TOP-REAL 2 st LE p4,p51,P,p1,p2 & LE
          p51,p9,P,p1,p2 & p51`1>e;
          then consider p51 being Point of TOP-REAL 2 such that
A115:     LE p4,p51,P,p1,p2 and
A116:     LE p51,p9,P,p1,p2 and
A117:     p51`1>e;
A118:     for p5 being Point of TOP-REAL 2 st LE p51,p5,P,p1,p2 & LE p5,
          p,P,p1,p2 holds p5`1>=e
          proof
            let p5 be Point of TOP-REAL 2;
            assume that
A119:       LE p51,p5,P,p1,p2 and
A120:       LE p5,p,P,p1,p2;
A121:       LE p4,p5,P,p1,p2 by A115,A119,JORDAN5C:13;
A122:       p5 in P by A119,JORDAN5C:def 3;
            then
A123:       p5=p9 implies LE p9,p5,P,p1,p2 by JORDAN5C:9;
            now
              per cases by A1,A73,A122,A123,JORDAN5C:14;
              case
                LE p5,p9,P,p1,p2;
                hence thesis by A99,A121;
              end;
              case
                LE p9,p5,P,p1,p2;
                hence thesis by A28,A120;
              end;
            end;
            hence thesis;
          end;
          LE p51,p,P,p1,p2 by A23,A116,JORDAN5C:13;
          hence p is_Rin P,p1,p2,e by A1,A3,A4,A117,A118,Def2;
        end;
        hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A100;
      end;
    end;
    hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e;
  end;
  hence thesis;
end;

theorem
  for P being non empty Subset of TOP-REAL 2, p1,p2,p being Point of
TOP-REAL 2,e being Real st P is_an_arc_of p1,p2 & p2`1>e & p in P & p`1=e holds
  p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e or p is_OSout P,p1,p2,e
proof
  let P be non empty Subset of TOP-REAL 2, p1,p2,p be Point of TOP-REAL 2,e be
  Real;
  assume that
A1: P is_an_arc_of p1,p2 and
A2: p2`1>e and
A3: p in P and
A4: p`1=e;
  now
    reconsider pr1a=proj1 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
    reconsider pro1=pr1a|P as Function of (TOP-REAL 2)|P,R^1 by PRE_TOPC:30;
    consider f being Function of I[01], (TOP-REAL 2)|P such that
A5: f is being_homeomorphism and
A6: f.0 = p1 and
A7: f.1 = p2 by A1,TOPREAL1:def 2;
A8: f is continuous by A5,TOPS_2:def 5;
A9: rng f=[#]((TOP-REAL 2)|P) by A5,TOPS_2:def 5;
    then p in rng f by A3,PRE_TOPC:def 10;
    then consider xs being set such that
A10: xs in dom f and
A11: p=f.xs by FUNCT_1:def 5;
A12: dom f=[#](I[01]) by A5,TOPS_2:def 5;
    then reconsider s2=xs as Real by A10,BORSUK_1:83;
A13: s2<=1 by A10,BORSUK_1:83,XXREAL_1:1;
    for q being Point of TOP-REAL 2 st q=f.1 holds q`1<>e by A2,A7;
    then
A14: 1 in {s where s is Real: 1>=s & s>=s2 & (for q being Point of
    TOP-REAL 2 st q=f.s holds q`1<>e)} by A13;
    {s where s is Real: 1>=s & s>=s2 & for q being Point of TOP-REAL 2 st
    q=f.s holds q`1<>e } c= REAL
    proof
      let x be set;
      assume x in {s where s is Real: 1>=s & s>=s2 & (for q being Point of
      TOP-REAL 2 st q=f.s holds q`1<>e)};
      then ex s being Real st s=x & 1>=s & s>=s2 & for q being Point of
      TOP-REAL 2 st q=f.s holds q`1<>e;
      hence thesis;
    end;
    then reconsider R={s where s is Real: 1>=s & s>=s2 & (for q being Point of
    TOP-REAL 2 st q=f.s holds q`1<>e)} as non empty Subset of REAL by A14;
    reconsider s0=lower_bound R as Real;
A15: for s being real number st s in R holds s > s2
    proof
      let s be real number;
      assume s in R;
      then
A16:  ex s3 being Real st s3=s & 1>=s3 & s3>=s2 & for q being Point of
      TOP-REAL 2 st q=f.s3 holds q`1<>e;
      then s<>s2 by A4,A11;
      hence thesis by A16,XXREAL_0:1;
    end;
    then for s being real number st s in R holds s >= s2;
    then
A17: s0 >= s2 by SEQ_4:60;
    for p7 being Point of (TOP-REAL 2)|P holds pro1.p7=proj1.p7
    proof
      let p7 be Point of (TOP-REAL 2)|P;
      the carrier of ((TOP-REAL 2)|P)=P by PRE_TOPC:29;
      hence thesis by FUNCT_1:72;
    end;
    then
A18: pro1 is continuous by JGRAPH_2:39;
    reconsider h=pro1*f as Function of I[01],R^1;
A19: dom h=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
    for s being ext-real number st s in R holds s >= s2 by A15;
    then s2 is LowerBound of R by XXREAL_2:def 2;
    then
A20: R is bounded_below by XXREAL_2:def 9;
A21: 0<=s2 by A10,BORSUK_1:83,XXREAL_1:1;
    R c= [.0,1.]
    proof
      let x be set;
      assume x in R;
      then ex s being Real st s=x & 1>=s & s>=s2 & for q being Point of
      TOP-REAL 2 st q=f.s holds q`1<>e;
      hence thesis by A21,XXREAL_1:1;
    end;
    then
A22: 1 >= s0 by A14,BORSUK_1:83,BORSUK_4:51;
    then s0 in dom f by A12,A21,A17,BORSUK_1:83,XXREAL_1:1;
    then f.s0 in rng f by FUNCT_1:def 5;
    then f.s0 in P by A9,PRE_TOPC:def 10;
    then reconsider p9=f.s0 as Point of TOP-REAL 2;
A23: LE p,p9,P,p1,p2 by A1,A5,A6,A7,A11,A21,A13,A22,A17,JORDAN5C:8;
A24: rng f=P by A9,PRE_TOPC:def 10;
A25: for p8 being Point of TOP-REAL 2 st LE p8,p9,P,p1,p2 & LE p,p8,P,p1,
    p2 holds p8`1=e
    proof
      let p8 be Point of TOP-REAL 2;
      assume that
A26:  LE p8,p9,P,p1,p2 and
A27:  LE p,p8,P,p1,p2;
A28:  p8 in P by A26,JORDAN5C:def 3;
      then consider x8 being set such that
A29:  x8 in dom f and
A30:  p8=f.x8 by A24,FUNCT_1:def 5;
      reconsider s8=x8 as Real by A12,A29,BORSUK_1:83;
A31:  s8<=1 by A29,BORSUK_1:83,XXREAL_1:1;
      0<=s8 by A29,BORSUK_1:83,XXREAL_1:1;
      then
A32:  s8>=s2 by A5,A6,A7,A11,A13,A27,A30,A31,JORDAN5C:def 3;
A33:  s0>=s8 by A5,A6,A7,A21,A22,A17,A26,A30,A31,JORDAN5C:def 3;
      now
        reconsider s8n=s8 as Point of RealSpace by METRIC_1:def 14;
        reconsider s8m=s8 as Point of Closed-Interval-MSpace(0,1) by A29,
BORSUK_1:83,TOPMETR:14;
        reconsider ee=abs(p8`1-e)/2 as Real;
        reconsider w=p8`1 as Element of RealSpace by METRIC_1:def 14;
        reconsider B=Ball(w,ee) as Subset of R^1 by METRIC_1:def 14,TOPMETR:24;
A34:    B={s7 where s7 is Real:p8`1-ee<s7 & s7<p8`1+ee} by JORDAN2B:21
          .= ].p8`1-ee,p8`1+ee.[ by RCOMP_1:def 2;
        assume
A35:    p8`1<>e;
        then p8`1-e<>0;
        then abs(p8`1-e)>0 by COMPLEX1:133;
        then
A36:    w in Ball(w,ee) by GOBOARD6:4,XREAL_1:141;
A37:    h"B is open & I[01]=TopSpaceMetr(Closed-Interval-MSpace(0,1)) by A8,A18
,TOPMETR:27,def 7,def 8,UNIFORM1:3;
        h.s8=pro1.(f.s8) by A19,A29,BORSUK_1:83,FUNCT_1:22
          .=proj1.p8 by A28,A30,FUNCT_1:72
          .=p8`1 by PSCOMP_1:def 28;
        then s8 in h"B by A19,A29,A36,BORSUK_1:83,FUNCT_1:def 13;
        then consider r0 being real number such that
A38:    r0>0 and
A39:    Ball(s8m,r0) c= h"B by A37,TOPMETR:22;
        reconsider r0 as Real by XREAL_0:def 1;
        reconsider r01=min(s8-s2,r0) as Real;
        the carrier of Closed-Interval-MSpace(0,1)=[.0,1.] & Ball(s8n,r01
        )= (].s8- r01,s8+r01 .[) by FRECHET:7,TOPMETR:14;
        then
A40:    Ball(s8m,r01)= (].s8-r01,s8+r01 .[) /\ [.0,1.] by TOPMETR:13;
        s8>s2 by A4,A11,A30,A32,A35,XXREAL_0:1;
        then s8-s2>0 by XREAL_1:52;
        then
A41:    r01>0 by A38,XXREAL_0:21;
        then
A42:    r01-r01/2+r01/2>0+r01/2 by XREAL_1:8;
        then
A43:    s8-r01<s8-r01/2 by XREAL_1:12;
A44:    r01/2>0 by A41,XREAL_1:141;
        then
A45:    s8+-(r01/2)<s8+r01/2 by XREAL_1:10;
        s8+r01/2<s8+r01 by A42,XREAL_1:10;
        then s8-r01/2<s8+r01 by A45,XXREAL_0:2;
        then
A46:    s8-r01/2 in ].s8-r01,s8+r01 .[ by A43,XXREAL_1:4;
A47:    s8-r01/2>s8-r01 by A42,XREAL_1:12;
        Ball(s8m,r01) c= Ball(s8m,r0) by PCOMPS_1:1,XXREAL_0:17;
        then
A48:    (].s8-r01,s8+r01 .[)/\ [.0,1.] c= h"B by A39,A40,XBOOLE_1:1;
        reconsider s70=s8-r01/2 as Real;
        s8-s2>=r01 by XXREAL_0:17;
        then -(s8-s2)<= -r01 by XREAL_1:26;
        then
A49:    s2-s8+s8<= -r01+s8 by XREAL_1:9;
        --(r01/2)> 0 by A41,XREAL_1:141;
        then -(r01/2) < 0;
        then
A50:    s8+0>s8+-r01/2 by XREAL_1:10;
        then
A51:    1>=s70 by A31,XXREAL_0:2;
        1-0>s8-r01/2 by A31,A44,XREAL_1:17;
        then s8-r01/2 in [.0,1.] by A21,A49,A47,XXREAL_1:1;
        then
A52:    s8-r01/2 in (].s8-r01,s8+r01 .[)/\ [.0,1.] by A46,XBOOLE_0:def 4;
        then
A53:    h.(s8-r01/2) in B by A48,FUNCT_1:def 13;
A54:    s8-r01/2 in dom h by A48,A52,FUNCT_1:def 13;
A55:    for p7 being Point of TOP-REAL 2 st p7=f.s70 holds p7`1<>e
        proof
          let p7 be Point of TOP-REAL 2;
          assume
A56:      p7=f.s70;
          s70 in [.0,1.] by A21,A49,A43,A51,XXREAL_1:1;
          then
A57:      p7 in rng f by A12,A56,BORSUK_1:83,FUNCT_1:def 5;
A58:      rng f=[#]((TOP-REAL 2)|P) by A5,TOPS_2:def 5
            .=P by PRE_TOPC:def 10;
A59:      h.s70=pro1.(f.s70) by A54,FUNCT_1:22
            .=pr1a.(p7) by A56,A57,A58,FUNCT_1:72
            .=p7`1 by PSCOMP_1:def 28;
          then
A60:      p7`1<p8`1+ee by A34,A53,XXREAL_1:4;
A61:      p8`1-ee<p7`1 by A34,A53,A59,XXREAL_1:4;
          now
            assume
A62:        p7`1=e;
            now
              per cases;
              case
A63:            p8`1-e>=0;
                then p8`1-(p8`1-e)/2<e by A61,A62,ABSVALUE:def 1;
                then p8`1/2+e/2<e/2+e/2;
                then p8`1/2<e/2 by XREAL_1:9;
                then
A64:            p8`1/2-e/2<e/2-e/2 by XREAL_1:16;
                (p8`1-e)/2>=0/2 by A63;
                hence contradiction by A64;
              end;
              case
A65:            p8`1-e<0;
                then e< p8`1+(-(p8`1-e))/2 by A60,A62,ABSVALUE:def 1;
                then p8`1/2+e/2>e/2+e/2;
                then p8`1/2>e/2 by XREAL_1:9;
                then
A66:            p8`1/2-e/2>e/2-e/2 by XREAL_1:16;
                (p8`1-e)/2<=0/2 by A65;
                hence contradiction by A66;
              end;
            end;
            hence contradiction;
          end;
          hence thesis;
        end;
        s70>=s2 by A49,A43,XXREAL_0:2;
        then consider s7 being Real such that
A67:    s8>s7 and
A68:    1>=s7 & s7>=s2 & for p7 being Point of TOP-REAL 2 st p7=f.s7
        holds p7`1 <>e by A50,A51,A55;
        s7 in R by A68;
        then s7 >=s0 by A20,SEQ_4:def 5;
        hence contradiction by A33,A67,XXREAL_0:2;
      end;
      hence thesis;
    end;
    assume not p is_OSout P,p1,p2,e;
    then consider p4 being Point of TOP-REAL 2 such that
A69: LE p9,p4,P,p1,p2 and
A70: p4<>p9 and
A71: (for p5 being Point of TOP-REAL 2 st LE p5,p4,P,p1,p2 & LE p9,p5
, P,p1,p2 holds p5`1<=e)or for p6 being Point of TOP-REAL 2 st LE p6,p4,P,p1,p2
    & LE p9,p6,P,p1,p2 holds p6`1>=e by A1,A3,A4,A23,A25,Def6;
A72: p9 in P by A69,JORDAN5C:def 3;
    now
      per cases by A71;
      case
A73:    for p5 being Point of TOP-REAL 2 st LE p5,p4,P,p1,p2 & LE p9
        ,p5,P,p1,p2 holds p5`1<=e;
A74:    now
          p4 in P by A69,JORDAN5C:def 3;
          then p4 in rng f by A9,PRE_TOPC:def 10;
          then consider xs4 being set such that
A75:      xs4 in dom f and
A76:      p4=f.xs4 by FUNCT_1:def 5;
          reconsider s4=xs4 as Real by A12,A75,BORSUK_1:83;
A77:      s4<=1 by A75,BORSUK_1:83,XXREAL_1:1;
          assume
A78:      not ex p51 being Point of TOP-REAL 2 st LE p51,p4,P,p1,p2
          & LE p9,p51,P,p1,p2 & p51`1<e;
A79:      for p51 being Point of TOP-REAL 2 st LE p51,p4,P,p1,p2 & LE p9
          ,p51,P,p1,p2 holds p51`1=e
          proof
            let p51 be Point of TOP-REAL 2;
            assume LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2;
            then p51`1>=e & p51`1<=e by A73,A78;
            hence thesis by XXREAL_0:1;
          end;
A80:      now
            assume s4>s0;
            then --(s4-s0)>0 by XREAL_1:52;
            then -(s4-s0)<0;
            then
A81:        (s0-s4)/2<0 by XREAL_1:143;
            then -((s0-s4)/2)>0 by XREAL_1:60;
            then consider r being real number such that
A82:        r in R and
A83:        r<s0+(-((s0-s4)/2)) by A20,SEQ_4:def 5;
            reconsider rss=r as Real by XREAL_0:def 1;
A84:        ex s7 being Real st s7=r & 1>=s7 & s7>=s2 & for q being Point
            of TOP-REAL 2 st q=f.s7 holds q`1<>e by A82;
            then r in [.0,1.] by A21,XXREAL_1:1;
            then f.rss in rng f by A12,BORSUK_1:83,FUNCT_1:def 5;
            then f.rss in P by A9,PRE_TOPC:def 10;
            then reconsider pss=f.rss as Point of TOP-REAL 2;
            s4+0>s4+(s0-s4)/2 by A81,XREAL_1:10;
            then
A85:        s4>r by A83,XXREAL_0:2;
            then
A86:        1>r by A77,XXREAL_0:2;
A87:        r>=s0 by A20,A82,SEQ_4:def 5;
            then
A88:        LE p9,pss,P,p1,p2 by A1,A5,A6,A7,A21,A22,A17,A86,JORDAN5C:8;
            LE pss,p4,P,p1,p2 by A1,A5,A6,A7,A21,A17,A76,A77,A87,A85,A86,
JORDAN5C:8;
            then pss`1=e by A79,A88;
            hence contradiction by A84;
          end;
          0<=s4 by A75,BORSUK_1:83,XXREAL_1:1;
          then s4>=s0 by A5,A6,A7,A22,A69,A76,A77,JORDAN5C:def 3;
          hence contradiction by A70,A76,A80,XXREAL_0:1;
        end;
        now
          assume ex p51 being Point of TOP-REAL 2 st LE p51,p4,P,p1,p2 & LE
          p9,p51,P,p1,p2 & p51`1<e;
          then consider p51 being Point of TOP-REAL 2 such that
A89:      LE p51,p4,P,p1,p2 and
A90:      LE p9,p51,P,p1,p2 and
A91:      p51`1<e;
A92:      for p5 being Point of TOP-REAL 2 st LE p5,p51,P,p1,p2 & LE p,
          p5,P,p1,p2 holds p5`1<=e
          proof
            let p5 be Point of TOP-REAL 2;
            assume that
A93:        LE p5,p51,P,p1,p2 and
A94:        LE p,p5,P,p1,p2;
A95:        LE p5,p4,P,p1,p2 by A89,A93,JORDAN5C:13;
A96:        p5 in P by A93,JORDAN5C:def 3;
            then
A97:        p5=p9 implies LE p9,p5,P,p1,p2 by JORDAN5C:9;
            now
              per cases by A1,A72,A96,A97,JORDAN5C:14;
              case
                LE p5,p9,P,p1,p2;
                hence thesis by A25,A94;
              end;
              case
                LE p9,p5,P,p1,p2;
                hence thesis by A73,A95;
              end;
            end;
            hence thesis;
          end;
          LE p,p51,P,p1,p2 by A23,A90,JORDAN5C:13;
          hence p is_Lout P,p1,p2,e by A1,A3,A4,A91,A92,Def3;
        end;
        hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e by A74;
      end;
      case
A98:    for p6 being Point of TOP-REAL 2 st LE p6,p4,P,p1,p2 & LE p9
        ,p6,P,p1,p2 holds p6`1>=e;
A99:    now
          p4 in P by A69,JORDAN5C:def 3;
          then p4 in rng f by A9,PRE_TOPC:def 10;
          then consider xs4 being set such that
A100:     xs4 in dom f and
A101:     p4=f.xs4 by FUNCT_1:def 5;
          reconsider s4=xs4 as Real by A12,A100,BORSUK_1:83;
A102:     s4<=1 by A100,BORSUK_1:83,XXREAL_1:1;
          assume
A103:     not ex p51 being Point of TOP-REAL 2 st LE p51,p4,P,p1,p2
          & LE p9,p51,P,p1,p2 & p51`1>e;
A104:     for p51 being Point of TOP-REAL 2 st LE p51,p4,P,p1,p2 & LE p9
          ,p51,P,p1,p2 holds p51`1=e
          proof
            let p51 be Point of TOP-REAL 2;
            assume LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2;
            then p51`1<=e & p51`1>=e by A98,A103;
            hence thesis by XXREAL_0:1;
          end;
A105:     now
            assume s4>s0;
            then --(s4-s0)>0 by XREAL_1:52;
            then -(s4-s0)<0;
            then
A106:       (s0-s4)/2<0 by XREAL_1:143;
            then -((s0-s4)/2)>0 by XREAL_1:60;
            then consider r being real number such that
A107:       r in R and
A108:       r<s0+(-((s0-s4)/2)) by A20,SEQ_4:def 5;
            reconsider rss=r as Real by XREAL_0:def 1;
A109:       ex s7 being Real st s7=r & 1>=s7 & s7>=s2 & for q being Point
            of TOP-REAL 2 st q=f.s7 holds q`1<>e by A107;
            then r in [.0,1.] by A21,XXREAL_1:1;
            then f.rss in rng f by A12,BORSUK_1:83,FUNCT_1:def 5;
            then f.rss in P by A9,PRE_TOPC:def 10;
            then reconsider pss=f.rss as Point of TOP-REAL 2;
            s4+0>s4+(s0-s4)/2 by A106,XREAL_1:10;
            then
A110:       s4>r by A108,XXREAL_0:2;
            then
A111:       1>r by A102,XXREAL_0:2;
A112:       r>=s0 by A20,A107,SEQ_4:def 5;
            then
A113:       LE p9,pss,P,p1,p2 by A1,A5,A6,A7,A21,A22,A17,A111,JORDAN5C:8;
            LE pss,p4,P,p1,p2 by A1,A5,A6,A7,A21,A17,A101,A102,A112,A110,A111,
JORDAN5C:8;
            then pss`1=e by A104,A113;
            hence contradiction by A109;
          end;
          0<=s4 by A100,BORSUK_1:83,XXREAL_1:1;
          then s4>=s0 by A5,A6,A7,A22,A69,A101,A102,JORDAN5C:def 3;
          hence contradiction by A70,A101,A105,XXREAL_0:1;
        end;
        now
          assume ex p51 being Point of TOP-REAL 2 st LE p51,p4,P,p1,p2 & LE
          p9,p51,P,p1,p2 & p51`1>e;
          then consider p51 being Point of TOP-REAL 2 such that
A114:     LE p51,p4,P,p1,p2 and
A115:     LE p9,p51,P,p1,p2 and
A116:     p51`1>e;
A117:     for p5 being Point of TOP-REAL 2 st LE p5,p51,P,p1,p2 & LE p,
          p5,P,p1,p2 holds p5`1>=e
          proof
            let p5 be Point of TOP-REAL 2;
            assume that
A118:       LE p5,p51,P,p1,p2 and
A119:       LE p,p5,P,p1,p2;
A120:       LE p5,p4,P,p1,p2 by A114,A118,JORDAN5C:13;
A121:       p5 in P by A118,JORDAN5C:def 3;
            then
A122:       p5=p9 implies LE p9,p5,P,p1,p2 by JORDAN5C:9;
            now
              per cases by A1,A72,A121,A122,JORDAN5C:14;
              case
                LE p9,p5,P,p1,p2;
                hence thesis by A98,A120;
              end;
              case
                LE p5,p9,P,p1,p2;
                hence thesis by A25,A119;
              end;
            end;
            hence thesis;
          end;
          LE p,p51,P,p1,p2 by A23,A115,JORDAN5C:13;
          hence p is_Rout P,p1,p2,e by A1,A3,A4,A116,A117,Def4;
        end;
        hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e by A99;
      end;
    end;
    hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e;
  end;
  hence thesis;
end;

theorem Th11:
  for P being Subset of I[01],s being Real st P=[.0,s.[ holds P is open
proof
A1: [#]I[01]=[.0,1.] by TOPMETR:25,27;
  let P be Subset of I[01],s be Real;
  assume
A2: P=[.0,s.[;
  per cases;
  suppose
A3: s in [.0,1.];
    reconsider T=[.0,1.] as Subset of R^1 by TOPMETR:24;
    0 in [.0,1.] by XXREAL_1:1;
    then [.0,s.[ c= [.0,s.] & [.0,s.] c= [.0,1.] by A3,XXREAL_1:24
,XXREAL_2:def 12;
    then [.0,s.[ c= [.0,1.] by XBOOLE_1:1;
    then P c= [#](R^1|T) by A2,PRE_TOPC:def 10;
    then reconsider P2=P as Subset of R^1|T;
    reconsider Q=].-1,s.[ as Subset of R^1 by TOPMETR:24;
A4: s<=1 by A3,XXREAL_1:1;
A5: [.0,s.[ c= ].-1,s.[ /\ [.0,1.]
    proof
      let x be set;
      assume
A6:   x in [.0,s.[;
      then reconsider sx=x as Real;
A7:   0<=sx by A6,XXREAL_1:3;
A8:   sx<s by A6,XXREAL_1:3;
      then sx<=1 by A4,XXREAL_0:2;
      then
A9:   x in [.0,1.] by A7,XXREAL_1:1;
      -1 < sx by A7,XXREAL_0:2;
      then x in ].-1,s.[ by A8,XXREAL_1:4;
      hence thesis by A9,XBOOLE_0:def 4;
    end;
    ].-1,s.[ /\ [.0,1.] c= [.0,s.[
    proof
