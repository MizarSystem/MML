          s0-((s0-s4)/2)-s4<r-s4 by E121,REAL_1:54;then
          s0-s4-((s0-s4)/2)<r-s4 by XCMPLX_1:21;then
          (s0-s4)/2+(s0-s4)/2-((s0-s4)/2)<r-s4 by XCMPLX_1:66;then
          ((s0-s4)/2)<r-s4 by XCMPLX_1:26;then
          s4+((s0-s4)/2)<r by REAL_1:86;then
          E175: s4<r by E124,AXIOMS:22;then
          E171: 0<r by C714;
          consider s7 being Real such that
          E130: s7=r & 0<=s7 & s7<=s2 &
          (for q being Point of TOP-REAL 2 st q=f.s7 holds q`1<>e) by E121;
          E176: r<=1 by C71,AXIOMS:22,E130;then
          E172: r in [.0,1.] by TOPREAL5:1,E171;
          reconsider rss=r as Real by XREAL_0:def 1;
          f.rss in rng f by B15,E172,FUNCT_1:def 5;then
          f.rss in P by B16,PRE_TOPC:def 10;then
          reconsider pss=f.rss as Point of TOP-REAL 2;
          LE p4,pss,P,p1,p2 & LE pss,p9,P,p1,p2
               by E171,E176,C714,B144,E175,B80,B82,JORDAN5C:8,B3,E551,A1;then
          pss`1=e by D771;
         hence contradiction by E130;
        end;
        hence contradiction by B7,B144,C715,AXIOMS:21;
       end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by E1;
     end;
     case C1: (for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p9,P,p1,p2
        holds p6`1>=e);
      E1: now assume ex p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2 & p51`1>e;then
        consider p51 being Point of TOP-REAL 2 such that
        C2: LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2 & p51`1>e;
        C6: LE p51,p,P,p1,p2 by C2,B7b,JORDAN5C:13;
       (for p5 being Point of TOP-REAL 2 st
        LE p51,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e)
        proof let p5 be Point of TOP-REAL 2;
          assume D1: LE p51,p5,P,p1,p2 & LE p5,p,P,p1,p2;then
          D2: LE p4,p5,P,p1,p2 by C2,JORDAN5C:13;then
          D5: p5 in P by JORDAN5C:def 3;then
          D3: p5<>p9 implies LE p5,p9,P,p1,p2 & not LE p9,p5,P,p1,p2 or
            LE p9,p5,P,p1,p2 & not LE p5,p9,P,p1,p2 by B11,A1,JORDAN5C:14;
          D6: p5=p9 implies LE p9,p5,P,p1,p2 by D5,JORDAN5C:9;
          now per cases by D3,D6;
          case LE p5,p9,P,p1,p2;
           hence p5`1>=e by C1,D2;
          end;
          case LE p9,p5,P,p1,p2;
           hence p5`1>=e by B9,D1;
          end;
          end;
         hence p5`1>=e;
        end;
       hence p is_Rin P,p1,p2,e by JJ110,A1,C2,C6;
      end;
       now assume not ex p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2 & p51`1>e;then
        E2: for p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2
        holds p51`1<=e;
        D771: for p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2
        holds p51`1=e
        proof let p51 be Point of TOP-REAL 2;
          assume F1: LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2;then
          F2: p51`1<=e by E2;
          p51`1>=e by F1,C1;
         hence p51`1=e by F2,AXIOMS:21;
        end;
        p4 in P by JORDAN5C:def 3,B7;then
        p4 in rng f by PRE_TOPC:def 10,B16;then
        consider xs4 being set such that
        B144: xs4 in dom f & p4=f.xs4 by FUNCT_1:def 5;
        reconsider s4=xs4 as Real by B144,B15;
        C714: 0<=s4 & s4<=1 by B144,B15,TOPREAL5:1;then
        C715: s4<=s0 by B7,B80,B82,JORDAN5C:def 3,B144,B3;
        now assume s4<s0;then
         s0-s4>0 by SQUARE_1:11;then
         E123: (s0-s4)/2>0 by REAL_2:127;then
         E124: s4<s4+(s0-s4)/2 by REAL_1:69;
          consider r being real number such that
          E121: r in R & s0-((s0-s4)/2)<r by E123,SEQ_4:def 4,B78;
          E551: r<=s0 by E121,SEQ_4:def 4,B78;
          s0-((s0-s4)/2)-s4<r-s4 by E121,REAL_1:54;then
          s0-s4-((s0-s4)/2)<r-s4 by XCMPLX_1:21;then
          (s0-s4)/2+(s0-s4)/2-((s0-s4)/2)<r-s4 by XCMPLX_1:66;then
          ((s0-s4)/2)<r-s4 by XCMPLX_1:26;then
          s4+((s0-s4)/2)<r by REAL_1:86;then
          E175: s4<r by E124,AXIOMS:22;then
          E171: 0<r by C714;
          consider s7 being Real such that
          E130: s7=r & 0<=s7 & s7<=s2 &
          (for q being Point of TOP-REAL 2 st q=f.s7 holds q`1<>e)
                          by E121;
          E176: r<=1 by C71,AXIOMS:22,E130;then
          E172: r in [.0,1.] by TOPREAL5:1,E171;
          reconsider rss=r as Real by XREAL_0:def 1;
          f.rss in rng f by B15,E172,FUNCT_1:def 5;then
          f.rss in P by B16,PRE_TOPC:def 10;then
          reconsider pss=f.rss as Point of TOP-REAL 2;
          LE p4,pss,P,p1,p2 & LE pss,p9,P,p1,p2
               by E171,E176,C714,B144,E175,B80,B82,JORDAN5C:8,B3,E551,A1;then
          pss`1=e by D771;
         hence contradiction by E130;
        end;
        hence contradiction by B7,B144,C715,AXIOMS:21;
       end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by E1;
     end;
     end;
    hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e;
   end;
  hence thesis;
end;

theorem for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_an_arc_of p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
  or p is_OSout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_an_arc_of p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
   now assume not p is_OSout P,p1,p2,e;then
    A3: for p7 being Point of TOP-REAL 2 st
     LE p,p7,P,p1,p2 &
    (for p8 being Point of TOP-REAL 2 st LE p8,p7,P,p1,p2 & LE p,p8,P,p1,p2
     holds p8`1=e)
     holds not(
    (for p4 being Point of TOP-REAL 2 st LE p7,p4,P,p1,p2 & p4<>p7 holds
    (ex p5 being Point of TOP-REAL 2 st
    LE p5,p4,P,p1,p2 & LE p7,p5,P,p1,p2 & p5`1>e)&
    (ex p6 being Point of TOP-REAL 2 st
    LE p6,p4,P,p1,p2 & LE p7,p6,P,p1,p2 & p6`1<e))) by JJ150,A1;
    LE p1,p,P,p1,p2 by A1,JORDAN5C:10;then
    B12: p1 in P & p in P &
      for g being map of I[01], (TOP-REAL 2)|P,
       s1, s2 being Real st
       g is_homeomorphism
       & g.0 = p1 & g.1 = p2
       & g.s1 = p1 & 0 <= s1 & s1 <= 1
       & g.s2 = p & 0 <= s2 & s2 <= 1
       holds s1 <= s2 by JORDAN5C:def 3;
     consider f being map of I[01], (TOP-REAL 2)|P such that
     B3: f is_homeomorphism & f.0 = p1 & f.1 = p2 by A1,TOPREAL1:def 2;
     B3b: f is continuous by B3,TOPS_2:def 5;
     B16: dom f=[#](I[01]) & rng f=[#]((TOP-REAL 2)|P) by TOPS_2:def 5,B3;then
     B15: dom f=[.0,1.] by BORSUK_1:83,PRE_TOPC:12;
     B17: rng f=P by B16,PRE_TOPC:def 10;
     p in rng f by PRE_TOPC:def 10,B12,B16;then
     consider xs being set such that
     B14: xs in dom f & p=f.xs by FUNCT_1:def 5;
     reconsider s2=xs as Real by B14,B15;
     C71: 0<=s2 & s2<=1 by TOPREAL5:1,B14,B15;
     reconsider pr1=proj1 as map of TOP-REAL 2,R^1 by JGRAPH_2:16;
     reconsider pro1=pr1|P as map of (TOP-REAL 2)|P,R^1 by JGRAPH_3:12;
     for p7 being Point of (TOP-REAL 2)|P holds
     pro1.p7=proj1.p7
     proof let p7 be Point of (TOP-REAL 2)|P;
       the carrier of ((TOP-REAL 2)|P)=P by JORDAN1:1;
      hence pro1.p7=proj1.p7 by FUNCT_1:72;
     end;then
     C77: pro1 is continuous by JGRAPH_2:39;
     reconsider h=pro1*f as map of I[01],R^1;
     C99: h is continuous by B3b,C77,TOPS_2:58;
     C102: dom h=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
     B11: f.s2=p by B14;
     (for q being Point of TOP-REAL 2 st q=f.1 holds q`1<>e) by A1,B3;then
     B79: 1 in
     {s where s is Real: 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)} by C71;
     {s where s is Real: 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)} c= REAL
     proof let x be set;assume x in
       {s where s is Real: 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)};then
       consider s being Real such that
       C4: s=x & 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e);
      thus x in REAL by C4;
     end;then
     reconsider R={s where s is Real: 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)}
        as non empty Subset of REAL by B79;
     reconsider s0=inf R as Real;
     R c= [.0,1.]
     proof let x be set;assume x in R;then
       consider s being Real such that
       C70: s=x & 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e);
      thus x in [.0,1.] by TOPREAL5:1,C70,C71;
     end;then
     B70: R is Subset of the carrier of I[01] by BORSUK_1:83;
     B80: 1 >= s0 by B79,BORSUK_4:51,B70;
     B79: for s being real number st s in R holds s > s2
     proof let s be real number;assume s in R;then
       consider s3 being Real such that
       C70: s3=s & 1>=s3 & s3>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s3 holds q`1<>e);
      s<>s2 by C70,B11,A1;
      hence thesis by C70,REAL_1:def 5;
     end;then
     for s being real number st s in R holds s >= s2;then
     B81: s0 >= s2 by PSCOMP_1:8;then
     B82: s0 >= 0 by C71;then
     s0 in dom f by B15,TOPREAL5:1,B80;then
     f.s0 in rng f by FUNCT_1:def 5;then
     f.s0 in P by B16,PRE_TOPC:def 10;then
     reconsider p9=f.s0 as Point of TOP-REAL 2;
     for s being real number st s in R holds s >= s2 by B79;then
     B78: R is bounded_below by SEQ_4:def 2;
     B7b: LE p,p9,P,p1,p2 by JORDAN5C:8,B81,B11,B3,B80,C71,A1;
     B9: for p8 being Point of TOP-REAL 2 st
       LE p8,p9,P,p1,p2 & LE p,p8,P,p1,p2
       holds p8`1=e
       proof let p8 be Point of TOP-REAL 2;
        assume G1: LE p8,p9,P,p1,p2 & LE p,p8,P,p1,p2;then
         G11: p8 in P by JORDAN5C:def 3;then
         consider x8 being set such that
         G10: x8 in dom f & p8=f.x8 by B17,FUNCT_1:def 5;
         reconsider s8=x8 as Real by G10,B15;
         G2: 0<=s8 & s8<=1 & f.s8=p8 by G10,B15,TOPREAL5:1;
         G3: s0>=s8 by G1,JORDAN5C:def 3,B3,G2,B80,B82;
         G3b: s8>=s2
           by G1,JORDAN5C:def 3,B11,B3,G2,C71;
         now assume D3: p8`1<>e;then
           D6: s8>s2 by G3b,REAL_1:def 5,A1,B11,G10;
           reconsider ee=abs(p8`1-e)/2 as Real by XREAL_0:def 1;
           p8`1-e<>0 by D3,XCMPLX_1:15;then
           abs(p8`1-e)>0 by ABSVALUE:6;then
           C108: ee>0 by REAL_2:127;
           reconsider w=p8`1 as Element of the carrier of RealSpace
                               by METRIC_1:def 14;
           C203: the carrier of R^1=REAL by TOPMETR:24;
           reconsider B=Ball(w,ee) as Subset of the carrier of R^1
                                   by C203,METRIC_1:def 14;
           C205: w in Ball(w,ee) by C108,GOBOARD6:4;
           C405: B={s7 where s7 is Real:p8`1-ee<s7 & s7<p8`1+ee}
                  by JORDAN2B:21,C108
            .= ].p8`1-ee,p8`1+ee.[ by RCOMP_1:def 2;
           C222: h"B is open by UNIFORM1:3,C99,TOPMETR:def 7;
           C303: the carrier of Closed-Interval-MSpace(0,1)=[.0,1.]
                              by TOPMETR:14; then
           reconsider s8m=s8 as Point of Closed-Interval-MSpace(0,1)
                              by G2,TOPREAL5:1;
           reconsider s8n=s8 as Point of RealSpace by METRIC_1:def 14;
           C223: I[01]=TopSpaceMetr(Closed-Interval-MSpace(0,1))
                             by TOPMETR:27,def 8;
           C224: s8 in dom h by G2,C102,TOPREAL5:1;then
           h.s8=pro1.(f.s8) by FUNCT_1:22
              .=proj1.p8 by G2,FUNCT_1:72,G11
              .=p8`1 by PSCOMP_1:def 28;then
           s8 in h"B by C205,C224,FUNCT_1:def 13;then
           consider r0 being real number such that
           C107: r0>0 & Ball(s8m,r0) c= h"B by TOPMETR:22,C222,C223;
           reconsider r0 as Real by XREAL_0:def 1;
           reconsider r01=min(s8-s2,r0) as Real by XREAL_0:def 1;
           s8-s2>0 by D6,SQUARE_1:11;then
           C107b: r01>0 by C107,JGRAPH_3:6;then
           Ball(s8n,r01)= (].s8-r01,s8+r01 .[) by FRECHET:7;then
           C555: Ball(s8m,r01)= (].s8-r01,s8+r01 .[) /\ [.0,1.]
                                by C303,TOPMETR:13;
           s8-s2>=r01 by SQUARE_1:35;then
           -(s8-s2)<= -r01 by REAL_1:50;then
           s2-s8<= -r01 by XCMPLX_1:143;then
           s2-s8+s8<= -r01+s8 by REAL_1:55;then
           C107d: s2<= -r01+s8 by XCMPLX_1:27;then
           C107e: s2<=s8-r01 by XCMPLX_0:def 8;
           r01<=r0 by SQUARE_1:35;then
           C166: Ball(s8m,r01) c= Ball(s8m,r0) by PCOMPS_1:1;
           C117: the carrier of RealSpace =REAL by METRIC_1:def 14;
           reconsider Q=[.0,1.] as non empty Subset of the carrier of
           RealSpace by C117,TOPREAL5:1;
           C302: s8-r01>=0 by XCMPLX_0:def 8,C107d,C71;
           C109: (].s8-r01,s8+r01 .[)/\ [.0,1.] c= h"B
                          by C555,C107,C166,XBOOLE_1:1;
           r01=r01/2+r01/2 by XCMPLX_1:66;then
           r01-r01/2=r01/2 by XCMPLX_1:26;then
           r01-r01/2>0 by C107b,REAL_2:127;then
           r01-r01/2+r01/2>0+r01/2 by REAL_1:53;then
           C772: r01/2<r01 by XCMPLX_1:27;then
           C198: s8-r01/2>s8-r01 by REAL_2:105;
           C199: r01/2>0 by C107b,REAL_2:127;then
           --(r01/2)> 0;then
           C775: -(r01/2) < 0 by REAL_1:66;
           C774: s8+r01/2<s8+r01 by C772,REAL_1:67;
           -(r01/2)<r01/2 by C199,C775,AXIOMS:22;then
           s8+-(r01/2)<s8+r01/2 by REAL_1:67;then
           s8-(r01/2)< s8+r01/2 by XCMPLX_0:def 8;then
           C600: s8-r01<s8-r01/2 & s8-r01/2<s8+r01 by C198,C774,AXIOMS:22;then
           C601: s8-r01/2 in ].s8-r01,s8+r01 .[ by JORDAN6:45;
           1-0>s8-r01/2 by G2,C199,REAL_1:92;then
           C602: 1> s8-r01/2;
           s8-r01/2>0 by C302,C198;then
           s8-r01/2 in [.0,1.] by C602,TOPREAL5:1;then
           s8-r01/2 in (].s8-r01,s8+r01 .[)/\ [.0,1.]
                                  by C601,XBOOLE_0:def 3;then
           C205: s8-r01/2 in dom h & h.(s8-r01/2) in B by C109,FUNCT_1:def 13;
           reconsider s70=s8-r01/2 as Real by XREAL_0:def 1;
           C406: h.s70 in ].p8`1-ee,p8`1+ee.[ by C405,C205;
           s8+0>s8+-r01/2 by C775,REAL_1:67;then
           C200: s8>s8-r01/2 by XCMPLX_0:def 8;
           C111: s8>s70 & 1>=s70 & s70>=s2 by C602,C200,C107e,C600,AXIOMS:22;
           C110: for p7 being Point of TOP-REAL 2 st p7=f.s70
               holds p7`1<>e
              proof let p7 be Point of TOP-REAL 2;
               assume D1: p7=f.s70;
                s70 in [.0,1.] by C111,C71,TOPREAL5:1;then
                D7: p7 in rng f by B15,D1,FUNCT_1:def 5;
                rng f=[#]((TOP-REAL 2)|P) by B3,TOPS_2:def 5
                    .=P by PRE_TOPC:def 10;then
                D2: p7 in P by D7;
                h.s70=pro1.(f.s70) by C205,FUNCT_1:22
                 .=pr1.(p7) by D2,D1,FUNCT_1:72
                 .=p7`1 by PSCOMP_1:def 28;then
                D77: p8`1-ee<p7`1 & p7`1<p8`1+ee by C406,JORDAN6:45;
                now assume p7`1=e;then
                  E66: p8`1- abs(p8`1-e)/2 <e & e<p8`1+abs(p8`1-e)/2
                                        by D77;
                  now per cases;
                  case F0: p8`1-e>=0;then
                    F11: p8`1-(p8`1-e)/2<e & e< p8`1+(p8`1-e)/2
                                    by E66,ABSVALUE:def 1;
                    F7: (p8`1-e)/2=p8`1/2-e/2 by XCMPLX_1:121;
                    p8`1=p8`1/2+p8`1/2 by XCMPLX_1:66;then
                    p8`1/2+p8`1/2-p8`1/2+e/2<e
                    & e<p8`1/2+p8`1/2+(p8`1/2-e/2) by F11,F7,XCMPLX_1:37;then
                    p8`1/2+e/2<e
                    & e<p8`1/2+p8`1/2+p8`1/2-e/2 by XCMPLX_1:26,29;then
                    p8`1/2+e/2<e/2+e/2 by XCMPLX_1:66;then
                    p8`1/2<e/2 by REAL_1:55;then
                    p8`1/2-e/2<e/2-e/2 by REAL_1:92;then
                    p8`1/2-e/2< 0 by XCMPLX_1:14;then
                    F10: (p8`1-e)/2<0 by XCMPLX_1:121;
                    (p8`1-e)/2>=0/2 by REAL_1:73,F0;
                   hence contradiction by F10;
                  end;
                  case F0: p8`1-e<0;then
                    F11: p8`1-(-(p8`1-e))/2<e & e< p8`1+(-(p8`1-e))/2
                                    by E66,ABSVALUE:def 1;
                    F7: (-(p8`1-e))/2=-((p8`1-e)/2) by XCMPLX_1:188
                    .= -(p8`1/2-e/2) by XCMPLX_1:121;
                    p8`1=p8`1/2+p8`1/2 by XCMPLX_1:66;then
                    p8`1/2+p8`1/2+--(p8`1/2-e/2)<e
                    & e<p8`1/2+p8`1/2+-(p8`1/2-e/2)
