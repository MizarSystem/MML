                then LE f/.i,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A3,A142,A174,JORDAN5C:24;
                then A209: f/.i in LSeg(f/.kk,f/.(kk+1)) by A201,A206,A207;
                A210: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A174,TOPREAL1:def 5;
                    f is special by A3,TOPREAL1:def 10;
                  then (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A174,TOPREAL1:def 7;
                 hence contradiction
                       by A3,A143,A172,A173,A174,A175,A198,A209,A210,Th6;
              end;
              then i-kk-1+1>=0+1 by XREAL_1:9;
              then i-'kk=i-kk by BINARITH:def 3;
              then A211: i-k2=i-(i-kk-1) by A208,BINARITH:def 3 .=kk+1;
              then A212: i-'k2>0 by BINARITH:def 3;
              A213: i-k2=i-'k2 by A211,BINARITH:def 3;
              A214: i-'k2>=0+1 by A212,NAT_1:13;
                  i-'k2<=len f by A9,BINARITH:68;
              then P[k2] by A197,A211,A213,A214,FINSEQ_4:24;
              then k2>=k by A15;
              then i-k2<=i-k by REAL_2:106;
              then A215: LE f/.(kk+1),f/.(i-'k),L~f,f/.1,f/.(len f)
                           by A3,A16,A22,A211,A213,A214,JORDAN5C:24;
                  LE f/.(i-'k),f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A21,A172,A179,JORDAN5C:13;
               hence contradiction by A2,A3,A21,A122,A197,A215,JORDAN5C:12;
              end;
              end;
            hence contradiction;
         end;
             for p5 being Point of TOP-REAL 2 st
            LE pk,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e
          proof let p5 be Point of TOP-REAL 2;
            assume A216: LE pk,p5,P,p1,p2 & LE p5,p,P,p1,p2;
             then A217: p5 in P by JORDAN5C:def 3;
                 now per cases by A2,A170,A217,Th19;
             case LE p5,p44,P,p1,p2;
              hence p5`1>=e by A171,A216;
             end;
             case LE p44,p5,P,p1,p2;
              hence p5`1>=e by A156,A216;
             end;
             end;
           hence p5`1>=e;
          end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A1,A2,A122,A169,Def2;
     end;
     end;
    hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e;
   end;
   end;
  hence thesis;
end;

theorem     for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_S-P_arc_joining p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
   then A2: P is_an_arc_of p1,p2 by TOPREAL4:3;
   consider f being FinSequence of (TOP-REAL 2) such that
   A3:  f is_S-Seq & P = L~f & p1=f/.1 & p2=f/.len f by A1,TOPREAL4:def 1;
   A4: L~f = union { LSeg(f,i) where i is Element of NAT :
    1 <= i & i+1 <= len f }
                          by TOPREAL1:def 6;
   then consider Y being set such that
   A5: p in Y & Y in
      { LSeg(f,i) where i is Element of NAT : 1 <= i & i+1 <= len f }
            by A1,A3,TARSKI:def 4;
   A6: Y c= L~f
   proof let x be set;assume x in Y;
    hence x in L~f by A4,A5,TARSKI:def 4;
   end;
   consider i being Element of NAT such that
   A7: Y=LSeg(f,i) &  (1 <= i & i+1 <= len f) by A5;
   A8: 1<i+1 by A7,NAT_1:13;
   A9: i<len f by A7,NAT_1:13;
       i+1 in Seg len f by A7,A8,FINSEQ_1:3;
   then A10: i+1 in dom f by FINSEQ_1:def 3;
   A11: 1<len f by A7,A9,XXREAL_0:2;
   A12: 1<i+1 by A7,NAT_1:13;
   defpred P[Element of NAT] means
    for p being Point of TOP-REAL 2 st p=f.(i+1+$1)
        holds p`1<>e;
   A13: len f - (i+1)>=0 by A7,XREAL_1:50;
       len f in Seg len f by A11,FINSEQ_1:3;
   then A14: len f in dom f by FINSEQ_1:def 3;
       i+1+((len f)-'(i+1))=i+1+(len f -(i+1)) by A13,BINARITH:def 3
                      .=len f;
   then A15: P[(len f)-'(i+1)] by A1,A3,A14,FINSEQ_4:def 4;
   then A16: ex k being Element of NAT st P[k];
       ex k being Element of NAT st P[k] & for n being Element of NAT st P[n]
       holds k <= n
                                  from NAT_1:sch 5(A16);
   then consider k being Element of NAT such that
   A17: P[k] & for n being Element of NAT st P[n] holds k <= n;
   A18: k<=len f -'(i+1) by A15,A17;
       len f -'(i+1)=len f -(i+1) by A13,BINARITH:def 3;
   then A19: k+(i+1)<= len f -(i+1)+(i+1) by A18,XREAL_1:9;
       i+k >=i by NAT_1:11;
   then A20: i+k+1 >=i+1 by XREAL_1:9;
   then A21: k+i+1>1 by A12,XXREAL_0:2;
   A22: i+1+k >1 by A12,A20,XXREAL_0:2;
       1<=i+1+k by A8,NAT_1:12;
   then i+1+k in Seg len f by A19,FINSEQ_1:3;
   then A23: i+1+k in dom f by FINSEQ_1:def 3;
   then A24: f/.(i+1+k)=f.(i+1+k) by FINSEQ_4:def 4;
   then reconsider pk=f.(i+1+k) as Point of TOP-REAL 2;
   A25: pk`1<> e by A17;
   reconsider kn=k+1 as Element of NAT;
       now per cases by A25,XXREAL_0:1;
   case A26: pk`1<e;
         now per cases;
     case A27: k=0;
      set p44=f/.(i+1);
        A28: pk=p44 by A10,A27,FINSEQ_4:def 4;
        A29: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A7,TOPREAL1:def 5;
            p44 in LSeg(p,f/.(i+1)) by TOPREAL1:6;
        then A30: LE p,p44,P,p1,p2 by A3,A5,A7,A9,SPRECT_4:5;
            for p5 being Point of TOP-REAL 2 st
         LE p5,p44,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume A31: LE p5,p44,P,p1,p2 & LE p,p5,P,p1,p2;
               Segment(P,p1,p2,p,p44)={p8 where p8 is Point of TOP-REAL 2:
            LE p,p8,P,p1,p2 & LE p8,p44,P,p1,p2} by JORDAN6:29;
           then A32: p5 in Segment(P,p1,p2,p,p44) by A31;
              p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p,p44) c= LSeg(f,i) by A5,A7,A29,TOPREAL1:12;
           then A33: LSeg(p,p44) c= P by A3,A6,A7,XBOOLE_1:1;
               now per cases;
           case p44<>p;
             then LSeg(p,p44) is_an_arc_of p,p44 by TOPREAL1:15;
             then Segment(P,p1,p2,p,p44)=LSeg(p,p44) by A2,A30,A33,Th28;
            hence p5`1<=e by A1,A26,A28,A32,TOPREAL1:9;
           end;
           case p44=p;
            hence p5`1<=e by A1,A10,A26,A27,FINSEQ_4:def 4;
           end;
           end;
          hence p5`1<=e;
         end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
                     by A1,A2,A26,A28,A30,Def3;
     end;
     case k<>0;
        then A34: k>0;
      set p44=f/.(i+1);
        A35: now assume (f/.(i+1))`1<>e;
         then for p9 being Point of TOP-REAL 2 st p9=f.(i+1+0)
            holds p9`1<>e by A10,FINSEQ_4:def 4;
         hence contradiction by A17,A34;
        end;
        A36: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A7,TOPREAL1:def 5;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
            p44 in LSeg(p,f/.(i+1)) by TOPREAL1:6;
        then A37: LE p,p44,P,p1,p2 by A3,A5,A7,A9,SPRECT_4:5;
        A38: for p5 being Point of TOP-REAL 2 st
         LE p,p5,P,p1,p2 & LE p5,p44,P,p1,p2 holds p5`1<=e
         proof let p5 be Point of TOP-REAL 2;
          assume A39: LE p,p5,P,p1,p2 & LE p5,p44,P,p1,p2;
               Segment(P,p1,p2,p,p44)={p8 where p8 is Point of TOP-REAL 2:
            LE p,p8,P,p1,p2 & LE p8,p44,P,p1,p2} by JORDAN6:29;
           then A40: p5 in Segment(P,p1,p2,p,p44) by A39;
              p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p,p44) c= LSeg(f,i) by A5,A7,A36,TOPREAL1:12;
           then A41: LSeg(p,p44) c= P by A3,A6,A7,XBOOLE_1:1;
               now per cases;
           case p44<>p;
             then LSeg(p,p44) is_an_arc_of p,p44 by TOPREAL1:15;
             then Segment(P,p1,p2,p,p44)=LSeg(p,p44) by A2,A37,A41,Th28;
            hence p5`1<=e by A1,A35,A40,TOPREAL1:9;
           end;
           case p44=p;
             then Segment(P,p1,p2,p,p44)={p44} by A1,A2,Th1;
            hence p5`1<=e by A35,A40,TARSKI:def 1;
           end;
           end;
          hence p5`1<=e;
         end;
        reconsider g=mid(f,1,i) as FinSequence of (TOP-REAL 2);
             i+1<=i+1+k by NAT_1:11;
         then A42: LE p44,pk,P,p1,p2
                 by A3,A8,A19,A24,JORDAN5C:24;
         then A43: LE p,pk,P,p1,p2 by A37,JORDAN5C:13;
         A44: p44 in P by A42,JORDAN5C:def 3;
         A45: now assume not
           for p51 being Point of TOP-REAL 2 st
            LE p44,p51,P,p1,p2 & LE p51,pk,P,p1,p2 holds p51`1<=e;
           then consider p51 being Point of TOP-REAL 2 such that
           A46: LE p44,p51,P,p1,p2 & LE p51,pk,P,p1,p2 & p51`1>e;
               p51 in P by A46,JORDAN5C:def 3;
           then consider Y3 being set such that
           A47: p51 in Y3 & Y3 in
            { LSeg(f,i5) where i5 is Element of NAT : 1 <= i5 & i5+1 <= len f }
                     by A3,A4,TARSKI:def 4;
           consider kk being Element of NAT such that
           A48: Y3=LSeg(f,kk) &  (1 <= kk & kk+1 <= len f) by A47;
           A49: kk-1>=0 by A48,XREAL_1:50;
           A50: kk<len f by A48,NAT_1:13;
           then A51: kk in dom f by A48,FINSEQ_3:27;
           A52: 1<kk+1 by A48,NAT_1:13;
           then A53: kk+1 in dom f by A48,FINSEQ_3:27;
           A54: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A48,TOPREAL1:def 5;
           A55: LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                     by A3,A47,A48,JORDAN5C:26;
           A56: LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A3,A47,A48,JORDAN5C:25;
           A57: f is one-to-one by A3,TOPREAL1:def 10;
               kk<kk+1 by NAT_1:13;
           then A58: f.kk <> f.(kk+1) by A51,A53,A57,FUNCT_1:def 8;
           A59: f/.kk=f.kk by A51,FINSEQ_4:def 4;
               now per cases by A46,A47,A48,A54,Th2;
           case A60: (f/.(kk+1))`1>e;
              set k2=kk-'i;
                A61: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A46,A55,JORDAN5C:13;
              A62: now assume kk-i<0; then kk-i+i<0+i by XREAL_1:8
;
                then kk+1<=i+1 by XREAL_1:9;
                then LE f/.(kk+1),f/.(i+1),L~f,f/.1,f/.(len f)
                         by A3,A7,A52,JORDAN5C:24;
               hence contradiction by A2,A3,A35,A60,A61,JORDAN5C:12;
              end;
              A63: 1<kk+1 by A48,NAT_1:13;
              A64: i+1+k2=1+i+(kk-i) by A62,BINARITH:def 3
                        .=kk+1;
               then P[k2] by A48,A60,A63,FINSEQ_4:24;
              then k2>=k by A17;
              then i+1+k2>=i+1+k by XREAL_1:9;
              then A65: LE f/.(i+1+k),f/.(i+1+k2),L~f,f/.1,f/.(len f)
                           by A3,A21,A48,A64,JORDAN5C:24;
              A66: LE p51,f/.(i+1+k),L~f,f/.1,f/.(len f) by A3,A23,A46,
FINSEQ_4:def 4;
              A67: LE f/.(kk),f/.(i+1+k),L~f,f/.1,f/.(len f)
                                 by A3,A24,A46,A56,JORDAN5C:13;
              then A68: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A64,A65,JORDAN5C:13;
                  Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              then A69: f/.(i+1+k) in Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk
+
1))
                                            by A64,A65,A67;
              A70: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A71: z in LSeg(f/.kk,f/.(kk+1));
                    LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Element of NAT :
                 1 <= i7 & i7+1 <= len f }
                               by A48,A54;
               hence z in L~f by A4,A71,TARSKI:def 4;
              end;
              A72: f is special by A3,TOPREAL1:def 10;
              A73: f is one-to-one by A3,TOPREAL1:def 10;
                  kk<kk+1 by NAT_1:13;
              then A74: f.kk <> f.(kk+1) by A51,A53,A73,FUNCT_1:def 8;
              A75: f/.kk=f.kk by A51,FINSEQ_4:def 4;
                  f/.(kk+1)=f.(kk+1) by A53,FINSEQ_4:def 4;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A74,A75,TOPREAL1:15;
              then A76: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A2,A3,A68,A70,Th28;
               A77: (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A48,A72,TOPREAL1:def 7;
                  (f/.(kk))`1<e by A24,A26,A60,A69,A76,Th3;
              then (f/.kk)`1< (f/.(kk+1))`1 by A60,XXREAL_0:2;
              then (f/.(i+1+k))`1>=p51`1
                     by A3,A47,A48,A50,A54,A66,A69,A76,A77,Th7;
            hence contradiction by A24,A26,A46,XXREAL_0:2;
           end;
           case A78: (f/.(kk))`1 >e & (f/.(kk+1))`1<=e;
              set k2=kk-'(i+1);
              A79: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A80: z in LSeg(f/.kk,f/.(kk+1));
                    LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Element of NAT :
                 1 <= i7 & i7+1 <= len f }
                               by A48,A54;
               hence z in L~f by A4,A80,TARSKI:def 4;
              end;
              A81: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              A82: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                          by A3,A48,JORDAN5C:23;
                  f/.(kk+1)=f.(kk+1) by A53,FINSEQ_4:def 4;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A58,A59,TOPREAL1:15;
              then A83: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A2,A3,A79,A82,Th28;
                  LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A47,A48,JORDAN5C:26;
                then A84: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A46,JORDAN5C:13;
              A85: now assume
                    kk-(i+1)<0;
                then kk-(i+1)+(i+1)<0+(i+1) by XREAL_1:8;
                then kk<=i by NAT_1:13;
                then A86: LE f/.kk,f/.i,L~f,f/.1,f/.(len f)
                         by A3,A9,A48,JORDAN5C:24;
                    LE f/.i,p44,L~f,f/.1,f/.(len f) by A3,A7,JORDAN5C:23;
                then LE f/.i,f/.(kk+1),L~f,f/.1,f/.(len f)
                             by A84,JORDAN5C:13;
                then A87: f/.i in LSeg(f/.kk,f/.(kk+1)) by A81,A83,A86;
                    f/.i in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
                then LSeg(f/.kk,f/.(kk+1)) /\ LSeg(f/.i,f/.(i+1))<>{}
                             by A87,XBOOLE_0:def 3;
                then A88: not (LSeg(f/.kk,f/.(kk+1)) misses LSeg(f/.i,f/.(i+1)
)
)
                             by XBOOLE_0:def 7;
                A89: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A48,TOPREAL1:def 5;
                A90: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A7,TOPREAL1:def 5;
                A91: f is s.n.c. by A3,TOPREAL1:def 10;
                then A92: kk+1>=i by A88,A89,A90,TOPREAL1:def 9;
                A93: kk-1=kk-'1  by A49,BINARITH:def 3;
                    i+1>=kk by A88,A89,A90,A91,TOPREAL1:def 9;
                then i+1-1>=kk-1 by XREAL_1:11;
                then A94: i=kk-'1 or i=kk-'1+1 or i=kk-'1+2 by A92,A93,
JORDAN2C:2;
                A95: now assume A96: i=kk-'1+2;
                  then kk+1<i+1 by A93,NAT_1:13;
                  then LE f/.(kk+1),p44,L~f,f/.1,f/.(len f)
                             by A3,A7,A52,JORDAN5C:24;
                  then p44=f/.(kk+1) by A2,A3,A84,JORDAN5C:12;
                  then f.(i+1)=f/.(kk+1) by A10,FINSEQ_4:def 4;
                  then A97: f.(i+1)=f.(kk+1) by A53,FINSEQ_4:def 4;
                      f is one-to-one by A3,TOPREAL1:def 10;
                  then i+1=kk+1 by A10,A53,A97,FUNCT_1:def 8;
                 hence contradiction by A93,A96;
                end;
                A98:now per cases by A93,A94,A95;
                 case i=kk;
                  hence p44 in LSeg(f,kk) by A36,TOPREAL1:6;
                 end;
                 case i=kk-1;
                  hence p44 in LSeg(f,kk) by A54,TOPREAL1:6;
                 end;
                 end;
                     f is special by A3,TOPREAL1:def 10;
                  then (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A48,TOPREAL1:def 7;
                 hence contradiction
                 by A3,A35,A46,A47,A48,A50,A78,A98,Th6;
              end;
              then A99: kk-'(i+1)=kk-(i+1) by BINARITH:def 3;
                 i+1+k2=i+1+(kk-(i+1)) by A85,BINARITH:def 3
                        .=kk;
               then P[k2] by A48,A50,A78,FINSEQ_4:24;
              then k2>=k by A17;
              then kk-(i+1)+(i+1)>=k+(i+1) by A99,XREAL_1:9;
              then A100: LE f/.(i+1+k),f/.(kk),L~f,f/.1,f/.(len f)
                  by A3,A22,A50,JORDAN5C:24;
                  LE f/.(kk),f/.(i+1+k),L~f,f/.1,f/.(len f)
                                 by A3,A24,A46,A56,JORDAN5C:13;
               hence contradiction by A2,A3,A24,A26,A78,A100,JORDAN5C:12;
              end;
              end;
            hence contradiction;
         end;
             for p5 being Point of TOP-REAL 2 st
            LE p5,pk,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1<=e
          proof let p5 be Point of TOP-REAL 2;
            assume A101: LE p5,pk,P,p1,p2 & LE p,p5,P,p1,p2;
             then A102: p5 in P by JORDAN5C:def 3;
                 now per cases by A2,A44,A102,Th19;
             case LE p44,p5,P,p1,p2;
              hence p5`1<=e by A45,A101;
             end;
             case LE p5,p44,P,p1,p2;
              hence p5`1<=e by A38,A101;
             end;
             end;
           hence p5`1<=e;
          end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
                    by A1,A2,A26,A43,Def3;
     end;
     end;
    hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e;
   end;
   case A103: pk`1>e;
         now per cases;
     case A104: k=0;
      set p44=f/.(i+1);
        A105: pk=p44 by A10,A104,FINSEQ_4:def 4;
        A106: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A7,TOPREAL1:def 5;
            p44 in LSeg(p,f/.(i+1)) by TOPREAL1:6;
        then A107: LE p,p44,P,p1,p2 by A3,A5,A7,A9,SPRECT_4:5;
            for p5 being Point of TOP-REAL 2 st
         LE p5,p44,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1>=e
         proof let p5 be Point of TOP-REAL 2;
          assume A108: LE p5,p44,P,p1,p2 & LE p,p5,P,p1,p2;
               Segment(P,p1,p2,p,p44)={p8 where p8 is Point of TOP-REAL 2:
            LE p,p8,P,p1,p2 & LE p8,p44,P,p1,p2} by JORDAN6:29;
           then A109: p5 in Segment(P,p1,p2,p,p44) by A108;
              p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p,p44) c= LSeg(f,i) by A5,A7,A106,TOPREAL1:12;
           then A110: LSeg(p,p44) c= P by A3,A6,A7,XBOOLE_1:1;
               now per cases;
           case p44<>p;
             then LSeg(p,p44) is_an_arc_of p,p44 by TOPREAL1:15;
             then Segment(P,p1,p2,p,p44)=LSeg(p,p44) by A2,A107,A110,Th28
;
            hence p5`1>=e by A1,A103,A105,A109,TOPREAL1:9;
           end;
           case p44=p;
            hence p5`1>=e by A1,A10,A103,A104,FINSEQ_4:def 4;
           end;
           end;
          hence p5`1>=e;
         end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
                     by A1,A2,A103,A105,A107,Def4;
     end;
     case k<>0;
        then A111: k>0;
      set p44=f/.(i+1);
        A112: now assume (f/.(i+1))`1<>e;
         then for p9 being Point of TOP-REAL 2 st p9=f.(i+1+0)
            holds p9`1<>e by A10,FINSEQ_4:def 4;
         hence contradiction by A17,A111;
        end;
        A113: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A7,TOPREAL1:def 5;
        reconsider g=mid(f,i,len f) as FinSequence of (TOP-REAL 2);
            p44 in LSeg(p,f/.(i+1)) by TOPREAL1:6;
        then A114: LE p,p44,P,p1,p2 by A3,A5,A7,A9,SPRECT_4:5;
        A115: for p5 being Point of TOP-REAL 2 st
         LE p,p5,P,p1,p2 & LE p5,p44,P,p1,p2 holds p5`1>=e
         proof let p5 be Point of TOP-REAL 2;
          assume A116: LE p,p5,P,p1,p2 & LE p5,p44,P,p1,p2;
               Segment(P,p1,p2,p,p44)={p8 where p8 is Point of TOP-REAL 2:
            LE p,p8,P,p1,p2 & LE p8,p44,P,p1,p2} by JORDAN6:29;
           then A117: p5 in Segment(P,p1,p2,p,p44) by A116;
              p44 in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
            then LSeg(p,p44) c= LSeg(f,i) by A5,A7,A113,TOPREAL1:12;
           then A118: LSeg(p,p44) c= P by A3,A6,A7,XBOOLE_1:1;
               now per cases;
           case p44<>p;
             then LSeg(p,p44) is_an_arc_of p,p44 by TOPREAL1:15;
             then Segment(P,p1,p2,p,p44)=LSeg(p,p44) by A2,A114,A118,Th28
;
            hence p5`1>=e by A1,A112,A117,TOPREAL1:9;
           end;
           case p44=p;
             then Segment(P,p1,p2,p,p44)={p44} by A1,A2,Th1;
            hence p5`1>=e by A112,A117,TARSKI:def 1;
           end;
           end;
          hence p5`1>=e;
         end;
        reconsider g=mid(f,1,i) as FinSequence of (TOP-REAL 2);
             i+1<=i+1+k by NAT_1:11;
         then A119: LE p44,pk,P,p1,p2
                 by A3,A8,A19,A24,JORDAN5C:24;
         then A120: LE p,pk,P,p1,p2 by A114,JORDAN5C:13;
         A121: p44 in P by A119,JORDAN5C:def 3;
         A122: now assume not
           for p51 being Point of TOP-REAL 2 st
            LE p44,p51,P,p1,p2 & LE p51,pk,P,p1,p2 holds p51`1>=e;
           then consider p51 being Point of TOP-REAL 2 such that
           A123: LE p44,p51,P,p1,p2 & LE p51,pk,P,p1,p2 & p51`1<e;
               p51 in P by A123,JORDAN5C:def 3;
           then consider Y3 being set such that
           A124: p51 in Y3 & Y3 in
            { LSeg(f,i5) where i5 is Element of NAT : 1 <= i5 & i5+1 <= len f }
                     by A3,A4,TARSKI:def 4;
           consider kk being Element of NAT such that
           A125: Y3=LSeg(f,kk) &  (1 <= kk & kk+1 <= len f) by A124;
           A126: kk-1>=0 by A125,XREAL_1:50;
           A127: kk<len f by A125,NAT_1:13;
           then kk in Seg len f by A125,FINSEQ_1:3;
           then A128: kk in dom f by FINSEQ_1:def 3;
           A129: 1<kk+1 by A125,NAT_1:13;
           then A130: kk+1 in dom f by A125,FINSEQ_3:27;
           A131: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A125,TOPREAL1:def 5;
           A132: LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                     by A3,A124,A125,JORDAN5C:26;
           A133: LE f/.kk,p51,L~f,f/.1,f/.(len f)
                                 by A3,A124,A125,JORDAN5C:25;
           A134: f is one-to-one by A3,TOPREAL1:def 10;
               kk<kk+1 by NAT_1:13;
           then A135: f.kk <> f.(kk+1) by A128,A130,A134,FUNCT_1:def 8;
           A136: f/.kk=f.kk by A128,FINSEQ_4:def 4;
               now per cases by A123,A124,A125,A131,Th3;
           case A137: (f/.(kk+1))`1<e;
              set k2=kk-'i;
                A138: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A123,A132,JORDAN5C:13;
              A139: now assume kk-i<0; then kk-i+i<0+i by XREAL_1:8
;
                then kk+1<=i+1 by XREAL_1:9;
                then LE f/.(kk+1),f/.(i+1),L~f,f/.1,f/.(len f)
                         by A3,A7,A129,JORDAN5C:24;
               hence contradiction by A2,A3,A112,A137,A138,JORDAN5C:12;
              end;
              A140: 1<kk+1 by A125,NAT_1:13;
              A141: i+1+k2=1+i+(kk-i) by A139,BINARITH:def 3
                        .=kk+1;
               then P[k2] by A125,A137,A140,FINSEQ_4:24;
              then k2>=k by A17;
              then i+1+k2>=i+1+k by XREAL_1:9;
              then A142: LE f/.(i+1+k),f/.(i+1+k2),L~f,f/.1,f/.(len f)
                           by A3,A21,A125,A141,JORDAN5C:24;
              A143: LE p51,f/.(i+1+k),L~f,f/.1,f/.(len f) by A3,A23,A123,
FINSEQ_4:def 4;
              A144: LE f/.(kk),f/.(i+1+k),L~f,f/.1,f/.(len f)
                                 by A3,A24,A123,A133,JORDAN5C:13;
              then A145: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A141,A142,JORDAN5C:13;
                  Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              then A146: f/.(i+1+k) in Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk
+1))
                                            by A141,A142,A144;
              A147: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A148: z in LSeg(f/.kk,f/.(kk+1));
                    LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Element of NAT :
                 1 <= i7 & i7+1 <= len f }
                               by A125,A131;
               hence z in L~f by A4,A148,TARSKI:def 4;
              end;
              A149: f is special by A3,TOPREAL1:def 10;
              A150: f is one-to-one by A3,TOPREAL1:def 10;
                  kk<kk+1 by NAT_1:13;
              then A151: f.kk <> f.(kk+1) by A128,A130,A150,FUNCT_1:def 8;
              A152: f/.kk=f.kk by A128,FINSEQ_4:def 4;
                  f/.(kk+1)=f.(kk+1) by A130,FINSEQ_4:def 4;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A151,A152,TOPREAL1:15;
              then A153: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A2,A3,A145,A147,Th28;
               A154: (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A125,A149,TOPREAL1:def 7;
                  (f/.(kk))`1>e by A24,A103,A137,A146,A153,Th2;
              then (f/.kk)`1> (f/.(kk+1))`1 by A137,XXREAL_0:2;
              then (f/.(i+1+k))`1<=p51`1
                     by A3,A124,A125,A127,A131,A143,A146,A153,A154,Th6;
            hence contradiction by A24,A103,A123,XXREAL_0:2;
           end;
           case A155: (f/.(kk))`1 <e & (f/.(kk+1))`1>=e;
              set k2=kk-'(i+1);
              A156: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A157: z in LSeg(f/.kk,f/.(kk+1));
                    LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Element of NAT :
                 1 <= i7 & i7+1 <= len f }
                               by A125,A131;
               hence z in L~f by A4,A157,TARSKI:def 4;
              end;
              A158: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              A159: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                          by A3,A125,JORDAN5C:23;
                  f/.(kk+1)=f.(kk+1) by A130,FINSEQ_4:def 4;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A135,A136,TOPREAL1:15;
              then A160: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A2,A3,A156,A159,Th28;
                  LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A124,A125,JORDAN5C:26;
                then A161: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A123,JORDAN5C:13;
              A162: now assume kk-(i+1)<0;
                then kk-(i+1)+(i+1)<0+(i+1) by XREAL_1:8;
                then kk<=i by NAT_1:13;
                then A163: LE f/.kk,f/.i,L~f,f/.1,f/.(len f)
                         by A3,A9,A125,JORDAN5C:24;
                    LE f/.i,p44,L~f,f/.1,f/.(len f) by A3,A7,JORDAN5C:23;
                then LE f/.i,f/.(kk+1),L~f,f/.1,f/.(len f)
                             by A161,JORDAN5C:13;
                then A164: f/.i in LSeg(f/.kk,f/.(kk+1)) by A158,A160,A163;
                    f/.i in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
                then LSeg(f/.kk,f/.(kk+1)) /\ LSeg(f/.i,f/.(i+1))<>{}
                             by A164,XBOOLE_0:def 3;
                then A165: not (LSeg(f/.kk,f/.(kk+1)) misses LSeg(f/.i,f/.(i+1)
))
                             by XBOOLE_0:def 7;
                A166: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A125,TOPREAL1:def 5;
                A167: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A7,TOPREAL1:def 5;
                A168: f is s.n.c. by A3,TOPREAL1:def 10;
                then A169: kk+1>=i by A165,A166,A167,TOPREAL1:def 9;
                A170: kk-1=kk-'1  by A126,BINARITH:def 3;
                    i+1>=kk by A165,A166,A167,A168,TOPREAL1:def 9;
                then i+1-1>=kk-1 by XREAL_1:11;
                then A171: i=kk-'1 or i=kk-'1+1 or i=kk-'1+2 by A169,A170,
JORDAN2C:2;
                A172: now assume A173: i=kk-'1+2;
                  then kk+1<i+1 by A170,NAT_1:13;
                  then LE f/.(kk+1),p44,L~f,f/.1,f/.(len f)
                             by A3,A7,A129,JORDAN5C:24;
                  then p44=f/.(kk+1) by A2,A3,A161,JORDAN5C:12;
                  then f.(i+1)=f/.(kk+1) by A10,FINSEQ_4:def 4;
                  then A174: f.(i+1)=f.(kk+1) by A130,FINSEQ_4:def 4;
                      f is one-to-one by A3,TOPREAL1:def 10;
                  then i+1=kk+1 by A10,A130,A174,FUNCT_1:def 8;
                 hence contradiction by A170,A173;
                end;
                A175:now per cases by A170,A171,A172;
                 case i=kk;
                  hence p44 in LSeg(f,kk) by A113,TOPREAL1:6;
                 end;
                 case i=kk-1;
                  hence p44 in LSeg(f,kk) by A131,TOPREAL1:6;
                 end;
                 end;
                     f is special by A3,TOPREAL1:def 10;
                  then (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A125,TOPREAL1:def 7;
                 hence contradiction
                 by A3,A112,A123,A124,A125,A127,A155,A175,Th7;
              end;
              then A176: kk-'(i+1)=kk-(i+1) by BINARITH:def 3;
                 i+1+k2=i+1+(kk-(i+1)) by A162,BINARITH:def 3
                        .=kk;
               then P[k2] by A125,A127,A155,FINSEQ_4:24;
              then k2>=k by A17;
              then kk-(i+1)+(i+1)>=k+(i+1) by A176,XREAL_1:9;
              then A177: LE f/.(i+1+k),f/.(kk),L~f,f/.1,f/.(len f)
                  by A3,A22,A127,JORDAN5C:24;
                  LE f/.(kk),f/.(i+1+k),L~f,f/.1,f/.(len f)
                                 by A3,A24,A123,A133,JORDAN5C:13;
               hence contradiction by A2,A3,A24,A103,A155,A177,JORDAN5C:12;
              end;
              end;
            hence contradiction;
         end;
             for p5 being Point of TOP-REAL 2 st
            LE p5,pk,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1>=e
          proof let p5 be Point of TOP-REAL 2;
            assume A178: LE p5,pk,P,p1,p2 & LE p,p5,P,p1,p2;
             then A179: p5 in P by JORDAN5C:def 3;
                 now per cases by A2,A121,A179,Th19;
             case LE p44,p5,P,p1,p2;
              hence p5`1>=e by A122,A178;
             end;
             case LE p5,p44,P,p1,p2;
              hence p5`1>=e by A115,A178;
             end;
             end;
           hence p5`1>=e;
          end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
                    by A1,A2,A103,A120,Def4;
     end;
     end;
    hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e;
   end;
   end;
  hence thesis;
end;
