theorem Th6: for f being S-Sequence_in_R2, p,q being Point of TOP-REAL 2 st
   1 <=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) &
   (f/.j)`2=(f/.(j+1))`2 & (f/.j)`1>(f/.(j+1))`1 &
   LE p,q,L~f,f/.1,f/.(len f)
  holds p`1>=q`1
proof let f be S-Sequence_in_R2, p,q be Point of TOP-REAL 2;
 assume A1: 1 <=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) &
   (f/.j)`2=(f/.(j+1))`2 & (f/.j)`1>(f/.(j+1))`1 &
   LE p,q,L~f,f/.1,f/.(len f);
  A2: L~f is_an_arc_of f/.1,f/.len f by TOPREAL1:31;
      j+1<=len f by A1,NAT_1:38;
     then A3: LSeg(f,j)=LSeg(f/.j,f/.(j+1)) by A1,TOPREAL1:def 5;
     per cases;
     suppose A4: p`1<> (f/.j)`1;
      (f/.j)`1>=p`1 by A1,A3,TOPREAL1:9;
     then (f/.j)`1>p`1 by A4,REAL_1:def 5;
     then A5: (f/.j)`1-p`1>0 by SQUARE_1:11;
     now assume A6: p`1<q`1;
     then A7: q`1-p`1>0 by SQUARE_1:11;
     A8: p`2=(f/.j)`2 by A1,A3,GOBOARD7:6;
     reconsider a=((f/.j)`1-q`1)/((f/.j)`1-p`1) as Real by XREAL_0:def 1;
     A9: (1-a)=((f/.j)`1-p`1)/((f/.j)`1-p`1)- ((f/.j)`1-q`1)/((f/.j)`1-p`1)
                         by A5,XCMPLX_1:60
         .=((f/.j)`1-p`1- ((f/.j)`1-q`1))/((f/.j)`1-p`1) by XCMPLX_1:121
         .=(q`1-p`1)/((f/.j)`1-p`1);
     A10: ((1-a)*(f/.j)+a*p)`1=((1-a)*(f/.j))`1+(a*p)`1 by TOPREAL3:7
     .=(1-a)*(f/.j)`1+ (a*p)`1 by TOPREAL3:9
     .=1* (f/.j)`1 -a*(f/.j)`1 +a*(p`1) by TOPREAL3:9
     .= (f/.j)`1 -(a*((f/.j)`1 -(p`1)))
     .= (f/.j)`1 -((f/.j)`1-q`1) by A5,XCMPLX_1:88
     .=q`1;
     A11: ((1-a)*(f/.j)+a*p)`2=((1-a)*(f/.j))`2+(a*p)`2 by TOPREAL3:7
     .=(1-a)*(f/.j)`2+ (a*p)`2 by TOPREAL3:9
     .=1* (f/.j)`2 -a*(f/.j)`2 +a*(p`2) by TOPREAL3:9
     .=q`2 by A1,A3,A8,GOBOARD7:6;
       (f/.j)`1>=q`1 & q`1>=(f/.(j+1))`1 by A1,A3,TOPREAL1:9;
     then A12: (f/.j)`1-q`1>=0 by SQUARE_1:12;
       (1-a)>=0 by A5,A7,A9,REAL_2:125;
     then 1-a+a>=0+a by REAL_1:55;
     then A13: 0<=a & a<=1 by A5,A12,REAL_2:125;
      q=(1-a)*(f/.j)+a*p by A10,A11,TOPREAL3:11;
     then q in { (1-lambda)*(f/.j) + lambda*p where lambda is Real :
          0 <= lambda & lambda <= 1 } by A13;
     then q in LSeg(f/.j,p) by TOPREAL1:def 4;
     then LE q,p,L~f,f/.1,f/.(len f) by A1,SPRECT_3:40;
    hence contradiction by A1,A2,A6,JORDAN5C:12;
   end;
 hence p`1>=q`1;
 end;
 suppose p`1=(f/.j)`1;
 hence p`1>=q`1 by A1,A3,TOPREAL1:9;
 end;
end;

theorem Th7: for f being S-Sequence_in_R2, p,q being Point of TOP-REAL 2 st
   1 <=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) &
   (f/.j)`2=(f/.(j+1))`2 & (f/.j)`1<(f/.(j+1))`1 &
   LE p,q,L~f,f/.1,f/.(len f)
  holds p`1<=q`1
proof let f be S-Sequence_in_R2, p,q be Point of TOP-REAL 2;
 assume A1: 1 <=j & j < len f & p in LSeg(f,j) & q in LSeg(f,j) &
   (f/.j)`2=(f/.(j+1))`2 & (f/.j)`1<(f/.(j+1))`1 &
   LE p,q,L~f,f/.1,f/.(len f);
  A2: L~f is_an_arc_of f/.1,f/.len f by TOPREAL1:31;
        j+1<=len f by A1,NAT_1:38;
     then A3: LSeg(f,j)=LSeg(f/.j,f/.(j+1)) by A1,TOPREAL1:def 5;
     per cases;
     suppose A4: p`1<> (f/.(j))`1;
        (f/.j)`1<=p`1 & p`1<=(f/.(j+1))`1 by A1,A3,TOPREAL1:9;
     then (f/.(j))`1<p`1 by A4,REAL_1:def 5;
     then A5: (f/.(j))`1 -p`1<0 by REAL_2:105;
      now assume A6: p`1>q`1;
     then A7: q`1-p`1<0 by REAL_2:105;
     A8: p`2=(f/.j)`2 by A1,A3,GOBOARD7:6;
     reconsider a=((f/.(j))`1-q`1)/((f/.(j))`1-p`1)
                             as Real by XREAL_0:def 1;
     A9: (1-a)=((f/.(j))`1-p`1)/((f/.(j))`1-p`1)
     - ((f/.(j))`1-q`1)/((f/.(j))`1-p`1)
                         by A5,XCMPLX_1:60
         .=((f/.(j))`1-p`1- ((f/.(j))`1-q`1))/((f/.(j))`1-p`1)
                         by XCMPLX_1:121
         .=(q`1-p`1)/((f/.(j))`1-p`1);
     A10: ((1-a)*(f/.(j))+a*p)`1=((1-a)*(f/.(j)))`1+(a*p)`1 by TOPREAL3:7
     .=(1-a)*(f/.(j))`1+ (a*p)`1 by TOPREAL3:9
     .=1* (f/.(j))`1 -a*(f/.(j))`1 +a*(p`1) by TOPREAL3:9
     .= (f/.(j))`1 -(a*((f/.(j))`1 -(p`1)))
     .= (f/.(j))`1 -((f/.(j))`1-q`1) by A5,XCMPLX_1:88
     .=q`1;
     A11: ((1-a)*(f/.(j))+a*p)`2=((1-a)*(f/.(j)))`2+(a*p)`2 by TOPREAL3:7
     .=(1-a)*(f/.(j))`2+ (a*p)`2 by TOPREAL3:9
     .=1* (f/.(j))`2 -a*(f/.(j))`2 +a*(p`2) by TOPREAL3:9
     .=q`2 by A1,A3,A8,GOBOARD7:6;
        (f/.j)`1<=q`1 & q`1<=(f/.(j+1))`1 by A1,A3,TOPREAL1:9;
     then A12: (f/.(j))`1-q`1<=0 by REAL_2:106;
        (1-a)>=0 by A5,A7,A9,REAL_2:125;
     then 1-a+a>=0+a by REAL_1:55;
     then A13: 0<=a & a<=1 by A5,A12,REAL_2:125;
        q=(1-a)*(f/.(j))+a*p by A10,A11,TOPREAL3:11;
     then q in { (1-lambda)*(f/.(j)) + lambda*p where lambda is Real :
          0 <= lambda & lambda <= 1 } by A13;
     then q in LSeg(f/.(j),p) by TOPREAL1:def 4;
     then LE q,p,L~f,f/.1,f/.(len f) by A1,SPRECT_3:40;
    hence contradiction by A1,A2,A6,JORDAN5C:12;
   end;
 hence p`1<=q`1;
 end;
 suppose p`1=(f/.(j))`1;
 hence p`1<=q`1 by A1,A3,TOPREAL1:9;
 end;
end;

definition
  let P be Subset of TOP-REAL 2, p1,p2,p be Point of TOP-REAL 2, e be Real;
  pred p is_Lin P,p1,p2,e means :Def1:
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    (ex p4 being Point of TOP-REAL 2 st p4`1<e & LE p4,p,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
    LE p4,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1<=e));
  pred p is_Rin P,p1,p2,e means :Def2:
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    (ex p4 being Point of TOP-REAL 2 st p4`1>e & LE p4,p,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
    LE p4,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e));
  pred p is_Lout P,p1,p2,e means :Def3:
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    (ex p4 being Point of TOP-REAL 2 st p4`1<e & LE p,p4,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
    LE p5,p4,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1<=e));
  pred p is_Rout P,p1,p2,e means :Def4:
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    (ex p4 being Point of TOP-REAL 2 st p4`1>e & LE p,p4,P,p1,p2 &
    (for p5 being Point of TOP-REAL 2 st
    LE p5,p4,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1>=e));
  pred p is_OSin P,p1,p2,e means :Def5: ::Oscilating In
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    ex p7 being Point of TOP-REAL 2 st LE p7,p,P,p1,p2 &
    (for p8 being Point of TOP-REAL 2 st LE p7,p8,P,p1,p2 & LE p8,p,P,p1,p2
    holds p8`1=e) &
    (for p4 being Point of TOP-REAL 2 st LE p4,p7,P,p1,p2 & p4<>p7 holds
    (ex p5 being Point of TOP-REAL 2 st
    LE p4,p5,P,p1,p2 & LE p5,p7,P,p1,p2 & p5`1>e) &
    (ex p6 being Point of TOP-REAL 2 st
    LE p4,p6,P,p1,p2 & LE p6,p7,P,p1,p2 & p6`1<e));
  pred p is_OSout P,p1,p2,e means :Def6: ::Oscilating Out
    P is_an_arc_of p1,p2 & p in P & p`1=e &
    ex p7 being Point of TOP-REAL 2 st LE p,p7,P,p1,p2 &
    (for p8 being Point of TOP-REAL 2 st LE p8,p7,P,p1,p2 & LE p,p8,P,p1,p2
    holds p8`1=e) &
    (for p4 being Point of TOP-REAL 2 st LE p7,p4,P,p1,p2 & p4<>p7 holds
    (ex p5 being Point of TOP-REAL 2 st
     LE p5,p4,P,p1,p2 & LE p7,p5,P,p1,p2 & p5`1>e)&
    (ex p6 being Point of TOP-REAL 2 st
     LE p6,p4,P,p1,p2 & LE p7,p6,P,p1,p2 & p6`1<e));
  correctness;
end;

theorem    for P being Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_an_arc_of p1,p2 & p1`1<=e & p2`1>=e
  holds ex p3 being Point of TOP-REAL 2 st p3 in P & p3`1=e
proof let P be Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume P is_an_arc_of p1,p2 & p1`1<=e & p2`1>=e;
   then P meets Vertical_Line(e) by JORDAN6:64;
   then A1: P /\ Vertical_Line(e) <> {} by XBOOLE_0:def 7;
   consider x being Element of P /\ Vertical_Line(e);
   A2: x in P & x in Vertical_Line(e) by A1,XBOOLE_0:def 3;
   then x in {p3 where p3 is Point of TOP-REAL 2: p3`1=e}
                       by JORDAN6:def 6;
   then consider p4 being Point of TOP-REAL 2 such that
   A3: p4=x & p4`1=e;
  thus thesis by A2,A3;
end;

reserve s,s1,r,t for Real;

theorem    for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_an_arc_of p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e
  or p is_OSin P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_an_arc_of p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
      now assume A2: not p is_OSin P,p1,p2,e;
       LE p1,p,P,p1,p2 by A1,JORDAN5C:10;
    then A3: p1 in P & p in P &
      for g being map of I[01], (TOP-REAL 2)|P,
       s1, s2 being Real st
       g is_homeomorphism
       & g.0 = p1 & g.1 = p2
       & g.s1 = p1 & 0 <= s1 & s1 <= 1
       & g.s2 = p & 0 <= s2 & s2 <= 1
       holds s1 <= s2 by JORDAN5C:def 3;
     consider f being map of I[01], (TOP-REAL 2)|P such that
     A4: f is_homeomorphism & f.0 = p1 & f.1 = p2 by A1,TOPREAL1:def 2;
     A5: f is continuous by A4,TOPS_2:def 5;
     A6: dom f=[#](I[01]) & rng f=[#]((TOP-REAL 2)|P) by A4,TOPS_2:def 5;
     then A7: dom f=[.0,1.] by BORSUK_1:83,PRE_TOPC:12;
     A8: rng f=P by A6,PRE_TOPC:def 10;
        p in rng f by A3,A6,PRE_TOPC:def 10;
     then consider xs being set such that
     A9: xs in dom f & p=f.xs by FUNCT_1:def 5;
     reconsider s2=xs as Real by A7,A9;
     A10: 0<=s2 & s2<=1 by A6,A9,BORSUK_1:83,TOPREAL5:1;
     reconsider pr1=proj1 as map of TOP-REAL 2,R^1 by JGRAPH_2:16;
     reconsider pro1=pr1|P as map of (TOP-REAL 2)|P,R^1 by JGRAPH_3:12;
        for p7 being Point of (TOP-REAL 2)|P holds
     pro1.p7=proj1.p7
     proof let p7 be Point of (TOP-REAL 2)|P;
          the carrier of ((TOP-REAL 2)|P)=P by JORDAN1:1;
      hence pro1.p7=proj1.p7 by FUNCT_1:72;
     end;
     then A11: pro1 is continuous by JGRAPH_2:39;
     reconsider h=pro1*f as map of I[01],R^1;
     A12: h is continuous by A5,A11,TOPS_2:58;
     A13: dom h=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
        for q being Point of TOP-REAL 2 st q=f.0 holds q`1<>e by A1,A4;
     then A14: 0 in
     {s where s is Real: 0<=s & s<=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)} by A10;
        {s where s is Real: 0<=s & s<=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)} c= REAL
     proof let x be set;assume x in
       {s where s is Real: 0<=s & s<=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)};
       then consider s being Real such that
       A15: s=x & 0<=s & s<=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e);
      thus x in REAL by A15;
     end;
     then reconsider R={s where s is Real: 0<=s & s<=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)}
        as non empty Subset of REAL by A14;
     reconsider s0=sup R as Real;
        R c= [.0,1.]
     proof let x be set;assume x in R;
       then consider s being Real such that
       A16: s=x & 0<=s & s<=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e);
          s<=1 by A10,A16,AXIOMS:22;
      hence x in [.0,1.] by A16,TOPREAL5:1;
     end;
     then reconsider R''=R as Subset of I[01] by BORSUK_1:83;
        R''=R;
     then A17: 0 <= s0 by A14,BORSUK_4:51;
     A18: for s being real number st s in R holds s < s2
     proof let s be real number;assume s in R;
       then consider s3 being Real such that
       A19: s3=s & 0<=s3 & s3<=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s3 holds q`1<>e);
         s<>s2 by A1,A9,A19;
      hence thesis by A19,REAL_1:def 5;
     end;
     then for s being real number st s in R holds s <= s2;
     then A20: s0 <= s2 by PSCOMP_1:10;
     then A21: s0 <=1 by A10,AXIOMS:22;
     then s0 in dom f by A7,A17,TOPREAL5:1;
     then f.s0 in rng f by FUNCT_1:def 5;
     then f.s0 in P by A6,PRE_TOPC:def 10;
     then reconsider p9=f.s0 as Point of TOP-REAL 2;
        for s being real number st s in R holds s <= s2
     by A18;
     then A22: R is bounded_above by SEQ_4:def 1;
     A23: LE p9,p,P,p1,p2 by A1,A4,A9,A10,A17,A20,A21,JORDAN5C:8;
     A24: for p8 being Point of TOP-REAL 2 st
       LE p9,p8,P,p1,p2 & LE p8,p,P,p1,p2
       holds p8`1=e
       proof let p8 be Point of TOP-REAL 2;
        assume A25: LE p9,p8,P,p1,p2 & LE p8,p,P,p1,p2;
         then A26: p8 in P by JORDAN5C:def 3;
         then consider x8 being set such that
         A27: x8 in dom f & p8=f.x8 by A8,FUNCT_1:def 5;
         reconsider s8=x8 as Real by A7,A27;
         A28: 0<=s8 & s8<=1 & f.s8=p8 by A6,A27,BORSUK_1:83,TOPREAL5:1;
         then A29: s0 <=s8 by A4,A17,A21,A25,JORDAN5C:def 3;
         A30: s8<=s2
           by A4,A9,A10,A25,A28,JORDAN5C:def 3;
            now assume A31: p8`1<>e;
           then A32: s8<s2 by A1,A9,A27,A30,REAL_1:def 5;
           reconsider ee=abs(p8`1-e)/2 as Real by XREAL_0:def 1;
              p8`1-e<>0 by A31,XCMPLX_1:15;
           then abs(p8`1-e)>0 by ABSVALUE:6;
           then A33: ee>0 by REAL_2:127;
           reconsider w=p8`1 as Element of the carrier of RealSpace
                               by METRIC_1:def 14;
           reconsider B=Ball(w,ee) as Subset of the carrier of R^1
                                   by METRIC_1:def 14,TOPMETR:24;
           A34: w in Ball(w,ee) by A33,GOBOARD6:4;
           A35: B={s7 where s7 is Real:p8`1-ee<s7 & s7<p8`1+ee}
                  by A33,JORDAN2B:21
            .= ].p8`1-ee,p8`1+ee.[ by RCOMP_1:def 2;
           A36: h"B is open by A12,TOPMETR:def 7,UNIFORM1:3;
           A37: the carrier of Closed-Interval-MSpace(0,1)=[.0,1.]
                              by TOPMETR:14;
           reconsider s8m=s8 as Point of Closed-Interval-MSpace(0,1)
                              by A6,A27,BORSUK_1:83,TOPMETR:14;
           reconsider s8n=s8 as Point of RealSpace by METRIC_1:def 14;
           A38: I[01]=TopSpaceMetr(Closed-Interval-MSpace(0,1))
                             by TOPMETR:27,def 8;
             h.s8=pro1.(f.s8) by A6,A13,A27,BORSUK_1:83,FUNCT_1:22
              .=proj1.p8 by A26,A27,FUNCT_1:72
              .=p8`1 by PSCOMP_1:def 28;
           then s8 in h"B by A6,A13,A27,A34,BORSUK_1:83,FUNCT_1:def 13;
           then consider r0 being real number such that
           A39: r0>0 & Ball(s8m,r0) c= h"B by A36,A38,TOPMETR:22;
           reconsider r0 as Real by XREAL_0:def 1;
           reconsider r01=min(s2-s8,r0) as Real by XREAL_0:def 1;
              s2-s8>0 by A32,SQUARE_1:11;
           then A40: r01>0 by A39,JGRAPH_3:6;
           then Ball(s8n,r01)= (].s8-r01,s8+r01 .[) by FRECHET:7;
           then A41: Ball(s8m,r01)= (].s8-r01,s8+r01 .[) /\ [.0,1.]
                                by A37,TOPMETR:13;
              s2-s8>=r01 by SQUARE_1:35;
           then A42: s2-s8+s8>=r01+s8 by REAL_1:55;
              r01<=r0 by SQUARE_1:35;
           then A43: Ball(s8m,r01) c= Ball(s8m,r0) by PCOMPS_1:1;
           reconsider Q=[.0,1.] as non empty Subset of the carrier of
           RealSpace by METRIC_1:def 14,TOPREAL5:1;
           A44: s8+r01<=1 by A10,A42,AXIOMS:22;
           A45: (].s8-r01,s8+r01 .[)/\ [.0,1.] c= h"B
                          by A39,A41,A43,XBOOLE_1:1;
            r01-r01/2>0 by A40,REAL_2:127;
           then A46: r01-r01/2+r01/2>0+r01/2 by REAL_1:53;
           then A47: s8+r01/2<s8+r01 by REAL_1:53;
           A48: r01/2>0 by A40,REAL_2:127;
           then 0+0<r01/2+r01 by A40,REAL_1:67;
           then s8+0<s8+(r01/2+r01) by REAL_1:53;
           then s8-r01+r01<s8+r01/2+r01;
           then s8-r01<s8+r01/2 & s8+r01/2<s8+r01 by A46,REAL_1:53,55;
           then A49: s8+r01/2 in ].s8-r01,s8+r01 .[ by JORDAN6:45;
           A50: 0+0<s8+r01/2 by A28,A48,REAL_1:67;
              s8+r01/2<1 by A44,A47,AXIOMS:22;
           then s8+r01/2 in [.0,1.] by A50,TOPREAL5:1;
           then s8+r01/2 in (].s8-r01,s8+r01 .[)/\ [.0,1.]
                                  by A49,XBOOLE_0:def 3;
           then A51: s8+r01/2 in dom h & h.(s8+r01/2) in B by A45,FUNCT_1:def
13;
           reconsider s70=s8+r01/2 as Real by XREAL_0:def 1;
              s8<s8+r01/2 by A48,REAL_1:69;
           then A52: s8<s70 & 0<=s70 & s70<=s2
                     by A28,A42,A47,AXIOMS:22;
              for p7 being Point of TOP-REAL 2 st p7=f.s70
               holds p7`1<>e
              proof let p7 be Point of TOP-REAL 2;
               assume A53: p7=f.s70;
                   s70<=1 by A10,A52,AXIOMS:22;
                then s70 in [.0,1.] by A52,TOPREAL5:1;
                then A54: p7 in rng f by A7,A53,FUNCT_1:def 5;
                A55: rng f=[#]((TOP-REAL 2)|P) by A4,TOPS_2:def 5
                    .=P by PRE_TOPC:def 10;
                   h.s70=pro1.(f.s70) by A51,FUNCT_1:22
                 .=pr1.(p7) by A53,A54,A55,FUNCT_1:72
                 .=p7`1 by PSCOMP_1:def 28;
                then A56: p8`1-ee<p7`1 & p7`1<p8`1+ee by A35,A51,JORDAN6:45;
                   now assume A57: p7`1=e;
                     now per cases;
                  case A58: p8`1-e>=0;
                    then p8`1-(p8`1-e)/2<e & e< p8`1+(p8`1-e)/2
                                    by A56,A57,ABSVALUE:def 1;
                    then p8`1/2+e/2<e/2+e/2;
                    then p8`1/2<e/2 by REAL_1:55;
                    then A59: p8`1/2-e/2<e/2-e/2 by REAL_1:92;
                       (p8`1-e)/2>=0/2 by A58,REAL_1:73;
                   hence contradiction by A59;
                  end;
                  case A60: p8`1-e<0;
                    then p8`1-(-(p8`1-e))/2<e & e< p8`1+(-(p8`1-e))/2
                                    by A56,A57,ABSVALUE:def 1;
                    then p8`1/2+e/2>e/2+e/2;
                    then p8`1/2>e/2 by REAL_1:55;
                    then A61: p8`1/2-e/2>e/2-e/2 by REAL_1:92;
                       (p8`1-e)/2<=0/2 by A60,REAL_1:73;
                   hence contradiction by A61;
                  end;
                  end;
                 hence contradiction;
                end;
               hence p7`1<>e;
              end;
           then consider s7 being Real such that
           A62: s8<s7 & 0<=s7 & s7<=s2 &
           (for p7 being Point of TOP-REAL 2 st p7=f.s7 holds p7`1<>e)
                                by A52;
              s7 in R by A62;
           then s7 <= s0 by A22,SEQ_4:def 4;
          hence contradiction by A29,A62,AXIOMS:22;
         end;
        hence p8`1=e;
       end;
    then consider p4 being Point of TOP-REAL 2 such that
    A63: LE p4,p9,P,p1,p2 & p4<>p9 &
     ((for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,p9,P,p1,p2
        holds p5`1<=e)or
     (for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p9,P,p1,p2
        holds p6`1>=e)) by A1,A2,A23,Def5;
     A64: p9 in P by A63,JORDAN5C:def 3;
        now per cases by A63;
     case A65: (for p5 being Point of TOP-REAL 2 st
        LE p4,p5,P,p1,p2 & LE p5,p9,P,p1,p2
        holds p5`1<=e);
      A66: now assume ex p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2 & p51`1<e;
        then consider p51 being Point of TOP-REAL 2 such that
        A67: LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2 & p51`1<e;
        A68: LE p51,p,P,p1,p2 by A23,A67,JORDAN5C:13;
          (for p5 being Point of TOP-REAL 2 st
        LE p51,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1<=e)
        proof let p5 be Point of TOP-REAL 2;
          assume A69: LE p51,p5,P,p1,p2 & LE p5,p,P,p1,p2;
          then A70: LE p4,p5,P,p1,p2 by A67,JORDAN5C:13;
          A71: p5 in P by A69,JORDAN5C:def 3;
          then A72: p5=p9 implies LE p9,p5,P,p1,p2 by JORDAN5C:9;
             now per cases by A1,A64,A71,A72,JORDAN5C:14;
          case LE p5,p9,P,p1,p2;
           hence p5`1<=e by A65,A70;
          end;
          case LE p9,p5,P,p1,p2;
           hence p5`1<=e by A24,A69;
          end;
          end;
         hence p5`1<=e;
        end;
       hence p is_Lin P,p1,p2,e by A1,A67,A68,Def1;
      end;
          now assume A73: not ex p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2 & p51`1<e;
        A74: for p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2
        holds p51`1=e
        proof let p51 be Point of TOP-REAL 2;
          assume A75: LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2;
          then A76: p51`1>=e by A73;
             p51`1<=e by A65,A75;
         hence p51`1=e by A76,AXIOMS:21;
        end;
           p4 in P by A63,JORDAN5C:def 3;
        then p4 in rng f by A6,PRE_TOPC:def 10;
        then consider xs4 being set such that
        A77: xs4 in dom f & p4=f.xs4 by FUNCT_1:def 5;
        reconsider s4=xs4 as Real by A7,A77;
        A78: 0<=s4 & s4<=1 by A6,A77,BORSUK_1:83,TOPREAL5:1;
        then A79: s4<= s0 by A4,A17,A21,A63,A77,JORDAN5C:def 3;
           now assume s4< s0;
         then s0-s4>0 by SQUARE_1:11;
         then A80: (s0-s4)/2>0 by REAL_2:127;
         then A81: s4<s4+(s0-s4)/2 by REAL_1:69;
          consider r being real number such that
          A82: r in R & s0-((s0-s4)/2)<r by A22,A80,SEQ_4:def 4;
          A83: r<=s0 by A22,A82,SEQ_4:def 4;
          A84: s4<r by A81,A82,AXIOMS:22;
          A85: 0<r by A78,A81,A82,AXIOMS:22;
          consider s7 being Real such that
          A86: s7=r & 0<=s7 & s7<=s2 &
          (for q being Point of TOP-REAL 2 st q=f.s7 holds q`1<>e) by A82;
          A87: r<=1 by A10,A86,AXIOMS:22;
          then A88: r in [.0,1.] by A85,TOPREAL5:1;
          reconsider rss=r as Real by XREAL_0:def 1;
             f.rss in rng f by A7,A88,FUNCT_1:def 5;
          then f.rss in P by A6,PRE_TOPC:def 10;
          then reconsider pss=f.rss as Point of TOP-REAL 2;
             LE p4,pss,P,p1,p2 & LE pss,p9,P,p1,p2
               by A1,A4,A17,A21,A77,A78,A83,A84,A85,A87,JORDAN5C:8;
          then pss`1=e by A74;
         hence contradiction by A86;
        end;
        hence contradiction by A63,A77,A79,AXIOMS:21;
       end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A66;
     end;
     case A89: (for p6 being Point of TOP-REAL 2 st
        LE p4,p6,P,p1,p2 & LE p6,p9,P,p1,p2
        holds p6`1>=e);
      A90: now assume ex p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2 & p51`1>e;
        then consider p51 being Point of TOP-REAL 2 such that
        A91: LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2 & p51`1>e;
        A92: LE p51,p,P,p1,p2 by A23,A91,JORDAN5C:13;
          (for p5 being Point of TOP-REAL 2 st
        LE p51,p5,P,p1,p2 & LE p5,p,P,p1,p2 holds p5`1>=e)
        proof let p5 be Point of TOP-REAL 2;
          assume A93: LE p51,p5,P,p1,p2 & LE p5,p,P,p1,p2;
          then A94: LE p4,p5,P,p1,p2 by A91,JORDAN5C:13;
          A95: p5 in P by A93,JORDAN5C:def 3;
          then A96: p5=p9 implies LE p9,p5,P,p1,p2 by JORDAN5C:9;
             now per cases by A1,A64,A95,A96,JORDAN5C:14;
          case LE p5,p9,P,p1,p2;
           hence p5`1>=e by A89,A94;
          end;
          case LE p9,p5,P,p1,p2;
           hence p5`1>=e by A24,A93;
          end;
          end;
         hence p5`1>=e;
        end;
       hence p is_Rin P,p1,p2,e by A1,A91,A92,Def2;
      end;
          now assume A97: not ex p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2 & p51`1>e;
        A98: for p51 being Point of TOP-REAL 2 st
        LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2
        holds p51`1=e
        proof let p51 be Point of TOP-REAL 2;
          assume A99: LE p4,p51,P,p1,p2 & LE p51,p9,P,p1,p2;
          then A100: p51`1<=e by A97;
             p51`1>=e by A89,A99;
         hence p51`1=e by A100,AXIOMS:21;
        end;
           p4 in P by A63,JORDAN5C:def 3;
        then p4 in rng f by A6,PRE_TOPC:def 10;
        then consider xs4 being set such that
        A101: xs4 in dom f & p4=f.xs4 by FUNCT_1:def 5;
        reconsider s4=xs4 as Real by A7,A101;
        A102: 0<=s4 & s4<=1 by A6,A101,BORSUK_1:83,TOPREAL5:1;
        then A103: s4<=s0 by A4,A17,A21,A63,A101,JORDAN5C:def 3;
           now assume s4<s0;
         then s0-s4>0 by SQUARE_1:11;
         then A104: (s0-s4)/2>0 by REAL_2:127;
         then A105: s4<s4+(s0-s4)/2 by REAL_1:69;
          consider r being real number such that
          A106: r in R & s0-((s0-s4)/2)<r by A22,A104,SEQ_4:def 4;
          A107: r<=s0 by A22,A106,SEQ_4:def 4;
          A108: s4<r by A105,A106,AXIOMS:22;
          A109: 0<r by A102,A105,A106,AXIOMS:22;
          consider s7 being Real such that
          A110: s7=r & 0<=s7 & s7<=s2 &
          (for q being Point of TOP-REAL 2 st q=f.s7 holds q`1<>e)
                          by A106;
          A111: r<=1 by A10,A110,AXIOMS:22;
          then A112: r in [.0,1.] by A109,TOPREAL5:1;
          reconsider rss=r as Real by XREAL_0:def 1;
             f.rss in rng f by A7,A112,FUNCT_1:def 5;
          then f.rss in P by A6,PRE_TOPC:def 10;
          then reconsider pss=f.rss as Point of TOP-REAL 2;
             LE p4,pss,P,p1,p2 & LE pss,p9,P,p1,p2
               by A1,A4,A17,A21,A101,A102,A107,A108,A109,A111,JORDAN5C:8;
          then pss`1=e by A98;
         hence contradiction by A110;
        end;
        hence contradiction by A63,A101,A103,AXIOMS:21;
       end;
      hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e by A90;
     end;
     end;
    hence p is_Lin P,p1,p2,e or p is_Rin P,p1,p2,e;
   end;
  hence thesis;
end;

theorem    for P being non empty Subset of TOP-REAL 2,
  p1,p2,p being Point of TOP-REAL 2,e being Real st
  P is_an_arc_of p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e
  holds p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
  or p is_OSout P,p1,p2,e
proof let P be non empty Subset of TOP-REAL 2,
  p1,p2,p be Point of TOP-REAL 2,e be Real;
  assume A1: P is_an_arc_of p1,p2 & p1`1<e & p2`1>e & p in P & p`1=e;
      now assume A2: not p is_OSout P,p1,p2,e;
       LE p1,p,P,p1,p2 by A1,JORDAN5C:10;
    then A3: p1 in P & p in P &
      for g being map of I[01], (TOP-REAL 2)|P,
       s1, s2 being Real st
       g is_homeomorphism
       & g.0 = p1 & g.1 = p2
       & g.s1 = p1 & 0 <= s1 & s1 <= 1
       & g.s2 = p & 0 <= s2 & s2 <= 1
       holds s1 <= s2 by JORDAN5C:def 3;
     consider f being map of I[01], (TOP-REAL 2)|P such that
     A4: f is_homeomorphism & f.0 = p1 & f.1 = p2 by A1,TOPREAL1:def 2;
     A5: f is continuous by A4,TOPS_2:def 5;
     A6: dom f=[#](I[01]) & rng f=[#]((TOP-REAL 2)|P) by A4,TOPS_2:def 5;
     then A7: dom f=[.0,1.] by BORSUK_1:83,PRE_TOPC:12;
     A8: rng f=P by A6,PRE_TOPC:def 10;
        p in rng f by A3,A6,PRE_TOPC:def 10;
     then consider xs being set such that
     A9: xs in dom f & p=f.xs by FUNCT_1:def 5;
     reconsider s2=xs as Real by A7,A9;
     A10: 0<=s2 & s2<=1 by A6,A9,BORSUK_1:83,TOPREAL5:1;
     reconsider pr1=proj1 as map of TOP-REAL 2,R^1 by JGRAPH_2:16;
     reconsider pro1=pr1|P as map of (TOP-REAL 2)|P,R^1 by JGRAPH_3:12;
        for p7 being Point of (TOP-REAL 2)|P holds
     pro1.p7=proj1.p7
     proof let p7 be Point of (TOP-REAL 2)|P;
          the carrier of ((TOP-REAL 2)|P)=P by JORDAN1:1;
      hence pro1.p7=proj1.p7 by FUNCT_1:72;
     end;
     then A11: pro1 is continuous by JGRAPH_2:39;
     reconsider h=pro1*f as map of I[01],R^1;
     A12: h is continuous by A5,A11,TOPS_2:58;
     A13: dom h=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
        (for q being Point of TOP-REAL 2 st q=f.1 holds q`1<>e) by A1,A4;
     then A14: 1 in
     {s where s is Real: 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)} by A10;
        {s where s is Real: 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)} c= REAL
     proof let x be set;assume x in
       {s where s is Real: 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)};
       then consider s being Real such that
       A15: s=x & 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e);
      thus x in REAL by A15;
     end;
     then reconsider R={s where s is Real: 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e)}
        as non empty Subset of REAL by A14;
     reconsider s0=inf R as Real;
       R c= [.0,1.]
     proof let x be set;assume x in R;
       then consider s being Real such that
       A16: s=x & 1>=s & s>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s holds q`1<>e);
      thus x in [.0,1.] by A10,A16,TOPREAL5:1;
     end;
     then A17: 1 >= s0 by A14,BORSUK_1:83,BORSUK_4:51;
     A18: for s being real number st s in R holds s > s2
     proof let s be real number;assume s in R;
       then consider s3 being Real such that
       A19: s3=s & 1>=s3 & s3>=s2 &
       (for q being Point of TOP-REAL 2 st q=f.s3 holds q`1<>e);
         s<>s2 by A1,A9,A19;
      hence thesis by A19,REAL_1:def 5;
     end;
     then for s being real number st s in R holds s >= s2;
     then A20: s0 >= s2 by PSCOMP_1:8;
      then s0 in dom f by A7,A10,A17,TOPREAL5:1;
     then f.s0 in rng f by FUNCT_1:def 5;
     then f.s0 in P by A6,PRE_TOPC:def 10;
     then reconsider p9=f.s0 as Point of TOP-REAL 2;
        for s being real number st s in R holds s >= s2 by A18;
     then A21: R is bounded_below by SEQ_4:def 2;
     A22: LE p,p9,P,p1,p2 by A1,A4,A9,A10,A17,A20,JORDAN5C:8;
     A23: for p8 being Point of TOP-REAL 2 st
       LE p8,p9,P,p1,p2 & LE p,p8,P,p1,p2
       holds p8`1=e
       proof let p8 be Point of TOP-REAL 2;
        assume A24: LE p8,p9,P,p1,p2 & LE p,p8,P,p1,p2;
         then A25: p8 in P by JORDAN5C:def 3;
         then consider x8 being set such that
         A26: x8 in dom f & p8=f.x8 by A8,FUNCT_1:def 5;
         reconsider s8=x8 as Real by A7,A26;
         A27: 0<=s8 & s8<=1 & f.s8=p8 by A6,A26,BORSUK_1:83,TOPREAL5:1;
         then A28: s0>=s8 by A4,A10,A17,A20,A24,JORDAN5C:def 3;
         A29: s8>=s2
           by A4,A9,A10,A24,A27,JORDAN5C:def 3;
            now assume A30: p8`1<>e;
           then A31: s8>s2 by A1,A9,A26,A29,REAL_1:def 5;
           reconsider ee=abs(p8`1-e)/2 as Real by XREAL_0:def 1;
              p8`1-e<>0 by A30,XCMPLX_1:15;
           then abs(p8`1-e)>0 by ABSVALUE:6;
           then A32: ee>0 by REAL_2:127;
           reconsider w=p8`1 as Element of the carrier of RealSpace
                               by METRIC_1:def 14;
           reconsider B=Ball(w,ee) as Subset of the carrier of R^1
                                   by METRIC_1:def 14,TOPMETR:24;
           A33: w in Ball(w,ee) by A32,GOBOARD6:4;
           A34: B={s7 where s7 is Real:p8`1-ee<s7 & s7<p8`1+ee}
                  by A32,JORDAN2B:21
            .= ].p8`1-ee,p8`1+ee.[ by RCOMP_1:def 2;
           A35: h"B is open by A12,TOPMETR:def 7,UNIFORM1:3;
           A36: the carrier of Closed-Interval-MSpace(0,1)=[.0,1.]
                              by TOPMETR:14;
           reconsider s8m=s8 as Point of Closed-Interval-MSpace(0,1)
                              by A6,A26,BORSUK_1:83,TOPMETR:14;
           reconsider s8n=s8 as Point of RealSpace by METRIC_1:def 14;
           A37: I[01]=TopSpaceMetr(Closed-Interval-MSpace(0,1))
                             by TOPMETR:27,def 8;
             h.s8=pro1.(f.s8) by A6,A13,A26,BORSUK_1:83,FUNCT_1:22
              .=proj1.p8 by A25,A26,FUNCT_1:72
              .=p8`1 by PSCOMP_1:def 28;
           then s8 in h"B by A6,A13,A26,A33,BORSUK_1:83,FUNCT_1:def 13;
           then consider r0 being real number such that
           A38: r0>0 & Ball(s8m,r0) c= h"B by A35,A37,TOPMETR:22;
           reconsider r0 as Real by XREAL_0:def 1;
           reconsider r01=min(s8-s2,r0) as Real by XREAL_0:def 1;
              s8-s2>0 by A31,SQUARE_1:11;
           then A39: r01>0 by A38,JGRAPH_3:6;
           then Ball(s8n,r01)= (].s8-r01,s8+r01 .[) by FRECHET:7;
           then A40: Ball(s8m,r01)= (].s8-r01,s8+r01 .[) /\ [.0,1.]
                                by A36,TOPMETR:13;
              s8-s2>=r01 by SQUARE_1:35;
           then -(s8-s2)<= -r01 by REAL_1:50;
           then A41: s2-s8+s8<= -r01+s8 by REAL_1:55;
              r01<=r0 by SQUARE_1:35;
           then A42: Ball(s8m,r01) c= Ball(s8m,r0) by PCOMPS_1:1;
           reconsider Q=[.0,1.] as non empty Subset of the carrier of
           RealSpace by METRIC_1:def 14,TOPREAL5:1;
           A43: (].s8-r01,s8+r01 .[)/\ [.0,1.] c= h"B
                          by A38,A40,A42,XBOOLE_1:1;
            r01-r01/2>0 by A39,REAL_2:127;
           then A44: r01-r01/2+r01/2>0+r01/2 by REAL_1:53;
           then A45: s8-r01/2>s8-r01 by REAL_2:105;
           A46: r01/2>0 by A39,REAL_2:127;
              --(r01/2)> 0 by A39,REAL_2:127;
           then A47: -(r01/2) < 0 by REAL_1:66;
           A48: s8+r01/2<s8+r01 by A44,REAL_1:67;
              -(r01/2)<r01/2 by A46,A47,AXIOMS:22;
           then s8+-(r01/2)<s8+r01/2 by REAL_1:67;
           then A49: s8-r01<s8-r01/2 & s8-r01/2<s8+r01 by A44,A48,AXIOMS:22,
REAL_2:105;
           then A50: s8-r01/2 in ].s8-r01,s8+r01 .[ by JORDAN6:45;
           A51: 1-0>s8-r01/2 by A27,A46,REAL_1:92;
              s8-r01/2>0 by A10,A41,A45;
           then s8-r01/2 in [.0,1.] by A51,TOPREAL5:1;
           then s8-r01/2 in (].s8-r01,s8+r01 .[)/\ [.0,1.]
                                  by A50,XBOOLE_0:def 3;
           then A52: s8-r01/2 in dom h & h.(s8-r01/2) in B by A43,FUNCT_1:def
13;
           reconsider s70=s8-r01/2 as Real by XREAL_0:def 1;
              s8+0>s8+-r01/2 by A47,REAL_1:67;
           then A53: s8>s70 & 1>=s70 & s70>=s2
             by A27,A41,A49,AXIOMS:22;
              for p7 being Point of TOP-REAL 2 st p7=f.s70
               holds p7`1<>e
              proof let p7 be Point of TOP-REAL 2;
               assume A54: p7=f.s70;
                   s70 in [.0,1.] by A10,A53,TOPREAL5:1;
                then A55: p7 in rng f by A7,A54,FUNCT_1:def 5;
                A56: rng f=[#]((TOP-REAL 2)|P) by A4,TOPS_2:def 5
                    .=P by PRE_TOPC:def 10;
                   h.s70=pro1.(f.s70) by A52,FUNCT_1:22
                 .=pr1.(p7) by A54,A55,A56,FUNCT_1:72
                 .=p7`1 by PSCOMP_1:def 28;
                then A57: p8`1-ee<p7`1 & p7`1<p8`1+ee by A34,A52,JORDAN6:45;
                   now assume A58: p7`1=e;
                     now per cases;
                  case A59: p8`1-e>=0;
                    then p8`1-(p8`1-e)/2<e & e< p8`1+(p8`1-e)/2
                                    by A57,A58,ABSVALUE:def 1;
                    then p8`1/2+e/2<e/2+e/2;
                    then p8`1/2<e/2 by REAL_1:55;
                    then A60: p8`1/2-e/2<e/2-e/2 by REAL_1:92;
                       (p8`1-e)/2>=0/2 by A59,REAL_1:73;
                   hence contradiction by A60;
                  end;
                  case A61: p8`1-e<0;
                    then p8`1-(-(p8`1-e))/2<e & e< p8`1+(-(p8`1-e))/2
                                    by A57,A58,ABSVALUE:def 1;
                    then p8`1/2+e/2>e/2+e/2;
                    then p8`1/2>e/2 by REAL_1:55;
                    then A62: p8`1/2-e/2>e/2-e/2 by REAL_1:92;
                       (p8`1-e)/2<=0/2 by A61,REAL_1:73;
                   hence contradiction by A62;
                  end;
                  end;
                 hence contradiction;
                end;
               hence p7`1<>e;
              end;
           then consider s7 being Real such that
           A63: s8>s7 & 1>=s7 & s7>=s2 &
           (for p7 being Point of TOP-REAL 2 st p7=f.s7 holds p7`1<>e)
                                by A53;
              s7 in R by A63;
           then s7 >=s0 by A21,SEQ_4:def 5;
          hence contradiction by A28,A63,AXIOMS:22;
         end;
        hence p8`1=e;
       end;
    then consider p4 being Point of TOP-REAL 2 such that
    A64: LE p9,p4,P,p1,p2 & p4<>p9 &
     ((for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE p9,p5,P,p1,p2
        holds p5`1<=e)or
     (for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p9,p6,P,p1,p2
        holds p6`1>=e)) by A1,A2,A22,Def6;
     A65: p9 in P by A64,JORDAN5C:def 3;
        now per cases by A64;
     case A66: (for p5 being Point of TOP-REAL 2 st
        LE p5,p4,P,p1,p2 & LE p9,p5,P,p1,p2
        holds p5`1<=e);
      A67: now assume ex p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2 & p51`1<e;
        then consider p51 being Point of TOP-REAL 2 such that
        A68: LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2 & p51`1<e;
        A69: LE p,p51,P,p1,p2 by A22,A68,JORDAN5C:13;
          (for p5 being Point of TOP-REAL 2 st
        LE p5,p51,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1<=e)
        proof let p5 be Point of TOP-REAL 2;
          assume A70: LE p5,p51,P,p1,p2 & LE p,p5,P,p1,p2;
          then A71: LE p5,p4,P,p1,p2 by A68,JORDAN5C:13;
          A72: p5 in P by A70,JORDAN5C:def 3;
          then A73: p5=p9 implies LE p9,p5,P,p1,p2 by JORDAN5C:9;
             now per cases by A1,A65,A72,A73,JORDAN5C:14;
          case LE p5,p9,P,p1,p2;
           hence p5`1<=e by A23,A70;
          end;
          case LE p9,p5,P,p1,p2;
           hence p5`1<=e by A66,A71;
          end;
          end;
         hence p5`1<=e;
        end;
       hence p is_Lout P,p1,p2,e by A1,A68,A69,Def3;
      end;
          now assume A74: not ex p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2 & p51`1<e;
        A75: for p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2
        holds p51`1=e
        proof let p51 be Point of TOP-REAL 2;
          assume A76: LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2;
          then A77: p51`1>=e by A74;
             p51`1<=e by A66,A76;
         hence p51`1=e by A77,AXIOMS:21;
        end;
           p4 in P by A64,JORDAN5C:def 3;
        then p4 in rng f by A6,PRE_TOPC:def 10;
        then consider xs4 being set such that
        A78: xs4 in dom f & p4=f.xs4 by FUNCT_1:def 5;
        reconsider s4=xs4 as Real by A7,A78;
        A79: 0<=s4 & s4<=1 by A6,A78,BORSUK_1:83,TOPREAL5:1;
        then A80: s4>=s0 by A4,A10,A17,A20,A64,A78,JORDAN5C:def 3;
           now assume s4>s0;
         then --(s4-s0)>0 by SQUARE_1:11;
         then -(s4-s0)<0 by REAL_1:66;
         then A81: (s0-s4)/2<0 by REAL_2:128;
         then A82: -((s0-s4)/2)>0 by REAL_1:66;
         A83: s4+0>s4+(s0-s4)/2 by A81,REAL_1:67;
          consider r being real number such that
          A84: r in R & r<s0+(-((s0-s4)/2)) by A21,A82,SEQ_4:def 5;
          A85: r>=s0 by A21,A84,SEQ_4:def 5;
          A86: s4>r by A83,A84,AXIOMS:22;
          then A87: 1>r by A79,AXIOMS:22;
          consider s7 being Real such that
          A88: s7=r & 1>=s7 & s7>=s2 &
          (for q being Point of TOP-REAL 2 st q=f.s7 holds q`1<>e) by A84;
          A89: r in [.0,1.] by A10,A88,TOPREAL5:1;
          reconsider rss=r as Real by XREAL_0:def 1;
             f.rss in rng f by A7,A89,FUNCT_1:def 5;
          then f.rss in P by A6,PRE_TOPC:def 10;
          then reconsider pss=f.rss as Point of TOP-REAL 2;
             LE pss,p4,P,p1,p2 & LE p9,pss,P,p1,p2
               by A1,A4,A10,A17,A20,A78,A79,A85,A86,A87,JORDAN5C:8;
          then pss`1=e by A75;
         hence contradiction by A88;
        end;
        hence contradiction by A64,A78,A80,AXIOMS:21;
       end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e by A67;
     end;
     case A90: (for p6 being Point of TOP-REAL 2 st
        LE p6,p4,P,p1,p2 & LE p9,p6,P,p1,p2
        holds p6`1>=e);
      A91: now assume ex p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2 & p51`1>e;
        then consider p51 being Point of TOP-REAL 2 such that
        A92: LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2 & p51`1>e;
        A93: LE p,p51,P,p1,p2 by A22,A92,JORDAN5C:13;
          (for p5 being Point of TOP-REAL 2 st
        LE p5,p51,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1>=e)
        proof let p5 be Point of TOP-REAL 2;
          assume A94: LE p5,p51,P,p1,p2 & LE p,p5,P,p1,p2;
          then A95: LE p5,p4,P,p1,p2 by A92,JORDAN5C:13;
          A96: p5 in P by A94,JORDAN5C:def 3;
          then A97: p5=p9 implies LE p9,p5,P,p1,p2 by JORDAN5C:9;
             now per cases by A1,A65,A96,A97,JORDAN5C:14;
          case LE p9,p5,P,p1,p2;
           hence p5`1>=e by A90,A95;
          end;
          case LE p5,p9,P,p1,p2;
           hence p5`1>=e by A23,A94;
          end;
          end;
         hence p5`1>=e;
        end;
       hence p is_Rout P,p1,p2,e by A1,A92,A93,Def4;
      end;
          now assume A98: not ex p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2 & p51`1>e;
        A99: for p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2
        holds p51`1=e
        proof let p51 be Point of TOP-REAL 2;
          assume A100: LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2;
          then A101: p51`1<=e by A98;
             p51`1>=e by A90,A100;
         hence p51`1=e by A101,AXIOMS:21;
        end;
           p4 in P by A64,JORDAN5C:def 3;
        then p4 in rng f by A6,PRE_TOPC:def 10;
        then consider xs4 being set such that
        A102: xs4 in dom f & p4=f.xs4 by FUNCT_1:def 5;
        reconsider s4=xs4 as Real by A7,A102;
        A103: 0<=s4 & s4<=1 by A6,A102,BORSUK_1:83,TOPREAL5:1;
        then A104: s4>=s0 by A4,A10,A17,A20,A64,A102,JORDAN5C:def 3;
           now assume s4>s0;
         then --(s4-s0)>0 by SQUARE_1:11;
         then -(s4-s0)<0 by REAL_1:66;
         then A105: (s0-s4)/2<0 by REAL_2:128;
         then A106: -((s0-s4)/2)>0 by REAL_1:66;
         A107: s4+0>s4+(s0-s4)/2 by A105,REAL_1:67;
          consider r being real number such that
          A108: r in R & r<s0+(-((s0-s4)/2)) by A21,A106,SEQ_4:def 5;
          A109: r>=s0 by A21,A108,SEQ_4:def 5;
          A110: s4>r by A107,A108,AXIOMS:22;
          then A111: 1>r by A103,AXIOMS:22;
          consider s7 being Real such that
          A112: s7=r & 1>=s7 & s7>=s2 &
          (for q being Point of TOP-REAL 2 st q=f.s7 holds q`1<>e) by A108;
          A113: r in [.0,1.] by A10,A112,TOPREAL5:1;
          reconsider rss=r as Real by XREAL_0:def 1;
             f.rss in rng f by A7,A113,FUNCT_1:def 5;
          then f.rss in P by A6,PRE_TOPC:def 10;
          then reconsider pss=f.rss as Point of TOP-REAL 2;
             LE pss,p4,P,p1,p2 & LE p9,pss,P,p1,p2
               by A1,A4,A10,A17,A20,A102,A103,A109,A110,A111,JORDAN5C:8;
          then pss`1=e by A99;
         hence contradiction by A112;
        end;
        hence contradiction by A64,A102,A104,AXIOMS:21;
       end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e by A91;
     end;
     end;
    hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e;
   end;
  hence thesis;
end;

reserve n for Nat;

theorem Th11:for P being Subset of I[01],s being Real st
 P=[.0,s.[ holds P is open
proof let P be Subset of I[01],s be Real;
  assume A1:P=[.0,s.[;
   A2: [#]I[01]=the carrier of I[01] by PRE_TOPC:12;
   then A3: [#]I[01]=[.0,1.] by TOPMETR:25,27;
  per cases;
  suppose A4: s in [.0,1.];
   then A5: 0<=s & s<=1 by TOPREAL5:1;
   reconsider Q=].-1,s.[ as Subset of R^1 by TOPMETR:24;
