                A139: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A124,A133,JORDAN5C:13;
              A140: now assume kk-i<0; then kk-i+i<0+i by REAL_1:53
;
                then kk+1<=i+1 by REAL_1:55;
                then LE f/.(kk+1),f/.(i+1),L~f,f/.1,f/.(len f)
                         by A3,A7,A130,JORDAN5C:24;
               hence contradiction by A2,A3,A113,A138,A139,JORDAN5C:12;
              end;
              A141: 1<kk+1 by A126,NAT_1:38;
              A142: i+1+k2=1+i+(kk-i) by A140,BINARITH:def 3
                        .=kk+1;
               then P[k2] by A126,A138,A141,FINSEQ_4:24;
              then k2>=k by A17;
              then i+1+k2>=i+1+k by REAL_1:55;
              then A143: LE f/.(i+1+k),f/.(i+1+k2),L~f,f/.1,f/.(len f)
                           by A3,A21,A126,A142,JORDAN5C:24;
              A144: LE p51,f/.(i+1+k),L~f,f/.1,f/.(len f) by A3,A23,A124,
FINSEQ_4:def 4;
              A145: LE f/.(kk),f/.(i+1+k),L~f,f/.1,f/.(len f)
                                 by A3,A24,A124,A134,JORDAN5C:13;
              then A146: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                         by A142,A143,JORDAN5C:13;
                 Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              then A147: f/.(i+1+k) in Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk
+1))
                                            by A142,A143,A145;
              A148: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A149: z in LSeg(f/.kk,f/.(kk+1));
                   LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A126,A132;
               hence z in L~f by A4,A149,TARSKI:def 4;
              end;
              A150: f is special by A3,TOPREAL1:def 10;
              A151: f is one-to-one by A3,TOPREAL1:def 10;
                 kk<kk+1 by NAT_1:38;
              then A152: f.kk <> f.(kk+1) by A129,A131,A151,FUNCT_1:def 8;
              A153: f/.kk=f.kk by A129,FINSEQ_4:def 4;
                 f/.(kk+1)=f.(kk+1) by A131,FINSEQ_4:def 4;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A152,A153,TOPREAL1:15;
              then A154: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A2,A3,A146,A148,Th28;
               A155: (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A126,A150,TOPREAL1:def 7;
                 (f/.(kk))`1>e by A24,A104,A138,A147,A154,Th2;
              then (f/.kk)`1> (f/.(kk+1))`1 by A138,AXIOMS:22;
              then (f/.(i+1+k))`1<=p51`1
                     by A3,A125,A126,A128,A132,A144,A147,A154,A155,Th6;
            hence contradiction by A24,A104,A124,AXIOMS:22;
           end;
           case A156: (f/.(kk))`1 <e & (f/.(kk+1))`1>=e;
              set k2=kk-'(i+1);
              A157: LSeg(f/.kk,f/.(kk+1)) c= L~f
              proof let z be set;assume
                A158: z in LSeg(f/.kk,f/.(kk+1));
                   LSeg(f/.kk,f/.(kk+1)) in
                { LSeg(f,i7) where i7 is Nat : 1 <= i7 & i7+1 <= len f }
                               by A126,A132;
               hence z in L~f by A4,A158,TARSKI:def 4;
              end;
              A159: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
              ={p8 where p8 is Point of TOP-REAL 2:
              LE f/.kk,p8,L~f,f/.1,f/.(len f)
              & LE p8,f/.(kk+1),L~f,f/.1,f/.(len f)}
                         by JORDAN6:29;
              A160: LE f/.kk,f/.(kk+1),L~f,f/.1,f/.(len f)
                          by A3,A126,JORDAN5C:23;
                 f/.(kk+1)=f.(kk+1) by A131,FINSEQ_4:def 4;
              then LSeg(f/.kk,f/.(kk+1)) is_an_arc_of f/.kk,f/.(kk+1)
                                  by A136,A137,TOPREAL1:15;
              then A161: Segment(L~f,f/.1,f/.(len f),f/.kk,f/.(kk+1))
                   =LSeg(f/.kk,f/.(kk+1)) by A2,A3,A157,A160,Th28;
                 LE p51,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A125,A126,JORDAN5C:26;
                then A162: LE p44,f/.(kk+1),L~f,f/.1,f/.(len f)
                                 by A3,A124,JORDAN5C:13;
              A163: now assume kk-(i+1)<0;
                then kk-(i+1)+(i+1)<0+(i+1) by REAL_1:53;
                then kk<=i by NAT_1:38;
                then A164: LE f/.kk,f/.i,L~f,f/.1,f/.(len f)
                         by A3,A9,A126,JORDAN5C:24;
                   LE f/.i,p44,L~f,f/.1,f/.(len f) by A3,A7,JORDAN5C:23;
                then LE f/.i,f/.(kk+1),L~f,f/.1,f/.(len f)
                             by A162,JORDAN5C:13;
                then A165: f/.i in LSeg(f/.kk,f/.(kk+1)) by A159,A161,A164;
                   f/.i in LSeg(f/.i,f/.(i+1)) by TOPREAL1:6;
                then LSeg(f/.kk,f/.(kk+1)) /\ LSeg(f/.i,f/.(i+1))<>{}
                             by A165,XBOOLE_0:def 3;
                then A166: not (LSeg(f/.kk,f/.(kk+1)) misses LSeg(f/.i,f/.(i+1)
))
                             by XBOOLE_0:def 7;
                A167: LSeg(f,kk)=LSeg(f/.kk,f/.(kk+1)) by A126,TOPREAL1:def 5;
                A168: LSeg(f,i)=LSeg(f/.i,f/.(i+1)) by A7,TOPREAL1:def 5;
                A169: f is s.n.c. by A3,TOPREAL1:def 10;
                then A170: kk+1>=i by A166,A167,A168,TOPREAL1:def 9;
                A171: kk-1=kk-'1  by A127,BINARITH:def 3;
                then A172: kk+1=kk-'1+1+1 by XCMPLX_1:27;
                   i+1>=kk by A166,A167,A168,A169,TOPREAL1:def 9;
                then i+1-1>=kk-1 by REAL_1:49;
                then A173: i=kk-'1 or i=kk-'1+1 or i=kk-'1+2 by A170,A171,A172,
JORDAN2C:2;
                A174: now assume i=kk-'1+2;
                  then A175: i=kk-1+1+1 by A171;
                  then kk+1<i+1 by NAT_1:38;
                  then LE f/.(kk+1),p44,L~f,f/.1,f/.(len f)
                             by A3,A7,A130,JORDAN5C:24;
                  then p44=f/.(kk+1) by A2,A3,A162,JORDAN5C:12;
                  then f.(i+1)=f/.(kk+1) by A10,FINSEQ_4:def 4;
                  then A176: f.(i+1)=f.(kk+1) by A131,FINSEQ_4:def 4;
                     f is one-to-one by A3,TOPREAL1:def 10;
                  then i+1=kk+1 by A10,A131,A176,FUNCT_1:def 8;
                 hence contradiction by A175,NAT_1:38;
                end;
                A177:now per cases by A171,A173,A174,XCMPLX_1:27;
                 case i=kk;
                  hence p44 in LSeg(f,kk) by A114,TOPREAL1:6;
                 end;
                 case i=kk-1;
                  hence p44 in LSeg(f,kk) by A132,TOPREAL1:6;
                 end;
                 end;
                    f is special by A3,TOPREAL1:def 10;
                  then (f/.kk)`1=(f/.(kk+1))`1 or
                      (f/.kk)`2=(f/.(kk+1))`2 by A126,TOPREAL1:def 7;
                 hence contradiction
                 by A3,A113,A124,A125,A126,A128,A156,A177,Th7;
              end;
              then A178: kk-'(i+1)=kk-(i+1) by BINARITH:def 3;
                i+1+k2=i+1+(kk-(i+1)) by A163,BINARITH:def 3
                        .=kk;
               then P[k2] by A126,A128,A156,FINSEQ_4:24;
              then k2>=k by A17;
              then kk-(i+1)+(i+1)>=k+(i+1) by A178,REAL_1:55;
              then A179: LE f/.(i+1+k),f/.(kk),L~f,f/.1,f/.(len f)
                  by A3,A22,A128,JORDAN5C:24;
                 LE f/.(kk),f/.(i+1+k),L~f,f/.1,f/.(len f)
                                 by A3,A24,A124,A134,JORDAN5C:13;
               hence contradiction by A2,A3,A24,A104,A156,A179,JORDAN5C:12;
              end;
              end;
            hence contradiction;
         end;
            for p5 being Point of TOP-REAL 2 st
            LE p5,pk,P,p1,p2 & LE p,p5,P,p1,p2 holds p5`1>=e
          proof let p5 be Point of TOP-REAL 2;
            assume A180: LE p5,pk,P,p1,p2 & LE p,p5,P,p1,p2;
             then A181: p5 in P by JORDAN5C:def 3;
                now per cases by A2,A122,A181,Th19;
             case LE p44,p5,P,p1,p2;
              hence p5`1>=e by A123,A180;
             end;
             case LE p5,p44,P,p1,p2;
              hence p5`1>=e by A116,A180;
             end;
             end;
           hence p5`1>=e;
          end;
      hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e
                    by A1,A2,A104,A121,Def4;
     end;
     end;
    hence p is_Lout P,p1,p2,e or p is_Rout P,p1,p2,e;
   end;
   end;
  hence thesis;
end;
