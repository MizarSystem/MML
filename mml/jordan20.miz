      end;
       now assume not ex p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2 & p51`1>e;then
        E2: for p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2
        holds p51`1<=e;
        D771: for p51 being Point of TOP-REAL 2 st
        LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2
        holds p51`1=e
        proof let p51 be Point of TOP-REAL 2;
          assume F1: LE p51,p4,P,p1,p2 & LE p9,p51,P,p1,p2;then
          F2: p51`1<=e by E2;
          p51`1>=e by F1,C1;
         hence p51`1=e by F2,AXIOMS:21;
        end;
        p4 in P by JORDAN5C:def 3,B7;then
        p4 in rng f by PRE_TOPC:def 10,B16;then
        consider xs4 being set such that
        B144: xs4 in dom f & p4=f.xs4 by FUNCT_1:def 5;
        reconsider s4=xs4 as Real by B144,B15;
        C714: 0<=s4 & s4<=1 by B144,B15,TOPREAL5:1;then
        C715: s4>=s0 by B7,B80,B82,JORDAN5C:def 3,B144,B3;
        now assume s4>s0;then
         --(s4-s0)>0 by SQUARE_1:11;then
         -(s4-s0)<0 by REAL_1:66;then
         s0-s4<0 by XCMPLX_1:143;then
         E123: (s0-s4)/2<0 by REAL_2:128;then
         E123b: -((s0-s4)/2)>0 by REAL_1:66;
         s4+0>s4+(s0-s4)/2 by E123,REAL_1:67;then
         E124: s4>s4+(s0-s4)/2;
          consider r being real number such that
