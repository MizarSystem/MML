           now assume s4>s0;
         then --(s4-s0)>0 by XREAL_1:52;
         then -(s4-s0)<0 by XREAL_1:60;
         then A105: (s0-s4)/2<0 by REAL_2:128;
         then A106: -((s0-s4)/2)>0 by XREAL_1:60;
         A107: s4+0>s4+(s0-s4)/2 by A105,XREAL_1:10;
          consider r being real number such that
          A108: r in R & r<s0+(-((s0-s4)/2)) by A21,A106,SEQ_4:def 5;
          A109: r>=s0 by A21,A108,SEQ_4:def 5;
          A110: s4>r by A107,A108,XXREAL_0:2;
          then A111: 1>r by A103,XXREAL_0:2;
          consider s7 being Real such that
          A112: s7=r & 1>=s7 & s7>=s2 &
