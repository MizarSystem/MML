    1 in [.0,1.] by XXREAL_1:1;
    then ].s,1.] c= [.s,1.] & [.s,1.] c= [.0,1.] by A3,XXREAL_1:23
,XXREAL_2:def 12;
    then ].s,1.] c= [.0,1.] by XBOOLE_1:1;
    then P c= [#](R^1|T) by A2,PRE_TOPC:def 10;
    then reconsider P2=P as Subset of R^1|T;
    reconsider Q=].s,2.[ as Subset of R^1 by TOPMETR:24;
A4: 0<=s by A3,XXREAL_1:1;
A5: ].s,1.] c= ].s,2.[ /\ [.0,1.]
    proof
      let x be set;
      assume
A6:   x in ].s,1.];
      then reconsider sx=x as Real;
A7:   s<sx by A6,XXREAL_1:2;
A8:   sx<=1 by A6,XXREAL_1:2;
      then 2 > sx by XXREAL_0:2;
      then
A9:   x in ].s,2.[ by A7,XXREAL_1:4;
      x in [.0,1.] by A4,A7,A8,XXREAL_1:1;
      hence thesis by A9,XBOOLE_0:def 4;
    end;
    ].s,2.[ /\ [.0,1.] c= ].s,1.]
    proof
      let x be set;
      assume
A10:  x in ].s,2.[ /\ [.0,1.];
      then reconsider sx=x as Real;
      x in [.0,1.] by A10,XBOOLE_0:def 4;
      then
A11:  sx<=1 by XXREAL_1:1;
      x in ].s,2.[ by A10,XBOOLE_0:def 4;
      then s<sx by XXREAL_1:4;
      hence thesis by A11,XXREAL_1:2;
    end;
    then ].s,1.] = ].s,2.[ /\ [.0,1.] by A5,XBOOLE_0:def 10;
    then
A12: P2=Q /\ [#](R^1|T) by A2,PRE_TOPC:def 10;
    Q is open & Closed-Interval-TSpace(0,1) =R^1|T by JORDAN6:46,TOPMETR:26;
    hence thesis by A12,TOPMETR:27,TOPS_2:32;
  end;
  suppose
A13: not s in [.0,1.];
    now
      per cases by A13,XXREAL_1:1;
      case
        s>1;
        then ].s,1.] ={} by XXREAL_1:26;
        then P in the topology of I[01] by A2,PRE_TOPC:5;
        hence thesis by PRE_TOPC:def 5;
      end;
      case
A14:    s<0;
A15:    for r being Real st s<r & r<=1 holds r>=0
        proof
          let r be Real;
          assume s<r & r<=1;
          then r in ].s,1.] by XXREAL_1:2;
          hence thesis by A2,A1,XXREAL_1:1;
        end;
        consider t being real number such that
A16:    s<t and
A17:    t<0 by A14,XREAL_1:7;
        reconsider t as Real by XREAL_0:def 1;
        t<=1 by A17;
        hence contradiction by A16,A17,A15;
