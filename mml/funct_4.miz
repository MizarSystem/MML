theorem
  for f being Function, a,b,c,d being set st a <> b holds (f +* ((a,b)
  -->(c,d))) .a = c & (f +* ((a,b)-->(c,d))) .b = d
proof
  let f be Function, a,b,c,d be set such that
A1: a <> b;
  set g = (a,b)-->(c,d);
A2: dom g = {a,b} by Th65;
