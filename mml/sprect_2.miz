    end;
    now
      per cases by A24,SPPOL_1:19;
      suppose
        LSeg(f,len f -' 1) is vertical;
        then
A29:    p`1 = (f/.(len f -' 1))`1 by A19,A15,SPPOL_1:def 3;
        then
A30:    f/.(len f -' 1) in W-most L~f by A2,A7,A16,Th12,GOBOARD1:1;
