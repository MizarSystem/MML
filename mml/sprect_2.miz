A32: M/.len M in L~M by A30,JORDAN3:34;
A33: 1 in dom M by FINSEQ_5:6;
A34: M/.1 = z/.1 by A5,A14,A15,Th12;
  per cases;
  suppose that
A35: NW-corner L~z = N-min L~z and
A36: NE-corner L~z = N-max L~z;
A37: M is_in_the_area_of z by A5,A14,Th25,Th26;
