  hence thesis by A165,XBOOLE_0:def 10;
end;

theorem Th39:
  for a,b,d,e being Real st a<=b & e>0 ex f being Function of
  Closed-Interval-TSpace(a,b),Closed-Interval-TSpace(e*a+d,e*b+d) st f is
  being_homeomorphism & for r being Real st r in [.a,b.] holds f.r=e*r+d
proof
  let a,b,d,e be Real;
  assume that
A1: a<=b and
A2: e>0;
  set S=Closed-Interval-TSpace(a,b);
  defpred P[set,set] means (for r being Real st $1=r holds $2=e*r+d);
  set X=the carrier of Closed-Interval-TSpace(a,b);
A3: X=[.a,b.] by A1,TOPMETR:25;
  then reconsider B=the carrier of S as Subset of R^1 by TOPMETR:24;
A4: R^1|B= S by A1,A3,TOPMETR:26;
  set T=Closed-Interval-TSpace(e*a+d,e*b+d);
  set Y=the carrier of Closed-Interval-TSpace(e*a+d,e*b+d);
A5: e*a<=e*b by A1,A2,XREAL_1:66;
  then
A6: Y=[.e*a+d,e*b+d.] by TOPMETR:25,XREAL_1:9;
  then reconsider C=the carrier of T as Subset of R^1 by TOPMETR:24;
  defpred P1[set,set] means for r being Real st r=$1 holds $2=e*r+d;
  T=TopSpaceMetr(Closed-Interval-MSpace(e*a+d,e*b+d)) by TOPMETR:def 8;
  then
A7: T is T_2 by PCOMPS_1:38;
A8: for x being set st x in X ex y being set st y in Y & P[x,y]
  proof
    let x be set;
    assume
A9: x in X;
    then reconsider r1=x as Real by A3;
    set y1=e*r1+d;
    r1<=b by A3,A9,XXREAL_1:1;
    then e*r1<=e*b by A2,XREAL_1:66;
    then
A10: y1 <=e*b+d by XREAL_1:9;
    a<=r1 by A3,A9,XXREAL_1:1;
    then e*a<=e*r1 by A2,XREAL_1:66;
    then e*a+d<=y1 by XREAL_1:9;
    then ( for r being Real st x=r holds y1=e*r+d)& y1 in Y by A6,A10,
    XXREAL_1:1;
    hence thesis;
  end;
  ex f being Function of X,Y st for x being set st x in X holds P[x,f.x]
  from FUNCT_2:sch 1(A8);
  then consider f1 being Function of X,Y such that
A11: for x being set st x in X holds P[x,f1.x];
  reconsider f2=f1 as Function of Closed-Interval-TSpace(a,b),
  Closed-Interval-TSpace(e*a+d,e*b+d);
A12: for r being Real st r in [.a,b.] holds f2.r=e*r+d by A3,A11;
A13: dom f2=the carrier of S by FUNCT_2:def 1;
  [#]T c= rng f2
  proof
    let y be set;
    assume
A14: y in [#]T;
    then reconsider ry=y as Real by A6;
    ry<=e*b+d by A6,A14,XXREAL_1:1;
    then e*b+d-d>=ry-d by XREAL_1:11;
    then b*e/e>=(ry-d)/e by A2,XREAL_1:74;
    then
A15: b>=(ry-d)/e by A2,XCMPLX_1:90;
    e*a+d <= ry by A6,A14,XXREAL_1:1;
    then e*a+d-d<=ry-d by XREAL_1:11;
    then a*e/e<=(ry-d)/e by A2,XREAL_1:74;
    then a<=(ry-d)/e by A2,XCMPLX_1:90;
    then
A16: (ry-d)/e in [.a,b.] by A15,XXREAL_1:1;
    then f2.((ry-d)/e)=e*((ry-d)/e)+d by A3,A11
      .=ry-d+d by A2,XCMPLX_1:88
      .=ry;
    hence thesis by A3,A13,A16,FUNCT_1:12;
  end;
  then
A17: rng f2 = [#]T by XBOOLE_0:def 10;
  then reconsider f3=f1 as Function of S,R^1 by A6,A13,FUNCT_2:4,TOPMETR:24;
  for x1,x2 being set st x1 in dom f2 & x2 in dom f2 & f2.x1=f2.x2 holds
  x1=x2
  proof
    let x1,x2 be set;
    assume that
A18: x1 in dom f2 and
A19: x2 in dom f2 and
A20: f2.x1=f2.x2;
    reconsider r2=x2 as Real by A3,A13,A19;
    reconsider r1=x1 as Real by A3,A13,A18;
    f2.x1=e*r1+d by A11,A18;
    then e*r1+d-d=e*r2+d-d by A11,A19,A20
      .=e*r2;
    then r1*e/e=r2 by A2,XCMPLX_1:90;
    hence thesis by A2,XCMPLX_1:90;
  end;
  then
A21: dom f2=[#]S & f2 is one-to-one by FUNCT_1:def 8,FUNCT_2:def 1;
A22: for x being set st x in the carrier of R^1 ex y being set st y in the
  carrier of R^1 & P1[x,y]
  proof
    let x be set;
    assume
    x in the carrier of R^1;
    then reconsider rx=x as Real by TOPMETR:24;
    reconsider ry=e*rx+d as Real;
    for r being Real st r=x holds ry=e*r+d;
    hence thesis by TOPMETR:24;
  end;
  ex f4 being Function of the carrier of R^1,the carrier of R^1 st for x
being set st x in the carrier of R^1 holds P1[x,f4.x] from FUNCT_2:sch 1(A22);
  then consider
  f4 being Function of the carrier of R^1,the carrier of R^1 such
  that
A23: for x being set st x in the carrier of R^1 holds P1[x,f4.x];
  reconsider f5=f4 as Function of R^1,R^1;
A24: for x being Real holds f5.x = e*x + d by A23,TOPMETR:24;
A25: (dom f5) /\ B =REAL /\ B by FUNCT_2:def 1,TOPMETR:24
    .=B by TOPMETR:24,XBOOLE_1:28;
A26: for x being set st x in dom f3 holds f3.x=f5.x
  proof
    let x be set;
    assume
A27: x in dom f3;
    then reconsider rx=x as Real by A3,A13;
    f4.x=e*rx+d by A23,TOPMETR:24;
    hence thesis by A11,A27;
  end;
  dom f3=B by FUNCT_2:def 1;
  then f3=f5|B by A25,A26,FUNCT_1:68;
  then
A28: f3 is continuous by A24,A4,TOPMETR:10,28;
A29: S is compact by A1,HEINE:11;
  R^1|C=T by A5,A6,TOPMETR:26,XREAL_1:9;
  then f2 is being_homeomorphism by A21,A17,A28,A29,A7,COMPTS_1:26,
  TOPMETR:9;
  hence thesis by A12;
end;

theorem Th40:
  for a,b,d,e being Real st a<=b & e<0 ex f being Function of
  Closed-Interval-TSpace(a,b),Closed-Interval-TSpace(e*b+d,e*a+d) st f is
  being_homeomorphism & for r being Real st r in [.a,b.] holds f.r=e*r+d
