  assume that
A1: -1<sn and
A2: sn<1 and
A3: q`1>0;
  now
    per cases;
    case
      q`2/|.q.|>=sn;
      hence thesis by A2,A3,JGRAPH_4:113;
    end;
    case
      q`2/|.q.|<sn;
      hence thesis by A1,A3,JGRAPH_4:114;
    end;
  end;
  hence thesis;
end;

theorem
  for sn being Real,q being Point of TOP-REAL 2 st -1<sn & sn<1 & q`1>=0
  & q`2/|.q.|<sn & |.q.|<>0 holds for p being Point of TOP-REAL 2 st p=(sn
  -FanMorphE).q holds p`1>=0 & p`2<0
proof
  let sn be Real,q be Point of TOP-REAL 2;
  assume that
A1: -1<sn and
A2: sn<1 and
A3: q`1>=0 & q`2/|.q.|<sn and
A4: |.q.|<>0;
  let p be Point of TOP-REAL 2;
  assume
A5: p=(sn-FanMorphE).q;
  now
    per cases;
    case
A6:   q`1=0;
      then |.q.|^2=(q`2)^2+0^2 by JGRAPH_3:10
        .=(q`2)^2;
      then
A7:   |.q.|=q`2 or |.q.|=-(q`2) by SQUARE_1:109;
      q=p by A5,A6,JGRAPH_4:89;
      hence thesis by A2,A3,A4,A7,XCMPLX_1:60;
    end;
    case
      q`1<>0;
      hence thesis by A1,A3,A5,JGRAPH_4:114;
    end;
  end;
  hence thesis;
end;

theorem Th27:
  for sn being Real,q1,q2 being Point of TOP-REAL 2 st -1<sn & sn<
  1 & q1`1>=0 & q2`1>=0 & |.q1.|<>0 & |.q2.|<>0 & q1`2/|.q1.|<q2`2/|.q2.| holds
for p1,p2 being Point of TOP-REAL 2 st p1=(sn-FanMorphE).q1 & p2=(sn-FanMorphE)
  .q2 holds p1`2/|.p1.|<p2`2/|.p2.|
proof
  let sn be Real,q1,q2 be Point of TOP-REAL 2;
  assume that
A1: -1<sn & sn<1 and
A2: q1`1>=0 and
A3: q2`1>=0 and
A4: |.q1.|<>0 and
A5: |.q2.|<>0 and
A6: q1`2/|.q1.|<q2`2/|.q2.|;
  now
    per cases by A2;
    case
A7:   q1`1>0;
      now
        per cases by A3;
        case
          q2`1>0;
          hence thesis by A1,A6,A7,JGRAPH_4:117;
        end;
        case
A8:       q2`1=0;
A9:       now
            (|.q1.|)^2=(q1`2)^2+(q1`1)^2 by JGRAPH_3:10;
            then (|.q1.|)^2-(q1`2)^2>=0 by XREAL_1:65;
            then (|.q1.|)^2-(q1`2)^2+(q1`2)^2>=0+(q1`2)^2 by XREAL_1:9;
            then -|.q1.|<=q1`2 by SQUARE_1:117;
            then
A10:        (-|.q1.|)/|.q1.|<=q1`2/|.q1.| by XREAL_1:74;
            assume
            |.q2.|=-(q2`2);
            then 1=(-(q2`2))/|.q2.| by A5,XCMPLX_1:60;
            then q1`2/|.q1.|< -1 by A6,XCMPLX_1:191;
            hence contradiction by A4,A10,XCMPLX_1:198;
          end;
          |.q2.|^2=(q2`2)^2+0^2 by A8,JGRAPH_3:10
            .=(q2`2)^2;
          then |.q2.|=q2`2 or |.q2.|=-(q2`2) by SQUARE_1:109;
          then
A11:      q2`2/|.q2.|=1 by A5,A9,XCMPLX_1:60;
          thus for p1,p2 being Point of TOP-REAL 2 st p1=(sn-FanMorphE).q1 &
          p2=(sn-FanMorphE).q2 holds p1`2/|.p1.|<p2`2/|.p2.|
          proof
            let p1,p2 be Point of TOP-REAL 2;
            assume that
A12:        p1=(sn-FanMorphE).q1 and
A13:        p2=(sn-FanMorphE).q2;
A14:        |.p1.|=|.q1.| by A12,JGRAPH_4:104;
A15:        (|.p1.|)^2=(p1`2)^2+(p1`1)^2 by JGRAPH_3:10;
A16:        p1`1>0 by A1,A7,A12,Th25;
A17:        now
              assume
              1= p1`2/|.p1.|;
              then (1)*|.p1.|=p1`2 by A4,A14,XCMPLX_1:88;
              hence contradiction by A15,A16,XCMPLX_1:6;
            end;
A18:        p2=q2 by A8,A13,JGRAPH_4:89;
            (|.p1.|)^2-(p1`2)^2>=0 by A15,XREAL_1:65;
            then (|.p1.|)^2-(p1`2)^2+(p1`2)^2>=0+(p1`2)^2 by XREAL_1:9;
            then p1`2<=|.p1.| by SQUARE_1:117;
            then (|.p1.|)/|.p1.|>=p1`2/|.p1.| by XREAL_1:74;
            then 1>= p1`2/|.p1.| by A4,A14,XCMPLX_1:60;
            hence thesis by A11,A18,A17,XXREAL_0:1;
          end;
        end;
      end;
      hence thesis;
    end;
    case
A19:  q1`1=0;
A20:  now
        (|.q2.|)^2=(q2`2)^2+(q2`1)^2 by JGRAPH_3:10;
        then (|.q2.|)^2-(q2`2)^2>=0 by XREAL_1:65;
        then (|.q2.|)^2-(q2`2)^2+(q2`2)^2>=0+(q2`2)^2 by XREAL_1:9;
        then q2`2<=|.q2.| by SQUARE_1:117;
        then
A21:    (|.q2.|)/|.q2.|>=q2`2/|.q2.| by XREAL_1:74;
        assume
        |.q1.|=(q1`2);
        then q2`2/|.q2.|> 1 by A4,A6,XCMPLX_1:60;
        hence contradiction by A5,A21,XCMPLX_1:60;
      end;
      |.q1.|^2=(q1`2)^2+0^2 by A19,JGRAPH_3:10
        .=(q1`2)^2;
      then |.q1.|=q1`2 or |.q1.|=-(q1`2) by SQUARE_1:109;
      then (-(q1`2))/|.q1.|=1 by A4,A20,XCMPLX_1:60;
      then
A22:  -(q1`2/|.q1.|)=1 by XCMPLX_1:188;
      thus for p1,p2 being Point of TOP-REAL 2 st p1=(sn-FanMorphE).q1 & p2=(
      sn-FanMorphE).q2 holds p1`2/|.p1.|<p2`2/|.p2.|
      proof
        let p1,p2 be Point of TOP-REAL 2;
        assume that
A23:    p1=(sn-FanMorphE).q1 and
A24:    p2=(sn-FanMorphE).q2;
A25:    |.p2.|=|.q2.| by A24,JGRAPH_4:104;
A26:    (|.p2.|)^2=(p2`2)^2+(p2`1)^2 by JGRAPH_3:10;
        then (|.p2.|)^2-(p2`2)^2>=0 by XREAL_1:65;
        then (|.p2.|)^2-(p2`2)^2+(p2`2)^2>=0+(p2`2)^2 by XREAL_1:9;
        then -|.p2.|<=p2`2 by SQUARE_1:117;
        then (-|.p2.|)/|.p2.|<=p2`2/|.p2.| by XREAL_1:74;
        then
A27:    -1 <= p2`2/|.p2.| by A5,A25,XCMPLX_1:198;
A28:    now
          per cases;
          case
            q2`1=0;
            then p2=q2 by A24,JGRAPH_4:89;
            hence p2`2/|.p2.|>-1 by A6,A22;
          end;
          case
            q2`1<>0;
            then
A29:        p2`1>0 by A1,A3,A24,Th25;
            now
              assume
              -1= p2`2/|.p2.|;
              then (-1)*|.p2.|=p2`2 by A5,A25,XCMPLX_1:88;
              then (|.p2.|)^2=(p2`2)^2;
              hence contradiction by A26,A29,XCMPLX_1:6;
            end;
            hence p2`2/|.p2.|>-1 by A27,XXREAL_0:1;
          end;
        end;
        p1=q1 by A19,A23,JGRAPH_4:89;
        hence thesis by A22,A28;
      end;
    end;
  end;
  hence thesis;
end;

theorem Th28:
  for cn being Real,q being Point of TOP-REAL 2 st -1<cn & cn<1 &
  q`2<0 holds for p being Point of TOP-REAL 2 st p=(cn-FanMorphS).q holds p`2<0
proof
  let cn be Real,q be Point of TOP-REAL 2;
  assume that
A1: -1<cn and
A2: cn<1 and
A3: q`2<0;
  now
    per cases;
    case
      q`1/|.q.|>=cn;
      hence thesis by A2,A3,JGRAPH_4:144;
    end;
    case
      q`1/|.q.|<cn;
      hence thesis by A1,A3,JGRAPH_4:145;
    end;
  end;
  hence thesis;
end;

theorem Th29:
  for cn being Real,q being Point of TOP-REAL 2 st -1<cn & cn<1 &
q`2<0 & q`1/|.q.|>cn holds for p being Point of TOP-REAL 2 st p=(cn-FanMorphS).
  q holds p`2<0 & p`1>0
proof
  let cn be Real,q be Point of TOP-REAL 2;
  assume that
A1: -1<cn and
A2: cn<1 and
A3: q`2<0 and
A4: q`1/|.q.|>cn;
  let p be Point of TOP-REAL 2;
  assume
A5: p=(cn-FanMorphS).q;
  now
    set q1=(|.p.|)*|[cn,-sqrt(1-cn^2)]|;
    set p1=(1/|.p.|)*p;
    set p2=(cn-FanMorphS).q1;
    (|[0,-1]|)`1=0 & (|[0,-1]|)`2=-1 by EUCLID:56;
    then
A6: |.p.|*(|[0,-1]|)=|[|.p.|*0,|.p.|*(-1)]| by EUCLID:61
      .=|[0,-(|.p.|)]|;
A7: (|[cn,-sqrt(1-cn^2)]|)`1=cn & (|[cn,-sqrt(1-cn^2)]|)`2=-sqrt(1-cn^2)
    by EUCLID:56;
    then
A8: q1=|[|.p.|*cn,|.p.|*(-sqrt(1-cn^2))]| by EUCLID:61;
    then
A9: q1`1=(|.p.|)*cn by EUCLID:56;
    assume
A10: p`1=0;
    then (|.p.|)^2=(p`2)^2+0^2 by JGRAPH_3:10
      .=(p`2)^2;
    then
A11: p`2=|.p.| or p`2= - |.p.| by SQUARE_1:109;
    then
A12: |.p.| <> 0 by A2,A3,A4,A5,JGRAPH_4:144;
A13: q1`2=-(sqrt(1-cn^2)*(|.p.|)) by A8,EUCLID:56;
    1^2>cn^2 by A1,A2,SQUARE_1:120;
    then
A14: 1-cn^2>0 by XREAL_1:52;
    then sqrt(1-cn^2)>0 by SQUARE_1:93;
    then --sqrt(1-cn^2)*(|.p.|)>0 by A12,XREAL_1:131;
    then
A15: q1`2<0 by A13;
A16: |.p.|*p1=(|.p.|*(1/|.p.|))*p by EUCLID:34
      .=(1)*p by A12,XCMPLX_1:107
      .=p by EUCLID:33;
A17: p1=|[(1/|.p.|)*p`1,(1/|.p.|)*p`2]| by EUCLID:61;
    then p1`2=-(|.p.|*(1/ |.p.|)) by A2,A3,A4,A5,A11,
    EUCLID:56,JGRAPH_4:144
      .=-1 by A12,XCMPLX_1:107;
    then
A18: p=|.p.|*(|[0,-1]|) by A10,A16,A17,EUCLID:56;
A19: |.q1.|=abs(|.p.|)*|.(|[cn,-sqrt(1-cn^2)]|).| by TOPRNS_1:8
      .=abs(|.p.|)*sqrt((cn)^2+(-sqrt(1-cn^2))^2) by A7,JGRAPH_3:10
      .=abs(|.p.|)*sqrt(cn^2+(sqrt(1-cn^2))^2)
      .=abs(|.p.|)*sqrt(cn^2+(1-cn^2)) by A14,SQUARE_1:def 4
      .=|.p.| by ABSVALUE:def 1,SQUARE_1:83;
    then
A20: |.p2.|=|.p.| by JGRAPH_4:135;
A21: q1`1/|.q1.|=cn by A12,A9,A19,XCMPLX_1:90;
    then
A22: p2`1=0 by A15,JGRAPH_4:149;
    then (|.p2.|)^2=(p2`2)^2+0^2 by JGRAPH_3:10
      .=(p2`2)^2;
    then p2`2=|.p2.| or p2`2= - |.p2.| by SQUARE_1:109;
    then
A23: p2=|[0,-(|.p.|)]| by A15,A21,A22,A20,EUCLID:57,JGRAPH_4:149;
    (cn-FanMorphS) is one-to-one & dom (cn-FanMorphS)=the carrier of
    TOP-REAL 2 by A1,A2,FUNCT_2:def 1,JGRAPH_4:140;
    then q1=q by A5,A18,A23,A6,FUNCT_1:def 8;
    hence contradiction by A4,A12,A9,A19,XCMPLX_1:90;
  end;
  hence thesis by A2,A3,A4,A5,JGRAPH_4:144;
end;

theorem Th30:
  for cn being Real,q1,q2 being Point of TOP-REAL 2 st -1<cn & cn<
  1 & q1`2<=0 & q2`2<=0 & |.q1.|<>0 & |.q2.|<>0 & q1`1/|.q1.|<q2`1/|.q2.| holds
for p1,p2 being Point of TOP-REAL 2 st p1=(cn-FanMorphS).q1 & p2=(cn-FanMorphS)
  .q2 holds p1`1/|.p1.|<p2`1/|.p2.|
proof
  let cn be Real,q1,q2 be Point of TOP-REAL 2;
  assume that
A1: -1<cn & cn<1 and
A2: q1`2<=0 and
A3: q2`2<=0 and
A4: |.q1.|<>0 and
A5: |.q2.|<>0 and
A6: q1`1/|.q1.|<q2`1/|.q2.|;
  now
    per cases by A2;
    case
A7:   q1`2<0;
      now
        per cases by A3;
        case
          q2`2<0;
          hence thesis by A1,A6,A7,JGRAPH_4:148;
        end;
        case
A8:       q2`2=0;
A9:       now
            (|.q1.|)^2=(q1`1)^2+(q1`2)^2 by JGRAPH_3:10;
            then (|.q1.|)^2-(q1`1)^2>=0 by XREAL_1:65;
            then (|.q1.|)^2-(q1`1)^2+(q1`1)^2>=0+(q1`1)^2 by XREAL_1:9;
            then -|.q1.|<=q1`1 by SQUARE_1:117;
            then
A10:        (-|.q1.|)/|.q1.|<=q1`1/|.q1.| by XREAL_1:74;
            assume
            |.q2.|=-(q2`1);
            then 1=(-(q2`1))/|.q2.| by A5,XCMPLX_1:60;
            then q1`1/|.q1.|< -1 by A6,XCMPLX_1:191;
            hence contradiction by A4,A10,XCMPLX_1:198;
          end;
          |.q2.|^2=(q2`1)^2+0^2 by A8,JGRAPH_3:10
            .=(q2`1)^2;
          then |.q2.|=q2`1 or |.q2.|=-(q2`1) by SQUARE_1:109;
          then
A11:      q2`1/|.q2.|=1 by A5,A9,XCMPLX_1:60;
          thus for p1,p2 being Point of TOP-REAL 2 st p1=(cn-FanMorphS).q1 &
          p2=(cn-FanMorphS).q2 holds p1`1/|.p1.|<p2`1/|.p2.|
          proof
            let p1,p2 be Point of TOP-REAL 2;
            assume that
A12:        p1=(cn-FanMorphS).q1 and
A13:        p2=(cn-FanMorphS).q2;
A14:        |.p1.|=|.q1.| by A12,JGRAPH_4:135;
A15:        (|.p1.|)^2=(p1`1)^2+(p1`2)^2 by JGRAPH_3:10;
A16:        p1`2<0 by A1,A7,A12,Th28;
A17:        now
              assume
              1= p1`1/|.p1.|;
              then (1)*|.p1.|=p1`1 by A4,A14,XCMPLX_1:88;
              hence contradiction by A15,A16,XCMPLX_1:6;
            end;
A18:        p2=q2 by A8,A13,JGRAPH_4:120;
            (|.p1.|)^2-(p1`1)^2>=0 by A15,XREAL_1:65;
            then (|.p1.|)^2-(p1`1)^2+(p1`1)^2>=0+(p1`1)^2 by XREAL_1:9;
            then p1`1<=|.p1.| by SQUARE_1:117;
            then (|.p1.|)/|.p1.|>=p1`1/|.p1.| by XREAL_1:74;
            then 1>= p1`1/|.p1.| by A4,A14,XCMPLX_1:60;
            hence thesis by A11,A18,A17,XXREAL_0:1;
          end;
        end;
      end;
      hence thesis;
    end;
    case
A19:  q1`2=0;
A20:  now
        (|.q2.|)^2=(q2`1)^2+(q2`2)^2 by JGRAPH_3:10;
        then (|.q2.|)^2-(q2`1)^2>=0 by XREAL_1:65;
        then (|.q2.|)^2-(q2`1)^2+(q2`1)^2>=0+(q2`1)^2 by XREAL_1:9;
        then q2`1<=|.q2.| by SQUARE_1:117;
        then
A21:    (|.q2.|)/|.q2.|>=q2`1/|.q2.| by XREAL_1:74;
        assume
        |.q1.|=(q1`1);
        then q2`1/|.q2.|> 1 by A4,A6,XCMPLX_1:60;
        hence contradiction by A5,A21,XCMPLX_1:60;
      end;
      |.q1.|^2=(q1`1)^2+0^2 by A19,JGRAPH_3:10
        .=(q1`1)^2;
      then |.q1.|=q1`1 or |.q1.|=-(q1`1) by SQUARE_1:109;
      then (-(q1`1))/|.q1.|=1 by A4,A20,XCMPLX_1:60;
      then
A22:  -(q1`1/|.q1.|)=1 by XCMPLX_1:188;
      thus for p1,p2 being Point of TOP-REAL 2 st p1=(cn-FanMorphS).q1 & p2=(
      cn-FanMorphS).q2 holds p1`1/|.p1.|<p2`1/|.p2.|
      proof
        let p1,p2 be Point of TOP-REAL 2;
        assume that
A23:    p1=(cn-FanMorphS).q1 and
A24:    p2=(cn-FanMorphS).q2;
A25:    |.p2.|=|.q2.| by A24,JGRAPH_4:135;
A26:    (|.p2.|)^2=(p2`1)^2+(p2`2)^2 by JGRAPH_3:10;
        then (|.p2.|)^2-(p2`1)^2>=0 by XREAL_1:65;
        then (|.p2.|)^2-(p2`1)^2+(p2`1)^2>=0+(p2`1)^2 by XREAL_1:9;
        then -|.p2.|<=p2`1 by SQUARE_1:117;
        then (-|.p2.|)/|.p2.|<=p2`1/|.p2.| by XREAL_1:74;
        then
A27:    -1 <= p2`1/|.p2.| by A5,A25,XCMPLX_1:198;
A28:    now
          per cases;
          case
            q2`2=0;
            then p2=q2 by A24,JGRAPH_4:120;
            hence p2`1/|.p2.|>-1 by A6,A22;
          end;
          case
            q2`2<>0;
            then
A29:        p2`2<0 by A1,A3,A24,Th28;
            now
              assume
              -1= p2`1/|.p2.|;
              then (-1)*|.p2.|=p2`1 by A5,A25,XCMPLX_1:88;
              then (|.p2.|)^2=(p2`1)^2;
              hence contradiction by A26,A29,XCMPLX_1:6;
            end;
            hence p2`1/|.p2.|>-1 by A27,XXREAL_0:1;
          end;
        end;
        p1=q1 by A19,A23,JGRAPH_4:120;
        hence thesis by A22,A28;
      end;
    end;
  end;
  hence thesis;
end;

begin :: Order of Points on Circle

Lm3: now
  let P be compact non empty Subset of TOP-REAL 2;
  assume
A1: P={q: |.q.|=1};
A2: [.-1,1.] c= proj1.:P
  proof
    let y be set;
    assume
    y in [.-1,1.];
    then y in {r where r is Real: -1<=r & r<=1 } by RCOMP_1:def 1;
    then consider r being Real such that
A3: y=r and
A4: -1<=r & r<=1;
    set q=|[r,sqrt(1-r^2)]|;
    1^2>=r^2 by A4,SQUARE_1:119;
    then
A5: 1-r^2>=0 by XREAL_1:50;
    q`1=r & q`2=sqrt(1-r^2) by EUCLID:56;
    then |.q.|=sqrt(r^2+(sqrt(1-r^2))^2) by JGRAPH_3:10
      .=sqrt(r^2+(1-r^2)) by A5,SQUARE_1:def 4
      .=1 by SQUARE_1:83;
    then
A6: dom proj1=the carrier of TOP-REAL 2 & q in P by A1,
    FUNCT_2:def 1;
    proj1.q=q`1 by PSCOMP_1:def 28
      .=r by EUCLID:56;
    hence thesis by A3,A6,FUNCT_1:def 12;
  end;
  proj1.:P c= [.-1,1.]
  proof
    let y be set;
    assume
    y in proj1.:P;
    then consider x being set such that
A7: x in dom proj1 and
A8: x in P and
A9: y=proj1.x by FUNCT_1:def 12;
    reconsider q=x as Point of TOP-REAL 2 by A7;
    ex q2 being Point of TOP-REAL 2 st q2=x & |.q2.|=1 by A1,A8;
    then
A10: (q`1)^2+(q`2)^2=1^2 by JGRAPH_3:10;
    0<=(q`2)^2 by XREAL_1:65;
    then 1-(q`1)^2+(q`1)^2 >=0+(q`1)^2 by A10,XREAL_1:9;
    then
A11: -1<=q`1 & q`1<=1 by SQUARE_1:121;
    y=q`1 by A9,PSCOMP_1:def 28;
    hence thesis by A11,XXREAL_1:1;
  end;
  hence proj1.:P=[.-1,1.] by A2,XBOOLE_0:def 10;
A12: [.-1,1.] c= proj2.:P
  proof
    let y be set;
    assume
    y in [.-1,1.];
    then y in {r where r is Real: -1<=r & r<=1 } by RCOMP_1:def 1;
    then consider r being Real such that
A13: y=r and
A14: -1<=r & r<=1;
    set q=|[sqrt(1-r^2),r]|;
    1^2>=r^2 by A14,SQUARE_1:119;
    then
A15: 1-r^2>=0 by XREAL_1:50;
    q`2=r & q`1=sqrt(1-r^2) by EUCLID:56;
    then |.q.|=sqrt((sqrt(1-r^2))^2+r^2) by JGRAPH_3:10
      .=sqrt((1-r^2)+r^2) by A15,SQUARE_1:def 4
      .=1 by SQUARE_1:83;
    then
A16: dom proj2=the carrier of TOP-REAL 2 & q in P by A1,
    FUNCT_2:def 1;
    proj2.q=q`2 by PSCOMP_1:def 29
      .=r by EUCLID:56;
    hence thesis by A13,A16,FUNCT_1:def 12;
  end;
  proj2.:P c= [.-1,1.]
  proof
    let y be set;
    assume
    y in proj2.:P;
    then consider x being set such that
A17: x in dom proj2 and
A18: x in P and
A19: y=proj2.x by FUNCT_1:def 12;
    reconsider q=x as Point of TOP-REAL 2 by A17;
    ex q2 being Point of TOP-REAL 2 st q2=x & |.q2.|=1 by A1,A18;
    then
A20: (q`1)^2+(q`2)^2=1^2 by JGRAPH_3:10;
    0<=(q`1)^2 by XREAL_1:65;
    then 1-(q`2)^2+(q`2)^2 >=0+(q`2)^2 by A20,XREAL_1:9;
    then
A21: -1<=q`2 & q`2<=1 by SQUARE_1:121;
    y=q`2 by A19,PSCOMP_1:def 29;
    hence thesis by A21,XXREAL_1:1;
  end;
  hence proj2.:P=[.-1,1.] by A12,XBOOLE_0:def 10;
end;

Lm4: for P being compact non empty Subset of TOP-REAL 2 st P={q: |.q.|=1}
holds W-bound(P)=-1

proof
  let P be compact non empty Subset of TOP-REAL 2;
  assume
  P={q: |.q.|=1};
  then proj1.:P=[.-1,1.] by Lm3;
  then (proj1|P).:P=[.-1,1.] by RELAT_1:162;

  then the carrier of ((TOP-REAL 2)|P) = P & lower_bound ((proj1|P).:P)=-1 by
  JORDAN5A:20,PRE_TOPC:29;

  then inf (proj1|P)=-1 by PSCOMP_1:def 20;
  hence thesis by PSCOMP_1:def 30;
end;

theorem Th31:
  for P being compact non empty Subset of TOP-REAL 2 st P={q: |.q
  .|=1} holds W-bound(P)=-1 & E-bound(P)=1 & S-bound(P)=-1 & N-bound(P)=1
proof
  let P be compact non empty Subset of TOP-REAL 2;
A1: the carrier of ((TOP-REAL 2)|P) =P by PRE_TOPC:29;
  assume
A2: P={q: |.q.|=1};
  hence W-bound(P)=-1 by Lm4;
  proj1.:P=[.-1,1.] by A2,Lm3;
  then (proj1|P).:P=[.-1,1.] by RELAT_1:162;
  then sup ((proj1|P).:the carrier of ((TOP-REAL 2)|P))=1 by A1,
  JORDAN5A:20;
  then sup (proj1|P)=1 by PSCOMP_1:def 21;
  hence E-bound P=1 by PSCOMP_1:def 32;
  proj2.:P=[.-1,1.] by A2,Lm3;
  then
A3: (proj2|P).:P=[.-1,1.] by RELAT_1:162;
  then lower_bound ((proj2|P).:P)=-1 by JORDAN5A:20;
  then inf (proj2|P)=-1 by A1,PSCOMP_1:def 20;
  hence S-bound P=-1 by PSCOMP_1:def 33;
  sup ((proj2|P).:P)=1 by A3,JORDAN5A:20;
  then sup (proj2|P)=1 by A1,PSCOMP_1:def 21;
  hence thesis by PSCOMP_1:def 31;
end;

theorem Th32:
  for P being compact non empty Subset of TOP-REAL 2 st P={q: |.q
  .|=1} holds W-min(P)=|[-1,0]|
proof
  let P be compact non empty Subset of TOP-REAL 2;
A1: the carrier of ((TOP-REAL 2)|P) = P by PRE_TOPC:29;
  assume
A2: P={q: |.q.|=1};
  then
A3: W-bound P=-1 by Lm4;
  proj2.:P=[.-1,1.] by A2,Lm3;
  then
A4: (proj2|P).:P=[.-1,1.] by RELAT_1:162;
  then sup ((proj2|P).:P)=1 by JORDAN5A:20;
  then sup (proj2|P)=1 by A1,PSCOMP_1:def 21;
  then N-bound P=1 by PSCOMP_1:def 31;
  then
A5: NW-corner P=|[-1,1]| by A3,PSCOMP_1:def 35;
  lower_bound ((proj2|P).:P)=-1 by A4,JORDAN5A:20;
  then inf (proj2|P)=-1 by A1,PSCOMP_1:def 20;
  then S-bound P=-1 by PSCOMP_1:def 33;
  then
A6: SW-corner P=|[-1,-1]| by A3,PSCOMP_1:def 34;
A7: LSeg(SW-corner P, NW-corner P)/\P c= {|[-1,0]|}
  proof
    let x be set;
    assume
A8: x in LSeg(SW-corner P, NW-corner P)/\P;
    then
A9: x in { (1-l)*(SW-corner P) + l*(NW-corner P) where l is Real: 0 <= l
    & l <= 1 } by XBOOLE_0:def 4;
    x in P by A8,XBOOLE_0:def 4;
    then
A10: ex q2 being Point of TOP-REAL 2 st q2=x & |.q2.|=1 by A2;
    consider l being Real such that
A11: x=(1-l)*(SW-corner P)+l*(NW-corner P) and
    0<=l and
    l<=1 by A9;
    reconsider q3=x as Point of TOP-REAL 2 by A11;
    x=|[(1-l)*(-1),(1-l)*(-1)]|+(l)*|[-1,1]| by A6,A5,A11,EUCLID:62;
    then x=|[(1-l)*(-1),(1-l)*(-1)]|+|[(l)*(-1),(l)*1]| by EUCLID:62;
    then
A12: x=|[(1-l)*(-1)+(l)*(-1),(1-l)*(-1)+(l)*1]| by EUCLID:60;
    then q3`1=-1 by EUCLID:56;
    then
A13: 1=sqrt((-1)^2+(q3`2)^2) by A10,JGRAPH_3:10
      .=sqrt(1+(q3`2)^2);
    now
      assume
      (q3`2)^2>0;
      then 1<1+(q3`2)^2 by XREAL_1:31;
      hence contradiction by A13,SQUARE_1:83,95;
    end;
    then (q3`2)^2=0 by XREAL_1:65;
    then
A14: q3`2=0 by XCMPLX_1:6;
    q3`2=(1-l)*(-1)+l by A12,EUCLID:56;
    hence thesis by A12,A14,TARSKI:def 1;
  end;
  {|[-1,0]|} c= LSeg(SW-corner P, NW-corner P)/\P
  proof
    set q=|[-1,0]|;
    let x be set;
    assume
    x in {|[-1,0]|};
    then
A15: x=|[-1,0]| by TARSKI:def 1;
    q`2=0 & q`1=-1 by EUCLID:56;
    then |.q.|=sqrt((-1)^2+0^2) by JGRAPH_3:10
      .=1 by SQUARE_1:83;
    then
A16: x in P by A2,A15;
    q=|[(1/2)*(-1)+(1/2)*(-1),(1/2)*(-1)+(1/2)*1]|;
    then q=|[(1/2)*(-1),(1/2)*(-1)]|+|[(1/2)*(-1),(1/2)*1]| by EUCLID:60;
    then q=|[(1/2)*(-1),(1/2)*(-1)]|+(1/2)*|[-1,1]| by EUCLID:62;
    then q=(1/2)*|[-1,-1]|+(1-(1/2))*|[-1,1]| by EUCLID:62;
    then x in LSeg(SW-corner P, NW-corner P) by A6,A5,A15;
    hence thesis by A16,XBOOLE_0:def 4;
  end;
  then LSeg(SW-corner P, NW-corner P)/\P={|[-1,0]|} by
  A7,XBOOLE_0:def 10;
  then
A17: W-most P={|[-1,0]|} by PSCOMP_1:def 38;
  (proj2|W-most P).:the carrier of ((TOP-REAL 2)|(W-most P)) =(proj2|(
  W-most P)).:(W-most P) by PRE_TOPC:29
    .=Im(proj2,|[-1,0]|) by A17,RELAT_1:162
    .={proj2.(|[-1,0]|)} by SETWISEO:13
    .={(|[-1,0]|)`2} by PSCOMP_1:def 29
    .={0} by EUCLID:56;
  then
  lower_bound ((proj2|W-most P).:the carrier of ((TOP-REAL 2)|(W-most P))
  ) =0 by SEQ_4:22;
  then inf (proj2|W-most P)=0 by PSCOMP_1:def 20;
  hence thesis by A3,PSCOMP_1:def 42;
end;

theorem Th33:
  for P being compact non empty Subset of TOP-REAL 2 st P={q: |.q
  .|=1} holds E-max(P)=|[1,0]|
proof
  let P be compact non empty Subset of TOP-REAL 2;
A1: the carrier of ((TOP-REAL 2)|P) =P by PRE_TOPC:29;
  assume
A2: P={q: |.q.|=1};
  then
A3: E-bound P=1 by Th31;
  proj2.:P=[.-1,1.] by A2,Lm3;
  then
A4: (proj2|P).:P=[.-1,1.] by RELAT_1:162;
  then sup ((proj2|P).:P)=1 by JORDAN5A:20;
  then sup (proj2|P)=1 by A1,PSCOMP_1:def 21;
  then N-bound P=1 by PSCOMP_1:def 31;
  then
A5: NE-corner P=|[1,1]| by A3,PSCOMP_1:def 36;
  lower_bound ((proj2|P).:P)=-1 by A4,JORDAN5A:20;
  then inf (proj2|P)=-1 by A1,PSCOMP_1:def 20;
  then S-bound P=-1 by PSCOMP_1:def 33;
  then
A6: SE-corner P=|[1,-1]| by A3,PSCOMP_1:def 37;
A7: LSeg(SE-corner P, NE-corner P)/\P c= {|[1,0]|}
  proof
    let x be set;
    assume
A8: x in LSeg(SE-corner P, NE-corner P)/\P;
    then
A9: x in { (1-l)*(SE-corner P) + l*(NE-corner P) where l is Real: 0 <= l
    & l <= 1 } by XBOOLE_0:def 4;
    x in P by A8,XBOOLE_0:def 4;
    then
A10: ex q2 being Point of TOP-REAL 2 st q2=x & |.q2.|=1 by A2;
    consider l being Real such that
A11: x=(1-l)*(SE-corner P)+l*(NE-corner P) and
    0<=l and
    l<=1 by A9;
    reconsider q3=x as Point of TOP-REAL 2 by A11;
    x=|[(1-l)*(1),(1-l)*(-1)]|+(l)*|[1,1]| by A6,A5,A11,EUCLID:62;
    then x=|[(1-l)*(1),(1-l)*(-1)]|+|[(l)*(1),(l)*1]| by EUCLID:62;
    then
A12: x=|[((1-l)+l)*(1),(1-l)*(-1)+(l)*1]| by EUCLID:60;
    then
A13: q3`1=1 by EUCLID:56;
    now
      assume
      (q3`2)^2>0;
      then 1^2<1+(q3`2)^2 by XREAL_1:31;
      hence contradiction by A13,A10,JGRAPH_3:10;
    end;
    then (q3`2)^2=0 by XREAL_1:65;
    then
A14: q3`2=0 by XCMPLX_1:6;
    q3`2=(1-l)*(-1)+l by A12,EUCLID:56;
    hence thesis by A12,A14,TARSKI:def 1;
  end;
  {|[1,0]|} c= LSeg(SE-corner P, NE-corner P)/\P
  proof
    set q=|[1,0]|;
    let x be set;
    assume
    x in {|[1,0]|};
    then
A15: x=|[1,0]| by TARSKI:def 1;
    q`2=0 & q`1=1 by EUCLID:56;
    then |.q.|=sqrt((1)^2+0^2) by JGRAPH_3:10
      .=1 by SQUARE_1:83;
    then
A16: x in P by A2,A15;
    q=|[(1/2)*(1)+(1/2)*(1),(1/2)*(-1)+(1/2)*1]|;
    then q=|[(1/2)*(1),(1/2)*(-1)]|+|[(1/2)*(1),(1/2)*1]| by EUCLID:60;
    then q=|[(1/2)*(1),(1/2)*(-1)]|+(1/2)*|[1,1]| by EUCLID:62;
    then q=(1/2)*|[1,-1]|+(1-(1/2))*|[1,1]| by EUCLID:62;
    then x in LSeg(SE-corner P, NE-corner P) by A6,A5,A15;
    hence thesis by A16,XBOOLE_0:def 4;
  end;
  then LSeg(SE-corner P, NE-corner P)/\P={|[1,0]|} by
  A7,XBOOLE_0:def 10;
  then
A17: E-most P={|[1,0]|} by PSCOMP_1:def 40;
  (proj2|E-most P).:the carrier of ((TOP-REAL 2)|(E-most P)) =(proj2|(
  E-most P)).:(E-most P) by PRE_TOPC:29
    .=Im(proj2,|[1,0]|) by A17,RELAT_1:162
    .={proj2.(|[1,0]|)} by SETWISEO:13
    .={(|[1,0]|)`2} by PSCOMP_1:def 29
    .={0} by EUCLID:56;
  then sup ((proj2|E-most P).:the carrier of ((TOP-REAL 2)|(E-most P))) =0 by
  SEQ_4:22;
  then sup (proj2|E-most P)=0 by PSCOMP_1:def 21;
  hence thesis by A3,PSCOMP_1:def 46;
end;

theorem
  for f being Function of TOP-REAL 2,R^1 st (for p being Point of
  TOP-REAL 2 holds f.p=proj1.p) holds f is continuous
proof
  let f be Function of TOP-REAL 2,R^1;
  assume
A1: for p being Point of TOP-REAL 2 holds f.p=proj1.p;
  reconsider f as Function of the TopStruct of TOP-REAL 2, R^1;
  (TOP-REAL 2)|([#](TOP-REAL 2))=the TopStruct of TOP-REAL 2 by TSEP_1:100;
  then f is continuous by A1,JGRAPH_2:39;
  hence thesis by PRE_TOPC:62;
end;

theorem Th35:
  for f being Function of TOP-REAL 2,R^1 st (for p being Point of
  TOP-REAL 2 holds f.p=proj2.p) holds f is continuous
proof
  let f be Function of TOP-REAL 2,R^1;
  assume
A1: for p being Point of TOP-REAL 2 holds f.p=proj2.p;
  reconsider f as Function of the TopStruct of TOP-REAL 2, R^1;
  (TOP-REAL 2)|([#](TOP-REAL 2))=the TopStruct of TOP-REAL 2 by TSEP_1:100;
  then f is continuous by A1,JGRAPH_2:40;
