            end;
            case
A145:         q3`1=-1;
A146:         0 in dom f2 by A103,XXREAL_1:1;
              q3=|[-1,0]| by A141,A145,EUCLID:57
                .=W-min(P) by A3,Th32;
              hence contradiction by A87,A92,A95,A135,A138,A139,A146,
