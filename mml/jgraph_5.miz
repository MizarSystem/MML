A134:     g.r1=0 and
A135:     t<r1 and
A136:     r1<r by A129,A133,A130,A132,A131,TOPREAL5:14;
          r1 in {r4 where r4 is Real: t<=r4 & r4<=r} by A135,A136;
          then
A137:     r1 in B by RCOMP_1:def 1;
          r1<1 by A109,A136,XXREAL_0:2;
          then r1 in {r2 where r2 is Real: 0<=r2 & r2<=1} by A95,A135;
          then
A138:     r1 in dom f2 by A103,RCOMP_1:def 1;
          then f2.r1 in rng f2 by FUNCT_1:def 5;
          then f2.r1 in R by A91;
          then f2.r1 in P by A27;
          then consider q3 being Point of TOP-REAL 2 such that
A139:     q3=f2.r1 and
A140:     |.q3.|=1 by A3;
A141:     q3`2=h2.(f2.r1) by A139,PSCOMP_1:def 29
            .=(h2*f2).r1 by A138,FUNCT_1:23
            .=0 by A134,A137,FUNCT_1:72;
          then
A142:     1^2=(q3`1)^2 +0^2 by A140,JGRAPH_3:10
            .=(q3`1)^2;
          now
            per cases by A142,SQUARE_1:110;
            case
A143:         q3`1=1;
A144:         1 in dom f2 by A103,XXREAL_1:1;
              q3=|[1,0]| by A141,A143,EUCLID:57
                .=E-max(P) by A3,Th33;
              hence contradiction by A88,A92,A109,A136,A138,A139,A144,
