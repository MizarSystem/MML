Lm8: (|[-1,0]|)`2=0 by EUCLID:56;

Lm9: (|[1,0]|)`1 =1 & (|[1,0]|)`2=0 by EUCLID:56;
Lm10: (|[0,-1]|)`1 =0 by EUCLID:56;
Lm11: (|[0,-1]|)`2=-1 by EUCLID:56;
Lm12: (|[0,1]|)`1 =0 by EUCLID:56;
