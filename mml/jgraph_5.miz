A10: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then
A11: Upper_Arc(P) /\ P4={W-min(P),E-max(P)} by JORDAN6:def 9;
A12: W-min(P)=|[-1,0]| by A1,Th32;
A13: p2 in Lower_Arc(P) by A3,A5,A8;
A14: Lower_Arc(P) is_an_arc_of E-max(P),W-min(P) by A10,JORDAN6:def 9;
  for g being Function of I[01], (TOP-REAL 2)|P4, s1, s2 being Real st g
is being_homeomorphism & g.0 = E-max(P) & g.1 = W-min(P) & g.s1 = p1 & 0 <= s1
  & s1 <= 1 & g.s2 = p2 & 0 <= s2 & s2 <= 1 holds s1 <= s2
  proof
    W-min(P) in {W-min(P),E-max(P)} by TARSKI:def 2;
    then
A15: W-min(P) in Lower_Arc(P) by A11,XBOOLE_0:def 4;
    set K0=Lower_Arc(P);
    reconsider g0=proj1 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
    reconsider g2=g0|K0 as Function of (TOP-REAL 2)|K0,R^1 by PRE_TOPC:30;
    Closed-Interval-TSpace(-1,1) =TopSpaceMetr(Closed-Interval-MSpace(-1,
    1)) by TOPMETR:def 8;
    then
A16: Closed-Interval-TSpace(-1,1) is T_2 by PCOMPS_1:38;
    reconsider g3=g2 as continuous Function of (TOP-REAL 2)|K0,
    Closed-Interval-TSpace(-1,1) by A1,Lm5;
    let g be Function of I[01], (TOP-REAL 2)|P4, s1, s2 be Real;
    assume that
A17: g is being_homeomorphism and
    g.0 = E-max(P) and
A18: g.1 = W-min(P) and
A19: g.s1 = p1 and
A20: 0 <= s1 & s1 <= 1 and
A21: g.s2 = p2 and
A22: 0 <= s2 & s2 <= 1;
A23: s2 in [.0,1.] by A22,XXREAL_1:1;
    reconsider h=g3*g as Function of Closed-Interval-TSpace(0,1),
    Closed-Interval-TSpace(-1,1) by TOPMETR:27;
A24: dom g3=[#]((TOP-REAL 2)|K0) & rng g3=[#](Closed-Interval-TSpace(-1,1)
    ) by A1,Lm5,FUNCT_2:def 1;
    g3 is one-to-one & K0 is non empty compact by A1,A14,Lm5,JORDAN5A:1;
    then g3 is being_homeomorphism by A24,A16,COMPTS_1:26;
    then
A25: h is being_homeomorphism by A17,TOPMETR:27,TOPS_2:71;
A26: dom g=[#](I[01]) by A17,TOPS_2:def 5
      .=[.0,1.] by BORSUK_1:83;
    then
A27: 1 in dom g by XXREAL_1:1;
A28: -1=(|[-1,0]|)`1 by EUCLID:56
      .=proj1.(|[-1,0]|) by PSCOMP_1:def 28
      .=g3.(|[-1,0]|) by A12,A15,FUNCT_1:72
      .= h.1 by A12,A18,A27,FUNCT_1:23;
A29: s1 in [.0,1.] by A20,XXREAL_1:1;
A30: p2`1=g0.p2 by PSCOMP_1:def 28
      .=g3.p2 by A13,FUNCT_1:72
      .= h.s2 by A21,A26,A23,FUNCT_1:23;
    p1`1=g0.p1 by PSCOMP_1:def 28
      .=g3.p1 by A9,FUNCT_1:72
      .= h.s1 by A19,A26,A29,FUNCT_1:23;
    hence thesis by A7,A25,A29,A23,A28,A30,Th12;
  end;
  then
A31: LE p1,p2,Lower_Arc(P),E-max(P),W-min(P) by A9,A13,JORDAN5C:def 3;
  p1 in Lower_Arc(P) & p2 in Lower_Arc(P) by A2,A3,A4,A5,A8;
  hence thesis by A6,A31,JORDAN6:def 10;
end;

theorem Th60:
  for cn being Real,q being Point of TOP-REAL 2 st -1<cn & cn<1 &
q`2<=0 holds for p being Point of TOP-REAL 2 st p=(cn-FanMorphS).q holds p`2<=0
proof
  let cn be Real,q be Point of TOP-REAL 2;
  assume that
A1: -1<cn and
A2: cn<1 and
A3: q`2<=0;
  let p be Point of TOP-REAL 2;
  assume
A4: p=(cn-FanMorphS).q;
  per cases by A3;
  suppose
A5: q`2<0;
    now
      per cases;
      case
        q`1/|.q.|<cn;
        hence thesis by A1,A4,A5,JGRAPH_4:145;
      end;
      case
        q`1/|.q.|>=cn;
        hence thesis by A2,A4,A5,JGRAPH_4:144;
      end;
    end;
    hence thesis;
  end;
  suppose
    q`2=0;
    hence thesis by A4,JGRAPH_4:120;
  end;
end;

theorem Th61:
  for cn being Real,p1,p2,q1,q2 being Point of TOP-REAL 2, P being
compact non empty Subset of TOP-REAL 2 st -1<cn & cn<1 & P={p where p is Point
of TOP-REAL 2: |.p.|=1} & LE p1,p2,P & q1=(cn-FanMorphS).p1 & q2=(cn-FanMorphS)
  .p2 holds LE q1,q2,P
proof
  let cn be Real,p1,p2,q1,q2 be Point of TOP-REAL 2, P be compact non empty
  Subset of TOP-REAL 2;
  assume that
A1: -1<cn & cn<1 and
A2: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A3: LE p1,p2,P and
A4: q1=(cn-FanMorphS).p1 and
A5: q2=(cn-FanMorphS).p2;
A6: P is being_simple_closed_curve by A2,JGRAPH_3:36;
  W-min(P)=|[-1,0]| by A2,Th32;
  then
A7: (W-min(P))`2=0 by EUCLID:56;
  then
A8: (cn-FanMorphS).(W-min(P))=W-min(P) by JGRAPH_4:120;
  p2 in the carrier of TOP-REAL 2;
  then
A9: p2 in dom ((cn-FanMorphS)) by FUNCT_2:def 1;
  W-min(P) in the carrier of TOP-REAL 2;
  then
A10: W-min(P) in dom ((cn-FanMorphS)) by FUNCT_2:def 1;
A11: Lower_Arc(P) c= P by A2,Th36;
A12: (cn-FanMorphS) is one-to-one by A1,JGRAPH_4:140;
A13: Upper_Arc(P) c= P by A2,Th36;
A14: now
    per cases by A3,JORDAN6:def 10;
    case
      p1 in Upper_Arc(P);
      hence p1 in P by A13;
    end;
    case
      p1 in Lower_Arc(P);
      hence p1 in P by A11;
    end;
  end;
A15: now
    assume
A16: q2=W-min(P);
    then p2=W-min(P) by A5,A8,A10,A9,A12,FUNCT_1:def 8;
    then LE p2,p1,P by A6,A14,JORDAN7:3;
    then
A17: q1=q2 by A2,A3,A4,A5,JGRAPH_3:36,JORDAN6:72;
    W-min(P) in Lower_Arc(P) by A6,JORDAN7:1;
    then LE q1,q2,P by A2,A11,A16,A17,JGRAPH_3:36,JORDAN6:71;
    hence q1 in Upper_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) or q1 in
Upper_Arc(P) & q2 in Upper_Arc(P) & LE q1,q2,Upper_Arc(P),W-min(P),E-max(P) or
q1 in Lower_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) & LE q1,q2,Lower_Arc(P
    ),E-max(P),W-min(P) by JORDAN6:def 10;
  end;
A18: Upper_Arc(P)={p where p is Point of TOP-REAL 2:p in P & p`2>=0} by A2,
  Th37;
A19: Lower_Arc(P)={p where p is Point of TOP-REAL 2:p in P & p`2<=0} by A2,
  Th38;
  per cases by A3,JORDAN6:def 10;
  suppose
A20: p1 in Upper_Arc(P) & p2 in Lower_Arc(P)& not p2=W-min(P);
A21: |.q2.|=|.p2.| by A5,JGRAPH_4:135;
A22: ex p9 being Point of TOP-REAL 2 st p9=p2 & p9 in P & p9 `2<=0 by A19,
    A20;
    then ex p10 being Point of TOP-REAL 2 st p10=p2 & |.p10.|=1 by A2;
    then
A23: q2 in P by A2,A21;
A24: ex p8 being Point of TOP-REAL 2 st p8=p1 & p8 in P & p8 `2>=0 by A18,
    A20;
    q2`2<=0 by A1,A5,A22,Th60;
    hence q1 in Upper_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) or q1 in
Upper_Arc(P) & q2 in Upper_Arc(P) & LE q1,q2,Upper_Arc(P),W-min(P),E-max(P) or
q1 in Lower_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) & LE q1,q2,Lower_Arc(P
    ),E-max(P),W-min(P) by A4,A19,A15,A20,A24,A23, JGRAPH_4:120;
  end;
  suppose
A25: p1 in Upper_Arc(P) & p2 in Upper_Arc(P) & LE p1,p2,Upper_Arc(P),
    W-min(P),E-max(P);
    then ex p8 being Point of TOP-REAL 2 st p8=p1 & p8 in P & p8 `2>=0 by A18;
    then
A26: p1=(cn-FanMorphS).p1 by JGRAPH_4:120;
    ex p9 being Point of TOP-REAL 2 st p9=p2 & p9 in P & p9 `2>=0 by A18,
    A25;
    hence q1 in Upper_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) or q1 in
Upper_Arc(P) & q2 in Upper_Arc(P) & LE q1,q2,Upper_Arc(P),W-min(P),E-max(P) or
q1 in Lower_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) & LE q1,q2,Lower_Arc(P
    ),E-max(P),W-min(P) by A4,A5,A25,A26,JGRAPH_4:120;
  end;
  suppose
A27: p1 in Lower_Arc(P) & p2 in Lower_Arc(P)& not p2=W-min(P) & LE p1,
    p2,Lower_Arc(P),E-max(P),W-min(P) & not p1 in Upper_Arc(P);
    then
A28: ex p8 being Point of TOP-REAL 2 st p8=p1 & p8 in P & p8 `2<=0 by A19;
    then
A29: ex p10 being Point of TOP-REAL 2 st p10=p1 & |.p10.|=1 by A2;
A30: ex p9 being Point of TOP-REAL 2 st p9=p2 & p9 in P & p9 `2<=0 by A19,
    A27;
    then
A31: ex p11 being Point of TOP-REAL 2 st p11=p2 & |.p11.|=1 by A2;
A32: q2`2<=0 by A1,A5,A30,Th60;
A33: |.q2.|=|.p2.| by A5,JGRAPH_4:135;
    then
A34: q2 in P by A2,A31;
A35: q1`2<=0 by A1,A4,A28,Th60;
A36: |.q1.|=|.p1.| by A4,JGRAPH_4:135;
    then
A37: q1 in P by A2,A29;
    now
      per cases;
      case
A38:    p1=W-min(P);
        then p1=(cn-FanMorphS).p1 by A7,JGRAPH_4:120;
        then LE q1,q2,P by A4,A6,A34,A38,JORDAN7:3;
        hence
        q1 in Upper_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) or q1 in
Upper_Arc(P) & q2 in Upper_Arc(P) & LE q1,q2,Upper_Arc(P),W-min(P),E-max(P) or
q1 in Lower_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) & LE q1,q2,Lower_Arc(P
        ),E-max(P),W-min(P) by JORDAN6:def 10;
      end;
      case
A39:    p1<>W-min(P);
        now
          per cases by A2,A3,A28,A39,Th51;
          case
A40:        p1`1=p2`1;
A41:        p2=|[p2`1,p2`2]| by EUCLID:57;
A42:        now
              assume
A43:          p1`2=-p2`2;
              then p2`2=0 by A28,A30,XREAL_1:60;
              hence p1=p2 by A40,A41,A43,EUCLID:57;
            end;
            (p1`1)^2+(p1`2)^2=1^2 by A29,JGRAPH_3:10
              .=(p1`1)^2+(p2`2)^2 by A31,A40,JGRAPH_3:10;
            then
A44:        p1`2=p2`2 or p1`2=-p2`2 by SQUARE_1:109;
            p1=|[p1`1,p1`2]| by EUCLID:57;
            then LE q1,q2,P by A2,A4,A5,A34,A40,A44,A41,A42,JGRAPH_3:36 ,
            JORDAN6:71;
            hence q1 in Upper_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) or
q1 in Upper_Arc(P) & q2 in Upper_Arc(P) & LE q1,q2,Upper_Arc(P),W-min(P),E-max(
P) or q1 in Lower_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) & LE q1,q2,
            Lower_Arc(P),E-max(P),W-min(P) by JORDAN6:def 10;
          end;
          case
            p1`1>p2`1;
            then p1`1/|.p1.|>p2`1/|.p2.| by A29,A31;
            then
A45:        q1`1/|.q1.|>=q2`1/|.q2.| by A1,A4,A5,A28,A30,A29,A31,
            Th30;
            q2<> W-min(P) by A5,A8,A10,A9,A12,A27,FUNCT_1:def 8;
            then LE q1,q2,P by A2,A36,A33,A35,A32,A29,A31,A37,A34,
            A45,Th59;
            hence q1 in Upper_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) or
q1 in Upper_Arc(P) & q2 in Upper_Arc(P) & LE q1,q2,Upper_Arc(P),W-min(P),E-max(
P) or q1 in Lower_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) & LE q1,q2,
            Lower_Arc(P),E-max(P),W-min(P) by JORDAN6:def 10;
          end;
        end;
        hence
        q1 in Upper_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) or q1 in
Upper_Arc(P) & q2 in Upper_Arc(P) & LE q1,q2,Upper_Arc(P),W-min(P),E-max(P) or
q1 in Lower_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) & LE q1,q2,Lower_Arc(P
        ),E-max(P),W-min(P);
      end;
    end;
    hence q1 in Upper_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) or q1 in
Upper_Arc(P) & q2 in Upper_Arc(P) & LE q1,q2,Upper_Arc(P),W-min(P),E-max(P) or
q1 in Lower_Arc(P) & q2 in Lower_Arc(P)& not q2=W-min(P) & LE q1,q2,Lower_Arc(P
    ),E-max(P),W-min(P);
  end;
end;

theorem Th62:
  for p1,p2,p3,p4 being Point of TOP-REAL 2, P being compact non
  empty Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} &
LE p1,p2,P & LE p2,p3,P & LE p3,p4,P & p1`1<0 & p1`2>=0 & p2`1<0 & p2`2>=0 & p3
`1<0 & p3`2>=0 & p4`1<0 & p4`2>=0 ex f being Function of TOP-REAL 2,TOP-REAL 2,
  q1,q2,q3,q4 being Point of TOP-REAL 2 st f is being_homeomorphism & (for q
being Point of TOP-REAL 2 holds |.(f.q).|=|.q.|)& q1=f.p1 & q2=f.p2 & q3=f.p3 &
q4=f.p4 & q1`1<0 & q1`2<0 & q2`1<0 & q2`2<0 & q3`1<0 & q3`2<0 & q4`1<0 & q4`2<0
  & LE q1,q2,P & LE q2,q3,P & LE q3,q4,P
proof
  let p1,p2,p3,p4 be Point of TOP-REAL 2, P be compact non empty Subset of
  TOP-REAL 2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: LE p2,p3,P and
A4: LE p3,p4,P and
A5: p1`1<0 and
A6: p1`2>=0 and
A7: p2`1<0 and
A8: p2`2>=0 and
A9: p3`1<0 and
A10: p3`2>=0 and
A11: p4`1<0 and
A12: p4`2>=0;
  consider r being real number such that
A13: p4`1<r and
A14: r<0 by A11,XREAL_1:7;
  reconsider r1=r as Real by XREAL_0:def 1;
  set s=sqrt(1-r1^2);
A15: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then p4 in P by A4,JORDAN7:5;
  then
A16: ex p being Point of TOP-REAL 2 st p=p4 & |.p.|=1 by A1;
  then -1<=p4`1 by Th3;
  then -1<=r1 by A13,XXREAL_0:2;
  then r1^2<=1^2 by A14,SQUARE_1:119;
  then
A17: 1-r1^2>=0 by XREAL_1:50;
  then
A18: s^2=1-r1^2 by SQUARE_1:def 4;
  then
A19: 1-s^2+s^2>0+s^2 by A14,SQUARE_1:74,XREAL_1:10;
  then
A20: -1<s by SQUARE_1:122;
A21: s<1 by A19,SQUARE_1:122;
  then consider f1 being Function of TOP-REAL 2,TOP-REAL 2 such that
A22: f1=s-FanMorphW and
A23: f1 is being_homeomorphism by A20,JGRAPH_4:48;
  set q11=f1.p1, q22=f1.p2, q33=f1.p3, q44=f1.p4;
A24: s>=0 by A17,SQUARE_1:def 4;
  p3 in P by A3,A15,JORDAN7:5;
  then
A25: ex p33 being Point of TOP-REAL 2 st p33=p3 & |.p33.|=1 by A1;
  then p3`2/|.p3.|<p4`2/|.p4.| or p3=p4 by A1,A4,A11,A12,A16,Th49;
  then
A26: q33`2/|.q33.|<q44`2/|.q44.| or p3=p4 by A9,A11,A20,A21,A22,
  JGRAPH_4:53;
  (p4`1)^2>r1^2 by A13,A14,SQUARE_1:114;
  then
A27: 1-(p4`1)^2<1-r1^2 by XREAL_1:17;
A28: p3`1<p4`1 or p3=p4 by A1,A4,A9,A10,A12,Th49;
  then -(p3`1)>= -(p4`1) by XREAL_1:26;
  then (-(p3`1))^2>=(-(p4`1))^2 by A11,SQUARE_1:77;
  then 1-((p3`1))^2<=1-((p4`1))^2 by XREAL_1:12;
  then
A29: 1-(p3`1)^2< s^2 by A27,A18,XXREAL_0:2;
  p2`1<p3`1 or p2=p3 by A1,A3,A7,A8,A10,Th49;
  then
A30: p2`1<=p4`1 by A28,XXREAL_0:2;
  then -(p2`1)>= -(p4`1) by XREAL_1:26;
  then (-(p2`1))^2>=(-(p4`1))^2 by A11,SQUARE_1:77;
  then 1-((p2`1))^2<=1-((p4`1))^2 by XREAL_1:12;
  then
A31: 1-(p2`1)^2< s^2 by A27,A18,XXREAL_0:2;
  p1`1<p2`1 or p1=p2 by A1,A2,A5,A6,A8,Th49;
  then p1`1<=p4`1 by A30,XXREAL_0:2;
  then -(p1`1)>= -(p4`1) by XREAL_1:26;
  then (-(p1`1))^2>=(-(p4`1))^2 by A11,SQUARE_1:77;
  then 1-((p1`1))^2<=1-((p4`1))^2 by XREAL_1:12;
  then
A32: 1-(p1`1)^2< s^2 by A27,A18,XXREAL_0:2;
  1^2=(p3`1)^2+(p3`2)^2 by A25,JGRAPH_3:10;
  then
A33: p3`2/|.p3.|<s by A25,A24,A29,SQUARE_1:118;
  then
A34: q33`1<0 by A9,A20,A22,JGRAPH_4:50;
  p2 in P by A2,A15,JORDAN7:5;
  then
A35: ex p22 being Point of TOP-REAL 2 st p22=p2 & |.p22.|=1 by A1;
  then
A36: |.q22.|=1 by A22,JGRAPH_4:40;
  then
A37: q22 in P by A1;
  p2`2/|.p2.|<p3`2/|.p3.| or p2=p3 by A1,A3,A9,A10,A35,A25,Th49;
  then
A38: q22`2/|.q22.|<q33`2/|.q33.| or p2=p3 by A7,A9,A20,A21,A22,
  JGRAPH_4
  :53;
A39: |.q33.|=1 by A25,A22,JGRAPH_4:40;
  then
A40: q33 in P by A1;
  1^2=(p2`1)^2+(p2`2)^2 by A35,JGRAPH_3:10;
  then
A41: p2`2/|.p2.|<s by A35,A24,A31,SQUARE_1:118;
  then
A42: q22`2<0 by A7,A20,A22,JGRAPH_4:50;
A43: q22`1<0 by A7,A20,A22,A41,JGRAPH_4:50;
  1^2=(p4`1)^2+(p4`2)^2 by A16,JGRAPH_3:10;
  then p4`2/|.p4.|<s by A27,A16,A18,A24,SQUARE_1:118;
  then
A44: q44`1<0 & q44`2<0 by A11,A20,A22,JGRAPH_4:50;
  p1 in P by A2,A15,JORDAN7:5;
  then
A45: ex p11 being Point of TOP-REAL 2 st p11=p1 & |.p11.|=1 by A1;
  then p1`2/|.p1.|<p2`2/|.p2.| or p1=p2 by A1,A2,A7,A8,A35,Th49;
  then
A46: q11`2/|.q11.|<q22`2/|.q22.| or p1=p2 by A5,A7,A20,A21,A22,
  JGRAPH_4
  :53;
  1^2=(p1`1)^2+(p1`2)^2 by A45,JGRAPH_3:10;
  then
A47: p1`2/|.p1.|<s by A45,A24,A32,SQUARE_1:118;
  then
A48: q11`1<0 by A5,A20,A22,JGRAPH_4:50;
A49: |.q11.|=1 by A45,A22,JGRAPH_4:40;
  then q11 in P by A1;
  then
A50: LE q11,q22,P by A1,A49,A36,A37,A48,A43,A42,A46,Th54;
A51: q22`1<0 & q22`2<0 by A7,A20,A22,A41,JGRAPH_4:50;
A52: q11`1<0 & q11`2<0 by A5,A20,A22,A47,JGRAPH_4:50;
A53: for q being Point of TOP-REAL 2 holds |.(f1.q).|=|.q.| by A22,JGRAPH_4:
  40;
  q33`1<0 & q33`2<0 by A9,A20,A22,A33,JGRAPH_4:50;
  then
A54: LE q22,q33,P by A1,A36,A37,A39,A40,A43,A38,Th54;
A55: q33`2<0 by A9,A20,A22,A33,JGRAPH_4:50;
A56: |.q44.|=1 by A16,A22,JGRAPH_4:40;
  then q44 in P by A1;
  then LE q33,q44,P by A1,A39,A40,A56,A34,A44,A26,Th54;
  hence thesis by A23,A53,A52,A51,A34,A55,A44,A50,A54;
end;

theorem Th63:
  for p1,p2,p3,p4 being Point of TOP-REAL 2, P being compact non
  empty Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} &
LE p1,p2,P & LE p2,p3,P & LE p3,p4,P & p1`2>=0 & p2`2>=0 & p3`2>=0 & p4`2>0 ex
f being Function of TOP-REAL 2,TOP-REAL 2, q1,q2,q3,q4 being Point of TOP-REAL
2 st f is being_homeomorphism & (for q being Point of TOP-REAL 2 holds |.(f.q)
.|=|.q.|)& q1=f.p1 & q2=f.p2 & q3=f.p3 & q4=f.p4 & q1`1<0 & q1`2>=0 & q2`1<0 &
q2`2>=0 & q3`1<0 & q3`2>=0 & q4`1<0 & q4`2>=0 & LE q1,q2,P & LE q2,q3,P & LE q3
  ,q4,P
proof
  let p1,p2,p3,p4 be Point of TOP-REAL 2, P be compact non empty Subset of
  TOP-REAL 2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: LE p2,p3,P and
A4: LE p3,p4,P and
A5: p1`2>=0 and
A6: p2`2>=0 and
A7: p3`2>=0 and
A8: p4`2>0;
A9: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then p4 in P by A4,JORDAN7:5;
  then
A10: ex p being Point of TOP-REAL 2 st p=p4 & |.p.|=1 by A1;
A11: now
    assume
    p4`1=1;
    then 1^2=1+(p4`2)^2 by A10,JGRAPH_3:10;
    hence contradiction by A8,XCMPLX_1:6;
  end;
  p4`1<=1 by A10,Th3;
  then p4`1<1 by A11,XXREAL_0:1;
  then consider r being real number such that
A12: p4`1<r and
A13: r<1 by XREAL_1:7;
  reconsider r1=r as Real by XREAL_0:def 1;
  -1<=p4`1 by A10,Th3;
  then
A14: -1<r1 by A12,XXREAL_0:2;
  then consider f1 being Function of TOP-REAL 2,TOP-REAL 2 such that
A15: f1=r1-FanMorphN and
A16: f1 is being_homeomorphism by A13,JGRAPH_4:81;
  set q11=f1.p1, q22=f1.p2, q33=f1.p3, q44=f1.p4;
A17: for q being Point of TOP-REAL 2 holds |.(f1.q).|=|.q.| by A15,JGRAPH_4:
  73;
A18: p3`1<p4`1 or p3=p4 by A1,A4,A8,Th50;
  then
A19: p3`1<r1 by A12,XXREAL_0:2;
  p3 in P by A3,A9,JORDAN7:5;
  then
A20: ex p33 being Point of TOP-REAL 2 st p33=p3 & |.p33.|=1 by A1;
  then p3`1/|.p3.|<p4`1/|.p4.| or p3=p4 by A1,A4,A8,A10,Th50;
  then
A21: q33`1/|.q33.|<q44`1/|.q44.| or p3=p4 by A7,A8,A10,A20,A13,A14,A15,
  Th24;
A22: p3`1/|.p3.|<r1 by A20,A12,A18,XXREAL_0:2;
  then
A23: q33`2>=0 by A7,A20,A13,A14,A15,Th23;
A24: p1`1<p2`1 or p1=p2 by A1,A2,A6,Th50;
  p4`1/|.p4.|<r1 by A10,A12;
  then
A25: q44`1<0 & q44`2>0 by A8,A14,A15,JGRAPH_4:83;
  p2 in P by A2,A9,JORDAN7:5;
  then
A26: ex p22 being Point of TOP-REAL 2 st p22=p2 & |.p22.|=1 by A1;
  then
A27: |.q22.|=1 by A15,JGRAPH_4:73;
  then
A28: q22 in P by A1;
A29: p2`1<p3`1 or p2=p3 by A1,A3,A7,Th50;
  then
A30: p2`1/|.p2.|<r1 by A26,A19,XXREAL_0:2;
  then
A31: q22`2>=0 by A6,A26,A13,A14,A15,Th23;
  p1 in P by A2,A9,JORDAN7:5;
  then
A32: ex p11 being Point of TOP-REAL 2 st p11=p1 & |.p11.|=1 by A1;
  then p1`1/|.p1.|<p2`1/|.p2.| or p1=p2 by A1,A2,A6,A26,Th50;
  then
A33: q11`1/|.q11.|<q22`1/|.q22.| or p1=p2 by A5,A6,A32,A26,A13,A14,A15,
  Th24;
  p2`1<r1 by A29,A19,XXREAL_0:2;
  then
A34: p1`1/|.p1.|<r1 by A32,A24,XXREAL_0:2;
  then
A35: q11`2>=0 by A5,A32,A13,A14,A15,Th23;
A36: q22`1<0 by A6,A26,A13,A14,A15,A30,Th23;
A37: |.q11.|=1 by A32,A15,JGRAPH_4:73;
  then q11 in P by A1;
  then
A38: LE q11,q22,P by A1,A37,A27,A28,A31,A36,A35,A33,Th56;
A39: |.q33.|=1 by A20,A15,JGRAPH_4:73;
  then
A40: q33 in P by A1;
A41: q33`1<0 by A7,A20,A13,A14,A15,A22,Th23;
A42: q22`1<0 & q22`2>=0 by A6,A26,A13,A14,A15,A30,Th23;
A43: q11`1<0 & q11`2>=0 or q11`1<0 & q11`2=0 by A5,A32,A13,A14,A15,A34,Th23;
A44: |.q44.|=1 by A10,A15,JGRAPH_4:73;
  then q44 in P by A1;
  then
A45: LE q33,q44,P by A1,A39,A40,A44,A25,A23,A21,Th56;
  p2`1/|.p2.|<p3`1/|.p3.| or p2=p3 by A1,A3,A7,A26,A20,Th50;
  then q22`1/|.q22.|<q33`1/|.q33.| or p2=p3 by A6,A7,A26,A20,A13,A14,A15,
  Th24;
  then LE q22,q33,P by A1,A27,A28,A39,A40,A31,A23,A41,Th56;
  hence thesis by A16,A17,A25,A43,A42,A38,A23,A41,A45;
end;

theorem Th64:
  for p1,p2,p3,p4 being Point of TOP-REAL 2, P being compact non
  empty Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} &
LE p1,p2,P & LE p2,p3,P & LE p3,p4,P & p1`2>=0 & p2`2>=0 & p3`2>=0 & p4`2>0 ex
f being Function of TOP-REAL 2,TOP-REAL 2, q1,q2,q3,q4 being Point of TOP-REAL
2 st f is being_homeomorphism & (for q being Point of TOP-REAL 2 holds |.(f.q)
  .|=|.q.|)& q1=f.p1 & q2=f.p2 & q3=f.p3 & q4=f.p4 & q1`1<0 & q1`2<0 & q2`1<0 &
q2`2<0 & q3`1<0 & q3`2<0 & q4`1<0 & q4`2<0 & LE q1,q2,P & LE q2,q3,P & LE q3,q4
  ,P
proof
  let p1,p2,p3,p4 be Point of TOP-REAL 2, P be compact non empty Subset of
  TOP-REAL 2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P & LE p2,p3,P & LE p3,p4,P & p1`2>=0 & p2`2>=0 & p3`2>=0 &
  p4`2>0;
  consider f being Function of TOP-REAL 2,TOP-REAL 2, q1,q2,q3,q4 being Point
  of TOP-REAL 2 such that
A3: f is being_homeomorphism and
A4: for q being Point of TOP-REAL 2 holds |.(f.q).|=|.q.| and
A5: q1=f.p1 & q2=f.p2 and
A6: q3=f.p3 & q4=f.p4 and
A7: q1`1<0 & q1`2>=0 & q2`1<0 & q2`2>=0 & q3`1<0 & q3`2>=0 & q4`1<0 & q4
  `2>=0 & LE q1,q2,P & LE q2,q3,P & LE q3,q4,P by A1,A2,Th63;
  consider f2 being Function of TOP-REAL 2,TOP-REAL 2, q1b,q2b,q3b,q4b being
  Point of TOP-REAL 2 such that
A8: f2 is being_homeomorphism and
A9: for q being Point of TOP-REAL 2 holds |.(f2.q).|=|.q.| and
A10: q1b=f2.q1 & q2b=f2.q2 and
A11: q3b=f2.q3 & q4b=f2.q4 and
A12: q1b`1<0 & q1b`2<0 & q2b`1<0 & q2b`2<0 & q3b`1<0 & q3b`2<0 & q4b`1<
  0 & q4b`2<0 & LE q1b,q2b,P & LE q2b,q3b,P & LE q3b,q4b,P by A1,A7,Th62;
  reconsider f3=f2*f as Function of TOP-REAL 2,TOP-REAL 2;
A13: f3 is being_homeomorphism by A3,A8,TOPS_2:71;
A14: dom f=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
  then
A15: f3.p3=q3b & f3.p4=q4b by A6,A11,FUNCT_1:23;
A16: for q being Point of TOP-REAL 2 holds |.(f3.q).|=|.q.|
  proof
    let q be Point of TOP-REAL 2;
    dom f=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
    then f3.q=f2.(f.q) by FUNCT_1:23;
    hence |.f3.q.|=|.(f.q).| by A9
      .=|.q.| by A4;
  end;
  f3.p1=q1b & f3.p2=q2b by A5,A10,A14,FUNCT_1:23;
  hence thesis by A12,A13,A16,A15;
end;

theorem Th65:
  for p1,p2,p3,p4 being Point of TOP-REAL 2, P being compact non
  empty Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} &
LE p1,p2,P & LE p2,p3,P & LE p3,p4,P & (p1`2>=0 or p1`1>=0)& (p2`2>=0 or p2`1>=
0) & (p3`2>=0 or p3`1>=0)& (p4`2>0 or p4`1>0) ex f being Function of TOP-REAL 2
,TOP-REAL 2, q1,q2,q3,q4 being Point of TOP-REAL 2 st f is being_homeomorphism
& (for q being Point of TOP-REAL 2 holds |.(f.q).|=|.q.|)& q1=f.p1 & q2=f.p2 &
q3=f.p3 & q4=f.p4 & q1`2>=0 & q2`2>=0 & q3`2>=0 & q4`2>0 & LE q1,q2,P & LE q2,
  q3,P & LE q3,q4,P
proof
  let p1,p2,p3,p4 be Point of TOP-REAL 2, P be compact non empty Subset of
  TOP-REAL 2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: LE p2,p3,P and
A4: LE p3,p4,P and
A5: p1`2>=0 or p1`1>=0 and
A6: p2`2>=0 or p2`1>=0 and
A7: p3`2>=0 or p3`1>=0 and
A8: p4`2>0 or p4`1>0;
A9: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then
A10: p4 in P by A4,JORDAN7:5;
  then
A11: ex p44 being Point of TOP-REAL 2 st p44=p4 & |.p44.|=1 by A1;
  then
A12: -1<=p4`2 by Th3;
  now
    assume
A13: p4`2=-1;
    1^2=(p4`1)^2+(p4`2)^2 by A11,JGRAPH_3:10
      .=(p4`1)^2+1 by A13;
    hence contradiction by A8,A13,XCMPLX_1:6;
  end;
  then p4`2> -1 by A12,XXREAL_0:1;
  then consider r being real number such that
A14: -1<r and
A15: r<p4`2 by XREAL_1:7;
  reconsider r1=r as Real by XREAL_0:def 1;
  p4`2<=1 by A11,Th3;
  then
A16: r1<1 by A15,XXREAL_0:2;
  then consider f1 being Function of TOP-REAL 2,TOP-REAL 2 such that
A17: f1=r1-FanMorphE and
A18: f1 is being_homeomorphism by A14,JGRAPH_4:112;
  set q11=f1.p1, q22=f1.p2, q33=f1.p3, q44=f1.p4;
A19: |.q44.|=1 by A11,A17,JGRAPH_4:104;
  then
A20: q44 in P by A1;
A21: p1 in P by A2,A9,JORDAN7:5;
  then
A22: ex p11 being Point of TOP-REAL 2 st p11=p1 & |.p11.|=1 by A1;
  then
A23: |.q11.|=1 by A17,JGRAPH_4:104;
  then
A24: q11 in P by A1;
A25: p3 in P by A3,A9,JORDAN7:5;
  then
A26: ex p33 being Point of TOP-REAL 2 st p33=p3 & |.p33.|=1 by A1;
  then
A27: |.q33.|=1 by A17,JGRAPH_4:104;
  then
A28: q33 in P by A1;
A29: p2 in P by A2,A9,JORDAN7:5;
  then
A30: ex p22 being Point of TOP-REAL 2 st p22=p2 & |.p22.|=1 by A1;
  then
A31: |.q22.|=1 by A17,JGRAPH_4:104;
  then
A32: q22 in P by A1;
  now
    per cases;
    case
A33:  p4`2<=0;
A34:  Upper_Arc(P) ={p7 where p7 is Point of TOP-REAL 2:p7 in P & p7`2>=0
      } by A1,Th37;
A35:  p4`2/|.p4.|>r1 by A11,A15;
      then
A36:  q44`1>0 by A8,A15,A17,A33,JGRAPH_4:113;
A37:  now
        set q8=|[sqrt(1-r1^2),r1]|;
        assume
A38:    q44`2=0;
        1^2=(q44`1)^2+(q44`2)^2 by A19,JGRAPH_3:10
          .=(q44`1)^2 by A38;
        then q44`1=-1 or q44`1=1 by SQUARE_1:110;
        then
A39:    q44=|[1,0]| by A8,A15,A17,A33,A35,A38,
        EUCLID:57 ,JGRAPH_4:113;
        set r8=f1.q8;
        1^2>r1^2 by A14,A16,SQUARE_1:120;
        then
A40:    1-r1^2>0 by XREAL_1:52;
A41:    q8`1=sqrt(1-r1^2) by EUCLID:56;
        then
A42:    q8`1>0 by A40,SQUARE_1:93;
        q8`2=r1 by EUCLID:56;
        then |.q8.|=sqrt((sqrt(1-r1^2))^2+r1^2)by A41,JGRAPH_3:10;
        then
A43:    |.q8.|=sqrt((1-r1^2)+r1^2) by A40,SQUARE_1:def 4
          .=1 by SQUARE_1:83;
        then
A44:    q8`2/|.q8.|=r1 by EUCLID:56;
        then
A45:    r8`2=0 by A17,A42,JGRAPH_4:118;
        |.r8.|=1 by A17,A43,JGRAPH_4:104;
        then 1^2=(r8`1)^2+(r8`2)^2 by JGRAPH_3:10
          .=(r8`1)^2 by A45;
        then r8`1=-1 or r8`1=1 by SQUARE_1:110;
        then
A46:    f1.(|[sqrt(1-r1^2),r1]|)=|[1,0]| by A17,A44,A42,A45, EUCLID:
        57,JGRAPH_4:118;
        f1 is one-to-one & dom f1=the carrier of TOP-REAL 2 by A14,A16,
        A17, FUNCT_2:def 1,JGRAPH_4:109;
        then p4=|[sqrt(1-r1^2),r1]| by A39,A46,FUNCT_1:def 8;
        hence contradiction by A15,EUCLID:56;
      end;
A47:  q44`2>=0 by A8,A15,A17,A33,A35,JGRAPH_4:113;
A48:  Lower_Arc(P) ={p7 where p7 is Point of TOP-REAL 2:p7 in P & p7`2<=0
      } by A1,Th38;
A49:  now
        per cases;
        case
A50:      p3`1<=0;
          then
A51:      q33=p3 by A17,JGRAPH_4:89;
A52:      now
            per cases by A50;
            case
A53:          p3`1=0;
A54:          now
                assume
                q33`2=-1;
                then -1>=p4`2 by A1,A4,A7,A8,A33,A51,Th53;
                hence contradiction by A14,A15,XXREAL_0:2;
              end;
              1^2=0^2+(q33`2)^2 by A26,A51,A53,JGRAPH_3:10
                .=(q33`2)^2;
              hence q33`2>=0 by A54,SQUARE_1:110;
            end;
            case
              p3`1<0;
              hence q33`2>=0 by A7,A17,JGRAPH_4:89;
            end;
          end;
          now
            per cases;
            case
A55:          p2<> W-min(P);
A56:          now
A57:            p3 in Upper_Arc(P) by A25,A34,A51,A52;
                assume
A58:            p2`2<0;
                then p2 in Lower_Arc(P) by A29,A48;
                then LE p3,p2,P by A55,A57,JORDAN6:def 10;
                hence contradiction by A1,A3,A51,A52,A58,JGRAPH_3:36 ,
                JORDAN6:72;
              end;
A59:          p2`1<=p3`1 by A1,A3,A51,A52,Th50;
              then
A60:          q22=p2 by A17,A50,JGRAPH_4:89;
              now
                per cases;
                case
A61:              p1<> W-min(P);
A62:              now
A63:                p2 in Upper_Arc(P) by A29,A34,A56;
                    assume
A64:                p1`2<0;
                    then p1 in Lower_Arc(P) by A21,A48;
                    then LE p2,p1,P by A61,A63,JORDAN6:def 10;
                    hence contradiction by A1,A2,A56,A64,JGRAPH_3:36 ,
                    JORDAN6:72;
                  end;
                  p1`1<=p2`1 by A1,A2,A56,Th50;
                  hence
                  q11`2>=0 & q22`2>=0 & q33`2>=0 & q44`2>0 & LE q11,q22,P
& LE q22,q33,P & LE q33,q44,P by A1,A2,A3,A17,A28,A20,A36,A47,A37,A51,A52,A56,
                  A59,A60,A62,Th57, JGRAPH_4:89;
                end;
                case
A65:              p1=W-min(P);
A66:              W-min(P)=|[-1,0]| by A1,Th32;
                  then p1`1=-1 by A65,EUCLID:56;
                  then p1=q11 by A17,JGRAPH_4:89;
                  hence
                  q11`2>=0 & q22`2>=0 & q33`2>=0 & q44`2>0 & LE q11,q22,P
& LE q22,q33,P & LE q33,q44,P by A1,A2,A3,A25,A17,A20,A36,A47,A37,A51,A52,A56,
                  A59,A65,A66,Th57,EUCLID:56 ,JGRAPH_4:89;
                end;
              end;
              hence q11`2>=0 & q22`2>=0 & q33`2>=0 & q44`2>0 & LE q11,q22,P &
              LE q22,q33,P & LE q33,q44,P;
            end;
            case
A67:          p2=W-min(P);
              W-min(P)=|[-1,0]| by A1,Th32;
              then
A68:          p2`1=-1 by A67,EUCLID:56;
              then p2=q22 & p1`1<=p2`1 by A1,A2,A6,A17,Th50,JGRAPH_4:89;
              hence q11`2>=0 & q22`2>=0 & q33`2>=0 & q44`2>0 & LE q11,q22,P &
LE q22,q33,P & LE q33,q44,P by A1,A2,A3,A5,A6,A14,A15,A17,A28,A20,A33,A36,A47,
              A37,A51,A52,A68,Th57, JGRAPH_4:89;
            end;
          end;
          hence q11`2>=0 & q22`2>=0 & q33`2>=0 & q44`2>0 & LE q11,q22,P & LE
          q22,q33,P & LE q33,q44,P;
        end;
        case
A69:      p3`1>0;
A70:      now
            per cases;
            case
A71:          p3<>p4;
A72:          now
A73:            LE p2,p4,P by A1,A3,A4,JGRAPH_3:36,JORDAN6:73;
                assume that
A74:            p2`1=0 and
A75:            p2`2=-1;
                p2`2<=p4`2 by A11,A75,Th3;
                then LE p4,p2,P by A1,A8,A29,A10,A33,A74,Th58;
                hence contradiction by A1,A8,A74,A75,A73,JGRAPH_3:36 ,
                JORDAN6:72;
              end;
              p3`2>p4`2 by A1,A4,A8,A33,A69,A71,Th53;
              then
A76:          p3`2/|.p3.|>=r1 by A26,A15,XXREAL_0:2;
              then
A77:          q33`1>0 by A16,A17,A69,JGRAPH_4:113;
A78:          q33`2>=0 by A16,A17,A69,A76,JGRAPH_4:113;
A79:          now
                assume
                p2`1=0;
                then 1^2=0^2+(p2`2)^2 by A30,JGRAPH_3:10;
                hence p2`2=1 or p2`2=-1 by SQUARE_1:109;
              end;
A80:          now
                per cases by A6,A79,A72;
                case
A81:              p2`1<=0 & p2`2>=0;
                  then q22=p2 by A17,JGRAPH_4:89;
                  hence q22`2>=0 & LE q22,q33,P by A1,A29,A28,A77,A78,A81,
                  Th57;
                end;
                case
A82:              p2`1>0;
                  then
A83:              q22`1>0 by A14,A16,A17,Th25;
                  now
                    per cases;
                    case
                      p2=p3;
                      hence
                      q22`2>=0 & LE q22,q33,P by A9,A16,A17,A28,A69,A76,
                      JGRAPH_4:113 ,JORDAN6:71;
                    end;
                    case
                      p2<>p3;
                      then p2`2/|.p2.|>p3`2/|.p3.| by A1,A3,A30,A26,A69,A82
                      ,Th53;
                      then q22`2/|.q22.|>q33`2/|.q33.| by A30,A26,A14,A16,A17
                      ,A69,A82,Th27;
                      hence q22`2>=0 & LE q22,q33,P by A1,A16,A17,A31,A32,A27,
                      A28,A69,A76,A77,A83, Th58,JGRAPH_4:113;
                    end;
                  end;
                  hence q22`2>=0 & LE q22,q33,P;
                end;
              end;
              p3`2/|.p3.|>p4`2/|.p4.| by A1,A4,A8,A11,A26,A33,A69,A71,
              Th53;
              then q33`2/|.q33.|>q44`2/|.q44.| by A8,A11,A26,A14,A15,A17,A33
              ,A69,Th27;
              then (q33`2) ^2 > ((q44`2))^2 by A27,A19,A47,SQUARE_1:78;
              then
A84:          1^2- ((q33`2))^2 < 1^2-((q44`2))^2 by XREAL_1:17;
              1^2=(q44`1)^2+(q44`2)^2 by A19,JGRAPH_3:10;
              then
A85:          (q44`1)=sqrt(1^2-((q44`2))^2) by A36, SQUARE_1:89;
A86:          1^2=(q33`1)^2+(q33`2)^2 by A27,JGRAPH_3:10;
              then (q33`1)=sqrt(1^2-((q33`2))^2) by A77, SQUARE_1:89;
              then q33`1< q44`1 by A86,A85,A84,SQUARE_1:95 ,XREAL_1:
              65;
              hence
              q22`2>=0 & LE q22,q33,P & q33`2>=0 & LE q33,q44,P by A1,A28
              ,A20,A47,A78,A80,Th57;
            end;
            case
A87:          p3=p4;
A88:          now
A89:            LE p2,p4,P by A1,A3,A4,JGRAPH_3:36,JORDAN6:73;
                assume
A90:            p2`1=0 & p2`2=-1;
                then LE p4,p2,P by A1,A8,A29,A10,A12,A33,Th58;
                hence contradiction by A1,A8,A90,A89,JGRAPH_3:36 ,
                JORDAN6:72;
              end;
A91:          now
                assume
                p2`1=0;
                then 1^2=0^2+(p2`2)^2 by A30,JGRAPH_3:10;
                hence p2`2=1 or p2`2=-1 by SQUARE_1:109;
              end;
A92:          p3`2/|.p3.|>=r1 by A26,A15,A87;
              then
A93:          q33`1>0 by A16,A17,A69,JGRAPH_4:113;
A94:          q33`2>=0 by A16,A17,A69,A92,JGRAPH_4:113;
              now
                per cases by A6,A91,A88;
                case
A95:              p2`1<=0 & p2`2>=0;
                  then q22=p2 by A17,JGRAPH_4:89;
                  hence q22`2>=0 & LE q22,q33,P by A1,A29,A28,A93,A94,A95,
                  Th57;
                end;
                case
A96:              p2`1>0;
                  then
A97:              q22`1>0 by A14,A16,A17,Th25;
                  now
                    per cases;
                    case
                      p2=p3;
                      hence
                      q22`2>=0 & LE q22,q33,P by A9,A16,A17,A28,A69,A92,
                      JGRAPH_4:113 ,JORDAN6:71;
                    end;
                    case
                      p2<>p3;
                      then p2`2/|.p2.|>p3`2/|.p3.| by A1,A3,A30,A26,A69,A96
                      ,Th53;
                      then q22`2/|.q22.|>q33`2/|.q33.| by A30,A26,A14,A16,A17
                      ,A69,A96,Th27;
                      hence q22`2>=0 & LE q22,q33,P by A1,A16,A17,A31,A32,A27,
                      A28,A69,A92,A93,A97, Th58,JGRAPH_4:113;
                    end;
                  end;
                  hence q22`2>=0 & LE q22,q33,P;
                end;
              end;
              hence
              q22`2>=0 & LE q22,q33,P & q33`2>=0 & LE q33,q44,P by A1,A28
              ,A36,A47,A87,Th57;
            end;
          end;
A98:      now
            LE p1,p3,P by A1,A2,A3,JGRAPH_3:36,JORDAN6:73;
            then
A99:        LE p1,p4,P by A1,A4,JGRAPH_3:36,JORDAN6:73;
            assume
A100:       p1`1=0 & p1`2=-1;
            then LE p4,p1,P by A1,A8,A21,A10,A12,A33,Th58;
            hence contradiction by A1,A8,A100,A99,JGRAPH_3:36,JORDAN6:72;
          end;
A101:     now
            assume
            p2`1=0;
            then 1^2=0^2+(p2`2)^2 by A30,JGRAPH_3:10;
            hence p2`2=1 or p2`2=-1 by SQUARE_1:109;
          end;
A102:     now
A103:       LE p2,p4,P by A1,A3,A4,JGRAPH_3:36,JORDAN6:73;
            assume that
A104:       p2`1=0 and
A105:       p2`2=-1;
            p2`2<=p4`2 by A11,A105,Th3;
            then LE p4,p2,P by A1,A8,A29,A10,A33,A104,Th58;
            hence contradiction by A1,A8,A104,A105,A103,JGRAPH_3:36 ,
            JORDAN6:72;
          end;
A106:     now
            assume
            p1`1=0;
            then 1^2=0^2+(p1`2)^2 by A22,JGRAPH_3:10;
            hence p1`2=1 or p1`2=-1 by SQUARE_1:109;
          end;
          now
            per cases by A5,A106,A98;
            case
A107:         p1`1<=0 & p1`2>=0;
              then
A108:         p1=q11 by A17,JGRAPH_4:89;
A109:         q11`2>=0 by A17,A107,JGRAPH_4:89;
              now
                per cases by A6,A101,A102;
                case
                  p2`1<=0 & p2`2>=0;
                  hence q11`2>=0 & LE q11,q22,P by A2,A17,A107,A108,
                  JGRAPH_4:89;
                end;
                case
                  p2`1>0;
                  then q11`1<q22`1 by A14,A16,A17,A107,A108,Th25;
                  hence q11`2>=0 & LE q11,q22,P by A1,A24,A32,A70,A109,
                  Th57;
                end;
              end;
              hence q11`2>=0 & LE q11,q22,P;
            end;
            case
A110:         p1`1>0;
              then
A111:         q11`1>0 by A14,A16,A17,Th25;
              now
                per cases by A6,A101,A102;
                case
A112:             p2`1<=0 & p2`2>=0;
                  now
A113:               p2 in Upper_Arc(P) by A29,A34,A112;
                    assume
A114:               p1`2<0;
                    W-min(P)=|[-1,0]| by A1,Th32;
                    then
A115:               p1<>W-min(P) by A114,EUCLID:56;
                    p1 in Lower_Arc(P) by A21,A48,A114;
                    then LE p2,p1,P by A113,A115,JORDAN6:def 10;
                    hence contradiction by A1,A2,A110,A112,JGRAPH_3:36 ,
                    JORDAN6:72;
                  end;
                  then LE p2,p1,P by A1,A21,A29,A110,A112,Th57;
                  then q11=q22 by A1,A2,JGRAPH_3:36,JORDAN6:72;
                  hence q11`2>=0 & LE q11,q22,P by A9,A17,A24,A112,
                  JGRAPH_4:89,JORDAN6:71;
                end;
                case
A116:             p2`1>0;
                  then
A117:             q22`1>0 by A14,A16,A17,Th25;
                  now
                    per cases;
                    case
                      p1=p2;
                      hence q11`2>=0 & LE q11,q22,P by A1,A24,A70,
                      JGRAPH_3:36,JORDAN6:71;
                    end;
                    case
                      p1<>p2;
                      then p1`2/|.p1.|>p2`2/|.p2.| by A1,A2,A22,A30,A110,A116
                      ,Th53;
                      then q11`2/|.q11.|>q22`2/|.q22.| by A22,A30,A14,A16,A17
                      ,A110,A116,Th27;
                      hence
                      q11`2>=0 & LE q11,q22,P by A1,A23,A24,A31,A32,A70,
                      A111,A117,Th58;
                    end;
                  end;
                  hence q11`2>=0 & LE q11,q22,P;
                end;
              end;
              hence q11`2>=0 & LE q11,q22,P;
            end;
          end;
          hence q11`2>=0 & q22`2>=0 & q33`2>=0 & q44`2>0 & LE q11,q22,P & LE
          q22,q33,P & LE q33,q44,P by A8,A15,A17,A33,A35,A37,A70, JGRAPH_4:113;
        end;
      end;
      for q being Point of TOP-REAL 2 holds |.(f1.q).|=|.q.| by A17,
      JGRAPH_4:104;
      hence thesis by A18,A49;
    end;
    case
A118: p4`2>0;
A119: Lower_Arc(P)={p8 where p8 is Point of TOP-REAL 2:p8 in P & p8`2<=0
      } by A1,Th38;
A120: now
        assume
        p4 in Lower_Arc(P);
        then ex p9 being Point of TOP-REAL 2 st p9=p4 & p9 in P & p9 `2<=0 by
        A119;
        hence contradiction by A118;
      end;
A121: Upper_Arc(P)={p7 where p7 is Point of TOP-REAL 2:p7 in P & p7`2>=0
      } by A1,Th37;
      p3 in Upper_Arc(P) & p4 in Lower_Arc(P)& not p4=W-min(P) or p3 in
Upper_Arc(P) & p4 in Upper_Arc(P) & LE p3,p4,Upper_Arc(P),W-min(P),E-max(P) or
p3 in Lower_Arc(P) & p4 in Lower_Arc(P)& not p4=W-min(P) & LE p3,p4,Lower_Arc(P
      ),E-max(P),W-min(P) by A4,JORDAN6:def 10;
      then
A122: ex p33 being Point of TOP-REAL 2 st p33=p3 & p33 in P & p33`2>=0 by
      A121,A120;
      set f4=id (TOP-REAL 2);
A123: f4.p3=p3 & f4.p4=p4 by FUNCT_1:35;
A124: for q being Point of TOP-REAL 2 holds |.(f4.q).|=|.q.| by FUNCT_1:
      35;
A125: LE p2,p4,P by A1,A3,A4,JGRAPH_3:36,JORDAN6:73;
      then p2 in Upper_Arc(P) & p4 in Lower_Arc(P)& not p4=W-min(P) or p2 in
Upper_Arc(P) & p4 in Upper_Arc(P) & LE p2,p4,Upper_Arc(P),W-min(P),E-max(P) or
p2 in Lower_Arc(P) & p4 in Lower_Arc(P)& not p4=W-min(P) & LE p2,p4,Lower_Arc(P
      ),E-max(P),W-min(P) by JORDAN6:def 10;
      then
A126: ex p22 being Point of TOP-REAL 2 st p22=p2 & p22 in P & p22`2>=0 by
      A121,A120;
      LE p1,p4,P by A1,A2,A125,JGRAPH_3:36,JORDAN6:73;
      then p1 in Upper_Arc(P) & p4 in Lower_Arc(P)& not p4=W-min(P) or p1 in
Upper_Arc(P) & p4 in Upper_Arc(P) & LE p1,p4,Upper_Arc(P),W-min(P),E-max(P) or
p1 in Lower_Arc(P) & p4 in Lower_Arc(P)& not p4=W-min(P) & LE p1,p4,Lower_Arc(P
      ),E-max(P),W-min(P) by JORDAN6:def 10;
      then
A127: ex p11 being Point of TOP-REAL 2 st p11=p1 & p11 in P & p11`2>=0 by
      A121,A120;
      f4.p1=p1 & f4.p2=p2 by FUNCT_1:35;
      hence thesis by A2,A3,A4,A118,A122,A126,A127,A123,A124;
    end;
  end;
  hence thesis;
end;

theorem Th66:
  for p1,p2,p3,p4 being Point of TOP-REAL 2, P being compact non
  empty Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} &
LE p1,p2,P & LE p2,p3,P & LE p3,p4,P & (p1`2>=0 or p1`1>=0)& (p2`2>=0 or p2`1>=
0) & (p3`2>=0 or p3`1>=0)& (p4`2>0 or p4`1>0) ex f being Function of TOP-REAL 2
,TOP-REAL 2, q1,q2,q3,q4 being Point of TOP-REAL 2 st f is being_homeomorphism
& (for q being Point of TOP-REAL 2 holds |.(f.q).|=|.q.|)& q1=f.p1 & q2=f.p2 &
q3=f.p3 & q4=f.p4 & q1`1<0 & q1`2<0 & q2`1<0 & q2`2<0 & q3`1<0 & q3`2<0 & q4`1<
  0 & q4`2<0 & LE q1,q2,P & LE q2,q3,P & LE q3,q4,P
proof
  let p1,p2,p3,p4 be Point of TOP-REAL 2, P be compact non empty Subset of
  TOP-REAL 2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P & LE p2,p3,P & LE p3,p4,P &( p1`2>=0 or p1`1>=0) & ( p2`2
  >=0 or p2`1>=0)&( p3`2>=0 or p3`1>=0) &( p4`2>0 or p4`1>0);
  consider f being Function of TOP-REAL 2,TOP-REAL 2, q1,q2,q3,q4 being Point
  of TOP-REAL 2 such that
A3: f is being_homeomorphism and
A4: for q being Point of TOP-REAL 2 holds |.(f.q).|=|.q.| and
A5: q1=f.p1 & q2=f.p2 and
A6: q3=f.p3 & q4=f.p4 and
A7: q1`2>=0 & q2`2>=0 & q3`2>=0 & q4`2>0 & LE q1,q2,P & LE q2,q3,P & LE
  q3,q4,P by A1,A2,Th65;
  consider f2 being Function of TOP-REAL 2,TOP-REAL 2, q1b,q2b,q3b,q4b being
  Point of TOP-REAL 2 such that
A8: f2 is being_homeomorphism and
A9: for q being Point of TOP-REAL 2 holds |.(f2.q).|=|.q.| and
A10: q1b=f2.q1 & q2b=f2.q2 and
A11: q3b=f2.q3 & q4b=f2.q4 and
A12: q1b`1<0 & q1b`2<0 & q2b`1<0 & q2b`2<0 & q3b`1<0 & q3b`2<0 & q4b`1<
  0 & q4b`2<0 & LE q1b,q2b,P & LE q2b,q3b,P & LE q3b,q4b,P by A1,A7,Th64;
  reconsider f3=f2*f as Function of TOP-REAL 2,TOP-REAL 2;
A13: f3 is being_homeomorphism by A3,A8,TOPS_2:71;
A14: dom f=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
  then
A15: f3.p3=q3b & f3.p4=q4b by A6,A11,FUNCT_1:23;
A16: for q being Point of TOP-REAL 2 holds |.(f3.q).|=|.q.|
  proof
    let q be Point of TOP-REAL 2;
    dom f=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
    then f3.q=f2.(f.q) by FUNCT_1:23;
    hence |.f3.q.|=|.(f.q).| by A9
      .=|.q.| by A4;
  end;
  f3.p1=q1b & f3.p2=q2b by A5,A10,A14,FUNCT_1:23;
  hence thesis by A12,A13,A16,A15;
end;

theorem
  for p1,p2,p3,p4 being Point of TOP-REAL 2, P being compact non empty
  Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} & p4=
W-min(P) & LE p1,p2,P & LE p2,p3,P & LE p3,p4,P ex f being Function of TOP-REAL
2,TOP-REAL 2, q1,q2,q3,q4 being Point of TOP-REAL 2 st f is being_homeomorphism
& (for q being Point of TOP-REAL 2 holds |.(f.q).|=|.q.|)& q1=f.p1 & q2=f.p2 &
q3=f.p3 & q4=f.p4 & q1`1<0 & q1`2<0 & q2`1<0 & q2`2<0 & q3`1<0 & q3`2<0 & q4`1<
  0 & q4`2<0 & LE q1,q2,P & LE q2,q3,P & LE q3,q4,P
proof
  let p1,p2,p3,p4 be Point of TOP-REAL 2, P be compact non empty Subset of
  TOP-REAL 2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: p4=W-min(P) and
A3: LE p1,p2,P and
A4: LE p2,p3,P and
A5: LE p3,p4,P;
A6: Upper_Arc(P) ={p7 where p7 is Point of TOP-REAL 2:p7 in P & p7`2>=0} by
  A1,Th37;
A7: W-min(P)=|[-1,0]| by A1,Th32;
  then
A8: (W-min(P))`2=0 by EUCLID:56;
A9: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then p4 in P by A5,JORDAN7:5;
  then
A10: p4 in Upper_Arc(P) by A2,A6,A8;
A11: Upper_Arc(P) is_an_arc_of W-min(P),E-max(P) by A9,JORDAN6:def 8;
A12: p3 in Upper_Arc(P) by A1,A5,A10,Th47;
  then LE p4,p3,Upper_Arc(P),W-min(P),E-max(P) by A2,A11,JORDAN5C:10;
  then LE p4,p3,P by A10,A12,JORDAN6:def 10;
  then
A13: p3=p4 by A1,A5,JGRAPH_3:36,JORDAN6:72;
A14: LE p2,p4,P by A1,A4,A5,JGRAPH_3:36,JORDAN6:73;
A15: p2 in Upper_Arc(P) by A1,A4,A12,Th47;
  then LE p4,p2,Upper_Arc(P),W-min(P),E-max(P) by A2,A11,JORDAN5C:10;
  then LE p4,p2,P by A10,A15,JORDAN6:def 10;
  then
A16: p2=p4 by A1,A14,JGRAPH_3:36,JORDAN6:72;
A17: (W-min(P))`1=-1 by A7,EUCLID:56;
A18: p1 in Upper_Arc(P) by A1,A3,A15,Th47;
  then LE p4,p1,Upper_Arc(P),W-min(P),E-max(P) by A2,A11,JORDAN5C:10;
  then LE p4,p1,P by A10,A18,JORDAN6:def 10;
  then p1=p4 by A1,A3,A16,JGRAPH_3:36,JORDAN6:72;
  hence thesis by A1,A2,A3,A17,A8,A13,A16,Th62;
end;

theorem Th68:
  for p1,p2,p3,p4 being Point of TOP-REAL 2, P being compact non
  empty Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} &
LE p1,p2,P & LE p2,p3,P & LE p3,p4,P ex f being Function of TOP-REAL 2,TOP-REAL
  2, q1,q2,q3,q4 being Point of TOP-REAL 2 st f is being_homeomorphism & (for q
being Point of TOP-REAL 2 holds |.(f.q).|=|.q.|)& q1=f.p1 & q2=f.p2 & q3=f.p3 &
q4=f.p4 & q1`1<0 & q1`2<0 & q2`1<0 & q2`2<0 & q3`1<0 & q3`2<0 & q4`1<0 & q4`2<0
  & LE q1,q2,P & LE q2,q3,P & LE q3,q4,P
proof
  let p1,p2,p3,p4 be Point of TOP-REAL 2, P be compact non empty Subset of
  TOP-REAL 2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: LE p2,p3,P and
A4: LE p3,p4,P;
A5: Lower_Arc(P) ={p7 where p7 is Point of TOP-REAL 2:p7 in P & p7`2<=0} by
  A1,Th38;
A6: W-min(P)=|[-1,0]| by A1,Th32;
  then
A7: (W-min(P))`2=0 by EUCLID:56;
A8: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then
A9: p1 in P by A2,JORDAN7:5;
A10: Upper_Arc(P) is_an_arc_of W-min(P),E-max(P) by A8,JORDAN6:def 8;
A11: Upper_Arc(P) ={p7 where p7 is Point of TOP-REAL 2:p7 in P & p7`2>=0} by
  A1,Th37;
A12: p4 in P by A4,A8,JORDAN7:5;
  then
A13: ex p44 being Point of TOP-REAL 2 st p44=p4 & |.p44.|=1 by A1;
  then
A14: p4`1<=1 by Th3;
A15: -1<=p4`1 by A13,Th3;
  now
    per cases;
    case
A16:  p4`1=-1;
      1^2=(p4`1)^2+(p4`2)^2 by A13,JGRAPH_3:10
        .=(p4`2)^2+1 by A16;
      then
A17:  p4`2=0 by XCMPLX_1:6;
      then
A18:  p4 in Upper_Arc(P) by A12,A11;
A19:  p4=W-min(P) by A6,A16,A17,EUCLID:57;
A20:  now
        per cases;
        case
A21:      p1 in Upper_Arc(P);
          then LE p4,p1,Upper_Arc(P),W-min(P),E-max(P) by A10,A19,
          JORDAN5C
          :10;
          hence LE p4,p1,P by A18,A21,JORDAN6:def 10;
        end;
        case
          not p1 in Upper_Arc(P);
          then
A22:      p1`2<0 by A9,A11;
          then p1 in Lower_Arc(P) by A9,A5;
          hence LE p4,p1,P by A7,A18,A22,JORDAN6:def 10;
        end;
      end;
      then
A23:  LE p4,p2,P by A1,A2,JGRAPH_3:36,JORDAN6:73;
      then LE p4,p3,P by A1,A3,JGRAPH_3:36,JORDAN6:73;
      then
A24:  p3=p4 by A1,A4,JGRAPH_3:36,JORDAN6:72;
      LE p2,p4,P by A1,A3,A4,JGRAPH_3:36,JORDAN6:73;
      then
A25:  p2=p4 by A1,A23,JGRAPH_3:36,JORDAN6:72;
      LE p1,p3,P by A1,A2,A3,JGRAPH_3:36,JORDAN6:73;
      then LE p1,p4,P by A1,A4,JGRAPH_3:36,JORDAN6:73;
      then p4=p1 by A1,A20,JGRAPH_3:36,JORDAN6:72;
      hence thesis by A1,A2,A16,A17,A25,A24,Th62;
    end;
    case
A26:  p4`1<>-1;
      then p4`1> -1 by A15,XXREAL_0:1;
      then consider r being real number such that
A27:  -1<r and
A28:  r<p4`1 by XREAL_1:7;
      reconsider r1=r as Real by XREAL_0:def 1;
A29:  r1<1 by A14,A28,XXREAL_0:2;
      then consider f1 being Function of TOP-REAL 2,TOP-REAL 2 such that
A30:  f1=r1-FanMorphS and
A31:  f1 is being_homeomorphism by A27,JGRAPH_4:143;
      set q11=f1.p1, q22=f1.p2, q33=f1.p3, q44=f1.p4;
      now
        per cases;
        case
A32:      p4`1>0 or p4`2>=0;
A33:      now
            assume that
A34:        p4`2=0 and
A35:        p4`1<=0;
            1^2 =(p4`1)^2+(p4`2)^2 by A13,JGRAPH_3:10
              .=(p4`1)^2 by A34;
            hence contradiction by A26,A35,SQUARE_1:109;
          end;
A36:      p3`1>=0 or p3`2>=0 by A1,A4,A32,Th52;
          then
A37:      p2`1>=0 or p2`2>=0 by A1,A3,Th52;
          then p1`1>=0 or p1`2>=0 by A1,A2,Th52;
          hence thesis by A1,A2,A3,A4,A32,A36,A37,A33,Th66;
        end;
        case
A38:      p4`1<=0 & p4`2<0;
          p4`1/|.p4.|>r1 by A13,A28;
          then
A39:      q44`1>0 by A27,A28,A30,A38,Th29;
A40:      LE q33,q44,P by A1,A4,A27,A29,A30,Th61;
          W-min(P)=|[-1,0]| by A1,Th32;
          then
A41:      (W-min(P))`2=0 by EUCLID:56;
A42:      now
            per cases;
            case
              q33`2>=0;
              hence q33`2>=0 or q33`1>=0;
            end;
            case
              q33`2<0;
              thus q33`2>=0 or q33`1>=0 by A1,A39,A40,A41,Th51;
            end;
          end;
A43:      LE q22,q33,P by A1,A3,A27,A29,A30,Th61;
A44:      now
            per cases;
            case
              q22`2>=0;
              hence q22`2>=0 or q22`1>=0;
            end;
            case
              q22`2<0;
              thus q22`2>=0 or q22`1>=0 by A1,A8,A39,A40,A43,A41,Th51,JORDAN6:
              73;
            end;
          end;
A45:      LE q11,q22,P by A1,A2,A27,A29,A30,Th61;
A46:      LE q22,q44,P by A1,A40,A43,JGRAPH_3:36,JORDAN6:73;
          now
            per cases;
            case
              q11`2>=0;
              hence q11`2>=0 or q11`1>=0;
            end;
            case
              q11`2<0;
              thus q11`2>=0 or q11`1>=0 by A1,A8,A39,A46,A45,A41,Th51,JORDAN6:
              73;
            end;
          end;
          then consider
          f2 being Function of TOP-REAL 2,TOP-REAL 2, q81,q82,q83,q84
          being Point of TOP-REAL 2 such that
A47:      f2 is being_homeomorphism and
A48:      for q being Point of TOP-REAL 2 holds |.(f2.q).|=|.q.| and
A49:      q81=f2.q11 & q82=f2.q22 and
A50:      q83=f2.q33 & q84=f2.q44 and
A51:      q81`1<0 & q81`2<0 & q82`1<0 & q82`2<0 & q83`1<0 & q83`2<0 &
q84`1< 0 & q84`2<0 & LE q81,q82,P & LE q82,q83,P & LE q83,q84,P by A1,A39,A40,
          A43,A45,A42,A44,Th66;
          reconsider f3=f2*f1 as Function of TOP-REAL 2,TOP-REAL 2;
A52:      dom f1=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
          then
A53:      f3.p1=q81 & f3.p2=q82 by A49,FUNCT_1:23;
A54:      for q being Point of TOP-REAL 2 holds |.(f3.q).|=|.q.|
          proof
            let q be Point of TOP-REAL 2;
            dom f1=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
            then f3.q=f2.(f1.q) by FUNCT_1:23;
            hence |.f3.q.|=|.(f1.q).| by A48
              .=|.q.| by A30,JGRAPH_4:135;
          end;
A55:      f3.p3=q83 & f3.p4=q84 by A50,A52,FUNCT_1:23;
          f3 is being_homeomorphism by A31,A47,TOPS_2:71;
          hence thesis by A51,A54,A53,A55;
        end;
      end;
      hence thesis;
    end;
  end;
  hence thesis;
end;

begin :: General Fashoda Meet Theorems

theorem Th69:
  for p1,p2,p3,p4 being Point of TOP-REAL 2, P being compact non
  empty Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} &
LE p1,p2,P & LE p2,p3,P & LE p3,p4,P & p1<>p2 & p2<>p3 & p3<>p4 & p1`1<0 & p2`1
<0 & p3`1<0 & p4`1<0 & p1`2<0 & p2`2<0 & p3`2<0 ex f being Function of TOP-REAL
  2,TOP-REAL 2 st f is being_homeomorphism & (for q being Point of TOP-REAL 2
holds |.(f.q).|=|.q.|)& |[-1,0]|=f.p1 & |[0,1]|=f.p2 & |[1,0]|=f.p3 & |[0,-1]|=
  f.p4
proof
  let p1,p2,p3,p4 be Point of TOP-REAL 2, P be compact non empty Subset of
  TOP-REAL 2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: LE p2,p3,P and
A4: LE p3,p4,P and
A5: p1<>p2 and
A6: p2<>p3 and
A7: p3<>p4 and
A8: p1`1<0 and
A9: p2`1<0 and
A10: p3`1<0 and
A11: p4`1<0 and
