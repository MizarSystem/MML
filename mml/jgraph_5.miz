      .= (pu`2)^2/(1+(pu`1/pu`2)^2)+(pu`1)^2/(1+(pu`1/pu`2)^2) by A215,
    SQUARE_1:def 4
      .= ((pu`2)^2+(pu`1)^2)/(1+(pu`1/pu`2)^2) by XCMPLX_1:63;
    then ((pu`2)^2+(pu`1)^2)/(1+(pu`1/pu`2)^2)*(1+(pu`1/pu`2)^2)=(1)*(1+(pu`1
    /pu `2)^2) by A217;
    then ((pu`2)^2+(pu`1)^2)=(1+(pu`1/pu`2)^2) by A215,XCMPLX_1:88;
    then
A231: (pu`2)^2+(pu`1)^2-1=(pu`1)^2/(pu`2)^2 by XCMPLX_1:77;
    pu`2<>0 by A225,A226,A227,A228,A229,XREAL_1:66;
    then ((pu`2)^2+(pu`1)^2-1)*(pu`2)^2=(pu`1)^2 by A231,XCMPLX_1:6,88;
    then
A232: ((pu`2)^2-1)*((pu`2)^2+(pu`1)^2)=0;
    ((pu`2)^2+(pu`1)^2)<>0 by A228,COMPLEX1:2;
    then (pu`2-1)*(pu`2+1)=0 by A232,XCMPLX_1:6;
