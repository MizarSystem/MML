A16: W-min(P)=|[-1,0]| by A2,Th32;
  Upper_Arc(P) is_an_arc_of W-min(P),E-max(P) by A4,JORDAN6:def 8;
  then K0 is non empty compact by JORDAN5A:1;
  then
A17: g3/" is being_homeomorphism by A9,A3,A8,A7,COMPTS_1:26,TOPS_2:
  70;
A18: g3/".(1)=g3/".((|[1,0]|)`1) by EUCLID:56
    .=E-max(P) by A6,A11,A15;
  g3/".(-1)=g3/".((|[-1,0]|)`1) by EUCLID:56
    .=W-min(P) by A14,A11,A16;
  hence thesis by A17,A11,A18;
end;

theorem Th45:
  for P being compact non empty Subset of TOP-REAL 2 st P={p where
  p is Point of TOP-REAL 2: |.p.|=1} ex f being Function of I[01],(TOP-REAL 2)|
Lower_Arc(P) st f is being_homeomorphism & (for q1,q2 being Point of TOP-REAL 2
  , r1,r2 being Real st f.r1=q1 & f.r2=q2 & r1 in [.0,1.] & r2 in [.0,1.] holds
  r1<r2 iff q1`1>q2`1)& f.0 = E-max(P) & f.1 = W-min(P)
proof
  let P be compact non empty Subset of TOP-REAL 2;
  reconsider T= (TOP-REAL 2)|Lower_Arc(P) as non empty TopSpace;
  consider g being Function of I[01],Closed-Interval-TSpace(-1,1) such that
A1: g is being_homeomorphism and
A2: for r being Real st r in [.0,1.] holds g.r=(-2)*r+1 and
A3: g.0=1 and
A4: g.1=-1 by Th41;
  assume
A5: P={p where p is Point of TOP-REAL 2: |.p.|=1};
  then consider
  f1 being Function of Closed-Interval-TSpace(-1,1),(TOP-REAL 2)|
  Lower_Arc(P) such that
A6: f1 is being_homeomorphism and
A7: for q being Point of TOP-REAL 2 st q in Lower_Arc(P) holds f1.(q`1)
  =q and
A8: f1.(-1)=W-min(P) and
A9: f1.1=E-max(P) by Th43;
  reconsider h=f1*g as Function of I[01],(TOP-REAL 2)|Lower_Arc(P);
A10: dom h=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
  then 0 in dom h by XXREAL_1:1;
  then
A11: h.0=E-max(P) by A9,A3,FUNCT_1:22;
A12: for q1,q2 being Point of TOP-REAL 2, r1,r2 being Real st h.r1=q1 & h.r2
  =q2 & r1 in [.0,1.] & r2 in [.0,1.] holds r1<r2 iff q1`1>q2`1
  proof
    let q1,q2 be Point of TOP-REAL 2,r1,r2 be Real;
    assume that
A13: h.r1=q1 and
A14: h.r2=q2 and
A15: r1 in [.0,1.] and
A16: r2 in [.0,1.];
A17: now
      set s1=(-2)*r2+1,s2=(-2)*r1+1;
      set p1=|[s1,-sqrt(1-s1^2)]|,p2=|[s2,-sqrt(1-s2^2)]|;
A18:  (|[s1,-sqrt(1-s1^2)]|)`2=-sqrt(1-s1^2) by EUCLID:56;
      r2<=1 by A16,XXREAL_1:1;
      then (-2)*r2>=(-2)*1 by XREAL_1:67;
      then (-2)*r2+1>=(-2)*1+1 by XREAL_1:9;
      then
A19:  -1<=s1;
      r2>=0 by A16,XXREAL_1:1;
      then (-2)*r2+1<=(-2)*0+1 by XREAL_1:9;
      then s1^2<=1^2 by A19,SQUARE_1:119;
      then
A20:  1-s1^2>=0 by XREAL_1:50;
      then
A21:  sqrt(1-s1^2)>=0 by SQUARE_1:def 4;
      |.p1.|=sqrt((p1`1)^2+(p1`2)^2) by JGRAPH_3:10
        .=sqrt((s1)^2+(sqrt(1-s1^2))^2) by A18,EUCLID:56
        .=sqrt((s1)^2+(1-s1^2)) by A20,SQUARE_1:def 4
        .=1 by SQUARE_1:83;
      then p1 in P by A5;
      then p1 in {p3 where p3 is Point of TOP-REAL 2: p3 in P & p3`2<=0} by
      A18,A21;
      then
A22:  (|[s1,-sqrt(1-s1^2)]|)`1=s1 & |[s1,-sqrt(1-s1^2)]| in Lower_Arc(P)
      by A5,Th38,EUCLID:56;
      g.r2=(-2)*r2+1 & dom h=[.0,1.] by A2,A16,BORSUK_1:83,FUNCT_2:def 1;
      then h.r2=f1.s1 by A16,FUNCT_1:22
        .=p1 by A7,A22;
      then
A23:  q2`1=s1 by A14,EUCLID:56;
A24:  (|[s2,-sqrt(1-s2^2)]|)`1=s2 by EUCLID:56;
      r1<=1 by A15,XXREAL_1:1;
      then (-2)*r1>=(-2)*1 by XREAL_1:67;
      then (-2)*r1+1>=(-2)*1+1 by XREAL_1:9;
      then
A25:  -1<=s2;
      r1>=0 by A15,XXREAL_1:1;
      then (-2)*r1+1<=(-2)*0+1 by XREAL_1:9;
      then s2^2<=1^2 by A25,SQUARE_1:119;
      then
A26:  1-s2^2>=0 by XREAL_1:50;
      then
A27:  sqrt(1-s2^2)>=0 by SQUARE_1:def 4;
      assume
      r2<r1;
      then
A28:  (-2)*r2 > (-2)*r1 by XREAL_1:71;
A29:  (|[s2,-sqrt(1-s2^2)]|)`2=-sqrt(1-s2^2) by EUCLID:56;
      |.p2.|=sqrt((p2`1)^2+(p2`2)^2) by JGRAPH_3:10
        .=sqrt((s2)^2+(sqrt(1-s2^2))^2) by A29,EUCLID:56
        .=sqrt((s2)^2+(1-s2^2)) by A26,SQUARE_1:def 4
        .=1 by SQUARE_1:83;
      then p2 in P by A5;
      then p2 in {p3 where p3 is Point of TOP-REAL 2: p3 in P & p3`2<=0} by
      A29,A27;
      then
A30:  |[s2,-sqrt(1-s2^2)]| in Lower_Arc(P) by A5,Th38;
      g.r1=(-2)*r1+1 & dom h=[.0,1.] by A2,A15,BORSUK_1:83,FUNCT_2:def 1;
      then h.r1=f1.s2 by A15,FUNCT_1:22
        .=p2 by A7,A24,A30;
      hence q2`1>q1`1 by A13,A28,A23,A24,XREAL_1:10;
    end;
A31: now
      assume
A32:  q1`1>q2`1;
      now
        assume
A33:    r1>=r2;
        now
          per cases by A33,XXREAL_0:1;
          case
            r1>r2;
            hence contradiction by A17,A32;
          end;
          case
            r1=r2;
            hence contradiction by A13,A14,A32;
          end;
        end;
        hence contradiction;
      end;
      hence r1<r2;
    end;
    now
      assume
      r1<r2;
      then (-2)*r1 > (-2)*r2 by XREAL_1:71;
      then
A34:  (-2)*r1 +1 > (-2)*r2 +1 by XREAL_1:10;
      set s1=(-2)*r1+1,s2=(-2)*r2+1;
      set p1=|[s1,-sqrt(1-s1^2)]|,p2=|[s2,-sqrt(1-s2^2)]|;
A35:  (|[s1,-sqrt(1-s1^2)]|)`2=-sqrt(1-s1^2) by EUCLID:56;
      r1<=1 by A15,XXREAL_1:1;
      then (-2)*r1>=(-2)*1 by XREAL_1:67;
      then (-2)*r1+1>=(-2)*1+1 by XREAL_1:9;
      then
A36:  -1<=s1;
      r1>=0 by A15,XXREAL_1:1;
      then (-2)*r1+1<=(-2)*0+1 by XREAL_1:9;
      then s1^2<=1^2 by A36,SQUARE_1:119;
      then
A37:  1-s1^2>=0 by XREAL_1:50;
      then
A38:  sqrt(1-s1^2)>=0 by SQUARE_1:def 4;
      |.p1.|=sqrt((p1`1)^2+(p1`2)^2) by JGRAPH_3:10
        .=sqrt((s1)^2+(sqrt(1-s1^2))^2) by A35,EUCLID:56
        .=sqrt((s1)^2+(1-s1^2)) by A37,SQUARE_1:def 4
        .=1 by SQUARE_1:83;
      then p1 in P by A5;
      then p1 in {p3 where p3 is Point of TOP-REAL 2: p3 in P & p3`2<=0} by
      A35,A38;
      then
A39:  (|[s1,-sqrt(1-s1^2)]|)`1=s1 & |[s1,-sqrt(1-s1^2)]| in Lower_Arc(P)
      by A5,Th38,EUCLID:56;
      g.r1=(-2)*r1+1 & dom h=[.0,1.] by A2,A15,BORSUK_1:83,FUNCT_2:def 1;
      then h.r1=f1.s1 by A15,FUNCT_1:22
        .=p1 by A7,A39;
      then
A40:  q1`1=s1 by A13,EUCLID:56;
A41:  (|[s2,-sqrt(1-s2^2)]|)`2=-sqrt(1-s2^2) by EUCLID:56;
      r2<=1 by A16,XXREAL_1:1;
      then (-2)*r2>=(-2)*1 by XREAL_1:67;
      then (-2)*r2+1>=(-2)*1+1 by XREAL_1:9;
      then
A42:  -1<=s2;
      r2>=0 by A16,XXREAL_1:1;
      then (-2)*r2+1<=(-2)*0+1 by XREAL_1:9;
      then s2^2<=1^2 by A42,SQUARE_1:119;
      then
A43:  1-s2^2>=0 by XREAL_1:50;
      then
A44:  sqrt(1-s2^2)>=0 by SQUARE_1:def 4;
      |.p2.|=sqrt((p2`1)^2+(p2`2)^2) by JGRAPH_3:10
        .=sqrt((s2)^2+(sqrt(1-s2^2))^2) by A41,EUCLID:56
        .=sqrt((s2)^2+(1-s2^2)) by A43,SQUARE_1:def 4
        .=1 by SQUARE_1:83;
      then p2 in P by A5;
      then p2 in {p3 where p3 is Point of TOP-REAL 2: p3 in P & p3`2<=0} by
      A41,A44;
      then
A45:  (|[s2,-sqrt(1-s2^2)]|)`1=s2 & |[s2,-sqrt(1-s2^2)]| in Lower_Arc(P)
      by A5,Th38,EUCLID:56;
      g.r2=(-2)*r2+1 & dom h=[.0,1.] by A2,A16,BORSUK_1:83,FUNCT_2:def 1;
      then h.r2=f1.s2 by A16,FUNCT_1:22
        .=p2 by A7,A45;
      hence q1`1>q2`1 by A14,A34,A40,EUCLID:56;
    end;
    hence thesis by A31;
  end;
  1 in dom h by A10,XXREAL_1:1;
  then
A46: h.1=W-min(P) by A8,A4,FUNCT_1:22;
  reconsider f2=f1 as Function of Closed-Interval-TSpace(-1,1),T;
  f2*g is being_homeomorphism by A6,A1,TOPS_2:71;
  hence thesis by A12,A11,A46;
end;

theorem Th46:
  for P being compact non empty Subset of TOP-REAL 2 st P={p where
  p is Point of TOP-REAL 2: |.p.|=1} ex f being Function of I[01],(TOP-REAL 2)|
Upper_Arc(P) st f is being_homeomorphism & (for q1,q2 being Point of TOP-REAL 2
  , r1,r2 being Real st f.r1=q1 & f.r2=q2 & r1 in [.0,1.] & r2 in [.0,1.] holds
  r1<r2 iff q1`1<q2`1)& f.0 = W-min(P) & f.1 = E-max(P)
proof
  let P be compact non empty Subset of TOP-REAL 2;
  reconsider T= (TOP-REAL 2)|Upper_Arc(P) as non empty TopSpace;
  consider g being Function of I[01],Closed-Interval-TSpace(-1,1) such that
A1: g is being_homeomorphism and
A2: for r being Real st r in [.0,1.] holds g.r=2*r-1 and
A3: g.0=-1 and
A4: g.1=1 by Th42;
  assume
A5: P={p where p is Point of TOP-REAL 2: |.p.|=1};
  then consider
  f1 being Function of Closed-Interval-TSpace(-1,1),(TOP-REAL 2)|
  Upper_Arc(P) such that
A6: f1 is being_homeomorphism and
A7: for q being Point of TOP-REAL 2 st q in Upper_Arc(P) holds f1.(q`1)
  =q and
A8: f1.(-1)=W-min(P) and
A9: f1.1=E-max(P) by Th44;
  reconsider h=f1*g as Function of I[01],(TOP-REAL 2)|Upper_Arc(P);
A10: dom h=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
  then 0 in dom h by XXREAL_1:1;
  then
A11: h.0=W-min(P) by A8,A3,FUNCT_1:22;
A12: for q1,q2 being Point of TOP-REAL 2, r1,r2 being Real st h.r1=q1 & h.r2
  =q2 & r1 in [.0,1.] & r2 in [.0,1.] holds r1<r2 iff q1`1<q2`1
  proof
    let q1,q2 be Point of TOP-REAL 2,r1,r2 be Real;
    assume that
A13: h.r1=q1 and
A14: h.r2=q2 and
A15: r1 in [.0,1.] and
A16: r2 in [.0,1.];
A17: now
      r2<=1 by A16,XXREAL_1:1;
      then 2*r2<=2*1 by XREAL_1:66;
      then
A18:  2*r2-1<=2*1-1 by XREAL_1:11;
      r1>=0 by A15,XXREAL_1:1;
      then
A19:  2*r1-1>=2*0-1 by XREAL_1:11;
      set s1=2*r1-1,s2=2*r2-1;
      set p1=|[s1,sqrt(1-s1^2)]|,p2=|[s2,sqrt(1-s2^2)]|;
A20:  (|[s1,sqrt(1-s1^2)]|)`1=s1 by EUCLID:56;
      r2>=0 by A16,XXREAL_1:1;
      then
A21:  2*r2-1>=2*0-1 by XREAL_1:11;
      2*0-1=-1;
      then s2^2<=1^2 by A18,A21,SQUARE_1:119;
      then
A22:  1-s2^2>=0 by XREAL_1:50;
      then
A23:  sqrt(1-s2^2)>=0 by SQUARE_1:def 4;
      r1<=1 by A15,XXREAL_1:1;
      then 2*r1<=2*1 by XREAL_1:66;
      then
A24:  2*r1-1<=2*1-1 by XREAL_1:11;
      assume
      r1>r2;
      then
A25:  2*r1 > 2*r2 by XREAL_1:70;
      2*0-1=-1;
      then s1^2<=1^2 by A24,A19,SQUARE_1:119;
      then
A26:  1-s1^2>=0 by XREAL_1:50;
      then
A27:  sqrt(1-s1^2)>=0 by SQUARE_1:def 4;
A28:  (|[s1,sqrt(1-s1^2)]|)`2=sqrt(1-s1^2) by EUCLID:56;
      then |.p1.|=sqrt((s1)^2+(sqrt(1-s1^2))^2) by A20,JGRAPH_3:10
        .=sqrt((s1)^2+(1-s1^2)) by A26,SQUARE_1:def 4
        .=1 by SQUARE_1:83;
      then p1 in P by A5;
      then p1 in {p3 where p3 is Point of TOP-REAL 2: p3 in P & p3`2>=0} by
      A28,A27;
      then
A29:  |[s1,sqrt(1-s1^2)]| in Upper_Arc(P) by A5,Th37;
      g.r1=2*r1-1 & dom h=[.0,1.] by A2,A15,BORSUK_1:83,FUNCT_2:def 1;
      then h.r1=f1.s1 by A15,FUNCT_1:22
        .=p1 by A7,A20,A29;
      then
A30:  q1`1=s1 by A13,EUCLID:56;
A31:  (|[s2,sqrt(1-s2^2)]|)`1=s2 by EUCLID:56;
A32:  (|[s2,sqrt(1-s2^2)]|)`2=sqrt(1-s2^2) by EUCLID:56;
      then |.p2.|=sqrt((s2)^2+(sqrt(1-s2^2))^2) by A31,JGRAPH_3:10
        .=sqrt((s2)^2+(1-s2^2)) by A22,SQUARE_1:def 4
        .=1 by SQUARE_1:83;
      then p2 in P by A5;
      then p2 in {p3 where p3 is Point of TOP-REAL 2: p3 in P & p3`2>=0} by
      A32,A23;
      then
A33:  |[s2,sqrt(1-s2^2)]| in Upper_Arc(P) by A5,Th37;
      g.r2=2*r2-1 & dom h=[.0,1.] by A2,A16,BORSUK_1:83,FUNCT_2:def 1;
      then h.r2=f1.s2 by A16,FUNCT_1:22
        .=p2 by A7,A31,A33;
      hence q1`1>q2`1 by A14,A25,A30,A31,XREAL_1:16;
    end;
A34: now
      assume
A35:  q1`1<q2`1;
      now
        assume
A36:    r1>=r2;
        now
          per cases by A36,XXREAL_0:1;
          case
            r1>r2;
            hence contradiction by A17,A35;
          end;
          case
            r1=r2;
            hence contradiction by A13,A14,A35;
          end;
        end;
        hence contradiction;
      end;
      hence r1<r2;
    end;
    now
      assume
      r2>r1;
      then
A37:  2*r2 > 2*r1 by XREAL_1:70;
      set s1=2*r2-1,s2=2*r1-1;
      set p1=|[s1,sqrt(1-s1^2)]|,p2=|[s2,sqrt(1-s2^2)]|;
A38:  (|[s1,sqrt(1-s1^2)]|)`1=s1 by EUCLID:56;
      r2>=0 by A16,XXREAL_1:1;
      then 2*r2-1>=2*0-1 by XREAL_1:11;
      then
A39:  -1<=s1;
      r2<=1 by A16,XXREAL_1:1;
      then 2*r2<=2*1 by XREAL_1:66;
      then 2*r2-1<=2*1-1 by XREAL_1:11;
      then s1^2<=1^2 by A39,SQUARE_1:119;
      then
A40:  1-s1^2>=0 by XREAL_1:50;
      then
A41:  sqrt(1-s1^2)>=0 by SQUARE_1:def 4;
A42:  (|[s1,sqrt(1-s1^2)]|)`2=sqrt(1-s1^2) by EUCLID:56;
      then |.p1.|=sqrt((s1)^2+(sqrt(1-s1^2))^2) by A38,JGRAPH_3:10
        .=sqrt((s1)^2+(1-s1^2)) by A40,SQUARE_1:def 4
        .=1 by SQUARE_1:83;
      then p1 in P by A5;
      then p1 in {p3 where p3 is Point of TOP-REAL 2: p3 in P & p3`2>=0} by
      A42,A41;
      then
A43:  |[s1,sqrt(1-s1^2)]| in Upper_Arc(P) by A5,Th37;
      g.r2=2*r2-1 & dom h=[.0,1.] by A2,A16,BORSUK_1:83,FUNCT_2:def 1;
      then h.r2=f1.s1 by A16,FUNCT_1:22
        .=p1 by A7,A38,A43;
      then
A44:  q2`1=s1 by A14,EUCLID:56;
A45:  (|[s2,sqrt(1-s2^2)]|)`1=s2 by EUCLID:56;
      r1>=0 by A15,XXREAL_1:1;
      then 2*r1-1>=2*0-1 by XREAL_1:11;
      then
A46:  -1<=s2;
      r1<=1 by A15,XXREAL_1:1;
      then 2*r1<=2*1 by XREAL_1:66;
      then 2*r1-1<=2*1-1 by XREAL_1:11;
      then s2^2<=1^2 by A46,SQUARE_1:119;
      then
A47:  1-s2^2>=0 by XREAL_1:50;
      then
A48:  sqrt(1-s2^2)>=0 by SQUARE_1:def 4;
A49:  (|[s2,sqrt(1-s2^2)]|)`2=sqrt(1-s2^2) by EUCLID:56;
      then |.p2.|=sqrt((s2)^2+(sqrt(1-s2^2))^2) by A45,JGRAPH_3:10
        .=sqrt((s2)^2+(1-s2^2)) by A47,SQUARE_1:def 4
        .=1 by SQUARE_1:83;
      then p2 in P by A5;
      then p2 in {p3 where p3 is Point of TOP-REAL 2: p3 in P & p3`2>=0} by
      A49,A48;
      then
A50:  |[s2,sqrt(1-s2^2)]| in Upper_Arc(P) by A5,Th37;
      g.r1=2*r1-1 & dom h=[.0,1.] by A2,A15,BORSUK_1:83,FUNCT_2:def 1;
      then h.r1=f1.s2 by A15,FUNCT_1:22
        .=p2 by A7,A45,A50;
      hence q2`1>q1`1 by A13,A37,A44,A45,XREAL_1:16;
    end;
    hence thesis by A34;
  end;
  1 in dom h by A10,XXREAL_1:1;
  then
A51: h.1=E-max(P) by A9,A4,FUNCT_1:22;
  reconsider f2=f1 as Function of Closed-Interval-TSpace(-1,1),T;
  f2*g is being_homeomorphism by A6,A1,TOPS_2:71;
  hence thesis by A12,A11,A51;
end;

theorem Th47:
  for p1,p2 being Point of TOP-REAL 2, P being compact non empty
  Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} & p2 in
  Upper_Arc(P) & LE p1,p2,P holds p1 in Upper_Arc(P)
proof
  let p1,p2 be Point of TOP-REAL 2, P be compact non empty Subset of TOP-REAL
  2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: p2 in Upper_Arc(P) and
A3: LE p1,p2,P;
  set P4b=Lower_Arc(P);
A4: p1 in Upper_Arc(P) & p2 in Lower_Arc(P)& not p2=W-min(P) or p1 in
Upper_Arc(P) & p2 in Upper_Arc(P) & LE p1,p2,Upper_Arc(P),W-min(P),E-max(P) or
p1 in Lower_Arc(P) & p2 in Lower_Arc(P)& not p2=W-min(P) & LE p1,p2,Lower_Arc(P
  ),E-max(P),W-min(P) by A3,JORDAN6:def 10;
A5: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then
A6: Lower_Arc(P) is_an_arc_of E-max(P),W-min(P) by JORDAN6:def 9;
A7: Upper_Arc(P) /\ P4b={W-min(P),E-max(P)} by A5,JORDAN6:def 9;
  then E-max(P) in Upper_Arc(P) /\ P4b by TARSKI:def 2;
  then
A8: E-max(P) in Upper_Arc(P) by XBOOLE_0:def 4;
  now
    assume
A9: not p1 in Upper_Arc(P);
    then p2 in Upper_Arc(P) /\ P4b by A2,A4,XBOOLE_0:def 4;
    then
A10: p2=E-max(P) by A7,A4,A9,TARSKI:def 2;
    then LE p2,p1,Lower_Arc(P),E-max(P),W-min(P) by A6,A4,A9,
    JORDAN5C:10;
    hence contradiction by A6,A8,A4,A9,A10,JORDAN5C:12;
  end;
  hence thesis;
end;

theorem Th48:
  for p1,p2 being Point of TOP-REAL 2, P being compact non empty
Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} & LE p1,
  p2,P & p1<>p2 & p1`1<0 & p1`2<0 & p2`2<0 holds p1`1>p2`1 & p1`2<p2`2
proof
  let p1,p2 be Point of TOP-REAL 2, P be compact non empty Subset of TOP-REAL
  2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: p1<>p2 and
A4: p1`1<0 and
A5: p1`2<0 and
A6: p2`2<0;
  consider f being Function of I[01],(TOP-REAL 2)|Lower_Arc(P) such that
A7: f is being_homeomorphism and
A8: for q1,q2 being Point of TOP-REAL 2, r1,r2 being Real st f.r1=q1 &
  f.r2=q2 & r1 in [.0,1.] & r2 in [.0,1.] holds r1<r2 iff q1`1>q2`1 and
A9: f.0=E-max(P) & f.1=W-min(P) by A1,Th45;
A10: rng f=[#]((TOP-REAL 2)|Lower_Arc(P)) by A7,TOPS_2:def 5
    .=Lower_Arc(P) by PRE_TOPC:def 10;
A11: Upper_Arc(P)={p where p is Point of TOP-REAL 2:p in P & p`2>=0} by A1,
  Th37;
A12: now
    assume
    p1 in Upper_Arc(P);
    then ex p being Point of TOP-REAL 2 st p1=p & p in P & p`2>= 0 by A11;
    hence contradiction by A5;
  end;
  then
A13: LE p1,p2,Lower_Arc(P),E-max(P),W-min(P) by A2,JORDAN6:def 10;
  p2 in Lower_Arc(P) by A2,A12,JORDAN6:def 10;
  then consider x2 being set such that
A14: x2 in dom f and
A15: p2=f.x2 by A10,FUNCT_1:def 5;
A16: dom f=[#](I[01]) by A7,TOPS_2:def 5
    .=[.0,1.] by BORSUK_1:83;
  then reconsider r22=x2 as Real by A14;
A17: 0<=r22 & r22<=1 by A14,A16,XXREAL_1:1;
  p1 in Lower_Arc(P) by A2,A12,JORDAN6:def 10;
  then consider x1 being set such that
A18: x1 in dom f and
A19: p1=f.x1 by A10,FUNCT_1:def 5;
  reconsider r11=x1 as Real by A18,A16;
  r11<=1 by A18,A16,XXREAL_1:1;
  then
A20: r11<=r22 by A13,A7,A9,A19,A15,A17,JORDAN5C:def 3;
A21: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then p1 in P by A2,JORDAN7:5;
  then ex p3 being Point of TOP-REAL 2 st p3=p1 & |.p3.|=1 by A1;
  then 1^2=(p1`1)^2+(p1`2)^2 by JGRAPH_3:10;
  then 1^2-(p1`1)^2=(-(p1`2))^2;
  then -(p1`2)=sqrt(1^2-(-(p1`1))^2) by A5,SQUARE_1:89;
  then
A22: (p1`2)=-sqrt(1^2-(-(p1`1))^2);
  p2 in P by A2,A21,JORDAN7:5;
  then ex p4 being Point of TOP-REAL 2 st p4=p2 & |.p4.|=1 by A1;
  then
A23: 1^2=(p2`1)^2+(p2`2)^2 by JGRAPH_3:10;
  then 1^2-(p2`1)^2=(-(p2`2))^2;
  then -(p2`2)=sqrt(1^2-(-(p2`1))^2) by A6,SQUARE_1:89;
  then
A24: (p2`2)=-sqrt(1^2-(-(p2`1))^2);
A25: r11<r22 iff p1`1>p2`1 by A8,A18,A19,A14,A15,A16;
  then -(p1`1)< -(p2`1) by A3,A19,A15,A20,XREAL_1:26,XXREAL_0:1;
  then (-(p1`1))^2 < (-(p2`1))^2 by A4,SQUARE_1:78;
  then 1^2- (-(p1`1))^2 > 1^2-(-(p2`1))^2 by XREAL_1:17;
  then sqrt(1^2- (-(p1`1))^2) > sqrt(1^2-(-(p2`1))^2) by A23,
  SQUARE_1:95 ,
  XREAL_1:65;
  hence thesis by A19,A15,A25,A20,A22,A24,XREAL_1:26,XXREAL_0:1;
end;

theorem Th49:
  for p1,p2 being Point of TOP-REAL 2, P being compact non empty
Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} & LE p1,
  p2,P & p1<>p2 & p2`1<0 & p1`2>=0 & p2`2>=0 holds p1`1<p2`1 & p1`2<p2`2
proof
  let p1,p2 be Point of TOP-REAL 2, P be compact non empty Subset of TOP-REAL
  2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: p1<>p2 and
A4: p2`1<0 and
A5: p1`2>=0 and
A6: p2`2>=0;
  set P4=Lower_Arc(P);
A7: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then
A8: Upper_Arc(P) /\ P4={W-min(P),E-max(P)} by JORDAN6:def 9;
A9: p1 in P by A2,A7,JORDAN7:5;
A10: now
    assume
    p2=W-min(P);
    then LE p2,p1,P by A7,A9,JORDAN7:3;
    hence contradiction by A1,A2,A3,JGRAPH_3:36,JORDAN6:72;
  end;
A11: p2 in P by A2,A7,JORDAN7:5;
  then ex p4 being Point of TOP-REAL 2 st p4=p2 & |.p4.|=1 by A1;
  then 1^2=(p2`1)^2+(p2`2)^2 by JGRAPH_3:10;
  then
A12: (p2`2)=sqrt(1^2-(-(p2`1))^2) by A6,SQUARE_1:89;
A13: Upper_Arc(P)={p where p is Point of TOP-REAL 2:p in P & p`2>=0} by A1,
  Th37;
A14: now
    assume
A15: p2 in Lower_Arc(P);
    p2 in Upper_Arc(P) by A6,A11,A13;
    then p2 in {W-min(P),E-max(P)} by A8,A15,XBOOLE_0:def 4;
    then
A16: p2=W-min(P) or p2=E-max(P) by TARSKI:def 2;
    E-max(P)=|[1,0]| by A1,Th33;
    hence contradiction by A4,A10,A16,EUCLID:56;
  end;
  then
A17: LE p1,p2,Upper_Arc(P),W-min(P),E-max(P) by A2,JORDAN6:def 10;
A18: ex p3 being Point of TOP-REAL 2 st p3=p1 & |.p3.|=1 by A1,A9;
  then 1^2=(p1`1)^2+(p1`2)^2 by JGRAPH_3:10;
  then
A19: (p1`2)=sqrt(1^2-(-(p1`1))^2) by A5,SQUARE_1:89;
  1^2=(p1`1)^2+(p1`2)^2 by A18,JGRAPH_3:10;
  then
A20: 1^2-(-(p1`1))^2>=0 by XREAL_1:65;
  consider f being Function of I[01],(TOP-REAL 2)|Upper_Arc(P) such that
A21: f is being_homeomorphism and
A22: for q1,q2 being Point of TOP-REAL 2, r1,r2 being Real st f.r1=q1 &
  f.r2=q2 & r1 in [.0,1.] & r2 in [.0,1.] holds r1<r2 iff q1`1<q2`1 and
A23: f.0=W-min(P) & f.1=E-max(P) by A1,Th46;
A24: rng f=[#]((TOP-REAL 2)|Upper_Arc(P)) by A21,TOPS_2:def 5
    .=Upper_Arc(P) by PRE_TOPC:def 10;
  p2 in Upper_Arc(P) by A2,A14,JORDAN6:def 10;
  then consider x2 being set such that
A25: x2 in dom f and
A26: p2=f.x2 by A24,FUNCT_1:def 5;
A27: dom f=[#](I[01]) by A21,TOPS_2:def 5
    .=[.0,1.] by BORSUK_1:83;
  then reconsider r22=x2 as Real by A25;
A28: 0<=r22 & r22<=1 by A25,A27,XXREAL_1:1;
  p1 in Upper_Arc(P) by A2,A14,JORDAN6:def 10;
  then consider x1 being set such that
A29: x1 in dom f and
A30: p1=f.x1 by A24,FUNCT_1:def 5;
  reconsider r11=x1 as Real by A29,A27;
  r11<=1 by A29,A27,XXREAL_1:1;
  then
A31: r11<=r22 by A17,A21,A23,A30,A26,A28,JORDAN5C:def 3;
A32: r11<r22 iff p1`1<p2`1 by A22,A29,A30,A25,A26,A27;
  then -(p1`1)> -(p2`1) by A3,A30,A26,A31,XREAL_1:26,XXREAL_0:1;
  then (-(p1`1))^2 > (-(p2`1))^2 by A4,SQUARE_1:78;
  then 1^2- (-(p1`1))^2 < 1^2-(-(p2`1))^2 by XREAL_1:17;
  hence thesis by A30,A26,A32,A31,A19,A12,A20,SQUARE_1:95,XXREAL_0:1;
end;

theorem Th50:
  for p1,p2 being Point of TOP-REAL 2, P being compact non empty
Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} & LE p1,
  p2,P & p1<>p2 & p2`2>=0 holds p1`1<p2`1
proof
  let p1,p2 be Point of TOP-REAL 2, P be compact non empty Subset of TOP-REAL
  2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: p1<>p2 and
A4: p2`2>=0;
A5: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then
A6: p1 in P by A2,JORDAN7:5;
  set P4=Lower_Arc(P);
A7: Upper_Arc(P)={p where p is Point of TOP-REAL 2:p in P & p`2>=0} by A1,
  Th37;
A8: Upper_Arc(P) /\ P4={W-min(P),E-max(P)} by A5,JORDAN6:def 9;
A9: p2 in P by A2,A5,JORDAN7:5;
A10: now
A11: now
      assume
      p2=W-min(P);
      then LE p2,p1,P by A5,A6,JORDAN7:3;
      hence contradiction by A1,A2,A3,JGRAPH_3:36,JORDAN6:72;
    end;
    assume
A12: p2 in Lower_Arc(P);
    p2 in Upper_Arc(P) by A4,A9,A7;
    then p2 in {W-min(P),E-max(P)} by A8,A12,XBOOLE_0:def 4;
    then p2=W-min(P) or p2=E-max(P) by TARSKI:def 2;
    then
A13: p2= |[1,0]| by A1,A11,Th33;
    then
A14: p2`1=1 by EUCLID:56;
A15: ex p8 being Point of TOP-REAL 2 st p8=p1 & |.p8.|=1 by A1,A6;
A16: now
      assume
A17:  p1`1=1;
      1^2=(p1`1)^2+(p1`2)^2 by A15,JGRAPH_3:10;
      then p1`2=0 by A17,XCMPLX_1:6;
      hence contradiction by A3,A13,A17,EUCLID:57;
    end;
    p1`1<=1 by A15,Th3;
    hence thesis by A14,A16,XXREAL_0:1;
  end;
  now
    assume
    p2=W-min(P);
    then LE p2,p1,P by A5,A6,JORDAN7:3;
    hence contradiction by A1,A2,A3,JGRAPH_3:36,JORDAN6:72;
  end;
  then
A18: p1 in Upper_Arc(P) & p2 in Upper_Arc(P)& not p2=W-min(P) & LE p1,p2,
  Upper_Arc(P),W-min(P),E-max(P) or p1`1<p2`1 by A2,A10,JORDAN6:def 10;
  consider f being Function of I[01],(TOP-REAL 2)|Upper_Arc(P) such that
A19: f is being_homeomorphism and
A20: for q1,q2 being Point of TOP-REAL 2, r1,r2 being Real st f.r1=q1 &
  f.r2=q2 & r1 in [.0,1.] & r2 in [.0,1.] holds r1<r2 iff q1`1<q2`1 and
A21: f.0=W-min(P) & f.1=E-max(P) by A1,Th46;
A22: rng f=[#]((TOP-REAL 2)|Upper_Arc(P)) by A19,TOPS_2:def 5
    .=Upper_Arc(P) by PRE_TOPC:def 10;
  now
    per cases;
    case
A23:  not p1`1 < p2`1;
      then consider x1 being set such that
A24:  x1 in dom f and
A25:  p1=f.x1 by A18,A22,FUNCT_1:def 5;
      consider x2 being set such that
A26:  x2 in dom f and
A27:  p2=f.x2 by A18,A22,A23,FUNCT_1:def 5;
A28:  dom f=[#](I[01]) by A19,TOPS_2:def 5
        .=[.0,1.] by BORSUK_1:83;
      then reconsider r22=x2 as Real by A26;
A29:  0<=r22 & r22<=1 by A26,A28,XXREAL_1:1;
      reconsider r11=x1 as Real by A24,A28;
A30:  r11<r22 iff p1`1<p2`1 by A20,A24,A25,A26,A27,A28;
      r11<=1 by A24,A28,XXREAL_1:1;
      then r11<=r22 or p1`1<p2`1 by A18,A19,A21,A25,A27,
      A29,JORDAN5C:def 3;
      hence thesis by A3,A25,A27,A30,XXREAL_0:1;
    end;
    case
      p1`1<p2`1;
      hence thesis;
    end;
  end;
  hence thesis;
end;

theorem Th51:
  for p1,p2 being Point of TOP-REAL 2, P being compact non empty
Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} & LE p1,
  p2,P & p1<>p2 & p1`2<=0 & p1<>W-min(P) holds p1`1>p2`1
proof
  let p1,p2 be Point of TOP-REAL 2, P be compact non empty Subset of TOP-REAL
  2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: p1<>p2 and
A4: p1`2<=0 and
A5: p1<>W-min(P);
A6: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then
A7: p2 in P by A2,JORDAN7:5;
  set P4=Lower_Arc(P);
A8: Lower_Arc(P)={p where p is Point of TOP-REAL 2:p in P & p`2<=0} by A1,
  Th38;
A9: Upper_Arc(P) /\ P4={W-min(P),E-max(P)} by A6,JORDAN6:def 9;
A10: p1 in P by A2,A6,JORDAN7:5;
  now
    assume
A11: p1 in Upper_Arc(P);
    p1 in Lower_Arc(P) by A4,A10,A8;
    then p1 in {W-min(P),E-max(P)} by A9,A11,XBOOLE_0:def 4;
    then p1=W-min(P) or p1=E-max(P) by TARSKI:def 2;
    then
A12: p1= |[1,0]| by A1,A5,Th33;
    then
A13: p1`1=1 by EUCLID:56;
A14: ex p9 being Point of TOP-REAL 2 st p9=p2 & |.p9.|=1 by A1,A7;
A15: now
      assume
A16:  p2`1=1;
      1^2 =(p2`1)^2+(p2`2)^2 by A14,JGRAPH_3:10;
      then p2`2=0 by A16,XCMPLX_1:6;
      hence contradiction by A3,A12,A16,EUCLID:57;
    end;
    p2`1<=1 by A14,Th3;
    hence thesis by A13,A15,XXREAL_0:1;
  end;
  then
A17: p1 in Lower_Arc(P) & p2 in Lower_Arc(P)& not p2=W-min(P) & LE p1,p2,
  Lower_Arc(P),E-max(P),W-min(P) or p1`1>p2`1 by A2,JORDAN6:def 10;
  consider f being Function of I[01],(TOP-REAL 2)|Lower_Arc(P) such that
A18: f is being_homeomorphism and
A19: for q1,q2 being Point of TOP-REAL 2, r1,r2 being Real st f.r1=q1 &
  f.r2=q2 & r1 in [.0,1.] & r2 in [.0,1.] holds r1<r2 iff q1`1>q2`1 and
A20: f.0=E-max(P) & f.1=W-min(P) by A1,Th45;
A21: rng f=[#]((TOP-REAL 2)|Lower_Arc(P)) by A18,TOPS_2:def 5
    .=Lower_Arc(P) by PRE_TOPC:def 10;
  now
    per cases;
    case
A22:  not p1`1 > p2`1;
      then consider x1 being set such that
A23:  x1 in dom f and
A24:  p1=f.x1 by A17,A21,FUNCT_1:def 5;
      consider x2 being set such that
A25:  x2 in dom f and
A26:  p2=f.x2 by A17,A21,A22,FUNCT_1:def 5;
A27:  dom f=[#](I[01]) by A18,TOPS_2:def 5
        .=[.0,1.] by BORSUK_1:83;
      then reconsider r22=x2 as Real by A25;
A28:  0<=r22 & r22<=1 by A25,A27,XXREAL_1:1;
      reconsider r11=x1 as Real by A23,A27;
A29:  r11<r22 iff p1`1>p2`1 by A19,A23,A24,A25,A26,A27;
      r11<=1 by A23,A27,XXREAL_1:1;
      then r11<=r22 or p1`1>p2`1 by A17,A18,A20,A24,A26,
      A28,JORDAN5C:def 3;
      hence thesis by A3,A24,A26,A29,XXREAL_0:1;
    end;
    case
      p1`1>p2`1;
      hence thesis;
    end;
  end;
  hence thesis;
end;

theorem Th52:
  for p1,p2 being Point of TOP-REAL 2, P being compact non empty
Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} & (p2`2>=
  0 or p2`1>=0) & LE p1,p2,P holds p1`2>=0 or p1`1>=0
proof
  let p1,p2 be Point of TOP-REAL 2, P be compact non empty Subset of TOP-REAL
  2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: p2`2>=0 or p2`1>=0 and
A3: LE p1,p2,P;
A4: Upper_Arc(P)={p where p is Point of TOP-REAL 2:p in P & p`2>=0} by A1,
  Th37;
A5: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then
A6: p2 in P by A3,JORDAN7:5;
A7: Lower_Arc(P) is_an_arc_of E-max(P),W-min(P) by A5,JORDAN6:def 9;
  per cases by A2;
  suppose
    p2`2>=0;
    then p2 in Upper_Arc(P) by A6,A4;
    then p1 in Upper_Arc(P) by A1,A3,Th47;
    then ex p8 being Point of TOP-REAL 2 st p8=p1 & p8 in P & p8 `2>=0 by A4;
    hence thesis;
  end;
  suppose
A8: p2`2<0 & p2`1>=0;
    then not ex p8 being Point of TOP-REAL 2 st p8=p2 & p8 in P & p8`2>=0;
    then
A9: not p2 in Upper_Arc(P) by A4;
    now
      per cases by A3,A9,JORDAN6:def 10;
      case
        p1 in Upper_Arc(P) & p2 in Lower_Arc(P)& not p2=W-min(P);
        then ex p8 being Point of TOP-REAL 2 st p8=p1 & p8 in P & p8 `2>=0 by
        A4;
        hence thesis;
      end;
      case
A10:    p1 in Lower_Arc(P) & p2 in Lower_Arc(P)& not p2=W-min(P)& LE
        p1,p2,Lower_Arc(P),E-max(P),W-min(P);
        now
          assume
A11:      p1=W-min(P);
          then LE p2,p1,Lower_Arc(P),E-max(P),W-min(P) by A7,A10,
          JORDAN5C
          :10;
          hence contradiction by A7,A10,A11,JORDAN5C:12;
        end;
        hence thesis by A1,A3,A8,Th51;
        consider p8 being Point of TOP-REAL 2;
      end;
    end;
    hence thesis;
  end;
end;

theorem Th53:
  for p1,p2 being Point of TOP-REAL 2, P being compact non empty
Subset of TOP-REAL 2 st P={p where p is Point of TOP-REAL 2: |.p.|=1} & LE p1,
  p2,P & p1<>p2 & p1`1>=0 & p2`1>=0 holds p1`2>p2`2
proof
  let p1,p2 be Point of TOP-REAL 2, P be compact non empty Subset of TOP-REAL
  2;
  assume that
A1: P={p where p is Point of TOP-REAL 2: |.p.|=1} and
A2: LE p1,p2,P and
A3: p1<>p2 and
A4: p1`1>=0 and
A5: p2`1>=0;
A6: Upper_Arc(P)={p where p is Point of TOP-REAL 2:p in P & p`2>=0} by A1,
  Th37;
A7: P is being_simple_closed_curve by A1,JGRAPH_3:36;
  then
A8: p2 in P by A2,JORDAN7:5;
  then
A9: ex p3 being Point of TOP-REAL 2 st p3=p2 & |.p3.|=1 by A1;
  W-min(P)=|[-1,0]| by A1,Th32;
  then
A10: (W-min(P))`2=0 by EUCLID:56;
A11: Lower_Arc(P)={p where p is Point of TOP-REAL 2:p in P & p`2<=0} by A1,
  Th38;
A12: p1 in P by A2,A7,JORDAN7:5;
  then
A13: ex p4 being Point of TOP-REAL 2 st p4=p1 & |.p4.|=1 by A1;
  now
    per cases;
    case
A14:  p1`2>=0 & p2`2>=0;
      then p1`1<p2`1 by A1,A2,A3,Th50;
      then (p1`1)^2 < ((p2`1))^2 by A4,SQUARE_1:78;
      then
A15:  1^2- ((p1`1))^2 > 1^2-((p2`1))^2 by XREAL_1:17;
      1^2=(p1`1)^2+(p1`2)^2 by A13,JGRAPH_3:10;
      then
A16:  p1`2=sqrt(1^2-((p1`1))^2) by A14,SQUARE_1:89;
A17:  1^2=(p2`1)^2+(p2`2)^2 by A9,JGRAPH_3:10;
      then (p2`2)=sqrt(1^2-((p2`1))^2) by A14,SQUARE_1:89;
      hence thesis by A15,A16,A17,SQUARE_1:95,XREAL_1:65;
    end;
    case
      p1`2>=0 & p2`2<0;
      hence thesis;
    end;
    case
A18:  p1`2<0 & p2`2>=0;
      then p1 in Lower_Arc(P) & p2 in Upper_Arc(P) by A12,A8,A6,A11;
      then LE p2,p1,P by A10,A18,JORDAN6:def 10;
      hence contradiction by A1,A2,A3,JGRAPH_3:36,JORDAN6:72;
    end;
    case
A19:  p1`2<0 & p2`2<0;
      ex p3 being Point of TOP-REAL 2 st p3=p1 & |.p3.|=1 by A1,A12;
      then
A20:  1^2=(p1`1)^2+(p1`2)^2 by JGRAPH_3:10;
      then 1^2-((p1`1))^2=(-(p1`2))^2;
      then
A21:  -(p1`2)=sqrt(1^2-((p1`1))^2) by A19,SQUARE_1:89;
      not ex p being Point of TOP-REAL 2 st p=p1 & p in P & p`2>=0 by A19;
      then
A22:  not p1 in Upper_Arc(P) by A6;
      then
A23:  LE p1,p2,Lower_Arc(P),E-max(P),W-min(P) by A2,JORDAN6:def 10;
      ex p4 being Point of TOP-REAL 2 st p4=p2 & |.p4.|=1 by A1,A8;
      then 1^2=(p2`1)^2+(p2`2)^2 by JGRAPH_3:10;
      then 1^2-((p2`1))^2=(-(p2`2))^2;
      then
A24:  -(p2`2)=sqrt(1^2-((p2`1))^2) by A19,SQUARE_1:89;
      consider f being Function of I[01],(TOP-REAL 2)|Lower_Arc(P) such that
A25:  f is being_homeomorphism and
A26:  for q1,q2 being Point of TOP-REAL 2, r1,r2 being Real st f.r1=
      q1 & f.r2=q2 & r1 in [.0,1.] & r2 in [.0,1.] holds r1<r2 iff q1`1>q2`1
      and
A27:  f.0=E-max(P) & f.1=W-min(P) by A1,Th45;
A28:  rng f=[#]((TOP-REAL 2)|Lower_Arc(P)) by A25,TOPS_2:def 5
        .=Lower_Arc(P) by PRE_TOPC:def 10;
      p2 in Lower_Arc(P) by A2,A22,JORDAN6:def 10;
      then consider x2 being set such that
A29:  x2 in dom f and
A30:  p2=f.x2 by A28,FUNCT_1:def 5;
A31:  dom f=[#](I[01]) by A25,TOPS_2:def 5
        .=[.0,1.] by BORSUK_1:83;
      then reconsider r22=x2 as Real by A29;
A32:  0<=r22 & r22<=1 by A29,A31,XXREAL_1:1;
      p1 in Lower_Arc(P) by A2,A22,JORDAN6:def 10;
      then consider x1 being set such that
A33:  x1 in dom f and
A34:  p1=f.x1 by A28,FUNCT_1:def 5;
      reconsider r11=x1 as Real by A33,A31;
A35:  r11<r22 iff p1`1>p2`1 by A26,A33,A34,A29,A30,A31;
      r11<=1 by A33,A31,XXREAL_1:1;
      then r11<=r22 by A23,A25,A27,A34,A30,A32,JORDAN5C:def 3;
      then (p1`1) ^2 > ((p2`1))^2 by A3,A5,A34,A30,A35,SQUARE_1:78 ,
      XXREAL_0:1;
      then 1^2- ((p1`1))^2 < 1^2-((p2`1))^2 by XREAL_1:17;
      then sqrt(1^2- ((p1`1))^2) < sqrt(1^2-((p2`1))^2) by A20,
      SQUARE_1:95
      ,XREAL_1:65;
      hence thesis by A21,A24,XREAL_1:26;
    end;
  end;
