theorem
  for i,j being Nat,M1,M2 being Matrix of COMPLEX
  st len M1=len M2 & width M1=width M2 & j in Seg len M1 holds
    Line(M1+M2,j)=Line(M1,j)+Line(M2,j)
proof let i,j be Nat,M1,M2 be Matrix of COMPLEX;
  assume A1: len M1=len M2 & width M1=width M2 & j in Seg len M1;
  A2: len (M1+M2)=len M1 & width (M1+M2)=width M1 by Th6;
  A3: len Line(M2,j)=width M1 by A1,MATRIX_1:def 8
       .= len Line(M1,j) by MATRIX_1:def 8;
  A4: len (Line(M1,j)+Line(M2,j))=len (Line(M1,j)) by COMPLSP2:6,A3
       .= width M1 by MATRIX_1:def 8;
  A5: len (Line(M1+M2,j))=width (M1+M2) by MATRIX_1:def 8
       .= width M1 by Th6;
  now let i be Element of NAT; assume
  A6: 1 <= i & i <= len Line(M1+M2,j);
  A7: i in Seg width M1 by FINSEQ_1:3,A6,A5;
  A8: i in Seg width M2 by FINSEQ_1:3,A6,A5,A1;
  A9: i in Seg len (Line(M1,j)+Line(M2,j)) by A4,FINSEQ_1:3,A6,A5;
  A10: i in dom (Line(M1,j)+Line(M2,j)) by FINSEQ_1:def 3,A9;
  A11: j in Seg len M1 & i in Seg width M1 by FINSEQ_1:3,A6,A5,A1;
  A12: [j,i] in [:Seg len M1,Seg width M1:] by ZFMISC_1:106,A11;
  A13: [j,i] in [:dom M1,Seg width M1:] by FINSEQ_1:def 3,A12;
  A14: [j,i] in Indices M1 by A13;
  i in Seg width (M1+M2) by FINSEQ_1:3,A6,A5,A2;
  hence (Line(M1+M2,j)).i
    = (M1+M2)*(j,i) by MATRIX_1:def 8
   .= M1*(j,i)+M2*(j,i) by A14,Th7,A1
   .= M1*(j,i)+(Line(M2,j)).i by A8,MATRIX_1:def 8
   .= (Line(M1,j)).i+(Line(M2,j)).i by A7,MATRIX_1:def 8
   .= (Line(M1,j)+Line(M2,j)).i by A10,COMPLSP2:1;
  end;
  hence thesis by FINSEQ_1:18,A4,A5;
end;

theorem Th43:
  for M being Matrix of COMPLEX st i in Seg len M holds
    Line(M,i)=(Line(M*',i))*'
proof  let M be Matrix of COMPLEX;
  assume A1: i in Seg len M;
  A2: len Line(M,i)=width M by MATRIX_1:def 8;
  A3: len ((Line(M*',i))*')=len (Line(M*',i)) by COMPLSP2:def 1
      .= width (M*') by MATRIX_1:def 8 .= width M by Def1;
  for j be Element of NAT st 1 <= j & j <= len (Line(M,i)) holds
   (Line(M,i)).j=((Line(M*',i))*').j
  proof let j be Element of NAT; assume
    A4: 1 <= j & j <= len Line(M,i);
    A5: 1 <= j & j <= width M by A4,MATRIX_1:def 8;
    A6: 1 <= j & j <= len ((Line(M*',i))*') by A4,MATRIX_1:def 8,A3;
    A7: 1 <= j & j <= len Line(M*',i) by A6,COMPLSP2:def 1;
    A8: 1 <= i & i <= len M by FINSEQ_1:3,A1;
    A9: [i,j] in Indices M by A8,A5,Th1;
    A10: j in Seg width M by A4,FINSEQ_1:3,A2;
    A11: j in Seg width (M*') by A10,Def1;
    (Line(M,i)).j = ((M*(i,j))*')*' by MATRIX_1:def 8,A10
      .= ((M*')*(i,j))*' by Def1,A9
      .= ((Line(M*',i)).j)*' by MATRIX_1:def 8,A11
      .= ((Line(M*',i))*').j by COMPLSP2:def 1,A7;
    hence thesis;
  end;
 hence thesis by FINSEQ_1:18,A2,A3;
end;

theorem Th44:
  for F being FinSequence of COMPLEX,M being Matrix of COMPLEX
  st len F=width M holds
   mlt(F,(Line((M*'),i))*') = mlt(Line((M*'),i),(F*'))*'
proof
  let F be FinSequence of COMPLEX,M be Matrix of COMPLEX;
  assume A1: len F=width M;
  len (Line((M*'),i))=width (M*') by MATRIX_1:def 8
      .= width M by Def1;
  hence thesis by Th25,A1;
end;

theorem Th45:
  for F being FinSequence of COMPLEX,M being Matrix of COMPLEX
   st len F = width M & len F >= 1 holds
    (M*F)*'=(M*')*(F*')
proof
  let F be FinSequence of COMPLEX,M be Matrix of COMPLEX;
  assume A1: len F = width M & len F >= 1;
  A2: len ((M*F)*')=len (M*F) by COMPLSP2:def 1 .= len M by Def7;
  A3: len (M*') = len M by Def1;
  A4: len (F*') = len F by COMPLSP2:def 1;
  A5: width (M*')=width M by Def1;
  A6: len (M*'*(F*'))=len (M*') by Def7
      .= len M by Def1;
  now let i be Element of NAT; assume
  A7: 1 <= i & i <= len ((M*F)*');
  A8: i in Seg len M by A7,FINSEQ_1:3,A2;
  A9: i in Seg len (M*') by A7,FINSEQ_1:3,A2,A3;
  A10: len (Line((M*'),i)) = len (F*') by MATRIX_1:def 8,A5,A1,A4;
  A11: len (mlt(Line((M*'),i),(F*'))) >= 1 by Lm2,A10,A4,A1;
  1 <= i & i <= len ((M*F)) by A7,COMPLSP2:def 1;
  hence ((M*F)*').i = ((M*F).i)*' by COMPLSP2:def 1
    .= Sum(mlt(F,Line(M,i)))*' by Def7,A8
    .= Sum(mlt(F,(Line((M*'),i))*'))*' by Th43,A8
    .= Sum((mlt(Line((M*'),i),(F*')))*')*' by Th44,A1
    .= (Sum(mlt(Line((M*'),i),(F*'))))*'*' by Th24,A11
    .= ((M*')*(F*')).i by Def7,A9;
  end;
 hence thesis by FINSEQ_1:18,A6,A2;
end;

theorem
  for F1,F2,F3 being FinSequence of COMPLEX
  st len F1 = len F2 & len F2 = len F3 holds
    mlt(F1,mlt(F2,F3)) = mlt(mlt(F1,F2),F3)
proof let F1,F2,F3 be FinSequence of COMPLEX;
  assume A1: len F1=len F2 & len F2=len F3;
  reconsider f1=F1 as Element of (len F1)-tuples_on COMPLEX by FINSEQ_2:110;
  reconsider f2=F2 as Element of (len F2)-tuples_on COMPLEX by FINSEQ_2:110;
  reconsider f3=F3 as Element of (len F3)-tuples_on COMPLEX by FINSEQ_2:110;
  thus mlt(F1,mlt(F2,F3))
        = multcomplex.:(multcomplex.:(f1,f2),f3) by FINSEQOP:29,A1
       .= mlt(mlt(F1,F2),F3);
end;

theorem
  for F being FinSequence of COMPLEX holds
   Sum -F = -(Sum F)
proof let F be FinSequence of COMPLEX;
  thus Sum -F
    = addcomplex $$(compcomplex*F) by COMPLSP1:def 9
   .= compcomplex.(addcomplex $$ F) by COMPLSP1:7,8,SETWOP_2:42
   .= -(Sum F) by BINOP_2:def 1;
end;

 Lm4:
  for z being Element of COMPLEX holds
    Sum <*z*> = z by FINSOP_1:12;

canceled;

theorem Th49:
  for F1,F2 being FinSequence of COMPLEX holds
    Sum(F1^F2) = Sum F1 + Sum F2
 proof let F1,F2 be FinSequence of COMPLEX;
  thus Sum(F1^F2)
    = addcomplex.(addcomplex $$ F1,addcomplex $$ F2) by FINSOP_1:6
   .= Sum F1 + Sum F2 by BINOP_2:def 3;
 end;

definition let M be Matrix of COMPLEX;
  func LineSum M -> FinSequence of COMPLEX means
  :Def10: len it=len M &
  for i being Nat st i in Seg len M holds it.i=Sum Line(M,i);
  existence
  proof
    deffunc F(Nat) = Sum Line(M,$1);
    consider z being FinSequence of COMPLEX such that
A1: len z = len M and
A2: for j be Element of NAT st j in Seg len M holds z.j = F(j)
       from FINSEQ_2:sch 1;
    take z;
    thus len z = len M by A1;
    let j be Nat;
    thus thesis by A2;
  end;
  uniqueness
    proof
     let p1,p2 be FinSequence of COMPLEX;
     assume that A3:len p1=len M and
                 A4:for j st j in Seg len M holds p1.j = Sum Line(M,j) and
                 A5:len p2=len M and
                 A6:for j st j in Seg len M holds p2.j = Sum Line(M,j);
     for j be Element of NAT st j in Seg len M holds p1.j=p2.j
     proof
      let j be Element of NAT;
      assume j in Seg len M;
      then p1.j = Sum Line(M,j) & p2.j = Sum Line(M,j) by A4,A6;
      hence thesis;
     end;
     hence thesis by A3,A5,FINSEQ_2:10;
    end;
end;

definition let M be Matrix of COMPLEX;
  func ColSum M -> FinSequence of COMPLEX means :Def11:
    len it=width M &
    for j being Nat st j in Seg width M holds it.j=Sum (Col(M,j));
  existence
  proof
    deffunc F(Nat) = Sum Col(M,$1);
    consider f being FinSequence of COMPLEX such that
A1: len f = width M and
A2: for j be Element of NAT st j in Seg width M holds f.j = F(j)
    from FINSEQ_2:sch 1;
    take f;
    thus len f = width M by A1;
    let j be Nat;
    thus thesis by A2;
    end;
  uniqueness
    proof
     let p1,p2 be FinSequence of COMPLEX;
     assume that A3:len p1=width M and
                 A4:for j st j in Seg width M holds p1.j = Sum (Col(M,j)) and
                 A5:len p2=width M and
                 A6:for j st j in Seg width M holds p2.j = Sum (Col(M,j));
     for j be Element of NAT st j in Seg width M holds p1.j=p2.j
     proof
      let j be Element of NAT;
      assume j in Seg width M;
      then p1.j = (Sum (Col(M,j))) & p2.j = (Sum (Col(M,j))) by A4,A6;
      hence thesis;
     end;
     hence thesis by A3,A5,FINSEQ_2:10;
    end;
end;

theorem Th50:
  for F being FinSequence of COMPLEX holds
    len F = 1 implies Sum F = F.1
proof let F be FinSequence of COMPLEX;
  assume A1: len F = 1;
  F = <* F.1 *> by A1,FINSEQ_1:57;
  hence thesis by Lm4;
end;

theorem Th51:
  for f,g being FinSequence of COMPLEX,n being Nat
  st len f = n + 1 & g = f|n holds
    Sum f = Sum g + f/.(len f)
proof let  f,g be FinSequence of COMPLEX,n be Nat;
  assume A1: len f = n + 1 & g = f|n;
  set n'=Seg n;
  A2: g=f|n' by FINSEQ_1:def 15,A1;
  set q = <*f/.len f*>;
  set p = g^q;
  A3: n <= len f by A1,NAT_1:11;
  A4: len q = 1 by FINSEQ_1:56;
    len(g^q) = len g + len q by FINSEQ_1:35
          .= len g + 1 by FINSEQ_1:57
          .= len f by A1,A3,FINSEQ_1:80;
  then A5: dom f = Seg len(g^q) by FINSEQ_1:def 3 .= dom(g^q)
     by FINSEQ_1:def 3;
  A6: dom f = Seg(n+1) by A1,FINSEQ_1:def 3;
  now let u be set;
    assume A7: u in dom f;
    then u in { k where k is Element of NAT : 1 <= k & k <= n+1 }
    by A6,FINSEQ_1:def 1;
    then consider i being Element of NAT such that
    A8: u = i & 1 <= i & i <= n+1;
    now per cases;
      case A9: i = n+1;
       then A10: len g + 1 <= i by A1,A3,FINSEQ_1:80;
         i <= len g + len q by A1,A3,A4,A9,FINSEQ_1:80;
       hence p.i = q.(i-len g) by A10,FINSEQ_1:36
             .= q.(n+1-n) by A1,A3,A9,FINSEQ_1:80
             .= f/.(n+1) by A1,FINSEQ_1:57
             .= f.i by A7,A8,A9,FINSEQ_4:def 4;
      end;
     case i <> n+1;
      then i < n+1 by A8,REAL_1:def 5;
      then i <= n by NAT_1:13;
      then i in { k where k is Element of NAT : 1 <= k & k <= n } by A8;
      then i in Seg n by FINSEQ_1:def 1;
      then A11: i in dom g by A2,A3,FINSEQ_1:21;
      then p.i = g.i by FINSEQ_1:def 7;
      hence p.i = f.i by A2,A11,FUNCT_1:68;
     end;
    end;
    hence f.u = p.u by A8;
  end;
  then f = g^<*(f/.(len f))*> by A5,FUNCT_1:9;
  hence Sum f = Sum g + Sum <*(f/.len f)*> by Th49
           .= Sum g + (f/.len f) by Lm4;
end;

theorem Th52:
  for M being Matrix of COMPLEX st len M > 0 holds
    Sum LineSum M = Sum ColSum M
proof let M be Matrix of COMPLEX;
  assume A1: len M > 0;
  A2: 0+1 <= len M by A1,NAT_1:8;
  0+1-1<=len M-1 by XREAL_1:11,A2;then
  A3: len M-'1=len M-1 by BINARITH:def 3;
  defpred P[Nat] means
    for N being Matrix of COMPLEX st $1+1=len N
     holds Sum LineSum N=Sum ColSum N;
  for N being Matrix of COMPLEX st 0+1=len N
     holds Sum LineSum N=Sum ColSum N
   proof let N be Matrix of COMPLEX;
     assume A4: 0+1=len N;then
    A5: len LineSum N=1 by Def10;
    A6: len ColSum N=width N by Def11;
    A7: len Line(N,1)=width N by MATRIX_1:def 8;
    A8: 1 in Seg 1 by FINSEQ_1:5;
    A9: for j be Element of NAT st 1<=j & j<= width N
    holds (ColSum(N)).j=Line(N,1).j
    proof let j be Element of NAT;
      assume A10: 1<=j & j<= width N;
      A11: j in Seg width N by A10,FINSEQ_1:3;
      A12: len (Col(N,j)) = 1 by MATRIX_1:def 9,A4;
      A13: 1 in dom N by A8,FINSEQ_1:def 3,A4;
      (ColSum N).j=Sum Col(N,j) by Def11,A11
      .= (Col(N,j)).1 by Th50,A12
      .= N*(1,j) by A13,MATRIX_1:def 9
      .= Line(N,1).j by MATRIX_1:def 8,A11;
      hence thesis;
    end;
    A14: 1 in Seg len N by FINSEQ_1:5,A4;
    Sum ColSum N = Sum Line(N,1) by A6,A7,FINSEQ_1:18,A9
      .=(LineSum N).1 by Def10,A14
      .= Sum LineSum N by Th50,A5;
   hence Sum LineSum N=Sum ColSum N;
  end;then
  A15: P[0];
  A16: for k being Nat st P[k] holds P[k+1]
  proof let k be Nat;
   assume A17: P[k];
    for N being Matrix of COMPLEX st k+1+1=len N
     holds Sum LineSum N=Sum ColSum N
     proof let N be Matrix of COMPLEX;
      assume A18: k+1+1=len N;
        A19: len N >= k+1 by NAT_1:11,A18;
        A20: 1+k>=1 by NAT_1:11;
        A21: len N>=1 by NAT_1:11,A18;
        reconsider k1=k+1 as Element of NAT by ORDINAL1:def 13;
        consider n such that
        A22: for x being set st x in rng N
        ex p being FinSequence of COMPLEX st x = p & len p = n by MATRIX_1:9;
        A23: rng (N|k1) c= rng N by FINSEQ_5:21;
        A24: for x being set st x in rng (N|k1)
        ex p being FinSequence of COMPLEX
        st x=p & len p=n by A22,A23;
        reconsider N2=N|k1 as Matrix of COMPLEX by MATRIX_1:9,A24;
        A25: len N2=k+1 by FINSEQ_1:80,A19;
        A26: len N2>=1 by A20,FINSEQ_1:80,A19;
        consider n3 being Nat such that
        A27: for x2 being set st x2 in rng N holds
           ex s being FinSequence st s=x2 & len s=n3 by MATRIX_1:def 1;
        1 in Seg len N by A21,FINSEQ_1:3;then
        1 in dom N by FINSEQ_1:def 3;then
        A28: N.1 in rng N by FUNCT_1:12;then
        consider s2 being FinSequence such that
        A29: s2=N.1 & len s2=n3 by A27;
        1 in Seg len N2 by A26,FINSEQ_1:3;then
        1 in dom N2 by FINSEQ_1:def 3;then
        A30: N2.1 in rng N2 by FUNCT_1:12;then
        consider s3 being FinSequence such that
        A31: s3=N2.1 & len s3=n3 by A27,A23;
        A32: width N2=n3 by MATRIX_1:def 4,A26,A31,A30;
        A33: width N2= width N by MATRIX_1:def 4,A18,A28,A29,A32;
        A34: for j st 1<=j & j<=width N holds
          (ColSum N2).j+N*(k+1+1,j)=(ColSum N).j
        proof let j;
          assume A35:1<=j & j<=width N;
          A36: j in Seg width N by FINSEQ_1:3,A35;
          A37: k+1+1 in Seg len N by A18,FINSEQ_1:6;
          A38: k+1+1 in dom N by FINSEQ_1:def 3,A37;
          A39: k+1+1 in Seg len Col(N,j) by A37,MATRIX_1:def 9;
          A40: k+1+1 in dom Col(N,j) by FINSEQ_1:def 3,A39;
          A41: len (Col(N,j))=len N by MATRIX_1:def 9;
          set f=Col(N,j);
          set g=Col(N2,j);
          A42: len g = len N2 by MATRIX_1:def 9
              .= k1 by FINSEQ_1:80,A19;
          A43: k1 <= len f by A19,MATRIX_1:def 9;
          A44: g=f|k1
          proof
            A45: len (f|k1)= len g by A42,FINSEQ_1:80,A43;
            for n be Element of NAT st 1 <=n & n <= len (f|k1) holds
              (f|k1).n=g.n
            proof let n be Element of NAT;
              assume A46: 1 <= n & n <= len (f|k1);
              A47: 1 <= n & n <= k1 by FINSEQ_1:80,A43,A46;
              A48: 1 <= n & n <= k1+1 by A47,NAT_1:13;
              A49: n in Seg len (f|k1) by FINSEQ_1:3,A46;
              A50: n in dom(f|k1) by FINSEQ_1:def 3,A49;
              A51: n in dom(f|Seg k1) by FINSEQ_1:def 15,A50;
              A52: n in Seg len N by A18,A48,FINSEQ_1:3;
              A53: n in dom N by FINSEQ_1:def 3,A52;
              A54: n in Seg k1 by A47,FINSEQ_1:3;
              A55: n in dom N2 by FINSEQ_1:def 3,A25,A54;
              A56: 1<= n & n<=len N & 1<=j & j<=width N by A47,NAT_1:13,A18,A35
;
              A57: [n,j] in Indices N by Th1,A56;
              A58: 1<= n & n<=len N2 & 1<=j & j<=width N2
                by FINSEQ_1:80,A43,A46,A25,MATRIX_1:def 4,
                   A18,A28,A29,A32,A35;
              A59: [n,j] in Indices N2 by Th1,A58;
              consider pn being FinSequence of COMPLEX such that
              A60: pn=N.n & N*(n,j)=pn.j by MATRIX_1:def 6,A57;
              A61: N2.n=N.n by FINSEQ_3:121,A47;
              (f|k1).n=(f|Seg k1).n by FINSEQ_1:def 15
                    .=(Col(N,j)).n by FUNCT_1:70,A51
                    .= N*(n,j) by MATRIX_1:def 9,A53
                    .= N2*(n,j) by A59,MATRIX_1:def 6,A60,A61
                    .= g.n by A55,MATRIX_1:def 9;
              hence thesis;
            end;
            hence thesis by FINSEQ_1:18,A45;
          end;
          (ColSum N2).j+N*(k+1+1,j)
           = Sum (Col(N2,j))+N*((k+1+1),j) by Def11,A36,A33
          .= Sum (Col(N2,j))+Col(N,j).(k+1+1) by MATRIX_1:def 9,A38
          .= Sum (Col(N2,j))+Col(N,j)/.(k+1+1) by FINSEQ_4:def 4,A40
          .= Sum (Col(N,j)) by Th51,A18,A41,A44
          .=(ColSum N).j by Def11,A36;
          hence thesis;
        end;
        A62: k+1+1 in Seg len N by A18,FINSEQ_1:5;
        A63: k+1+1 in Seg len LineSum N by A62,Def10;
        A64: k+1+1 in dom LineSum N by A63,FINSEQ_1:def 3;
        A65: len ColSum N2=width N2 by Def11;
        A66: len ColSum N2=width N2 by Def11
            .= width N by MATRIX_1:def 4,A18,A28,A29,A32
            .= len Line(N,(k+1+1)) by MATRIX_1:def 8;
        A67: len Line(N,(k+1+1))=width N by MATRIX_1:def 8;
        A68: len (ColSum(N2)+Line(N,(k+1+1)))
             = width N by COMPLSP2:6,A65,A67,A33
            .= len ColSum N by Def11;
        A69: for j be Element of NAT st 1<=j & j<= len ColSum N holds
           (ColSum(N2)+Line(N,k+1+1)).j=(ColSum N).j
        proof let j be Element of NAT;
          assume A70: 1<=j & j<= len ColSum N;
          A71: j in Seg len ColSum N by A70,FINSEQ_1:3;
          A72: j in Seg len ((ColSum(N2)+Line(N,k+1+1)))
               by A70,FINSEQ_1:3,A68;
          A73: j in dom ((ColSum(N2)+Line(N,k+1+1)))
               by A72,FINSEQ_1:def 3;
          A74: j in Seg width N2 by A33,Def11,A71;
          A75: 1<=j & j<=width N by FINSEQ_1:3,A74,A33;
          (ColSum(N2)+Line(N,(k+1+1))).j
           = (ColSum N2).j+(Line(N,k+1+1)).j by A73,COMPLSP2:1
          .= (ColSum N2).j+N*(k+1+1,j) by MATRIX_1:def 8,A74,A33
          .= (ColSum N).j by A34,A75;
          hence thesis;
        end;
        set g1=LineSum N2;
        set f3=LineSum N;
        A76: len LineSum N = len N by Def10;
        A77: len f3= k1+1 by A18,Def10;
        A78: k1 <= len f3 by NAT_1:11,A77;
        A79: len g1 = len N2 by Def10
              .= k1 by FINSEQ_1:80,A19;
        g1=f3|k1
        proof
          A80: len (f3|k1) = len g1 by A79,FINSEQ_1:80,A78;
          for n being Element of NAT st
          1 <= n & n <= len (f3|k1) holds (f3|k1).n=g1.n
          proof let n be Element of NAT;
            assume A81: 1 <= n & n <= len (f3|k1);
       A82: 1 <= n & n <= k1+1 by A80,A81,NAT_1:13,A79;
       A83: n in Seg len (f3|k1) by FINSEQ_1:3,A81;
       A84: n in dom(f3|k1) by FINSEQ_1:def 3,A83;
       A85: n in dom(f3|Seg k1) by FINSEQ_1:def 15,A84;
       A86: n in Seg len N by A18,A82,FINSEQ_1:3;
       A87: n in Seg k1 by A80,A81,FINSEQ_1:3,A79;
       A88: n in Seg len N2 by FINSEQ_1:80,A19,A87;
       A89: Line(N,n)=Line(N2,n)
            proof
         A90: len Line(N,n) = width N by MATRIX_1:def 8
                    .= width N2 by MATRIX_1:def 4,A18,A28,A29,A32
                    .= len (Line(N2,n)) by MATRIX_1:def 8;
              for n1 being Element of NAT st 1 <= n1 & n1 <= len (Line(N,n))
              holds Line(N,n).n1=Line(N2,n).n1
              proof let n1 be Element of NAT;
                  assume A91: 1 <= n1 & n1 <= len (Line(N,n));
                  A92: n1 in Seg (len (Line(N,n))) by FINSEQ_1:3,A91;
                  A93: n1 in Seg width N by MATRIX_1:def 8,A92;
                  A94: 1 <= n1 & n1 <= width N by A93,FINSEQ_1:3;
                  A95: n1 in Seg width N by MATRIX_1:def 8,A92;
                  A96: n1 in Seg width N2 by MATRIX_1:def 8,A92,A33;
                  A97: 1<= n & n<=len N & 1<=n1 & n1<=width N
                    by A93,FINSEQ_1:3,A80,A81,NAT_1:13,A79,A18;
                  A98: [n,n1] in Indices N by Th1,A94,A82,A18;
                  A99: 1<= n & n<=len N2 & 1<=n1 & n1<=width N2
                    by A80,A81,Def10,MATRIX_1:def 4,A28,A29,A32,A97;
                  A100: [n,n1] in Indices (N2) by Th1,A99;
                  consider pn being FinSequence of COMPLEX such that
                  A101: pn=N.n & N*(n,n1)=pn.n1 by MATRIX_1:def 6,A98;
                  A102: N2.n=N.n by FINSEQ_3:121,A80,A81,A79;
                  Line(N,n).n1=N*(n,n1) by MATRIX_1:def 8,A95
                        .=N2*(n,n1) by A100,MATRIX_1:def 6,A102,A101
                        .=Line(N2,n).n1 by MATRIX_1:def 8,A96;
                  hence thesis;
                end;
                hence thesis by FINSEQ_1:18,A90;
              end;
            (f3|k1).n=(f3|Seg k1).n by FINSEQ_1:def 15
                    .=(LineSum(N)).n by FUNCT_1:70,A85
                    .= Sum (Line(N2,n)) by A89,Def10,A86
                    .= g1.n by A88,Def10;
            hence thesis;
          end;
          hence thesis by FINSEQ_1:18,A80;
        end;then
        Sum LineSum N
          =Sum (LineSum N2)+(LineSum N)/.(k+1+1) by Th51,A76,A18
         .=Sum (LineSum N2)+(LineSum N).(k+1+1) by FINSEQ_4:def 4,A64
         .=Sum (ColSum N2)+(LineSum N).(k+1+1) by A17,A25
         .=Sum (ColSum N2)+Sum (Line(N,(k+1+1))) by Def10,A62
         .=Sum (ColSum(N2)+Line(N,(k+1+1))) by Th37,A66
         .=Sum ColSum N by A68,FINSEQ_1:18,A69;
      hence Sum LineSum N=Sum ColSum N;
     end;
   hence thesis;
  end;
 A103: for i holds P[i] from NAT_1:sch 2(A15,A16);
 A104: (len M)-'1+1=len M implies
 Sum LineSum M=Sum ColSum M by A103;
 thus thesis by A3,A104;
end;

definition let M be Matrix of COMPLEX;
  func SumAll M -> Element of COMPLEX equals
    Sum LineSum M;
  coherence;
end;

theorem Th53:
  for M being Matrix of COMPLEX holds
    ColSum M = LineSum(M@)
proof let M be Matrix of COMPLEX;
  A1: len (LineSum(M@))=len (M@) by Def10;
  A2: len ColSum M=width M by Def11;
  A3: len ColSum M=len LineSum(M@) by MATRIX_1:def 7,A1,A2;
  now let i be Element of NAT; assume
    A4: 1 <= i & i <= len ColSum M;
    A5: 1 <= i & i <= len LineSum(M@) by A4,MATRIX_1:def 7,A1,A2;
    A6: i in Seg len LineSum(M@) by FINSEQ_1:3,A5;
    A7: i in Seg len (M@) by Def10,A6;
    A8: i in Seg width M by FINSEQ_1:3,A4,A2;
    hence (ColSum M).i = Sum Col(M,i) by Def11
      .= Sum (Line(M@,i)) by MATRIX_2:17,A8
      .= (LineSum(M@)).i by Def10,A7;
  end;
 hence thesis by FINSEQ_1:18,A3;
end;

theorem Th54:
  for M being Matrix of COMPLEX st len M > 0 holds
    SumAll M = SumAll (M@)
proof let M be Matrix of COMPLEX;
  assume A1: len M > 0;
  SumAll M = Sum ColSum M by Th52,A1
  .= SumAll(M@) by Th53;
  hence thesis;
end;

definition let x,y be FinSequence of COMPLEX,M be Matrix of COMPLEX;
  assume A1: len x=len M & len y=width M;
  func QuadraticForm(x,M,y) -> Matrix of COMPLEX means :Def13:
    len it = len x & width it=len y &
    for i,j being Nat st [i,j] in Indices M holds
      it*(i,j)=(x.i)*(M*(i,j))*((y.j)*');
existence
 proof
  deffunc F(Nat,Nat) = (x.$1)*(M*($1,$2))*((y.$2)*');
  consider M1 being Matrix of len M,width M,COMPLEX such that
A2:for i,j st [i,j] in Indices M1 holds M1*(i,j) = F(i,j)
    from MATRIX_1:sch 1;
  take M1;
A3:len M1=len x by MATRIX_1:def 3,A1;
A4:now per cases;
      case A5:len M= 0;
        then width M1=0 by A3,MATRIX_1:def 4,A1;
        hence width M1=len y by A5,MATRIX_1:def 4,A1;
      end;
      case len M>0;
        hence width M1=len y by MATRIX_1:24,A1;
      end;
     end;
  dom M = dom M1 by A3,FINSEQ_3:31,A1;
  hence thesis by A2,MATRIX_1:def 3,A1,A4;
 end;
  uniqueness
  proof
   let M1,M2 be Matrix of COMPLEX;
   assume that
A6: len M1=len x & width M1=len y & for i,j st [i,j] in Indices M holds
           M1*(i,j)=(x.i)*(M*(i,j))*((y.j)*') and
A7: len M2=len x & width M2=len y & for i,j st [i,j] in Indices M holds
           M2*(i,j)=(x.i)*(M*(i,j))*((y.j)*');
    now let i,j;
      assume A8:[i,j] in Indices M1;
      dom M1 = dom M by A6,FINSEQ_3:31,A1;
    then A9: Indices M1=[:dom M,Seg width M:] &
             Indices M= [:dom M,Seg width M:] by A6,A1;
      hence M1*(i,j)=(x.i)*(M*(i,j))*((y.j)*') by A6,A8
                  .=M2*(i,j) by A7,A8,A9;
    end;
    hence M1=M2 by A6,A7,MATRIX_1:21;
  end;
end;

theorem Th55:
  for x,y being FinSequence of COMPLEX,M being Matrix of COMPLEX st
   len x = len M & len y = width M & len x > 0& len y > 0 holds
    (QuadraticForm(x,M,y))@ = (QuadraticForm(y,M@",x))*'
proof let x,y be FinSequence of COMPLEX,M be Matrix of COMPLEX;
  assume A1: len x=len M & len y =width M & len x > 0& len y > 0;
A2: len (x*') = len x by COMPLSP2:def 1;
A3: len (M@") = len (M@) by Def1
      .= len y by A1,MATRIX_1:def 7;
A4: width (M@") = width (M@) by Def1
      .= len x by MATRIX_2:12,A1;
A5: len ((QuadraticForm(x,M,y))@) = width (QuadraticForm(x,M,y))
      by MATRIX_1:def 7 .= len y by Def13,A1;
A6: len ((QuadraticForm(x,M,y))@) = width (QuadraticForm(x,M,y))
      by MATRIX_1:def 7;
A7: len ((QuadraticForm(y,M@",x))*')=len (QuadraticForm(y,M@",x))
      by Def1 .= len y by Def13,A3,A4;
A8: width (QuadraticForm(x,M,y))=len y by Def13,A1;
A9: len (M@")=len (M@) by Def1;
A10: len (M@")=width M by A9,MATRIX_1:def 7;
A11: len QuadraticForm(y,M@",x) = len y by Def13,A3,A4;
A12: width ((QuadraticForm(x,M,y))@)
       = len (QuadraticForm(x,M,y)) by MATRIX_2:12,A1,A8;
A13: width ((QuadraticForm(x,M,y))@)
       = len (QuadraticForm(x,M,y)) by MATRIX_2:12,A1,A8
      .= len x by Def13,A1;
A14: width ((QuadraticForm(y,M@",x))*')
      = width (QuadraticForm(y,M@",x)) by Def1
     .= len x by Def13,A3,A4;
A15: width (QuadraticForm(y,M@",x))= len x by Def13,A3,A4;
  for i,j st [i,j] in Indices ((QuadraticForm(x,M,y))@)
  holds
    ((QuadraticForm(y,M@",x))*')*(i,j)
     = ((QuadraticForm(x,M,y))@)*(i,j)
  proof let i,j;
     reconsider i'=i, j'=j as Element of NAT by ORDINAL1:def 13;
    assume [i,j] in Indices ((QuadraticForm(x,M,y))@);then
    A16: 1<=i & i<=width QuadraticForm(x,M,y) &
      1<=j & j<=len QuadraticForm(x,M,y) by A6,A12,Th1;
    A17: 1<=i & i<=len y &
      1<=j & j<=len x by Def13,A16,A1;
    A18: 1<=i & i<=width M &
      1<=j & j<=len M by Def13,A16,A1;
    A19: [j,i] in Indices M by A18,Th1;
    A20: 1<=i & i<=len QuadraticForm(y,M@",x) &
      1<=j & j<=width QuadraticForm(y,M@",x) by Def13,A16,A1,A11,A15;
    A21: [i,j] in Indices QuadraticForm(y,M@",x) by A20,Th1;
    A22: 1<=j & j<=len x by Def13,A16,A1;
    A23: 1<=i & i<=len (M@") &
      1<=j & j<=width (M@") by A10,Def13,A16,A1,A4;
    A24: [i,j] in Indices (M@") by A23,Th1;
    A25: [j,i] in Indices (QuadraticForm(x,M,y)) by A16,Th1;
    ((QuadraticForm(y,M@",x))*')*(i,j)
       = ((QuadraticForm(y,M@",x))*(i,j)*') by Def1,A21
      .= ((y.i)*((M@")*(i,j))*((x.j)*'))*' by Def13,A24,A3,A4
      .= ((y.i)*((M@")*(i',j'))*((x*'.j)))*' by COMPLSP2:def 1,A22
      .= (((y.i)*((M@")*(i,j)))*')*((x*'.j)*') by COMPLEX1:121
      .= (((y.i)*((M@")*(i',j')))*')*((x*')*'.j) by COMPLSP2:def 1,A17,A2
      .= ((y.i)*')*(((M@")*(i,j)*'))*((x*')*'.j) by COMPLEX1:121
      .= ((y.i)*')*(((M@")*')*(i,j))*((x*')*'.j) by Def1,A24
      .= ((y.i)*')*(((M@")*')*(i,j))*(x.j) by COMPLSP2:22
      .= ((y.i)*')*((M@)*(i,j))*(x.j) by Th2
      .= ((y.i)*')*(M*(j,i))*(x.j) by MATRIX_1:def 7,A19
      .= (x.j)*(M*(j,i))*((y.i)*')
      .= (QuadraticForm(x,M,y))*(j,i) by Def13,A19,A1
      .= ((QuadraticForm(x,M,y))@)*(i,j) by MATRIX_1:def 7,A25;
      hence thesis;
    end;
  hence thesis by MATRIX_1:21,A5,A7,A13,A14;
end;

theorem Th56:
  for x,y being FinSequence of COMPLEX,M being Matrix of COMPLEX st
   len x=len M & len y =width M holds
    (QuadraticForm(x,M,y))*'=QuadraticForm(x*',M*',y*')
proof let x,y be FinSequence of COMPLEX,M be Matrix of COMPLEX;
  assume A1: len x=len M & len y =width M;
  A2: len (y*') = len y by COMPLSP2:def 1;
  A3: len (M*') = len M by Def1;
  A4: len (x*') = len (M*') by COMPLSP2:def 1,A3,A1;
  A5: len (y*') = width (M*') by Def1,A1,A2;
  A6: len ((QuadraticForm(x,M,y))*') = len QuadraticForm(x,M,y) by Def1;
  A7: width ((QuadraticForm(x,M,y))*')=width QuadraticForm(x,M,y) by Def1;
  A8: len ((QuadraticForm(x,M,y))*') = len QuadraticForm(x,M,y) by Def1
      .= len M by Def13,A1;
  A9: len (QuadraticForm(x*',M*',y*')) = len (x*') by Def13,A4,A5
      .= len M by A1,COMPLSP2:def 1;
  A10: width ((QuadraticForm(x,M,y))*')=width (QuadraticForm(x,M,y)) by Def1
      .= len y by Def13,A1;
  A11: width QuadraticForm(x*',M*',y*')=len (y*') by Def13,A4,A5
      .= len y by COMPLSP2:def 1;
  for i,j st [i,j] in Indices ((QuadraticForm(x,M,y))*') holds
    ((QuadraticForm(x,M,y))*')*(i,j)
     = (QuadraticForm(x*',M*',y*'))*(i,j)
  proof let i,j;
     reconsider i'=i, j'=j as Element of NAT by ORDINAL1:def 13;
    assume [i,j] in Indices ((QuadraticForm(x,M,y))*');then
    A12: 1<=i & i<=len (QuadraticForm(x,M,y)) &
    1<=j & j<=width (QuadraticForm(x,M,y)) by A6,A7,Th1;
    A13: [i,j] in Indices QuadraticForm(x,M,y) by Th1,A12;
    A14: 1<=i & i<=len x & 1<=j & j<=len y by A12,Def13,A1;
    A15: 1<=i & i<=len M & 1<=j & j<=width M by A12,Def13,A1;
    A16: 1<=i & i<=len (M*') & 1<=j & j<=width (M*') by A15,Def1;
    A17: [i,j] in Indices (M*') by A16,Th1;
    A18: 1<=i & i<=len M & 1<=j & j<=width M by A12,A1,Def13;
    A19: [i,j] in Indices M by Th1,A18;
    ((QuadraticForm(x,M,y))*')*(i,j)
        =((QuadraticForm(x,M,y))*(i,j))*' by Def1,A13
       .=((x.i)*(M*(i,j))*((y.j)*'))*' by Def13,A19,A1
       .=((x.i)*(M*(i',j'))*((y*'.j)))*' by COMPLSP2:def 1,A14
       .= (((x.i)*(M*(i,j)))*')*((y*'.j)*') by COMPLEX1:121
       .= (((x.i)*(M*(i',j')))*')*((y*')*'.j) by COMPLSP2:def 1,A14,A2
       .= ((x.i)*')*((M*(i,j)*'))*(((y*')*'.j)) by COMPLEX1:121
       .= ((x.i)*')*((M*')*(i,j))*(((y*')*'.j)) by Def1,A19
       .= ((x.i)*')*((M*')*(i',j'))*(((y*').j)*') by COMPLSP2:def 1,A14,A2
       .= ((x*').i)*((M*')*(i',j'))*(((y*').j)*') by COMPLSP2:def 1,A14
       .= (QuadraticForm(x*',M*',y*'))*(i,j) by Def13,A17,A5,A4;
      hence thesis;
    end;
  hence thesis by MATRIX_1:21,A8,A9,A10,A11;
end;

theorem Th57:
  for x,y being FinSequence of COMPLEX st len x=len y & 0 < len y holds
    |(x,y)|=(|(y,x)|)*'
proof let x,y be FinSequence of COMPLEX;
   assume A1: len x=len y & 0 < len y;
   A2: 0+1 <= len x by A1,NAT_1:8;
   A3: len (x*') = len (x) by COMPLSP2:def 1;
   A4: len (mlt(y,(x*'))) = len y by Lm2,A1,A3;
   (|(y,x)|)*' = (Sum (mlt(y,(x*'))))*' by Th40,A1
  .= Sum (mlt(y,(x*'))*') by Th24,A1,A4,A2
  .= Sum (mlt(x,(y*'))) by Th25,A1
  .= |(x, y)| by Th40,A1;
    hence thesis;
end;

theorem Th58:
  for x,y being FinSequence of COMPLEX st len x=len y & 0 < len y holds
    (|(x,y)|)*'=|(x*',y*')|
proof let x,y be FinSequence of COMPLEX;
  assume A1: len x=len y & 0 < len y;
A2: 0+1 <= len x by A1,NAT_1:8;
A3: len (y*') = len y by COMPLSP2:def 1;
A4: len (mlt(x,y*')) = len x by Lm2,A1,A3;
A5: len (x*') = len x by COMPLSP2:def 1;
   |(x*',y*')| = Sum (mlt(x*',(y*')*')) by Th40,A1,A3,A5
  .= Sum ((mlt(x,y*'))*') by Th28,A1,A3
  .= (Sum (mlt(x,y*')))*' by Th24,A2,A4
  .= (|(x, y)|)*' by Th40,A1;
  hence thesis;
end;

theorem
  for M being Matrix of COMPLEX st width M > 0 holds
    (M@)*' =(M*')@
proof let M be Matrix of COMPLEX;
  assume A1: width M > 0;
  A2: len ((M@)*') = len (M@) by Def1;
  A3: width ((M@)*') = width (M@) by Def1;
  A4: len ((M@)*') = len (M@) by Def1 .= width M by MATRIX_1:def 7;
  A5: len ((M*')@) = width (M*') by MATRIX_1:def 7 .= width M by Def1;
  A6: width ((M@)*') = width (M@) by Def1 .= len M by MATRIX_2:12,A1;
  A7: width (M*') = width M by Def1;
  A8: width ((M*')@) = len (M*') by MATRIX_2:12,A7,A1 .= len M by Def1;
  for i,j st [i,j] in Indices ((M@)*') holds
    ((M@)*')*(i,j) = ((M*')@)*(i,j)
  proof let i,j;
    assume A9: [i,j] in Indices ((M@)*');
    A10: 1<= i & i<=len (M@) & 1<=j & j<=width (M@) by A2,A3,Th1,A9;
    A11: 1<= i & i<=len ((M@)*') & 1<=j & j<=width ((M@)*') by Th1,A9;
    A12: 1<= j & j<=len (M*') & 1<=i & i<=width (M*') by A11,Def1,A4,A6;
    A13: [i,j] in Indices (M@) by Th1,A10;
    A14: [j,i] in Indices (M) by MATRIX_1:def 7,A13;
    A15: [j,i] in Indices (M*') by Th1,A12;
    ((M@)*')*(i,j) = ((M@)*(i,j))*' by A13,Def1
        .= (M*(j,i))*' by MATRIX_1:def 7,A14
        .= ((M*'))*(j,i) by Def1,A14
        .= ((M*')@)*(i,j) by MATRIX_1:def 7,A15;
    hence thesis;
  end;
  hence thesis by MATRIX_1:21,A4,A5,A6,A8;
end;

theorem Th60:
  for x,y being FinSequence of COMPLEX,M being Matrix of COMPLEX st
   len x=width M & len y = len M & len x>0 & len y>0 holds
    |(x,(M@")*y)| = SumAll QuadraticForm(x,M@,y)
proof let x,y be FinSequence of COMPLEX,M be Matrix of COMPLEX;
 assume A1: len x=width M & len y = len M & len x>0 & len y>0;
  A2: len (M@)=len x by MATRIX_1:def 7,A1;
  A3: len (M@")=len (M@) by Def1
       .=len x by MATRIX_1:def 7,A1;
  A4: 0+1<=len y by A1,NAT_1:13;
  A5: width (M@) = len y by A1,MATRIX_2:12;
  A6: width ((M@)*')=len y by Def1,A5;
  A7: len ((M@")*y)= len x by A3,Def7;
  A8: len (((M@")*y)*')= len x by A7,COMPLSP2:def 1;
  A9: dom LineSum QuadraticForm(x,M@,y)
    = Seg len LineSum QuadraticForm(x,M@,y) by FINSEQ_1:def 3
   .= Seg len QuadraticForm(x,M@,y) by Def10;
  A10: len LineSum(QuadraticForm(x,M@,y))
      = len QuadraticForm(x,M@,y) by Def10
     .= len x by A2,A5,Def13;then
  A11: len LineSum(QuadraticForm(x,M@,y))
     = len mlt(x,((M@")*y)*') by A8,Lm2;
  A12: for i be Element of NAT st
  1<=i & i<=len (LineSum(QuadraticForm(x,M@,y)))
  holds (LineSum(QuadraticForm(x,M@,y))).i = (mlt(x,((M@")*y)*')).i
    proof let i be Element of NAT;assume
      A13: 1<=i & i<=len LineSum(QuadraticForm(x,M@,y));
      i in Seg len LineSum(QuadraticForm(x,M@,y)) by FINSEQ_1:3,A13;then
      A14: i in dom LineSum(QuadraticForm(x,M@,y)) by FINSEQ_1:def 3;
       i in Seg len mlt(x,((M@")*y)*') by A11,FINSEQ_1:3,A13;then
       A15: i in dom mlt(x,((M@")*y)*') by FINSEQ_1:def 3;
       A16: (mlt(x,((M@")*y)*')).i
           =(x.i)*((((M@")*y)*').i) by A15,Th19
          .=(x.i)*((((M@")*y).i)*') by A13,A7,A10,COMPLSP2:def 1;
      A17: len (M@)=len QuadraticForm(x,M@,y) by Def13,A2,A5;
      A18: len (M@)=len LineSum(QuadraticForm(x,M@,y)) by Def10,A17;
      A19: i in Seg len (M@") by FINSEQ_1:3,A13,A2,A3,A18;
      A20: i in Seg len ((M@)*') by A19;
      A21: i in Seg len (M@) by A20,Def1;
      A22: 1 <= i & i <= len (M@) by A21,FINSEQ_1:3;
      A23: len (Line(M@",i))=len y by  MATRIX_1:def 8,A6;
      A24: len mlt(Line(M@",i),y)>=1 by A4,Lm2,A23;
      A25: (mlt(x,((M@")*y)*')).i
         = (x.i)*((Sum(mlt(Line((M@"),i),y)))*') by A16,A19,Def7
        .=(x.i)*(Sum((mlt(Line((M@"),i),y))*')) by Th24,A24
        .=Sum((x.i)*(mlt(Line((M@"),i),y))*') by Th29;
        A26: len y = width QuadraticForm(x,M@,y) by Def13,A2,A5
                  .=len Line(QuadraticForm(x,M@,y),i) by MATRIX_1:def 8;
        A27: len Line((M@"),i)=len y by  MATRIX_1:def 8,A6;
        A28: len ((x.i)*(mlt(Line((M@"),i),y))*')
              =len ((mlt(Line((M@"),i),y))*') by COMPLSP2:3
             .=len (mlt(Line((M@"),i),y)) by COMPLSP2:def 1
             .=len (Line(QuadraticForm(x,M@,y),i)) by A26,A27,Lm2;
        A29: for j being Element of NAT st 1<=j &
        j<= len (Line(QuadraticForm(x,M@,y),i))
         holds ((x.i)*(mlt(Line((M@"),i),y))*').j
             =(Line(QuadraticForm(x,M@,y),i)).j
         proof let j be Element of NAT;
           assume A30: 1<=j & j<= len (Line(QuadraticForm(x,M@,y),i));
           A31: j in Seg len (Line(QuadraticForm(x,M@,y),i))
                                   by FINSEQ_1:3,A30;
           A32: j in Seg width QuadraticForm(x,M@,y)
              by MATRIX_1:def 8,A31;
           A33: len (Line(M@,i))=width (M@) by MATRIX_1:def 8
               .=len y by A1,MATRIX_2:12
               .=len (y*') by COMPLSP2:def 1;
           j in Seg len y by FINSEQ_1:3,A30,A26;then
           j in Seg len (y*') by COMPLSP2:def 1;then
           j in Seg len mlt(Line(M@,i),y*') by A33,Lm2;then
           A34: j in dom mlt(Line(M@,i),y*') by FINSEQ_1:def 3;
           A35: j in Seg width (M@) by FINSEQ_1:3,A30,A26,A5;
           A36: 1 <= j & j <= width (M@) by A30,A26,A1,MATRIX_2:12;
           A37: [i,j] in Indices (M@) by A36,A22,Th1;
           ((x.i)*(mlt(Line((M@"),i),y))*').j
            =(x.i)*(((mlt(Line((M@"),i),y))*').j) by COMPLSP2:16
           .=(x.i)*((mlt((Line((M@"),i))*',y*')).j) by Th28,A23
           .=(x.i)*((mlt(Line(M@,i),y*')).j) by Th43,A2,A3,A19
           .=(x.i)*(((Line(M@,i)).j)*((y*').j)) by Th19,A34
           .=(x.i)*(((Line(M@,i)).j)*((y.j)*')) by COMPLSP2:def 1,A30,A26
           .=(x.i)*((Line(M@,i)).j)*((y.j)*')
           .=(x.i)*((M@)*(i,j))*((y.j)*') by MATRIX_1:def 8,A35
           .=(QuadraticForm(x,M@,y))*(i,j) by Def13,A2,A5,A37;
          hence ((x.i)*(mlt(Line((M@"),i),y))*').j
                   =(Line (QuadraticForm(x,M@,y),i)).j by A32,MATRIX_1:def 8;
         end;
        (mlt(x,((M@")*y)*')).i=Sum (Line(QuadraticForm(x,M@,y),i))
         by A25,FINSEQ_1:18,A28,A29;
      hence (LineSum(QuadraticForm(x,M@,y))).i = (mlt(x,((M@")*y)*')).i
                   by Def10,A9,A14;
    end;
    Sum LineSum(QuadraticForm(x,M@,y))=Sum mlt(x,((M@")*y)*')
     by FINSEQ_1:18,A11,A12;
 hence |(x,(M@")*y)| = SumAll QuadraticForm(x,M@,y)
    by Th40,A1,A7;
end;

theorem Th61:
  for x,y being FinSequence of COMPLEX,M being Matrix of COMPLEX st
   len y=len M & len x =width M & len x>0 & len y>0 & len M > 0 holds
    |(M*x,y)| = SumAll QuadraticForm(x,M@,y)
proof let x,y be FinSequence of COMPLEX,
  M be Matrix of COMPLEX;
  assume A1: len y=len M & len x =width M & len x>0 & len y>0& len M > 0;
   A2: 0+1<=len x by A1,NAT_1:13;
   A3: len (M*x)=len M by Def7;
   A4: len (x*')=len x by COMPLSP2:def 1;
   A5: len (y*')=len y by COMPLSP2:def 1;
   A6: len (M@)=width M by MATRIX_1:def 7;
   A7: width (M@)=len M by MATRIX_2:12,A1;
   A8: len (y*')= len M by A1,COMPLSP2:def 1;
   A9: len (x*')= width M by A1,COMPLSP2:def 1;
   A10: len QuadraticForm(y*',M,x*') = len M by A8,A9,Def13;
   A11: (M@)@"=M*' by MATRIX_2:15,A1;
   thus |(M*x,y)| = (|(y,M*x)|)*' by Th57,A1,A3
            .= (|(y*',(M*x)*')|) by Th58,A3,A1
            .= |(y*',((M*')*(x*')) )| by Th45,A1,A2
            .= SumAll QuadraticForm(y*',(M@)@,x*')
               by Th60,A1,A4,A5,A6,A7,A11
            .= SumAll QuadraticForm(y*',M,x*') by MATRIX_2:15,A1
            .= SumAll((QuadraticForm(y*',M,x*'))@) by Th54,A1,A10
            .= SumAll((QuadraticForm(x*',(M@"),y*'))*') by Th55,A1,A4,A5
            .= SumAll(((QuadraticForm(x,(M@),y))*')*') by Th56,A1,A6,A7
            .= SumAll QuadraticForm(x,M@,y) by Th2;
end;

theorem Th62:
  for x,y being FinSequence of COMPLEX, M being Matrix of COMPLEX st
    len x=width M & len y =len M & width M>0 & len M>0 holds
      |(M*x,y)| = |(x,(M@")*y)|
proof let x,y be FinSequence of COMPLEX, M be Matrix of COMPLEX;
 assume A1: len x=width M & len y =len M & width M>0 & len M >0;
  |(x,(M@")*y)| = SumAll(QuadraticForm(x,M@,y)) by Th60,A1;
 hence |(M*x,y)| = |(x,(M@")*y)| by Th61,A1;
end;

theorem
  for x,y being FinSequence of COMPLEX,M being Matrix of COMPLEX st
   len x=len M & len y =width M & width M>0 & len M>0 & len x > 0 holds
    |(x,M*y)| = |((M@")*x,y)|
proof let x,y be FinSequence of COMPLEX, M be Matrix of COMPLEX;
  assume A1: len x=len M & len y =width M & width M>0 & len M>0 &
             len x > 0;
A2: len ((M@")*x)=len (M@") by Def7
                  .=len (M@) by Def1
                  .=width M by MATRIX_1:def 7;
  len (M*y) =len x by A1,Def7;
  hence |(x,M*y)| = (|(M*y,x)|)*' by Th57,A1
                .= (|(y,(M@")*x)|)*' by Th62,A1
                .= |((M@")*x,y)| by A2,A1,Th57;
end;
