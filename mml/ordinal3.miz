theorem
  A in B implies B = A+^(B-^A)
proof
  assume A in B;
  then A c= B by ORDINAL1:def 2;
  hence thesis by Def6;
end;

canceled 4;

theorem Th65:
  A+^B-^A = B
proof
  A c= A+^B by Th27;
  hence thesis by Def6;
end;

theorem Th66:
  A in B & (C c= A or C in A) implies A-^C in B-^C
proof
  assume that
A1: A in B and
A2: C c= A or C in A;
  A c= B by A1,ORDINAL1:def 2;
  then C c= B by A2,ORDINAL1:def 2,XBOOLE_1:1;
  then
A3: B = C+^(B-^C) by Def6;
  C c= A by A2,ORDINAL1:def 2;
  then A = C+^(A-^C) by Def6;
  hence thesis by A1,A3,Th25;
end;

theorem Th67:
  A-^A = {}
proof
  A+^{} = A by ORDINAL2:44;
  hence thesis by Def6;
end;

theorem
  A in B implies B-^A <> {} & {} in B-^A
proof
  assume A in B;
  then A-^A in B-^A by Th66;
  hence thesis by Th67;
end;

theorem Th69:
  A-^{} = A & {}-^A = {}
proof
A1: {}+^A = A by ORDINAL2:47;
  {} c= A by XBOOLE_1:2;
  hence A-^{} = A by A1,Def6;
  not A c= {} or A c= {};
  then thesis or A = {} by Def6,XBOOLE_1:3;
  hence thesis by A1,Def6;
end;

theorem
  A-^(B+^C) = (A-^B)-^C
proof
  now
    per cases;
    suppose
      B+^C c= A;
      then A = B+^C+^(A-^(B+^C)) by Def6;
      then A = B+^(C+^(A-^(B+^C))) by Th33;
      then C+^(A-^(B+^C)) = A-^B by Th65;
      hence thesis by Th65;
    end;
    suppose
A1:   not B+^C c= A;
A2:   now
        assume A = B+^(A-^B);
        then not C c= A-^B by A1,ORDINAL2:50;
        hence A-^B-^C = {} by Def6;
      end;
      B c= A or not B c= A;
      then
A3:   A = B+^(A-^B) or A-^B = {} by Def6;
      A-^(B+^C) = {} by A1,Def6;
      hence thesis by A3,A2,Th69;
    end;
  end;
  hence thesis;
end;

theorem
  A c= B implies C-^B c= C-^A
proof
  assume
A1: A c= B;
  then
A2: B = A+^(B-^A) by Def6;
A3: now
    assume
A4: B c= C;
    then
A5: C = B+^(C-^B) by Def6;
    A c= C by A1,A4,XBOOLE_1:1;
    then B+^(C-^B) = A+^(C-^A) by A5,Def6;
    then A+^((B-^A)+^(C-^B)) = A+^(C-^A) by A2,Th33;
    then (B-^A)+^(C-^B) = C-^A by Th24;
    hence thesis by Th27;
  end;
  now
    assume not B c= C;
    then C-^B = {} by Def6;
    hence thesis by XBOOLE_1:2;
  end;
  hence thesis by A3;
end;

theorem
  A c= B implies A-^C c= B-^C
proof
  assume
A1: A c= B;
A2: now
    assume
A3: C c= A;
    then
A4: A = C+^(A-^C) by Def6;
    C c= B by A1,A3,XBOOLE_1:1;
    then C+^(A-^C) c= C+^(B-^C) by A1,A4,Def6;
    hence thesis by Th26;
  end;
  now
    assume not C c= A;
    then A-^C = {} by Def6;
    hence thesis by XBOOLE_1:2;
  end;
  hence thesis by A2;
end;

theorem
  C <> {} & A in B+^C implies A-^B in C
proof
  assume
A1: C <> {};
A2: B+^C-^B = C by Th65;
  not B c= A implies A-^B = {} by Def6;
  hence thesis by A1,A2,Th10,Th66;
end;

theorem
  A+^B in C implies B in C-^A
proof
A1: A+^B-^A = B by Th65;
  A c= A+^B by Th27;
  hence thesis by A1,Th66;
end;

theorem
  A c= B+^(A-^B)
proof
  now
    per cases;
    suppose
      B c= A;
      hence thesis by Def6;
    end;
    suppose
A1:   not B c= A;
      then A-^B = {} by Def6;
      hence thesis by A1,ORDINAL2:44;
    end;
  end;
  hence thesis;
end;

theorem
  A*^C -^ B*^C = (A-^B)*^C
proof
A1: now
    assume
A2: not B c= A;
    then
A3: not B*^C c= A*^C or C = {} by Th38;
A4: {}*^C = {} by ORDINAL2:52;
A5: A*^{} = {} by ORDINAL2:55;
    A-^B = {} by A2,Def6;
    hence thesis by A3,A5,A4,Def6,Th69;
  end;
  now
    assume B c= A;
    then A = B+^(A-^B) by Def6;
    then A*^C = B*^C+^(A-^B)*^C by Th54;
    hence thesis by Th65;
  end;
  hence thesis by A1;
end;

theorem Th77:
  (A div^ B)*^B c= A
proof
  now
    per cases;
    suppose
      B <> {};
      then ex C st A = (A div^ B)*^B+^C & C in B by Def7;
      hence thesis by Th27;
    end;
    suppose
      B = {};
      then A div^ B = {} by Def7;
      then (A div^ B)*^B = {} by ORDINAL2:52;
      hence thesis by XBOOLE_1:2;
    end;
  end;
  hence thesis;
end;

theorem Th78:
  A = (A div^ B)*^B+^(A mod^ B)
proof
  (A div^ B)*^B c= A by Th77;
  hence thesis by Def6;
end;

theorem
  A = B*^C+^D & D in C implies B = A div^ C & D = A mod^ C
proof
  assume that
A1: A = B*^C+^D and
A2: D in C;
  thus B = A div^ C by A1,A2,Def7;
  hence thesis by A1,Th65;
end;

theorem
  A in B*^C implies A div^ C in B & A mod^ C in C
proof
A1: A = (A div^ C)*^C+^(A mod^ C) by Th78;
  assume
A2: A in B*^C;
  then C <> {} by ORDINAL2:55;
  then
A3: ex D st A = (A div^ C)*^C+^D & D in C by Def7;
  then
A4: (A div^ C)*^C c= A by Th27;
  assume not thesis;
  then B*^C c= (A div^ C)*^C by A3,A1,Th24,ORDINAL1:26,ORDINAL2:58;
  hence contradiction by A2,A4,ORDINAL1:7;
end;

theorem Th81:
  B <> {} implies A*^B div^ B = A
proof
  assume B <> {};
  then
A1: {} in B by Th10;
  A*^B = A*^B+^{} by ORDINAL2:44;
  hence thesis by A1,Def7;
end;

theorem
  A*^B mod^ B = {}
proof
A1: A*^{} = {} by ORDINAL2:55;
A2: A*^B-^A*^B = {} by Th67;
  {}-^(A*^B div^ B)*^B = {} by Th69;
  hence thesis by A1,A2,Th81;
end;

theorem
  {} div^ A = {} & {} mod^ A = {} & A mod^ {} = A
proof
A1: A = {} or A <> {};
  {} = {}*^A by ORDINAL2:52;
  hence {} div^ A = {} by A1,Def7,Th81;
  thus {} mod^ A = {} by Th69;
  thus A mod^ {} = A-^{} by ORDINAL2:55
    .= A by Th69;
end;

theorem
  A div^ 1 = A & A mod^ 1 = {}
proof
A1: A = A*^1 by ORDINAL2:56;
A2: A = A+^{} by ORDINAL2:44;
A3: {} in 1 by Th10;
  hence A div^ 1 = A by A1,A2,Def7;
  thus A mod^ 1 = A-^A by A1,A2,A3,Def7
    .= {} by Th67;
end;

