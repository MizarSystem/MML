:: from ZFREFLE1, 2007.03.14, A.T.

reserve e,u for set;

theorem
  succ A is_cofinal_with 1
proof
  deffunc F(set) = A;
  consider psi such that
A1: dom psi = 1 & for B st B in 1 holds psi.B = F(B) from ORDINAL2:sch 3;
  take psi;
  thus dom psi = 1 by A1;
  thus rng psi c= succ A
  proof
    let e;
    assume e in rng psi;
    then consider u such that
A2: u in 1 and
A3: e = psi.u by A1,FUNCT_1:def 5;
    reconsider u as Ordinal by A2;
    psi.u = A by A1,A2;
    hence thesis by A3,ORDINAL1:10;
  end;
  thus psi is increasing
  proof
    let B,C;
    assume that
A4: B in C and
A5: C in dom psi;
    thus thesis by A1,A4,A5,Th17;
  end;
A6: psi.{} = A by A1,Lm1,ORDINAL1:10;
  rng psi = {psi.{}} by A1,Lm1,FUNCT_1:14;
  hence thesis by A6,ORDINAL2:31;
end;
 
:: from ARYTM_3, 2007.10.23, A.T.

theorem Th87:
  for a,b being Ordinal st a+^b is natural holds a in omega & b in omega
proof
  let x,y be Ordinal such that
A1: x+^y in omega;
A2: y c= x+^y by Th27;
  x c= x+^y by Th27;
  hence thesis by A1,A2,ORDINAL1:22;
end;

registration
  let a, b be natural Ordinal;
  cluster a -^ b -> natural;
  coherence
  proof
    not b c= a or b c= a;
    then a -^ b = {} or a = b+^(a-^b) by Def6;
    hence a-^b in omega by Th87,ORDINAL1:def 12;
  end;
  cluster a *^ b -> natural;
  coherence
  proof
    defpred P[natural Ordinal] means $1*^b is natural;
A1: now
      let a be natural Ordinal;
      assume P[a];
      then reconsider c = a*^b as natural Ordinal;
      (succ a)*^b = c+^b by ORDINAL2:53;
      hence P[succ a];
    end;
A2: P[{}] by ORDINAL2:52;
    P[a] from ORDINAL2:sch 17(A2,A1);
    hence thesis;
  end;
end;

theorem
  for a,b being Ordinal st a*^b is natural non empty holds a in omega &
  b in omega
proof
  let x,y be Ordinal such that
A1: x*^y in omega;
  assume
A2: x*^y is non empty;
  then y <> {} by ORDINAL2:55;
  then
A3: x c= x*^y by Th39;
  x <> {} by A2,ORDINAL2:52;
  then y c= x*^y by Th39;
  hence thesis by A1,A3,ORDINAL1:22;
end;

definition
  let a,b be natural Ordinal;
  redefine func a+^b;
  commutativity
  proof
    let a,b be natural Ordinal;
    defpred R[natural Ordinal] means a+^$1 = $1+^a;
A1: now
      let b be natural Ordinal;
      assume
A2:   R[b];
      defpred P[natural Ordinal] means (succ b)+^$1 = succ (b+^$1);
A3:   now
        let a be natural Ordinal;
        assume
A4:     P[a];
        (succ b)+^succ a = succ ((succ b)+^a) by ORDINAL2:45
          .= succ (b+^succ a) by A4,ORDINAL2:45;
        hence P[succ a];
      end;
      (succ b)+^{} = succ b by ORDINAL2:44
        .= succ (b+^{}) by ORDINAL2:44;
      then
A5:   P[{}];
      P[a] from ORDINAL2:sch 17(A5,A3);
      hence R[succ b] by A2,ORDINAL2:45;
    end;
    a+^{} = a by ORDINAL2:44
      .= {}+^a by ORDINAL2:47;
    then
A6: R[{}];
    thus R[b] from ORDINAL2:sch 17(A6,A1);
  end;
end;

definition
  let a,b be natural Ordinal;
  redefine func a*^b;
  commutativity
  proof
    let a,b be natural Ordinal;
    defpred R[natural Ordinal] means a*^$1 = $1*^a;
A1: now
      let b be natural Ordinal;
      defpred P[natural Ordinal] means $1*^succ b = $1*^b+^$1;
      assume
A2:   R[b];
A3:   now
        let a be natural Ordinal;
        assume
A4:     P[a];
        (succ a)*^succ b = a*^(succ b)+^succ b by ORDINAL2:53
          .= a*^b+^(a+^succ b) by A4,Th33
          .= a*^b+^succ (a+^b) by ORDINAL2:45
          .= succ (a*^b+^(a+^b)) by ORDINAL2:45
          .= succ (a*^b+^b+^a) by Th33
          .= succ ((succ a)*^b+^a) by ORDINAL2:53
          .= (succ a)*^b+^succ a by ORDINAL2:45;
        hence P[succ a];
      end;
      {}*^succ b = {} by ORDINAL2:52
        .= {}+^{} by ORDINAL2:44
        .= {}*^b+^{} by ORDINAL2:52;
      then
A5:   P[{}];
      P[a] from ORDINAL2:sch 17(A5,A3);
      hence R[succ b] by A2,ORDINAL2:53;
    end;
    a*^{} = {} by ORDINAL2:55
      .= {}*^a by ORDINAL2:52;
    then
A6: R[{}];
    thus R[b] from ORDINAL2:sch 17(A6,A1);
  end;
end;
