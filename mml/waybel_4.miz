A11: x <= sup ({x} "/\" D) by WAYBEL_3:def 5;
    sup ({x} "/\" D) <= x by Lm11;
    hence x = sup ({x} "/\" D) by A11,ORDERS_2:25;
  end;
  then for x being Element of L, D being non empty directed Subset of L
  st x <= sup D holds x = sup ({x} "/\" D);
  hence thesis by WAYBEL_2:52;
end;

