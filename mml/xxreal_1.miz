theorem Th5: :: BORSUK_4:10
 x in [.p,q.] implies x in ].p,q.[ or x = p or x = q
  proof
   assume
A1:   x in [. p, q .];
     then reconsider s = x as ext-real number by MEMBERED:def 2;
      p <= s & s <= q by A1,Th1;
     then (p = s or p < s) & (s = q or s < q) by XXREAL_0:1;
    hence x in ].p,q.[ or x = p or x = q by Th4;
  end;

