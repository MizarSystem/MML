theorem :: MEASURE5:12,15
  p <= q implies ].q,p.] = {}
proof
  assume
A1: p <= q;
  assume ].q,p.] <> {};
  then consider r such that
A2: r in ].q,p.] by MEMBERED:8;
  q < r & r <= p by A2,Th2;
  hence contradiction by A1,XXREAL_0:2;
end;

