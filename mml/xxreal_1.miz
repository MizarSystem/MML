  cluster [.r,r.] -> non empty;
  coherence
  proof
    [.r,r.] = {r} by Th17;
    hence thesis;
  end;
  cluster [.r,r.[ -> empty;
  coherence by Th18;
  cluster ].r,r.] -> empty;
  coherence by Th19;
  cluster ].r,r.[ -> empty;
  coherence by Th20;
end;

