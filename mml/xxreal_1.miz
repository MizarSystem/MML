theorem Th51:
  r < s & ].r,s.[ c= [.p,q.] implies p <= r & s <= q
proof
  assume that
A1: r < s and
A2: ].r,s.[ c= [.p,q.];
  now
    let t;
    assume r < t & t < s;
    then t in ].r,s.[ by Th4;
    hence p <= t by A2,Th1;
  end;
  hence p <= r by A1,XREAL_1:230;
  now
