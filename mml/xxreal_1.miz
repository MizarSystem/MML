begin

 reserve x,y,A,B for set,
         p,q,r,s,t,u for ext-real number,
         a,b for Element of ExtREAL,
         X,Y for ext-real-membered set;

scheme Conti { P,Q[set] }:
 ex s st (for r st P[r] holds r <= s) & for r st Q[r] holds s <= r
provided
A1: for r,s st P[r] & Q[s] holds r <= s
proof
  set A = { a : P[a]},  B = { a : Q[a]};
  reconsider X = A /\ REAL,  Y = B /\ REAL as Subset of REAL by XBOOLE_1:17;
 per cases;
 suppose
A2: X = {};
  per cases;
  suppose
A3: +infty in A;
   take +infty;
   thus for r st P[r] holds r <= +infty by XXREAL_0:3;
     ex a st a = +infty & P[a] by A3;
   hence thesis by A1;
  end;
  suppose
A4:  not +infty in A;
   take -infty;
   thus for r st P[r] holds r <= -infty
    proof let r such that
A5:    P[r];
       r in ExtREAL by XXREAL_0:def 1;
      then A6:     r in A by A5;
     per cases by A6,A4, XXREAL_0:14;
     suppose r = -infty;
     hence thesis;
     end;
     suppose r in REAL;
     hence thesis by A2,A6,XBOOLE_0:def 3;
     end;
    end;
   thus thesis by XXREAL_0:5;
  end;
 end;
 suppose
A7: Y = {};
  per cases;
  suppose
A8: -infty in B;
   take -infty;
   thus for r st P[r] holds r <= -infty
    proof ex a st a = -infty & Q[a] by A8;
     hence thesis by A1;
    end;
   thus thesis by XXREAL_0:5;
  end;
  suppose
A9:  not -infty in B;
   take +infty;
   thus for r st P[r] holds r <= +infty by XXREAL_0:3;
   let r such that
A10:  Q[r];
     r in ExtREAL by XXREAL_0:def 1;
    then A11:  r in B by A10;
   per cases by A11,A9,XXREAL_0:14;
    suppose r = +infty;
     hence thesis;
    end;
    suppose r in REAL;
     hence thesis by A7,A11,XBOOLE_0:def 3;
    end;
  end;
 end;
 suppose that
A12: X <> {} and
A13: Y <> {};
    for x,y being real number st x in X & y in Y holds x <= y
    proof let x,y be real number;
     assume x in X;
      then x in A by XBOOLE_0:def 3;
      then consider a such that
A14:    a = x and
A15:    P[a];
     assume y in Y;
      then y in B by XBOOLE_0:def 3;
      then ex a st a = y & Q[a];
     hence x <= y by A14,A15,A1;
    end;
   then consider s being real number such that
A16:   for x,y being real number st x in X & y in Y holds x <= s & s <= y
                  by AXIOMS:26;
   reconsider s as ext-real number;
  take s;
  thus for r st P[r] holds r <= s
   proof let r;
     consider x being Element of REAL such that
A17:    x in Y by A13,SUBSET_1:10;
      x in B by A17,XBOOLE_0:def 3;
     then A18:    ex a st x = a & Q[a];
    assume
A19:  P[r];
     then A20:   r <= x by A18,A1;
    per cases by A20,XXREAL_0:13;
    suppose
A21:   r in REAL;
     then reconsider r as real number by XREAL_0:def 1;
      r is Element of ExtREAL by XXREAL_0:def 1;
     then r in A by A19;
     then r in X by A21,XBOOLE_0:def 3;
    hence thesis by A17,A16;
    end;
    suppose r = -infty;
    hence r <= s by XXREAL_0:5;
    end;
   end;
  let r;
   consider x being Element of REAL such that
A22:  x in X by A12,SUBSET_1:10;
    x in A by A22,XBOOLE_0:def 3;
   then A23:  ex a st x = a & P[a];
  assume
A24: Q[r];
   then A25: x <= r by A23,A1;
  per cases by A25,XXREAL_0:10;
  suppose
A26: r in REAL;
   then reconsider r as real number by XREAL_0:def 1;
    r is Element of ExtREAL by XXREAL_0:def 1;
   then r in B by A24;
   then r in Y by A26,XBOOLE_0:def 3;
  hence thesis by A22,A16;
  end;
  suppose r = +infty;
  hence s <= r by XXREAL_0:3;
  end;
 end;
end;

begin :: Intervals

definition let r,s;
 func [.r,s.] equals
  { a : r <= a & a <= s };
 correctness;
 func [.r,s.[ equals
  { a : r <= a & a < s };
 correctness;
 func ].r,s.] equals
  { a : r < a & a <= s };
 correctness;
 func ].r,s.[ equals
  { a : r < a & a < s };
 correctness;
end;

theorem Th1: :: RCOMP_1:48
 t in [.r,s.] iff r <= t & t <= s
 proof
  hereby assume t in [.r,s.];
    then ex a st a = t & r <= a & a <= s;
   hence r <= t & t <= s;
  end;
    t is Element of ExtREAL by XXREAL_0:def 1;
  hence thesis;
 end;

theorem Th2: :: RCOMP_2:4
 t in ].r,s.] iff r < t & t <= s
 proof
  hereby assume t in ].r,s.];
    then ex a st a = t & r < a & a <= s;
   hence r < t & t <= s;
  end;
    t is Element of ExtREAL by XXREAL_0:def 1;
  hence thesis;
 end;

theorem Th3: :: RCOMP_2:3
