A3: t in [.r,s.];
  then
A4: r <= t by Th1;
A5: t <= s by A3,Th1;
A6: p <= t by A1,A4,XXREAL_0:2;
  t <= q by A2,A5,XXREAL_0:2;
  hence thesis by A6,Th1;
end;

