    = |[ diff(g1,f1.t0)*diff(f1,t0),diff(g2*f2,t0),diff(g3*f3,t0) ]|
      by A1,A2,FDIFF_2:13
   .= |[ diff(g1,f1.t0)*diff(f1,t0),diff(g2,f2.t0)*diff(f2,t0),
      diff(g3*f3,t0) ]| by A1,A2,FDIFF_2:13
   .= |[ diff(g1,f1.t0)*diff(f1,t0),diff(g2,f2.t0)*diff(f2,t0),
      diff(g3,f3.t0)*diff(f3,t0) ]| by A1,A2,FDIFF_2:13;
