end;

theorem
  0 * <e1> = |[ 0,0,0 ]|
proof
<e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
   then 0 * <e1> = |[ 0 * 1,0 * 0,0 * 0 ]| by Lm3
 .= |[ 0,0,0 ]|;
   hence thesis;
end;

theorem
  0 * <e2> = |[ 0,0,0 ]|
proof
<e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
