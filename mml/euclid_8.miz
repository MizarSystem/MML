   hence thesis;
end;

theorem
  -<e1> = |[ -1,0,0 ]|
proof
A1:<e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
  -<e1> = |[ (-1)*<e1>.1, (-1)*<e1>.2, (-1)*<e1>.3 ]| by Lm3
      .= |[ -1,0,0 ]| by A1;
  hence thesis;
end;

theorem
  -<e2> = |[ 0,-1,0 ]|
proof
