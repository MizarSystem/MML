theorem
  p = p.1*<e1> + p.2*<e2> + p.3*<e3>
proof
A2:  <e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
A3:  <e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
A4:  <e3>.1 = 0 & <e3>.2 = 0 & <e3>.3 = 1 by FINSEQ_1:62;
A5:  p.1*<e1> + p.2*<e2> + p.3*<e3>
     = |[ p.1*1,p.1*0,p.1*0 ]| + p.2*<e2> + p.3*<e3> by A2,Lm3
    .= |[ p.1,0,0 ]| + |[ p.2*0,p.2*1,p.2*0 ]| + p.3*<e3> by A3,Lm3
    .= |[ p.1,0,0 ]| + |[ 0,p.2,0 ]| + |[ p.3*0,p.3*0,p.3*1 ]| by A4,Lm3
    .= |[ p.1,0,0 ]| + |[ 0,p.2,0 ]| + |[ 0,0,p.3 ]|;
A6:  |[ p.1,0,0 ]|.1 = p.1 & |[ p.1,0,0 ]|.2 = 0 & |[ p.1,0,0 ]|.3 = 0
     by FINSEQ_1:62;
A7:  |[ 0,p.2,0 ]|.1 = 0 & |[ 0,p.2,0 ]|.2 = p.2 & |[ 0,p.2,0 ]|.3 = 0
     by FINSEQ_1:62;
A8:  |[ 0,0,p.3 ]|.1 = 0 & |[ 0,0,p.3 ]|.2 = 0 & |[ 0,0,p.3 ]|.3 = p.3
     by FINSEQ_1:62;
A9:  p.1*<e1> + p.2*<e2> + p.3*<e3>
     = |[ p.1+0,0+p.2,0+0 ]| + |[ 0,0,p.3 ]| by A5,A6,A7,Lm4
    .= |[ p.1,p.2,0 ]| + |[ 0,0,p.3 ]|;
 |[ p.1,p.2,0 ]|.1 = p.1 & |[ p.1,p.2,0 ]|.2 = p.2 &
     |[ p.1,p.2,0 ]|.3 = 0 by FINSEQ_1:62;
     then
p.1*<e1> + p.2*<e2> + p.3*<e3> = |[ p.1+0,p.2+0,0 +p.3]| by A8,A9,Lm4
    .= |[ p.1,p.2,p.3 ]|;
     hence p = p.1*<e1> + p.2*<e2> + p.3*<e3> by Lm1;
end;

theorem ThA7:
  r*p = r*p.1*<e1> + r*p.2*<e2> + r*p.3*<e3>
proof
A2:  <e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
A3:  <e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
A4:  <e3>.1 = 0 & <e3>.2 = 0 & <e3>.3 = 1 by FINSEQ_1:62;
A5:  r*p.1*<e1> + r*p.2*<e2> + r*p.3*<e3>
     = |[ r*p.1*1,r*p.1*0,r*p.1*0 ]| + r*p.2*<e2> + r*p.3*<e3>
       by A2,Lm3
    .= |[ r*p.1,0,0 ]| + |[ r*p.2*0,r*p.2*1,r*p.2*0 ]| + r*p.3*<e3>
       by A3,Lm3
    .= |[ r*p.1,0,0 ]| + |[ 0,r*p.2,0 ]| + |[ r*p.3*0,r*p.3*0,r*p.3*1 ]|
       by A4,Lm3
    .= |[ r*p.1,0,0 ]| + |[ 0,r*p.2,0 ]| + |[ 0,0,r*p.3 ]|;
A6:  |[ r*p.1,0,0 ]|.1 = r*p.1 & |[ r*p.1,0,0 ]|.2 = 0 & |[ r*p.1,0,0 ]|.3 = 0
     by FINSEQ_1:62;
A7:  |[ 0,r*p.2,0 ]|.1 = 0 & |[ 0,r*p.2,0 ]|.2 = r*p.2 & |[ 0,r*p.2,0 ]|.3 = 0
     by FINSEQ_1:62;
A8:  |[ 0,0,r*p.3 ]|.1 = 0 & |[ 0,0,r*p.3 ]|.2 = 0 & |[ 0,0,r*p.3 ]|.3 = r*p.3
     by FINSEQ_1:62;
A9:  r*p.1*<e1> + r*p.2*<e2> + r*p.3*<e3>
     = |[ r*p.1+0,0+r*p.2,0+0 ]| + |[ 0,0,r*p.3 ]| by A5,A6,A7,Lm4
    .= |[ r*p.1,r*p.2,0 ]| + |[ 0,0,r*p.3 ]|;
 |[ r*p.1,r*p.2,0 ]|.1 = r*p.1 & |[ r*p.1,r*p.2,0 ]|.2 = r*p.2 &
     |[ r*p.1,r*p.2,0 ]|.3 = 0 by FINSEQ_1:62;
     then r*p.1*<e1> + r*p.2*<e2> + r*p.3*<e3>
     = |[ r*p.1+0,r*p.2+0,0+r*p.3]| by A8,A9,Lm4
    .= |[ r*p.1,r*p.2,r*p.3 ]|;
     hence r*p = r*p.1*<e1> + r*p.2*<e2> + r*p.3*<e3> by Lm3;
end;

theorem Th29:
  |[ x,y,z ]| = x*<e1> + y*<e2> + z*<e3>
proof
A2:  <e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
A3:  <e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
A4:  <e3>.1 = 0 & <e3>.2 = 0 & <e3>.3 = 1 by FINSEQ_1:62;
    set p = |[ x,y,z ]|;
 A6:  x*<e1> + y*<e2> + z*<e3> = |[ x*1,x*0,x*0 ]| + y*<e2> + z*<e3> by A2,Lm3
     .= |[ x,0,0 ]| + |[ y*0,y*1,y*0 ]| + z*<e3> by A3,Lm3
     .= |[ x,0,0 ]| + |[ 0,y,0 ]| + |[ z*0,z*0,z*1 ]| by A4,Lm3
     .= |[ x,0,0 ]| + |[ 0,y,0 ]| + |[ 0,0,z ]|;
 A7:  |[ x,0,0 ]|.1 = x & |[ x,0,0 ]|.2 = 0 & |[ x,0,0 ]|.3 = 0 by FINSEQ_1:62;
 A8:  |[ 0,y,0 ]|.1 = 0 & |[ 0,y,0 ]|.2 = y & |[ 0,y,0 ]|.3 = 0 by FINSEQ_1:62;
 A9:  |[ 0,0,z ]|.1 = 0 & |[ 0,0,z ]|.2 = 0 & |[ 0,0,z ]|.3 = z by FINSEQ_1:62;
 A10: x*<e1> + y*<e2> + z*<e3>
      = |[ x+0,y+0,0+0 ]| + |[ 0,0,z ]| by A6,A7,A8,Lm4
     .= |[ x,y,0 ]| + |[ 0,0,z ]|;
  |[ x,y,0 ]|.1 = x & |[ x,y,0 ]|.2 = y & |[ x,y,0 ]|.3 = 0 by FINSEQ_1:62;
      then x*<e1> + y*<e2> + z*<e3> = |[ x+0,y+0,0+z ]| by A9,A10,Lm4
       .= |[ x,y,z ]|;
      hence p = x*<e1> + y*<e2> + z*<e3>;
end;

theorem ThA8:
  r*|[ x,y,z ]| = r*x*<e1> + r*y*<e2> + r*z*<e3>
proof
A2:  <e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
A3:  <e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
A4:  <e3>.1 = 0 & <e3>.2 = 0 & <e3>.3 = 1 by FINSEQ_1:62;
    set p = |[ x,y,z ]|;
 A6:  r*x*<e1> + r*y*<e2> + r*z*<e3>
      = |[ r*x*1,r*x*0,r*x*0 ]| + r*y*<e2> + r*z*<e3> by A2,Lm3
     .= |[ r*x,0,0 ]| + |[ r*y*0,r*y*1,r*y*0 ]| + r*z*<e3> by A3,Lm3
     .= |[ r*x,0,0 ]| + |[ 0,r*y,0 ]| + |[ r*z*0,r*z*0,r*z*1 ]| by A4,Lm3
     .= |[ r*x,0,0 ]| + |[ 0,r*y,0 ]| + |[ 0,0,r*z ]|;
 A7:  |[ r*x,0,0 ]|.1 = r*x & |[ r*x,0,0 ]|.2 = 0 & |[ r*x,0,0 ]|.3 = 0
      by FINSEQ_1:62;
 A8:  |[ 0,r*y,0 ]|.1 = 0 & |[ 0,r*y,0 ]|.2 = r*y & |[ 0,r*y,0 ]|.3 = 0
      by FINSEQ_1:62;
 A9:  |[ 0,0,r*z ]|.1 = 0 & |[ 0,0,r*z ]|.2 = 0 & |[ 0,0,r*z ]|.3 = r*z
      by FINSEQ_1:62;
 A10: r*x*<e1> + r*y*<e2> + r*z*<e3>
      = |[ r*x+0,r*y+0,0+0 ]| + |[ 0,0,r*z ]| by A6,A7,A8,Lm4
     .= |[ r*x,r*y,0 ]| + |[ 0,0,r*z ]|;
  |[ r*x,r*y,0 ]|.1 = r*x & |[ r*x,r*y,0 ]|.2 = r*y & |[ r*x,r*y,0 ]|.3 = 0
      by FINSEQ_1:62;
      then r*x*<e1> + r*y*<e2> + r*z*<e3>
       = |[ r*x+0,r*y+0,0+r*z ]| by A9,A10,Lm4
      .= |[ r*x,r*y,r*z ]|;
      hence r*p = r*x*<e1> + r*y*<e2> + r*z*<e3> by LmA8;
end;

theorem
  -p = - p.1*<e1> - p.2*<e2> - p.3*<e3>
proof
   -1 is Element of REAL by XREAL_0:def 1; then
   -p = (-1)*p.1*<e1> + (-1)*p.2*<e2> + (-1)*p.3*<e3> by ThA7
     .= -p.1*<e1> + ((-1)*p.2)*<e2> + (-p.3)*<e3> by RVSUM_1:71
     .= -p.1*<e1> + -p.2*<e2> +((-1)*p.3)*<e3> by RVSUM_1:71
     .= - p.1*<e1> - p.2*<e2> - p.3*<e3> by RVSUM_1:71;
   hence thesis;
end;

theorem
  -|[ x,y,z ]| = -x*<e1> - y*<e2> - z*<e3>
proof
  -1 in REAL by XREAL_0:def 1; then
  -|[ x,y,z ]| = (-1)*x*<e1> + (-1)*y*<e2> + (-1)*z*<e3> by ThA8
  .=-x*<e1> + ((-1)*y)*<e2> + (-z)*<e3> by RVSUM_1:71
  .=-x*<e1> + -y*<e2> + ((-1)*z)*<e3> by RVSUM_1:71
  .=-x*<e1> - y*<e2> - z*<e3> by RVSUM_1:71;
  hence thesis;
end;

theorem
  p1+p2 = (p1.1+p2.1)*<e1>+(p1.2+p2.2)*<e2>+(p1.3+p2.3)*<e3>
proof
A1:  <e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
A2:  <e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
A3:  <e3>.1 = 0 & <e3>.2 = 0 & <e3>.3 = 1 by FINSEQ_1:62;
A4: (p1+p2).1 = p1.1+p2.1 by RVSUM_1:27;
A5: (p1+p2).2 = p1.2+p2.2 by RVSUM_1:27;
A6: (p1+p2).3 = p1.3+p2.3 by RVSUM_1:27;
A0: (p1+p2).1*<e1> + (p1+p2).2*<e2> + (p1+p2).3*<e3>
  = |[ (p1+p2).1*1,(p1+p2).1*0,(p1+p2).1*0 ]| + (p1+p2).2*<e2>
    + (p1+p2).3*<e3> by A1,Lm3
 .= |[ (p1+p2).1,0,0 ]| + |[ (p1+p2).2*0,(p1+p2).2*1,(p1+p2).2*0 ]|
    + (p1+p2).3*<e3> by A2,Lm3
 .= |[ (p1+p2).1,0,0 ]| + |[ 0,(p1+p2).2,0 ]|
    + |[ (p1+p2).3*0,(p1+p2).3*0,(p1+p2).3*1 ]| by A3,Lm3
 .= |[ (p1+p2).1,0,0 ]| + |[ 0,(p1+p2).2,0 ]| + |[ 0,0,(p1+p2).3 ]|;
A7: |[ (p1+p2).1,0,0 ]|.1 = (p1+p2).1 & |[ (p1+p2).1,0,0 ]|.2 = 0 &
    |[ (p1+p2).1,0,0 ]|.3 = 0 by FINSEQ_1:62;
A8: |[ 0,(p1+p2).2,0 ]|.1 = 0 & |[ 0,(p1+p2).2,0 ]|.2 = (p1+p2).2 &
    |[ 0,(p1+p2).2,0 ]|.3 = 0 by FINSEQ_1:62;
A9: |[ 0,0,(p1+p2).3 ]|.1 = 0 & |[ 0,0,(p1+p2).3 ]|.2 = 0 &
    |[ 0,0,(p1+p2).3 ]|.3 = (p1+p2).3 by FINSEQ_1:62;
A10: (p1+p2).1*<e1> + (p1+p2).2*<e2> + (p1+p2).3*<e3>
  = |[ (p1+p2).1+0,0+(p1+p2).2,0+0 ]| + |[ 0,0,(p1+p2).3 ]|
    by A0,A7,A8,Lm4
 .= |[ (p1+p2).1,(p1+p2).2,0 ]| + |[ 0,0,(p1+p2).3 ]|;
 |[ (p1+p2).1,(p1+p2).2,0 ]|.1 = (p1+p2).1 &
     |[ (p1+p2).1,(p1+p2).2,0 ]|.2 = (p1+p2).2 &
     |[ (p1+p2).1,(p1+p2).2,0 ]|.3 = 0 by FINSEQ_1:62;
 then (p1+p2).1*<e1> + (p1+p2).2*<e2> + (p1+p2).3*<e3>
   = |[ (p1+p2).1+0,(p1+p2).2+0,0 +(p1+p2).3]| by A9,A10,Lm4
  .= |[ (p1+p2).1,(p1+p2).2,(p1+p2).3 ]|;
     hence thesis by A4,A5,A6,Lm1;
end;

theorem ThA18:
  p1-p2 = (p1.1-p2.1)*<e1>+(p1.2-p2.2)*<e2>+(p1.3-p2.3)*<e3>
proof
A1:  <e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
A2:  <e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
A3:  <e3>.1 = 0 & <e3>.2 = 0 & <e3>.3 = 1 by FINSEQ_1:62;
A4: (p1-p2).1 = p1.1-p2.1 by RVSUM_1:48;
A5: (p1-p2).2 = p1.2-p2.2 by RVSUM_1:48;
A6: (p1-p2).3 = p1.3-p2.3 by RVSUM_1:48;
A0: (p1-p2).1*<e1> + (p1-p2).2*<e2> + (p1-p2).3*<e3>
  = |[ (p1-p2).1*1,(p1-p2).1*0,(p1-p2).1*0 ]| + (p1-p2).2*<e2>
    + (p1-p2).3*<e3> by A1,Lm3
 .= |[ (p1-p2).1,0,0 ]| + |[ (p1-p2).2*0,(p1-p2).2*1,(p1-p2).2*0 ]|
    + (p1-p2).3*<e3> by A2,Lm3
 .= |[ (p1-p2).1,0,0 ]| + |[ 0,(p1-p2).2,0 ]|
    + |[ (p1-p2).3*0,(p1-p2).3*0,(p1-p2).3*1 ]| by A3,Lm3
 .= |[ (p1-p2).1,0,0 ]| + |[ 0,(p1-p2).2,0 ]| + |[ 0,0,(p1-p2).3 ]|;
A7: |[ (p1-p2).1,0,0 ]|.1 = (p1-p2).1 & |[ (p1-p2).1,0,0 ]|.2 = 0 &
    |[ (p1-p2).1,0,0 ]|.3 = 0 by FINSEQ_1:62;
A8: |[ 0,(p1-p2).2,0 ]|.1 = 0 & |[ 0,(p1-p2).2,0 ]|.2 = (p1-p2).2 &
    |[ 0,(p1-p2).2,0 ]|.3 = 0 by FINSEQ_1:62;
A9: |[ 0,0,(p1-p2).3 ]|.1 = 0 & |[ 0,0,(p1-p2).3 ]|.2 = 0 &
    |[ 0,0,(p1-p2).3 ]|.3 = (p1-p2).3 by FINSEQ_1:62;
A10: (p1-p2).1*<e1> + (p1-p2).2*<e2> + (p1-p2).3*<e3>
  = |[ (p1-p2).1+0,0+(p1-p2).2,0+0 ]| + |[ 0,0,(p1-p2).3 ]|
    by A0,A7,A8,Lm4
 .= |[ (p1-p2).1,(p1-p2).2,0 ]| + |[ 0,0,(p1-p2).3 ]|;
 |[ (p1-p2).1,(p1-p2).2,0 ]|.1 = (p1-p2).1 &
     |[ (p1-p2).1,(p1-p2).2,0 ]|.2 = (p1-p2).2 &
     |[ (p1-p2).1,(p1-p2).2,0 ]|.3 = 0 by FINSEQ_1:62;
    then (p1-p2).1*<e1> + (p1-p2).2*<e2> + (p1-p2).3*<e3>
   = |[ (p1-p2).1+0,(p1-p2).2+0,0 +(p1-p2).3]| by A9,A10,Lm4
  .= |[ (p1-p2).1,(p1-p2).2,(p1-p2).3 ]|;
     hence thesis by A4,A5,A6,Lm1;
end;

theorem
  |[ x1,x2,x3 ]| + |[ y1,y2,y3 ]| = (x1+y1)*<e1> + (x2+y2)*<e2> + (x3+y3)*<e3>
proof
A2:  <e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
A3:  <e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
A4:  <e3>.1 = 0 & <e3>.2 = 0 & <e3>.3 = 1 by FINSEQ_1:62;
A1: (x1+y1)*<e1> + (x2+y2)*<e2> + (x3+y3)*<e3>
  = |[ (x1+y1)*1,(x1+y1)*0,(x1+y1)*0 ]| + (x2+y2)*<e2>
    + (x3+y3)*<e3> by A2,Lm3
 .= |[ x1+y1,0,0 ]| + |[ (x2+y2)*0,(x2+y2)*1,(x2+y2)*0 ]|
    + (x3+y3)*<e3> by A3,Lm3
 .= |[ x1+y1,0,0 ]| + |[ 0,x2+y2,0 ]|
    + |[ (x3+y3)*0,(x3+y3)*0,(x3+y3)*1 ]| by A4,Lm3
 .= |[ x1+y1,0,0 ]| + |[ 0,x2+y2,0 ]| + |[ 0,0,x3+y3 ]|;
A5: |[ x1+y1,0,0 ]|.1 = x1+y1 & |[ x1+y1,0,0 ]|.2 = 0
    & |[ x1+y1,0,0 ]|.3 = 0 by FINSEQ_1:62;
A6: |[ 0,x2+y2,0 ]|.1 = 0 & |[ 0,x2+y2,0 ]|.2 = x2+y2
    & |[ 0,x2+y2,0 ]|.3 = 0 by FINSEQ_1:62;
A7: |[ 0,0,x3+y3 ]|.1 = 0 & |[ 0,0,x3+y3 ]|.2 = 0
    & |[ 0,0,x3+y3 ]|.3 = x3+y3 by FINSEQ_1:62;
A0: (x1+y1)*<e1> + (x2+y2)*<e2> + (x3+y3)*<e3>
  = |[ (x1+y1) + 0,0 + (x2+y2),0 + 0 ]| + |[ 0,0,x3+y3 ]| by A1,A5,A6,Lm4
 .= |[ x1+y1,x2+y2,0 ]| + |[ 0,0,x3+y3 ]|;
 |[ x1+y1,x2+y2,0 ]|.1 = x1+y1 & |[ x1+y1,x2+y2,0 ]|.2 = x2+y2
    & |[ x1+y1,x2+y2,0 ]|.3 = 0 by FINSEQ_1:62;
then A9: (x1+y1)*<e1> + (x2+y2)*<e2> + (x3+y3)*<e3>
  = |[ (x1+y1) + 0 ,(x2+y2) + 0,0 + (x3+y3) ]| by A0,A7,Lm4
 .= |[ x1+y1,x2+y2,x3+y3 ]|;
B1: |[ x1,x2,x3 ]|.1 = x1 & |[ x1,x2,x3 ]|.2 = x2 & |[ x1,x2,x3 ]|.3 = x3
    by FINSEQ_1:62;
 |[ y1,y2,y3 ]|.1 = y1 & |[ y1,y2,y3 ]|.2 = y2 & |[ y1,y2,y3 ]|.3 = y3
    by FINSEQ_1:62;
   hence thesis by B1,Lm4,A9;
end;

theorem
  |[ x1,x2,x3 ]| - |[ y1,y2,y3 ]| = (x1-y1)*<e1> + (x2-y2)*<e2> + (x3-y3)*<e3>
proof
A1: |[ y1,y2,y3 ]|.1 = y1 by FINSEQ_1:62;
A2: |[ y1,y2,y3 ]|.2 = y2 by FINSEQ_1:62;
A3: |[ y1,y2,y3 ]|.3 = y3 by FINSEQ_1:62;
A4: |[ x1,x2,x3 ]|.1 - |[ y1,y2,y3 ]|.1 = x1-y1 by A1,FINSEQ_1:62;
A5: |[ x1,x2,x3 ]|.2 - |[ y1,y2,y3 ]|.2 = x2-y2 by A2,FINSEQ_1:62;
 |[ x1,x2,x3 ]|.3 - |[ y1,y2,y3 ]|.3 = x3-y3 by A3,FINSEQ_1:62;
    hence thesis by A4,A5,ThA18;
end;

theorem
  p1.1*<e1> + p1.2*<e2> + p1.3*<e3>
 = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3> iff
  p2.1*<e1> + p2.2*<e2> + p2.3*<e3>
 = (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
proof
F2:r1*R - r2*R = (r1-r2)*R
proof
   r1*R - r2*R = r1*R + ((-1)*r2)*R by RVSUM_1:71
.= (r1 + -r2)*R by RVSUM_1:72;
   hence thesis;
end;
A1:  <e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
A2:  <e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
A3:  <e3>.1 = 0 & <e3>.2 = 0 & <e3>.3 = 1 by FINSEQ_1:62;
    thus p1.1*<e1> + p1.2*<e2> + p1.3*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3> implies
    p2.1*<e1> + p2.2*<e2> + p2.3*<e3>
  = (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
    proof
      assume p1.1*<e1> + p1.2*<e2> + p1.3*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>;
then
 p1.1*<e1> + (p1.2*<e2> + p1.3*<e3>) - p3.1*<e1> - p3.2*<e2> - p3.3*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    + (p2.3 + p3.3)*<e3> - p3.1*<e1> - p3.2*<e2> - p3.3*<e3> by RVSUM_1:32;
then
 p1.1*<e1> - p3.1*<e1> + (p1.2*<e2> + p1.3*<e3>) - p3.2*<e2> - p3.3*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    + (p2.3 + p3.3)*<e3> - p3.1*<e1> - p3.2*<e2> - p3.3*<e3>
      by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2*<e2> + p1.3*<e3>) - p3.2*<e2> - p3.3*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    + (p2.3 + p3.3)*<e3> - p3.1*<e1> - p3.2*<e2> - p3.3*<e3> by F2;
then  (p1.1 - p3.1)*<e1> + p1.2*<e2> + p1.3*<e3> - p3.2*<e2> - p3.3*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    + (p2.3 + p3.3)*<e3> - p3.1*<e1> - p3.2*<e2> - p3.3*<e3> by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + p1.2*<e2> - p3.2*<e2> + p1.3*<e3> - p3.3*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    + (p2.3 + p3.3)*<e3> - p3.1*<e1> - p3.2*<e2> - p3.3*<e3>
      by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2*<e2> + -p3.2*<e2>) + p1.3*<e3> + -p3.3*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    + (p2.3 + p3.3)*<e3> - p3.1*<e1> - p3.2*<e2> - p3.3*<e3> by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2*<e2> - p3.2*<e2>) + (p1.3*<e3> - p3.3*<e3>)
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    + (p2.3 + p3.3)*<e3> - p3.1*<e1> - p3.2*<e2> - p3.3*<e3> by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3*<e3> - p3.3*<e3>)
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    + (p2.3 + p3.3)*<e3> - p3.1*<e1> - p3.2*<e2> - p3.3*<e3> by F2;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    + (p2.3 + p3.3)*<e3> - p3.1*<e1> - p3.2*<e2> - p3.3*<e3> by F2;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2>
    - p3.1*<e1> + (p2.3 + p3.3)*<e3> - p3.2*<e2> - p3.3*<e3>
      by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = (p2.1 + p3.1)*<e1> - p3.1*<e1> + (p2.2 + p3.2)*<e2>
     + (p2.3 + p3.3)*<e3> - p3.2*<e2> - p3.3*<e3> by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = (p2.1 + p3.1)*<e1> - p3.1*<e1> + (p2.2 + p3.2)*<e2> - p3.2*<e2>
     + (p2.3 + p3.3)*<e3> - p3.3*<e3> by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = (p2.1*<e1> + p3.1*<e1> + -p3.1*<e1> + (p2.2 + p3.2)*<e2>- p3.2*<e2>)
    + (p2.3 + p3.3)*<e3> - p3.3*<e3> by RVSUM_1:72;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + (p3.1*<e1> - p3.1*<e1>) + (p2.2 + p3.2)*<e2> - p3.2*<e2>
     + (p2.3 + p3.3)*<e3> - p3.3*<e3> by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + 0.REAL 3 + (p2.2 + p3.2)*<e2> - p3.2*<e2>
     + (p2.3 + p3.3)*<e3> - p3.3*<e3> by EUCLIDLP:7;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + (p2.2 + p3.2)*<e2> - p3.2*<e2>
     + (p2.3 + p3.3)*<e3> - p3.3*<e3> by EUCLID_4:1;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + (p2.2*<e2> + p3.2*<e2>) - p3.2*<e2>
     + (p2.3 + p3.3)*<e3> - p3.3*<e3> by RVSUM_1:72;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = (p2.1*<e1> + p2.2*<e2>) + p3.2*<e2> + -p3.2*<e2>
     + (p2.3 + p3.3)*<e3> - p3.3*<e3> by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = (p2.1*<e1> + p2.2*<e2>) + (p3.2*<e2> + -p3.2*<e2>)
     + (p2.3 + p3.3)*<e3> - p3.3*<e3> by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + p2.2*<e2> + 0.REAL 3
     + (p2.3 + p3.3)*<e3> - p3.3*<e3> by EUCLIDLP:7;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + p2.2*<e2> + (p2.3 + p3.3)*<e3> - p3.3*<e3> by EUCLID_4:1;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + p2.2*<e2> + (p2.3*<e3> + p3.3*<e3>) - p3.3*<e3>
    by RVSUM_1:72;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + p2.2*<e2> + p2.3*<e3> + p3.3*<e3> - p3.3*<e3>
    by RVSUM_1:32;
then  (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + p2.2*<e2> + p2.3*<e3> + (p3.3*<e3> + -p3.3*<e3>)
    by RVSUM_1:32;
 then (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
  = p2.1*<e1> + p2.2*<e2> + p2.3*<e3> + 0.REAL 3 by EUCLIDLP:7;
      hence thesis by EUCLID_4:1;
    end;
      now assume
C1: p2.1*<e1> + p2.2*<e2> + p2.3*<e3>
  = (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>;
 (p2.1*<e1> + p2.2*<e2> + p2.3*<e3> + p3.1*<e1>) + p3.2*<e2> + p3.3*<e3>
  = (p2.1*<e1> + p2.2*<e2> + p2.3*<e3>) + (p3.1*<e1> + p3.2*<e2> + p3.3*<e3>)
    proof
D1: (p2.1*<e1>).1 = p2.1*1 by A1,RVSUM_1:66 .= p2.1;
D2: (p2.1*<e1>).2 = p2.1*0 by A1,RVSUM_1:66 .= 0;
D3: (p2.1*<e1>).3 = p2.1*0 by A1,RVSUM_1:66 .= 0;
D4: (p2.2*<e2>).1 = p2.2*<e2>.1 by RVSUM_1:66 .= 0 by A2;
D5: (p2.2*<e2>).2 = p2.2*1 by A2,RVSUM_1:66 .= p2.2;
D6: (p2.2*<e2>).3 = p2.2*0 by A2,RVSUM_1:66 .= 0;
D7: (p2.3*<e3>).1 = p2.3*0 by A3,RVSUM_1:66 .= 0;
D8: (p2.3*<e3>).2 = p2.3*0 by A3,RVSUM_1:66 .= 0;
D9: (p2.3*<e3>).3 = p2.3*1 by A3,RVSUM_1:66 .= p2.3;
D10: (p3.1*<e1>).1 = p3.1*1 by A1,RVSUM_1:66 .= p3.1;
D11: (p3.1*<e1>).2 = p3.1*0 by A1,RVSUM_1:66 .= 0;
D12: (p3.1*<e1>).3 = p3.1*0 by A1,RVSUM_1:66 .= 0;
D13: (p3.2*<e2>).1 = p3.2*<e2>.1 by RVSUM_1:66 .= 0 by A2;
D14: (p3.2*<e2>).2 = p3.2*1 by A2,RVSUM_1:66 .= p3.2;
D15: (p3.2*<e2>).3 = p3.2*0 by A2,RVSUM_1:66 .= 0;
D16: (p3.3*<e3>).1 = p3.3*0 by A3,RVSUM_1:66 .= 0;
D17: (p3.3*<e3>).2 = p3.3*0 by A3,RVSUM_1:66 .= 0;
D18: (p3.3*<e3>).3 = p3.3*1 by A3,RVSUM_1:66 .= p3.3;
D19: p2.1*<e1> + p2.2*<e2> + p2.3*<e3>
   = p2.1*<e1> + (p2.2*<e2> + p2.3*<e3>) by RVSUM_1:32
  .= p2.1*<e1> + |[ 0+0, p2.2+0, 0+p2.3 ]| by D4,D5,D6,D7,D8,D9,Lm4
  .= |[ p2.1, 0, 0 ]| + |[ 0, p2.2, p2.3 ]| by D1,D2,D3,Lm1
  .= |[ p2.1+0, 0+p2.2, 0+p2.3 ]| by LmA31
  .= |[ p2.1, p2.2, p2.3 ]|;
D20: p3.1*<e1> + p3.2*<e2> + p3.3*<e3>
   = p3.1*<e1> + (p3.2*<e2> + p3.3*<e3>) by RVSUM_1:32
  .= p3.1*<e1> + |[ 0+0, p3.2+0, 0+p3.3 ]| by D13,D14,D15,D16,D17,D18,Lm4
  .= |[ p3.1, 0, 0 ]| + |[ 0, p3.2, p3.3 ]| by D10,D11,D12,Lm1
  .= |[ p3.1+0, 0+p3.2, 0+p3.3 ]| by LmA31
  .= |[ p3.1, p3.2, p3.3 ]|;
     (p2.1*<e1> + p2.2*<e2> + p2.3*<e3> + p3.1*<e1>) + p3.2*<e2> + p3.3*<e3>
   = p2.1*<e1> + (p2.2*<e2> + p2.3*<e3>) + p3.1*<e1> + p3.2*<e2> + p3.3*<e3>
      by RVSUM_1:32
  .= p2.1*<e1> + |[ 0+0, p2.2+0, 0+p2.3 ]| + p3.1*<e1> + p3.2*<e2> + p3.3*<e3>
     by D4,D5,D6,D7,D8,D9,Lm4
  .= |[ p2.1, 0, 0 ]| + |[ 0, p2.2, p2.3 ]| + p3.1*<e1> + p3.2*<e2> + p3.3*<e3>
     by D1,D2,D3,Lm1
  .= |[ p2.1+0, 0+p2.2, 0+p2.3 ]| + p3.1*<e1> + p3.2*<e2> + p3.3*<e3> by LmA31
  .= |[ p2.1, p2.2, p2.3 ]| + |[ p3.1, 0, 0 ]| + p3.2*<e2> + p3.3*<e3>
     by D10,D11,D12,Lm1
  .= |[ p2.1+p3.1, p2.2+0, p2.3+0 ]| + p3.2*<e2> + p3.3*<e3> by LmA31
  .= |[ p2.1+p3.1, p2.2, p2.3 ]| + |[ 0, p3.2, 0 ]| + p3.3*<e3>
     by D13,D14,D15,Lm1
  .= |[ p2.1+p3.1+0, p2.2+p3.2, p2.3+0 ]| + p3.3*<e3> by LmA31
  .= |[ p2.1+p3.1, p2.2+p3.2, p2.3 ]| + |[ 0, 0, p3.3 ]| by D16,D17,D18,Lm1
  .= |[ p2.1+p3.1+0, p2.2+p3.2+0, p2.3+p3.3 ]| by LmA31;
      hence thesis by D19,D20,LmA31;
    end;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = (p1.1 - p3.1)*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
    + p3.1*<e1> + p3.2*<e2> + p3.3*<e3> by EUCLIDLP:29,C1;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = (p1.1 - p3.1)*<e1> + ((p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>)
    + p3.1*<e1> + p3.2*<e2> + p3.3*<e3> by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> - p3.1*<e1> + ((p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>)
    + p3.1*<e1> + p3.2*<e2> + p3.3*<e3> by F2;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + ((p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>) - p3.1*<e1>
    + p3.1*<e1> + p3.2*<e2> + p3.3*<e3> by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + ((p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>)
    + (p3.1*<e1> - p3.1*<e1>) + p3.2*<e2> + p3.3*<e3> by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + ((p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>)
    + 0.REAL 3 + p3.2*<e2> + p3.3*<e3> by RVSUM_1:58;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + ((p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>)
    + p3.2*<e2> + p3.3*<e3> by EUCLID_4:1;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + (p1.2 - p3.2)*<e2> + (p1.3 - p3.3)*<e3>
    + p3.2*<e2> + p3.3*<e3> by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + (p1.2 - p3.2)*<e2> + (p1.3*<e3> - p3.3*<e3>)
    + p3.2*<e2> + p3.3*<e3> by F2;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = (p1.1*<e1> + (p1.2 - p3.2)*<e2> + p1.3*<e3> - p3.3*<e3>)
    + p3.2*<e2> + p3.3*<e3> by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = (p1.1*<e1> + (p1.2 - p3.2)*<e2> + p1.3*<e3> + p3.2*<e2>)
     - p3.3*<e3> + p3.3*<e3> by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + (p1.2 - p3.2)*<e2> + p1.3*<e3> + p3.2*<e2>
    + (p3.3*<e3> - p3.3*<e3>) by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + (p1.2 - p3.2)*<e2> + p1.3*<e3> + p3.2*<e2> + 0.REAL 3
    by RVSUM_1:58;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + (p1.2 - p3.2)*<e2> + p1.3*<e3> + p3.2*<e2> by EUCLID_4:1;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + (p1.2 - p3.2)*<e2> + (p1.3*<e3> + p3.2*<e2>) by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + (p1.2*<e2> - p3.2*<e2>) + (p1.3*<e3> + p3.2*<e2>) by F2;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + p1.2*<e2> + -p3.2*<e2> + (p1.3*<e3> + p3.2*<e2>)
    by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + p1.2*<e2> - p3.2*<e2> + p1.3*<e3> + p3.2*<e2>
    by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + p1.2*<e2> + p1.3*<e3> - p3.2*<e2> + p3.2*<e2>
    by RVSUM_1:32;
then  (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + p1.2*<e2> + p1.3*<e3> + (p3.2*<e2> - p3.2*<e2>)
    by RVSUM_1:32;
 then (p2.1 + p3.1)*<e1> + (p2.2 + p3.2)*<e2> + (p2.3 + p3.3)*<e3>
  = p1.1*<e1> + p1.2*<e2> + p1.3*<e3> + 0.REAL 3 by RVSUM_1:58;
      hence thesis by EUCLID_4:1;
    end;
  hence thesis;
end;

definition
  let f1,f2,f3 be PartFunc of REAL,REAL;
  func VFunc(f1,f2,f3) -> Function of REAL,REAL 3 means :Def1:
  for t holds it.t = |[ f1.t,f2.t,f3.t ]|;
existence
proof
    defpred P[set,set] means $2 = |[ f1.$1,f2.$1,f3.$1 ]|;
A1: for x being Element of REAL ex y being Element of REAL 3 st P[x,y];
    consider F being Function of REAL,REAL 3 such that
A2: for t be Element of REAL holds P[t,F.t] from FUNCT_2:sch 3(A1);
    take F;
    thus thesis by A2;
end;
uniqueness
proof
    let F,G be Function of REAL,REAL 3;
    assume that
A4: for t holds F.t = |[ f1.t,f2.t,f3.t ]| and
A5: for t holds G.t = |[ f1.t,f2.t,f3.t ]|;
    now
      let t be Element of REAL;
      F.t = |[ f1.t,f2.t,f3.t ]| by A4;
      hence F.t = G.t by A5;
    end;
    hence thesis by FUNCT_2:113;
end;
end;

theorem
  VFunc(f1,f2,f3).t = f1.t*<e1> + f2.t*<e2> + f3.t*<e3>
proof
A2:  <e1>.1 = 1 & <e1>.2 = 0 & <e1>.3 = 0 by FINSEQ_1:62;
A3:  <e2>.1 = 0 & <e2>.2 = 1 & <e2>.3 = 0 by FINSEQ_1:62;
A4:  <e3>.1 = 0 & <e3>.2 = 0 & <e3>.3 = 1 by FINSEQ_1:62;
A5:  f1.t*<e1> + f2.t*<e2> + f3.t*<e3>
     = |[ f1.t*1,f1.t*0,f1.t*0 ]| + f2.t*<e2> + f3.t*<e3> by A2,Lm3
    .= |[ f1.t,0,0 ]| + |[ f2.t*0,f2.t*1,f2.t*0 ]| + f3.t*<e3> by A3,Lm3
    .= |[ f1.t,0,0 ]| + |[ 0,f2.t,0 ]| + |[ f3.t*0,f3.t*0,f3.t*1 ]| by A4,Lm3
    .= |[ f1.t,0,0 ]| + |[ 0,f2.t,0 ]| + |[ 0,0,f3.t ]|;
A6:  |[ f1.t,0,0 ]|.1 = f1.t & |[ f1.t,0,0 ]|.2 = 0 & |[ f1.t,0,0 ]|.3 = 0
     by FINSEQ_1:62;
A7:  |[ 0,f2.t,0 ]|.1 = 0 & |[ 0,f2.t,0 ]|.2 = f2.t & |[ 0,f2.t,0 ]|.3 = 0
     by FINSEQ_1:62;
A8:  |[ 0,0,f3.t ]|.1 = 0 & |[ 0,0,f3.t ]|.2 = 0 & |[ 0,0,f3.t ]|.3 = f3.t
     by FINSEQ_1:62;
A9:  f1.t*<e1> + f2.t*<e2> + f3.t*<e3>
     = |[ f1.t+0,0+f2.t,0+0 ]| + |[ 0,0,f3.t ]| by A5,A6,A7,Lm4
    .= |[ f1.t,f2.t,0 ]| + |[ 0,0,f3.t ]|;
 |[ f1.t,f2.t,0 ]|.1 = f1.t & |[ f1.t,f2.t,0 ]|.2 = f2.t &
     |[ f1.t,f2.t,0 ]|.3 = 0 by FINSEQ_1:62;
     then f1.t*<e1> + f2.t*<e2> + f3.t*<e3>
     = |[ f1.t+0,f2.t+0,0+f3.t ]| by A8,A9,Lm4
    .= |[ f1.t,f2.t,f3.t ]|;
     hence VFunc(f1,f2,f3).t = f1.t*<e1> + f2.t*<e2> + f3.t*<e3> by Def1;
end;

theorem Th34:
  p = VFunc(f1,f2,f3).t iff p.1 = f1.t & p.2 = f2.t & p.3 = f3.t
proof
  thus p = VFunc(f1,f2,f3).t implies p.1 = f1.t & p.2 = f2.t & p.3 = f3.t
  proof
    assume p = VFunc(f1,f2,f3).t;
    then p = |[ f1.t,f2.t,f3.t ]| by Def1;
    hence thesis by FINSEQ_1:62;
  end;
  assume p.1 = f1.t & p.2 = f2.t & p.3 = f3.t;
  then p = |[ f1.t,f2.t,f3.t ]| by Lm1;
  hence p = VFunc(f1,f2,f3).t by Def1;
end;

theorem Th35:
  len p = 3 & dom p = Seg 3
proof
  p = |[ p.1,p.2,p.3 ]| by Lm1;
  hence thesis by FINSEQ_1:62,FINSEQ_3:30;
end;

theorem Th36:
  mlt(p,q) = <* p.1*q.1, p.2*q.2, p.3*q.3 *>
proof
 len p = 3 & len q = 3 by Th35;
    hence thesis by EUCLID_5:28;
end;

theorem
  r*p = |[ r*p.1, r*p.2, r*p.3 ]| by Lm3;

theorem Th39:
  -p = |[ -p.1,-p.2,-p.3 ]|
proof
  -p = |[ (-1)*p.1,(-1)*p.2,(-1)*p.3 ]| by Lm3
    .= |[ -p.1,-p.2,-p.3 ]|;
  hence thesis;
end;

theorem
  len(-p) = len p
proof
A2: len p = 3 by Th35;
    -p = |[ (-p).1,(-p).2,(-p).3 ]| by Lm1;
    hence thesis by A2,FINSEQ_1:62;
end;

theorem Th44:
  p+q = |[ p.1+q.1, p.2+q.2, p.3+q.3 ]| by Lm4;

theorem
  (p = VFunc(f1,f2,f3).t1 & q = VFunc(g1,g2,g3).t2 & p = q) implies
  (f1.t1 = g1.t2 & f2.t1 = g2.t2 & f3.t1 = g3.t2)
proof
    assume
A1: p = VFunc(f1,f2,f3).t1 & q = VFunc(g1,g2,g3).t2 & p = q;
then A3: p.1 = f1.t1 & q.1 = g1.t2 by Th34;
A4: p.2 = f2.t1 & q.2 = g2.t2 by A1,Th34;
    p.3 = f3.t1 & q.3 = g3.t2 by A1,Th34;
    hence thesis by A1,A3,A4;
end;

theorem
  (f1.t1 = g1.t2 & f2.t1 = g2.t2 & f3.t1 = g3.t2) implies
  VFunc(f1,f2,f3).t1 = VFunc(g1,g2,g3).t2
proof
    assume
A1: f1.t1 = g1.t2 & f2.t1 = g2.t2 & f3.t1 = g3.t2;
    set p = |[ f1.t1,f2.t1,f3.t1 ]|;
    set q = |[ g1.t2,g2.t2,g3.t2 ]|;
    p = VFunc(f1,f2,f3).t1 & q = VFunc(g1,g2,g3).t2 by Def1;
    hence thesis by A1;
end;

theorem Th8:
  for r being real number holds r*p = |[ r*p.1, r*p.2, r*p.3 ]|
proof
    let r be real number;
A1: (r*p).1 = r*(p.1) by RVSUM_1:66;
A2: (r*p).2 = r*(p.2) by RVSUM_1:66;
    (r*p).3 = r*(p.3) by RVSUM_1:66;
    hence r*p = |[ r*p.1, r*p.2, r*p.3]| by A1,A2,Lm1;
end;

theorem Th9:
  for r being real number holds r*|[ x,y,z ]| = |[ r*x, r*y, r*z ]|
proof
  let r be real number;
  set p = |[ x,y,z ]|;
  r*p = |[ r*p.1, r*p.2, r*p.3 ]| by Th8
    .=|[ r*x, r*p.2, r*p.3 ]| by FINSEQ_1:62
    .=|[ r*x, r*y, r*p.3 ]| by FINSEQ_1:62
    .=|[ r*x, r*y, r*z ]| by FINSEQ_1:62;
    hence thesis;
end;

theorem Th10:
  -p = |[ -p.1,-p.2,-p.3 ]|
proof
    reconsider r = -1 as Element of REAL by XREAL_0:def 1;
    r*p = |[ (-1)*p.1,(-1)*p.2,(-1)*p.3 ]| by Th8
    .= |[ -p.1,-p.2,-p.3 ]|;
    hence thesis;
end;

theorem Th11:
  (-p).1 = -p.1 & (-p).2 = -p.2 & (-p).3 = -p.3
proof
  -p = |[ -p.1, -p.2, -p.3 ]| by Th10;
  hence thesis by FINSEQ_1:62;
end;

theorem Th23:
  p1-p2 = |[ p1.1-p2.1, p1.2-p2.2, p1.3-p2.3 ]|
proof
A2:  (-p2).1 = -p2.1 by Th11;
A3:  (-p2).2 = -p2.2 by Th11;
  (-p2).3 = -p2.3 by Th11;
     then p1+-p2 = |[ p1.1+-p2.1, p1.2+-p2.2, p1.3+-p2.3 ]| by A2,A3,Lm4;
     hence thesis;
end;

theorem
  |(p,q)| = p.1*q.1+p.2*q.2+p.3*q.3 by Lm8;

theorem Th52:
  |(p,p)| = (p.1)^2 + (p.2)^2 + (p.3)^2
proof
  p = |[ p.1,p.2,p.3 ]| by Lm1;
  hence thesis by LmTh8;
end;

theorem Th53:
  |.p.| = sqrt ((p.1)^2+(p.2)^2+(p.3)^2)
proof
  |.p.| = sqrt Sum<* p.1*p.1,p.2*p.2,p.3*p.3 *> by Th36
      .= sqrt ((p.1)^2+(p.2)^2+(p.3)^2) by RVSUM_1:108;
  hence thesis;
end;

theorem
  |.r*p.| = abs(r)*(sqrt ((p.1)^2+(p.2)^2+(p.3)^2))
proof
  |.r*p.| = abs(r)*|.p.| by EUCLID:14
   .= abs(r)*(sqrt ((p.1)^2+(p.2)^2+(p.3)^2)) by Th53;
  hence thesis;
end;

theorem
  r1*p+r2*p = (r1+r2)*|[ p.1, p.2, p.3 ]|
proof
  r1*p+r2*p = (r1+r2)*p by RVSUM_1:72;
  hence thesis by Lm1;
end;

theorem Th80:
  |(r*p1,p2)| = r*|(p1,p2)|
proof
A1:  r*p1 = |[ r*p1.1, r*p1.2, r*p1.3]| by Th8;
A2:  r*|(p1,p2)| = r*(p1.1*p2.1 + p1.2*p2.2 + p1.3*p2.3 ) by Lm8;
A3:  (r*p1).1*p2.1 = (r*p1.1)*p2.1 by A1,FINSEQ_1:62
     .= r*(p1.1*p2.1);
A4:  (r*p1).2*p2.2 =(r*p1.2)*p2.2 by A1,FINSEQ_1:62
     .=r*(p1.2*p2.2);
  (r*p1).3*p2.3 = (r*p1.3)*p2.3 by A1,FINSEQ_1:62
     .= r*(p1.3*p2.3);
     then |(r*p1,p2)| = (r*(p1.1*p2.1) + r*(p1.2*p2.2)) + r*(p1.3*p2.3)
     by A3,A4,Lm8;
     hence thesis by A2;
end;

theorem
  r1*p-r2*p = (r1-r2)*|[ p.1, p.2, p.3 ]|
proof
  r1*R - r2*R = (r1-r2)*R
  proof
    r1*R - r2*R = r1*R + ((-1)*r2)*R by RVSUM_1:71
     .= (r1 + -r2)*R by RVSUM_1:72;
    hence thesis;
  end; then
  r1*p-r2*p = (r1-r2)*p;
  hence thesis by Lm1;
end;

theorem
  |(r*p,q)| = r*(p.1*q.1+p.2*q.2+p.3*q.3)
proof
  |(r*p,q)| = r*|(p,q)| by LmAA30;
  hence thesis by Lm8;
end;

theorem
  |(p,0.REAL 3)| = 0
proof
A1: 0.REAL 3 = |[ 0,0,0 ]| by FINSEQ_2:76;
then A2: (0.REAL 3).1 = 0 by FINSEQ_1:62;
A3: (0.REAL 3).2 = 0 by A1,FINSEQ_1:62;
 (0.REAL 3).3 = 0 by A1,FINSEQ_1:62;
    then |(p,0.REAL 3)| = p.1*0 +p.2*0 +p.3*0 by A2,A3,Lm8;
    hence thesis;
end;

theorem
  |(-p1,p2)| = -|(p1,p2)|
proof
A2:  -p1 = |[ -p1.1, -p1.2, -p1.3]| by Th39;
then A3:  (-p1).1 = -p1.1 by FINSEQ_1:62;
A4:  (-p1).2 = -p1.2 by A2,FINSEQ_1:62;
A5:  (-p1).3 = -p1.3 by A2,FINSEQ_1:62;
A6:  |(p1,p2)| = p1.1*p2.1 + p1.2*p2.2 + p1.3*p2.3 by Lm8;
     |(-p1,p2)| = (-p1.1)*p2.1 + (-p1.2)*p2.2 + (-p1.3)*p2.3 by A3,A4,A5,Lm8;
     hence thesis by A6;
end;

theorem
  |(-p1,-p2)| = |(p1,p2)|
proof
A3:  -p1 = |[ -p1.1, -p1.2, -p1.3]| by Th39;
A4:  -p2 = |[ -p2.1, -p2.2, -p2.3]| by Th39;
A5:  (-p1).1 = -p1.1 by A3,FINSEQ_1:62;
A6:  (-p1).2 = -p1.2 by A3,FINSEQ_1:62;
A7:  (-p1).3 = -p1.3 by A3,FINSEQ_1:62;
A8:  (-p2).1 = -p2.1 by A4,FINSEQ_1:62;
A9:  (-p2).2 = -p2.2 by A4,FINSEQ_1:62;
A10: (-p2).3 = -p2.3 by A4,FINSEQ_1:62;
A11: |(p1,p2)| = p1.1*p2.1 + p1.2*p2.2 + p1.3*p2.3 by Lm8;
    |(-p1, -p2)| = (-p1.1)*(-p2.1) + (-p1.2)*(-p2.2) + (-p1.3)*(-p2.3)
       by A5,A6,A7,A8,A9,A10,Lm8;
    hence thesis by A11;
end;

theorem Th88:
  |(p1-p2,q)| = |(p1,q)| - |(p2,q)|
proof
A1:  |(p1,q)| = p1.1*q.1+p1.2*q.2+p1.3*q.3 by Lm8;
A2:  |(p2,q)| = p2.1*q.1+p2.2*q.2+p2.3*q.3 by Lm8;
A3:  p1-p2 = |[ p1.1-p2.1, p1.2-p2.2, p1.3-p2.3 ]| by Th23;
     set p = p1-p2;
A5:  p.2 = p1.2-p2.2 by A3,FINSEQ_1:62;
A6:  p.3 = p1.3-p2.3 by A3,FINSEQ_1:62;
  |(p,q)| = p.1*q.1+p.2*q.2+p.3*q.3 by Lm8;
     then |(p,q)| = (p1.1-p2.1)*q.1+(p1.2-p2.2)*q.2+(p1.3-p2.3)*q.3
     by A3,A5,A6,FINSEQ_1:62;
     hence thesis by A1,A2;
end;

theorem Th81:
  |(p1+p2,q)| = |(p1,q)|+|(p2,q)|
proof
A1: |(p1,q)| = p1.1*q.1 + p1.2*q.2 + p1.3*q.3 by Lm8;
A2: |(p2,q)| = p2.1*q.1 + p2.2*q.2 + p2.3*q.3 by Lm8;
A3: p1+p2 = |[ p1.1+p2.1, p1.2+p2.2, p1.3+p2.3]| by Th44;
    set p = p1+p2;
A5: p.2 = p1.2 + p2.2 by A3,FINSEQ_1:62;
A6: p.3 = p1.3 + p2.3 by A3,FINSEQ_1:62;
 |(p,q)| = p.1*q.1 + p.2*q.2 + p.3*q.3 by Lm8;
    then |(p,q)| = (p1.1+p2.1)*q.1 + (p1.2+p2.2)*q.2 + (p1.3+p2.3)*q.3
       by A3,A5,A6,FINSEQ_1:62;
    hence thesis by A1,A2;
end;

theorem
  |(r1*p1+r2*p2,q)| = r1*|(p1,q)| + r2*|(p2,q)|
proof
    |((r1*p1+r2*p2),q)| = |(r1*p1,q)| + |(r2*p2,q)| by Th81; then
    |((r1*p1+r2*p2),q)| = r1*|(p1,q)| + |(r2*p2,q)| by Th80;
    hence thesis by Th80;
end;

theorem Th92:
  |(p1+p2,q1+q2)| = |(p1,q1)|+|(p1,q2)|+|(p2,q1)|+|(p2,q2)|
proof
A1: |(p1+p2,q1+q2)| = |(p1,q1+q2)|+|(p2,q1+q2)| by Th81;
A2: |(p1,q1+q2)| = |(p1,q1)|+|(p1,q2)| by Th81;
    |(p2,q1+q2)| = |(p2,q1)|+|(p2,q2)| by Th81;
    hence thesis by A1,A2;
end;

theorem Th93:
  |(p1-p2,q1-q2)| = |(p1,q1)|-|(p1,q2)|-|(p2,q1)|+|(p2,q2)|
proof
A1: |(p1-p2,q1-q2)| = |(p1,q1-q2)|-|(p2,q1-q2)| by Th88;
A2: |(p1,q1-q2)| = |(p1,q1)|-|(p1,q2)| by Th88;
    |(p2,q1-q2)| = |(p2,q1)|-|(p2,q2)| by Th88;
    hence thesis by A1,A2;
end;

theorem ThA41:
  |(p,p)| = 0 iff p = 0.REAL 3
proof
  thus |(p,p)|=0 implies p =0.REAL 3
  proof
    assume |(p,p)|= 0; then
    Sum sqr(p) = 0;
    hence thesis by RVSUM_1:121;
  end;
  assume p= 0.REAL 3; then
A4:p = |[ 0,0,0 ]| by FINSEQ_2:76;
then A9:p.1 = 0 by FINSEQ_1:62;
A10:p.2 = 0 by A4,FINSEQ_1:62;
   p.3 = 0 by A4,FINSEQ_1:62; then
   |(p,p)| = 0^2 + 0^2 + 0^2 by A9,A10,Th52;
  hence |(p,p)| = 0;
end;

theorem
  |.p.| = 0 iff p = 0.REAL 3
proof
  thus |.p.| = 0 implies p = 0.REAL 3
  proof
    assume
A2: |.p.| = 0;
 |(p, p)| >= 0 by RVSUM_1:149;
    then (sqrt|(p,p)|)^2 = |(p,p)| by SQUARE_1:def 4;
    hence thesis by A2,ThA41;
  end;
    assume p = 0.REAL 3; then
A4: p = |[ 0,0,0 ]| by FINSEQ_2:76;
then A5:p.1 = 0 by FINSEQ_1:62;
A6:p.2 = 0 by A4,FINSEQ_1:62;
p.3 = 0 by A4,FINSEQ_1:62;
   then |(p,p)| = 0^2 + 0^2 + 0^2 by A5,A6,Th52;
   hence thesis by SQUARE_1:82;
end;

theorem
  |(p-q,p-q)| = |(p,p)|-2*|(p,q)|+|(q,q)|
proof
  |(p-q,p-q)| = |(p,p)|-|(p,q)|-|(q,p)|+|(q,q)| by Th93;
  hence thesis;
end;

theorem
  |(p+q,p+q)| = |(p,p)|+2*|(p,q)|+|(q,q)|
proof
  |(p+q,p+q)| = |(p,p)|+|(p,q)|+|(q,p)|+|(q,q)| by Th92;
  hence thesis;
end;

theorem Th128:
  p1<X>p2 = -p2<X>p1
proof
  -p2<X>p1 = |[ (-1)*((p2.2*p1.3)-(p2.3*p1.2)),(-1)*((p2.3*p1.1)-(p2.1*p1.3)),
       (-1)*((p2.1*p1.2)-(p2.2*p1.1)) ]| by Th9
          .= p1<X>p2;
   hence thesis;
end;

theorem Th129:
  |[ x1,x2,x3 ]| <X> |[ y1,y2,y3 ]|
  = |[ (x2*y3)-(x3*y2),(x3*y1)-(x1*y3),(x1*y2)-(x2*y1) ]|
proof
    set p1 = |[ x1,x2,x3 ]|;
A2: p1.1 = x1 & p1.2 = x2 & p1.3 = x3 by FINSEQ_1:62;
    set p2 = |[ y1,y2,y3 ]|;
    p2.1 = y1 & p2.2 = y2 & p2.3 = y3 by FINSEQ_1:62;
    hence thesis by A2;
end;

theorem
  (r*p1)<X>p2 = r*(p1<X>p2) & (r*p1)<X>p2 = p1<X>(r*p2)
proof
A1: (r*p1)<X>p2 = |[ r*p1.1,r*p1.2,r*p1.3 ]|<X>p2 by Th8
   .= |[ r*p1.1,r*p1.2,r*p1.3 ]|<X>|[ p2.1,p2.2,p2.3 ]| by Lm1
   .= |[ (r*p1.2*p2.3)-(r*p1.3*p2.2),(r*p1.3*p2.1)-(r*p1.1*p2.3),
      (r*p1.1*p2.2)-(r*p1.2*p2.1) ]| by Th129; then
A2: (r*p1)<X>p2 = |[ r*((p1.2*p2.3)-(p1.3*p2.2)),r*((p1.3*p2.1)-(p1.1*p2.3)),
      r*((p1.1*p2.2)-(p1.2*p2.1)) ]|
   .= r*(p1<X>p2) by Th9;
    (r*p1)<X>p2 = |[ p1.2*(r*p2.3)-p1.3*(r*p2.2),p1.3*(r*p2.1)-p1.1*(r*p2.3),
      p1.1*(r*p2.2)-p1.2*(r*p2.1) ]| by A1
   .= |[ p1.1,p1.2,p1.3 ]|<X>|[ r*p2.1,r*p2.2,r*p2.3]| by Th129
   .= p1<X>|[ r*p2.1,r*p2.2,r*p2.3 ]| by Lm1
   .= p1<X>(r*p2) by Th8;
   hence thesis by A2;
end;

theorem Th137:
  p1<X>(p2+p3) = (p1<X>p2)+(p1<X>p3)
proof
A1: p2+p3 = |[ p2.1+p3.1, p2.2+p3.2, p2.3+p3.3 ]| by Th44; then
A2: (p2+p3).1 = p2.1+p3.1 by FINSEQ_1:62;
A3: (p2+p3).2 = p2.2+p3.2 by A1,FINSEQ_1:62;
A4: (p2+p3).3 = p2.3+p3.3 by A1,FINSEQ_1:62;
    (p1<X>p2)+(p1<X>p3) = |[ (p1.2*p2.3-p1.3*p2.2)+(p1.2*p3.3-p1.3*p3.2),
      (p1.3*p2.1-p1.1*p2.3)+(p1.3*p3.1-p1.1*p3.3),
      (p1.1*p2.2-p1.2*p2.1)+(p1.1*p3.2-p1.2*p3.1) ]| by LmA31
   .= |[ p1.2*p2.3-p1.3*p2.2+p1.2*p3.3-p1.3*p3.2,
      p1.3*p2.1-p1.1*p2.3+p1.3*p3.1-p1.1*p3.3,
      p1.1*p2.2-p1.2*p2.1+p1.1*p3.2-p1.2*p3.1 ]|;
    hence thesis by A2,A3,A4;
end;

theorem Th138:
  (p1+p2)<X>p3 = (p1<X>p3)+(p2<X>p3)
proof
  (p1+p2)<X>p3 = -(p3<X>(p1+p2)) by Th128
   .= -((p3<X>p1)+(p3<X>p2)) by Th137
   .= -((p3<X>p1)-(p2<X>p3)) by Th128
   .= -(p3<X>p1)+(p2<X>p3) by RVSUM_1:57;
  hence thesis by Th128;
end;

theorem
  (p1+p2)<X>(q1+q2) = p1<X>q1+p1<X>q2+p2<X>q1+p2<X>q2
proof
 (p1+p2)<X>(q1+q2) = (p1<X>(q1+q2))+(p2<X>(q1+q2)) by Th138;
then  (p1+p2)<X>(q1+q2) = (p1<X>q1+p1<X>q2)+(p2<X>(q1+q2)) by Th137;
    then (p1+p2)<X>(q1+q2) = p1<X>q1+p1<X>q2+(p2<X>q1+p2<X>q2) by Th137;
    hence thesis by RVSUM_1:32;
end;

theorem
  p1<X>(p2<X>p3) = (|(p1,p3)|*p2)-(|(p1,p2)|*p3)
proof
A1: p2 = |[ p2.1, p2.2, p2.3 ]| by Lm1;
A2: p3 = |[ p3.1, p3.2, p3.3 ]| by Lm1;
 p1<X>(p2<X>p3) = |[ p1.1, p1.2, p1.3 ]| <X> (|[ (p2.2*p3.3)-(p2.3*p3.2),
      (p2.3*p3.1)-(p2.1*p3.3), (p2.1*p3.2)-(p2.2*p3.1) ]|) by Lm1;
then  p1<X>(p2<X>p3)
    = |[ (p1.2*((p2.1*p3.2)-(p2.2*p3.1)))-(p1.3*((p2.3*p3.1)-(p2.1*p3.3))),
    (p1.3*((p2.2*p3.3)-(p2.3*p3.2)))-(p1.1*((p2.1*p3.2)-(p2.2*p3.1))),
    (p1.1*((p2.3*p3.1)-(p2.1*p3.3)))-(p1.2*((p2.2*p3.3)-(p2.3*p3.2))) ]|
    by Th129;
then A5: p1<X>(p2<X>p3) = |[(p1.2*p3.2+p1.3*p3.3+p1.1*p3.1)*p2.1-
    (p1.2*p2.2+p1.3*p2.3+p1.1*p2.1)*p3.1,
    (p1.3*p3.3+p1.1*p3.1+p1.2*p3.2)*p2.2-
    (p1.3*p2.3+p1.1*p2.1+p1.2*p2.2)*p3.2,
    (p1.1*p3.1+p1.2*p3.2+p1.3*p3.3)*p2.3
    -(p1.1*p2.1+p1.2*p2.2+p1.3*p2.3)*p3.3 ]|;
 |(p1,p3)| = p1.1*p3.1+p1.2*p3.2+p1.3*p3.3 by Lm8;
then A7: p1<X>(p2<X>p3) = |[ |(p1,p3)|*p2.1, |(p1,p3)|*p2.2, |(p1,p3)|*p2.3 ]|-
    |[ (p1.1*p2.1+p1.2*p2.2+p1.3*p2.3)*p3.1,
    (p1.1*p2.1+p1.2*p2.2+p1.3*p2.3)*p3.2,
    (p1.1*p2.1+p1.2*p2.2+p1.3*p2.3)*p3.3 ]| by A5,LmA63;
 |(p1,p2)| = p1.1*p2.1+p1.2*p2.2+p1.3*p2.3 by Lm8;
    then p1<X>(p2<X>p3) = (|(p1,p3)|)*|[ p2.1, p2.2, p2.3 ]|-
    |[ |(p1,p2)|*p3.1, |(p1,p2)|*p3.2, |(p1,p2)|*p3.3 ]| by A7,Th9;
    hence thesis by A1,A2,Th9;
end;

definition let p1,p2,p3;
  func |{ p1,p2,p3 }| -> real number equals
  |(p1,p2<X>p3)|;
  coherence;
end;

theorem
  |{ p1,p1,p2 }| = 0
proof
A7:(p1<X>p2).1 = (p1.2*p2.3)-(p1.3*p2.2) by FINSEQ_1:62;
A8:(p1<X>p2).2 = (p1.3*p2.1)-(p1.1*p2.3) by FINSEQ_1:62;
(p1<X>p2).3 = (p1.1*p2.2)-(p1.2*p2.1) by FINSEQ_1:62;
   then |{ p1,p1,p2 }| = p1.1*((p1.2*p2.3)-(p1.3*p2.2))
   +p1.2*((p1.3*p2.1)-(p1.1*p2.3))
   +p1.3*((p1.1*p2.2)-(p1.2*p2.1)) by A8,A7,Lm8
     .= 0;
   hence thesis;
end;

theorem
  |{ p2,p1,p2 }| = 0
proof
A7:(p1<X>p2).1 = (p1.2*p2.3)-(p1.3*p2.2) by FINSEQ_1:62;
A8:(p1<X>p2).2 = (p1.3*p2.1)-(p1.1*p2.3) by FINSEQ_1:62;
(p1<X>p2).3 = (p1.1*p2.2)-(p1.2*p2.1) by FINSEQ_1:62;
   then |{ p2,p1,p2 }| = p2.1*((p1.2*p2.3)-(p1.3*p2.2))
  +p2.2*((p1.3*p2.1)-(p1.1*p2.3))
  +p2.3*((p1.1*p2.2)-(p1.2*p2.1)) by A8,A7,Lm8
.= 0;
  hence thesis;
end;

theorem
  |{ p1,p2,p2 }| = 0
proof
  |{ p1,p2,p2 }| = p1.1*(p2<X>p2).1+p1.2*(p2<X>p2).2+p1.3*(p2<X>p2).3 by Lm8
 .= p1.1*((p2.2*p2.3)-(p2.3*p2.2))+p1.2*(p2<X>p2).2+p1.3*(p2<X>p2).3
    by FINSEQ_1:62
 .= p1.1*((p2.2*p2.3)-(p2.3*p2.2))+p1.2*((p2.3*p2.1)-(p2.1*p2.3))+
    p1.3*(p2<X>p2).3 by FINSEQ_1:62
 .= (p1.1*(p2.2*p2.3)-p1.1*(p2.3*p2.2))+p1.2*((p2.3*p2.1)-(p2.1*p2.3))+
    p1.3*((p2.1*p2.2)-(p2.2*p2.1)) by FINSEQ_1:62
 .= 0-p2.2*(p2.1*p2.3)+p2.2*(p2.1*p2.3);
  hence thesis;
end;

theorem ThA52:
  |{ p1,p2,p3 }| = |{ p2,p3,p1 }|
proof
  |{ p1,p2,p3 }| = |(|[ p1.1, p1.2, p1.3 ]|, |[ (p2.2*p3.3)-(p2.3*p3.2),
    (p2.3*p3.1)-(p2.1*p3.3), (p2.1*p3.2)-(p2.2*p3.1) ]|)| by Lm1
 .= p1.1*((p2.2*p3.3)-(p2.3*p3.2))+p1.2*((p2.3*p3.1)-(p2.1*p3.3))+
    p1.3*((p2.1*p3.2)-(p2.2*p3.1)) by EUCLID_5:30
 .= p2.1*(p3.2*p1.3-p3.3*p1.2)+p2.2*(p3.3*p1.1-p3.1*p1.3)+
    p2.3*(p3.1*p1.2-p3.2*p1.1)
 .= |(|[ p2.1, p2.2, p2.3 ]|,
    |[ p3.2*p1.3-p3.3*p1.2, p3.3*p1.1-p3.1*p1.3, p3.1*p1.2-p3.2*p1.1 ]| )|
    by EUCLID_5:30
 .= |(p2, p3<X>p1)| by Lm1;
  hence thesis;
end;

theorem
  |{ p1,p2,p3 }| = |(p1<X>p2,p3)|
proof
    |{ p1,p2,p3 }| = |(|[ p1.1, p1.2, p1.3 ]|,
    |[ (p2.2*p3.3)-(p2.3*p3.2),(p2.3*p3.1)-(p2.1*p3.3),
    (p2.1*p3.2)-(p2.2*p3.1) ]|)| by Lm1
 .= p1.1*((p2.2*p3.3)-(p2.3*p3.2))+p1.2*((p2.3*p3.1)-(p2.1*p3.3))+
    p1.3*((p2.1*p3.2)-(p2.2*p3.1)) by EUCLID_5:30
 .= p2.2*(p1.1*p3.3)-p2.3*(p1.1*p3.2)+(p2.3*(p1.2*p3.1)-
    p2.1*(p1.2*p3.3))+(p2.1*(p1.3*p3.2)-p2.2*(p1.3*p3.1));
    then |{ p1,p2,p3 }|
  = (p2.3*p1.2-p2.2*p1.3)*p3.1+(p2.1*p1.3-p2.3*p1.1)*p3.2+
    (p2.2*p1.1-p2.1*p1.2)*p3.3
 .= |( p1<X>p2, |[ p3.1, p3.2, p3.3 ]| )| by EUCLID_5:30
 .= |( p1<X>p2, p3 )| by Lm1;
    hence thesis;
end;

theorem
  |{ p1,p2,q }| = |(q<X>p1,p2)|
proof
  |{ p1,p2,q }| = |{ p2,q,p1 }| by ThA52;
  hence thesis;
end;


begin :: the Differentiation Formulas of Vector Function in 3-dimensional Euclidean Spaces

definition
  let f1,f2,f3 be PartFunc of REAL,REAL;
  let t0 be Real;
  func VFuncdiff(f1,f2,f3,t0) -> Element of REAL 3 equals
  |[ diff(f1,t0),diff(f2,t0),diff(f3,t0) ]|;
  coherence;
end;

theorem
  f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
  f3 is_differentiable_in t0 implies VFuncdiff(f1,f2,f3,t0) =
  diff(f1,t0)*<e1>+diff(f2,t0)*<e2>+diff(f3,t0)*<e3> by Th29;

theorem Th57:
  (f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
  f3 is_differentiable_in t0) & (g1 is_differentiable_in t0 &
  g2 is_differentiable_in t0 & g3 is_differentiable_in t0)
  implies VFuncdiff(f1+g1,f2+g2,f3+g3,t0) =
  VFuncdiff(f1,f2,f3,t0)+VFuncdiff(g1,g2,g3,t0)
proof
    assume that
A1: f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
    f3 is_differentiable_in t0 and
A2: g1 is_differentiable_in t0 & g2 is_differentiable_in t0 &
    g3 is_differentiable_in t0;
    set p = |[ diff(f1,t0),diff(f2,t0),diff(f3,t0) ]|;
    set q = |[ diff(g1,t0),diff(g2,t0),diff(g3,t0) ]|;
A4: p.1 = diff(f1,t0) & p.2 = diff(f2,t0) & p.3 = diff(f3,t0) by FINSEQ_1:62;
A5: q.1 = diff(g1,t0) & q.2 = diff(g2,t0) & q.3 = diff(g3,t0) by FINSEQ_1:62;
    VFuncdiff(f1+g1,f2+g2,f3+g3,t0)
    = |[ diff(f1,t0)+diff(g1,t0),diff(f2+g2,t0),diff(f3+g3,t0) ]|
      by A1,A2,FDIFF_1:21
   .= |[ diff(f1,t0)+diff(g1,t0),diff(f2,t0)+diff(g2,t0),diff(f3+g3,t0) ]|
      by A1,A2,FDIFF_1:21
   .= |[ p.1+q.1,p.2+q.2,p.3+q.3 ]| by A1,A2,A4,A5,FDIFF_1:21
   .= VFuncdiff(f1,f2,f3,t0)+VFuncdiff(g1,g2,g3,t0) by Lm4;
    hence thesis;
end;

theorem Th58:
  (f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
  f3 is_differentiable_in t0) & (g1 is_differentiable_in t0 &
  g2 is_differentiable_in t0 & g3 is_differentiable_in t0)
  implies VFuncdiff(f1-g1,f2-g2,f3-g3,t0) =
  VFuncdiff(f1,f2,f3,t0)-VFuncdiff(g1,g2,g3,t0)
proof
    assume that
A1: f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
    f3 is_differentiable_in t0 and
A2: g1 is_differentiable_in t0 & g2 is_differentiable_in t0 &
    g3 is_differentiable_in t0;
    set p = |[ diff(f1,t0),diff(f2,t0),diff(f3,t0) ]|;
    set q = |[ diff(g1,t0),diff(g2,t0),diff(g3,t0) ]|;
A4: p.1 = diff(f1,t0) & p.2 = diff(f2,t0) & p.3 = diff(f3,t0) by FINSEQ_1:62;
A5: q.1 = diff(g1,t0) & q.2 = diff(g2,t0) & q.3 = diff(g3,t0) by FINSEQ_1:62;
    VFuncdiff(f1-g1,f2-g2,f3-g3,t0)
    = |[ diff(f1,t0)-diff(g1,t0),diff(f2-g2,t0),diff(f3-g3,t0) ]|
      by A1,A2,FDIFF_1:22
   .= |[ diff(f1,t0)-diff(g1,t0),diff(f2,t0)-diff(g2,t0),diff(f3-g3,t0) ]|
      by A1,A2,FDIFF_1:22
   .= |[ p.1-q.1,p.2-q.2,p.3-q.3 ]| by A1,A2,A4,A5,FDIFF_1:22
   .= VFuncdiff(f1,f2,f3,t0)-VFuncdiff(g1,g2,g3,t0) by Lm7;
    hence thesis;
end;

theorem Th59:
  (f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
  f3 is_differentiable_in t0) implies
  VFuncdiff(r(#)f1,r(#)f2,r(#)f3,t0) = r*VFuncdiff(f1,f2,f3,t0)
proof
    assume
A1: f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
    f3 is_differentiable_in t0;
    set p = |[ diff(f1,t0),diff(f2,t0),diff(f3,t0) ]|;
A3: p.1 = diff(f1,t0) & p.2 = diff(f2,t0) & p.3 = diff(f3,t0) by FINSEQ_1:62;
    VFuncdiff(r(#)f1,r(#)f2,r(#)f3,t0)
    = |[ r*diff(f1,t0),diff(r(#)f2,t0),diff(r(#)f3,t0) ]| by A1,FDIFF_1:23
   .= |[ r*diff(f1,t0),r*diff(f2,t0),diff(r(#)f3,t0) ]| by A1,FDIFF_1:23
   .= |[ r*p.1,r*p.2,r*p.3 ]| by A1,A3,FDIFF_1:23
   .= r*VFuncdiff(f1,f2,f3,t0) by Lm3;
    hence thesis;
end;

theorem Th60:
  (f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
  f3 is_differentiable_in t0) & (g1 is_differentiable_in t0 &
  g2 is_differentiable_in t0 & g3 is_differentiable_in t0)
  implies VFuncdiff(f1(#)g1,f2(#)g2,f3(#)g3,t0) =
  |[ (g1.t0)*diff(f1,t0),(g2.t0)*diff(f2,t0),(g3.t0)*diff(f3,t0) ]|+
  |[ (f1.t0)*diff(g1,t0),(f2.t0)*diff(g2,t0),(f3.t0)*diff(g3,t0) ]|
proof
    assume that
A1: f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
    f3 is_differentiable_in t0 and
A2: g1 is_differentiable_in t0 & g2 is_differentiable_in t0 &
    g3 is_differentiable_in t0;
    set p = |[ (g1.t0)*diff(f1,t0),(g2.t0)*diff(f2,t0),(g3.t0)*diff(f3,t0) ]|;
    set q = |[ (f1.t0)*diff(g1,t0),(f2.t0)*diff(g2,t0),(f3.t0)*diff(g3,t0) ]|;
A4: p.1 = (g1.t0)*diff(f1,t0) & p.2 = (g2.t0)*diff(f2,t0) &
    p.3 = (g3.t0)*diff(f3,t0) by FINSEQ_1:62;
A5: q.1 = (f1.t0)*diff(g1,t0) & q.2 = (f2.t0)*diff(g2,t0) &
    q.3 = (f3.t0)*diff(g3,t0) by FINSEQ_1:62;
    VFuncdiff(f1(#)g1,f2(#)g2,f3(#)g3,t0)
    = |[ (g1.t0)*diff(f1,t0)+(f1.t0)*diff(g1,t0),diff(f2(#)g2,t0),
      diff(f3(#)g3,t0) ]| by A1,A2,FDIFF_1:24
   .= |[ (g1.t0)*diff(f1,t0)+(f1.t0)*diff(g1,t0),
      (g2.t0)*diff(f2,t0)+(f2.t0)*diff(g2,t0),diff(f3(#)g3,t0) ]|
      by A1,A2,FDIFF_1:24
   .= |[ p.1+q.1,p.2+q.2,p.3+q.3 ]| by A1,A2,A4,A5,FDIFF_1:24
   .= |[ (g1.t0)*diff(f1,t0),(g2.t0)*diff(f2,t0),(g3.t0)*diff(f3,t0) ]|+
      |[ (f1.t0)*diff(g1,t0),(f2.t0)*diff(g2,t0),(f3.t0)*diff(g3,t0) ]|
      by Lm4;
    hence thesis;
end;

theorem Th61:
  (f1 is_differentiable_in t0 & f2 is_differentiable_in t0 &
  f3 is_differentiable_in t0) & (g1 is_differentiable_in f1.t0 &
  g2 is_differentiable_in f2.t0 & g3 is_differentiable_in f3.t0)
  implies VFuncdiff(g1*f1,g2*f2,g3*f3,t0) =
  |[ diff(g1,f1.t0)*diff(f1,t0),diff(g2,f2.t0)*diff(f2,t0),
