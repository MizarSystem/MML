 .= |[ (g1.t0)*(r*diff(f1,t0)),(g2.t0)*(r*diff(f2,t0)),
    (g3.t0)*(r*diff(f3,t0)) ]|+
    |[ (r*f1.t0)*diff(g1,t0),((r(#)f2).t0)*diff(g2,t0),
    ((r(#)f3).t0)*diff(g3,t0) ]| by VALUED_1:6
 .= |[ (g1.t0)*(r*diff(f1,t0)),(g2.t0)*(r*diff(f2,t0)),
    (g3.t0)*(r*diff(f3,t0)) ]|+
    |[ (r*f1.t0)*diff(g1,t0),(r*f2.t0)*diff(g2,t0),
    ((r(#)f3).t0)*diff(g3,t0) ]| by VALUED_1:6
 .= |[ (g1.t0)*(r*diff(f1,t0)),(g2.t0)*(r*diff(f2,t0)),
    (g3.t0)*(r*diff(f3,t0)) ]|+
    |[ r*(f1.t0)*diff(g1,t0),r*(f2.t0)*diff(g2,t0),
    r*(f3.t0)*diff(g3,t0) ]| by VALUED_1:6
 .= |[ (g1.t0)*(r*diff(f1,t0)),(g2.t0)*(r*diff(f2,t0)),
    (g3.t0)*(r*diff(f3,t0)) ]|+
    |[ r*((f1.t0)*diff(g1,t0)),r*((f2.t0)*diff(g2,t0)),
