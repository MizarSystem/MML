definition
  let CNS;
  let x0 be Point of CNS;
  mode Neighbourhood of x0 -> Subset of CNS means
    :Def5:
    ex g be Real st 0<g &
    {y where y is Point of CNS: ||.y-x0 .|| < g} c= it;
  existence
  proof
    set N= {y where y is Point of CNS : ||.y-x0 .|| < 1};
    take N;
    N c= the carrier of CNS
    proof
      let x be set;
      assume
      x in {y where y is Point of CNS : ||.y-x0 .|| < 1};
      then ex y be Point of CNS st x=y & ||.y-x0 .|| < 1;
      hence thesis;
    end;
    hence thesis;
  end;
end;

theorem Th3:
  for x0 be Point of CNS for g be Real st 0 < g holds {y where y is
  Point of CNS: ||.y-x0 .|| < g} is Neighbourhood of x0
proof
  let x0 be Point of CNS;
  let g be Real such that
A1: g >0;
  set N= {y where y is Point of CNS : ||.y-x0 .|| < g};
  N c= the carrier of CNS
  proof
    let x be set;
    assume
    x in {y where y is Point of CNS : ||.y-x0 .|| < g};
    then ex y be Point of CNS st x=y & ||.y-x0 .|| < g;
    hence thesis;
  end;
  hence thesis by A1,Def5;
end;

theorem Th4:
  for x0 be Point of CNS for N being Neighbourhood of x0 holds x0
  in N
proof
  let x0 be Point of CNS;
  let N be Neighbourhood of x0;
  consider g be Real such that
A1: 0<g and
A2: {z where z is Point of CNS : ||.z-x0 .|| < g} c= N by Def5;
  ||.x0-x0 .|| = ||.0.CNS.|| by RLVECT_1:28
    .=0 by CLVECT_1:103;
  then x0 in {z where z is Point of CNS : ||.z-x0 .|| < g} by A1;
  hence thesis by A2;
end;

definition
  let CNS;
  let X be Subset of CNS;
  attr X is compact means
  :Def6:
  for s1 be sequence of CNS st rng s1 c= X ex
s2 be sequence of CNS st s2 is subsequence of s1 & s2 is convergent & lim s2 in
  X;
end;

definition
  let CNS;
  let X be Subset of CNS;
  attr X is closed means
  for s1 be sequence of CNS st rng s1 c= X & s1 is
  convergent holds lim s1 in X;
end;

definition
  let CNS;
  let X be Subset of CNS;
  attr X is open means
  X` is closed;
end;

definition
  canceled 6;
  let CNS1,CNS2;
  let f be PartFunc of CNS1,CNS2;
  let x0 be Point of CNS1;
  pred f is_continuous_in x0 means
  :Def15:
  x0 in dom f & for seq be sequence
of CNS1 st rng seq c= dom f & seq is convergent & lim seq = x0 holds f/*seq is
  convergent & f/.x0 = lim (f/*seq);
end;

