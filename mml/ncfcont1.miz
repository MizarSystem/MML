  f & x1 in N holds f/.x1 in N1
  proof
    assume
A1: f is_continuous_in x0;
    hence x0 in dom f by Def15;
    let N1 be Neighbourhood of f/.x0;
    consider r such that
A2: 0<r and
A3: {y where y is Point of CNS2 : ||.y-f/.x0 .|| < r} c= N1 by Def5;
