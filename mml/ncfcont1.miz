theorem Th8:
  for seq be sequence of CNS, x be set holds x in rng seq iff ex n
  st x = seq.n
proof
  let seq be sequence of CNS;
  let x be set;
  thus x in rng seq implies ex n st x = seq.n
  proof
    assume
    x in rng seq;
    then consider y be set such that
A1: y in dom seq and
A2: x = seq.y by FUNCT_1:def 5;
    reconsider m=y as Element of NAT by A1;
    take m;
    thus thesis by A2;
  end;
  given n such that
A3: x = seq.n;
  n in NAT;
  then n in dom seq by FUNCT_2:def 1;
  hence thesis by A3,FUNCT_1:def 5;
end;

canceled 20;

theorem Th29:
  for f be PartFunc of CNS1,CNS2, x0 be Point of CNS1 holds f
  is_continuous_in x0 iff x0 in dom f & for r st 0<r ex s st 0<s & for x1 be
  Point of CNS1 st x1 in dom f & ||. x1- x0 .|| <s holds ||. f/.x1-f/.x0 .||<r
proof
  let f be PartFunc of CNS1,CNS2;
  let x0 be Point of CNS1;
  thus f is_continuous_in x0 implies x0 in dom f & for r st 0<r ex s st 0<s &
for x1 be Point of CNS1 st x1 in dom f & ||. x1- x0 .|| <s holds ||. f/.x1- f/.
  x0 .|| <r
  proof
    assume
A1: f is_continuous_in x0;
    hence x0 in dom f by Def15;
    given r such that
A2: 0<r and
A3: for s holds not 0<s or ex x1 be Point of CNS1 st x1 in dom f & ||.
    x1- x0 .|| <s & not ||. f/.x1- f/.x0 .|| <r;
    defpred P[Element of NAT ,Point of CNS1] means $2 in dom f & ||. $2-x0 .||
    < 1/($1+1) & not ||.f/.$2-f/.x0 .||<r;
A4: for n ex p be Point of CNS1 st P[n,p]
    proof
      let n;
      0<n+1 by NAT_1:3;
      then 0<(n+1)" by XREAL_1:124;
      then 0<1/(n+1) by XCMPLX_1:217;
      then consider p be Point of CNS1 such that
A5:   p in dom f & ||.p-x0 .|| < 1/(n+1) & not ||.f/.p-f/.x0 .|| <r by A3;
      take p;
      thus thesis by A5;
    end;
    consider s1 being Function of NAT,the carrier of CNS1 such that
A6: for n being Element of NAT holds P[n,s1.n] from FUNCT_2:sch 3(A4);
    reconsider s1 as sequence of CNS1;
A7: rng s1 c= dom f
    proof
A8:   dom s1= NAT by FUNCT_2:def 1;
      let v be set;
      assume
      v in rng s1;
      then ex n be set st n in NAT & v=s1.n by A8,FUNCT_1:def 5;
      hence thesis by A6;
    end;
A9: now
      let n;
      not ||.f/.(s1.n)-f/.x0 .||<r by A6;
      hence not ||.(f/*s1).n-f/.x0 .||<r by A7,FUNCT_2:186;
    end;
A10: now
      let s be Real;
      consider n such that
A11:  s"<n by SEQ_4:10;
      assume
      0<s;
      then
A12:  0<s" by XREAL_1:124;
      s"+0 <n+1 by A11,XREAL_1:10;
      then 1/(n+1)<1/s" by A12,XREAL_1:78;
      then
A13:  1/(n+1)<s by XCMPLX_1:218;
      take k=n;
      let m;
      assume
      k<=m;
      then k+1<=m+1 by XREAL_1:8;
      then 1/(m+1)<=1/(k+1) by NAT_1:3,XREAL_1:120;
      then 1/(m+1)<s by A13,XXREAL_0:2;
      hence ||. s1.m-x0 .|| <s by A6,XXREAL_0:2;
    end;
    then
A14: s1 is convergent by CLVECT_1:def 16;
    then lim s1=x0 by A10,CLVECT_1:def 18;
    then f/*s1 is convergent & f/.x0=lim(f/*s1) by A1,A7,A14,Def15;
    then consider n such that
A15: for m st n<=m holds ||.(f/*s1).m-f/.x0.||<r by A2,CLVECT_1:def 18;
    ||.(f/*s1).n-f/.x0.||<r by A15;
    hence contradiction by A9;
  end;
  assume that
A16: x0 in dom f and
A17: for r st 0<r ex s st 0<s & for x1 be Point of CNS1 st x1 in dom f &
  ||. x1- x0 .|| <s holds ||. f/.x1- f/.x0 .|| <r;
  now
    let s1 be sequence of CNS1 such that
A18: rng s1 c= dom f and
A19: s1 is convergent & lim s1 = x0;
A20: now
      let p be Real;
      assume
      0<p;
      then consider s such that
A21:  0<s and
A22:  for x1 be Point of CNS1 st x1 in dom f & ||. x1- x0 .|| <s
      holds ||. f/.x1- f/.x0 .|| <p by A17;
      consider n such that
A23:  for m st n<=m holds ||. s1.m-x0 .|| <s by A19,A21,CLVECT_1:def 18;
      take k=n;
      let m;
      assume
      k<=m;
      then
A24:  ||. s1.m-x0 .|| <s by A23;
      dom s1 = NAT by FUNCT_2:def 1;
      then s1.m in rng s1 by FUNCT_1:12;
      then ||.f/.(s1.m)-f/.x0.||<p by A18,A22,A24;
      hence ||.(f/*s1).m - f/.x0.||<p by A18,FUNCT_2:186;
    end;
    then f/*s1 is convergent by CLVECT_1:def 16;
    hence f/*s1 is convergent & f/.x0 = lim (f/*s1) by A20,CLVECT_1:def 18;
  end;
  hence thesis by A16,Def15;
end;

theorem Th30:
  for f be PartFunc of CNS,RNS, x0 be Point of CNS holds f
  is_continuous_in x0 iff x0 in dom f & for r st 0<r ex s st 0<s & for x1 be
  Point of CNS st x1 in dom f & ||. x1- x0 .|| <s holds ||. f/.x1-f/.x0 .||<r
proof
  let f be PartFunc of CNS,RNS;
  let x0 be Point of CNS;
  thus f is_continuous_in x0 implies x0 in dom f & for r st 0<r ex s st 0<s &
for x1 be Point of CNS st x1 in dom f & ||. x1- x0 .|| <s holds ||. f/.x1- f/.
  x0 .|| <r
  proof
    assume
A1: f is_continuous_in x0;
    hence x0 in dom f by Def16;
    given r such that
A2: 0<r and
A3: for s holds not 0<s or ex x1 be Point of CNS st x1 in dom f & ||.
    x1 - x0 .|| <s & not ||. f/.x1- f/.x0 .|| <r;
    defpred P[Element of NAT ,Point of CNS] means $2 in dom f & ||. $2-x0 .||
    < 1/($1+1) & not ||.f/.$2-f/.x0 .||<r;
A4: for n ex p be Point of CNS st P[n,p]
    proof
      let n;
      0<n+1 by NAT_1:3;
      then 0<(n+1)" by XREAL_1:124;
      then 0<1/(n+1) by XCMPLX_1:217;
      then consider p be Point of CNS such that
A5:   p in dom f & ||.p-x0 .|| < 1/(n+1) & not ||.f/.p-f/.x0 .|| <r by A3;
      take p;
      thus thesis by A5;
    end;
    consider s1 being Function of NAT,the carrier of CNS such that
A6: for n being Element of NAT holds P[n,s1.n] from FUNCT_2:sch 3(A4);
    reconsider s1 as sequence of CNS;
A7: rng s1 c= dom f
    proof
A8:   dom s1= NAT by FUNCT_2:def 1;
      let v be set;
      assume
      v in rng s1;
      then ex n be set st n in NAT & v=s1.n by A8,FUNCT_1:def 5;
      hence thesis by A6;
    end;
A9: now
      let n;
      not ||.f/.(s1.n)-f/.x0 .||<r by A6;
      hence not ||.(f/*s1).n-f/.x0 .||<r by A7,FUNCT_2:186;
    end;
A10: now
      let s be Real;
      consider n such that
A11:  s"<n by SEQ_4:10;
      assume
      0<s;
      then
A12:  0<s" by XREAL_1:124;
      s"+0 <n+1 by A11,XREAL_1:10;
      then 1/(n+1)<1/s" by A12,XREAL_1:78;
      then
A13:  1/(n+1)<s by XCMPLX_1:218;
      take k=n;
      let m;
      assume
      k<=m;
      then k+1<=m+1 by XREAL_1:8;
      then 1/(m+1)<=1/(k+1) by NAT_1:3,XREAL_1:120;
      then 1/(m+1)<s by A13,XXREAL_0:2;
      hence ||. s1.m-x0 .|| <s by A6,XXREAL_0:2;
    end;
    then
A14: s1 is convergent by CLVECT_1:def 16;
    then lim s1=x0 by A10,CLVECT_1:def 18;
    then f/*s1 is convergent & f/.x0=lim(f/*s1) by A1,A7,A14,Def16;
    then consider n such that
A15: for m st n<=m holds ||.(f/*s1).m-f/.x0.||<r by A2,NORMSP_1:def 11;
    ||.(f/*s1).n-f/.x0.||<r by A15;
    hence contradiction by A9;
  end;
  assume that
A16: x0 in dom f and
A17: for r st 0<r ex s st 0<s & for x1 be Point of CNS st x1 in dom f &
  ||. x1- x0 .|| <s holds ||. f/.x1- f/.x0 .|| <r;
  now
    let s1 be sequence of CNS such that
A18: rng s1 c= dom f and
A19: s1 is convergent & lim s1 = x0;
A20: now
      let p be Real;
      assume
      0<p;
      then consider s such that
A21:  0<s and
A22:  for x1 be Point of CNS st x1 in dom f & ||. x1- x0 .|| <s holds
      ||. f/.x1- f/.x0 .|| <p by A17;
      consider n such that
A23:  for m st n<=m holds ||. s1.m-x0 .|| <s by A19,A21,CLVECT_1:def 18;
      take k=n;
      let m;
      assume
      k<=m;
      then
A24:  ||. s1.m-x0 .|| <s by A23;
      dom s1 = NAT by FUNCT_2:def 1;
      then s1.m in rng s1 by FUNCT_1:12;
      then ||.f/.(s1.m)-f/.x0.||<p by A18,A22,A24;
      hence ||.(f/*s1).m - f/.x0.||<p by A18,FUNCT_2:186;
    end;
    then f/*s1 is convergent by NORMSP_1:def 9;
    hence f/*s1 is convergent & f/.x0 = lim (f/*s1) by A20,NORMSP_1:def 11;
