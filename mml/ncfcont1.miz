theorem
  for seq1,seq2 be sequence of CNS holds seq1 - seq2 = seq1 +- seq2 by
NFCONT_1:1;

theorem Th2:
  for seq be sequence of CNS holds -seq=(-1r)*seq
proof
  let seq be sequence of CNS;
  now
    let n;
    thus ((-1r)*seq).n =(-1r)*seq.n by CLVECT_1:def 15
      .=-seq.n by CLVECT_1:4
      .=(-seq).n by BHSP_1:def 10;
  end;
  hence thesis by FUNCT_2:113;
end;

definition
  canceled;
  let CNS1,CNS2;
  let f be PartFunc of CNS1,CNS2;
  func ||.f.|| -> PartFunc of the carrier of CNS1,REAL means
  :Def2:
  dom it =
  dom f & for c being Point of CNS1 st c in dom it holds it.c = ||.f/.c.||;
  existence
  proof
    deffunc U(Element of CNS1) = ||.f/.$1 .||;
    defpred X[set] means $1 in dom f;
    consider F be PartFunc of the carrier of CNS1,REAL such that
A1: for c being Element of CNS1 holds c in dom F iff X[c] and
A2: for c being Element of CNS1 st c in dom F holds F.c = U(c) from
    SEQ_1:sch 3;
    reconsider F as PartFunc of the carrier of CNS1,REAL;
    take F;
    thus dom F = dom f by A1,SUBSET_1:8;
    thus thesis by A2;
  end;
  uniqueness
  proof
    deffunc U(Element of CNS1) = ||. f/.$1 .||;
    for f1, f2 be PartFunc of the carrier of CNS1,REAL st (dom f1 = dom f
& for c being Element of CNS1 st c in dom f1 holds f1.c = U(c)) & (dom f2 = dom
f & for c being Element of CNS1 st c in dom f2 holds f2.c = U(c)) holds f1 = f2
    from SEQ_1:sch 4;
    hence thesis;
  end;
end;

