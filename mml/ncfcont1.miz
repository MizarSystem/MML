VFUNCT_1:def 4
        .= ||. r*(f/.x1) - r*(f/.x2).|| by A4,A11,VFUNCT_1:def 4
        .= ||. r*(f/.x1 - f/.x2).|| by RLVECT_1:48
        .= abs(r)*||. f/.x1 - f/.x2.|| by NORMSP_1:def 2;
      hence ||. (r(#)f)/.x1-(r(#)f)/.x2.|| <= g*||. x1-x2.|| by A12;
    end;
  end;
  hence thesis;
