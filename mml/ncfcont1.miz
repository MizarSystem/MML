  then ||.(f|Y).||/.x1 =||.(f|Y).||.x1 by PARTFUN1:def 8
    .=||. (f|Y)/.x1.|| by A13,Def2
    .=||. f/.x1.|| by A6,PARTFUN2:32
    .=||. f.||.x1 by A2,A5,A6,A9,Def2
    .=||. f.||/.x1 by A2,A5,A6,A9,PARTFUN1:def 8;
  hence thesis by A8,A12,A10;
