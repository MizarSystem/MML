    hence f|X is_continuous_in x0 by A5,A6,Th30;
  end;
  hence thesis by A4,Def22;
end;

theorem Th139:
  for f be PartFunc of RNS,CNS st f is_Lipschitzian_on X holds f
  is_continuous_on X
proof
  let f be PartFunc of RNS,CNS;
  assume
A1: f is_Lipschitzian_on X;
  then consider r be Real such that
A2: 0 < r and
A3: for x1,x2 be Point of RNS st x1 in X & x2 in X holds ||. f/.x1-f/.x2
  .||<=r*||. x1-x2.|| by Def29;
A4: X c= dom f by A1,Def29;
  then
A5: dom (f|X) = X by RELAT_1:91;
  now
    let x0 be Point of RNS such that
A6: x0 in X;
    now
      let g be Real such that
A7:   0<g;
      set s=g/r;
      take s9=s;
A8:   now
        let x1 be Point of RNS;
        assume that
A9:     x1 in dom (f|X) and
A10:    ||. x1- x0 .|| <s;
        r*||. x1- x0 .|| <(g/r)*r by A2,A10,XREAL_1:70;
        then
A11:    r*||. x1- x0 .|| <g by A2,XCMPLX_1:88;
        ||. f/.x1- f/.x0 .|| <=r*||. x1- x0 .|| by A3,A5,A6,A9;
        then ||. f/.x1- f/.x0 .|| <g by A11,XXREAL_0:2;
        then ||. (f|X)/.x1-f/.x0 .||<g by A9,PARTFUN2:32;
        hence ||. (f|X)/.x1-(f|X)/.x0 .||<g by A5,A6,PARTFUN2:32;
      end;
      0<r" & s9=g*r" by A2,XCMPLX_0:def 9,XREAL_1:124;
      hence
      0<s9 & for x1 be Point of RNS st x1 in dom (f|X) & ||. x1- x0 .|| <
      s9 holds ||. (f|X)/.x1-(f|X)/.x0.||<g by A7,A8,XREAL_1:131;
    end;
    hence f|X is_continuous_in x0 by A5,A6,Th31;
  end;
  hence thesis by A4,Def23;
end;

