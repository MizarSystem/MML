      hence r in dom ((||.f.|| )|X) by RELAT_1:90;
      let s1 be sequence of CNS;
      assume that
A7:   rng s1 c= dom ((||.f.|| )|X) and
A8:   s1 is convergent & lim s1 = r;
      rng s1 c= dom (||.f.|| ) /\ X by A7,RELAT_1:90;
      then rng s1 c= dom f /\ X by Def3;
      then
A9:   rng s1 c= dom (f|X) by RELAT_1:90;
      then
A10:  (f|X)/.r = lim ((f|X)/*s1) by A5,A8,Def16;
      now
        let n;
        dom s1 = NAT by FUNCT_2:def 1;
        then
A11:    s1.n in rng s1 by FUNCT_1:12;
        then s1.n in dom (f|X) by A9;
        then
A12:    s1.n in dom f /\ X by RELAT_1:90;
        then
A13:    s1.n in X by XBOOLE_0:def 4;
        s1.n in dom f by A12,XBOOLE_0:def 4;
        then
A14:    s1.n in dom (||.f.|| ) by Def3;
        thus (||.(f|X)/*s1.||).n = ||. ((f|X)/*s1).n .|| by NORMSP_1:def 10
          .=||.(f|X)/.(s1.n).|| by A9,FUNCT_2:186
          .=||.f/.(s1.n).|| by A9,A11,PARTFUN2:32
          .=(||.f.|| ).(s1.n) by A14,Def3
          .=((||.f.|| )|X).(s1.n) by A13,FUNCT_1:72
          .=((||.f.|| )|X)/.(s1.n) by A7,A11,PARTFUN1:def 8
          .=(((||.f.|| )|X)/*s1).n by A7,FUNCT_2:186;
      end;
      then
A15:  ||.(f|X)/*s1.|| = ((||.f.|| )|X)/*s1 by FUNCT_2:113;
A16:  (f|X)/*s1 is convergent by A5,A8,A9,Def16;
      hence ((||.f.|| )|X)/*s1 is convergent by A15,NORMSP_1:39;
      ||.(f|X)/.r.|| = ||.f/.r.|| by A2,A4,PARTFUN2:35
        .= (||.f.|| ).r by A3,A4,Def3
        .= (||.f.|| )/.r by A3,A4,PARTFUN1:def 8
        .= ((||.f.|| )|X)/.r by A6,PARTFUN2:34;
      hence thesis by A16,A10,A15,LOPBAN_1:24;
    end;
  end;
  (-1)(#)f is_continuous_on X by A1,Th90;
  hence thesis by VFUNCT_1:29;
end;

theorem Th94:
  for f be PartFunc of RNS,CNS st f is_continuous_on X holds ||.f
  .|| is_continuous_on X & -f is_continuous_on X
proof
  let f be PartFunc of RNS,CNS;
  assume
A1: f is_continuous_on X;
  thus ||.f.|| is_continuous_on X
  proof
A2: X c= dom f by A1,Def23;
    hence
A3: X c= dom (||.f.|| ) by Def4;
    let r be Point of RNS;
    assume
A4: r in X;
    then
A5: f|X is_continuous_in r by A1,Def23;
    thus (||.f.|| )|X is_continuous_in r
    proof
A6:   r in dom (||.f.|| ) /\ X by A3,A4,XBOOLE_0:def 4;
      hence r in dom ((||.f.|| )|X) by RELAT_1:90;
      let s1 be sequence of RNS;
      assume that
A7:   rng s1 c= dom ((||.f.|| )|X) and
A8:   s1 is convergent & lim s1 = r;
      rng s1 c= dom (||.f.|| ) /\ X by A7,RELAT_1:90;
      then rng s1 c= dom f /\ X by Def4;
      then
A9:   rng s1 c= dom (f|X) by RELAT_1:90;
      then
A10:  (f|X)/.r = lim ((f|X)/*s1) by A5,A8,Def17;
      now
        let n;
        dom s1 = NAT by FUNCT_2:def 1;
        then
A11:    s1.n in rng s1 by FUNCT_1:12;
        then s1.n in dom (f|X) by A9;
        then
A12:    s1.n in dom f /\ X by RELAT_1:90;
        then
A13:    s1.n in X by XBOOLE_0:def 4;
        s1.n in dom f by A12,XBOOLE_0:def 4;
        then
A14:    s1.n in dom (||.f.|| ) by Def4;
        thus (||.(f|X)/*s1.||).n = ||. ((f|X)/*s1).n .|| by CLVECT_1:def 17
          .=||.(f|X)/.(s1.n).|| by A9,FUNCT_2:186
          .=||.f/.(s1.n).|| by A9,A11,PARTFUN2:32
          .=(||.f.|| ).(s1.n) by A14,Def4
          .=((||.f.|| )|X).(s1.n) by A13,FUNCT_1:72
          .=((||.f.|| )|X)/.(s1.n) by A7,A11,PARTFUN1:def 8
          .=(((||.f.|| )|X)/*s1).n by A7,FUNCT_2:186;
      end;
      then
A15:  ||.(f|X)/*s1.|| = ((||.f.|| )|X)/*s1 by FUNCT_2:113;
A16:  (f|X)/*s1 is convergent by A5,A8,A9,Def17;
      hence ((||.f.|| )|X)/*s1 is convergent by A15,CLVECT_1:119;
      ||.(f|X)/.r.|| = ||.f/.r.|| by A2,A4,PARTFUN2:35
        .= (||.f.|| ).r by A3,A4,Def4
        .= (||.f.|| )/.r by A3,A4,PARTFUN1:def 8
        .= ((||.f.|| )|X)/.r by A6,PARTFUN2:34;
      hence thesis by A16,A10,A15,CLOPBAN1:22;
    end;
  end;
  (-1r)(#)f is_continuous_on X by A1,Th91;
  hence thesis by VFUNCT_2:23;
end;

theorem
  for f be PartFunc of CNS1,CNS2 st f is total & (for x1,x2 be Point of
  CNS1 holds f/.(x1+x2) = f/.x1 + f/.x2) & (ex x0 be Point of CNS1 st f
  is_continuous_in x0) holds f is_continuous_on the carrier of CNS1
proof
  let f be PartFunc of CNS1,CNS2;
  assume that
A1: f is total and
A2: for x1,x2 be Point of CNS1 holds f/.(x1+x2) = f/.x1 + f/.x2;
A3: dom f = the carrier of CNS1 by A1,PARTFUN1:def 4;
  given x0 be Point of CNS1 such that
A4: f is_continuous_in x0;
  f/.x0 + 0.CNS2 = f/.x0 by RLVECT_1:10
    .= f/.(x0+0.CNS1) by RLVECT_1:10
    .= f/.x0+f/.0.CNS1 by A2;
  then
A5: f/.0.CNS1 = 0.CNS2 by RLVECT_1:21;
A6: now
    let x1 be Point of CNS1;
    0.CNS2 = f/.(x1+-x1) by A5,RLVECT_1:16
      .= f/.x1+f/.(-x1) by A2;
    hence -(f/.x1)=f/.(-x1) by RLVECT_1:19;
  end;
A7: now
    let x1,x2 be Point of CNS1;
    thus f/.(x1-x2)=f/.(x1+-x2) by RLVECT_1:def 14
      .= f/.x1 + f/.(-x2) by A2
      .= f/.x1 +- f/.x2 by A6
      .= f/.x1 - f/.x2 by RLVECT_1:def 14;
  end;
  now
    let x1 be Point of CNS1;
    let r be Real;
    assume that
    x1 in the carrier of CNS1 and
A8: r>0;
    set y=x1-x0;
    consider s such that
A9: 0<s and
A10: for x1 be Point of CNS1 st x1 in dom f & ||. x1- x0 .|| <s holds
    ||. f/.x1- f/.x0 .|| <r by A4,A8,Th29;
    take s;
    thus s>0 by A9;
    let x2 be Point of CNS1 such that
    x2 in the carrier of CNS1 and
A11: ||. x2-x1 .||<s;
A12: y+x0 =x1-(x0-x0) by RLVECT_1:43
      .=x1- 0.CNS1 by RLVECT_1:28
      .=x1 by RLVECT_1:26;
    then
A13: ||.x2-y-x0.||=||.x2-x1.|| by RLVECT_1:41;
    ||.f/.x2-f/.x1.|| = ||.f/.x2-(f/.y+f/.x0).|| by A2,A12
      .= ||.f/.x2-f/.y-f/.x0 .|| by RLVECT_1:41
      .= ||.f/.(x2-y)-f/.x0.|| by A7;
    hence ||.f/.x2-f/.x1.||<r by A3,A10,A11,A13;
  end;
  hence thesis by A3,Th65;
end;

