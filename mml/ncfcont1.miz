theorem Th5:
  for seq be sequence of CNS1, h be PartFunc of CNS1,CNS2 st rng
  seq c= dom h holds seq.n in dom h
proof
  let seq be sequence of CNS1;
  let h be PartFunc of CNS1,CNS2;
  n in NAT;
  then
A1: n in dom seq by FUNCT_2:def 1;
  assume
  rng seq c= dom h;
  then n in dom ((h qua Function)*seq) by A1,RELAT_1:46;
  hence thesis by FUNCT_1:21;
end;

theorem Th6:
  for seq be sequence of CNS, h be PartFunc of CNS,RNS st rng seq
  c= dom h holds seq.n in dom h
proof
