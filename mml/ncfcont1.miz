theorem
  for f1,f2 be PartFunc of CNS1,CNS2, x0 be Point of CNS1 st f1
is_continuous_in x0 & f2 is_continuous_in x0 holds f1+f2 is_continuous_in x0 &
  f1-f2 is_continuous_in x0
proof
  let f1,f2 be PartFunc of CNS1,CNS2;
  let x0 be Point of CNS1;
  assume that
A1: f1 is_continuous_in x0 and
A2: f2 is_continuous_in x0;
A3: x0 in dom f1 & x0 in dom f2 by A1,A2,Def15;
  now
    x0 in dom f1 /\ dom f2 by A3,XBOOLE_0:def 4;
    hence
A4: x0 in dom (f1+f2) by VFUNCT_2:def 1;
    let s1 be sequence of CNS1;
    assume that
A5: rng s1 c= dom(f1+f2) and
A6: s1 is convergent & lim s1=x0;
A7: rng s1 c= dom f1 /\ dom f2 by A5,VFUNCT_2:def 1;
    dom (f1+f2) = dom f1 /\ dom f2 by VFUNCT_2:def 1;
    then dom (f1+f2) c= dom f2 by XBOOLE_1:17;
    then
A8: rng s1 c= dom f2 by A5,XBOOLE_1:1;
    then
A9: f2/*s1 is convergent by A2,A6,Def15;
    dom (f1+f2) = dom f1 /\ dom f2 by VFUNCT_2:def 1;
    then dom (f1+f2) c= dom f1 by XBOOLE_1:17;
    then
A10: rng s1 c= dom f1 by A5,XBOOLE_1:1;
    then
A11: f1/*s1 is convergent by A1,A6,Def15;
    then (f1/*s1)+(f2/*s1) is convergent by A9,CLVECT_1:115;
    hence (f1+f2)/*s1 is convergent by A7,Th44;
A12: f1/.x0 = lim (f1/*s1) by A1,A6,A10,Def15;
A13: f2/.x0 = lim (f2/*s1) by A2,A6,A8,Def15;
    thus (f1+f2)/.x0 = f1/.x0 + f2/.x0 by A4,VFUNCT_2:def 1
      .= lim (f1/*s1 + f2/*s1) by A11,A12,A9,A13,CLVECT_1:121
      .= lim ((f1+f2)/*s1) by A7,Th44;
  end;
  hence f1+f2 is_continuous_in x0 by Def15;
  now
    x0 in dom f1 /\ dom f2 by A3,XBOOLE_0:def 4;
    hence
A14: x0 in dom (f1-f2) by VFUNCT_2:def 2;
    let s1 be sequence of CNS1;
    assume that
A15: rng s1 c= dom(f1-f2) and
A16: s1 is convergent & lim s1=x0;
A17: rng s1 c= dom f1 /\ dom f2 by A15,VFUNCT_2:def 2;
    dom (f1-f2) = dom f1 /\ dom f2 by VFUNCT_2:def 2;
    then dom (f1-f2) c= dom f2 by XBOOLE_1:17;
    then
A18: rng s1 c= dom f2 by A15,XBOOLE_1:1;
    then
A19: f2/*s1 is convergent by A2,A16,Def15;
    dom (f1-f2) = dom f1 /\ dom f2 by VFUNCT_2:def 2;
    then dom (f1-f2) c= dom f1 by XBOOLE_1:17;
    then
A20: rng s1 c= dom f1 by A15,XBOOLE_1:1;
    then
A21: f1/*s1 is convergent by A1,A16,Def15;
    then f1/*s1-f2/*s1 is convergent by A19,CLVECT_1:116;
    hence (f1-f2)/*s1 is convergent by A17,Th44;
A22: f1/.x0 = lim (f1/*s1) by A1,A16,A20,Def15;
A23: f2/.x0 = lim (f2/*s1) by A2,A16,A18,Def15;
    thus (f1-f2)/.x0 = f1/.x0 - f2/.x0 by A14,VFUNCT_2:def 2
      .= lim (f1/*s1 - f2/*s1) by A21,A22,A19,A23,CLVECT_1:122
      .= lim ((f1-f2)/*s1) by A17,Th44;
  end;
  hence thesis by Def15;
end;

