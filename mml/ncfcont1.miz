theorem Th111:
  for f be PartFunc of CNS1,CNS2 holds (||.f.||)|X = ||.(f|X).||
proof
  let f be PartFunc of CNS1,CNS2;
A1: dom ((||.f.||)|X) = dom (||.f.||) /\ X by RELAT_1:90
    .= dom f /\ X by Def2
    .= dom (f|X) by RELAT_1:90
    .= dom ( ||.(f|X).||) by Def2;
  now
    let c be Point of CNS1;
    assume
A2: c in dom ((||.f.||)|X);
    then
A3: c in dom (f|X) by A1,Def2;
    c in dom (||.f.||) /\ X by A2,RELAT_1:90;
    then
A4: c in dom (||.f.||) by XBOOLE_0:def 4;
    thus ((||.f.||)|X).c = (||.f.||).c by A2,FUNCT_1:70
      .= ||.f/.c.|| by A4,Def2
      .= ||.(f|X)/.c.|| by A3,PARTFUN2:32
      .= ||.(f|X).||.c by A1,A2,Def2;
  end;
  hence thesis by A1,PARTFUN1:34;
end;

