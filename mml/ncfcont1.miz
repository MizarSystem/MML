  hence 0<p;
  let x1,x2 be Point of RNS;
  assume that
A11: x1 in X/\X1 and
A12: x2 in X/\X1;
A13: ||. f2/.x1-f2/.x2.||<=g*||. x1-x2.|| by A10,A11,A12;
  ||. f1/.x1-f1/.x2.||<=s*||. x1-x2.|| by A5,A11,A12;
  then
A14: ||. f1/.x1-f1/.x2.||+||. f2/.x1-f2/.x2.||<= s*||. x1-x2.||+g*||. x1-x2
  .|| by A13,XREAL_1:9;
  ||. (f1-f2)/.x1-(f1-f2)/.x2.|| = ||. f1/.x1 - f2/.x1-(f1-f2)/.x2.|| by A8,A11
,VFUNCT_2:def 2
    .= ||. f1/.x1 - f2/.x1 - (f1/.x2-f2/.x2).|| by A8,A12,VFUNCT_2:def 2
    .= ||. f1/.x1 - (f2/.x1 + (f1/.x2-f2/.x2)).|| by RLVECT_1:41
    .= ||. f1/.x1 - (f1/.x2 + f2/.x1-f2/.x2).|| by RLVECT_1:42
    .= ||. f1/.x1 - (f1/.x2 + (f2/.x1-f2/.x2)).|| by RLVECT_1:42
    .= ||. f1/.x1 - f1/.x2 - (f2/.x1-f2/.x2).|| by RLVECT_1:41;
  then ||. (f1-f2)/.x1-(f1-f2)/.x2.|| <=||. f1/.x1-f1/.x2.||+||. f2/.x1-f2 /.
  x2.|| by CLVECT_1:105;
  hence thesis by A14,XXREAL_0:2;
end;

theorem Th127:
  for f be PartFunc of CNS1,CNS2 st f is_Lipschitzian_on X holds
  z(#)f is_Lipschitzian_on X
proof
  let f be PartFunc of CNS1,CNS2;
  assume
A1: f is_Lipschitzian_on X;
  then consider s be Real such that
A2: 0 < s and
A3: for x1,x2 be Point of CNS1 st x1 in X & x2 in X holds ||. f/.x1-f/.
  x2.||<=s*||. x1-x2.|| by Def27;
  X c= dom f by A1,Def27;
  hence
A4: X c= dom (z(#)f) by VFUNCT_2:def 4;
  now
    per cases;
    suppose
A5:   z=0;
      take s;
      thus 0<s by A2;
      let x1,x2 be Point of CNS1;
      assume that
A6:   x1 in X and
A7:   x2 in X;
      0<=||. x1-x2.|| by CLVECT_1:106;
      then
A8:   s*0<=s*||. x1-x2.|| by A2,XREAL_1:66;
      ||. (z(#)f)/.x1-(z(#)f)/.x2.|| = ||. z*(f/.x1)-(z(#)f)/.x2.|| by A4,A6,
VFUNCT_2:def 4
        .= ||. 0.CNS2-(z(#)f)/.x2.|| by A5,CLVECT_1:2
        .= ||. 0.CNS2 - z*(f/.x2).|| by A4,A7,VFUNCT_2:def 4
        .= ||. 0.CNS2-0.CNS2.|| by A5,CLVECT_1:2
        .= ||. 0.CNS2.|| by RLVECT_1:26
        .= 0 by CLVECT_1:103;
      hence ||. (z(#)f)/.x1-(z(#)f)/.x2.|| <=s*||. x1-x2.|| by A8;
    end;
    suppose
A9:   z<>0;
      take g = |.z.| *s;
      0<|.z.| by A9,COMPLEX1:133;
      then 0*s<|.z.|*s by A2,XREAL_1:70;
      hence 0<g;
      let x1,x2 be Point of CNS1;
      assume that
A10:  x1 in X and
A11:  x2 in X;
      0<=|.z.| by COMPLEX1:132;
      then
A12:  |.z.|*||. f/.x1-f/.x2.|| <= |.z.|*(s*||. x1-x2.||) by A3,A10,A11,
XREAL_1:66;
      ||. (z(#)f)/.x1-(z(#)f)/.x2.|| = ||. z*(f/.x1)-(z(#)f)/.x2.|| by A4,A10,
VFUNCT_2:def 4
        .= ||. z*(f/.x1) - z*(f/.x2).|| by A4,A11,VFUNCT_2:def 4
        .= ||. z*(f/.x1 - f/.x2).|| by CLVECT_1:10
        .= |.z.|*||. f/.x1 - f/.x2.|| by CLVECT_1:def 14;
      hence ||. (z(#)f)/.x1-(z(#)f)/.x2.|| <= g*||. x1-x2.|| by A12;
    end;
  end;
  hence thesis;
