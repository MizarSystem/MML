end;

definition
  let CNS1,CNS2 be ComplexNormSpace;
  let X be set;
  let f be PartFunc of CNS1,CNS2;
  pred f is_Lipschitzian_on X means
  :Def27:
  X c= dom f & ex r st 0 < r & for
x1,x2 be Point of CNS1 st x1 in X & x2 in X holds ||.f/.x1-f/.x2.||<=r*||.x1-x2
  .||;
end;

