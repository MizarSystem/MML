A89: for i being Element of NAT st i <= mk+1+1
holds IC (Computation s).i = insloc i
      proof
       let i be Element of NAT;
       assume A90: i <= mk+1+1;
       defpred P[Element of NAT] means $1<=mk+1+1
       implies IC (Computation s).$1=insloc $1;
A91:    P[0] by A1,AMI_1:def 19;
A92:    for n being Element of NAT st P[n] holds P[n + 1]
         proof
          let n be Element of NAT;
          assume A93: P[n];
          assume A94: n+1 <= mk+1+1;
then A95:      n < mk+1+1 by NAT_1:13;
          per cases;
          suppose A96: n=0;
           hence IC (Computation s).(n+1) = Exec(a:= intloc 0,s).IC SCM+FSA by
A86
           .= Next insloc n by A1,A96,SCMFSA_2:89
           .= insloc (n+1) by SCMFSA_2:32;
          end;
          suppose A97: n>0;
              n + 0 <= n + 1 by XREAL_1:9;
then A98:       CurInstr (Computation s).n
            = s.insloc n by  A93,A94,XXREAL_0:2,AMI_1:54
           .= SubFrom(a,intloc 0) by A80,A95,A97;
A99:       (Computation s).(n+1) = Following (Computation s).n by AMI_1:def 19
           .= Exec(SubFrom(a,intloc 0),(Computation s).n) by A98;
           thus IC (Computation s).(n+1) =
 Next IC (Computation s).n by A99,SCMFSA_2:91
           .= insloc (n+1) by A93,A94,NAT_1:13,SCMFSA_2:32;
         end;
         end;
          for i being Element of NAT holds P[i] from NAT_1:sch 1(A91,A92);
       hence IC (Computation s).i = insloc i by A90;
      end;
     defpred Q[Element of NAT] means $1 <= mk+1+1 implies
        (1 <= $1 implies (Computation s).$1.a = -$1+1+1) &
        (for b being Int-Location st b <> a holds (Computation s).$1.b=s.b) &
        (for f being FinSeq-Location holds (Computation s).$1.f = s.f);
A100:  Q[0] by AMI_1:def 19;
A101:  for n being Element of NAT st Q[n] holds Q[n + 1]
       proof
        let n be Element of NAT;
        assume A102: Q[n];
        assume A103: n + 1 <= mk+1+1;
        per cases;
          suppose A104: n = 0;
           hereby assume 1 <= n + 1;
              thus (Computation s).(n+1).a = Exec(a:= intloc 0,s).a by A86,A104
              .= -(n + 1)+1+1 by A2,A104,SCMFSA_2:89;
             end;
           hereby let b be Int-Location;
              assume A105: b <> a;
              thus (Computation s).(n+1).b = Exec(a:= intloc 0,s).b by A86,A104
              .= s.b by A105,SCMFSA_2:89;
             end;
           let f be FinSeq-Location;
           thus (Computation s).(n+1).f = Exec(a:= intloc 0,s).f by A86,A104
           .= s.f by SCMFSA_2:89;
          end;
          suppose A106: n > 0;
           then A107: 0 + 1 < n + 1 by XREAL_1:8;
           A108: n + 0 <= n + 1 by XREAL_1:9;
then A109:       n <= mk+1+1 by A103,XXREAL_0:2;
A110:       0 < n & n < mk+1+1 by A103,A106,NAT_1:13;
A111:       CurInstr (Computation s).n
           = ((Computation s).n).insloc n by A89,A109
           .= s.insloc n by AMI_1:54
           .= SubFrom(a,intloc 0) by A80,A110;
A112:       (Computation s).(n+1) = Following (Computation s).n by AMI_1:def 19
           .= Exec(SubFrom(a,intloc 0),(Computation s).n) by A111;
           hereby assume 1 <= n + 1;
              thus (Computation s).(n+1).a =
 -n+1+1 - (Computation s).n.intloc 0 by A102,A103,A107,A112,NAT_1:13
,SCMFSA_2:91
              .= -n+1+1 - s.intloc 0 by A4,A102,A103,A108,XXREAL_0:2
