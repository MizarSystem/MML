theorem
    for s being State of SCM+FSA st IC s = insloc 0 & s.intloc 0 = 1
 for a being Int-Location, k being Integer st a:=k c= s & a<>intloc 0 holds
     s is halting &
     (Result s).a = k &
     (for b being Int-Location st b <> a holds (Result s).b = s.b) &
     (for f being FinSeq-Location holds (Result s).f = s.f)
 proof
   let s be State of SCM+FSA;
   assume that
A1:  IC s = insloc 0 and
A2:  s.intloc 0 = 1;
   let a be Int-Location, k be Integer;
   assume that
A3:  a:=k c= s and
A4:  a <> intloc 0;
    per cases;
    suppose
A5:   k > 0;
    then consider k1 being Element of NAT such that
A6:     k1 + 1 = k &
        a:=k = Load (<*a:=intloc 0*> ^ (k1 |-> AddTo(a,intloc 0)) ^
            <*halt SCM+FSA*>) by Def2;
    set f = <*a:=intloc 0*> ^ (k1 |-> AddTo(a,intloc 0)) ^ <*halt SCM+FSA*>;
A7: len(<*a:=intloc 0*>^(k1|->AddTo(a,intloc 0)))
        = len<*a:=intloc 0*> + len(k1|->AddTo(a,intloc 0)) by FINSEQ_1:35
    .= 1 + len(k1|->AddTo(a,intloc 0)) by FINSEQ_1:56
    .= k by A6,FINSEQ_2:69;
    reconsider k as Element of NAT by A5,INT_1:16;
A8: len f = len(<*a:=intloc 0*>^(k1|->AddTo(a,intloc 0)))+len<*halt SCM+FSA*>
        by FINSEQ_1:35
    .= k + 1 by A7,FINSEQ_1:56;
then A9: dom f = Seg (k + 1) by FINSEQ_1:def 3;
A10: now let i be Element of NAT;
       assume A11: 1 <= i & i <= k + 1;
A12:   dom Load f = {insloc (m-'1): m in dom f} by Def1;
       thus i in dom f by A9,A11,FINSEQ_1:3;
       hence insloc (i-'1) in dom Load f by A12;
      end;
A13: now let i be Element of NAT;
       assume 0 <= i & i <= k;
then A14:     0 + 1 <= i + 1 & i + 1 <= k + 1 by XREAL_1:8;
       hence i + 1 in dom f by A10;
          insloc (i + 1 -' 1) in dom Load f by A10,A14;
       hence insloc i in dom Load f by BINARITH:39;
      end;
A15: now let i be Element of NAT;
       assume 0 <= i & i <= k;
then A16:     i + 1 in dom f & insloc i in dom Load f by A13;
       hence s.insloc i = (Load f).insloc i by A3,A6,GRFUNC_1:8
       .= f/.(i+1) by A16,Def1
       .= f.(i+1) by A16,FINSEQ_4:def 4;
      end;
A17: f.1 = (<*a:=intloc 0*>^((k1 |-> AddTo(a,intloc 0))^<*halt SCM+FSA*>)).1
        by FINSEQ_1:45
    .= a:= intloc 0 by FINSEQ_1:58;
A18: s.insloc 0 = f.(0+1) by A15
    .= a:= intloc 0 by A17;
A19: now let i be Element of NAT;
       assume A20: 1 < i & i <= k;
then A21:   1 <= i - 1 by Lm4;
       then 0 <= i - 1;
       then reconsider i1 = i - 1 as Element of NAT by INT_1:16;
          i - 1 <= k - 1 by A20,XREAL_1:11;
then A22:   i1 in Seg k1 by A6,A21,FINSEQ_1:3;
A23:   len <*a:= intloc 0*> = 1 by FINSEQ_1:56;
          dom (<*a:=intloc 0*>^(k1|->AddTo(a,intloc 0))) = Seg k
           by A7,FINSEQ_1:def 3;
       then i in dom (<*a:=intloc 0*>^(k1|->AddTo(a,intloc 0))) by A20,
FINSEQ_1:3;
       hence f.i=(<*a:=intloc 0*>^(k1|->AddTo(a,intloc 0))).i by FINSEQ_1:def 7
       .= (k1|->AddTo(a,intloc 0)).(i - 1) by A7,A20,A23,FINSEQ_1:37
       .= AddTo(a,intloc 0) by A22,FUNCOP_1:13;
      end;
A24: now let i be Element of NAT;
       assume A25: 0 < i & i < k;
then A26:   0 + 1 < i + 1 by XREAL_1:8;
A27:   i + 1 <= k by A25,NAT_1:13;
       thus s.insloc i = f.(i+1) by A15,A25
       .=AddTo(a,intloc 0) by A19,A26,A27;
      end;
       k < k+1 by XREAL_1:31;
then f.(k+1) = <* halt SCM+FSA *>.(k+1 - k) by A7,A8,FINSEQ_1:37
    .= halt SCM+FSA by FINSEQ_1:def 8;
then A28: s.insloc k = halt SCM+FSA by A15;
A29: now let n be Element of NAT;
       assume n = 0; hence
A30:   (Computation s).n = s by AMI_1:def 19; hence
A31:   CurInstr (Computation s).n = a:= intloc 0 by A1,A18;
       thus (Computation s).(n+1) = Following (Computation s).n by AMI_1:def 19
       .= Exec(a:= intloc 0,s) by A30,A31;
      end;
A32: for i being Element of NAT st i <= k holds IC (Computation s).i = insloc i
      proof
       let i be Element of NAT;
       assume A33: i <= k;
       defpred P[Element of NAT] means
        $1 <= k implies IC (Computation s).$1 = insloc $1;
A34:    P[0] by A1,AMI_1:def 19;
A35:    for n being Element of NAT st P[n] holds P[n + 1]
         proof
          let n be Element of NAT;
          assume A36: P[n];
          assume A37: n+1 <= k;
then A38:      n < k by NAT_1:13;
          per cases;
          suppose A39: n=0;
           hence IC (Computation s).(n+1) = Exec(a:= intloc 0,s).IC SCM+FSA by
A29
           .= Next insloc n by A1,A39,SCMFSA_2:89
           .= insloc (n+1) by SCMFSA_2:32;
          end;
          suppose A40: n>0;
              n + 0 <= n + 1 by XREAL_1:9;
then A41:       CurInstr (Computation s).n
            = s.insloc n by  A36,A37,XXREAL_0:2,AMI_1:54
           .= AddTo(a,intloc 0) by A24,A38,A40;
A42:       (Computation s).(n+1) = Following (Computation s).n by AMI_1:def 19
           .= Exec(AddTo(a,intloc 0),(Computation s).n) by A41;
           thus IC (Computation s).(n+1) =
 Next IC (Computation s).n by A42,SCMFSA_2:90
           .= insloc (n+1) by A36,A37,NAT_1:13,SCMFSA_2:32;
         end;
         end;
          for i being Element of NAT holds P[i] from NAT_1:sch 1(A34,A35);
       hence IC (Computation s).i = insloc i by A33;
      end;
     defpred Q[Element of NAT] means $1 <= k implies
        (1 <= $1 implies (Computation s).$1.a = $1) &
        (for b being Int-Location st b <> a holds (Computation s).$1.b=s.b) &
        (for f being FinSeq-Location holds (Computation s).$1.f = s.f);
A43:  Q[0] by AMI_1:def 19;
A44:  for n being Element of NAT st Q[n] holds Q[n + 1]
       proof
        let n be Element of NAT;
        assume A45: Q[n];
        assume A46: n + 1 <= k;
        per cases;
          suppose A47: n = 0;
           hereby assume 1 <= n + 1;
              thus (Computation s).(n+1).a = Exec(a:= intloc 0,s).a by A29,A47
              .= n + 1 by A2,A47,SCMFSA_2:89;
             end;
           hereby let b be Int-Location;
              assume A48: b <> a;
              thus (Computation s).(n+1).b = Exec(a:= intloc 0,s).b by A29,A47
              .= s.b by A48,SCMFSA_2:89;
             end;
           let f be FinSeq-Location;
           thus (Computation s).(n+1).f = Exec(a:= intloc 0,s).f by A29,A47
           .= s.f by SCMFSA_2:89;
          end;
          suppose A49: n > 0;
then A50:       0 + 1 <= n by INT_1:20;
           A51: n + 0 <= n + 1 by XREAL_1:9;
then A52:       n <= k by A46,XXREAL_0:2;
A53:       0 < n & n < k by A46,A49,NAT_1:13;
A54:       CurInstr (Computation s).n
           = ((Computation s).n).insloc n by A32,A52
           .= s.insloc n by AMI_1:54
           .= AddTo(a,intloc 0) by A24,A53;
A55:       (Computation s).(n+1) = Following (Computation s).n by AMI_1:def 19
           .= Exec(AddTo(a,intloc 0),(Computation s).n) by A54;
           hereby assume 1 <= n + 1;
              thus (Computation s).(n+1).a = n + (Computation s).n.intloc 0
by A45,A46,A50,A51,A55,XXREAL_0:2,SCMFSA_2:90
              .= n + 1 by A2,A4,A45,A46,A51,XXREAL_0:2;
             end;
           hereby let b be Int-Location;
              assume A56: b <> a;
              hence (Computation s).(n+1).b = (Computation s).n.b
                  by A55,SCMFSA_2:90
              .= s.b by A45,A46,A51,A56,XXREAL_0:2;
             end;
           let f be FinSeq-Location;
           thus ((Computation s).(n+1)).f = (Computation s).n.f
               by A55,SCMFSA_2:90
           .= s.f by A45,A46,A51,XXREAL_0:2;
       end;
       end;
A57: for i being Element of NAT holds Q[i] from NAT_1:sch 1(A43,A44);
A58:   CurInstr (Computation s).k
      = ((Computation s).k).insloc k by A32
     .= halt SCM+FSA by A28,AMI_1:54;
     hence s is halting by AMI_1:def 20;
then A59: (Computation s).k = Result s by A58,AMI_1:def 22;
        0 + 1 < k + 1 by A5,XREAL_1:8;
     then 1 <= k by NAT_1:13;
     hence thesis by A57,A59;
    end;
    suppose
A60:  k <= 0;
then   -0 <= - k by XREAL_1:26;
    then reconsider mk = - k as Element of NAT by INT_1:16;
    consider k1 being Element of NAT such that
A61:     k1 + k = 1 &
        a:=k = Load (<*a:=intloc 0*> ^ (k1 |-> SubFrom(a,intloc 0)) ^
            <*halt SCM+FSA*>) by A60,Def2;
    set f = <*a:=intloc 0*> ^ (k1 |-> SubFrom(a,intloc 0)) ^ <*halt SCM+FSA*>;
A62: len (<*a:=intloc 0*>^(k1|->SubFrom(a,intloc 0)))
        = len<*a:=intloc 0*> + len(k1|->SubFrom(a,intloc 0)) by FINSEQ_1:35
    .= 1 + len(k1|->SubFrom(a,intloc 0)) by FINSEQ_1:56
    .= mk+1+1 by A61,FINSEQ_2:69;
A63: len f = len(<*a:=intloc 0*>^(k1|->SubFrom(a,intloc 0)))+len<*halt SCM+FSA
*>
        by FINSEQ_1:35
    .= mk+1+1 + 1 by A62,FINSEQ_1:56;
then A64: dom f = Seg (mk+1+1 + 1) by FINSEQ_1:def 3;
A65: now let i be Element of NAT;
       assume A66: 1 <= i & i <= mk+1+1 + 1;
A67:   dom Load f = {insloc (m-'1): m in dom f} by Def1;
       thus i in dom f by A64,A66,FINSEQ_1:3;
       hence insloc (i-'1) in dom Load f by A67;
      end;
A68: now let i be Element of NAT;
       assume 0 <= i & i <= mk+1+1;
       then A69: 0 + 1 <= i + 1 & i + 1 <= mk+1+1 + 1 by XREAL_1:8;
       hence i + 1 in dom f by A65;
          insloc (i + 1 -' 1) in dom Load f by A65,A69;
       hence insloc i in dom Load f by BINARITH:39;
      end;
A70: now let i be Element of NAT;
       assume 0 <= i & i <= mk+1+1;
then A71:     i + 1 in dom f & insloc i in dom Load f by A68;
       hence s.insloc i = (Load f).insloc i by A3,A61,GRFUNC_1:8
       .= f/.(i+1) by A71,Def1
       .= f.(i+1) by A71,FINSEQ_4:def 4;
      end;
A72: f.1 = (<*a:=intloc 0*>^((k1|->SubFrom(a,intloc 0))^<*halt SCM+FSA*>)).1
        by FINSEQ_1:45
    .= a:= intloc 0 by FINSEQ_1:58;
A73: s.insloc 0 = f.(0+1) by A70
    .= a:= intloc 0 by A72;
A74: now let i be Element of NAT;
       assume A75: 1 < i & i <= mk+1+1;
       then A76: 1 - 1 < i - 1 by XREAL_1:11;
       then A77: 1 - 1 + 1 <= i - 1 by INT_1:20;
       reconsider i1 = i - 1 as Element of NAT by A76,INT_1:16;
          i - 1 <= mk+1+1 - 1 by A75,XREAL_1:11;
then A78:   i1 in Seg k1 by A61,A77,FINSEQ_1:3;
A79:   len <*a:= intloc 0*> = 1 by FINSEQ_1:56;
          dom (<*a:=intloc 0*>^(k1|->SubFrom(a,intloc 0))) = Seg (mk+1+1)
           by A62,FINSEQ_1:def 3;
       then i in dom (<*a:=intloc 0*>^(k1|->SubFrom(a,intloc 0))) by A75,
FINSEQ_1:3;
       hence f.i = (<*a:=intloc 0*>^(k1|->SubFrom(a,intloc 0))).i
           by FINSEQ_1:def 7
       .= (k1|->SubFrom(a,intloc 0)).(i - 1) by A62,A75,A79,FINSEQ_1:37
       .= SubFrom(a,intloc 0) by A78,FUNCOP_1:13;
      end;
A80: now let i be Element of NAT;
       assume A81: 0 < i & i < mk+1+1;
then A82:   0 + 1 < i + 1 by XREAL_1:8;
A83:   i + 1 <= mk+1+1 by A81,NAT_1:13;
       thus s.insloc i = f.(i+1) by A70,A81
       .=SubFrom(a,intloc 0) by A74,A82,A83;
