environ

 vocabularies FUNCT_1, TARSKI, ZFMISC_1, SETFAM_1, XBOOLE_0, CARD_1, SUBSET_1,
      ORDINAL1, ORDINAL2, RELAT_1, NUMBERS, NAT_1, ARYTM_3, FUNCT_2, CLASSES1;
 notations TARSKI, XBOOLE_0, ZFMISC_1, SUBSET_1, SETFAM_1, RELAT_1, FUNCT_1,
      FUNCT_2, CARD_1, NUMBERS, NAT_1, ORDINAL1, MCART_1, ORDINAL2;
 constructors SETFAM_1, MCART_1, WELLORD2, ORDINAL2, NAT_1, CARD_1, FUNCT_2,
      RELSET_1;
 registrations XBOOLE_0, SUBSET_1, ORDINAL1, FUNCT_1, FUNCT_2, RELSET_1,
      RELAT_1;
 requirements NUMERALS, SUBSET, BOOLE;
 definitions ORDINAL1, TARSKI, WELLORD2, FUNCT_1, XBOOLE_0, RELAT_1;
 theorems TARSKI, SETFAM_1, ZFMISC_1, ORDINAL1, MCART_1, FUNCT_1, CARD_1,
      WELLORD2, ENUMSET1, XBOOLE_0, XBOOLE_1, ORDINAL2, RELAT_1, FUNCT_2;
 schemes ORDINAL1, ORDINAL2, TARSKI, PARTFUN1, NAT_1, XBOOLE_0, WELLORD2;

begin

reserve W,X,Y,Z for set,
  f,g for Function,
  a,x,y,z for set;

definition
  let B be set;
  attr B is subset-closed means
  :Def1:
  for X,Y holds X in B & Y c= X implies Y in B;
end;

definition
  let B be set;
  attr B is Tarski means
  :Def2:
  B is subset-closed &
  (for X holds X in B implies bool X in B) &
  for X holds X c= B implies X,B are_equipotent or X in B;
end;

definition
  let A,B be set;
  pred B is_Tarski-Class_of A means
  :Def3:
  A in B & B is Tarski;
end;

definition
  let A be set;
  func Tarski-Class A -> set means
  :Def4:
  it is_Tarski-Class_of A &
  for D being set st D is_Tarski-Class_of A holds it c= D;
  existence
  proof
    consider Big being set such that
A1: A in Big and
A2: for X,Y holds X in Big & Y c= X implies Y in Big and
    A3: (
 for X holds X in Big implies bool X in Big)& for X holds X c= Big implies X,
    Big are_equipotent or X in Big
    by ZFMISC_1:136;
A4: Big is subset-closed by A2,Def1;
A5: Big is Tarski by A3,A4,Def2;
    defpred P[set] means $1 is_Tarski-Class_of A;
    consider Classes being set such that
A6: X in Classes iff X in bool Big & P[X] from XBOOLE_0:sch 1;
    set IT = meet Classes;
    A7: Big
 in bool Big & Big is_Tarski-Class_of A by A1,A5,Def3,ZFMISC_1:def 1;
A8: Big in Classes by A6,A7;
A9: Classes <> {} by A6,A7;
A10: now
      let X;
      assume
A11:  X in Classes;
A12:  X is_Tarski-Class_of A by A6,A11;
      thus A in X by A12,Def3;
    end;
A13: A in IT by A9,A10,SETFAM_1:def 1;
    take IT;
    thus A in IT by A9,A10,SETFAM_1:def 1;
    thus
A14: X in IT & Y c= X implies Y in IT
    proof
      assume that
A15:  X in IT and
A16:  Y c= X;
A17:  now
        let Z;
        assume
A18:    Z in Classes;
A19:    Z is_Tarski-Class_of A by A6,A18;
A20:    Z is Tarski by A19,Def3;
A21:    Z is subset-closed by A20,Def2;
A22:    X in Z by A15,A18,SETFAM_1:def 1;
        thus Y in Z by A16,A21,A22,Def1;
      end;
      thus thesis by A9,A17,SETFAM_1:def 1;
    end;
    thus
A23: X in IT implies bool X in IT
    proof
      assume
A24:  X in IT;
A25:  now
        let Z;
        assume
A26:    Z in Classes;
A27:    Z is_Tarski-Class_of A by A6,A26;
A28:    Z is Tarski by A27,Def3;
A29:    X in Z by A24,A26,SETFAM_1:def 1;
        thus bool X in Z by A28,A29,Def2;
      end;
      thus thesis by A9,A25,SETFAM_1:def 1;
    end;
    thus
A30: X c= IT implies X,IT are_equipotent or X in IT
    proof
      assume that
A31:  X c= IT and
A32:  not X,IT are_equipotent;
A33:  now
        let Z;
        assume
A34:    Z in Classes;
A35:    Z is_Tarski-Class_of A by A6,A34;
A36:    Z is Tarski by A35,Def3;
A37:    IT c= Z by A34,SETFAM_1:4;
A38:    X c= Z by A31,A37,XBOOLE_1:1;
A39:    X,Z are_equipotent or X in Z by A36,A38,Def2;
        thus X in Z by A31,A32,A37,A39,CARD_1:44;
      end;
      thus thesis by A9,A33,SETFAM_1:def 1;
    end;
    let D be set;
    assume
A40: D is_Tarski-Class_of A;
A41: A in D by A40,Def3;
A42: D is Tarski by A40,Def3;
A43: D is subset-closed by A42,Def2;
A44: IT /\ D is_Tarski-Class_of A
    proof
      thus A in IT /\ D by A13,A41,XBOOLE_0:def 4;
      thus X in IT /\ D & Y c= X implies Y in IT /\ D
      proof
        assume that
A45:    X in IT /\ D and
A46:    Y c= X;
A47:    X in IT by A45,XBOOLE_0:def 4;
A48:    X in D by A45,XBOOLE_0:def 4;
A49:    Y in IT by A14,A46,A47;
A50:    Y in D by A43,A46,A48,Def1;
        thus thesis by A49,A50,XBOOLE_0:def 4;
      end;
      thus X in IT /\ D implies bool X in IT /\ D
      proof
        assume
A51:    X in IT /\ D;
A52:    X in IT by A51,XBOOLE_0:def 4;
A53:    X in D by A51,XBOOLE_0:def 4;
A54:    bool X in IT by A23,A52;
A55:    bool X in D by A42,A53,Def2;
        thus thesis by A54,A55,XBOOLE_0:def 4;
      end;
      let X such that
A56:  X c= IT /\ D and
A57:  not X,IT /\ D are_equipotent;
A58:  IT /\ D c= IT by XBOOLE_1:17;
A59:  IT /\ D c= D by XBOOLE_1:17;
A60:  not X,IT are_equipotent by A56,A57,A58,CARD_1:44;
      A61:  X c= D & not X,D are_equipotent by A56,A57,A59,CARD_1:44,XBOOLE_1:1
      ;
A62:  X in IT by A30,A56,A58,A60,XBOOLE_1:1;
A63:  X in D by A42,A61,Def2;
      thus thesis by A62,A63,XBOOLE_0:def 4;
    end;
A64: IT /\ D c= Big
    proof
      let x;
      assume
A65:  x in IT /\ D;
A66:  x in IT by A65,XBOOLE_0:def 4;
      thus thesis by A8,A66,SETFAM_1:def 1;
    end;
A67: IT /\ D in Classes by A6,A44,A64;
A68: IT c= IT /\ D by A67,SETFAM_1:4;
A69: IT /\ D c= D by XBOOLE_1:17;
    thus thesis by A68,A69,XBOOLE_1:1;
  end;
  uniqueness
  proof
    let D1,D2 be set such that
    A70: (
 D1 is_Tarski-Class_of A & for D being set st D is_Tarski-Class_of A holds
    D1 c= D )&( D2 is_Tarski-Class_of A & for D being set st D
    is_Tarski-Class_of A holds D2 c= D );
    thus D1 c= D2 & D2 c= D1 by A70;
  end;
end;

registration
  let A be set;
  cluster Tarski-Class A -> non empty;
  coherence
  proof
A1: Tarski-Class A is_Tarski-Class_of A by Def4;
    thus thesis by A1,Def3;
  end;
end;

canceled;

theorem
  W is Tarski iff W is subset-closed &
  (for X st X in W holds bool X in W) &
  for X st X c= W & card X in card W holds X in W
proof
  hereby
    assume
A1: W is Tarski;
    thus W is subset-closed & for X st X in W holds bool X in W by A1,Def2;
    let X;
    assume that
A2: X c= W and
A3: card X in card W;
A4: card X <> card W by A3;
A5: not X,W are_equipotent by A4,CARD_1:21;
    thus X in W by A1,A2,A5,Def2;
  end;
A6: now
    assume
A7: for X st X c= W & card X in card W holds X in W;
    let X;
    assume
A8: X c= W;
A9: card X c= card W & not card X in card W or X in W by A7,A8,CARD_1:27;
A10: card X = card W or X in W by A9,CARD_1:13;
    thus X,W are_equipotent or X in W by A10,CARD_1:21;
  end;
  thus thesis by A6,Def2;
end;

canceled 2;

theorem Th5:
  X in Tarski-Class X
proof
A1: Tarski-Class X is_Tarski-Class_of X by Def4;
  thus thesis by A1,Def3;
end;

theorem Th6:
  Y in Tarski-Class X & Z c= Y implies Z in Tarski-Class X
proof
A1: Tarski-Class X is_Tarski-Class_of X by Def4;
A2: Tarski-Class X is Tarski by A1,Def3;
A3: Tarski-Class X is subset-closed by A2,Def2;
  thus thesis by A3,Def1;
end;

theorem Th7:
  Y in Tarski-Class X implies bool Y in Tarski-Class X
proof
A1: Tarski-Class X is_Tarski-Class_of X by Def4;
A2: Tarski-Class X is Tarski by A1,Def3;
  thus thesis by A2,Def2;
end;

theorem Th8:
  Y c= Tarski-Class X implies
  Y,Tarski-Class X are_equipotent or Y in Tarski-Class X
proof
A1: Tarski-Class X is_Tarski-Class_of X by Def4;
A2: Tarski-Class X is Tarski by A1,Def3;
  thus thesis by A2,Def2;
end;

theorem
  Y c= Tarski-Class X & card Y in card Tarski-Class X implies
  Y in Tarski-Class X
proof
  assume that
A1: Y c= Tarski-Class X and
A2: card Y in card Tarski-Class X;
A3: card Y <> card Tarski-Class X by A2;
A4: not Y,Tarski-Class X are_equipotent by A3,CARD_1:21;
  thus thesis by A1,A4,Th8;
end;

reserve u,v for Element of Tarski-Class(X),
  A,B,C for Ordinal,
  L for T-Sequence;

definition
  let X,A;
  func Tarski-Class(X,A) means
  :Def5:
  ex L st it = last L & dom L = succ A & L.{} = { X } &
  (for C st succ C in succ A holds
  L.succ C = { u : ex v st v in L.C & u c= v } \/ { bool v : v in L.C } \/
  bool(L.C) /\ Tarski-Class X) &
  for C st C in succ A & C <> {} & C is limit_ordinal
  holds L.C = (union rng(L|C)) /\ Tarski-Class X;
  correctness
  proof
    deffunc C(Ordinal,set) = { u : ex v st v in $2 & u c= v } \/
    { bool v : v in $2 } \/ bool $2 /\ Tarski-Class X;
    deffunc D(Ordinal,T-Sequence) = (union rng $2) /\ Tarski-Class X;
    thus (ex x,L st x = last L & dom L = succ A & L.{} = {X} &
    (for C st succ C in succ A holds L.succ C = C(C,L.C)) &
    for C st C in succ A & C <> {} & C is limit_ordinal
    holds L.C = D(C,L|C) ) & for x1,x2 being set st
    (ex L st x1 = last L & dom L = succ A & L.{} = {X} &
    (for C st succ C in succ A holds L.succ C = C(C,L.C)) &
    for C st C in succ A & C <> {} & C is limit_ordinal
    holds L.C = D(C,L|C) ) &
    (ex L st x2 = last L & dom L = succ A & L.{} = {X} &
    (for C st succ C in succ A holds L.succ C = C(C,L.C)) &
    for C st C in succ A & C <> {} & C is limit_ordinal holds L.C = D(C,L|C) )
    holds x1 = x2 from ORDINAL2:sch 7;
  end;
end;

Lm1: now
  let X;
  deffunc F(Ordinal) = Tarski-Class(X,$1);
  deffunc C(Ordinal,set) = { u : ex v st v in $2 & u c= v } \/
  { bool v : v in $2 } \/ bool $2 /\ Tarski-Class X;
  deffunc D(Ordinal,T-Sequence) = (union rng $2) /\ Tarski-Class X;
A1: for A,x holds x = F(A) iff
  ex L st x = last L & dom L = succ A & L.{} = { X } &
  (for C st succ C in succ A holds L.succ C = C(C,L.C)) &
  for C st C in succ A & C <> {} & C is limit_ordinal
  holds L.C = D(C,L|C) by Def5;
  thus F({}) = { X } from ORDINAL2:sch 8(A1);
  thus for A holds F(succ A) = C(A,F(A)) from ORDINAL2:sch 9(A1);
  thus A <> {} & A is limit_ordinal & dom L = A &
  (for B st B in A holds L.B = Tarski-Class(X,B)) implies
  Tarski-Class(X,A) = (union rng L) /\ Tarski-Class X
  proof
    assume that
A2: A <> {} & A is limit_ordinal and
A3: dom L = A and
A4: for B st B in A holds L.B = F(B);
    thus F(A) = D(A,L) from ORDINAL2:sch 10(A1,A2,A3,A4);
  end;
end;

definition
  let X,A;
  redefine func Tarski-Class(X,A) -> Subset of Tarski-Class X;
  coherence
  proof
    defpred P[Ordinal] means Tarski-Class(X,$1) is Subset of Tarski-Class X;
A1: { X } c= Tarski-Class X
    proof
      let x;
      assume
A2:   x in { X };
A3:   x = X by A2,TARSKI:def 1;
      thus thesis by A3,Th5;
    end;
A4: P[{}] by A1,Lm1;
A5: P[B] implies P[succ B]
    proof
      assume
A6:   Tarski-Class(X,B) is Subset of Tarski-Class X;
      reconsider S = Tarski-Class(X,B) as Subset of Tarski-Class X by A6;
      set Y = Tarski-Class(X,succ B);
A7:   Y c= Tarski-Class X
      proof
        let x;
        assume
A8:     x in Y;
A9:     x in { u : ex v st v in S & u c= v } \/ { bool v : v in S } \/
        bool S /\ Tarski-Class X by A8,Lm1;
        A10:    x in { u : ex v st v in S & u c= v } \/ { bool v : v in S } or
        x in bool S /\ Tarski-Class X by A9,XBOOLE_0:def 3;
A11:    now
          assume
A12:      x in { u : ex v st v in S & u c= v };
A13:      ex u st x = u & ex v st v in S & u c= v by A12;
          thus thesis by A13;
        end;
A14:    now
          assume
A15:      x in { bool v : v in S };
A16:      ex v st x = bool v & v in S by A15;
          thus thesis by A16,Th7;
        end;
        thus thesis by A10,A11,A14,XBOOLE_0:def 3,def 4;
      end;
      thus thesis by A7;
    end;
A17: for B st B <> {} & B is limit_ordinal &
    for C st C in B holds P[C] holds P[B]
    proof
      let B such that
A18:  B <> {} & B is limit_ordinal and
      for C st C in B holds Tarski-Class(X,C) is Subset of Tarski-Class X;
      deffunc f(Ordinal) = Tarski-Class(X,$1);
      consider L such that
A19:  dom L = B & for C st C in B holds L.C = f(C) from ORDINAL2:sch 2;
A20:  Tarski-Class(X,B) = (union rng L) /\ Tarski-Class X by A18,A19,Lm1;
      thus thesis by A20,XBOOLE_1:17;
    end;
A21: for A holds P[A] from ORDINAL2:sch 1(A4,A5,A17);
    thus thesis by A21;
  end;
end;

theorem
  Tarski-Class(X,{}) = { X } by Lm1;

theorem
  Tarski-Class(X,succ A) = { u : ex v st v in Tarski-Class(X,A) & u c= v } \/
  { bool v : v in Tarski-Class(X,A) } \/
  bool Tarski-Class(X,A) /\ Tarski-Class X by Lm1;

theorem Th12:
  A <> {} & A is limit_ordinal implies
  Tarski-Class(X,A) = { u : ex B st B in A & u in Tarski-Class(X,B) }
proof
  assume
A1: A <> {} & A is limit_ordinal;
  deffunc f(Ordinal) = Tarski-Class(X,$1);
  consider L such that
A2: dom L = A & for B st B in A holds L.B = f(B) from ORDINAL2:sch 2;
A3: Tarski-Class(X,A) = (union rng L) /\ Tarski-Class X by A1,A2,Lm1;
  thus
  Tarski-Class(X,A) c= { u : ex B st B in A & u in Tarski-Class(X,B) }
  proof
    let x;
    assume
A4: x in Tarski-Class(X,A);
A5: x in union rng L by A3,A4,XBOOLE_0:def 4;
    consider Y such that
A6: x in Y and
A7: Y in rng L by A5,TARSKI:def 4;
    consider y such that
A8: y in dom L and
A9: Y = L.y by A7,FUNCT_1:def 5;
    reconsider y as Ordinal by A8;
A10: Y = Tarski-Class(X,y) by A2,A8,A9;
    thus thesis by A2,A6,A8,A10;
  end;
  let x;
  assume
A11: x in { u : ex B st B in A & u in Tarski-Class(X,B) };
  consider u such that
A12: x = u and
A13: ex B st B in A & u in Tarski-Class(X,B) by A11;
  consider B such that
A14: B in A and
A15: u in Tarski-Class(X,B) by A13;
A16: L.B = Tarski-Class(X,B) by A2,A14;
A17: Tarski-Class(X,B) in rng L by A2,A14,A16,FUNCT_1:def 5;
A18: u in union rng L by A15,A17,TARSKI:def 4;
  thus thesis by A3,A12,A18,XBOOLE_0:def 4;
end;

theorem Th13:
  Y in Tarski-Class(X,succ A) iff
  Y c= Tarski-Class(X,A) & Y in Tarski-Class X or
  ex Z st Z in Tarski-Class(X,A) & (Y c= Z or Y = bool Z)
proof
  set T1 = { u : ex v st v in Tarski-Class(X,A) & u c= v };
  set T2 = { bool v : v in Tarski-Class(X,A) };
  set T3 = bool Tarski-Class(X,A) /\ Tarski-Class X;
A1: Tarski-Class(X,succ A) = T1 \/ T2 \/ T3 by Lm1;
  thus Y in Tarski-Class(X,succ A) implies
  Y c= Tarski-Class(X,A) & Y in Tarski-Class X or
  ex Z st Z in Tarski-Class(X,A) & (Y c= Z or Y = bool Z)
  proof
    assume
A2: Y in Tarski-Class(X,succ A);
A3: Y in T1 \/ T2 or Y in T3 by A1,A2,XBOOLE_0:def 3;
A4: now
      assume
A5:   Y in T1;
A6:   ex u st Y = u & ex v st v in Tarski-Class(X,A) & u c= v by A5;
      thus ex Z st Z in Tarski-Class(X,A) & (Y c= Z or Y = bool Z)
      by A6;
    end;
A7: now
      assume
A8:   Y in T2;
A9:   ex v st Y = bool v & v in Tarski-Class(X,A) by A8;
      thus ex Z st Z in Tarski-Class(X,A) & (Y c= Z or Y = bool Z)
      by A9;
    end;
A10: now
      assume
A11:  Y in T3;
A12:  Y in bool Tarski-Class(X,A) by A11,XBOOLE_0:def 4;
      thus thesis by A11,A12,XBOOLE_0:def 4;
    end;
    thus thesis by A3,A4,A7,A10,XBOOLE_0:def 3;
  end;
  assume
A13: Y c= Tarski-Class(X,A) & Y in Tarski-Class X or
  ex Z st Z in Tarski-Class(X,A) & (Y c= Z or Y = bool Z);
A14: now
    assume
A15: Y c= Tarski-Class(X,A) & Y in Tarski-Class X;
A16: Y in T3 by A15,XBOOLE_0:def 4;
    thus thesis by A1,A16,XBOOLE_0:def 3;
  end;
A17: now
    given Z such that
A18: Z in Tarski-Class(X,A) and
A19: Y c= Z or Y = bool Z;
    reconsider Z as Element of Tarski-Class X by A18;
    reconsider y = Y as Element of Tarski-Class X by A18,A19,Th6,Th7;
A20: now
      assume
A21:  Y c= Z;
A22:  y in T1 by A18,A21;
A23:  Y in T1 \/ T2 by A22,XBOOLE_0:def 3;
      thus thesis by A1,A23,XBOOLE_0:def 3;
    end;
A24: now
      assume
A25:  Y = bool Z;
A26:  y in T2 by A18,A25;
A27:  Y in T1 \/ T2 by A26,XBOOLE_0:def 3;
      thus thesis by A1,A27,XBOOLE_0:def 3;
    end;
    thus thesis by A19,A20,A24;
  end;
  thus thesis by A13,A14,A17;
end;

theorem
  Y c= Z & Z in Tarski-Class(X,A) implies Y in Tarski-Class(X,succ A) by Th13;

theorem
  Y in Tarski-Class(X,A) implies bool Y in Tarski-Class(X,succ A) by Th13;

theorem Th16:
  A <> {} & A is limit_ordinal implies
  (x in Tarski-Class(X,A) iff ex B st B in A & x in Tarski-Class(X,B))
proof
  assume
A1: A <> {} & A is limit_ordinal;
A2: Tarski-Class(X,A) = { u : ex B st B in A & u in Tarski-Class(X,B) }
  by A1,Th12;
  thus
  x in Tarski-Class(X,A) implies ex B st B in A & x in Tarski-Class(X,B)
  proof
    assume
A3: x in Tarski-Class(X,A);
A4: ex u st x = u & ex B st B in A & u in Tarski-Class(X,B) by A2,A3;
    thus thesis by A4;
  end;
  given B such that
A5: B in A and
A6: x in Tarski-Class(X,B);
  reconsider u = x as Element of Tarski-Class X by A6;
A7: u in { v : ex B st B in A & v in Tarski-Class(X,B) } by A5,A6;
  thus thesis by A1,A7,Th12;
end;

theorem
  A <> {} & A is limit_ordinal & Y in Tarski-Class(X,A) &
  (Z c= Y or Z = bool Y) implies Z in Tarski-Class(X,A)
proof
  assume that
A1: A <> {} and
A2: A is limit_ordinal and
A3: Y in Tarski-Class(X,A);
  consider B such that
A4: B in A and
A5: Y in Tarski-Class(X,B) by A1,A2,A3,Th16;
A6: bool Y in Tarski-Class(X,succ B) by A5,Th13;
A7: Z c= Y implies Z in Tarski-Class(X,succ B) by A5,Th13;
A8: succ B in A by A2,A4,ORDINAL1:41;
  assume
A9: Z c= Y or Z = bool Y;
  thus thesis by A2,A6,A7,A8,A9,Th16;
end;

theorem Th18:
  Tarski-Class(X,A) c= Tarski-Class(X,succ A)
proof
  let x;
  assume
A1: x in Tarski-Class(X,A);
A2: x in { u : ex v st v in Tarski-Class(X,A) & u c= v } by A1;
A3: x in { u : ex v st v in Tarski-Class(X,A) & u c= v } \/
  { bool v : v in Tarski-Class(X,A) } by A2,XBOOLE_0:def 3;
  A4: Tarski-Class
(X,succ A) = { u : ex v st v in Tarski-Class(X,A) & u c= v } \/
  { bool v : v in Tarski-Class(X,A) } \/
  bool Tarski-Class(X,A) /\ Tarski-Class X by Lm1;
  thus thesis by A3,A4,XBOOLE_0:def 3;
end;

theorem Th19:
  A c= B implies Tarski-Class(X,A) c= Tarski-Class(X,B)
proof
  defpred OnP[Ordinal] means
  A c= $1 implies Tarski-Class(X,A) c= Tarski-Class(X,$1);
A1: for B st for C st C in B holds OnP[C] holds OnP[B]
  proof
    let B such that
A2: for C st C in B holds OnP[C] and
A3: A c= B;
    let x;
    assume
A4: x in Tarski-Class(X,A);
A5: now
      assume
A6:   A <> B;
A7:   A c< B by A3,A6,XBOOLE_0:def 8;
A8:   A in B by A7,ORDINAL1:21;
A9:   B <> {} by A3,A6;
A10:  now
        given C such that
A11:    B = succ C;
A12:    A c= C & C in B by A8,A11,ORDINAL1:34;
A13:    Tarski-Class(X,A) c= Tarski-Class(X,C) by A2,A12;
A14:    Tarski-Class(X,C) c= Tarski-Class(X,B) by A11,Th18;
A15:    Tarski-Class(X,A) c= Tarski-Class(X,B) by A13,A14,XBOOLE_1:1;
        thus thesis by A4,A15;
      end;
A16:  now
        assume
A17:    for C holds B <> succ C;
A18:    B is limit_ordinal by A17,ORDINAL1:42;
A19:    Tarski-Class(X,B) = { v : ex C st C in B & v in Tarski-Class(X,C)
        } by A9,A18,Th12;
        thus thesis by A4,A8,A19;
      end;
      thus thesis by A10,A16;
    end;
    thus thesis by A4,A5;
  end;
A20: for B holds OnP[B] from ORDINAL1:sch 2(A1);
  thus thesis by A20;
end;

theorem Th20:
  ex A st Tarski-Class(X,A) = Tarski-Class(X,succ A)
proof
  assume
A1: for A holds Tarski-Class(X,A) <> Tarski-Class(X,succ A);
  defpred P[set] means ex A st $1 in Tarski-Class(X,A);
  consider Z such that
A2: x in Z iff x in Tarski-Class X & P[x] from XBOOLE_0:sch 1;
  defpred P[set,set] means
  ex A st $2 = A & $1 in Tarski-Class(X,succ A) & not $1 in Tarski-Class(X,A);
A3: for x,y,z st P[x,y] & P[x,z] holds y = z
  proof
    let x,y,z;
    given A such that
A4: y = A and
A5: x in Tarski-Class(X,succ A) and
A6: not x in Tarski-Class(X,A);
    given B such that
A7: z = B and
A8: x in Tarski-Class(X,succ B) and
A9: not x in Tarski-Class(X,B);
    assume
A10: y <> z;
A11: A c= B or B c= A;
A12: A c< B or B c< A by A4,A7,A10,A11,XBOOLE_0:def 8;
A13: now
      assume
A14:  A c< B;
A15:  A in B by A14,ORDINAL1:21;
A16:  succ A c= B by A15,ORDINAL1:33;
A17:  Tarski-Class(X,succ A) c= Tarski-Class(X,B) by A16,Th19;
      thus contradiction by A5,A9,A17;
    end;
A18: B in A by A12,A13,ORDINAL1:21;
A19: succ B c= A by A18,ORDINAL1:33;
A20: Tarski-Class(X,succ B) c= Tarski-Class(X,A) by A19,Th19;
    thus contradiction by A6,A8,A20;
  end;
  consider Y such that
A21: x in Y iff ex y st y in Z & P[y,x] from TARSKI:sch 1(A3);
A22: now
    let A;
A23: Tarski-Class(X,A) <> Tarski-Class(X,succ A) by A1;
A24: Tarski-Class(X,A) c= Tarski-Class(X,succ A) by Th18;
    consider x such that
A25: not (x in Tarski-Class(X,A) iff x in Tarski-Class(X,succ A)) by A23,
    TARSKI:2;
A26: x in Z by A2,A25;
    thus A in Y by A21,A24,A25,A26;
  end;
  thus contradiction by A22,ORDINAL1:38;
end;

theorem Th21:
  Tarski-Class(X,A) = Tarski-Class(X,succ A) implies
  Tarski-Class(X,A) = Tarski-Class X
proof
  assume
A1: Tarski-Class(X,A) = Tarski-Class(X,succ A);
A2: {} c= A;
A3: Tarski-Class(X,{}) c= Tarski-Class(X,A) by A2,Th19;
A4: Tarski-Class(X,{}) = { X } & X in { X } by Lm1,TARSKI:def 1;
A5: Tarski-Class(X,A) is_Tarski-Class_of X
  proof
    thus X in Tarski-Class(X,A) by A3,A4;
A6: Tarski-Class(X,succ A) =
    { u : ex v st v in Tarski-Class(X,A) & u c= v } \/
    { bool v : v in Tarski-Class(X,A) } \/
    bool Tarski-Class(X,A) /\ Tarski-Class X by Lm1;
A7: Tarski-Class X is_Tarski-Class_of X by Def4;
A8: Tarski-Class X is Tarski by A7,Def3;
    thus Z in Tarski-Class(X,A) & Y c= Z implies Y in Tarski-Class(X,A)
    proof
      assume
A9:   Z in Tarski-Class(X,A) & Y c= Z;
A10:  Tarski-Class X is_Tarski-Class_of X by Def4;
A11:  Tarski-Class X is Tarski by A10,Def3;
A12:  Tarski-Class X is subset-closed by A11,Def2;
      reconsider y = Y as Element of Tarski-Class X by A9,A12,Def1;
A13:  ex v st v in Tarski-Class(X,A) & y c= v by A9;
A14:  Y in { u : ex v st v in Tarski-Class(X,A) & u c= v } by A13;
A15:  Y in { u : ex v st v in Tarski-Class(X,A) & u c= v } \/
      { bool v : v in Tarski-Class(X,A) } by A14,XBOOLE_0:def 3;
      thus thesis by A1,A6,A15,XBOOLE_0:def 3;
    end;
    thus Y in Tarski-Class(X,A) implies bool Y in Tarski-Class(X,A)
    proof
      assume
A16:  Y in Tarski-Class(X,A);
A17:  bool Y in { bool u : u in Tarski-Class(X,A) } by A16;
A18:  bool Y in { u : ex v st v in Tarski-Class(X,A) & u c= v } \/
      { bool v : v in Tarski-Class(X,A) } by A17,XBOOLE_0:def 3;
      thus thesis by A1,A6,A18,XBOOLE_0:def 3;
    end;
    let Y;
    assume that
A19: Y c= Tarski-Class(X,A) and
A20: not Y,Tarski-Class(X,A) are_equipotent;
A21: Y c= Tarski-Class X by A19,XBOOLE_1:1;
A22: Y,Tarski-Class X are_equipotent or Y in Tarski-Class X by A8,A21,Def2;
    thus thesis by A1,A19,A20,A22,Th13,CARD_1:44;
  end;
  thus Tarski-Class(X,A) c= Tarski-Class X &
  Tarski-Class X c= Tarski-Class(X,A) by A5,Def4;
end;

theorem Th22:
  ex A st Tarski-Class(X,A) = Tarski-Class X
proof
  consider A such that
A1: Tarski-Class(X,A) = Tarski-Class(X,succ A) by Th20;
  take A;
  thus thesis by A1,Th21;
end;

theorem
  ex A st Tarski-Class(X,A) = Tarski-Class X &
  for B st B in A holds Tarski-Class(X,B) <> Tarski-Class X
proof
  defpred P[Ordinal] means Tarski-Class(X,$1) = Tarski-Class X;
A1: ex A st P[A] by Th22;
  consider A such that
A2: P[A] & for B st P[B] holds A c= B from ORDINAL1:sch 1(A1);
  take A;
  thus Tarski-Class(X,A) = Tarski-Class X by A2;
  let B;
  assume
A3: B in A;
  thus thesis by A2,A3,ORDINAL1:7;
end;

theorem
  Y <> X & Y in Tarski-Class X implies
  ex A st not Y in Tarski-Class(X,A) & Y in Tarski-Class(X,succ A)
proof
  assume that
A1: Y <> X and
A2: Y in Tarski-Class X;
  defpred P[Ordinal] means Y in Tarski-Class(X,$1);
A3: ex A st Tarski-Class(X,A) = Tarski-Class X by Th22;
A4: ex A st P[A] by A2,A3;
  consider A such that
A5: P[A] & for B st P[B] holds A c= B from ORDINAL1:sch 1(A4);
A6: not Y in { X } by A1,TARSKI:def 1;
A7: Tarski-Class(X,{}) = { X } by Lm1;
A8: now
    assume
A9: A is limit_ordinal;
A10: ex B st B in A & Y in Tarski-Class(X,B) by A5,A6,A7,A9,Th16;
    thus contradiction by A5,A10,ORDINAL1:7;
  end;
  consider B such that
A11: A = succ B by A8,ORDINAL1:42;
  take B;
A12: not A c= B by A11,ORDINAL1:7,10;
  thus thesis by A5,A11,A12;
end;

theorem Th25:
  X is epsilon-transitive implies
  for A st A <> {} holds Tarski-Class(X,A) is epsilon-transitive
proof
  assume
A1: Y in X implies Y c= X;
  defpred OnP[Ordinal] means
  $1 <> {} implies Tarski-Class(X,$1) is epsilon-transitive;
A2: for A st for B st B in A holds OnP[B] holds OnP[A]
  proof
    let A such that
A3: for B st B in A holds OnP[B] and
A4: A <> {};
    let Y such that
A5: Y in Tarski-Class(X,A);
A6: now
      given B such that
A7:   A = succ B;
A8:   B in A by A7,ORDINAL1:10;
A9:   B c= A by A8,ORDINAL1:def 2;
A10:  OnP[B] by A3,A7,ORDINAL1:10;
A11:  Tarski-Class(X,B) c= Tarski-Class(X,A) by A9,Th19;
A12:  now
        assume
A13:    not Y c= Tarski-Class(X,B);
        consider Z such that
A14:    Z in Tarski-Class(X,B) and
A15:    Y c= Z or Y = bool Z by A5,A7,A13,Th13;
A16:    now
          assume
A17:      Y = bool Z;
          thus thesis
          proof
            let x;
            assume
A18:        x in Y;
            thus thesis by A7,A14,A17,A18,Th13;
          end;
        end;
A19:    now
          assume
A20:      Y c= Z;
          thus thesis
          proof
            let x;
            assume
A21:        x in Y;
A22:        x in Z by A20,A21;
A23:        now
              assume
A24:          B = {};
A25:          Tarski-Class(X,B) = { X } by A24,Lm1;
A26:          Z = X by A14,A25,TARSKI:def 1;
A27:          x c= X by A1,A20,A21,A26;
              thus thesis by A7,A14,A26,A27,Th13;
            end;
A28:        now
              assume
A29:          B <> {};
A30:          Z c= Tarski-Class(X,B) by A10,A14,A29,ORDINAL1:def 2;
A31:          x in Tarski-Class(X,B) by A22,A30;
              thus thesis by A11,A31;
            end;
            thus thesis by A23,A28;
          end;
        end;
        thus thesis by A15,A16,A19;
      end;
      thus thesis by A11,A12,XBOOLE_1:1;
    end;
A32: now
      assume
A33:  for B holds A <> succ B;
A34:  A is limit_ordinal by A33,ORDINAL1:42;
      consider B such that
A35:  B in A and
A36:  Y in Tarski-Class(X,B) by A4,A5,A34,Th16;
A37:  succ B c= A by A35,ORDINAL1:33;
A38:  succ B <> A by A33;
A39:  Tarski-Class(X,B) c= Tarski-Class(X,succ B) by Th18;
A40:  succ B c< A by A37,A38,XBOOLE_0:def 8;
A41:  Tarski-Class(X,succ B) c= Tarski-Class(X,A) by A37,Th19;
A42:  Tarski-Class(X,succ B) is epsilon-transitive by A3,A40,ORDINAL1:21;
A43:  Y c= Tarski-Class(X,succ B) by A36,A39,A42,ORDINAL1:def 2;
      thus thesis by A41,A43,XBOOLE_1:1;
    end;
    thus thesis by A6,A32;
  end;
  thus for A holds OnP[A] from ORDINAL1:sch 2(A2);
end;

theorem
  Tarski-Class(X,{}) in Tarski-Class(X,1) &
  Tarski-Class(X,{}) <> Tarski-Class(X,1)
proof
A1: Tarski-Class(X,{}) = { X } by Lm1;
A2: X in Tarski-Class(X,{}) by A1,TARSKI:def 1;
A3: bool X in Tarski-Class X by A2,Th7;
A4: { X } c= bool X
  proof
    let a;
    assume
A5: a in { X };
A6: a = X by A5,TARSKI:def 1;
    thus thesis by A6,ZFMISC_1:def 1;
  end;
A7: 1 = succ 0 & { X } in Tarski-Class X by A3,A4,Th6;
  thus
A8: Tarski-Class(X,{}) in Tarski-Class(X,1) by A1,A7,Th13;
  assume
A9: Tarski-Class(X,{}) = Tarski-Class(X,1);
  thus contradiction by A8,A9;
end;

theorem Th27:
  X is epsilon-transitive implies Tarski-Class X is epsilon-transitive
proof
  consider A such that
A1: Tarski-Class(X,A) = Tarski-Class X by Th22;
A2: Tarski-Class(X,A) c= Tarski-Class(X,succ A) by Th18;
A3: Tarski-Class(X,A) = Tarski-Class(X,succ A) by A1,A2,XBOOLE_0:def 10;
  assume
A4: X is epsilon-transitive;
  thus thesis by A1,A3,A4,Th25;
end;

theorem Th28:
  Y in Tarski-Class X implies card Y in card Tarski-Class X
proof
  assume
A1: Y in Tarski-Class X;
A2: bool Y c= Tarski-Class X
  proof
    let x;
    assume
A3: x in bool Y;
    thus thesis by A1,A3,Th6;
  end;
  A4: card
 Y in card bool Y & card bool Y c= card Tarski-Class X by A2,CARD_1:27
  ,30;
  thus thesis by A4;
end;

theorem Th29:
  Y in Tarski-Class X implies not Y,Tarski-Class X are_equipotent
proof
  assume
A1: Y in Tarski-Class X;
A2: card Y in card Tarski-Class X by A1,Th28;
A3: card Y <> card Tarski-Class X by A2;
  thus thesis by A3,CARD_1:21;
end;

theorem Th30:
  x in Tarski-Class X & y in Tarski-Class X implies
  {x} in Tarski-Class X & {x,y} in Tarski-Class X
proof
  assume that
A1: x in Tarski-Class X and
A2: y in Tarski-Class X;
A3: bool x in Tarski-Class X by A1,Th7;
  thus
A4: {x} in Tarski-Class X by A3,Th6,ZFMISC_1:80;
A5: bool {x} = {{},{x}} by ZFMISC_1:30;
A6: not {{},{x}},Tarski-Class X are_equipotent by A4,A5,Th7,Th29;
A7: now
    assume
A8: x <> y;
A9: {{},{x}},{x,y} are_equipotent
    proof
      defpred C[set] means $1 = {};
      deffunc f(set) = x;
      deffunc g(set) = y;
      consider f such that
A10:  dom f = {{},{x}} & for z st z in {{},{x}} holds
      (C[z] implies f.z = f(z)) & (not C[z] implies f.z = g(z))
      from PARTFUN1:sch 1;
      take f;
      thus f is one-to-one
      proof
        let x1,x2 be set;
        assume that
A11:    x1 in dom f and
A12:    x2 in dom f;
A13:    x2 = {} or x2 = {x} by A10,A12,TARSKI:def 2;
A14:    x1 = {} implies f.x1 = x by A10,A11;
A15:    x1 <> {} implies f.x1 = y by A10,A11;
        thus thesis by A8,A10,A11,A12,A13,A14,A15,TARSKI:def 2;
      end;
      thus dom f = {{},{x}} by A10;
      thus rng f c= {x,y}
      proof
        let z;
        assume
A16:    z in rng f;
A17:    ex u being set st u in dom f & z = f.u by A16,FUNCT_1:def 5;
A18:    z = x or z = y by A10,A17;
        thus thesis by A18,TARSKI:def 2;
      end;
      let z;
      assume
A19:  z in {x,y};
A20:  z = x or z = y by A19,TARSKI:def 2;
A21:  {} in dom f by A10,TARSKI:def 2;
A22:  {x} in dom f by A10,TARSKI:def 2;
A23:  f.{} = x by A10,A21;
A24:  f.{x} = y by A10,A22;
      thus thesis by A20,A21,A22,A23,A24,FUNCT_1:def 5;
    end;
A25: not {x,y},Tarski-Class X are_equipotent by A6,A9,WELLORD2:22;
A26: {x,y} c= Tarski-Class X by A1,A2,ZFMISC_1:38;
    thus thesis by A25,A26,Th8;
  end;
  thus thesis by A4,A7,ENUMSET1:69;
end;

theorem Th31:
  x in Tarski-Class X & y in Tarski-Class X implies [x,y] in Tarski-Class X
proof
  assume
A1: x in Tarski-Class X & y in Tarski-Class X;
  A2: {x,y} in Tarski-Class X & {x} in Tarski-Class X by A1,Th30;
  thus thesis by A2,Th30;
end;

theorem
  Y c= Tarski-Class X & Z c= Tarski-Class X implies [:Y,Z:] c= Tarski-Class X
proof
  assume
A1: Y c= Tarski-Class X & Z c= Tarski-Class X;
  let x,y;
  assume [x,y] in [:Y,Z:];
  then x in Y & y in Z by ZFMISC_1:106;
  hence thesis by A1,Th31;
end;

definition
  let A;
  func Rank(A) means
  :Def6:
  ex L st it = last L & dom L = succ A & L.{} = {} &
  (for C st succ C in succ A holds L.succ C = bool(L.C)) &
  for C st C in succ A & C <> {} & C is limit_ordinal
  holds L.C = union rng(L|C);
  correctness
  proof
    deffunc C(Ordinal,set) = bool $2;
    deffunc D(Ordinal,T-Sequence) = union rng $2;
    thus (ex x,L st x = last L & dom L = succ A & L.{} = {} &
    (for C st succ C in succ A holds L.succ C = C(C,L.C)) &
    for C st C in succ A & C <> {} & C is limit_ordinal
    holds L.C = D(C,L|C) ) & for x1,x2 being set st
    (ex L st x1 = last L & dom L = succ A & L.{} = {} &
    (for C st succ C in succ A holds L.succ C = C(C,L.C)) &
    for C st C in succ A & C <> {} & C is limit_ordinal
    holds L.C = D(C,L|C) ) &
    (ex L st x2 = last L & dom L = succ A & L.{} = {} &
    (for C st succ C in succ A holds L.succ C = C(C,L.C)) &
    for C st C in succ A & C <> {} & C is limit_ordinal holds L.C = D(C,L|C) )
    holds x1 = x2 from ORDINAL2:sch 7;
  end;
end;
deffunc F(Ordinal) = Rank $1;

Lm2: now
  deffunc C(Ordinal,set) = bool $2;
  deffunc D(Ordinal,T-Sequence) = union rng $2;
A1: for A,x holds x = F(A) iff
  ex L st x = last L & dom L = succ A & L.{} = {} &
  (for C st succ C in succ A holds L.succ C = C(C,L.C)) &
  for C st C in succ A & C <> {} & C is limit_ordinal
  holds L.C = D(C,L|C) by Def6;
  thus F({}) = {} from ORDINAL2:sch 8(A1);
  thus for A holds F(succ A) = C(A,F(A)) from ORDINAL2:sch 9(A1);
  thus A <> {} & A is limit_ordinal & dom L = A &
  (for B st B in A holds L.B = Rank B) implies Rank A = union rng L
  proof
    assume that
A2: A <> {} & A is limit_ordinal and
A3: dom L = A and
A4: for B st B in A holds L.B = F(B);
    thus F(A) = D(A,L) from ORDINAL2:sch 10(A1,A2,A3,A4);
  end;
end;

theorem
  Rank {} = {} by Lm2;

theorem
  Rank succ A = bool Rank A by Lm2;

theorem Th35:
  A <> {} & A is limit_ordinal implies
  for x holds x in Rank A iff ex B st B in A & x in Rank B
proof
  assume
A1: A <> {} & A is limit_ordinal;
  consider L such that
A2: dom L = A & for B st B in A holds L.B = F(B) from ORDINAL2:sch 2;
A3: Rank A = union rng L by A1,A2,Lm2;
  let x;
  thus x in Rank A implies ex B st B in A & x in Rank B
  proof
    assume
A4: x in Rank A;
    consider Y such that
A5: x in Y and
A6: Y in rng L by A3,A4,TARSKI:def 4;
    consider y such that
A7: y in dom L and
A8: Y = L.y by A6,FUNCT_1:def 5;
    reconsider y as Ordinal by A7;
    take y;
    thus thesis by A2,A5,A7,A8;
  end;
  given B such that
A9: B in A and
A10: x in Rank B;
A11: L.B = Rank B by A2,A9;
A12: Rank B in rng L by A2,A9,A11,FUNCT_1:def 5;
  thus thesis by A3,A10,A12,TARSKI:def 4;
end;

theorem Th36:
  X c= Rank A iff X in Rank succ A
proof
  thus X c= Rank A implies X in Rank succ A
  proof
    assume
A1: X c= Rank A;
A2: X in bool Rank A by A1;
    thus thesis by A2,Lm2;
  end;
  assume
A3: X in Rank succ A;
A4: X in bool Rank A by A3,Lm2;
  thus thesis by A4;
end;

registration
