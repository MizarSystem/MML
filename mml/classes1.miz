  assume
A1: x in union Rank A;
  consider X such that
A2: x in X and
A3: X in Rank A by A1,TARSKI:def 4;
A4: X c= Rank A by A3,ORDINAL1:def 2;
  thus thesis by A2,A4;
end;

theorem
  X in Rank A implies union X in Rank A
proof
  assume
A1: X in Rank A;
A2: now
    given B such that
A3: A = succ B;
A4: X c= Rank B by A1,A3,Th36;
A5: union X c= union Rank B by A4,ZFMISC_1:95;
A6: union Rank B c= Rank B by Th40;
A7: union X c= Rank B by A5,A6,XBOOLE_1:1;
    thus thesis by A3,A7,Th36;
  end;
A8: now
    assume that
A9: A <> {} and
A10: for B holds A <> succ B;
A11: A is limit_ordinal by A10,ORDINAL1:42;
    consider B such that
A12: B in A and
A13: X in Rank B by A1,A9,A11,Th35;
A14: X c= Rank B by A13,ORDINAL1:def 2;
A15: union X c= union Rank B by A14,ZFMISC_1:95;
A16: union Rank B c= Rank B by Th40;
A17: union X c= Rank B by A15,A16,XBOOLE_1:1;
A18: succ B c= A by A12,ORDINAL1:33;
A19: succ B <> A by A10;
A20: succ B c< A by A18,A19,XBOOLE_0:def 8;
A21: union X in Rank succ B by A17,Th36;
A22: succ B in A by A20,ORDINAL1:21;
    thus thesis by A11,A21,A22,Th35;
  end;
  thus thesis by A1,A2,A8,Lm2;
end;

theorem Th42:
  A in B iff Rank A in Rank B
proof
  defpred OnP[Ordinal,Ordinal] means $1 in $2 implies Rank $1 in Rank $2;
A1: now
    let A;
    defpred P[Ordinal] means OnP[A,$1];
A2: for B st for C st C in B holds P[C] holds P[B]
    proof
      let B such that
A3:   for C st C in B holds OnP[A,C] and
A4:   A in B;
A5:   now
        given C such that
A6:     B = succ C;
A7:     A in C implies Rank A in Rank C by A3,A6,ORDINAL1:10;
A8:     now
          assume
A9:       not A in C;
A10:      A c= C & A <> C iff A c< C by XBOOLE_0:def 8;
          thus Rank A = Rank C by A4,A6,A9,A10,ORDINAL1:21,34;
        end;
A11:    Rank A c= Rank C by A7,A8,ORDINAL1:def 2;
        thus thesis by A6,A11,Th36;
      end;
A12:  now
        assume
A13:    for C holds B <> succ C;
A14:    B is limit_ordinal by A13,ORDINAL1:42;
A15:    B <> succ A by A13;
A16:    succ A c= B by A4,ORDINAL1:33;
A17:    succ A c< B by A15,A16,XBOOLE_0:def 8;
A18:    succ A in B by A17,ORDINAL1:21;
A19:    Rank A in Rank succ A by Th36;
        thus thesis by A14,A18,A19,Th35;
      end;
      thus thesis by A5,A12;
    end;
    thus for B holds P[B] from ORDINAL1:sch 2(A2);
  end;
  thus OnP[A,B] by A1;
  assume that
A20: Rank A in Rank B and
A21: not A in B;
A22: B in A or B = A by A21,ORDINAL1:24;
  thus contradiction by A1,A20,A22;
end;

theorem Th43:
  A c= B iff Rank A c= Rank B
proof
  thus A c= B implies Rank A c= Rank B
  proof
A1: A c< B iff A c= B & A <> B by XBOOLE_0:def 8;
    assume
A2: A c= B;
A3: A = B or A in B by A1,A2,ORDINAL1:21;
A4: Rank A = Rank B or Rank A in Rank B by A3,Th42;
    thus thesis by A4,ORDINAL1:def 2;
  end;
  assume that
A5: Rank A c= Rank B and
A6: not A c= B;
A7: B in A by A6,ORDINAL1:26;
A8: Rank B in Rank A by A7,Th42;
  thus contradiction by A5,A8,ORDINAL1:7;
end;

theorem Th44:
  A c= Rank A
proof
  defpred P[Ordinal] means $1 c= Rank $1;
A1: P[{}] by XBOOLE_1:2;
A2: P[B] implies P[succ B]
  proof
    assume
A3: B c= Rank B;
A4: B in Rank succ B by A3,Th36;
    A5: B c= Rank succ B & {B} c= Rank succ B by A4,ORDINAL1:def 2,ZFMISC_1:37;
    thus thesis by A5,XBOOLE_1:8;
  end;
A6: for A st A <> {} & A is limit_ordinal & for B st B in A holds P[B]
  holds P[A]
  proof
    let A such that
    A <> {} and
A7: A is limit_ordinal and
A8: for B st B in A holds B c= Rank B;
    let x;
    assume
A9: x in A;
    reconsider B = x as Ordinal by A9;
A10: succ B in A by A7,A9,ORDINAL1:41;
A11: B c= Rank B by A8,A9;
A12: succ B c= A by A10,ORDINAL1:def 2;
A13: B in Rank succ B by A11,Th36;
A14: Rank succ B c= Rank A by A12,Th43;
    thus thesis by A13,A14;
  end;
A15: for B holds P[B] from ORDINAL2:sch 1(A1,A2,A6);
  thus thesis by A15;
end;

theorem
  for A,X st X in Rank A holds
  not X,Rank A are_equipotent & card X in card Rank A
proof
  defpred OnP[Ordinal] means
  for X st X in Rank $1 holds not X,Rank $1 are_equipotent;
A1: for A st for B st B in A holds OnP[B] holds OnP[A]
  proof
    let A such that
A2: for B st B in A holds OnP[B];
    let X;
    assume
A3: X in Rank A;
A4: now
      given B such that
A5:   A = succ B;
A6:   B in A by A5,ORDINAL1:10;
A7:   B c= A by A6,ORDINAL1:def 2;
A8:   Rank succ B = bool Rank B by Lm2;
A9:   not Rank B,Rank A are_equipotent by A5,A8,CARD_1:29;
A10:  Rank B c= Rank A by A7,Th43;
      thus thesis by A3,A5,A8,A9,A10,CARD_1:44;
    end;
A11: now
      assume that
A12:  A <> {} and
A13:  for B holds A <> succ B;
A14:  A is limit_ordinal by A13,ORDINAL1:42;
      consider B such that
A15:  B in A and
A16:  X in Rank B by A3,A12,A14,Th35;
A17:  Rank B in Rank A by A15,Th42;
      A18:  (
 not X,Rank B are_equipotent)& X c= Rank B by A2,A15,A16,ORDINAL1:def 2;
A19:  Rank B c= Rank A by A17,ORDINAL1:def 2;
      thus thesis by A18,A19,CARD_1:44;
    end;
    thus thesis by A3,A4,A11,Lm2;
  end;
A20: for A holds OnP[A] from ORDINAL1:sch 2(A1);
  let A,X;
  assume
A21: X in Rank A;
A22: X c= Rank A by A21,ORDINAL1:def 2;
A23: not X,Rank A are_equipotent by A20,A21;
A24: card X c= card Rank A by A22,CARD_1:27;
A25: card X <> card Rank A by A23,CARD_1:21;
  thus thesis by A20,A21,A24,A25,CARD_1:13;
end;

theorem
  X c= Rank A iff bool X c= Rank succ A
proof
  thus X c= Rank A implies bool X c= Rank succ A
  proof
    assume
A1: X c= Rank A;
    let x;
    assume
A2: x in bool X;
A3: x c= Rank A by A1,A2,XBOOLE_1:1;
A4: Rank succ A = bool Rank A by Lm2;
    thus thesis by A3,A4;
  end;
  assume
A5: bool X c= Rank succ A;
  let x;
  assume
A6: x in X;
A7: { x } c= X by A6,ZFMISC_1:37;
A8: { x } in bool X by A7;
A9: Rank succ A = bool Rank A & x in { x } by Lm2,TARSKI:def 1;
  thus thesis by A5,A8,A9;
end;

theorem Th47:
  X c= Y & Y in Rank A implies X in Rank A
proof
  assume that
A1: X c= Y and
A2: Y in Rank A;
A3: now
    given B such that
A4: A = succ B;
A5: Rank succ B = bool Rank B by Lm2;
A6: X c= Rank B by A1,A2,A4,A5,XBOOLE_1:1;
    thus thesis by A4,A5,A6;
  end;
A7: now
    assume
A8: for B holds A <> succ B;
A9: A is limit_ordinal by A8,ORDINAL1:42;
    consider B such that
A10: B in A and
A11: Y in Rank B by A2,A9,Lm2,Th35;
A12: Y c= Rank B by A11,ORDINAL1:def 2;
A13: X c= Rank B by A1,A12,XBOOLE_1:1;
A14: bool Rank B = Rank succ B by Lm2;
A15: succ B in A by A9,A10,ORDINAL1:41;
    thus thesis by A9,A13,A14,A15,Th35;
  end;
  thus thesis by A3,A7;
end;

theorem Th48:
  X in Rank A iff bool X in Rank succ A
proof
  thus X in Rank A implies bool X in Rank succ A
  proof
    assume
A1: X in Rank A;
A2: bool X c= Rank A
    proof
      let x;
      assume
A3:   x in bool X;
      thus thesis by A1,A3,Th47;
    end;
    thus thesis by A2,Th36;
  end;
  assume
A4: bool X in Rank succ A;
A5: X in bool X & bool X c= Rank A by A4,Th36,ZFMISC_1:def 1;
