theorem Th58:
  the_transitive-closure_of X is epsilon-transitive
proof
  let Y;
  assume
A1: Y in the_transitive-closure_of X;
  consider f,n such that
A2: Y in f.n and
A3: dom f = NAT & f.0 = X and
A4: for k being Nat holds f.(k+1) = union(f.k) by A1,Def7;
A5: f.(n+1) = union(f.n) by A4;
  let x;
  assume
A6: x in Y;
A7: x in union(f.n) by A2,A6,TARSKI:def 4;
  thus thesis by A3,A4,A5,A7,Def7;
end;

theorem Th59:
  X c= the_transitive-closure_of X
proof
  let x such that
A1: x in X;
A2: ex f st dom f = NAT & f.0 = X &
  for n being Nat holds f.(n+1) = f(n,f.n) from NAT_1:sch 11;
  thus thesis by A1,A2,Def7;
end;

theorem Th60:
  X c= Y & Y is epsilon-transitive implies the_transitive-closure_of X c= Y
proof
  assume that
A1: X c= Y and
A2: Y is epsilon-transitive;
  let x;
  assume
A3: x in the_transitive-closure_of X;
  consider f,n such that
A4: x in f.n
  and dom f = NAT and
A5: f.0 = X and
A6: for k being Nat holds f.(k+1) = union(f.k) by A3,Def7;
  defpred P[Element of NAT] means f.$1 c= Y;
A7: P[0] by A1,A5;
A8: for k st P[k] holds P[k+1]
  proof
    let k;
    assume
A9: f.k c= Y;
A10: union (f.k) c= union Y by A9,ZFMISC_1:95;
A11: f.(k+1) = union (f.k) & union Y c= Y by A2,A6,Th54;
    thus thesis by A10,A11,XBOOLE_1:1;
  end;
A12: P[k] from NAT_1:sch 1(A7,A8);
A13: f.n c= Y by A12;
  thus thesis by A4,A13;
end;

theorem Th61:
  (for Z st X c= Z & Z is epsilon-transitive holds Y c= Z) & X c= Y &
  Y is epsilon-transitive implies the_transitive-closure_of X = Y
proof
  assume
A1: for Z st X c= Z & Z is epsilon-transitive holds Y c= Z;
  assume
A2: X c= Y & Y is epsilon-transitive;
  thus the_transitive-closure_of X c= Y by A2,Th60;
A3: the_transitive-closure_of X is epsilon-transitive by Th58;
  thus thesis by A1,A3,Th59;
end;

theorem Th62:
  X is epsilon-transitive implies the_transitive-closure_of X = X
proof
A1: for Z st X c= Z & Z is epsilon-transitive holds X c= Z;
  thus thesis by A1,Th61;
end;

theorem
  the_transitive-closure_of {} = {} by Th62;

theorem
  the_transitive-closure_of A = A by Th62;

theorem Th65:
  X c= Y implies the_transitive-closure_of X c= the_transitive-closure_of Y
proof
  assume
A1: X c= Y;
A2: Y c= the_transitive-closure_of Y by Th59;
A3: X c= the_transitive-closure_of Y by A1,A2,XBOOLE_1:1;
  thus thesis by A3,Th58,Th60;
end;

theorem
  the_transitive-closure_of the_transitive-closure_of X =
  the_transitive-closure_of X by Th58,Th62;

theorem
  the_transitive-closure_of (X \/ Y) =
  the_transitive-closure_of X \/ the_transitive-closure_of Y
proof
  A1: X
 c= the_transitive-closure_of X & Y c= the_transitive-closure_of Y by Th59;
A2: X \/ Y c= the_transitive-closure_of X \/ the_transitive-closure_of Y by
  A1,XBOOLE_1:13;
  A3: the_transitive-closure_of X is epsilon-transitive &
  the_transitive-closure_of Y is epsilon-transitive by Th58;
A4: the_transitive-closure_of (X \/ Y) c= the_transitive-closure_of (
  the_transitive-closure_of X \/ the_transitive-closure_of Y) by A2,Th65;
  thus the_transitive-closure_of (X \/ Y) c=
  the_transitive-closure_of X \/ the_transitive-closure_of Y by A3,A4,Th55,Th62
  ;
  A5: the_transitive-closure_of X c= the_transitive-closure_of (X \/ Y) &
  the_transitive-closure_of Y c= the_transitive-closure_of (X \/ Y) by Th65,
  XBOOLE_1:7;
  thus thesis by A5,XBOOLE_1:8;
end;

theorem
  the_transitive-closure_of (X /\ Y) c=
  the_transitive-closure_of X /\ the_transitive-closure_of Y
proof
  A1: the_transitive-closure_of (X /\ Y) c= the_transitive-closure_of X &
  the_transitive-closure_of (X /\ Y) c= the_transitive-closure_of Y by Th65,
  XBOOLE_1:17;
  thus thesis by A1,XBOOLE_1:19;
end;

theorem Th69:
  ex A st X c= Rank A
proof
  consider A such that
A1: Tarski-Class the_transitive-closure_of X c= Rank A by Th53,Th58;
  take A;
A2: the_transitive-closure_of X in Tarski-Class the_transitive-closure_of X
  by Th5;
A3: X in Tarski-Class the_transitive-closure_of X by A2,Th6,Th59;
  thus thesis by A1,A3,ORDINAL1:def 2;
end;

definition
  let X;
  func the_rank_of X -> Ordinal means
  :Def8:
  X c= Rank it & for B st X c= Rank B holds it c= B;
  existence
  proof
    defpred P[Ordinal] means X c= Rank $1;
A1: ex A st P[A] by Th69;
    thus ex A st P[A] & for B st P[B] holds A c= B from ORDINAL1:sch 1(
    A1);
  end;
  uniqueness
  proof
    let A1,A2 be Ordinal such that
    A2: ( X c= Rank A1 & for B st X c= Rank B holds A1 c= B )&( X c= Rank A2 &
    for B st X c= Rank B holds A2 c= B );
    thus A1 c= A2 & A2 c= A1 by A2;
  end;
end;

canceled;

theorem Th71:
  the_rank_of bool X = succ the_rank_of X
proof
A1: X c= Rank the_rank_of X by Def8;
A2: bool X c= Rank succ the_rank_of X
  proof
    let x;
    assume
A3: x in bool X;
A4: x c= Rank the_rank_of X by A1,A3,XBOOLE_1:1;
A5: bool Rank the_rank_of X = Rank succ the_rank_of X by Lm2;
    thus thesis by A4,A5;
  end;
A6: for A st bool X c= Rank A holds succ the_rank_of X c= A
  proof
    let A such that
A7: bool X c= Rank A;
    defpred P[Ordinal] means X in Rank $1;
A8: X in bool X by ZFMISC_1:def 1;
A9: ex A st P[A] by A7,A8;
    consider B such that
A10: P[B] & for C st P[C] holds B c= C from ORDINAL1:sch 1(A9);
A11: now
      assume
A12:  for C holds B <> succ C;
A13:  B is limit_ordinal by A12,ORDINAL1:42;
A14:  ex C st C in B & X in Rank C by A10,A13,Lm2,Th35;
      thus contradiction by A10,A14,ORDINAL1:7;
    end;
    consider C such that
A15: B = succ C by A11;
A16: X c= Rank C by A10,A15,Th36;
A17: the_rank_of X c= C by A16,Def8;
A18: the_rank_of X in B by A15,A17,ORDINAL1:34;
A19: B c= A by A7,A8,A10;
    thus thesis by A18,A19,ORDINAL1:33;
  end;
  thus thesis by A2,A6,Def8;
end;

theorem
  the_rank_of Rank A = A
proof
A1: for B st Rank A c= Rank B holds A c= B by Th43;
  thus thesis by A1,Def8;
end;

theorem Th73:
  X c= Rank A iff the_rank_of X c= A
proof
  thus X c= Rank A implies the_rank_of X c= A by Def8;
  assume
A1: the_rank_of X c= A;
A2: Rank the_rank_of X c= Rank A by A1,Th43;
A3: X c= Rank the_rank_of X by Def8;
  thus thesis by A2,A3,XBOOLE_1:1;
end;

theorem Th74:
  X in Rank A iff the_rank_of X in A
proof
  thus X in Rank A implies the_rank_of X in A
  proof
    assume
A1: X in Rank A;
A2: bool X in Rank succ A by A1,Th48;
A3: bool X c= Rank A by A2,Th36;
A4: the_rank_of bool X = succ the_rank_of X by Th71;
A5: the_rank_of X in the_rank_of bool X by A4,ORDINAL1:10;
A6: the_rank_of bool X c= A by A3,Def8;
    thus thesis by A5,A6;
  end;
  assume
A7: the_rank_of X in A;
A8: succ the_rank_of X c= A by A7,ORDINAL1:33;
A9: X c= Rank the_rank_of X by Def8;
A10: X in Rank succ the_rank_of X by A9,Th36;
A11: Rank succ the_rank_of X c= Rank A by A8,Th43;
  thus thesis by A10,A11;
end;

theorem
  X c= Y implies the_rank_of X c= the_rank_of Y
proof
  assume
A1: X c= Y;
A2: Y c= Rank the_rank_of Y by Def8;
A3: X c= Rank the_rank_of Y by A1,A2,XBOOLE_1:1;
  thus thesis by A3,Def8;
end;

theorem Th76:
  X in Y implies the_rank_of X in the_rank_of Y
proof
  assume
A1: X in Y;
A2: Y c= Rank the_rank_of Y by Def8;
  thus thesis by A1,A2,Th74;
end;

theorem Th77:
  the_rank_of X c= A iff for Y st Y in X holds the_rank_of Y in A
proof
  set R = the_rank_of X;
A1: X c= Rank R by Def8;
  thus
  the_rank_of X c= A implies for Y st Y in X holds the_rank_of Y in A
  proof
    assume
A2: the_rank_of X c= A;
    let Y;
    assume
A3: Y in X;
A4: Y in Rank R by A1,A3;
A5: Rank R c= Rank A by A2,Th43;
    thus thesis by A4,A5,Th74;
  end;
  assume
A6: for Y st Y in X holds the_rank_of Y in A;
A7: X c= Rank A
  proof
    let x;
    assume
A8: x in X;
A9: the_rank_of x in A by A6,A8;
    thus thesis by A9,Th74;
  end;
  thus thesis by A7,Def8;
end;

theorem Th78:
  A c= the_rank_of X iff for B st B in A ex Y st Y in X & B c= the_rank_of Y
proof
  thus A c= the_rank_of X implies
  for B st B in A ex Y st Y in X & B c= the_rank_of Y
  proof
    assume
A1: A c= the_rank_of X;
    let B;
    assume
A2: B in A;
A3: not the_rank_of X c= B by A1,A2,ORDINAL1:7;
A4: not X c= Rank B by A3,Def8;
A5: X \ Rank B <> {} by A4,XBOOLE_1:37;
    consider x being Element of X \ Rank B;
    take x;
A6: not x in Rank B by A5,XBOOLE_0:def 5;
    thus x in X by A5,XBOOLE_0:def 5;
A7: not the_rank_of x in B by A6,Th74;
    thus thesis by A7,ORDINAL1:26;
  end;
  assume
A8: for B st B in A ex Y st Y in X & B c= the_rank_of Y;
  let x;
  assume
A9: x in A;
  reconsider x as Ordinal by A9;
  consider Y such that
A10: Y in X and
A11: x c= the_rank_of Y by A8,A9;
A12: the_rank_of Y in the_rank_of X by A10,Th76;
  thus thesis by A11,A12,ORDINAL1:22;
end;

theorem
  the_rank_of X = {} iff X = {}
proof
  thus the_rank_of X = {} implies X = {}
  proof
    assume
A1: the_rank_of X = {};
A2: X c= Rank {} by A1,Def8;
    thus thesis by A2,Lm2;
  end;
  assume
A3: X = {};
A4: for Y st Y in X holds the_rank_of Y in {} by A3;
  thus the_rank_of X c= {} by A4,Th77;
  thus {} c= the_rank_of X;
end;

theorem Th80:
  the_rank_of X = succ A implies ex Y st Y in X & the_rank_of Y = A
proof
  assume
A1: the_rank_of X = succ A;
A2: A in succ A by ORDINAL1:10;
  consider Y such that
A3: Y in X and
A4: A c= the_rank_of Y by A1,A2,Th78;
  take Y;
A5: the_rank_of Y in the_rank_of X by A3,Th76;
A6: the_rank_of Y c= A by A1,A5,ORDINAL1:34;
  thus thesis by A3,A4,A6,XBOOLE_0:def 10;
end;

theorem
  the_rank_of A = A
proof
A1: A c= Rank A by Th44;
  thus the_rank_of A c= A by A1,Th73;
  defpred P[Ordinal] means $1 c= the_rank_of $1;
A2: for A st for B st B in A holds P[B] holds P[A]
  proof
    let A such that
A3: for B st B in A holds B c= the_rank_of B;
A4: now
      let B such that
A5:   B in A;
      reconsider Y = B as set;
      take Y;
      thus Y in A & B c= the_rank_of Y by A3,A5;
    end;
    thus thesis by A4,Th78;
  end;
A6: P[B] from ORDINAL1:sch 2(A2);
  thus thesis by A6;
end;

theorem
  the_rank_of Tarski-Class X <> {} &
  the_rank_of Tarski-Class X is limit_ordinal
proof
A1: Tarski-Class X c= Rank the_rank_of Tarski-Class X by Def8;
  thus the_rank_of Tarski-Class X <> {}
  proof
    assume
A2: the_rank_of Tarski-Class X = {};
A3: Tarski-Class X c= {} by A2,Def8,Lm2;
    thus contradiction by A3;
  end;
  assume
A4: not thesis;
  consider A such that
A5: the_rank_of Tarski-Class X = succ A by A4,ORDINAL1:42;
  consider Y such that
A6: Y in Tarski-Class X and
A7: the_rank_of Y = A by A5,Th80;
A8: bool Y in Tarski-Class X by A6,Th7;
A9: the_rank_of bool Y = succ A by A7,Th71;
A10: bool Y c= Rank A by A1,A5,A8,Th36;
A11: succ A c= A by A9,A10,Def8;
A12: A in A by A11,ORDINAL1:33;
  thus contradiction by A12;
end;

begin :: Addenda

:: from ZFREFLE1, 2007.03, A.T.

reserve e,u for set;

scheme NonUniqFuncEx { X() -> set, P[set,set] }:
  ex f being Function st dom f = X() & for e st e in X() holds P[e,f.e]
provided
A1: for e st e in X() ex u st P[e,u]
proof
  per cases;
  suppose
A2: X() = {};
A3: for e st e in X() ex u st u in {} & P[e,u] by A2;
A4: ex f being Function st dom f = X() & rng f c= {} &
    for e st e in X() holds P[e,f.e] from WELLORD2:sch 1(A3);
    thus thesis by A4;
  end;
  suppose
A5: X() <> {};
    reconsider D = X() as non empty set by A5;
    defpred Q[set,Ordinal] means ex u st u in Rank $2 & P[$1,u];
A6: for e being Element of D ex A st Q[e,A]
    proof
      let e be Element of D;
      consider u such that
A7:   P[e,u] by A1;
A8:   u c= Rank the_rank_of u by Def8;
A9:   u in Rank succ the_rank_of u by A8,Th36;
      thus thesis by A7,A9;
    end;
    consider F be Function such that
A10: dom F = D & for d being Element of D ex A st A = F.d & Q[d,A] &
    for B st Q[d,B] holds A c= B from ORDINAL1:sch 6(A6);
A11: for e st e in X() ex u st u in Rank sup rng F & P[e,u]
    proof
      let e;
      assume
A12:  e in X();
      consider A such that
A13:  A = F.e and
A14:  ex u st u in Rank A & P[e,u]
      and for B st ex u st u in Rank B & P[e,u] holds A c= B by A10,A12;
      consider u such that
A15:  u in Rank A & P[e,u] by A14;
      take u;
A16:  A in rng F by A10,A12,A13,FUNCT_1:def 5;
A17:  A in sup rng F by A16,ORDINAL2:27;
A18:  A c= sup rng F by A17,ORDINAL1:def 2;
A19:  Rank A c= Rank sup rng F by A18,Th43;
      thus thesis by A15,A19;
    end;
A20: ex f being Function st dom f = X() & rng f c= Rank sup rng F &
    for e st e in X() holds P[e, f.e] from WELLORD2:sch 1(A11);
    thus thesis by A20;
  end;
end;

:: from RFINSEQ, 2008.10.10, A.T.

definition
  let F,G be Relation;
  pred F,G are_fiberwise_equipotent means
  :Def9:
  for x be set holds card Coim(F,x) = card Coim(G,x);
  reflexivity;
  symmetry;
end;

Lm3: for F be Function, x be set st not x in rng F holds Coim(F,x) = {}
proof
  let F be Function,x;
  assume
A1: not x in rng F;
A2: now
    assume
A3: rng F meets {x};

A4: ex y being set st y in rng F & y in {x} by A3,XBOOLE_0:3;
    thus contradiction by A1,A4,TARSKI:def 1;
  end;
  thus thesis by A2,RELAT_1:173;
end;

theorem Th83:
  for F,G be Function st F,G are_fiberwise_equipotent holds rng F = rng G
proof
  let F,G be Function;
  assume
A1: F,G are_fiberwise_equipotent;
  thus rng F c= rng G
  proof
    let x;
    assume that
A2: x in rng F and
A3: not x in rng G;
A4: card Coim(F,x) = card Coim(G,x) by A1,Def9;
A5: Coim(F,x), Coim(G,x) are_equipotent by A4,CARD_1:21;
A6: ex y st y in dom F & F.y = x by A2,FUNCT_1:def 5;
A7: Coim(G,x) = {} by A3,Lm3;
A8: x in {x} & F"{x} = {} by A5,A7,CARD_1:46,TARSKI:def 1;
    thus contradiction by A6,A8,FUNCT_1:def 13;
  end;
  let x;
  assume that
A9: x in rng G and
A10: not x in rng F;
A11: card Coim(G,x) = card Coim(F,x) by A1,Def9;
A12: G"{x},F"{x} are_equipotent by A11,CARD_1:21;
A13: ex y st y in dom G & G.y = x by A9,FUNCT_1:def 5;
A14: Coim(F,x) = {} by A10,Lm3;
A15: x in {x} & Coim(G,x) = {} by A12,A14,CARD_1:46,TARSKI:def 1;
  thus contradiction by A13,A15,FUNCT_1:def 13;
end;

theorem
  for F,G,H be Function
  st F,G are_fiberwise_equipotent & F,H are_fiberwise_equipotent holds
  G,H are_fiberwise_equipotent
proof
  let F,G,H be Function;
  assume that
A1: F,G are_fiberwise_equipotent and
A2: F,H are_fiberwise_equipotent;
  let x;
  thus card Coim(G,x) = card Coim(F,x) by A1,Def9
    .= card Coim(H,x) by A2,Def9;
end;

theorem Th85:
  for F,G be Function holds F,G are_fiberwise_equipotent iff
  ex H be Function st dom H = dom F & rng H = dom G & H is one-to-one & F = G*H
proof
  let F,G be Function;
  thus F,G are_fiberwise_equipotent implies
  ex H be Function st dom H = dom F & rng H = dom G & H is one-to-one & F = G*H
  proof
    assume
A1: F,G are_fiberwise_equipotent;
    defpred P[set,set] means $2 is Function &
    for f be Function st $2 = f holds dom f = F"{$1} & rng f = G"{$1} &
    f is one-to-one;
A2: for x st x in rng F ex y st P[x,y]
    proof
      let x;
      assume x in rng F;
A3:   card Coim(F,x) = card Coim(G,x) by A1,Def9;
A4:   F"{x},G"{x} are_equipotent by A3,CARD_1:21;
      consider H be Function such that
A5:   H is one-to-one & dom H = F"{x} & rng H = G"{x}
      by A4,WELLORD2:def 4;
      take H;
      thus H is Function;
      let g be Function;
      assume
A6:   g = H;
      thus thesis by A5,A6;
    end;
    consider W be Function such that
A7: dom W = rng F & for x st x in rng F holds P[x,W.x]
    from NonUniqFuncEx(A2);
    defpred Q[set,set] means for H be Function st H = W.(F.$1) holds $2 = H.$1;
    set df = dom F, dg = dom G;
A8: for x st x in df ex y st y in dg & Q[x,y]
    proof
      let x;
      assume
A9:   x in df;
A10:  F.x in rng F by A9,FUNCT_1:def 5;
      reconsider f = W.(F.x) as Function by A7,A10;
A11:  dom f = F"{F.x} & rng f = G"{F.x} by A7,A10;
      take y = f.x;
A12:  F.x in {F.x} by TARSKI:def 1;
A13:  x in F"{F.x} by A9,A12,FUNCT_1:def 13;
A14:  f.x in G"{F.x} by A11,A13,FUNCT_1:def 5;
      thus y in dg by A14,FUNCT_1:def 13;
      let g be Function;
      assume
A15:  g = W.(F.x);
      thus thesis by A15;
    end;
    consider IT be Function such that
A16: dom IT = df & rng IT c= dg & for x st x in df holds Q[x,IT.x]
    from WELLORD2:sch 1(A8);
    take IT;
    thus dom IT = df by A16;
A17: dom G c= rng IT
    proof
      let x;
      assume
A18:  x in dg;
A19:  G.x in rng G by A18,FUNCT_1:def 5;
A20:  rng F = rng G by A1,Th83;
      reconsider f = W.(G.x) as Function by A7,A19,A20;
A21:  dom f = F"{G.x} by A7,A19,A20;
A22:  rng f = G"{G.x} by A7,A19,A20;
A23:  G.x in {G.x} by TARSKI:def 1;
A24:  x in rng f by A18,A22,A23,FUNCT_1:def 13;
      consider z be set such that
A25:  z in dom f and
A26:  f.z = x by A24,FUNCT_1:def 5;
A27:  z in df by A21,A25,FUNCT_1:def 13;
A28:  F.z in {G.x} by A21,A25,FUNCT_1:def 13;
A29:  F.z = G.x by A28,TARSKI:def 1;
A30:  IT.z = x by A16,A26,A27,A29;
      thus thesis by A16,A27,A30,FUNCT_1:def 5;
    end;
    thus rng IT = dg by A16,A17,XBOOLE_0:def 10;
A31: now
      let x,y;
      assume that
A32:  x in dom IT and
A33:  y in dom IT and
A34:  IT.x = IT.y;
A35:  F.x in rng F by A16,A32,FUNCT_1:def 5;
A36:  F.y in rng F by A16,A33,FUNCT_1:def 5;
      reconsider f1 = W.(F.x), f2 = W.(F.y) as Function by A7,A35,A36;
A37:  IT.x = f1.x & IT.y = f2.y by A16,A32,A33;
A38:  dom f1 = F"{F.x} by A7,A35;
A39:  rng f1 = G"{F.x} by A7,A35;
A40:  f1 is one-to-one by A7,A35;
A41:  dom f2 = F"{F.y} by A7,A36;
A42:  rng f2 = G"{F.y} by A7,A36;
A43:  F.x in {F.x} by TARSKI:def 1;
A44:  F.y in {F.y} by TARSKI:def 1;
A45:  x in F"{F.x} by A16,A32,A43,FUNCT_1:def 13;
A46:  y in F"{F.y} by A16,A33,A44,FUNCT_1:def 13;
A47:  f1.x in rng f1 by A38,A45,FUNCT_1:def 5;
A48:  f2.y in rng f2 by A41,A46,FUNCT_1:def 5;
A49:  f1.x in G"{F.x} /\ G"{F.y} by A34,A37,A39,A42,A47,A48,XBOOLE_0:def 4;
A50:  f1.x in G"({F.x} /\ {F.y}) by A49,FUNCT_1:137;
A51:  G.(f1.x) in {F.x} /\ {F.y} by A50,FUNCT_1:def 13;
A52:  G.(f1.x) in {F.x} by A51,XBOOLE_0:def 4;
A53:  G.(f1.x) in {F.y} by A51,XBOOLE_0:def 4;
A54:  G.(f1.x) = F.x by A52,TARSKI:def 1;
A55:  G.(f1.x) = F.y by A53,TARSKI:def 1;
      thus x = y by A34,A37,A38,A40,A45,A46,A54,A55,FUNCT_1:def 8;
    end;
    thus IT is one-to-one by A31,FUNCT_1:def 8;
A56: dom(G*IT) = df by A16,RELAT_1:46;
A57: now
      let x;
      assume
A58:  x in df;
A59:  F.x in rng F by A58,FUNCT_1:def 5;
      reconsider f = W.(F.x) as Function by A7,A59;
A60:  dom f = F"{F.x} & rng f = G"{F.x} by A7,A59;
A61:  F.x in {F.x} by TARSKI:def 1;
A62:  x in F"{F.x} by A58,A61,FUNCT_1:def 13;
A63:  f.x in G"{F.x} by A60,A62,FUNCT_1:def 5;
A64:  G.(f.x) in {F.x} by A63,FUNCT_1:def 13;
A65:  G.(f.x) = F.x by A64,TARSKI:def 1;
A66:  IT.x = f.x by A16,A58;
      thus F.x = (G*IT).x by A16,A58,A65,A66,FUNCT_1:23;
    end;
    thus thesis by A56,A57,FUNCT_1:9;
  end;
  given H be Function such that
A67: dom H = dom F and
A68: rng H = dom G and
A69: H is one-to-one and
A70: F = G*H;
  let x;
  set t = H|(F"{x});
A71: t is one-to-one by A69,FUNCT_1:84;
A72: dom t = F"{x} by A67,RELAT_1:91,167;
A73: rng t = G"{x}
  proof
    thus rng t c= G"{x}
    proof
      let z be set;
      assume
A74:  z in rng t;
      consider y such that
A75:  y in dom t and
A76:  t.y = z by A74,FUNCT_1:def 5;
A77:  F.y in {x} by A72,A75,FUNCT_1:def 13;
A78:  F.y = x by A77,TARSKI:def 1;
A79:  z = H.y by A75,A76,FUNCT_1:70;
A80:  dom t = dom H /\ F"{x} by RELAT_1:90;
A81:  y in dom H by A75,A80,XBOOLE_0:def 4;
A82:  z in dom G by A68,A79,A81,FUNCT_1:def 5;
A83:  x = G.z by A70,A78,A79,A81,FUNCT_1:23;
A84:  G.z in {x} by A83,TARSKI:def 1;
      thus thesis by A82,A84,FUNCT_1:def 13;
    end;
    let z be set;
    assume
A85: z in G"{x};
A86: z in dom G by A85,FUNCT_1:def 13;
A87: G.z in {x} by A85,FUNCT_1:def 13;
A88: G.z = x by A87,TARSKI:def 1;
    consider y such that
A89: y in dom H and
A90: H.y = z by A68,A86,FUNCT_1:def 5;
A91: F.y = x by A70,A88,A89,A90,FUNCT_1:23;
A92: F.y in {x} by A91,TARSKI:def 1;
A93: y in dom t by A67,A72,A89,A92,FUNCT_1:def 13;
A94: t.y in rng t by A93,FUNCT_1:def 5;
    thus thesis by A90,A93,A94,FUNCT_1:70;
  end;
A95: F"{x},G"{x} are_equipotent by A71,A72,A73,WELLORD2:def 4;
  thus thesis by A95,CARD_1:21;
end;

theorem Th86:
  for F,G be Function holds
  F,G are_fiberwise_equipotent iff for X be set holds card (F"X) = card (G"X)
proof
  let F,G be Function;
  thus F,G are_fiberwise_equipotent implies
  for X be set holds card(F"X) = card(G"X)
  proof
    assume
A1: F,G are_fiberwise_equipotent;
    consider H be Function such that
A2: dom H = dom F and
A3: rng H = dom G and
A4: H is one-to-one and
A5: F = G*H by A1,Th85;
    let X be set;
    set t = H|(F"X);
A6: t is one-to-one by A4,FUNCT_1:84;
A7: dom t = F"X by A2,RELAT_1:91,167;
A8: rng t = G"X
    proof
      thus rng t c= G"X
      proof
        let z be set;
        assume
A9:     z in rng t;
        consider y such that
A10:    y in dom t and
A11:    t.y = z by A9,FUNCT_1:def 5;
A12:    F.y in X by A7,A10,FUNCT_1:def 13;
A13:    z = H.y by A10,A11,FUNCT_1:70;
A14:    dom t = dom H /\ F"X by RELAT_1:90;
A15:    y in dom H by A10,A14,XBOOLE_0:def 4;
A16:    z in dom G by A3,A13,A15,FUNCT_1:def 5;
A17:    G.z in X by A5,A12,A13,A15,FUNCT_1:23;
        thus thesis by A16,A17,FUNCT_1:def 13;
      end;
      let z be set;
      assume
A18:  z in G"X;
A19:  z in dom G by A18,FUNCT_1:def 13;
A20:  G.z in X by A18,FUNCT_1:def 13;
      consider y such that
A21:  y in dom H and
A22:  H.y = z by A3,A19,FUNCT_1:def 5;
A23:  F.y in X by A5,A20,A21,A22,FUNCT_1:23;
A24:  y in dom t by A2,A7,A21,A23,FUNCT_1:def 13;
A25:  t.y in rng t by A24,FUNCT_1:def 5;
      thus thesis by A22,A24,A25,FUNCT_1:70;
    end;
A26: F"X,G"X are_equipotent by A6,A7,A8,WELLORD2:def 4;
    thus thesis by A26,CARD_1:21;
  end;
  assume
A27: for X be set holds card(F"X) = card(G"X);
  thus for x holds card Coim(F,x) = card Coim(G,x) by A27;
end;

theorem
  for D be non empty set, F,G be Function st rng F c= D & rng G c= D &
  for d be Element of D holds card Coim(F,d) = card Coim(G,d)
  holds F,G are_fiberwise_equipotent
proof
  let D be non empty set, F,G be Function;
  assume that
A1: rng F c= D and
A2: rng G c= D;
  assume
A3: for d be Element of D holds card Coim(F,d) = card Coim(G,d);
  let x;
  per cases;
  suppose
A4: not x in rng F;
A5: Coim(F,x) = {} by A4,Lm3;
A6: now
      assume
A7:   x in rng G;
      reconsider d = x as Element of D by A2,A7;
A8:   card Coim(G,d) = card Coim(F,d) by A3;
A9:   G"{d},F"{d} are_equipotent by A8,CARD_1:21;
A10:  G"{x} = {} by A5,A9,CARD_1:46;
      consider y such that
A11:  y in dom G and
A12:  G.y = x by A7,FUNCT_1:def 5;
A13:  G.y in {x} by A12,TARSKI:def 1;
      thus contradiction by A10,A11,A13,FUNCT_1:def 13;
    end;
    thus thesis by A5,A6,Lm3;
  end;
  suppose
A14: x in rng F;
    reconsider d = x as Element of D by A1,A14;
A15: card Coim(F,d) = card Coim(G,d) by A3;
    thus thesis by A15;
  end;
end;

theorem
  for F,G be Function st dom F = dom G holds
  F,G are_fiberwise_equipotent iff ex P be Permutation of dom F st F = G*P
proof
  let F,G be Function;
  assume
A1: dom F = dom G;
  thus F,G are_fiberwise_equipotent implies
  ex P be Permutation of dom F st F = G*P
  proof
    assume
A2: F,G are_fiberwise_equipotent;
    consider I be Function such that
A3: dom I = dom F and
A4: rng I = dom G and
A5: I is one-to-one and
A6: F = G*I by A2,Th85;
    reconsider I as Function of dom F,dom F by A1,A3,A4,FUNCT_2:4;
    reconsider I as Permutation of dom F by A1,A4,A5,FUNCT_2:83;
    take I;
    thus thesis by A6;
  end;
  given P be Permutation of dom F such that
A7: F = G*P;
A8: P is Function of dom F,dom F & dom F = {} implies dom F ={};
A9: rng P = dom F & dom P = dom F by A8,FUNCT_2:def 1,def 3;
  thus thesis by A1,A7,A9,Th85;
end;

theorem
  for F,G be Function
  st F,G are_fiberwise_equipotent holds card dom F = card dom G
proof
  let F,G be Function;
  assume
A1: F,G are_fiberwise_equipotent;
A2: card(F"(rng F)) = card(G"(rng F)) & rng F = rng G by A1,Th83,Th86;
  thus card(dom F) = card(G"(rng G)) by A2,RELAT_1:169
    .= card(dom G) by RELAT_1:169;
end;

:: from CIRCCOMB, 2009.01.26, A.T.

theorem
  for f being set, p being Relation
  for x being set st x in rng p holds the_rank_of x in the_rank_of [p,f]
proof
  let f be set;
  let p be Relation;
  let y be set;
  assume
A1: y in rng p;
  consider x being set such that
A2: [x,y] in p by A1,RELAT_1:def 5;
A3: p in {p,f} by TARSKI:def 2;
A4: {p,f} in {{p,f},{p}} by TARSKI:def 2;
A5: y in {x,y} by TARSKI:def 2;
A6: {x,y} in {{x,y},{x}} by TARSKI:def 2;
A7: the_rank_of y in the_rank_of {x,y} by A5,Th76;
A8: the_rank_of {x,y} in the_rank_of [x,y] by A6,Th76;
A9: the_rank_of p in the_rank_of {p,f} by A3,Th76;
A10: the_rank_of {p,f} in the_rank_of [p,f] by A4,Th76;
A11: the_rank_of y in the_rank_of [x,y] by A7,A8,ORDINAL1:19;
A12: the_rank_of [x,y] in the_rank_of p by A2,Th76;
A13: the_rank_of p in the_rank_of [p,f] by A9,A10,ORDINAL1:19;
A14: the_rank_of y in the_rank_of p by A11,A12,ORDINAL1:19;
  thus thesis by A13,A14,ORDINAL1:19;
end;

