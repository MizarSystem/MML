:: from RFUNCT_2, 2008.09.14, A.T.

theorem
  for F,G be Function, X holds (G|(F.:X))*(F|X) = (G*F)|X
proof
  let F,G be Function,X;
  set Y = dom ((G*F)|X);
  now
    let x;
    thus x in dom ((G|(F.:X))*(F|X)) implies x in Y
    proof
      assume
A1:   x in dom ((G|(F.:X))*(F|X));
      then
A2:   x in dom (F|X) by Th21;
      then
A3:   x in dom F /\ X by RELAT_1:90;
      then
A4:   x in X by XBOOLE_0:def 4;
      (F|X).x in dom (G|(F.:X)) by A1,Th21;
      then F.x in dom (G|(F.:X)) by A2,Th70;
      then F.x in dom G /\ (F.:X) by RELAT_1:90;
      then
A5:   F.x in dom G by XBOOLE_0:def 4;
      x in dom F by A3,XBOOLE_0:def 4;
      then x in dom (G*F) by A5,Th21;
      then x in dom (G*F)/\ X by A4,XBOOLE_0:def 4;
      hence thesis by RELAT_1:90;
    end;
    assume x in Y;
    then
A6: x in dom (G*F) /\ X by RELAT_1:90;
    then
A7: x in dom (G*F) by XBOOLE_0:def 4;
    then
A8: F.x in dom G by Th21;
A9: x in X by A6,XBOOLE_0:def 4;
    x in dom F by A7,Th21;
    then x in dom F /\ X by A9,XBOOLE_0:def 4;
    then
A10: x in dom (F|X) by RELAT_1:90;
    x in dom F by A7,Th21;
    then F.x in F.:X by A9,Def12;
    then F.x in dom G /\ (F.:X) by A8,XBOOLE_0:def 4;
    then F.x in dom (G|(F.:X)) by RELAT_1:90;
    then (F|X).x in dom (G|(F.:X)) by A10,Th70;
    hence x in dom ((G|(F.:X))*(F|X)) by A10,Th21;
  end;
  then
A11: Y = dom ((G|(F.:X))*(F|X)) by TARSKI:2;
  now
    let x;
    assume
A12: x in Y;
    then
A13: x in dom (G*F) /\ X by RELAT_1:90;
    then x in dom (G*F) by XBOOLE_0:def 4;
    then
A14: x in dom F by Th21;
A15: x in X by A13,XBOOLE_0:def 4;
    then
A16: F.x in F.:X by A14,Def12;
    thus ((G|(F.:X))*(F|X)).x =(G|(F.:X)).((F|X).x) by A11,A12,Th22
      .= (G|(F.:X)).(F.x) by A15,Th72
      .= G.(F.x) by A16,Th72
      .= (G*F).x by A14,Th23
      .= ((G*F)|X).x by A13,Th71;
  end;
  hence thesis by A11,Th9;
end;

theorem
  for F,G be Function, X,X1 holds (G|X1)*(F|X) = (G*F)|(X /\ (F"X1))
proof
  let F,G be Function,X,X1;
  set Y = dom ((G|X1)*(F|X));
  now
    let x;
    thus x in dom ((G*F)|(X /\ (F"X1))) implies x in Y
    proof
      assume x in dom ((G*F)|(X /\ (F"X1)));
      then
A1:   x in dom (G*F) /\ (X /\ (F"X1)) by RELAT_1:90;
      then
A2:   x in dom (G*F) by XBOOLE_0:def 4;
A3:   x in X /\ (F"X1) by A1,XBOOLE_0:def 4;
      then
A4:   x in X by XBOOLE_0:def 4;
      x in dom F by A2,Th21;
      then x in dom F /\ X by A4,XBOOLE_0:def 4;
      then
A5:   x in dom (F|X) by RELAT_1:90;
      x in (F"X1) by A3,XBOOLE_0:def 4;
      then
A6:   F.x in X1 by Def13;
      F.x in dom G by A2,Th21;
      then F.x in dom G /\ X1 by A6,XBOOLE_0:def 4;
      then F.x in dom (G|X1) by RELAT_1:90;
      then (F|X).x in dom (G|X1) by A5,Th70;
      hence thesis by A5,Th21;
    end;
    assume
A7: x in Y;
    then
A8: x in dom(F|X) by Th21;
    then
A9: x in dom F /\ X by RELAT_1:90;
    then
A10: x in dom F by XBOOLE_0:def 4;
A11: x in X by A9,XBOOLE_0:def 4;
    (F|X).x in dom(G|X1) by A7,Th21;
    then F.x in dom (G|X1) by A8,Th70;
    then
A12: F.x in dom G /\ X1 by RELAT_1:90;
    then F.x in X1 by XBOOLE_0:def 4;
    then x in F"X1 by A10,Def13;
    then
A13: x in X /\ F"X1 by A11,XBOOLE_0:def 4;
    F.x in dom G by A12,XBOOLE_0:def 4;
    then x in dom (G*F) by A10,Th21;
    then x in dom (G*F) /\ (X/\(F"X1)) by A13,XBOOLE_0:def 4;
    hence x in dom ((G*F)|(X /\ (F"X1))) by RELAT_1:90;
  end;
  then
A14: Y = dom ((G*F)|(X /\ (F"X1))) by TARSKI:2;
  now
    let x;
    assume
A15: x in Y;
    then
A16: x in dom (F|X) by Th21;
    then
A17: x in dom F /\ X by RELAT_1:90;
    then
A18: x in dom F by XBOOLE_0:def 4;
A19: (F|X).x in dom (G|X1) by A15,Th21;
    then
A20: F.x in dom (G|X1) by A16,Th70;
A21: x in X by A17,XBOOLE_0:def 4;
    F.x in dom (G|X1) by A16,A19,Th70;
    then F.x in dom G /\ X1 by RELAT_1:90;
    then F.x in X1 by XBOOLE_0:def 4;
    then x in F"X1 by A18,Def13;
    then
A22: x in X /\ F"X1 by A21,XBOOLE_0:def 4;
    thus ((G|X1)*(F|X)).x =(G|X1).((F|X).x) by A15,Th22
      .= (G|X1).(F.x) by A16,Th70
      .= G.(F.x) by A20,Th70
      .= (G*F).x by A18,Th23
      .= ((G*F)|(X/\(F"X1))).x by A22,Th72;
  end;
  hence thesis by A14,Th9;
end;

theorem
  for F,G be Function,X holds X c= dom (G*F) iff X c= dom F & F.:X c= dom G
proof
  let F,G be Function,X;
  thus X c= dom (G*F) implies X c= dom F & F.:X c= dom G
  proof
    assume
A1: X c= dom (G*F);
    then for x st x in X holds x in dom F by Th21;
    hence X c= dom F by TARSKI:def 3;
    let x;
    assume x in F.:X;
    then ex y st y in dom F & y in X & x=F.y by Def12;
    hence thesis by A1,Th21;
  end;
  assume that
A2: X c= dom F and
A3: F.:X c= dom G;
  let x;
  assume
A4: x in X;
  then F.x in F.:X by A2,Def12;
  hence thesis by A2,A3,A4,Th21;
end;
 
