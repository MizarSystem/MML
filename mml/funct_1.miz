  y in Y by A1,XBOOLE_0:def 4;
  then x in f"Y by A2,A4,Def13;
  then x in X /\ f"Y by A3,XBOOLE_0:def 4;
  hence thesis by A2,A4,Def12;
end;

theorem
  X /\ R"Y c= R"(R.:X /\ Y)
proof
  let x;
  assume
A1: x in X /\ R"Y;
  then x in R"Y by XBOOLE_0:def 4;
  then consider Rx being set such that
A2: [x,Rx] in R and
A3: Rx in Y by RELAT_1:def 14;
  x in X by A1,XBOOLE_0:def 4;
  then Rx in R.:X by A2,RELAT_1:def 13;
  then Rx in R.:X /\ Y by A3,XBOOLE_0:def 4;
  hence thesis by A2,RELAT_1:def 14;
end;

theorem Th152:
  f is one-to-one implies f"(f.:X) c= X
proof
  assume
A1: f is one-to-one;
  let x;
  assume
A2: x in f"(f.:X);
  then f.x in f.:X by Def13;
  then
A3: ex z st z in dom f & z in X & f.x = f.z by Def12;
  x in dom f by A2,Def13;
  hence thesis by A1,A3,Def8;
end;

theorem
  (for X holds f"(f.:X) c= X) implies f is one-to-one
proof
  assume
A1: for X holds f"(f.:X) c= X;
  given x1,x2 such that
A2: x1 in dom f and
A3: x2 in dom f and
A4: f.x1 = f.x2 & x1 <> x2;
A5: f"(f.:{x1}) c= {x1} by A1;
A6: Im(f,x2) = {f.x2} by A3,Th117;
A7: Im(f,x1) = {f.x1} by A2,Th117;
  f.x1 in rng f by A2,Def5;
  then f"(f.:{x1}) <> {} by A7,Th142;
  then f"(f.:{x1}) = {x1} by A5,ZFMISC_1:39;
  hence contradiction by A1,A4,A7,A6,ZFMISC_1:6;
end;

theorem
  f is one-to-one implies f.:X = (f")"X
proof
  assume
A1: f is one-to-one;
  y in f.:X iff y in (f")"X
  proof
    thus y in f.:X implies y in (f")"X
    proof
      assume y in f.:X;
      then consider x such that
A2:   x in dom f and
A3:   x in X and
A4:   y = f.x by Def12;
      y in rng f by A2,A4,Def5;
      then
A5:   y in dom(f") by A1,Th54;
      (f").(f.x) = x by A1,A2,Th54;
      hence thesis by A3,A4,A5,Def13;
    end;
    assume
A6: y in (f")"X;
    then
A7: (f").y in X by Def13;
    y in dom(f") by A6,Def13;
    then y in rng(f) by A1,Th54;
    then consider x such that
A8: x in dom(f) & y = f.x by Def5;
    (f").y = x by A1,A8,Th56;
    hence thesis by A7,A8,Def12;
  end;
  hence thesis by TARSKI:2;
end;

theorem
  f is one-to-one implies f"Y = (f").:Y
proof
  assume
A1: f is one-to-one;
  x in f"Y iff x in (f").:Y
  proof
    thus x in f"Y implies x in (f").:Y
    proof
      assume
A2:   x in f"Y;
      then
A3:   f.x in Y by Def13;
A4:   x in dom f by A2,Def13;
      then f.x in rng(f) by Def5;
      then
A5:   f.x in dom(f") by A1,Th54;
      (f").(f.x) = x by A1,A4,Th54;
      hence thesis by A3,A5,Def12;
    end;
    assume x in (f").:Y;
    then consider y such that
A6: y in dom(f") and
A7: y in Y and
A8: x = (f").y by Def12;
    dom(f") = rng f by A1,Th54;
    then y = f.x & x in dom f by A1,A6,A8,Th54;
    hence thesis by A7,Def13;
  end;
  hence thesis by TARSKI:2;
end;
 
