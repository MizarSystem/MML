theorem
  f is g-compatible & dom f = dom g implies g is non-empty proof assume
Z: f is g-compatible & dom f = dom g;
  let x;
  assume x in dom g;
   then f.x in g.x by Z,Def20;
  hence g.x is non empty;
 end;

