then A3: Effect(f) = VERUM by Def6;
 1 <= n & n <= len f implies f,n is_a_correct_step_wrt X
  proof
    assume 1 <= n & n <= len f;
then A4: n=1 by A1,XXREAL_0:1;
A5: (f.1)`2 = 1 by A2,MCART_1:7;
 (f.n)`1 = VERUM by A2,A4,MCART_1:7;
    hence thesis by A4,A5,Def4;
  end;
then  f is_a_proof_wrt X by Def5;
  hence thesis by A3;
