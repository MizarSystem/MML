theorem Th64:
  f is_a_proof_wrt X & 1 <= l & l <= len f implies (f.l)`1 in Cn(X)
proof
  assume that
A1: f is_a_proof_wrt X and
A2: 1 <= l & l <= len f;
 for n holds 1 <= n & n <= len f implies (f.n)`1 in Cn(X)
  proof
    defpred P[Nat] means 1 <= $1 & $1 <= len f implies (f.$1)`1 in Cn(X);
A3: for n being Nat st for k being Nat st k < n holds P[k] holds P[n]
    proof
      let n be Nat;
      assume
A4:   for k being Nat st k < n holds P[k];
A5:   n in NAT by ORDINAL1:def 13;
      assume that
A6:   1 <= n and
A7:   n <= len f;
A8:   f,n is_a_correct_step_wrt X by A1,A5,A6,A7,Def5;
  now per cases by A6,A7,Th45;
        suppose
      (f.n)`2 = 0;
then A9:      (f.n)`1 in X by A8,Def4;
      X c= Cn(X) by Th38;
          hence thesis by A9;
        end;
        suppose
      (f.n)`2 = 1;
then       (f.n)`1 = VERUM by A8,Def4;
          hence thesis by Th27;
        end;
        suppose
      (f.n)`2 = 2;
then       ex p st (f.n)`1 = ('not' p => p) => p by A8,Def4;
          hence thesis by Th28;
        end;
        suppose
      (f.n)`2 = 3;
then       ex p,q st (f.n)`1 = p => ('not' p => q) by A8,Def4;
          hence thesis by Th29;
        end;
        suppose
      (f.n)`2 = 4;
then       ex p,q,r st (f.n)`1 = (p => q) => ('not'(q '&' r) => 'not'
          (p '&' r)) by A8,Def4;
          hence thesis by Th30;
        end;
        suppose
      (f.n)`2 = 5;
then       ex p,q st (f.n)`1 = p '&' q => q '&' p by A8,Def4;
          hence thesis by Th31;
        end;
        suppose
      (f.n)`2 = 6;
then       ex p,x st (f.n)`1 = All(x,p) => p by A8,Def4;
          hence thesis by Th33;
        end;
        suppose
      (f.n)`2 = 7;
          then consider i,j,p,q such that
A10:      1 <= i and
A11:      i < n and
A12:      1 <= j and
A13:      j < i and
A14:      p = (f.j)`1 & q = (f.n)`1 & (f.i)`1 = p => q by A8,Def4;
A15:      j < n by A11,A13,XXREAL_0:2;
A16:      i <= len f by A7,A11,XXREAL_0:2;
then       j <= len f by A13,XXREAL_0:2;
then A17:      (f.j)`1 in Cn(X) by A4,A12,A15;
      (f.i)`1 in Cn(X) by A4,A10,A11,A16;
          hence thesis by A14,A17,Th32;
        end;
        suppose
      (f.n)`2 = 8;
          then consider i,p,q,x such that
A18:      1 <= i and
A19:      i < n and
          A20:      (
f.i)`1 = p => q & not x in still_not-bound_in p & (f.n)`1 = p => All(x,
          q)
          by A8,Def4;
      i <= len f by A7,A19,XXREAL_0:2;
          hence thesis by A4,A18,A19,A20,Th34;
        end;
        suppose
      (f.n)`2 = 9;
          then consider i,x,y,s such that
A21:      1 <= i and
A22:      i < n and
          A23:      s
.x in CQC-WFF & s.y in CQC-WFF & ( not x in still_not-bound_in s)& s.x =
          (f.i)`1 & (f.n)`1 = s.y
          by A8,Def4;
      i <= len f by A7,A22,XXREAL_0:2;
          hence thesis by A4,A21,A22,A23,Th35;
        end;
      end;
      hence thesis;
    end;
 for n being Nat holds P[n] from NAT_1:sch 4(A3);
    hence thesis;
  end;
  hence thesis by A2;
end;

definition
  let f;
  assume
A1: f <> {};
  func Effect(f) -> Element of CQC-WFF equals
  :Def6:
  (f.(len f))`1;
  coherence
  proof
 0 <= len f by NAT_1:2;
then  0+1 <= len f by A1,NAT_1:13;
then A2: len f in Seg len f by FINSEQ_1:3;
 Seg len f = dom f by FINSEQ_1:def 3;
then A3: f.(len f) in rng f by A2,FUNCT_1:def 5;
 rng f c= [:CQC-WFF,Proof_Step_Kinds:] by FINSEQ_1:def 4;
    hence thesis by A3,MCART_1:10;
  end;
end;

