  set S2 = BitGFA3Str(A1,cin,dm);
  set A2 = GFA3AdderOutput(A1,cin,dm);
  set C2 = GFA3CarryOutput(A1,cin,dm);
  set ambm0 = [<*am,bm*>, xor2 ];
  set ambm = [<*am,bm*>, and2b];
  set bmcm = [<*bm,cm*>, and2b];
  set cmam = [<*cm,am*>, and2b];
