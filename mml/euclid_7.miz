of REAL n,k being Nat st u2=q/.$1 & 1<=k & k< $1 & $1<=len p+1 & p0/.k=s2 holds
        |( s2,u2 )| =0;
        let i be Nat,s be Element of REAL n;
        assume that
A93:    1<=i and
A94:    i<=len p and
A95:    p0/.i=s;
A96:    for k being Nat st Q[k] holds Q[k+1]
