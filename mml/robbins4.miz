    end;
A21: z9 c= 2 by A2,A18,YELLOW_1:3;
A22: now
      assume z9 = 3\1;
      then
A23:  2 in z9 by TARSKI:def 2,YELLOW11:3;
      not 2 in 2;
      hence contradiction by A21,A23;
    end;
A24: now
      assume z9= 3;
      then
A25:  2 in z9 by CARD_1:89,ENUMSET1:def 1;
      not 2 in 2;
      hence contradiction by A21,A25;
    end;
A26: now
      assume z9 = 3\2;
