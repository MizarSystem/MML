    let n be non empty Element of NAT,
    s,t be Element of n-tuples_on BOOLEAN;
    now let j be Nat;
      assume A31: j in Seg n;
      thus (Op-XOR(Op-XOR(s,t),t)).j
      = (Op-XOR(s,t)).j 'xor' t.j by A31,defXOR
