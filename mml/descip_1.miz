      MEMBERED, XBOOLEAN, MARGREL1, INT_1, SUBSET_1, FINSEQ_1, NAT_1, RELAT_1,
      XXREAL_0, XBOOLE_0, FUNCT_1, VALUED_0, CARD_1, FINSEQ_4, BVFUNC_1,
      AFINSQ_2, XTUPLE_0;
 requirements NUMERALS, BOOLE, SUBSET, REAL, ARITHM;
