      XXREAL_0, ORDINAL2, XXREAL_1, CARD_1, INTEGRA1, TARSKI, XREAL_0,
      ORDINAL1, ARYTM_3, ARYTM_1, FUZZY_2, ZFMISC_1, PARTFUN1, VALUED_1, SEQ_4,
      FUZZY_4;
