    hence numerator(p)<=0;
   end;
     now assume A3:numerator(p)<=0;
      now per cases by A3;
     suppose numerator(p)=0;
      hence p<=0 by Th36;
     suppose numerator(p)<0;
      hence p<=0 by Th76;
