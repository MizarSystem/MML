 theorem Th29:
  1<=denominator(p)
   proof 0<denominator(p) by Th27;
   then 0+1<=denominator(p) by NAT_1:38;
   hence thesis;
  end;

 theorem Th30:
  0<denominator(p)"
   proof
      0<denominator(p) by Th27;
    hence thesis by REAL_1:72;
   end;

