    proof
      let l be Element of NAT;
      dom (NAT .-->NAT) = {NAT} by FUNCOP_1:19;
      then
A5:   not l in dom (NAT .-->NAT) by TARSKI:def 1;
      thus (the Object-Kind of STC N).l = ((NAT --> III)+*(NAT .-->NAT)).l
                by Def11
        .= (NAT --> III).l by A5,FUNCT_4:12
