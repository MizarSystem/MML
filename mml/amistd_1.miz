Z:   0 in X* by FINSEQ_1:66;
     0 in NAT* by FINSEQ_1:66;
     then [0,0,0] in [:NAT,NAT*,X*:] & [1,0,0] in [:NAT,NAT*,X*:]
      by Z,MCART_1:73;
   hence the Instructions of STC N c= [:NAT,NAT*,X*:] by A2,ZFMISC_1:38;
