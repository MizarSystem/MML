begin :: The Real Norm Space of l^p Real Sequences

definition
  let x be Real_Sequence;
  let p be Real;
  func x rto_power p -> Real_Sequence means :Def1:
  for n be Nat holds it.n = abs(x.n) to_power p;
     existence
     proof
    deffunc F(set)=( abs(x.$1) ) to_power p;
A1:     ex q1 be  Real_Sequence st
        for n be Nat holds q1.n=F(n) from SEQ_1:sch 1;
        consider q1 be Real_Sequence such that
A2:    for n be Nat holds
        q1.n= abs(x.n) to_power p by A1;
        take IT=q1;
        thus thesis by A2;
     end;
     uniqueness
     proof
      let a1,a2 be Real_Sequence;
      assume that
A3:   for n be Nat holds a1.n = abs(x.n) to_power p and
A4:   for n be Nat holds a2.n = abs(x.n) to_power p;
        for s be set st s in NAT holds a1.s = a2.s
       proof
        let s be set such that
A5:      s in NAT;
        a1.s = abs(x.s) to_power p by A3,A5
              .= a2.s by A4,A5;
        hence thesis;
       end;
       hence thesis by SEQ_1:8;
     end;
     end;

definition
  let p be Real;
  assume A1:p >= 1;
  func the_set_of_RealSequences_l^p
  -> non empty Subset of the carrier of Linear_Space_of_RealSequences means
     :Def2:
     for x being set holds x in it iff
     (x in the_set_of_RealSequences & seq_id(x) rto_power p is summable);
   existence
   proof
   defpred P[set] means seq_id($1) rto_power p is summable;
   consider IT being set such that
A2:for x being set holds x in IT
    iff x in the_set_of_RealSequences & P[x] from XBOOLE_0:sch 1;
A3:IT is non empty
   proof
      seq_id(Zeroseq) rto_power p is summable
      proof
A4:    p > 0 by A1;
         reconsider rseq=seq_id(Zeroseq) rto_power p as Real_Sequence;
         now let n be Nat;
         thus rseq.n =abs((seq_id(Zeroseq)).n) to_power p by Def1
                    .=abs(0) to_power p by RSSPACE:def 6
                    .=0 to_power p by ABSVALUE:7
                    .=0 by A4,POWER:def 2;
       end;
       then rseq is absolutely_summable by COMSEQ_3:3;
       hence thesis by SERIES_1:40;
     end;
     hence thesis by A2;
  end;
IT is Subset of the_set_of_RealSequences
   proof
     for x be set st x in IT holds x in the_set_of_RealSequences by A2;
     hence thesis by TARSKI:def 3;
   end;
   hence thesis by A2,A3,RSSPACE:def 7;
  end;
uniqueness
     proof
   let X1,X2 be non empty Subset of the carrier
   of Linear_Space_of_RealSequences;
   assume that
A5: for x being set holds x in X1 iff
     (x in the_set_of_RealSequences &  seq_id(x) rto_power p is summable)
   and
A6: for x being set holds x in X2 iff
     (x in the_set_of_RealSequences &  seq_id(x) rto_power p is summable);
A7: X1 c= X2
    proof
      let x be set;
      assume x in X1; then
      x in the_set_of_RealSequences &
      (x in the_set_of_RealSequences & seq_id(x) rto_power p is summable)
      by A5;
      hence x in X2 by A6;
   end;
   X2 c= X1
   proof
       let x be set;
        assume x in X2; then
        x in the_set_of_RealSequences & (x in the_set_of_RealSequences
        & seq_id(x) rto_power p is summable) by A6;
      hence x in X1 by A5;
    end;
  hence thesis by A7,XBOOLE_0:def 10;
  end;
end;

 reserve x1,x2,y1,y2,p,q,a,b,c for Real;

Lm1:
x1 >= 0 & y1 > 0 implies x1 to_power y1 >= 0
proof
assume that
A1:x1 >= 0 and
A2:y1 > 0;
   x1>0 or x1=0 by A1;
hence x1 to_power y1 >= 0 by A2,POWER:39,def 2;
end;

Lm2:
y1>0 & x1>=0 & x2>=0 implies
  (x1*x2) to_power y1 = (x1 to_power y1) * (x2 to_power y1)
proof
assume
A1: y1>0 & x1>=0 & x2>=0;
per cases by A1;
   suppose x1>0 & x2>0;
    hence (x1*x2) to_power y1=(x1 to_power y1) * (x2 to_power y1) by POWER:35;
  end;
    suppose A2:x1>0 & x2=0;
    x2 to_power y1 = 0 by A1,A2,POWER:def 2;
      hence (x1*x2) to_power y1=(x1 to_power y1) * (x2 to_power y1) by A2;
    end;
    suppose A3:x1=0 & x2>0;
    x1 to_power y1 = 0 by A1,A3,POWER:def 2;
      hence (x1*x2) to_power y1=(x1 to_power y1) * (x2 to_power y1) by A3;
    end;
    suppose A4: x1=0 & x2=0;
     x2 to_power y1 = 0 by A1,A4,POWER:def 2;
      hence (x1*x2) to_power y1=(x1 to_power y1) * (x2 to_power y1) by A4;
    end;
end;

theorem Th1:
  a >= 0 & a < b & c > 0 implies a to_power c < b to_power c
proof
   a = 0 & c > 0 implies a to_power c = 0 by POWER:def 2;
  hence thesis by POWER:39,42;
end;

Lm3:
 for p be Real st 1 = p
 for a,b be Real_Sequence
 for n be Nat holds
  (Partial_Sums((a + b) rto_power p).n) to_power (1/p) <=
  (Partial_Sums(a rto_power p).n) to_power (1/p) +
  (Partial_Sums(b rto_power p).n) to_power (1/p)
   proof
      let p be Real such that
A1:p=1;
      let a,b be Real_Sequence;
      let n be Nat;
A2: now let n be Nat;
      thus ((a + b) rto_power p).n = abs((a + b).n) to_power p by Def1
      .=abs((a + b).n) by A1,POWER:30;thus
    (a rto_power p).n = abs(a .n) to_power p by Def1
      .=abs(a .n) by A1,POWER:30;thus
    (b rto_power p).n = abs(b .n) to_power p by Def1
      .=abs(b .n) by A1,POWER:30;
      end; then
A3: (a + b) rto_power p = abs(a + b) by SEQ_1:16;
A4: a rto_power p = abs(a) by A2,SEQ_1:16;
A5: b rto_power p = abs(b) by A2,SEQ_1:16;
A6: (Partial_Sums((a + b) rto_power p).n) to_power (1/p)
      =(Partial_Sums(abs(a + b)).n) by A1,A3,POWER:30;
A7: (Partial_Sums(a rto_power p).n) to_power (1/p)
      =(Partial_Sums(abs(a)).n) by A1,A4,POWER:30;
A8: (Partial_Sums(b rto_power p).n) to_power (1/p)
      =(Partial_Sums(abs(b)).n) by A1,A5,POWER:30;
A9:for n be Nat holds abs((a + b).n) <= abs(b .n) + abs(a .n)
   proof
    let n be Nat;
    abs((a + b).n)=abs(a.n + b.n) by SEQ_1:11;
    hence thesis by ABSVALUE:13;
    end;
   deffunc F(Nat) =abs(a .($1)) + abs(b .($1));
   consider c be Real_Sequence such that
A10:for n be Nat holds c.n= F(n) from SEQ_1:sch 1;
A11:now let n be Nat;
A12:abs((a + b).n) <= abs(b .n) + abs(a .n) by A9;
A13:abs((a + b).n)=abs(a+b).n by SEQ_1:16;
   thus abs(a+b).n <= c.n by A10,A12,A13;
 end;
A14:now let n be Nat;
A15: abs(b.n)=abs (b).n by SEQ_1:16;
A16: abs(a.n)=abs (a).n by SEQ_1:16;
    thus c.n=abs (a).n + abs (b).n by A10,A15,A16
        .=(abs a + abs b).n by SEQ_1:11;
     end;
 A17: c = abs a + abs b
       proof
       for n be set st n in NAT holds
       c.n = (abs (a) + abs (b)).n by A14;
   hence c = (abs (a) + abs (b)) by SEQ_1:8;
  end;
 Partial_Sums(abs(a+b)).n<=Partial_Sums(c).n by A11,SERIES_1:17; then
A18: Partial_Sums(abs(a+b)).n <=
      Partial_Sums(abs (a) + abs (b)).n by A17;
      Partial_Sums(abs (a) + abs (b)) =
      Partial_Sums(abs a) + Partial_Sums(abs(b)) by SERIES_1:8;
       hence (Partial_Sums((a + b) rto_power p).n) to_power (1/p)
       <=(Partial_Sums(a rto_power p).n) to_power (1/p)
       + (Partial_Sums(b rto_power p).n) to_power (1/p)
         by A6,A7,A8,A18,SEQ_1:11;
   end;

theorem Th2:
 for p be Real st 1 <= p
 for a,b be Real_Sequence
 for n be Nat holds
  (Partial_Sums((a + b) rto_power p).n) to_power (1/p) <=
  (Partial_Sums(a rto_power p).n) to_power (1/p) +
  (Partial_Sums(b rto_power p).n) to_power (1/p)
   proof
   let p be Real such that
   A1: 1<=p;
   let a,b be Real_Sequence;
   set ap = a rto_power p;
   set bp = b rto_power p;
   set ab= ((a + b) rto_power p);
   let n be Nat;
   now per cases by A1,REAL_1:def 5;
   case
   A2:p > 1;
now let n be Nat;
A3:  (a rto_power p).n =abs(a .n) to_power p by Def1;
A4:  ((a + b) rto_power p).n =abs((a + b).n) to_power p by Def1
      .=abs(a.n+b.n) to_power p by SEQ_1:11;
        thus ap.n = abs(a .n) to_power p by A3;
        thus bp.n = abs(b .n) to_power p by Def1;
        thus ab.n = abs(a.n+b.n) to_power p by A4;
    end;
    hence (Partial_Sums((a + b) rto_power p).n) to_power (1/p) <=
        (Partial_Sums(a rto_power p).n) to_power (1/p) +
        (Partial_Sums(b rto_power p).n) to_power (1/p) by A2,HOLDER_1:7;
    end;
    case p=1;
    hence (Partial_Sums((a + b) rto_power p).n) to_power (1/p) <=
     (Partial_Sums(a rto_power p).n) to_power (1/p) +
     (Partial_Sums(b rto_power p).n) to_power (1/p) by Lm3;
    end;
   end;
 hence thesis;
 end;

Lm4:
for a,b be Real_Sequence
for p be Real st 1 = p &
  ((a rto_power p) is summable) &
  ((b rto_power p) is summable) holds
  ((a+b) rto_power p) is summable &
 (Sum((a + b) rto_power p)) to_power (1/p) <=
 (Sum(a rto_power p)) to_power (1/p) + (Sum(b rto_power p)) to_power (1/p)
proof
 let a,b be Real_Sequence;
 let p be Real such that
A1:p=1 and
A2:a rto_power p is summable and
A3:b rto_power p is summable;
A4: now let n be Nat;thus
       ((a + b) rto_power p).n = abs((a + b).n) to_power p by Def1
      .=abs((a + b).n) by A1,POWER:30;thus
      (a rto_power p).n = abs(a .n) to_power p by Def1
      .=abs(a .n) by A1,POWER:30;
      thus (b rto_power p).n = abs(b .n) to_power p by Def1
      .=abs(b .n) by A1,POWER:30;
      end; then
A5: (a + b) rto_power p = abs(a + b) by SEQ_1:16;
A6: a rto_power p = abs(a) by A4,SEQ_1:16;
A7: b rto_power p = abs(b) by A4,SEQ_1:16;
A8:(Sum((a + b) rto_power p)) to_power (1/p)
      =Sum(abs(a + b)) by A1,A5,POWER:30;
A9:(Sum((a) rto_power p)) to_power (1/p)
      =Sum(abs(a)) by A1,A6,POWER:30;
A10:(Sum((b) rto_power p)) to_power (1/p)
      =Sum(abs(b)) by A1,A7,POWER:30;
A11:abs(a) is summable by A2,A6;
A12:abs(b) is summable by A3,A7;
A13:a is absolutely_summable by A11,SERIES_1:def 5;
A14:b is absolutely_summable by A12,SERIES_1:def 5;
A15:a = seq_id (a) & b = seq_id (b) by RSSPACE:3;
A16:seq_id (a) is absolutely_summable &
      seq_id (b) is absolutely_summable by A13,A14,A15;
      reconsider a1=a as VECTOR of l1_Space by A16,RSSPACE3:8;
      reconsider b1=b as VECTOR of l1_Space by A16,RSSPACE3:8;
A17: ||.a1+b1.|| <= ||.a1.|| + ||.b1.|| by RSSPACE3:9;
A18: ||.a1+b1.|| = (the norm of l1_Space).(a1+b1) by NORMSP_1:def 1
       .=Sum(abs(seq_id(a1+b1))) by RSSPACE3:def 2,def 3
       .=Sum(abs(seq_id(seq_id(a1)+seq_id(b1)))) by RSSPACE3:8
       .=Sum(abs(seq_id(a1)+seq_id(b1))) by RSSPACE:3;
A19: ||.a1.|| = (the norm of l1_Space).(a1) by NORMSP_1:def 1
       .=Sum(abs(seq_id(a1))) by RSSPACE3:def 2,def 3;
A20: ||.b1.|| = (the norm of l1_Space).(b1) by NORMSP_1:def 1
       .=Sum(abs(seq_id(b1))) by RSSPACE3:def 2,def 3;
A21: Sum(abs(seq_id(a1)+seq_id(b1))) <=
       Sum(abs(seq_id(a1)))+ Sum(abs(seq_id(b1))) by A17,A18,A19,A20;
A22: seq_id(a1)=a by RSSPACE:3;
A23: seq_id(b1)=b by RSSPACE:3;
A24: Sum(abs(a+b)) <= Sum(abs(a)) + Sum(abs(b)) by A21,A22,A23;
A25:(abs(a) + abs(b) is summable) &
       Sum(abs(a) + abs(b))=Sum(abs(a))+Sum(abs(b))
     by A11,A12,SERIES_1:10;
    deffunc F(Nat) =abs(a .($1)) + abs(b .($1));
    consider c be Real_Sequence such that
A26:for n be Nat holds c.n= F(n) from SEQ_1:sch 1;
A27:now let n be Nat;
A28:abs(a+b).n=abs((a+b).n) by SEQ_1:16;
    hence abs(a+b).n>= 0 by ABSVALUE:5;
    abs((a+b).n) =abs((a.n)+(b.n)) by SEQ_1:11; then
A29:abs(a+b).n <= abs(a.n)+abs(b.n) by A28,ABSVALUE:13; thus
 c.n=abs(a.n)+abs(b.n) by A26;
A30: abs(a.n)+abs(b.n) = abs(a).n+abs(b.n) by SEQ_1:16
     .=abs(a).n+abs(b).n by SEQ_1:16;thus
     c.n =abs(a).n+abs(b).n by A26,A30;
     thus abs(a+b).n <= c.n by A26,A29;
end;
c = abs(a) + abs(b) by A27,SEQ_1:11;
 hence (a + b) rto_power p is summable by A5,A25,A27,SERIES_1:24;
thus (Sum((a + b) rto_power p)) to_power (1/p)
       <=(Sum((a) rto_power p)) to_power (1/p) +
          (Sum((b) rto_power p)) to_power (1/p) by A8,A9,A10,A24;
end;

theorem Th3:
for a,b be Real_Sequence
for p be Real st 1 <= p &
  (a rto_power p) is summable &
  (b rto_power p) is summable
 holds ((a+b) rto_power p) is summable &
 (Sum((a + b) rto_power p)) to_power (1/p) <=
 (Sum(a rto_power p)) to_power (1/p) +
 (Sum(b rto_power p)) to_power (1/p)
 proof
     let a,b be Real_Sequence;
     let p be Real such that
A1: 1<=p and
A2:((a rto_power p) is summable) and
A3:((b rto_power p) is summable);
    set ap = a rto_power p;
    set bp = b rto_power p;
    set ab= ((a + b) rto_power p);
A4:now let n be Nat;
      A5:  (a rto_power p).n =abs(a .n) to_power p by Def1;
      A6:  (b rto_power p).n =abs(b .n) to_power p by Def1;
      A7:  ((a + b) rto_power p).n =abs((a + b).n) to_power p by Def1
            .=abs(a.n+b.n) to_power p by SEQ_1:11;
            thus ap.n= abs(a .n) to_power p by A5;
            thus bp.n= abs(b .n) to_power p by A6;
            thus ab.n= abs(a.n+b.n) to_power p by A7;
          end;
   now per cases by A1,REAL_1:def 5;
      case
      A8:p > 1;
        thus Sum(ab) to_power (1/p) <= Sum ap to_power (1/p)
         + Sum(bp) to_power (1/p)
         & ab is summable by A2,A3,A4,A8,HOLDER_1:13;
         end;
       case
       A9:p=1;
        thus Sum(ab) to_power (1/p) <= Sum(ap) to_power (1/p)
                 + Sum(bp) to_power (1/p)
         & ab is summable by A2,A3,A9,Lm4;
       end;
      end;
    hence thesis;
 end;

theorem Th4:
for p be Real st 1 <= p holds
the_set_of_RealSequences_l^p is lineary-closed
proof
 let p be Real such that
 A1: p >=1;
 set W = the_set_of_RealSequences_l^p;
  A2:for v,u be VECTOR of Linear_Space_of_RealSequences st
      v in the_set_of_RealSequences_l^p &
      u in the_set_of_RealSequences_l^p
        holds v + u in the_set_of_RealSequences_l^p
 proof
     let v,u be VECTOR of Linear_Space_of_RealSequences such that
A3: v in W & u in W;
A4:  seq_id(v +u) rto_power p is summable
    proof
    A5: seq_id(v) rto_power p is summable by A1,A3,Def2;
    A6: seq_id(u) rto_power p is summable by A1,A3,Def2;
A7: p > 0 by A1;
    then A8: p <> 0;
A9:  1/p > 0 by A7,REAL_2:127;
    set p1=1/p;
    set vp=seq_id(v) rto_power p;
    set up=seq_id(u) rto_power p;
A10:  p * (1/p) = (p*1)/p by XCMPLX_1:75
              .= 1 by A8,XCMPLX_1:60;
A11:  vp is summable by A5;
A12:  up is summable by A6;
A13: now
        let n be Nat;
     thus A14: vp.n=abs((seq_id(v)).n) to_power p by Def1; thus
           A15: up.n=abs((seq_id(u)).n) to_power p by Def1; thus
           A16: 0 <= abs((seq_id(v)).n) by ABSVALUE:5; thus
           A17: 0 < abs((seq_id(v)).n) or 0=abs((seq_id(v)).n) by A16;
           thus 0 <= vp.n by A7,A14,A17,POWER:39,def 2; thus
           A18: 0 <= abs((seq_id(u)).n) by ABSVALUE:5; thus
           A19: 0 < abs((seq_id(u)).n) or 0=abs((seq_id(u)).n)
         by A18; thus 0 <= up.n by A7,A15,A19,POWER:39,def 2;
    end;
A20: Partial_Sums(up) is non-decreasing by A13,SERIES_1:19;
A21: Partial_Sums(up) is bounded_above by A12,A13,SERIES_1:20;
        consider r1 be real number such that
         A22:for n be set st n in dom Partial_Sums(up)
        holds Partial_Sums(up).n<r1 by A21,SEQ_2:def 1;
        reconsider r1 as Real by XREAL_0:def 1;
        A23: dom Partial_Sums(up)=NAT by SEQ_1:4;
       A24:now
          let n be set such that A25:n in dom Partial_Sums(up);
          A26:Partial_Sums(up).n<r1 by A22,A25;
          reconsider n1=n as Nat by A23,A25;
          A27:Partial_Sums(up).n1<r1 by A26;
   A28: Partial_Sums(up).0<=Partial_Sums(up).n1 by A20,SEQM_3:21;
   0<=up.0 by A13;
   then 0<=Partial_Sums(up).0 by SERIES_1:def 1;
hence (Partial_Sums(up).n) to_power p1 < r1 to_power p1
 by A9,A27,A28,Th1;
end;
deffunc F1(set)=(Partial_Sums(up).$1) to_power p1;
A29:ex q1 be Real st for n be set st n in dom Partial_Sums(up)
