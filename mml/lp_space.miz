theorem Th11:
  for p be Real st p>=1 holds for rseq be Real_Sequence st (for n
  be Element of NAT holds rseq.n=0) holds rseq rto_power p is summable & ( Sum(
  rseq rto_power p) ) to_power (1/p)=0
proof
  let p be Real such that
A1: p>=1;
A2: 1/p > 0 by A1,XREAL_1:141;
  let rseq be Real_Sequence such that
A3: for n be Element of NAT holds rseq.n=0;
A4: for n be Element of NAT holds (rseq rto_power p).n=0
  proof
    let n be Element of NAT;
    rseq.n=0 by A3;
    then abs((rseq).n) =0 by ABSVALUE:7;
