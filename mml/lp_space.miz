theorem LM22:
for p be Real st 1 <=p holds
  for lp be non empty NORMSTR st lp
     =  NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
  the carrier of lp = the_set_of_RealSequences_l^p &
 ( for x be set holds x is VECTOR of lp
    iff x is Real_Sequence & seq_id(x) rto_power p is summable ) &
 0.lp = Zeroseq &
 ( for x be VECTOR of lp holds x =seq_id(x) ) &
 ( for x,y be VECTOR of lp holds x+y =seq_id(x)+seq_id(y) ) &
( for r be Real for x be VECTOR of lp holds r*x = r(#)seq_id(x) ) &
( for x be VECTOR of lp holds
   -x = -seq_id(x) & seq_id(-x) = -seq_id(x) ) &
( for x,y be VECTOR of lp holds x-y =seq_id(x)-seq_id(y) ) &
( for x be VECTOR of lp holds
seq_id(x) rto_power p is summable ) &
( for x be VECTOR of
lp holds ||.x.|| = ( Sum(seq_id(x) rto_power p) ) to_power (1/p) )
proof
 let p be Real such that
  q1: 1<= p;
 let lp be non empty NORMSTR such that
  q2:   lp = NORMSTR (# the_set_of_RealSequences_l^p,
           Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
         l_norm^p  #);
A1:for x be set holds x is Element of lp
     iff x is Real_Sequence & seq_id(x) rto_power p is summable
   proof
     let x be set;  
   a2:   x is Element of the carrier of lp iff x is Element of
          the_set_of_RealSequences_l^p by q2;
   a4:  x in the_set_of_RealSequences_l^p iff seq_id(x) rto_power p
          is summable & x in the_set_of_RealSequences by def_2,q1;
     hence thesis by a2,RSSPACE:def 1;
     end;
 A2: for x be set holds x is VECTOR of lp implies x =seq_id(x)
   proof
     let x be set;
a1:     x is VECTOR of lp iff x is Element of
                       the_set_of_RealSequences_l^p by q2;
a2:     x in the_set_of_RealSequences_l^p iff seq_id(x) rto_power p
          is summable & x in the_set_of_RealSequences by def_2,q1;
 a3:  x in the_set_of_RealSequences iff x is Real_Sequence by RSSPACE:def 1;
 hence thesis by q2,a2,RSSPACE:3;
 end;
A3:0.lp = Zeroseq
   proof
a2:     the_set_of_RealSequences_l^p is lineary-closed by LM14,q1;
         thus 0.lp = Zero_(the_set_of_RealSequences_l^p
                                       ,Linear_Space_of_RealSequences)
                 by RLVECT_1:def 2,q2
              .= 0.Linear_Space_of_RealSequences 
                        by RSSPACE:def 10,a2
              .= Zeroseq by RLVECT_1:def 2,RSSPACE:def 7;
  end; 
A5:for x,y be VECTOR of lp holds x+y =seq_id(x)+seq_id(y)
   proof
     let x,y be VECTOR of lp;     
e1:   lp is Subspace of Linear_Space_of_RealSequences by LM21,q1,q2;
        reconsider x1=x as 
           VECTOR of Linear_Space_of_RealSequences by RLSUB_1:18,e1;
        reconsider y1=y as 
           VECTOR of Linear_Space_of_RealSequences by RLSUB_1:18,e1;
     set L1=Linear_Space_of_RealSequences;
     set W = the_set_of_RealSequences_l^p;
z1:     x in W by q2;
z2:    y in W by q2;
qw:        W is lineary-closed by LM14,q1;
          dom (the add of L1)
            = [:the carrier of L1,the carrier of L1:] by FUNCT_2:def 1; then
          dom ((the add of Linear_Space_of_RealSequences) | [:W,W:])
            =[:W,W:] by RELAT_1:91; then 
e3:    [x,y] in dom ((the add of Linear_Space_of_RealSequences)|[:W,W:])
          by q2;
         x+y=(the add of lp).[x,y] by RLVECT_1:def 3
            .= ((the add of Linear_Space_of_RealSequences)|[:W,W:]).[x,y]
              by RSSPACE:def 8, qw,q2
             .=(the add of Linear_Space_of_RealSequences).[x,y]
            by e3,FUNCT_1:70
             .=x1+y1 by RLVECT_1:def 3;
          hence thesis by RSSPACE:4,RSSPACE:def 7;
          end;           
A6:for r be Real for x be VECTOR of lp holds r*x =r(#)seq_id(x)
   proof
     let r be Real;
     let x be VECTOR of lp;
 e1:   lp is Subspace of Linear_Space_of_RealSequences by LM21,q1,q2;
         reconsider x1=x as 
           VECTOR of Linear_Space_of_RealSequences by RLSUB_1:18,e1;
     set L1=Linear_Space_of_RealSequences;
     set W = the_set_of_RealSequences_l^p;
A7: [:REAL,W:] c= [:REAL,the carrier of L1:] by ZFMISC_1:119;
     dom (the Mult of L1) = [:REAL,the carrier of L1:] by FUNCT_2:def 1; then
     dom ((the Mult of Linear_Space_of_RealSequences) | [:REAL,W:])
       =[:REAL,W:] by A7,RELAT_1:91; then
A8:  [r,x] in dom ((the Mult of Linear_Space_of_RealSequences)|[:REAL,W:])
     by q2;
qw:        W is lineary-closed by LM14,q1;       
    r*x =(the Mult of lp).[r,x] by RLVECT_1:def 4
        .=((the Mult of Linear_Space_of_RealSequences)|[:REAL,W:]).[r,x]
        by RSSPACE:def 9,qw,q2
        .=(the Mult of Linear_Space_of_RealSequences).[r,x]
        by A8,FUNCT_1:70
        .=r*x1 by RLVECT_1:def 4;
     hence thesis by RSSPACE:5,def 7;
   end;
A8:for x be VECTOR of lp holds (-x =-seq_id(x) & seq_id(-x)=-seq_id(x))
   proof
     let x be VECTOR of lp;
     lp is Subspace of Linear_Space_of_RealSequences by LM21,q1,q2;
     then -x = (-1)*x by RLVECT_1:29
         .= (-1) (#) seq_id(x) by A6
        .= -seq_id(x) by SEQ_1:25;
     hence thesis by A2;
   end;
   A9:for x,y be VECTOR of lp holds x-y =seq_id(x)-seq_id(y)
      proof
     let x,y be VECTOR of lp;
     thus x-y = x+(-y) by RLVECT_1:def 11
                .= seq_id(x)+seq_id(-y) by A5
                .= seq_id(x)+(-seq_id(y)) by A8
                .= seq_id(x)-seq_id(y) by SEQ_1:15;
      end;
   for x be VECTOR of lp holds ||.x.|| =
           ( Sum(seq_id(x) rto_power p) ) to_power (1/p)
   proof
     let x be VECTOR of lp;
     thus ||.x.|| = (the norm of lp).x by NORMSP_1:def 1
       .= ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by def_3,q2;
   end;
   hence thesis by A1,A2,A3,A5,A6,A8,A9,q2;
end;

theorem LM23:
for p be Real st p>=1 holds
for rseq be Real_Sequence
   st (for n be Nat holds rseq.n=0) holds
    (rseq rto_power p is summable
 &  ( Sum(rseq rto_power p) ) to_power (1/p)=0 )
proof
     let p be Real such that
A00:      p>=1;
e02: p>0 by A00,AXIOMS:22;
z2:  1/p > 0 by e02,REAL_2:127;
     let rseq be Real_Sequence such that
A01: for n be Nat holds rseq.n=0;
A02: for n be Nat holds (rseq rto_power p).n=0
   proof      
       let n be Nat;
       rseq.n=0 by A01;then
       abs((rseq).n) =0 by ABSVALUE:7;then
q01: abs((rseq).n) to_power p =0 by POWER:def 2,e02;
       hence thesis by def_1;
     end;
A04:  for m be Nat holds Partial_Sums (rseq rto_power p).m = 0
     proof
       let m be Nat;
       defpred P[Nat] means (rseq rto_power p).$1 
       = (Partial_Sums(rseq rto_power p)).$1;
A05:    P[0] by SERIES_1:def 1;
A06:    for k be Nat st P[k] holds P[k+1]
       proof
         let k be Nat such that
A07:      (rseq rto_power p).k
             = (Partial_Sums (rseq rto_power p)).k;
         thus (rseq rto_power p).(k+1) = 0 + (rseq rto_power p).(k+1)
          .= (rseq rto_power p).k + (rseq rto_power p).(k+1) by A02
          .= (Partial_Sums ((rseq rto_power p))).(k+1) by A07,SERIES_1:def 1;
       end;
       for n be Nat holds P[n] from NAT_1:sch 1(A05,A06);
       hence (Partial_Sums ((rseq rto_power p))).m 
                = (rseq rto_power p).m .= 0 by A02;
     end;
A08:  Sum(rseq rto_power p) =0 & (rseq rto_power p) is summable
     proof
A09:    for e be real number st 0<e
        ex n be Nat st for m be Nat st n<=m holds
         abs((Partial_Sums (rseq rto_power p)).m-0)<e
       proof
         let e be real number such that
A10:   0<e;
         take 0;
         let m be Nat such that 0<=m;
         abs((Partial_Sums (rseq rto_power p)).m-0)
            = abs(0-0) by A04
           .= 0 by ABSVALUE:def 1;
         hence abs((Partial_Sums (rseq rto_power p)).m-0)<e by A10;
       end; then
A11:    Partial_Sums (rseq rto_power p) is convergent 
        by SEQ_2:def 6; then
A12:   (rseq rto_power p) is summable by SERIES_1:def 2;
       lim (Partial_Sums (rseq rto_power p)) =0 by A09,A11,SEQ_2:def 7;
       hence thesis by A12,SERIES_1:def 3;
     end;
     hence thesis by z2,POWER:def 2;
end;

theorem LM24:
  for p be Real st 1 <=p
  for rseq be Real_Sequence st
  (rseq rto_power p is summable
   & ( Sum(rseq rto_power p) ) to_power (1/p)=0 ) holds
  for n be Nat holds rseq.n = 0
proof
   let p be Real such that
A00: 1 <=p;
A02: p>0 by A00,AXIOMS:22; then
z0:  p <> 0;
z2:  1/p > 0 by A02,REAL_2:127;
   let rseq being Real_Sequence such that
A03:rseq rto_power p is summable and
A04: Sum(rseq rto_power p) to_power (1/p)=0;
A05:  for i be Nat holds ((rseq) rto_power p).i >= 0
        proof
         let i be Nat;
j1:      ( (rseq) rto_power p).i
           =abs((rseq).i) to_power p by def_1;
j2:       abs((rseq).i) >=0 by ABSVALUE:5;
          hence ((rseq) rto_power p).i >= 0 by j1,A02,REM04;
        end;
 A06:(Sum(rseq rto_power p) ) >= 0 by A05,A03,SERIES_1:21;
A07: ( (Sum(rseq rto_power p) ) to_power (1/p)) to_power p
      =(Sum(rseq rto_power p) ) to_power((1/p)*p)
    by A02,z2,A06,HOLDER_1:2
     .=(Sum(rseq rto_power p) ) to_power(1) by XCMPLX_1:107,z0
     .=(Sum(rseq rto_power p) ) by POWER:30;
A08: 0 to_power p =0 by POWER:def 2,A02; then
A09: (Sum(rseq rto_power p) ) = 0 by A07,A04;
A10: for n be Nat holds (rseq rto_power p).n =0
     by A03,A05,A09,RSSPACE:21;
     now
       let n be Nat;
       (rseq rto_power p).n =abs(rseq.n) to_power p by def_1; then
 A13: abs(rseq.n) to_power p =0 by A10;
 A11: abs(rseq.n) >= 0 by ABSVALUE:5;
 A14: (abs(rseq.n) to_power p) to_power (1/p)
        =abs(rseq.n) to_power (p*(1/p)) by A11,HOLDER_1:2,A02,z2
        .=abs(rseq.n) to_power (1) by XCMPLX_1:107,z0
        .=abs(rseq.n) by POWER:30;
        0 to_power (1/p) =0 by POWER:def 2,z2;      
        hence rseq.n =0 by ABSVALUE:7,A13,A14;
       end;
   hence thesis;
end;

REM20:
for p be Real st 1 <=p
  for lp be non empty NORMSTR st lp
     =  NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
for x being Point of lp, a be Real holds
(seq_id(a *(x)) rto_power p )=
((abs(a) to_power p)(#)(seq_id(x) rto_power p))
proof
 let p be Real such that
  q1: 1<= p;  
  QS00: p > 0 by q1,AXIOMS:22;
 let lp be non empty NORMSTR such that
A0:  lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #);
   let x be Point of lp;
   let a be Real;
now let n1 be set such that q8:n1 in NAT;
reconsider n =n1 as Nat by q8;
 q4:abs(a) >=0 & abs((seq_id(x)).n) >=0 by ABSVALUE:5; 
 q5:(seq_id(a *(x)) rto_power p ).n
  =abs(((seq_id(a*x)).n)) to_power p by def_1
  .=abs(((seq_id(a(#)seq_id(x))).n)) to_power p by LM22,A0,q1
.=abs(((a(#)seq_id(x)).n)) to_power p by RSSPACE:3
.=abs(a*(seq_id(x)).n ) to_power p by SEQ_1:13
.=(abs(a) * abs((seq_id(x)).n)) to_power p by ABSVALUE:10
.=abs(a) to_power p * abs((seq_id(x)).n) to_power p by REM06,q4,QS00
.=abs(a) to_power p*(seq_id(x) rto_power p).n by def_1
.=((abs(a) to_power p)(#)(seq_id(x) rto_power p)).n by SEQ_1:13;
reconsider n as set; 
thus (seq_id(a *(x)) rto_power p ).n1=
((abs(a) to_power p)(#)(seq_id(x) rto_power p)).n1 by q5;
end;
hence (seq_id(a *(x)) rto_power p )=
((abs(a) to_power p)(#)(seq_id(x) rto_power p)) by SEQ_1:8;
end;

REM21:
for p be Real st 1 <=p
  for lp be non empty NORMSTR st lp
     = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
for x being Point of lp, a be Real holds
 ( Sum(seq_id(a*x) rto_power p) ) 
 =(abs(a) to_power p) * Sum(seq_id(x) rto_power p)
 proof
  let p be Real such that
   q1: 1<= p;
 let lp be non empty NORMSTR such that
A0:  lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #);
    let x be Point of lp;
    let a be Real;
q3: (seq_id(x) rto_power p) is summable by LM22,q1,A0;  
thus ( Sum(seq_id(a*x) rto_power p) )
 = ( Sum((abs(a) to_power p)(#)(seq_id(x) rto_power p)) ) by A0,REM20,q1
 .=(abs(a) to_power p) * Sum(seq_id(x) rto_power p) by SERIES_1:13,q3;
 end;
 
REM22:
for p be Real st 1 <=p
 for lp be non empty NORMSTR st lp
     = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
for x being Point of lp, a be Real holds
 ( Sum(seq_id(a*x) rto_power p) ) to_power (1/p)
 =(abs(a) )*(Sum(seq_id(x) rto_power p) to_power (1/p))
proof
   let p be Real such that
    q1: 1<= p;
  let lp be non empty NORMSTR such that
 A0:  lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #);
    a1:  1 <= p by q1;
  h1:  0 < p by a1,AXIOMS:22; then
  h2:p <>0;
  h3:(1/p)>0 by h1,REAL_2:127; 
  let x be Point of lp;
   let a be Real;
 q3: (seq_id(x) rto_power p) is summable by LM22,q1,A0;
q4: now let n be Nat;
 e1: (seq_id(x) rto_power p).n =abs((seq_id(x)).n) to_power p by def_1; 
 e2:abs((seq_id(x)).n)>=0 by ABSVALUE:5;
hence (seq_id(x) rto_power p).n >= 0 by e1,h1,REM04;
 end;
 e3: abs(a) >= 0 by ABSVALUE:5;
 e4:0<=Sum(seq_id(x) rto_power p) by q3,q4,SERIES_1:21;
e5: (abs(a) to_power p) >= 0 by REM04,e3,h1;
thus ( Sum(seq_id(a*x) rto_power p) ) to_power (1/p)
  =((abs(a) to_power p) * Sum(seq_id(x) rto_power p) )
                                    to_power (1/p) by REM21,A0,q1
 .= ((abs(a) to_power p) to_power (1/p)) *
      ( Sum(seq_id(x) rto_power p) to_power (1/p)) by REM06,e4,e5,h3
 .= (abs(a) to_power (p*(1/p))) *
       ( Sum(seq_id(x) rto_power p) to_power (1/p))
      by HOLDER_1:2,h1,h3,e3
.= (abs(a) to_power 1) *
    ( Sum(seq_id(x) rto_power p) to_power (1/p)) by XCMPLX_1:107,h2
.= abs(a)* ( Sum(seq_id(x) rto_power p) to_power (1/p)) by POWER:30;
 end;

theorem LM25:
 for p be Real st 1 <= p
 for lp be non empty NORMSTR st lp
     = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
for x, y being Point of lp, a be Real holds
  ( ||.x.|| = 0 iff x = 0.lp ) &
  0 <= ||.x.|| &
  ||.x+y.|| <= ||.x.|| + ||.y.|| &
  ||.(a*x).|| = abs(a) * ||.x.||
proof
  let p be Real such that
  q1: 1<= p;
  let lp be non empty NORMSTR such that
  q2: lp = NORMSTR (# the_set_of_RealSequences_l^p,
           Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
         l_norm^p  #);
 a1:  1 <= p by q1;
 h1:  0 < p by a1,AXIOMS:22;
   let x, y be Point of lp;
   let a be Real;
   1 > 0 & p >= 1 by q1; then
    p > 0; then
   q3:  1/p > 0 by REAL_2:127;
A1:now assume
a2: ||.x.|| = 0;
a3: ||.x.|| = (the norm of lp). x by NORMSP_1:def 1
            .= ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by def_3,q2;
a4:  seq_id(x) rto_power p is summable by LM22,q1,q2;
a5: for n be Nat holds 0 = (seq_id(x)).n by a2,a3,a4,LM24,q1;
     x in the_set_of_RealSequences by def_2,q1,q2;
     hence x = Zeroseq by RSSPACE:def 6,a5
     .=0.lp by LM22,q1,q2;
   end;
A2:now assume
e1:x=0.lp;
    x=Zeroseq by e1,LM22,q1,q2; then
e2: for n be Nat holds (seq_id(x)).n=0 by RSSPACE:def 6;    
thus ||.x.|| = (the norm of lp). x by NORMSP_1:def 1
            .=  ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by def_3,q2
            .= 0 by LM23,e2,q1;
   end;
D1:for n be Nat holds 0 <= (seq_id(x) rto_power p).n
proof
 let n be Nat;
 set xp=seq_id(x) rto_power p;
 e2:   xp.n=abs((seq_id(x)).n) to_power p by def_1;
 e3:  0 <= abs((seq_id(x)).n) by ABSVALUE:5; 
 e4:  0 < abs((seq_id(x)).n) or 0=abs((seq_id(x)).n) by e3; thus
   0 <= xp.n by e2,e4,POWER:39,h1,POWER:def 2;
   end;
   seq_id(x) rto_power p is summable by q2,def_2,q1;then
 D2: 0<= ( Sum(seq_id(x) rto_power p) ) by SERIES_1:21,D1;
 D3:( Sum(seq_id(x) rto_power p) ) to_power (1/p) >=0 by q3,REM04,D2;
     ( Sum(seq_id(x) rto_power p) ) to_power (1/p) = (the norm of lp). x
     by def_3,q2
       .= ||.x.|| by NORMSP_1:def 1; then
A3:     ||.x.|| >=0 by D3;
A4:now
 A11:||.x.|| = (the norm of lp). x by NORMSP_1:def 1
            .= ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by def_3,q2;
 A12:||.y.|| = (the norm of lp). y by NORMSP_1:def 1
           .= ( Sum(seq_id(y) rto_power p) ) to_power (1/p) by def_3,q2;
A1:((seq_id(x))+(seq_id(y)))
=seq_id((seq_id(x))+(seq_id(y))) by RSSPACE:3
.=seq_id(x+y) by LM22,q1,q2;
A13: Sum((((seq_id(x))+(seq_id(y))) rto_power p)) to_power (1/p)
      = (the norm of lp).(x+y) by def_3,q2,A1
     .= ||.x + y.|| by NORMSP_1:def 1;
A14:(seq_id(x) rto_power p) is summable by LM22,q1,q2;
(seq_id(y) rto_power p) is summable by LM22,q1,q2;
hence ||.x + y.|| <= ||.x.|| + ||.y.|| by A11,A12,A13,A14,q1,LM11;
end;
 Q1:||.x.|| = (the norm of lp). x by NORMSP_1:def 1
            .= ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by def_3,q2;
 ||.(a*x).|| = (the norm of lp). (a*x) by NORMSP_1:def 1
 .= ( Sum(seq_id(a*x) rto_power p) ) to_power (1/p) by def_3,q2
 .=(abs(a))* ||.x.|| by Q1,REM22,q1,q2;
 hence thesis by A1,A2, A3, A4;
end;

theorem Th9d:
for p be Real st p >= 1
  for lp be non empty NORMSTR
     st lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
 lp is RealNormSpace-like
 proof
 let p be Real such that 
A1: p >=1;
 let lp be non empty NORMSTR such that
A11: lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #);
     for x, y being Point of lp, a be Real holds
    ( ||.x.|| = 0 iff x = 0.lp ) &
    0 <= ||.x.|| &
    ||.x+y.|| <= ||.x.|| + ||.y.|| &
    ||.(a*x).|| = abs(a) * ||.x.|| by LM25,A1,A11;
 hence thesis by NORMSP_1:def 2; 
 end;
 
theorem TH10d:
 for p be Real st p >= 1
 for lp be non empty NORMSTR st lp
      = NORMSTR (# the_set_of_RealSequences_l^p,
        Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
        Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
        Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
      l_norm^p #) holds
 lp is RealNormSpace by Th9d,LM21;

 reserve NRM for non empty RealNormSpace,
         seq for sequence of NRM;

Lm6:
for c be Real, seq be Real_Sequence st seq is convergent
for rseq be Real_Sequence st
 ( for i be Nat holds rseq .i = abs(seq.i-c) ) holds
 ( rseq is convergent & lim rseq = abs(lim seq-c) ) by RSSPACE3:1;

Lm7:
for p be Real st 0 < p
for c be Real, seq be Real_Sequence st seq is convergent
for rseq be Real_Sequence st
 ( for i be Nat holds rseq .i = (abs(seq.i -c)) to_power p ) 
 holds
 ( rseq is convergent & lim rseq = (abs(lim seq-c)) to_power p )
proof
  let p be Real such that   
Q01:0 <p;
  let c be Real;
  let seq be Real_Sequence such that
Q02:seq is convergent;
  let rseq be Real_Sequence such that
Q03: ( for i be Nat holds rseq .i = (abs(seq.i -c)) to_power p ); 
 deffunc F(set) = abs(seq.$1-c);
   consider b be Real_Sequence such that
Q04:for n be Nat holds b.n=F(n) from SEQ_1:sch 1;
Q05:b is convergent by Q04,Lm6,Q02;
Q06:lim b = abs(lim seq - c) by Q04,Lm6,Q02;
Q07:for n be Nat holds 0 <=b.n 
  proof
  let n be Nat;
  b.n=abs(seq.n-c) by Q04;
  hence thesis by ABSVALUE:5;
  end;
  for n be Nat holds rseq.n=(b.n) to_power p
  proof
    let n be Nat;
    rseq.n=(abs(seq.n -c)) to_power p by Q03
         .=(b.n) to_power p by Q04;
    hence thesis;
  end;
  hence thesis by Q06,Q05,Q07,Q01,HOLDER_1:10;
end;

Lm7d:
for p be Real st 0 < p
for c be Real, seq, seq1 be Real_Sequence st (seq is convergent
& seq1 is convergent)
for rseq be Real_Sequence st
 ( for i be Nat holds rseq .i = (abs(seq.i -c)) to_power p +seq1.i ) 
 holds
 ( rseq is convergent & lim rseq = (abs(lim seq-c)) to_power p + lim seq1 )
proof
  let p be Real such that   
Q01:0 < p;
  let c be Real;
  let seq,seq1 be Real_Sequence such that
Q02:seq is convergent and
Q00:seq1 is convergent;  
  let rseq be Real_Sequence such that
Q03:  for i be Nat holds rseq .i =( (abs(seq.i -c)) to_power p ) + seq1.i;
 deffunc F(set) = abs(seq.$1-c) to_power p;
   consider b be Real_Sequence such that
Q04:for n be Nat holds b.n=F(n) from  SEQ_1:sch 1;
Q05:b is convergent by Q04,Lm7,Q02,Q01;
Q06:lim b = (abs(lim seq-c)) to_power p by Q04,Lm7,Q02,Q01;
now let n be Nat;thus
  rseq .n = ( (abs(seq.n -c)) to_power p ) + seq1.n by Q03
           .=b.n + seq1.n by Q04;
  end; then
 Q07:rseq = b + seq1 by SEQ_1:11;
 Q08: lim rseq
     =(abs(lim seq-c)) to_power p + lim seq1 by Q06,Q00,Q05,SEQ_2:20,Q07;
     thus rseq is convergent by Q07,Q05,Q00,SEQ_2:19;
     thus lim rseq = (abs(lim seq-c)) to_power p + lim seq1 by Q08;
end;

Lm8:
 for e be Real, seq be Real_Sequence st ( seq is convergent &
  ex k be Nat st (for i be Nat st k <= i holds seq.i <=e )) holds
   lim seq <=e by RSSPACE2:6;

Lm9:
for a,b be Real_Sequence holds a=a+b-b
proof
 let a,b be Real_Sequence;
 now let n be Nat;
 q: a+b-b = a+b+-b by SEQ_1:15
   .=a+(b+(-b)) by SEQ_1:20;
   (a+(b+(-b))).n=a.n+(b+(-b)).n by SEQ_1:11
   .=a.n+(b.n+(-b).n) by SEQ_1:11
   .=a.n+(b.n+(-b.n)) by SEQ_1:14
   .=a.n;
   hence (a+b-b).n=a.n by q;
 end;
hence thesis by FUNCT_2:113;
end;

Lmd1:
for p be Real st p >= 1
for a,b be Real_Sequence st (a rto_power p is summable &
    b rto_power p is summable) holds (a + b) rto_power p is summable
proof
      let p be Real such that 
q00:p >= 1;
      let a,b be Real_Sequence such that 
q01: a rto_power p is summable and
q02: b rto_power p is summable;
      reconsider a1=a, b1=b as set;
q03:a1 in the_set_of_RealSequences by RSSPACE:def 1;
q04:b1 in the_set_of_RealSequences by RSSPACE:def 1;
q05:seq_id(a1) = a by RSSPACE:def 2,q03;
q06:seq_id(b1) = b by RSSPACE:def 2,q04;
q07:seq_id(a1) rto_power p is summable by q01,q05;
q08:seq_id(b1) rto_power p is summable by q02,q06;
q09:a1 in the_set_of_RealSequences_l^p by def_2,q07,q03,q00;
q10:b1 in the_set_of_RealSequences_l^p by def_2,q08,q04,q00;
     reconsider a1 as VECTOR of Linear_Space_of_RealSequences by q09;
     reconsider b1 as VECTOR of Linear_Space_of_RealSequences by q10;
     the_set_of_RealSequences_l^p is lineary-closed by LM14,q00;
     then a1+b1 in the_set_of_RealSequences_l^p by q09,q10,RLSUB_1:def 1;
     then 
q13:seq_id(a1+b1) rto_power p is summable by def_2,q00;
     seq_id(a1+b1)=seq_id(seq_id(a1)+seq_id(b1)) by RSSPACE:4,RSSPACE:def 7
     .=a+b by q05,q06,RSSPACE:3;
     hence (a+b) rto_power p is summable by q13;
end;

theorem LM26:
 for p be Real st 1 <=p
 for lp be RealNormSpace 
    st lp = NORMSTR (# the_set_of_RealSequences_l^p,
      Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
      Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
      Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
      l_norm^p #) holds
 for vseq be sequence of lp st vseq is Cauchy_sequence_by_Norm
 holds vseq is convergent
proof
  let p be Real such that
  q1: 1<= p;
 Q6:  p >0 by q1,AXIOMS:22;
 then z0: p <> 0;
  z2:  1/p > 0 by Q6,REAL_2:127;
  z3:  p * (1/p) = (p*1)/p by XCMPLX_1:75
                .= 1 by Q6, XCMPLX_1:60;
   let lp be RealNormSpace such that
     q2:  lp = NORMSTR (# the_set_of_RealSequences_l^p,
           Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
         l_norm^p #);
 let vseq be sequence of lp such that
 A1:vseq is Cauchy_sequence_by_Norm;
   defpred P[set,set] means
        ex i be Nat st $1=i &
     ex rseqi be Real_Sequence st
      (for n be Nat holds rseqi.n=(seq_id(vseq.n)).i) &
     rseqi is convergent & $2 = lim rseqi;
A2:for x be set st x in NAT ex y be set st y in REAL & P[x,y]
   proof
     let x be set such that
A3: x in NAT;
     reconsider i=x as Nat by A3;
     deffunc F(Nat) = (seq_id(vseq.$1)).i;
     consider rseqi be Real_Sequence such that
A4:  for n be Nat holds rseqi.n= F(n) from SEQ_1:sch 1;
A5:  rseqi is convergent
      proof
      now
         let e1 be real number such that
A6:     e1 > 0;
         reconsider e=e1 as Real by XREAL_0:def 1;
    thus ex k be Nat st
          for m be Nat st k <= m holds abs(rseqi.m -rseqi.k) < e1
          proof
           consider k be Nat such that
A8:       for n, m be Nat st ( n >= k & m >= k ) holds
           ||.(vseq.n) - (vseq.m).|| < e by A1,A6,RSSPACE3:10;
 for m being Nat st k <= m holds abs(rseqi.m-rseqi.k) < e
           proof  
            let m be Nat such that
A9:        k<=m;
a9:        0 <= ||.(vseq.m) - (vseq.k).|| by NORMSP_1:8;
a9d:       ||.(vseq.m) - (vseq.k).||
            = (the norm of lp).((vseq.m)-(vseq.k)) by NORMSP_1:def 1
           .= ( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) ) 
             to_power (1/p) by def_3,q2; then
             ( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) ) to_power
                                               (1/p) < e by A8,A9; then
A11:        (( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) ) to_power
                                (1/p)) to_power p < e to_power p
                              by REM03,Q6,a9,a9d;
 e3: (seq_id((vseq.m)-(vseq.k)) rto_power p) is summable by LM22,q1,q2;
 
e4:now
 let i be Nat;
   j1:  (seq_id((vseq.m)-(vseq.k)) rto_power p).i 
             =abs((seq_id((vseq.m)-(vseq.k))).i) to_power p
           by def_1;
   j2:abs((seq_id((vseq.m)-(vseq.k))).i) >=0 by ABSVALUE:5;
 hence (seq_id((vseq.m)-(vseq.k)) rto_power p).i >= 0 by j1,Q6,REM04;
end;
e5:0<=Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) by e3,e4,SERIES_1:21;        
e6:    (( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) ) to_power
                                (1/p)) to_power p
         =( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) )
           to_power ((1/p)*p) by HOLDER_1:2,e5,Q6,z2
        .= ( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) ) by POWER:30,z3;
az1:(e to_power p) to_power (1/p)
        = e to_power ((1/p) * p) by POWER:38,A6
        .= e to_power(1) by XCMPLX_1:107,z0
        .=e by POWER:30;
ee0: Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) < e to_power p by e6,A11;
 ee1:(seq_id((vseq.m)-(vseq.k)) rto_power p).i <=
      Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) by e4,e3,RSSPACE2:4;
 e8: abs((seq_id((vseq.m)-(vseq.k))).i) >=0 by ABSVALUE:5;
ww1: now
       let a,b,c be Real such that q:a =0 & b>0 & c>0;
       a to_power b =0 by POWER:def 2,q;
       hence (a to_power b) to_power c =0 by POWER:def 2,q;
       end;
 ww2: now 
       let a,b,c be Real such that q:a =0 & b>0 & c>0;
       b*c >0 by REAL_2:122,q;
       hence a to_power (b *c) =0 by POWER:def 2,q;
       end;  
  ww3: now 
       let a,b,c be Real such that q:a =0 & b>0 & c>0;
       thus (a to_power b) to_power c
         = 0 by ww1,q
        .= a to_power (b *c) by ww2,q;
       end;  
  ww4:for a,b,c be Real st a >0 & b>0 & c>0
       holds (a to_power b) to_power c =
       a to_power (b*c) by POWER:38;
  ww5:for a,b,c be Real st (a >0 or a=0) & b>0 & c>0
       holds (a to_power b) to_power c =
       a to_power (b*c) by ww4,ww3;
 ww6: for a,b,c be Real st a>=0 & b>0 & c>0 holds
      (a to_power b) to_power c = a to_power (b*c)
      proof
       let a,b,c be Real such that
       q:a>=0 & b>0 & c>0;
       ((a >0) or (a=0)) & b>0 & c>0 by q;
      hence (a to_power b) to_power c = a to_power (b*c) by ww5;
       end;
 r1: (seq_id((vseq.m)-(vseq.k)) rto_power p).i
     =abs((seq_id((vseq.m)-(vseq.k))).i) to_power p by def_1;
 r2:((seq_id((vseq.m)-(vseq.k)) rto_power p).i) to_power (1/p)
    =abs((seq_id((vseq.m)-(vseq.k))).i) to_power (1) by z3,Q6,z2,e8,ww6,r1
    .=abs((seq_id((vseq.m)-(vseq.k))).i) by POWER:30;       
qw1:  (seq_id((vseq.m)-(vseq.k)) rto_power p).i >= 0 by e4;    
    (seq_id((vseq.m)-(vseq.k)) rto_power p).i <e to_power p
      by ee0,ee1,AXIOMS:22;
  then ((seq_id((vseq.m)-(vseq.k)) rto_power p).i) to_power (1/p)
  < (e to_power p) to_power (1/p) by REM03,qw1,z2; then
kw1:   abs((seq_id((vseq.m)-(vseq.k))).i) <e by az1,r2;
 qw2:  rseqi.m=(seq_id(vseq.m)).i & rseqi.k=(seq_id(vseq.k)).i by A4;
   reconsider vsem = (vseq.m), vsek = (vseq.k) as VECTOR of lp;
(vsem)-(vsek) =(seq_id(vsem)) - (seq_id(vsek)) by LM22,q1,q2; then
  abs((seq_id((vseq.m)-(vseq.k))).i)
   =abs((seq_id(vseq.m)-seq_id(vseq.k)).i) by RSSPACE:3
  .=abs((seq_id(vseq.m)+-seq_id(vseq.k)).i) by SEQ_1:15
  .=abs((seq_id(vseq.m)).i+(-(seq_id(vseq.k))).i) by SEQ_1:11
  .=abs((seq_id(vseq.m)).i +-(seq_id(vseq.k)).i) by SEQ_1:14
  .=abs(rseqi.m -rseqi.k) by qw2;
 hence abs(rseqi.m -rseqi.k) < e by kw1;
end;        
hence thesis;
end;  
end;
hence rseqi is convergent by SEQ_4:58;
end;
   take y = lim rseqi;
   thus thesis by A4,A5;
 end;
   consider f be Function of NAT,REAL such that
A16:for x be set st x in NAT holds P[x,f.x] from FUNCT_2:sch 1(A2);
   reconsider tseq=f as Real_Sequence;
A17:now
      let i be Nat;
      reconsider x=i as set;
      ex i0 be Nat st x=i0 &
       ex rseqi be Real_Sequence st
        ( for n be Nat holds rseqi.n=(seq_id(vseq.n)).i0 ) &
       rseqi is convergent &
       f.x=lim rseqi by A16;
       hence ex rseqi be Real_Sequence st
        ( for n be Nat holds rseqi.n=(seq_id(vseq.n)).i ) &
        rseqi is convergent &
        tseq.i=lim rseqi;
      end;
A18:for e be Real st e >0
   ex k be Nat st for n be Nat st n >= k holds
    ((seq_id(tseq)-seq_id(vseq.n)) rto_power p ) is summable &
      Sum((seq_id(tseq)-seq_id(vseq.n)) rto_power p) < e
   proof
       let e2 be Real such that
A00: e2 >0;
       set e=e2/2;
A19: e >0 by A00,SEQ_2:3;
A21: e2 >e by A00,SEQ_2:4;
       set e1=e to_power (1/p);
A20: e1>0 by A19,POWER:39;
       consider k be Nat such that
A22: for n, m be Nat st ( n >= k & m >= k ) holds
         ||.(vseq.n) - (vseq.m).|| < e1 by A1,A20,RSSPACE3:10;
A23: for m,n be Nat st ( n >= k & m >= k ) holds
        ((seq_id((vseq.m)-(vseq.n)) rto_power p) is summable &
           Sum( (seq_id((vseq.m)-(vseq.n)) rto_power p) ) < e &
    for i be Nat holds 0 <=  ((seq_id((vseq.m)-(vseq.n))) rto_power p).i)
        proof
        let m,n be Nat such that
e0:  n >= k & m >= k;
e1:  (seq_id((vseq.m)-(vseq.n)) rto_power p) is summable by LM22,q1,q2;
e1d:       for i be Nat holds (seq_id((vseq.m)-(vseq.n)) rto_power p).i >= 0
       proof
         let i be Nat;
j1:      (seq_id((vseq.m)-(vseq.n)) rto_power p).i 
         =abs((seq_id((vseq.m)-(vseq.n))).i) to_power p by def_1;
j2:      abs((seq_id((vseq.m)-(vseq.n))).i) >=0 by ABSVALUE:5;
        hence
        (seq_id((vseq.m)-(vseq.n)) rto_power p).i >= 0 by j1,Q6,REM04;
       end;
       ||.(vseq.m) - (vseq.n).|| < e1 by e0,A22; then
     (the norm of lp).((vseq.m)-(vseq.n))<e1 by NORMSP_1:def 1;
     then
e3:  (( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) )       
                                       to_power (1/p)) < e1 by def_3,q2;
 e4: (seq_id((vseq.m)-(vseq.n)) rto_power p) is summable by LM22,q1,q2;
 e5:0<=Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) by e1d,e4,SERIES_1:21;
 e6:    (( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) ) to_power 
                                 (1/p)) to_power p
          =( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) )
            to_power ((1/p)*p) by HOLDER_1:2,e5,Q6,z2
        .= ( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) ) by POWER:30,z3;
 e7:e1 to_power p = e to_power ((1/p) * p) by POWER:38,A19
     .= e to_power(1) by XCMPLX_1:107,z0
     .= e by POWER:30;
  (( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) ) to_power
                                 (1/p)) >=0 by e5,z2,REM04;
 then ( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) ) < e by e7,e6,e3,Q6,REM03;
hence thesis by e1,e1d;
 end;
A27:  for n be Nat for i be Nat holds
      for rseq be Real_Sequence st
       ( for m be Nat holds
         rseq.m = Partial_Sums ((seq_id((vseq.m)-(vseq.n))) rto_power p).i )
      holds
       ( rseq is convergent & lim rseq =
              Partial_Sums ((seq_id(tseq)-seq_id(vseq.n)) rto_power p) .i )
     proof
       let n be Nat;
A28:   for m,k be Nat holds
         seq_id((vseq.m) - (vseq.k)) = seq_id(vseq.m)-seq_id(vseq.k)         
         proof
             let m,k be Nat;
             seq_id((vseq.m) - (vseq.k))
              = seq_id(seq_id((vseq.m))-seq_id((vseq.k))) by LM22,q1,q2;
             hence thesis by RSSPACE:3;
          end;
         defpred P[Nat] means
         for rseq be Real_Sequence st for m be Nat holds
         rseq.m= Partial_Sums 
                 ((seq_id((vseq.m)-(vseq.n))) rto_power p).$1 holds
         rseq is convergent &
         lim rseq = Partial_Sums
                  ((seq_id(tseq)-seq_id(vseq.n)) rto_power p).$1;
A29:  P[0]
       proof
         now
           let rseq be Real_Sequence such that
A30:     for m be Nat holds
           rseq.m= Partial_Sums 
                 ((seq_id((vseq.m)-(vseq.n))) rto_power p).0;
           thus
           rseq is convergent &
           lim rseq = Partial_Sums
                  ((seq_id(tseq)-seq_id(vseq.n)) rto_power p).0
           proof
            consider rseq0 be Real_Sequence such that
A31:       ( for m be Nat holds rseq0.m=(seq_id(vseq.m)).0 ) &
            rseq0 is convergent &
            tseq.0=lim rseq0 by A17;
A32:       for m being Nat holds rseq.m =
            abs(rseq0.m -(seq_id(vseq.n)).0) to_power p       
            proof
               let m be Nat;
              rseq.m =Partial_Sums
                 ((seq_id((vseq.m)-(vseq.n))) rto_power p).0 by A30
              .= ((seq_id((vseq.m)-(vseq.n))) rto_power p).0
                                                        by SERIES_1:def 1
              .= ((seq_id(vseq.m)-seq_id(vseq.n)) rto_power p).0 by A28
              .=((seq_id(vseq.m)+-seq_id(vseq.n)) rto_power p).0 by SEQ_1:15
