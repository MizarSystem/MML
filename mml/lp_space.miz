begin :: The Real Norm Space of l^p Real Sequences

definition
  let x be Real_Sequence;
  let p be Real;
  func x rto_power p -> Real_Sequence means :def_1:
  for n be Nat holds it.n = abs(x.n) to_power p;
     existence
     proof
    deffunc F(set)=( abs(x.$1) ) to_power p;
a1:     ex q1 be  Real_Sequence st
        for n be Nat holds q1.n=F(n) from SEQ_1:sch 1;
        consider q1 be Real_Sequence such that
qq1:    for n be Nat holds
        q1.n= abs(x.n) to_power p by a1;
        take IT=q1;
        thus thesis by qq1;
     end;
     uniqueness
     proof
      let a1,a2 be Real_Sequence;
      assume that
aa1:   for n be Nat holds a1.n = abs(x.n) to_power p and
aa2:   for n be Nat holds a2.n = abs(x.n) to_power p;
        for s be set st s in NAT holds a1.s = a2.s
       proof
        let s be set such that
B:      s in NAT;
        a1.s = abs(x.s) to_power p by aa1,B
              .= a2.s by aa2,B;      
        hence thesis;
       end;
       hence thesis by SEQ_1:8;
     end;
     end;

definition
  let p be Real;
  assume AS0:p >= 1;
  func the_set_of_RealSequences_l^p
  -> non empty Subset of the carrier of Linear_Space_of_RealSequences means
     :def_2:
     for x being set holds x in it iff
     (x in the_set_of_RealSequences & seq_id(x) rto_power p is summable);
   existence
   proof
   defpred P[set] means seq_id($1) rto_power p is summable;
   consider IT being set such that
A1:for x being set holds x in IT
    iff x in the_set_of_RealSequences & P[x] from XBOOLE_0:sch 1;
A2:IT is non empty
   proof
      seq_id(Zeroseq) rto_power p is summable
      proof
        1 > 0; then
S2:    p > 0 by AS0;
         reconsider rseq=seq_id(Zeroseq) rto_power p as Real_Sequence;
         now let n be Nat;
         thus rseq.n =abs((seq_id(Zeroseq)).n) to_power p by def_1
                    .=abs(0) to_power p by RSSPACE:def 6
                    .=0 to_power p by ABSVALUE:7
                    .=0 by POWER:def 2,S2;
       end;
       then rseq is absolutely_summable by COMSEQ_3:3;
       hence thesis by SERIES_1:40;
     end;
     hence thesis by A1;
  end;
IT is Subset of the_set_of_RealSequences
   proof
     for x be set st x in IT holds x in the_set_of_RealSequences by A1;
     hence thesis by TARSKI:def 3;
   end;
   hence thesis by A1,A2,RSSPACE:def 7;
  end;
uniqueness
     proof
   let X1,X2 be non empty Subset of the carrier
   of Linear_Space_of_RealSequences;
   assume that
A3: for x being set holds x in X1 iff
     (x in the_set_of_RealSequences &  seq_id(x) rto_power p is summable)
   and
A4: for x being set holds x in X2 iff
     (x in the_set_of_RealSequences &  seq_id(x) rto_power p is summable);
A5: X1 c= X2
    proof
      let x be set;
      assume x in X1; then
      x in the_set_of_RealSequences &
      (x in the_set_of_RealSequences & seq_id(x) rto_power p is summable)
      by A3;
      hence x in X2 by A4;
   end;
   X2 c= X1
   proof
       let x be set;
        assume x in X2; then
        x in the_set_of_RealSequences & (x in the_set_of_RealSequences
        & seq_id(x) rto_power p is summable) by A4;
      hence x in X1 by A3;
    end;
  hence thesis by A5,XBOOLE_0:def 10;
  end;
end;

 reserve x1,x2,y1,y2,p,q,a,b,c for Real;

REM04:
x1 >= 0 & y1 > 0 implies x1 to_power y1 >= 0
proof
assume that
q1:x1 >= 0 and 
q2:y1 > 0;
   x1>0 or x1=0 by q1;
hence x1 to_power y1 >= 0 by q2,POWER:def 2,POWER:39;
end;

REM06:
y1>0 & x1>=0 & x2>=0 implies
  (x1*x2) to_power y1 = (x1 to_power y1) * (x2 to_power y1)
proof
assume 
A00: y1>0 & x1>=0 & x2>=0; then
A01: (x1>0 & x2>0) or (x1>0 & x2=0) or
       (x1=0 & x2>0) or (x1=0 & x2=0);
per cases by A00;
   suppose x1>0 & x2>0;
    hence (x1*x2) to_power y1=(x1 to_power y1) * (x2 to_power y1) by POWER:35;
  end;
    suppose Q:x1>0 & x2=0; then
q1:   x1 * x2=0;
q2:   x2 to_power y1 = 0 by Q,A00,POWER:def 2;
      hence (x1*x2) to_power y1=(x1 to_power y1) * (x2 to_power y1) by Q;
    end;
    suppose Q:x1=0 & x2>0; then
q1:   x1 * x2=0;
q2:   x1 to_power y1 = 0 by Q,A00,POWER:def 2;
      hence (x1*x2) to_power y1=(x1 to_power y1) * (x2 to_power y1) by Q;
    end;
    suppose Q: x1=0 & x2=0; then
q1:   x1 * x2=0;
q2:   x2 to_power y1 = 0 by Q,A00,POWER:def 2;
      hence (x1*x2) to_power y1=(x1 to_power y1) * (x2 to_power y1) by Q;
    end;
end;

theorem REM03:
  a >= 0 & a < b & c > 0 implies a to_power c < b to_power c
proof
P:  a > 0 & a < b & c > 0 implies a to_power c < b to_power c by POWER:42;
P1:  a = 0 & c > 0 implies a to_power c = 0 by POWER:def 2;
  hence thesis by POWER:42,POWER:39;
end;

sub1:
 for p be Real st 1 = p 
 for a,b be Real_Sequence
 for n be Nat holds
  (Partial_Sums((a + b) rto_power p).n) to_power (1/p) <=
  (Partial_Sums(a rto_power p).n) to_power (1/p) +
  (Partial_Sums(b rto_power p).n) to_power (1/p)
   proof
      let p be Real such that
A01:p=1;
      let a,b be Real_Sequence;
      let n be Nat;
A03: now let n be Nat;
      thus ((a + b) rto_power p).n = abs((a + b).n) to_power p by def_1
      .=abs((a + b).n) by A01,POWER:30;thus
    (a rto_power p).n = abs(a .n) to_power p by def_1
      .=abs(a .n) by A01,POWER:30;thus
    (b rto_power p).n = abs(b .n) to_power p by def_1
      .=abs(b .n) by A01,POWER:30;
      end; then
A04: (a + b) rto_power p = abs(a + b) by SEQ_1:16;
A05: a rto_power p = abs(a) by SEQ_1:16,A03;
A06: b rto_power p = abs(b) by SEQ_1:16,A03;
A07: (Partial_Sums((a + b) rto_power p).n) to_power (1/p)
      =(Partial_Sums(abs(a + b)).n) by A01,POWER:30,A04;
A08: (Partial_Sums(a rto_power p).n) to_power (1/p)
      =(Partial_Sums(abs(a)).n) by A01,POWER:30,A05;
A09: (Partial_Sums(b rto_power p).n) to_power (1/p)
      =(Partial_Sums(abs(b)).n) by A01,POWER:30,A06;
A10:for n be Nat holds abs((a + b).n) <= abs(b .n) + abs(a .n)
   proof
    let n be Nat;
    abs((a + b).n)=abs(a.n + b.n) by SEQ_1:11;
    hence thesis by ABSVALUE:13;
    end;
   deffunc F(Nat) =abs(a .($1)) + abs(b .($1));
   consider c be Real_Sequence such that
A11:for n be Nat holds c.n= F(n) from SEQ_1:sch 1;
A12:now let n be Nat;
q00:abs((a + b).n) <= abs(b .n) + abs(a .n) by A10;
q01:abs((a + b).n)=abs(a+b).n by SEQ_1:16;
   thus abs(a+b).n <= c.n by q00,q01,A11;
 end;
A13:now let n be Nat;
q2: abs(b.n)=abs (b).n by SEQ_1:16;
q3: abs(a.n)=abs (a).n by SEQ_1:16;
    thus c.n=abs (a).n + abs (b).n by q2,q3,A11
        .=(abs a + abs b).n by SEQ_1:11;
     end;
 A14: c = abs a + abs b
       proof
       for n be set st n in NAT holds
       c.n = (abs (a) + abs (b)).n by A13;
   hence c = (abs (a) + abs (b)) by SEQ_1:8;
  end; 
W1: Partial_Sums(abs(a+b)).n<=Partial_Sums(c).n by SERIES_1:17,A12; then
W2: Partial_Sums(abs(a+b)).n <=
      Partial_Sums(abs (a) + abs (b)).n by A14;
      Partial_Sums(abs (a) + abs (b)) =
      Partial_Sums(abs a) + Partial_Sums(abs(b)) by SERIES_1:8;
       hence (Partial_Sums((a + b) rto_power p).n) to_power (1/p)
       <=(Partial_Sums(a rto_power p).n) to_power (1/p)
       + (Partial_Sums(b rto_power p).n) to_power (1/p)
         by A07,A08,A09,W2,SEQ_1:11;
   end;

theorem LM10:
 for p be Real st 1 <= p
 for a,b be Real_Sequence
 for n be Nat holds
  (Partial_Sums((a + b) rto_power p).n) to_power (1/p) <=
  (Partial_Sums(a rto_power p).n) to_power (1/p) +
  (Partial_Sums(b rto_power p).n) to_power (1/p)
   proof
   let p be Real such that
   A01: 1<=p;
   let a,b be Real_Sequence;
   set ap = a rto_power p;
   set bp = b rto_power p;
   set ab= ((a + b) rto_power p);
   let n be Nat;
L: p > 1 or p=1 by A01,REAL_1:def 5;
   now per cases by A01,REAL_1:def 5;
   case 
   L1:p > 1;
a0:now let n be Nat;
a1:  (a rto_power p).n =abs(a .n) to_power p by def_1;
a2:  (b rto_power p).n =abs(b .n) to_power p by def_1;
a3:  ((a + b) rto_power p).n =abs((a + b).n) to_power p by def_1
      .=abs(a.n+b.n) to_power p by SEQ_1:11;
        thus ap.n = abs(a .n) to_power p by a1;
        thus bp.n = abs(b .n) to_power p by def_1;
        thus ab.n = abs(a.n+b.n) to_power p by a3;
    end;  
    hence (Partial_Sums((a + b) rto_power p).n) to_power (1/p) <=
        (Partial_Sums(a rto_power p).n) to_power (1/p) +
        (Partial_Sums(b rto_power p).n) to_power (1/p) by L1,HOLDER_1:7;
    end;
    case p=1;
    hence (Partial_Sums((a + b) rto_power p).n) to_power (1/p) <=
     (Partial_Sums(a rto_power p).n) to_power (1/p) +
     (Partial_Sums(b rto_power p).n) to_power (1/p) by sub1;
    end;
   end;
 hence thesis;
 end;

sub2:
for a,b be Real_Sequence
for p be Real st 1 = p &
  ((a rto_power p) is summable) &
  ((b rto_power p) is summable) holds
  ((a+b) rto_power p) is summable &
 (Sum((a + b) rto_power p)) to_power (1/p) <=
 (Sum(a rto_power p)) to_power (1/p) + (Sum(b rto_power p)) to_power (1/p)
proof
 let a,b be Real_Sequence;
 let p be Real such that
A01:p=1 and
A00:a rto_power p is summable and
A02:b rto_power p is summable;
A03: now let n be Nat;thus
       ((a + b) rto_power p).n = abs((a + b).n) to_power p by def_1
      .=abs((a + b).n) by A01,POWER:30;thus
      (a rto_power p).n = abs(a .n) to_power p by def_1
      .=abs(a .n) by A01,POWER:30;
      thus (b rto_power p).n = abs(b .n) to_power p by def_1
      .=abs(b .n) by A01,POWER:30;
      end; then
A04: (a + b) rto_power p = abs(a + b) by SEQ_1:16;
A05: a rto_power p = abs(a) by SEQ_1:16,A03;
A06: b rto_power p = abs(b) by SEQ_1:16,A03;
A07:(Sum((a + b) rto_power p)) to_power (1/p)
      =Sum(abs(a + b)) by A01,POWER:30,A04;
A08:(Sum((a) rto_power p)) to_power (1/p)
      =Sum(abs(a)) by A01,POWER:30,A05;
A09:(Sum((b) rto_power p)) to_power (1/p)
      =Sum(abs(b)) by A01,POWER:30,A06;
A11:abs(a) is summable by A00,A05;
A12:abs(b) is summable by A02,A06;
A13:a is absolutely_summable by A11,SERIES_1:def 5;
A14:b is absolutely_summable by A12,SERIES_1:def 5;
A15:a = seq_id (a) & b = seq_id (b) by RSSPACE:3;
A16:seq_id (a) is absolutely_summable &
      seq_id (b) is absolutely_summable by A13,A14,A15;
      reconsider a1=a as VECTOR of l1_Space by A16,RSSPACE3:8;
      reconsider b1=b as VECTOR of l1_Space by A16,RSSPACE3:8;
A17: ||.a1+b1.|| <= ||.a1.|| + ||.b1.|| by RSSPACE3:9;
A18: ||.a1+b1.|| = (the norm of l1_Space).(a1+b1) by NORMSP_1:def 1
       .=Sum(abs(seq_id(a1+b1))) by RSSPACE3:def 2,RSSPACE3:def 3
       .=Sum(abs(seq_id(seq_id(a1)+seq_id(b1)))) by RSSPACE3:8
       .=Sum(abs(seq_id(a1)+seq_id(b1))) by RSSPACE:3;
A19: ||.a1.|| = (the norm of l1_Space).(a1) by NORMSP_1:def 1
       .=Sum(abs(seq_id(a1))) by RSSPACE3:def 2,RSSPACE3:def 3;
A20: ||.b1.|| = (the norm of l1_Space).(b1) by NORMSP_1:def 1
       .=Sum(abs(seq_id(b1))) by RSSPACE3:def 2,RSSPACE3:def 3;
A21: Sum(abs(seq_id(a1)+seq_id(b1))) <=
       Sum(abs(seq_id(a1)))+ Sum(abs(seq_id(b1))) by A17,A19,A20,A18;
A25: seq_id(a1)=a by RSSPACE:3;
A26: seq_id(b1)=b by RSSPACE:3;
A28: Sum(abs(a+b)) <= Sum(abs(a)) + Sum(abs(b)) by A21,A25,A26;
A34:(abs(a) + abs(b) is summable) &
       Sum(abs(a) + abs(b))=Sum(abs(a))+Sum(abs(b))       
     by SERIES_1:10,A11,A12;
    deffunc F(Nat) =abs(a .($1)) + abs(b .($1));
    consider c be Real_Sequence such that
q4:for n be Nat holds c.n= F(n) from SEQ_1:sch 1;
q0:now let n be Nat;
q1:abs(a+b).n=abs((a+b).n) by SEQ_1:16;
    hence abs(a+b).n>= 0 by ABSVALUE:5;
q2: abs((a+b).n) =abs((a.n)+(b.n)) by SEQ_1:11; then
q3:abs(a+b).n <= abs(a.n)+abs(b.n) by q1,ABSVALUE:13; thus
q5: c.n=abs(a.n)+abs(b.n) by q4;
q6: abs(a.n)+abs(b.n) = abs(a).n+abs(b.n) by SEQ_1:16
     .=abs(a).n+abs(b).n by SEQ_1:16;thus
     c.n =abs(a).n+abs(b).n by q4,q6;
     thus abs(a+b).n <= c.n by q3,q4;
end;
c = abs(a) + abs(b) by q0,SEQ_1:11;
 hence (a + b) rto_power p is summable by A04,A34,q0,SERIES_1:24;
thus (Sum((a + b) rto_power p)) to_power (1/p)
       <=(Sum((a) rto_power p)) to_power (1/p) +
          (Sum((b) rto_power p)) to_power (1/p) by A28,A07,A08,A09;
end;

theorem LM11:
for a,b be Real_Sequence
for p be Real st 1 <= p &
  (a rto_power p) is summable &
  (b rto_power p) is summable
 holds ((a+b) rto_power p) is summable &
 (Sum((a + b) rto_power p)) to_power (1/p) <=
 (Sum(a rto_power p)) to_power (1/p) +
 (Sum(b rto_power p)) to_power (1/p)
 proof  
     let a,b be Real_Sequence;
     let p be Real such that 
A01: 1<=p and
W1:((a rto_power p) is summable) and 
W2:((b rto_power p) is summable);
    set ap = a rto_power p;
    set bp = b rto_power p;
    set ab= ((a + b) rto_power p);
a0:now let n be Nat;
      a1:  (a rto_power p).n =abs(a .n) to_power p by def_1;
      a2:  (b rto_power p).n =abs(b .n) to_power p by def_1;
      a3:  ((a + b) rto_power p).n =abs((a + b).n) to_power p by def_1
            .=abs(a.n+b.n) to_power p by SEQ_1:11;
            thus ap.n= abs(a .n) to_power p by a1;
            thus bp.n= abs(b .n) to_power p by a2;
            thus ab.n= abs(a.n+b.n) to_power p by a3;
          end;      
L:  p > 1 or p=1 by A01,REAL_1:def 5;
   now per cases by A01,REAL_1:def 5;
      case 
      L1:p > 1;
        thus Sum(ab) to_power (1/p) <= Sum ap to_power (1/p)
         + Sum(bp) to_power (1/p)
         & ab is summable by a0,L1,W1,W2,HOLDER_1:13;
         end;
       case
       L2:p=1;
        thus Sum(ab) to_power (1/p) <= Sum(ap) to_power (1/p)
                 + Sum(bp) to_power (1/p)
         & ab is summable by sub2,L2,W1,W2;
       end;
      end;
    hence thesis;
 end;

theorem LM14:
for p be Real st 1 <= p holds
the_set_of_RealSequences_l^p is lineary-closed
proof
 let p be Real such that
 AS0: p >=1;
 set W = the_set_of_RealSequences_l^p;
  P1:for v,u be VECTOR of Linear_Space_of_RealSequences st
      v in the_set_of_RealSequences_l^p &
      u in the_set_of_RealSequences_l^p
        holds v + u in the_set_of_RealSequences_l^p
 proof
     let v,u be VECTOR of Linear_Space_of_RealSequences such that
AS1: v in W & u in W;
AS4:  seq_id(v +u) rto_power p is summable
    proof
    AS41: seq_id(v) rto_power p is summable by def_2,AS1,AS0;
    AS42: seq_id(u) rto_power p is summable by def_2,AS1,AS0;
    1 > 0; then
QS00: p > 0 by AS0;
    then QS1: p <> 0;
QS01:  1/p > 0 by QS00,REAL_2:127;
    set p1=1/p;
    set vp=seq_id(v) rto_power p;
    set up=seq_id(u) rto_power p;
QS2:  p * (1/p) = (p*1)/p by XCMPLX_1:75
              .= 1 by QS1, XCMPLX_1:60;
Q2:  vp is summable by AS41;
Q3:  up is summable by AS42;
W0: now
        let n be Nat;
     thus Q6S: vp.n=abs((seq_id(v)).n) to_power p by def_1; thus
           Q7S: up.n=abs((seq_id(u)).n) to_power p by def_1; thus
           Q8S: 0 <= abs((seq_id(v)).n) by ABSVALUE:5; thus
           Q9S: 0 < abs((seq_id(v)).n) or 0=abs((seq_id(v)).n) by Q8S;
           thus 0 <= vp.n by Q6S,Q9S,POWER:39,QS00,POWER:def 2; thus
           Q11S: 0 <= abs((seq_id(u)).n) by ABSVALUE:5; thus
           Q12S: 0 < abs((seq_id(u)).n) or 0=abs((seq_id(u)).n)
