         l_norm^p  #);
A3:for x be set holds x is Element of lp
     iff x is Real_Sequence & seq_id(x) rto_power p is summable
   proof
     let x be set;
   A4:   x is Element of the carrier of lp iff x is Element of
          the_set_of_RealSequences_l^p by A2;
     x in the_set_of_RealSequences_l^p iff seq_id(x) rto_power p
          is summable & x in the_set_of_RealSequences by A1,Def2;
     hence thesis by A4,RSSPACE:def 1;
     end;
 A5: for x be set holds x is VECTOR of lp implies x =seq_id(x)
   proof
     let x be set;
A6:     x in the_set_of_RealSequences_l^p iff seq_id(x) rto_power p
          is summable & x in the_set_of_RealSequences by A1,Def2;
  x in the_set_of_RealSequences iff x is Real_Sequence by RSSPACE:def 1;
 hence thesis by A2,A6,RSSPACE:3;
 end;
A7:0.lp = Zeroseq
   proof
A8:     the_set_of_RealSequences_l^p is lineary-closed by A1,Th4;
         thus 0.lp = Zero_(the_set_of_RealSequences_l^p
                                       ,Linear_Space_of_RealSequences)
                 by A2,RLVECT_1:def 2
              .= 0.Linear_Space_of_RealSequences
                        by A8,RSSPACE:def 10
              .= Zeroseq by RLVECT_1:def 2,RSSPACE:def 7;
  end;
A9:for x,y be VECTOR of lp holds x+y =seq_id(x)+seq_id(y)
   proof
     let x,y be VECTOR of lp;
A10:   lp is Subspace of Linear_Space_of_RealSequences by A1,A2,Th9;
        reconsider x1=x as
           VECTOR of Linear_Space_of_RealSequences by A10,RLSUB_1:18;
        reconsider y1=y as
           VECTOR of Linear_Space_of_RealSequences by A10,RLSUB_1:18;
     set L1=Linear_Space_of_RealSequences;
     set W = the_set_of_RealSequences_l^p;
A11:        W is lineary-closed by A1,Th4;
          dom (the add of L1)
            = [:the carrier of L1,the carrier of L1:] by FUNCT_2:def 1; then
          dom ((the add of Linear_Space_of_RealSequences) | [:W,W:])
            =[:W,W:] by RELAT_1:91; then
A12:    [x,y] in dom ((the add of Linear_Space_of_RealSequences)|[:W,W:])
          by A2;
         x+y=(the add of lp).[x,y] by RLVECT_1:def 3
            .= ((the add of Linear_Space_of_RealSequences)|[:W,W:]).[x,y]
              by A2,A11,RSSPACE:def 8
             .=(the add of Linear_Space_of_RealSequences).[x,y]
            by A12,FUNCT_1:70
             .=x1+y1 by RLVECT_1:def 3;
          hence thesis by RSSPACE:4,def 7;
          end;
A13:for r be Real for x be VECTOR of lp holds r*x =r(#)seq_id(x)
   proof
     let r be Real;
     let x be VECTOR of lp;
 A14:   lp is Subspace of Linear_Space_of_RealSequences by A1,A2,Th9;
         reconsider x1=x as
           VECTOR of Linear_Space_of_RealSequences by A14,RLSUB_1:18;
     set L1=Linear_Space_of_RealSequences;
     set W = the_set_of_RealSequences_l^p;
A15: [:REAL,W:] c= [:REAL,the carrier of L1:] by ZFMISC_1:119;
     dom (the Mult of L1) = [:REAL,the carrier of L1:] by FUNCT_2:def 1; then
     dom ((the Mult of Linear_Space_of_RealSequences) | [:REAL,W:])
       =[:REAL,W:] by A15,RELAT_1:91; then
A16:  [r,x] in dom ((the Mult of Linear_Space_of_RealSequences)|[:REAL,W:])
     by A2;
A17:        W is lineary-closed by A1,Th4;
    r*x =(the Mult of lp).[r,x] by RLVECT_1:def 4
        .=((the Mult of Linear_Space_of_RealSequences)|[:REAL,W:]).[r,x]
        by A2,A17,RSSPACE:def 9
        .=(the Mult of Linear_Space_of_RealSequences).[r,x]
        by A16,FUNCT_1:70
        .=r*x1 by RLVECT_1:def 4;
     hence thesis by RSSPACE:5,def 7;
   end;
A18:for x be VECTOR of lp holds (-x =-seq_id(x) & seq_id(-x)=-seq_id(x))
   proof
     let x be VECTOR of lp;
     lp is Subspace of Linear_Space_of_RealSequences by A1,A2,Th9;
     then -x = (-1)*x by RLVECT_1:29
         .= (-1) (#) seq_id(x) by A13
        .= -seq_id(x) by SEQ_1:25;
     hence thesis by A5;
   end;
   A19:for x,y be VECTOR of lp holds x-y =seq_id(x)-seq_id(y)
      proof
     let x,y be VECTOR of lp;
     thus x-y = x+(-y) by RLVECT_1:def 11
                .= seq_id(x)+seq_id(-y) by A9
                .= seq_id(x)+(-seq_id(y)) by A18
                .= seq_id(x)-seq_id(y) by SEQ_1:15;
      end;
   for x be VECTOR of lp holds ||.x.|| =
           ( Sum(seq_id(x) rto_power p) ) to_power (1/p)
   proof
     let x be VECTOR of lp;
     thus ||.x.|| = (the norm of lp).x by NORMSP_1:def 1
       .= ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by A2,Def3;
   end;
   hence thesis by A2,A3,A5,A7,A9,A13,A18,A19;
end;

theorem Th11:
for p be Real st p>=1 holds
for rseq be Real_Sequence
   st (for n be Nat holds rseq.n=0) holds
    (rseq rto_power p is summable
 &  ( Sum(rseq rto_power p) ) to_power (1/p)=0 )
proof
     let p be Real such that
A1:      p>=1;
A2: p>0 by A1;
A3:  1/p > 0 by A2,REAL_2:127;
     let rseq be Real_Sequence such that
A4: for n be Nat holds rseq.n=0;
A5: for n be Nat holds (rseq rto_power p).n=0
   proof
       let n be Nat;
       rseq.n=0 by A4;then
       abs((rseq).n) =0 by ABSVALUE:7;then
 abs((rseq).n) to_power p =0 by A2,POWER:def 2;
       hence thesis by Def1;
     end;
A6:  for m be Nat holds Partial_Sums (rseq rto_power p).m = 0
     proof
       let m be Nat;
       defpred P[Nat] means (rseq rto_power p).$1
       = (Partial_Sums(rseq rto_power p)).$1;
A7:    P[0] by SERIES_1:def 1;
A8:    for k be Nat st P[k] holds P[k+1]
       proof
         let k be Nat such that
A9:      (rseq rto_power p).k
             = (Partial_Sums (rseq rto_power p)).k;
         thus (rseq rto_power p).(k+1) = 0 + (rseq rto_power p).(k+1)
          .= (rseq rto_power p).k + (rseq rto_power p).(k+1) by A5
          .= (Partial_Sums ((rseq rto_power p))).(k+1) by A9,SERIES_1:def 1;
       end;
       for n be Nat holds P[n] from NAT_1:sch 1(A7,A8);
       hence (Partial_Sums ((rseq rto_power p))).m
                = (rseq rto_power p).m .= 0 by A5;
     end;
  Sum(rseq rto_power p) =0 & (rseq rto_power p) is summable
     proof
A10:    for e be real number st 0<e
        ex n be Nat st for m be Nat st n<=m holds
         abs((Partial_Sums (rseq rto_power p)).m-0)<e
       proof
         let e be real number such that
A11:   0<e;
         take 0;
         let m be Nat such that 0<=m;
         abs((Partial_Sums (rseq rto_power p)).m-0)
            = abs(0-0) by A6
           .= 0 by ABSVALUE:def 1;
         hence abs((Partial_Sums (rseq rto_power p)).m-0)<e by A11;
       end; then
A12:    Partial_Sums (rseq rto_power p) is convergent
        by SEQ_2:def 6; then
A13:   (rseq rto_power p) is summable by SERIES_1:def 2;
       lim (Partial_Sums (rseq rto_power p)) =0 by A10,A12,SEQ_2:def 7;
       hence thesis by A13,SERIES_1:def 3;
     end;
     hence thesis by A3,POWER:def 2;
end;

theorem Th12:
  for p be Real st 1 <=p
  for rseq be Real_Sequence st
  (rseq rto_power p is summable
   & ( Sum(rseq rto_power p) ) to_power (1/p)=0 ) holds
  for n be Nat holds rseq.n = 0
proof
   let p be Real such that
A1: 1 <=p;
A2: p>0 by A1; then
A3:  p <> 0;
A4:  1/p > 0 by A2,REAL_2:127;
   let rseq being Real_Sequence such that
A5:rseq rto_power p is summable and
A6: Sum(rseq rto_power p) to_power (1/p)=0;
A7:  for i be Nat holds ((rseq) rto_power p).i >= 0
        proof
         let i be Nat;
A8:      ( (rseq) rto_power p).i
           =abs((rseq).i) to_power p by Def1;
       abs((rseq).i) >=0 by ABSVALUE:5;
          hence ((rseq) rto_power p).i >= 0 by A2,A8,Lm1;
        end;
 A9:(Sum(rseq rto_power p) ) >= 0 by A5,A7,SERIES_1:21;
A10: ( (Sum(rseq rto_power p) ) to_power (1/p)) to_power p
      =(Sum(rseq rto_power p) ) to_power((1/p)*p)
    by A2,A4,A9,HOLDER_1:2
     .=(Sum(rseq rto_power p) ) to_power(1) by A3,XCMPLX_1:107
     .=(Sum(rseq rto_power p) ) by POWER:30;
 0 to_power p =0 by A2,POWER:def 2; then
A11: (Sum(rseq rto_power p) ) = 0 by A6,A10;
A12: for n be Nat holds (rseq rto_power p).n =0
     by A5,A7,A11,RSSPACE:21;
     now
       let n be Nat;
       (rseq rto_power p).n =abs(rseq.n) to_power p by Def1; then
 A13: abs(rseq.n) to_power p =0 by A12;
 A14: abs(rseq.n) >= 0 by ABSVALUE:5;
 A15: (abs(rseq.n) to_power p) to_power (1/p)
        =abs(rseq.n) to_power (p*(1/p)) by A2,A4,A14,HOLDER_1:2
        .=abs(rseq.n) to_power (1) by A3,XCMPLX_1:107
        .=abs(rseq.n) by POWER:30;
        0 to_power (1/p) =0 by A4,POWER:def 2;
        hence rseq.n =0 by A13,A15,ABSVALUE:7;
       end;
   hence thesis;
end;

Lm6:
for p be Real st 1 <=p
  for lp be non empty NORMSTR st lp
     =  NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
for x being Point of lp, a be Real holds
(seq_id(a *(x)) rto_power p )=
((abs(a) to_power p)(#)(seq_id(x) rto_power p))
proof
 let p be Real such that
  A1: 1<= p;
  A2: p > 0 by A1;
 let lp be non empty NORMSTR such that
A3:  lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #);
   let x be Point of lp;
   let a be Real;
now let n1 be set such that A4:n1 in NAT;
reconsider n =n1 as Nat by A4;
 A5:abs(a) >=0 & abs((seq_id(x)).n) >=0 by ABSVALUE:5;
 A6:(seq_id(a *(x)) rto_power p ).n
  =abs(((seq_id(a*x)).n)) to_power p by Def1
  .=abs(((seq_id(a(#)seq_id(x))).n)) to_power p by A1,A3,Th10
.=abs(((a(#)seq_id(x)).n)) to_power p by RSSPACE:3
.=abs(a*(seq_id(x)).n ) to_power p by SEQ_1:13
.=(abs(a) * abs((seq_id(x)).n)) to_power p by ABSVALUE:10
.=abs(a) to_power p * abs((seq_id(x)).n) to_power p by A2,A5,Lm2
.=abs(a) to_power p*(seq_id(x) rto_power p).n by Def1
.=((abs(a) to_power p)(#)(seq_id(x) rto_power p)).n by SEQ_1:13;
reconsider n as set;
thus (seq_id(a *(x)) rto_power p ).n1=
((abs(a) to_power p)(#)(seq_id(x) rto_power p)).n1 by A6;
end;
hence (seq_id(a *(x)) rto_power p )=
((abs(a) to_power p)(#)(seq_id(x) rto_power p)) by SEQ_1:8;
end;

Lm7:
for p be Real st 1 <=p
  for lp be non empty NORMSTR st lp
     = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
for x being Point of lp, a be Real holds
 ( Sum(seq_id(a*x) rto_power p) )
 =(abs(a) to_power p) * Sum(seq_id(x) rto_power p)
 proof
  let p be Real such that
   A1: 1<= p;
 let lp be non empty NORMSTR such that
A2:  lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #);
    let x be Point of lp;
    let a be Real;
A3: (seq_id(x) rto_power p) is summable by A1,A2,Th10;
thus ( Sum(seq_id(a*x) rto_power p) )
 = ( Sum((abs(a) to_power p)(#)(seq_id(x) rto_power p)) ) by A1,A2,Lm6
 .=(abs(a) to_power p) * Sum(seq_id(x) rto_power p) by A3,SERIES_1:13;
 end;

Lm8:
for p be Real st 1 <=p
 for lp be non empty NORMSTR st lp
     = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
for x being Point of lp, a be Real holds
 ( Sum(seq_id(a*x) rto_power p) ) to_power (1/p)
 =(abs(a) )*(Sum(seq_id(x) rto_power p) to_power (1/p))
proof
   let p be Real such that
    A1: 1<= p;
  let lp be non empty NORMSTR such that
 A2:  lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #);
    A3:  1 <= p by A1;
  A4:  0 < p by A3; then
  A5:p <>0;
  A6:(1/p)>0 by A4,REAL_2:127;
  let x be Point of lp;
   let a be Real;
 A7: (seq_id(x) rto_power p) is summable by A1,A2,Th10;
A8: now let n be Nat;
 A9: (seq_id(x) rto_power p).n =abs((seq_id(x)).n) to_power p by Def1;
abs((seq_id(x)).n)>=0 by ABSVALUE:5;
hence (seq_id(x) rto_power p).n >= 0 by A4,A9,Lm1;
 end;
 A10: abs(a) >= 0 by ABSVALUE:5;
 A11:0<=Sum(seq_id(x) rto_power p) by A7,A8,SERIES_1:21;
A12: (abs(a) to_power p) >= 0 by A4,A10,Lm1;
thus ( Sum(seq_id(a*x) rto_power p) ) to_power (1/p)
  =((abs(a) to_power p) * Sum(seq_id(x) rto_power p) )
                                    to_power (1/p) by A1,A2,Lm7
 .= ((abs(a) to_power p) to_power (1/p)) *
      ( Sum(seq_id(x) rto_power p) to_power (1/p)) by A6,A11,A12,Lm2
 .= (abs(a) to_power (p*(1/p))) *
       ( Sum(seq_id(x) rto_power p) to_power (1/p))
      by A4,A6,A10,HOLDER_1:2
.= (abs(a) to_power 1) *
    ( Sum(seq_id(x) rto_power p) to_power (1/p)) by A5,XCMPLX_1:107
.= abs(a)* ( Sum(seq_id(x) rto_power p) to_power (1/p)) by POWER:30;
 end;

theorem Th13:
 for p be Real st 1 <= p
 for lp be non empty NORMSTR st lp
     = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
for x, y being Point of lp, a be Real holds
  ( ||.x.|| = 0 iff x = 0.lp ) &
  0 <= ||.x.|| &
  ||.x+y.|| <= ||.x.|| + ||.y.|| &
  ||.(a*x).|| = abs(a) * ||.x.||
proof
  let p be Real such that
  A1: 1<= p;
  let lp be non empty NORMSTR such that
  A2: lp = NORMSTR (# the_set_of_RealSequences_l^p,
           Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
         l_norm^p  #);
 A3:  1 <= p by A1;
 A4:  0 < p by A3;
   let x, y be Point of lp;
   let a be Real;
   1 > 0 & p >= 1 by A1; then
    p > 0; then
   A5:  1/p > 0 by REAL_2:127;
A6:now assume
A7: ||.x.|| = 0;
A8: ||.x.|| = (the norm of lp). x by NORMSP_1:def 1
            .= ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by A2,Def3;
A9:  seq_id(x) rto_power p is summable by A1,A2,Th10;
A10: for n be Nat holds 0 = (seq_id(x)).n by A1,A7,A8,A9,Th12;
     x in the_set_of_RealSequences by A1,A2,Def2;
     hence x = Zeroseq by A10,RSSPACE:def 6
     .=0.lp by A1,A2,Th10;
   end;
A11:now assume
A12:x=0.lp;
    x=Zeroseq by A1,A2,A12,Th10; then
A13: for n be Nat holds (seq_id(x)).n=0 by RSSPACE:def 6;
thus ||.x.|| = (the norm of lp). x by NORMSP_1:def 1
            .=  ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by A2,Def3
            .= 0 by A1,A13,Th11;
   end;
A14:for n be Nat holds 0 <= (seq_id(x) rto_power p).n
proof
 let n be Nat;
 set xp=seq_id(x) rto_power p;
 A15:   xp.n=abs((seq_id(x)).n) to_power p by Def1;
 A16:  0 <= abs((seq_id(x)).n) by ABSVALUE:5;
 A17:  0 < abs((seq_id(x)).n) or 0=abs((seq_id(x)).n) by A16; thus
   0 <= xp.n by A4,A15,A17,POWER:39,def 2;
   end;
   seq_id(x) rto_power p is summable by A1,A2,Def2;then
 A18: 0<= ( Sum(seq_id(x) rto_power p) ) by A14,SERIES_1:21;
 A19:( Sum(seq_id(x) rto_power p) ) to_power (1/p) >=0 by A5,A18,Lm1;
     ( Sum(seq_id(x) rto_power p) ) to_power (1/p) = (the norm of lp). x
     by A2,Def3
       .= ||.x.|| by NORMSP_1:def 1; then
A20:     ||.x.|| >=0 by A19;
A21:now
 A22:||.x.|| = (the norm of lp). x by NORMSP_1:def 1
            .= ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by A2,Def3;
 A23:||.y.|| = (the norm of lp). y by NORMSP_1:def 1
           .= ( Sum(seq_id(y) rto_power p) ) to_power (1/p) by A2,Def3;
A24:((seq_id(x))+(seq_id(y)))
=seq_id((seq_id(x))+(seq_id(y))) by RSSPACE:3
.=seq_id(x+y) by A1,A2,Th10;
A25: Sum((((seq_id(x))+(seq_id(y))) rto_power p)) to_power (1/p)
      = (the norm of lp).(x+y) by A2,A24,Def3
     .= ||.x + y.|| by NORMSP_1:def 1;
A26:(seq_id(x) rto_power p) is summable by A1,A2,Th10;
(seq_id(y) rto_power p) is summable by A1,A2,Th10;
hence ||.x + y.|| <= ||.x.|| + ||.y.|| by A1,A22,A23,A25,A26,Th3;
end;
 A27:||.x.|| = (the norm of lp). x by NORMSP_1:def 1
            .= ( Sum(seq_id(x) rto_power p) ) to_power (1/p) by A2,Def3;
 ||.(a*x).|| = (the norm of lp). (a*x) by NORMSP_1:def 1
 .= ( Sum(seq_id(a*x) rto_power p) ) to_power (1/p) by A2,Def3
 .=(abs(a))* ||.x.|| by A1,A2,A27,Lm8;
 hence thesis by A6,A11,A20,A21;
end;

theorem Th14:
for p be Real st p >= 1
  for lp be non empty NORMSTR
     st lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #) holds
 lp is RealNormSpace-like
 proof
 let p be Real such that
A1: p >=1;
 let lp be non empty NORMSTR such that
A2: lp = NORMSTR (# the_set_of_RealSequences_l^p,
       Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
       Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
     l_norm^p #);
     for x, y being Point of lp, a be Real holds
    ( ||.x.|| = 0 iff x = 0.lp ) &
    0 <= ||.x.|| &
    ||.x+y.|| <= ||.x.|| + ||.y.|| &
    ||.(a*x).|| = abs(a) * ||.x.|| by A1,A2,Th13;
 hence thesis by NORMSP_1:def 2;
 end;

theorem Th15:
 for p be Real st p >= 1
 for lp be non empty NORMSTR st lp
      = NORMSTR (# the_set_of_RealSequences_l^p,
        Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
        Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
        Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
      l_norm^p #) holds
 lp is RealNormSpace by Th9,Th14;

 reserve NRM for non empty RealNormSpace,
         seq for sequence of NRM;

Lm9:
for c be Real, seq be Real_Sequence st seq is convergent
for rseq be Real_Sequence st
 ( for i be Nat holds rseq .i = abs(seq.i-c) ) holds
 ( rseq is convergent & lim rseq = abs(lim seq-c) ) by RSSPACE3:1;

Lm10:
for p be Real st 0 < p
for c be Real, seq be Real_Sequence st seq is convergent
for rseq be Real_Sequence st
 ( for i be Nat holds rseq .i = (abs(seq.i -c)) to_power p )
 holds
 ( rseq is convergent & lim rseq = (abs(lim seq-c)) to_power p )
proof
  let p be Real such that
A1:0 <p;
  let c be Real;
  let seq be Real_Sequence such that
A2:seq is convergent;
  let rseq be Real_Sequence such that
A3: ( for i be Nat holds rseq .i = (abs(seq.i -c)) to_power p );
 deffunc F(set) = abs(seq.$1-c);
   consider b be Real_Sequence such that
A4:for n be Nat holds b.n=F(n) from SEQ_1:sch 1;
A5:b is convergent by A2,A4,Lm9;
A6:lim b = abs(lim seq - c) by A2,A4,Lm9;
A7:for n be Nat holds 0 <=b.n
  proof
  let n be Nat;
  b.n=abs(seq.n-c) by A4;
  hence thesis by ABSVALUE:5;
  end;
  for n be Nat holds rseq.n=(b.n) to_power p
  proof
    let n be Nat;
    rseq.n=(abs(seq.n -c)) to_power p by A3
         .=(b.n) to_power p by A4;
    hence thesis;
  end;
  hence thesis by A1,A5,A6,A7,HOLDER_1:10;
end;

Lm11:
for p be Real st 0 < p
for c be Real, seq, seq1 be Real_Sequence st (seq is convergent
& seq1 is convergent)
for rseq be Real_Sequence st
 ( for i be Nat holds rseq .i = (abs(seq.i -c)) to_power p +seq1.i )
 holds
 ( rseq is convergent & lim rseq = (abs(lim seq-c)) to_power p + lim seq1 )
proof
  let p be Real such that
A1:0 < p;
  let c be Real;
  let seq,seq1 be Real_Sequence such that
A2:seq is convergent and
A3:seq1 is convergent;
  let rseq be Real_Sequence such that
A4:  for i be Nat holds rseq .i =( (abs(seq.i -c)) to_power p ) + seq1.i;
 deffunc F(set) = abs(seq.$1-c) to_power p;
   consider b be Real_Sequence such that
A5:for n be Nat holds b.n=F(n) from  SEQ_1:sch 1;
A6:b is convergent by A1,A2,A5,Lm10;
A7:lim b = (abs(lim seq-c)) to_power p by A1,A2,A5,Lm10;
now let n be Nat;thus
  rseq .n = ( (abs(seq.n -c)) to_power p ) + seq1.n by A4
           .=b.n + seq1.n by A5;
  end; then
 A8:rseq = b + seq1 by SEQ_1:11;
 A9: lim rseq
     =(abs(lim seq-c)) to_power p + lim seq1 by A3,A6,A7,A8,SEQ_2:20;
     thus rseq is convergent by A3,A6,A8,SEQ_2:19;
     thus lim rseq = (abs(lim seq-c)) to_power p + lim seq1 by A9;
end;

Lm12:
 for e be Real, seq be Real_Sequence st ( seq is convergent &
  ex k be Nat st (for i be Nat st k <= i holds seq.i <=e )) holds
   lim seq <=e by RSSPACE2:6;

Lm13:
for a,b be Real_Sequence holds a=a+b-b
proof
 let a,b be Real_Sequence;
 now let n be Nat;
 A1: a+b-b = a+b+-b by SEQ_1:15
   .=a+(b+(-b)) by SEQ_1:20;
   (a+(b+(-b))).n=a.n+(b+(-b)).n by SEQ_1:11
   .=a.n+(b.n+(-b).n) by SEQ_1:11
   .=a.n+(b.n+(-b.n)) by SEQ_1:14
   .=a.n;
   hence (a+b-b).n=a.n by A1;
 end;
hence thesis by FUNCT_2:113;
end;

Lm14:
for p be Real st p >= 1
for a,b be Real_Sequence st (a rto_power p is summable &
    b rto_power p is summable) holds (a + b) rto_power p is summable
proof
      let p be Real such that
A1:p >= 1;
      let a,b be Real_Sequence such that
A2: a rto_power p is summable and
A3: b rto_power p is summable;
      reconsider a1=a, b1=b as set;
A4:a1 in the_set_of_RealSequences by RSSPACE:def 1;
A5:b1 in the_set_of_RealSequences by RSSPACE:def 1;
A6:seq_id(a1) = a by A4,RSSPACE:def 2;
A7:seq_id(b1) = b by A5,RSSPACE:def 2;
A8:seq_id(a1) rto_power p is summable by A2,A6;
A9:seq_id(b1) rto_power p is summable by A3,A7;
A10:a1 in the_set_of_RealSequences_l^p by A1,A4,A8,Def2;
A11:b1 in the_set_of_RealSequences_l^p by A1,A5,A9,Def2;
     reconsider a1 as VECTOR of Linear_Space_of_RealSequences by A10;
     reconsider b1 as VECTOR of Linear_Space_of_RealSequences by A11;
     the_set_of_RealSequences_l^p is lineary-closed by A1,Th4;
     then a1+b1 in the_set_of_RealSequences_l^p by A10,A11,RLSUB_1:def 1;
     then
A12:seq_id(a1+b1) rto_power p is summable by A1,Def2;
     seq_id(a1+b1)=seq_id(seq_id(a1)+seq_id(b1)) by RSSPACE:4,def 7
     .=a+b by A6,A7,RSSPACE:3;
     hence (a+b) rto_power p is summable by A12;
end;

theorem Th16:
 for p be Real st 1 <=p
 for lp be RealNormSpace
    st lp = NORMSTR (# the_set_of_RealSequences_l^p,
      Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
      Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
      Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
      l_norm^p #) holds
 for vseq be sequence of lp st vseq is Cauchy_sequence_by_Norm
 holds vseq is convergent
proof
  let p be Real such that
  A1: 1<= p;
 A2:  p >0 by A1;
 then A3: p <> 0;
  A4:  1/p > 0 by A2,REAL_2:127;
  A5:  p * (1/p) = (p*1)/p by XCMPLX_1:75
                .= 1 by A2,XCMPLX_1:60;
   let lp be RealNormSpace such that
     A6:  lp = NORMSTR (# the_set_of_RealSequences_l^p,
           Zero_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Add_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
           Mult_(the_set_of_RealSequences_l^p,Linear_Space_of_RealSequences),
         l_norm^p #);
 let vseq be sequence of lp such that
 A7:vseq is Cauchy_sequence_by_Norm;
   defpred P[set,set] means
        ex i be Nat st $1=i &
     ex rseqi be Real_Sequence st
      (for n be Nat holds rseqi.n=(seq_id(vseq.n)).i) &
     rseqi is convergent & $2 = lim rseqi;
A8:for x be set st x in NAT ex y be set st y in REAL & P[x,y]
   proof
     let x be set such that
A9: x in NAT;
     reconsider i=x as Nat by A9;
     deffunc F(Nat) = (seq_id(vseq.$1)).i;
     consider rseqi be Real_Sequence such that
A10:  for n be Nat holds rseqi.n= F(n) from SEQ_1:sch 1;
A11:  rseqi is convergent
      proof
      now
         let e1 be real number such that
A12:     e1 > 0;
         reconsider e=e1 as Real by XREAL_0:def 1;
    thus ex k be Nat st
          for m be Nat st k <= m holds abs(rseqi.m -rseqi.k) < e1
          proof
           consider k be Nat such that
A13:       for n, m be Nat st ( n >= k & m >= k ) holds
           ||.(vseq.n) - (vseq.m).|| < e by A7,A12,RSSPACE3:10;
 for m being Nat st k <= m holds abs(rseqi.m-rseqi.k) < e
           proof
            let m be Nat such that
A14:        k<=m;
A15:        0 <= ||.(vseq.m) - (vseq.k).|| by NORMSP_1:8;
A16:       ||.(vseq.m) - (vseq.k).||
            = (the norm of lp).((vseq.m)-(vseq.k)) by NORMSP_1:def 1
           .= ( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) )
             to_power (1/p) by A6,Def3; then
             ( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) ) to_power
                                               (1/p) < e by A13,A14; then
A17:        (( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) ) to_power
                                (1/p)) to_power p < e to_power p
                              by A2,A15,A16,Th1;
 A18: (seq_id((vseq.m)-(vseq.k)) rto_power p) is summable by A1,A6,Th10;

A19:now
 let i be Nat;
   A20:  (seq_id((vseq.m)-(vseq.k)) rto_power p).i
             =abs((seq_id((vseq.m)-(vseq.k))).i) to_power p
           by Def1;
  abs((seq_id((vseq.m)-(vseq.k))).i) >=0 by ABSVALUE:5;
 hence (seq_id((vseq.m)-(vseq.k)) rto_power p).i >= 0 by A2,A20,Lm1;
end;
A21:0<=Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) by A18,A19,SERIES_1:21;
A22:    (( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) ) to_power
                                (1/p)) to_power p
         =( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) )
           to_power ((1/p)*p) by A2,A4,A21,HOLDER_1:2
        .= ( Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) ) by A5,POWER:30;
A23:(e to_power p) to_power (1/p)
        = e to_power ((1/p) * p) by A12,POWER:38
        .= e to_power(1) by A3,XCMPLX_1:107
        .=e by POWER:30;
A24: Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) < e to_power p by A17,A22;
 A25:(seq_id((vseq.m)-(vseq.k)) rto_power p).i <=
      Sum(seq_id((vseq.m)-(vseq.k)) rto_power p) by A18,A19,RSSPACE2:4;
 A26: abs((seq_id((vseq.m)-(vseq.k))).i) >=0 by ABSVALUE:5;
A27: now
       let a,b,c be Real such that A28:a =0 & b>0 & c>0;
       a to_power b =0 by A28,POWER:def 2;
       hence (a to_power b) to_power c =0 by A28,POWER:def 2;
       end;
 A29: now
       let a,b,c be Real such that A30:a =0 & b>0 & c>0;
       b*c >0 by A30,REAL_2:122;
       hence a to_power (b *c) =0 by A30,POWER:def 2;
       end;
  A31: now
       let a,b,c be Real such that A32:a =0 & b>0 & c>0;
       thus (a to_power b) to_power c
         = 0 by A27,A32
        .= a to_power (b *c) by A29,A32;
       end;
  A33:for a,b,c be Real st a >0 & b>0 & c>0
       holds (a to_power b) to_power c =
       a to_power (b*c) by POWER:38;
  A34:for a,b,c be Real st (a >0 or a=0) & b>0 & c>0
       holds (a to_power b) to_power c =
       a to_power (b*c) by A31,A33;
 A35: for a,b,c be Real st a>=0 & b>0 & c>0 holds
      (a to_power b) to_power c = a to_power (b*c)
      proof
       let a,b,c be Real such that
       A36:a>=0 & b>0 & c>0;
       ((a >0) or (a=0)) & b>0 & c>0 by A36;
      hence (a to_power b) to_power c = a to_power (b*c) by A34;
       end;
 A37: (seq_id((vseq.m)-(vseq.k)) rto_power p).i
     =abs((seq_id((vseq.m)-(vseq.k))).i) to_power p by Def1;
 A38:((seq_id((vseq.m)-(vseq.k)) rto_power p).i) to_power (1/p)
    =abs((seq_id((vseq.m)-(vseq.k))).i) to_power (1) by A2,A4,A5,A26,A35,A37
    .=abs((seq_id((vseq.m)-(vseq.k))).i) by POWER:30;
A39:  (seq_id((vseq.m)-(vseq.k)) rto_power p).i >= 0 by A19;
    (seq_id((vseq.m)-(vseq.k)) rto_power p).i <e to_power p
      by A24,A25,AXIOMS:22;
  then ((seq_id((vseq.m)-(vseq.k)) rto_power p).i) to_power (1/p)
  < (e to_power p) to_power (1/p) by A4,A39,Th1; then
A40:   abs((seq_id((vseq.m)-(vseq.k))).i) <e by A23,A38;
 A41:  rseqi.m=(seq_id(vseq.m)).i & rseqi.k=(seq_id(vseq.k)).i by A10;
   reconsider vsem = (vseq.m), vsek = (vseq.k) as VECTOR of lp;
(vsem)-(vsek) =(seq_id(vsem)) - (seq_id(vsek)) by A1,A6,Th10; then
  abs((seq_id((vseq.m)-(vseq.k))).i)
   =abs((seq_id(vseq.m)-seq_id(vseq.k)).i) by RSSPACE:3
  .=abs((seq_id(vseq.m)+-seq_id(vseq.k)).i) by SEQ_1:15
  .=abs((seq_id(vseq.m)).i+(-(seq_id(vseq.k))).i) by SEQ_1:11
  .=abs((seq_id(vseq.m)).i +-(seq_id(vseq.k)).i) by SEQ_1:14
  .=abs(rseqi.m -rseqi.k) by A41;
 hence abs(rseqi.m -rseqi.k) < e by A40;
end;
hence thesis;
end;
end;
hence rseqi is convergent by SEQ_4:58;
end;
   take y = lim rseqi;
   thus thesis by A10,A11;
 end;
   consider f be Function of NAT,REAL such that
A42:for x be set st x in NAT holds P[x,f.x] from FUNCT_2:sch 1(A8);
   reconsider tseq=f as Real_Sequence;
A43:now
      let i be Nat;
      reconsider x=i as set;
      ex i0 be Nat st x=i0 &
       ex rseqi be Real_Sequence st
        ( for n be Nat holds rseqi.n=(seq_id(vseq.n)).i0 ) &
       rseqi is convergent &
       f.x=lim rseqi by A42;
       hence ex rseqi be Real_Sequence st
        ( for n be Nat holds rseqi.n=(seq_id(vseq.n)).i ) &
        rseqi is convergent &
        tseq.i=lim rseqi;
      end;
A44:for e be Real st e >0
   ex k be Nat st for n be Nat st n >= k holds
    ((seq_id(tseq)-seq_id(vseq.n)) rto_power p ) is summable &
      Sum((seq_id(tseq)-seq_id(vseq.n)) rto_power p) < e
   proof
       let e2 be Real such that
A45: e2 >0;
       set e=e2/2;
A46: e >0 by A45,SEQ_2:3;
A47: e2 >e by A45,SEQ_2:4;
       set e1=e to_power (1/p);
A48: e1>0 by A46,POWER:39;
       consider k be Nat such that
A49: for n, m be Nat st ( n >= k & m >= k ) holds
         ||.(vseq.n) - (vseq.m).|| < e1 by A7,A48,RSSPACE3:10;
A50: for m,n be Nat st ( n >= k & m >= k ) holds
        ((seq_id((vseq.m)-(vseq.n)) rto_power p) is summable &
           Sum( (seq_id((vseq.m)-(vseq.n)) rto_power p) ) < e &
    for i be Nat holds 0 <=  ((seq_id((vseq.m)-(vseq.n))) rto_power p).i)
        proof
        let m,n be Nat such that
A51:  n >= k & m >= k;
A52:  (seq_id((vseq.m)-(vseq.n)) rto_power p) is summable by A1,A6,Th10;
A53:       for i be Nat holds (seq_id((vseq.m)-(vseq.n)) rto_power p).i >= 0
       proof
         let i be Nat;
A54:      (seq_id((vseq.m)-(vseq.n)) rto_power p).i
         =abs((seq_id((vseq.m)-(vseq.n))).i) to_power p by Def1;
      abs((seq_id((vseq.m)-(vseq.n))).i) >=0 by ABSVALUE:5;
        hence
        (seq_id((vseq.m)-(vseq.n)) rto_power p).i >= 0 by A2,A54,Lm1;
       end;
       ||.(vseq.m) - (vseq.n).|| < e1 by A49,A51; then
     (the norm of lp).((vseq.m)-(vseq.n))<e1 by NORMSP_1:def 1;
     then
A55:  (( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) )
                                       to_power (1/p)) < e1 by A6,Def3;
 A56: (seq_id((vseq.m)-(vseq.n)) rto_power p) is summable by A1,A6,Th10;
 A57:0<=Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) by A53,A56,SERIES_1:21;
 A58:    (( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) ) to_power
                                 (1/p)) to_power p
          =( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) )
            to_power ((1/p)*p) by A2,A4,A57,HOLDER_1:2
        .= ( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) ) by A5,POWER:30;
 A59:e1 to_power p = e to_power ((1/p) * p) by A46,POWER:38
     .= e to_power(1) by A3,XCMPLX_1:107
     .= e by POWER:30;
  (( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) ) to_power
                                 (1/p)) >=0 by A4,A57,Lm1;
 then ( Sum(seq_id((vseq.m)-(vseq.n)) rto_power p) ) < e by A2,A55,A58,A59,Th1;
hence thesis by A52,A53;
 end;
A60:  for n be Nat for i be Nat holds
      for rseq be Real_Sequence st
       ( for m be Nat holds
         rseq.m = Partial_Sums ((seq_id((vseq.m)-(vseq.n))) rto_power p).i )
      holds
       ( rseq is convergent & lim rseq =
              Partial_Sums ((seq_id(tseq)-seq_id(vseq.n)) rto_power p) .i )
     proof
       let n be Nat;
A61:   for m,k be Nat holds
