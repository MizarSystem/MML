           let rseq be Real_Sequence such that
A63:     for m be Nat holds
           rseq.m= Partial_Sums
                 ((seq_id((vseq.m)-(vseq.n))) rto_power p).0;
           thus
           rseq is convergent &
           lim rseq = Partial_Sums
                  ((seq_id(tseq)-seq_id(vseq.n)) rto_power p).0
           proof
            consider rseq0 be Real_Sequence such that
A64:       ( for m be Nat holds rseq0.m=(seq_id(vseq.m)).0 ) &
            rseq0 is convergent &
            tseq.0=lim rseq0 by A43;
A65:       for m being Nat holds rseq.m =
            abs(rseq0.m -(seq_id(vseq.n)).0) to_power p
            proof
               let m be Nat;
              rseq.m =Partial_Sums
                 ((seq_id((vseq.m)-(vseq.n))) rto_power p).0 by A63
              .= ((seq_id((vseq.m)-(vseq.n))) rto_power p).0
                                                        by SERIES_1:def 1
              .= ((seq_id(vseq.m)-seq_id(vseq.n)) rto_power p).0 by A61
              .=((seq_id(vseq.m)+-seq_id(vseq.n)) rto_power p).0 by SEQ_1:15
              .=abs((seq_id(vseq.m)+-seq_id(vseq.n)).0) to_power p by Def1
              .=abs((seq_id(vseq.m)).0+(-seq_id(vseq.n)).0) to_power p
                                                        by SEQ_1:11
              .=abs((seq_id(vseq.m)).0+-(seq_id(vseq.n)).0) to_power p
