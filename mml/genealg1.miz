then A21:  n5 >= len crossover(p1,p2,n1,n3,n4) by Def1;
         crossover(p1,p2,n1,n2,n3,n4,n5)
     =crossover(crossover(p1,p2,n1,n3,n4),
      crossover(p2,p1,n1,n2,n3,n4),n5) by A20,Th33;
