     .=crossover(crossover(p1,p2,n3,n2,n4),crossover(p2,p1,n3,n2,n4),n1);
     hence thesis;
   end;
A13:crossover(p1,p2,n1,n2,n3,n4) = crossover(p1,p2,n3,n4,n1,n2)
   proof
     set q1=crossover(p1,p2,n3,n1);
