     =crossover(crossover(p2,p1,n1),crossover(p2,p1,n1,0,0,0),n5) by Th31
     .=crossover(crossover(p2,p1,n1),crossover(p1,p2,n1),n5) by Th31;
     hence thesis;
   end;
A8:crossover(p1,p2,n1,0,0,n4,0)=crossover(p2,p1,n1,n4)
