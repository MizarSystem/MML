  end;
A10: crossover(p1,p2,n1,n2,n3,n4) = crossover(p1,p2,n2,n4,n1,n3)
  proof
    set q2=crossover(p2,p1,n2,n1);
    set q1=crossover(p1,p2,n2,n1);
    crossover(p1,p2,n1,n2,n3,n4) =crossover(crossover(p1,p2,n2,n1,n3),
