        thus *(x,+(z,y)) = [0,x999 *' (y99 -' z99)] by A42,A44,ZFMISC_1:33
          .= (x9 *' z9) - (x99 *' y9) by A26,A32,A34,A29,A38,A41,A43,ARYTM_1
        :27
          .= +(*(x,z),*(x,y)) by A33,A36,A37,ZFMISC_1:33;
      end;
