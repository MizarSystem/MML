theorem Th112:
  for f being PartFunc of {},Y holds f is total
 proof let f be PartFunc of {},Y; thus dom f = {} by Th54; end;

theorem Th113:
  for f being Function st <:f,X,Y:> is total holds X c= dom f
 proof let f be Function such that
A1:  dom <:f,X,Y:> = X;
   let x such that
A2:  x in X;
