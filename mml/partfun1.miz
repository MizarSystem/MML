   hence [x,x] in R by A2,A3,A6,RELAT_2:def 8;
  end;
end;

registration let X;
 cluster id X -> symmetric antisymmetric transitive;
 coherence by Lm5;
end;

definition let X;
 redefine func id X -> total Relation of X;
 coherence by Lm4,Lm6;
end;
