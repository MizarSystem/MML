theorem
  for s1, s3, s4, l being real number st
  s1 <= s3 & s1 < s4 & 0 <= l & l <= 1 holds s1 <= (1-l) * s3+l*s4
proof
  let s1, s3, s4, l be real number;
  assume
A1: s1 <= s3 & s1 < s4 & 0 <= l & l <= 1;
  now per cases;
    suppose l=0;
      hence thesis by A1;
    end;
    suppose l=1;
      hence thesis by A1;
    end;
    suppose
A2:   not(l=0 or l=1);
      then
A3:   l>0 & l<1 by A1,XXREAL_0:1;
A4:   l*s1<l*s4 by A1,A2,XREAL_1:70;
      1-l>0 by A3,XREAL_1:52;
      then
A5:   (1-l)*s1<=(1-l)*s3 by A1,XREAL_1:66;
      (1-l)*s1+l*s1=1 * s1;
      hence thesis by A4,A5,XREAL_1:10;
    end;
  end;
  hence thesis;
end;

