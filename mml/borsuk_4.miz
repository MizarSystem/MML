theorem Th32:
  for A being Subset of I[01],
  a, b being real number st a <= b & A = [.a,b.] holds 0 <= a & b <= 1
proof
  let A be Subset of I[01], a, b be real number;
  assume a <= b & A = [.a,b.];
  then a in A & b in A by XXREAL_1:1;
  hence thesis by BORSUK_1:86;
end;

theorem Th33:
  for A, B being Subset of I[01], a, b, c being real number st a < b & b < c &
  A = [.a,b.[ & B = ].b,c.] holds A, B are_separated
proof
  let A, B be Subset of I[01], a, b, c be real number;
  assume
A1: a < b & b < c & A = [.a,b.[ & B = ].b,c.];
  then Cl A = [.a,b.] by Th24;
  hence Cl A misses B by A1,XXREAL_1:90;
  Cl B = [.b,c.] by A1,Th23;
  hence thesis by A1,XXREAL_1:95;
end;

