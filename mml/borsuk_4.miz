theorem Th17:
  for A being Subset of I[01],
  a, b being real number st a < b & A = ].a,b.[ holds
  [.a,b.] c= the carrier of I[01]
proof
  let A be Subset of I[01], a, b be real number;
  assume
A1: a < b;
  assume
A2: A = ].a,b.[;
  then
A3: 0 <= a by A1,BORSUK_1:83,XXREAL_1:51;
  b <= 1 by A1,A2,BORSUK_1:83,XXREAL_1:51;
  hence thesis by A3,BORSUK_1:83,XXREAL_1:34;
end;

theorem Th18:
  for A being Subset of I[01],
  a, b being real number st a < b & A = ].a,b.] holds
  [.a,b.] c= the carrier of I[01]
proof
  let A be Subset of I[01], a, b be real number;
  assume
A1: a < b;
A2: ].a,b.[ c= ]. a,b .] by XXREAL_1:21;
  assume A = ].a,b.];
  then
A3: ].a,b.[ c= [. 0,1 .] by A2,BORSUK_1:83,XBOOLE_1:1;
  then
A4: 0 <= a by A1,XXREAL_1:51;
  b <= 1 by A1,A3,XXREAL_1:51;
  hence thesis by A4,BORSUK_1:83,XXREAL_1:34;
end;

theorem Th19:
  for A being Subset of I[01],
  a, b being real number st a < b & A = [.a,b.[ holds
  [.a,b.] c= the carrier of I[01]
proof
  let A be Subset of I[01], a, b be real number;
  assume
A1: a < b;
A2: ].a,b.[ c= [. a,b .[ by XXREAL_1:22;
  assume A = [.a,b.[;
  then
A3: ].a,b.[ c= [. 0,1 .] by A2,BORSUK_1:83,XBOOLE_1:1;
  then
A4: 0 <= a by A1,XXREAL_1:51;
  b <= 1 by A1,A3,XXREAL_1:51;
  hence thesis by A4,BORSUK_1:83,XXREAL_1:34;
end;

theorem Th20:
  for a, b being real number st a <> b holds Cl ].a,b.] = [.a,b.]
proof
  let a, b be real number;
  assume
A1: a <> b;
  ].a,b.] c= [.a,b.] by XXREAL_1:23;
  then
A2: Cl ].a,b.] c= [.a,b.] by PSCOMP_1:32;
  ].a,b.[ c= ].a,b.] by XXREAL_1:21;
  then Cl ].a,b.[ c= Cl ].a,b.] by PSCOMP_1:37;
  then [.a,b.] c= Cl ].a,b.] by A1,TOPREAL6:82;
  hence thesis by A2,XBOOLE_0:def 10;
end;

theorem Th21:
  for a, b being real number st a <> b holds Cl [.a,b.[ = [.a,b.]
proof
  let a, b be real number;
  assume
A1: a <> b;
  [.a,b.[ c= [.a,b.] by XXREAL_1:24;
  then
A2: Cl [.a,b.[ c= [.a,b.] by PSCOMP_1:32;
  ].a,b.[ c= [.a,b.[ by XXREAL_1:22;
  then Cl ].a,b.[ c= Cl [.a,b.[ by PSCOMP_1:37;
  then [.a,b.] c= Cl [.a,b.[ by A1,TOPREAL6:82;
  hence thesis by A2,XBOOLE_0:def 10;
end;

theorem
  for A being Subset of I[01],
  a, b being real number st a < b & A = ].a,b.[ holds Cl A = [.a,b.]
proof
  let A be Subset of I[01], a, b be real number;
  assume
A1: a < b & A = ].a,b.[;
  then
A2: Cl ].a,b.[ = [.a,b.] by TOPREAL6:82;
  reconsider A1 = ].a,b.[ as Subset of R^1 by TOPMETR:24;
  Cl A = (Cl A1) /\ [#]I[01] by A1,PRE_TOPC:47
    .= [.a,b.] /\ [#]I[01] by A2,TOPREAL6:80
    .= [.a,b.] by A1,Th17,XBOOLE_1:28;
  hence thesis;
end;

theorem Th23:
  for A being Subset of I[01],
  a, b being real number st a < b & A = ].a,b.] holds Cl A = [.a,b.]
proof
  let A be Subset of I[01], a, b be real number;
  assume
A1: a < b & A = ].a,b.];
  then
A2: Cl ].a,b.] = [.a,b.] by Th20;
  reconsider A1 = ].a,b.] as Subset of R^1 by TOPMETR:24;
  Cl A = (Cl A1) /\ [#]I[01] by A1,PRE_TOPC:47
    .= [.a,b.] /\ [#]I[01] by A2,TOPREAL6:80
    .= [.a,b.] by A1,Th18,XBOOLE_1:28;
  hence thesis;
end;

theorem Th24:
