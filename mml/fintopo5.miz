theorem
  for n,m being non zero Nat holds FTSS2(n,m) is symmetric
  proof let n,m be non zero Nat;
    for x, y being Element of FTSS2(n,m) holds
      y in U_FT x implies x in U_FT y
    proof let x, y be Element of FTSS2(n,m);
      assume A1: y in U_FT x;
      consider xu,xv being set such that
  A2: xu in Seg n & xv in Seg m & x=[xu,xv] by ZFMISC_1:def 2;
      reconsider i=xu, j=xv as Nat by A2;
A3:   FTSL1 m = FT_Space_Str(# Seg m,Nbdl1 m #) by FINTOPO4:def 4;
A4:   FTSL1 n = FT_Space_Str(# Seg n,Nbdl1 n #) by FINTOPO4:def 4;
      reconsider pi=i as Element of FTSL1 n by A2,A4;
      reconsider pj=j as Element of FTSL1 m by A2,A3;
      consider yu,yv being set such that
  A5: yu in Seg n & yv in Seg m & y=[yu,yv] by ZFMISC_1:def 2;
      reconsider i2=yu, j2=yv as Nat by A5;
      reconsider pi2=i2 as Element of FTSL1 n by A5,A4;
      reconsider pj2=j2 as Element of FTSL1 m by A5,A3;
      y in (the Nbd of FTSS2(n,m)).x by FIN_TOPO:def 1,A1;then
      y in [:{i}, (Nbdl1 m).j:] \/ [:(Nbdl1 n).i,{j}:]
                                         by Def4,A2;then
      A6: y in [:{i}, (Nbdl1 m).j:] or y in [:(Nbdl1 n).i,{j}:]
                                           by XBOOLE_0:def 2;
      now per cases by A6;
        case y in [:{i}, (Nbdl1 m).j:];then
          consider y1,y2 being set such that
           A7: y1 in {i} & y2 in (Nbdl1 m).j & y=[y1,y2]
                                          by ZFMISC_1:def 2;
          A8: y1 = i by A7,TARSKI:def 1;
          A9: y1=yu & y2=yv by A7,A5,ZFMISC_1:33;then
          A10: pj2 in U_FT pj by FIN_TOPO:def 1,A3,A7;
          FTSL1 m is symmetric by FINTOPO4:19;then
          pj in U_FT pj2 by A10,FIN_TOPO:def 14;then
          A11: j in (Nbdl1 m).j2 by A3,FIN_TOPO:def 1;
          i in {i2} by TARSKI:def 1,A9,A8;
          hence x in [:{i2}, (Nbdl1 m).j2:] by A11,ZFMISC_1:def 2,A2;
        end;
        case y in [:(Nbdl1 n).i,{j}:];then
          consider y1,y2 being set such that
      A12: y1 in (Nbdl1 n).i & y2 in {j} & y=[y1,y2] by ZFMISC_1:def 2;
      A13: y2 = j by A12,TARSKI:def 1;
      A14: y1=yu & y2=yv by A12,A5,ZFMISC_1:33;then
      A15: pi2 in U_FT pi by FIN_TOPO:def 1,A4,A12;
          FTSL1 n is symmetric by FINTOPO4:19;then
          pi in U_FT pi2 by A15,FIN_TOPO:def 14;then
      A16: i in (Nbdl1 n).i2 by A4,FIN_TOPO:def 1;
          j in {j2} by TARSKI:def 1,A14,A13;
          hence x in [:(Nbdl1 n).i2, {j2}:] by A16,ZFMISC_1:def 2,A2;
        end;
     end;then
     x in [:{i2}, (Nbdl1 m).j2:] \/ [:(Nbdl1 n).i2, {j2}:]
                              by XBOOLE_0:def 2;then
     x in (the Nbd of FTSS2(n,m)).y by Def4,A5;
     hence x in U_FT y by FIN_TOPO:def 1;
   end;
   hence thesis by FIN_TOPO:def 14;
