theorem
  for n being non zero Nat
   ex h being Function of FTSS2(n,1), FTSL1 n st h is_homeomorphism
  proof let n be non zero Nat;
    defpred P[set,set] means [$2,1]=$1;
A1: FTSL1 n = FT_Space_Str(# Seg n,Nbdl1 n #) by FINTOPO4:def 4;
A2: for x be set st x in the carrier of FTSS2(n,1)
    ex y be set st y in the carrier of FTSL1 n & P[x,y]
    proof let x be set;
      assume x in the carrier of FTSS2(n,1);then
      consider u,v being set such that
  A3: u in Seg n & v in Seg 1 & x= [u,v] by ZFMISC_1:def 2;
      reconsider nu=u,nv=v as Nat by A3;
      1<=nv & nv<=1 by A3,FINSEQ_1:3;then
  A4: P[x,nu] by A3,XXREAL_0:1;
      FTSL1 n = FT_Space_Str(# Seg n,Nbdl1 n #) by FINTOPO4:def 4;
      hence thesis by A3,A4;
    end;
    ex f being Function of FTSS2(n,1), FTSL1 n
       st for x be set st x in the carrier of FTSS2(n,1) holds P[x,f.x]
               from FUNCT_2:sch 1(A2);then
    consider f being Function of FTSS2(n,1), FTSL1 n such that
A5: for x be set st x in the carrier of FTSS2(n,1) holds P[x,f.x];
    for x1,x2 being set st x1 in dom f & x2 in dom f & f.x1=f.x2
      holds x1=x2
    proof let x1,x2 be set;
      assume A6: x1 in dom f & x2 in dom f & f.x1=f.x2;then
 A7: x1 in [:Seg n,Seg 1:] by FUNCT_2:def 1;
 A8: x2 in [:Seg n,Seg 1:] by A6,FUNCT_2:def 1;
      [f.x1,1]=x1 by A5,A7;
      hence x1=x2 by A6,A5,A8;
    end;then
A9: f is one-to-one by FUNCT_1:def 8;
A10: the carrier of FTSL1 n c= rng f
    proof let x be set;
      assume x in the carrier of FTSL1 n;then
 A11: x in Seg n by A1;
      set z=[x,1];
      1 in Seg 1 by FINSEQ_1:3;then
 A12: z in the carrier of FTSS2(n,1) by A11,ZFMISC_1:def 2;then
 A13: z in dom f by FUNCT_2:def 1;
      [f.z,1]=z by A5,A12;then
      f.z=x by ZFMISC_1:33;
      hence x in rng f by A13,FUNCT_1:def 5;
    end;
    rng f c= the carrier of FTSL1 n by RELSET_1:12;then
    rng f= the carrier of FTSL1 n by A10,XBOOLE_0:def 10;then
A14: f is onto by FUNCT_2:def 3;
    set FT1=FTSS2(n,1),FT2= FTSL1 n;
    for x being Element of FT1 holds
      f.:((the Nbd of FT1).x)=(the Nbd of FT2).(f.x)
    proof let x be Element of FT1;
      consider u,v being set such that
 A15: u in Seg n & v in Seg 1 & x= [u,v] by ZFMISC_1:def 2;
 A16: f.:((the Nbd of FT1).x) c= (the Nbd of FT2).(f.x)
      proof let y be set;
        assume y in f.:((the Nbd of FT1).x);then
        consider x2 being set such that
A17:    x2 in dom f & x2 in ((Nbds2(n,1)).x) & y=f.x2
             by FUNCT_1:def 12;
A18:    (Nbds2(n,1)).x = [:{u}, (Nbdl1 1).v:] \/ [:(Nbdl1 n).u,{v}:]
                        by A15,Def4;
A19:    x2 in the carrier of FT1 by A17,FUNCT_2:def 1;then
        consider u2,v2 being set such that
A20:    u2 in Seg n & v2 in Seg 1 & x2= [u2,v2] by ZFMISC_1:def 2;
        x=[f.x,1] by A5;then
A21:    u=f.x & v=1 by ZFMISC_1:33,A15;
A22:    x2=[f.x2,1] by A5,A19;then
A23:    u2=f.x2 & v2=1 by A20,ZFMISC_1:33;then
A24:     [u2,v2] in [:{u}, (Nbdl1 1).v:] or [u2,v2] in [:(Nbdl1 n).u,{v}:]
                        by XBOOLE_0:def 2,A17,A18,A22;
        now per cases by A24;
          suppose [u2,v2] in [:{u}, (Nbdl1 1).v:];then
            u2 in {u} by ZFMISC_1:106;then
            A25: u2 = u by TARSKI:def 1;
            reconsider pu=u as Element of FTSL1 n by A1,A15;
            (FTSL1 n) is filled by FINTOPO4:18;then
            u in U_FT pu by FIN_TOPO:def 3;
            hence u2 in (Nbdl1 n).u by A25,A1,FIN_TOPO:def 1;
          end;
          suppose [u2,v2] in [:(Nbdl1 n).u,{v}:];
           hence u2 in (Nbdl1 n).u by ZFMISC_1:106;
          end;
        end;
        hence y in (the Nbd of FT2).(f.x) by A1,A17,A21,A23;
      end;
      (the Nbd of FT2).(f.x) c= f.:((the Nbd of FT1).x)
      proof let y be set;
        assume A26: y in (the Nbd of FT2).(f.x);
        (Nbdl1 n).(f.x) in bool Seg n by FUNCT_2:7,A1;then
        (Nbdl1 n).(f.x) c= rng f by XBOOLE_1:1,A10,A1;then
        consider x3 being set such that
   A27: x3 in dom f & y=f.x3 by FUNCT_1:def 5,A26,A1;
        set u2=f.x3,v2=1;
   A28: x3 in the carrier of FT1 by A27,FUNCT_2:def 1;then
        x3=[f.x3,1] by A5;then
   A29: u2 in Seg n & v2 in Seg 1 & x3=[u2,v2] by ZFMISC_1:106,A28;
        reconsider nv=v as Nat by A15;
        1 <= nv & nv <= 1 by FINSEQ_1:3,A15;then
   A30: nv = 1 by XXREAL_0:1;
  A31: (Nbdl1 1).v = {nv, max(nv-'1,1),min(nv+1,1)} by FINTOPO4:def 3,A15
                   .= {1,max(0,1),min(2,1)} by NAT_2:10,A30
                   .= {1,1,min(2,1)} by XXREAL_0:def 9
                   .= {1, min(2,1)} by ENUMSET1:70
                   .= {1, 1} by XXREAL_0:def 8
                   .= {1} by ENUMSET1:69;then
        A32: v2 in (Nbdl1 1).v by ZFMISC_1:37;
        x=[f.x,1] by A5;then
        u2 in (Nbdl1 n).u by A1,A26,A27,ZFMISC_1:33,A15;then
   A33: [u2,v2] in [:(Nbdl1 n).u, (Nbdl1 1).v:] by A32,ZFMISC_1:def 2;
        set X=(Nbdl1 n).u, Y=(Nbdl1 1).v;
  A34: [u2,v2] in [:X,{v}:] \/ [:{u},Y:] by XBOOLE_0:def 2,A30,A31,A33;
        (Nbds2(n,1)).x = [:{u}, Y:] \/ [:X,{v}:] by A15,Def4;
       hence y in f.:((the Nbd of FT1).x) by FUNCT_1:def 12,A27,A34,A29;
      end;
      hence f.:((the Nbd of FT1).x)=(the Nbd of FT2).(f.x)
                        by A16,XBOOLE_0:def 10;
    end;then
    f is_homeomorphism by Def1,A9,A14;
    hence thesis;
  end;
