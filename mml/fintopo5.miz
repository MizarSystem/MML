theorem
  for n being non zero Nat
  ex h being Function of FTSL2(n,1), FTSL1 n st h is_homeomorphism
  proof let n be non zero Nat;
    defpred P[set,set] means [$2,1]=$1;
A1: FTSL1 n = FT_Space_Str(# Seg n,Nbdl1 n #) by FINTOPO4:def 4;
    A2: for x be set st x in the carrier of FTSL2(n,1)
    ex y be set st y in the carrier of FTSL1 n & P[x,y]
    proof let x be set;
      assume x in the carrier of FTSL2(n,1);then
      consider u,v being set such that
        A3: u in Seg n & v in Seg 1 & x= [u,v] by ZFMISC_1:def 2;
      reconsider nu=u,nv=v as Nat by A3;
      1<=nv & nv<=1 by A3,FINSEQ_1:3;then
      A4: P[x,nu] by A3,XXREAL_0:1;
      FTSL1 n = FT_Space_Str(# Seg n,Nbdl1 n #) by FINTOPO4:def 4;
      hence ex y be set st y in the carrier of FTSL1 n & P[x,y] by A3,A4;
    end;
    ex f being Function of FTSL2(n,1), FTSL1 n
       st for x be set st x in the carrier of FTSL2(n,1) holds P[x,f.x]
               from FUNCT_2:sch 1(A2);then
    consider f being Function of FTSL2(n,1), FTSL1 n such that
A5: for x be set st x in the carrier of FTSL2(n,1) holds P[x,f.x];
    for x1,x2 being set st x1 in dom f & x2 in dom f & f.x1=f.x2
      holds x1=x2
    proof let x1,x2 be set;
      assume A6: x1 in dom f & x2 in dom f & f.x1=f.x2;then
A7:  x1 in [:Seg n,Seg 1:] by FUNCT_2:def 1;
A8:  x2 in [:Seg n,Seg 1:] by A6,FUNCT_2:def 1;
      [f.x1,1]=x1 by A5,A7;
      hence x1=x2 by A6,A5,A8;
    end;then
A9: f is one-to-one by FUNCT_1:def 8;
A10: the carrier of FTSL1 n c= rng f
    proof let x be set;
      assume x in the carrier of FTSL1 n;then
 A11: x in Seg n by A1;
      set z=[x,1];
      1 in Seg 1 by FINSEQ_1:3;then
 A12: z in the carrier of FTSL2(n,1) by A11,ZFMISC_1:def 2;then
 A13: z in dom f by FUNCT_2:def 1;
      [f.z,1]=z by A5,A12;then
      f.z=x by ZFMISC_1:33;
      hence x in rng f by A13,FUNCT_1:def 5;
    end;
    rng f c= the carrier of FTSL1 n by RELSET_1:12;then
    rng f= the carrier of FTSL1 n by A10,XBOOLE_0:def 10;then
A14: f is onto by FUNCT_2:def 3;
    set FT1=FTSL2(n,1),FT2= FTSL1 n;
    for x being Element of FT1 holds
      f.:((the Nbd of FT1).x)=(the Nbd of FT2).(f.x)
    proof let x be Element of FT1;
      consider u,v being set such that
 A15: u in Seg n & v in Seg 1 & x= [u,v] by ZFMISC_1:def 2;
 A16: f.:((the Nbd of FT1).x) c= (the Nbd of FT2).(f.x)
      proof let y be set;
        assume y in f.:((the Nbd of FT1).x);then
        consider x2 being set such that
   A17: x2 in dom f & x2 in (Nbdl2(n,1)).x & y=f.x2 by FUNCT_1:def 12;
   A18: (Nbdl2(n,1)).x =[:(Nbdl1 n).u,(Nbdl1 1).v:] by A15,Def2;
   A19: x2 in the carrier of FT1 by A17,FUNCT_2:def 1;
        x=[f.x,1] by A5;then
   A20: u=f.x & v=1 by ZFMISC_1:33,A15;
        x2=[f.x2,1] by A5,A19;
        hence y in (the Nbd of FT2).(f.x) by A1,A17,A20,ZFMISC_1:106,A18;
      end;
      (the Nbd of FT2).(f.x) c= f.:((the Nbd of FT1).x)
      proof let y be set;
        assume A21: y in (the Nbd of FT2).(f.x);
        (Nbdl1 n).(f.x) in bool (Seg n) by FUNCT_2:7,A1;then
        (Nbdl1 n).(f.x) c= rng f by XBOOLE_1:1,A10,A1;then
        consider x3 being set such that
   A22: x3 in dom f & y=f.x3 by FUNCT_1:def 5,A21,A1;
   A23: f.x3 in (Nbdl1 n).(f.x) by A1,A21,A22;
        set u2=f.x3,v2=1;
   A24: x3 in the carrier of FT1 by A22,FUNCT_2:def 1;then
        x3=[f.x3,1] by A5;then
   A25: u2 in Seg n & v2 in Seg 1 & x3=[u2,v2] by ZFMISC_1:106,A24;
        reconsider nv=v as Nat by A15;
        1 <= nv & nv <= 1 by FINSEQ_1:3,A15;then
   A26: nv = 1 by XXREAL_0:1;
        (Nbdl1 1).v = {nv, max(nv-'1,1),min(nv+1,1)} by FINTOPO4:def 3,A15
                   .= {1,max(0,1),min(2,1)} by NAT_2:10,A26
                   .= {1,1,min(2,1)} by XXREAL_0:def 9
                   .= {1, min(2,1)} by ENUMSET1:70
                   .= {1, 1} by XXREAL_0:def 8
                   .= {1} by ENUMSET1:69;then
   A27: v2 in (Nbdl1 1).v by ZFMISC_1:37;
        x=[f.x,1] by A5;then
        u2 in (Nbdl1 n).u by A23,ZFMISC_1:33,A15;then
   A28: [u2,v2] in [:(Nbdl1 n).u,(Nbdl1 1).v:]
                     by A27,ZFMISC_1:def 2;
        (Nbdl2(n,1)).x = [:(Nbdl1 n).u,(Nbdl1 1).v:]
                     by A15,Def2;
        hence y in f.:((the Nbd of FT1).x)
                           by FUNCT_1:def 12,A22,A28,A25;
      end;
      hence f.:((the Nbd of FT1).x)=(the Nbd of FT2).(f.x)
                        by A16,XBOOLE_0:def 10;
    end;then
    f is_homeomorphism by Def1,A9,A14;
    hence thesis;
  end;

:: 2-dimensional small FT_Str
definition let n,m be Nat;
  func Nbds2(n,m) -> Function of [:Seg n, Seg m:], bool [:Seg n, Seg m:] means
  :Def4: for x being set st x in [:Seg n, Seg m:]
    holds for i,j being Nat st x=[i,j] holds
    it.x = [:{i}, (Nbdl1 m).j:] \/ [:(Nbdl1 n).i,{j}:];
  existence
  proof
    defpred P[set,set] means for i,j being Nat st $1=[i,j] holds
    $2= [:{i}, (Nbdl1 m).j:] \/ [:(Nbdl1 n).i,{j}:];
A1: for x being set st x in [:Seg n, Seg m:] ex y being set
      st y in bool [:Seg n, Seg m:] & P[x,y]
    proof let x be set;
      assume x in [:Seg n, Seg m:];then
      consider u,y being set such that
  A2: u in Seg n & y in Seg m & x = [u,y] by ZFMISC_1:def 2;
      reconsider i=u, j=y as Nat by A2;
      set y3= [:{i}, (Nbdl1 m).j:] \/ [:(Nbdl1 n).i,{j}:];
      y3 c= [:Seg n, Seg m:]
      proof let z be set;
        assume z in y3;then
    A3: z in [:{i}, (Nbdl1 m).j:] or z in [:(Nbdl1 n).i,{j}:]
                                              by XBOOLE_0:def 2;
        per cases by A3;
          suppose z in [:{i}, (Nbdl1 m).j:];then
            consider x4,y4 being set such that
        A4: x4 in {i} & y4 in (Nbdl1 m).j & z=[x4,y4] by ZFMISC_1:def 2;
       A5: x4 = i by TARSKI:def 1,A4;
        A6: i in Seg n & j in Seg m by A2;then
            (Nbdl1 m).j in bool Seg m by FUNCT_2:7;
            hence z in [:Seg n, Seg m:] by A4,ZFMISC_1:def 2,A6,A5;
          end;
          suppose z in [:(Nbdl1 n).i,{j}:];then
            consider x4,y4 being set such that
            A7: x4 in (Nbdl1 n).i & y4 in {j} & z=[x4,y4] by ZFMISC_1:def 2;
            (Nbdl1 n).i in bool Seg n by FUNCT_2:7,A2;then
            A8: x4 in Seg n by A7;
            y4 in Seg m by A2,TARSKI:def 1,A7;
            hence z in [:Seg n, Seg m:] by A7,ZFMISC_1:def 2,A8;
          end;
       end;then
  A9: y3 in bool [:Seg n, Seg m:];
      for i4,j4 being Nat st x=[i4,j4] holds
        y3= [:{i4}, (Nbdl1 m).j4:] \/ [:(Nbdl1 n).i4,{j4}:]
      proof let i4,j4 be Nat;
        assume x=[i4,j4];then
        i4=u & j4=y by A2,ZFMISC_1:33;
        hence y3= [:{i4}, (Nbdl1 m).j4:] \/ [:(Nbdl1 n).i4,{j4}:];
      end;
      hence ex y2 being set st y2 in bool [:Seg n, Seg m:] & P[x,y2] by A9;
    end;
    ex f being Function of [:Seg n, Seg m:],bool [:Seg n, Seg m:]
      st for x being set st x in [:Seg n, Seg m:] holds P[x,f.x]
      from FUNCT_2:sch 1(A1);
    hence thesis;
  end;
  uniqueness
  proof let f1,f2 be Function of [:Seg n, Seg m:],bool [:Seg n, Seg m:];
    assume
  A10: ( for x being set st x in [:Seg n, Seg m:]
      holds (for i,j being Nat st x=[i,j] holds
      f1.x = [:{i}, (Nbdl1 m).j:] \/ [:(Nbdl1 n).i,{j}:])) &
      for x being set st x in [:Seg n, Seg m:]
      holds (for i,j being Nat st x=[i,j] holds
      f2.x = [:{i}, (Nbdl1 m).j:] \/ [:(Nbdl1 n).i,{j}:]);
A11: dom f1= [:Seg n, Seg m:] by FUNCT_2:def 1;
A12: dom f2= [:Seg n, Seg m:] by FUNCT_2:def 1;
    for x being set st x in dom f1 holds f1.x=f2.x
    proof let x be set;
      assume A13: x in dom f1;then
      consider u,y being set such that
A14:  u in Seg n & y in Seg m & x=[u,y] by A11,ZFMISC_1:def 2;
      reconsider i=u, j=y as Nat by A14;
      f1.x= [:{i}, (Nbdl1 m).j:] \/ [:(Nbdl1 n).i,{j}:] by A10,A13,A11,A14;
      hence f1.x=f2.x by A10,A11,A13,A14;
    end;
    hence f1=f2 by FUNCT_1:9,A11,A12;
  end;
end;

definition let n,m be Nat;
  func FTSS2(n,m) -> strict FT_Space_Str equals
  FT_Space_Str(# [:Seg n, Seg m:], Nbds2(n,m) #);
