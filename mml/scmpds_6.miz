  = Comput(ProgramPart(s3),s3,1), s5 = Comput(ProgramPart(s3),s3,2);
I2: s +* Initialize pIF = Initialize s +* pIF by COMPOS_1:125;
A2: IF =i ';' (j ';' I) by SCMPDS_4:52;
A3: not b in dom Initialize pIF by SCMPDS_4:31;
  not a in dom Initialize pIF by SCMPDS_4:31;
  then
A4: s3.DataLoc(s3.a,k1)=s3.b by I2,FUNCT_4:12
    .= s.b by A3,I2,FUNCT_4:12;
A5: IC s3 = 0 by I2,FUNCT_4:26,SCMPDS_5:18;
  Comput(ProgramPart( s3), s3,0 + 1) = Following(ProgramPart s3,
  Comput(ProgramPart(s3),s3,0)) by EXTPRO_1:4
    .= Following(ProgramPart s3,s3) by EXTPRO_1:3
    .= Exec(i,s3) by A2,Th22,I2;
  then
A6: IC s4 = succ IC s3 by A1,A4,SCMPDS_2:68
    .= (0+1) by A5;
A7: Initialize pIF c= s3 by I2,FUNCT_4:26;
  then
A8: pIF c= s4 by AMI_1:81,COMPOS_1:132;
A9:  1 in dom IF by Lm9;
  then  1 in dom pIF by Th18;
  then
A10: s4. 1 = pIF. 1 by A8,GRFUNC_1:8
    .=IF. 1 by A9,Th19
    .=j by Lm10;
A11: card IF=card I+2 by Lm8;
  then
A12: (card I+2) in dom pIF by Th25;
Y:  (ProgramPart s4)/.IC s4
 = s4.IC s4 by COMPOS_1:38;
T: ProgramPart s3 = ProgramPart s4
by AMI_1:123;
  Comput(ProgramPart( s3), s3,1 + 1) = Following(ProgramPart s3,s4) by
EXTPRO_1:4
    .= Exec(j,s4) by A6,A10,Y,T;
  then
A13: IC s5 = ICplusConst(s4,card I+1) by SCMPDS_2:66
    .= (card I+1+1) by A6,Th23
    .= (card I+(1+1));
Y:  (ProgramPart s5)/.IC s5
 = s5.IC s5 by COMPOS_1:38;
TX: ProgramPart s3 = ProgramPart s5
by AMI_1:123;
  pIF c= s5 by A7,AMI_1:81,COMPOS_1:132;
  then s5.(card I+2) = pIF.(card I+2) by A12,GRFUNC_1:8
    .=halt SCMPDS by A11,Th25;
  then
A14: CurInstr(ProgramPart s3,s5) = halt SCMPDS by A13,Y,TX;
  now
    let k be Element of NAT;
A15: k = 0 or 0 + 1 <= k by INT_1:20;
    per cases by A15,XXREAL_0:1;
    suppose
      k = 0;
      then Comput(ProgramPart(s3),s3,k) = s3 by EXTPRO_1:3;
      hence IC Comput(ProgramPart(s3),s3,k) in dom pIF by A5,COMPOS_1:135;
    end;
    suppose
      k = 1;
      hence IC Comput(ProgramPart(s3),s3,k) in dom pIF by A9,A6,Th18;
    end;
    suppose
      1 < k;
      then 1+1 <= k by INT_1:20;
      hence IC Comput(ProgramPart(s3),s3,k) in dom pIF
      by A12,A13,A14,EXTPRO_1:6;
    end;
  end;
  hence IF is_closed_on s by Def2;
  ProgramPart s3 halts_on s3 by A14,EXTPRO_1:30;
  hence thesis by Def3;
end;

theorem Th104:
