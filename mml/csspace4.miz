begin :: Complex Banach space of bounded complex sequences

Lm1:
 for rseq be Real_Sequence
 for K be real number st (for n be Nat holds rseq.n <= K)
 holds sup rng rseq <= K
 proof
   let rseq be Real_Sequence;
   let K be real number such that
   A1:  for n be Nat holds rseq.n <= K;
      now let s be real number such that
    A2: s in rng rseq;
       NAT= dom rseq by FUNCT_2:def 1;
    then rseq is Function of NAT, rng rseq by FUNCT_2:3;
    then consider n be set such that
    A3: n in NAT & rseq.n=s by A2,FUNCT_2:17;
    thus s <=K by A1,A3;
   end;
   hence thesis by SEQ_4:62;
end;

Lm2:
for rseq be Real_Sequence st rseq is bounded holds
   for n be Nat holds rseq.n <= sup rng rseq
proof
   let rseq be Real_Sequence such that
A1:rseq is bounded;
   let n be Nat;
      rng rseq is bounded by A1,PSCOMP_1:7;
then A2:  rng rseq is bounded_above by SEQ_4:def 3;
      NAT = dom rseq by FUNCT_2:def 1;
     then rseq.n in rng rseq by FUNCT_1:12;
   hence thesis by A2,SEQ_4:def 4;
end;

definition
func the_set_of_BoundedComplexSequences -> Subset of
     Linear_Space_of_ComplexSequences means :Def1:
   for x being set holds x in it iff
    (x in the_set_of_ComplexSequences & seq_id x is bounded);
existence
proof
   defpred P[set] means seq_id $1 is bounded;
   consider IT being set such that
A1:for x being set holds x in IT
    iff x in the_set_of_ComplexSequences & P[x] from XBOOLE_0:sch 1;
    IT is Subset of the_set_of_ComplexSequences
   proof
      for x be set st x in IT holds x in the_set_of_ComplexSequences by A1;
     hence thesis by TARSKI:def 3;
   end;
   hence thesis by A1,CSSPACE:def 7;
end;
uniqueness
proof
   let X1,X2 be Subset of Linear_Space_of_ComplexSequences;
   assume that
A2: for x being set holds x in X1 iff
     (x in the_set_of_ComplexSequences & seq_id(x) is bounded) and
A3: for x being set holds x in X2 iff
     (x in the_set_of_ComplexSequences & seq_id(x) is bounded);
    thus X1 c= X2
    proof
      let x be set;
      assume x in X1;
      then x in the_set_of_ComplexSequences & seq_id(x) is bounded by A2;
      hence thesis by A3;
    end;
    let x be set;
    assume x in X2;
    then x in the_set_of_ComplexSequences & seq_id(x) is bounded by A3;
    hence thesis by A2;
  end;
end;

Lm3:
for seq1,seq2 be Complex_Sequence st
 seq1 is bounded & seq2 is bounded holds
  seq1 + seq2 is bounded
proof
   let seq1,seq2 be Complex_Sequence;
   assume
A1:seq1 is bounded & seq2 is bounded;
   then consider r1 being Real such that
A2:0 < r1 & for n be Nat holds |.seq1.n.| < r1 by COMSEQ_2:8;
   consider r2 being Real such that
A3:0 < r2 & for n be Nat holds |.seq2.n.| < r2 by A1,COMSEQ_2:8;
    for n be Nat holds |.(seq1+seq2).n.| < r1+r2
   proof
     let n be Nat;
      |.(seq1+seq2).n.| = |.(seq1.n + seq2.n).| by COMSEQ_1:def 4;
then A4:  |.(seq1+seq2).n.| <= |.seq1.n.| + |.seq2.n.| by COMPLEX1:142;
      |.seq1.n.| < r1 & |.seq2.n.| < r2 by A2,A3;
     then |.seq1.n.| + |.seq2.n.| < r1 + r2 by XREAL_1:10;
     hence thesis by A4,XREAL_1:2;
   end;
   hence thesis by COMSEQ_2:def 3;
end;

Lm4:
for c be Complex, seq be Complex_Sequence st seq is bounded holds
 c(#)seq is bounded
proof
   let c be Complex;
   let seq be Complex_Sequence such that
A1:seq is bounded;
   consider r be Real such that
A2:0 < r & for n be Nat holds |.seq.n.| < r by A1,COMSEQ_2:8;
    ex r1 be Real st for n be Nat holds |.(c(#)seq).n.| < r1
   proof
      now per cases;
       case A3:c = 0c;
A4:      for n be Nat holds |.(c(#)seq).n.| < 1
         proof
           let n be Nat;
            (c(#)seq).n = c * seq.n by COMSEQ_1:def 7
             .= 0 by A3;
           hence thesis by COMPLEX1:130;
         end;
         take r1 = 1;
         thus thesis by A4;
       end;
       case A5:c <> 0c;
A6:      for n be Nat holds |.(c(#)seq).n.| < |.c.|*r
         proof
           let n be Nat;
A7:        |.c.| > 0 & r > |.seq.n.| by A2,A5,COMPLEX1:133;
            |.(c(#)seq).n.| = |.c * seq.n.| by COMSEQ_1:def 7
            .= |.c.|*|.seq.n.| by COMPLEX1:151;
           hence thesis by A7,XREAL_1:70;
         end;
         take r1 = |.c.|*r;
         thus thesis by A6;
       end;
     end;
     hence thesis;
   end;
   hence thesis by COMSEQ_2:def 3;
end;

registration
  cluster the_set_of_BoundedComplexSequences -> non empty;
coherence
proof
    seq_id CZeroseq is bounded
   proof
     reconsider seq=seq_id CZeroseq as Complex_Sequence;
      for n being Nat holds |.seq.n.| < 1 by CSSPACE:def 6,COMPLEX1:130;
     hence thesis by COMSEQ_2:8;
   end;
   hence thesis by Def1;
end;
  cluster the_set_of_BoundedComplexSequences -> lineary-closed;
  coherence
proof
   set W = the_set_of_BoundedComplexSequences;
A1:for v,u be VECTOR of Linear_Space_of_ComplexSequences st
    v in the_set_of_BoundedComplexSequences &
    u in the_set_of_BoundedComplexSequences
      holds v + u in the_set_of_BoundedComplexSequences
   proof
     let v,u be VECTOR of Linear_Space_of_ComplexSequences such that
A2:  v in W & u in W;
      seq_id(v+u) is bounded
     proof
A3:    seq_id v is bounded by A2,Def1;
A4:    seq_id u is bounded by A2,Def1;

        seq_id(v+u)=seq_id(seq_id v+seq_id u) by CSSPACE:4,def 7
                 .=seq_id v + seq_id u by CSSPACE:3;

       hence thesis by A3,A4,Lm3;
     end;
     hence v+u in W by Def1,CSSPACE:def 7;
   end;
    for c be Complex, v be VECTOR of Linear_Space_of_ComplexSequences st
    v in W holds c * v in W
   proof
     let c be Complex;
     let v be VECTOR of Linear_Space_of_ComplexSequences such that
A5: v in W;
      seq_id(c*v) is bounded
     proof
A6: seq_id v is bounded by A5,Def1;
        seq_id(c*v) =seq_id(c(#)seq_id(v)) by CSSPACE:5,def 7
                  .=c(#)seq_id(v) by CSSPACE:3;
       hence thesis by A6,Lm4;
     end;
     hence c*v in W by Def1,CSSPACE:def 7;
   end;
   hence thesis by A1,CLVECT_1:def 3;
end;
end;

 Lm5:
CLSStruct (# the_set_of_BoundedComplexSequences,
  Zero_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Add_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Mult_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences) #)
is Subspace of Linear_Space_of_ComplexSequences by CSSPACE:13;

registration
cluster CLSStruct (# the_set_of_BoundedComplexSequences,
  Zero_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Add_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Mult_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences) #)
   -> Abelian add-associative right_zeroed right_complementable
        ComplexLinearSpace-like;
  coherence by CSSPACE:13;
end;

Lm6:
CLSStruct (# the_set_of_BoundedComplexSequences,
  Zero_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Add_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Mult_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences) #)
  is Abelian add-associative right_zeroed
    right_complementable ComplexLinearSpace-like;

Lm7:
ex NORM be Function of the_set_of_BoundedComplexSequences,REAL st
  for x be set st x in the_set_of_BoundedComplexSequences holds
    NORM.x = sup rng |.seq_id(x).|
proof
  deffunc F(set) = sup rng |.seq_id($1).|;
A1:for z be set st z in the_set_of_BoundedComplexSequences
   holds F(z) in REAL;
      ex f being Function of the_set_of_BoundedComplexSequences,REAL st
     for x being set st x in the_set_of_BoundedComplexSequences holds
       f.x = F(x) from FUNCT_2:sch 2(A1);
   hence thesis;
end;

definition
  func Complex_linfty_norm ->
  Function of the_set_of_BoundedComplexSequences, REAL means :Def2:
  for x be set st x in the_set_of_BoundedComplexSequences holds
    it.x = sup rng |.seq_id x.|;
existence by Lm7;
uniqueness
proof
   let NORM1,NORM2
   be Function of the_set_of_BoundedComplexSequences, REAL such that
A1:for x be set st x in the_set_of_BoundedComplexSequences holds
     NORM1.x = sup rng |.seq_id x.| and
A2:for x be set st x in the_set_of_BoundedComplexSequences holds
     NORM2.x = sup rng |.seq_id x.|;
A3:dom NORM1 = the_set_of_BoundedComplexSequences &
     dom NORM2 = the_set_of_BoundedComplexSequences by FUNCT_2:def 1;
      for z be set st z in the_set_of_BoundedComplexSequences holds
       NORM1.z = NORM2.z
   proof
     let z be set such that
A4:  z in the_set_of_BoundedComplexSequences;
      NORM1.z = sup rng |.seq_id(z).| by A1,A4;
     hence thesis by A2,A4;
  end;
  hence thesis by A3,FUNCT_1:9;
end;
end;

Lm8:
for seq be Complex_Sequence st (for n be Nat holds seq.n=0c) holds
      seq is bounded & sup rng |.seq.| = 0
proof
   let seq be Complex_Sequence such that
A1:for n be Nat holds seq.n=0c;
    for n be Nat holds |.seq.n.| < 1 by A1,COMPLEX1:130;
   hence seq is bounded by COMSEQ_2:def 3;
    NAT = dom |.seq.| by FUNCT_2:def 1;
then A2:|.seq.| is Function of NAT, rng |.seq.| by FUNCT_2:3;
    rng |.seq.| = {0}
   proof
      rng |.seq.| c= {0}
     proof
       let y be set;
       assume y in rng |.seq.|;
       then consider n be set such that
A3:    n in NAT & (|.seq.|).n=y by A2,FUNCT_2:17;
       reconsider n as Nat by A3;
        |.seq.| .n = |. seq.n .| by COMSEQ_1:def 14 .= |.0c.| by A1;
       hence y in {0} by A3,COMPLEX1:130,TARSKI:def 1;
     end;
     hence thesis by ZFMISC_1:39;
   end;
   hence thesis by SEQ_4:22;
end;

Lm9:
for seq be Complex_Sequence st seq is bounded holds
 |.seq.| is bounded
proof
   let seq be Complex_Sequence;
   assume seq is bounded;
   then consider r be Real such that
A1:for n be Nat holds |.seq.n.| < r by COMSEQ_2:def 3;
A2:|. seq.0 .| < r & 0 <= |. seq.0 .| by A1,COMPLEX1:132;
    for n be Nat holds abs (|.seq.| .n) < r
   proof
     let n be Nat;
      |.seq.| .n = |.seq.n.| by COMSEQ_1:def 14;
     then 0 <= |.seq.| .n by COMPLEX1:132;
     then abs(|.seq.| .n) = |.seq.| .n by ABSVALUE:def 1
      .= |.seq.n.| by COMSEQ_1:def 14;
     hence thesis by A1;
   end;
   hence thesis by A2,SEQ_2:15;
end;

Lm10:
for seq be Complex_Sequence st |.seq.| is bounded holds
 seq is bounded
proof
   let seq be Complex_Sequence;
   assume |.seq.| is bounded;
   then consider r be real number such that
A1:0 < r & for n be Nat holds abs(|.seq.| .n)<r by SEQ_2:15;
   reconsider r as Real by XREAL_0:def 1;
    for n be Nat holds |.seq.n.| < r
   proof
     let n be Nat;
      |.seq.| .n = |.seq.n.| by COMSEQ_1:def 14;
     then |.seq.| .n >= 0 by COMPLEX1:132;
     then abs(|.seq.| .n) = |.seq.| .n by ABSVALUE:def 1
      .= |.seq.n.| by COMSEQ_1:def 14;
     hence thesis by A1;
   end;
   hence thesis by COMSEQ_2:def 3;
end;

Lm11:
for seq be Complex_Sequence st
 ( seq is bounded & sup rng |.seq.| = 0 ) holds
  for n be Nat holds seq.n = 0c
proof
   let seq be Complex_Sequence such that
A1:seq is bounded and
A2:sup rng |.seq.| = 0;
   let n be Nat;

A3:|.seq.| is bounded by A1,Lm9;
    0 <= |.seq.n.| by COMPLEX1:132;
   then 0 <= |.seq.| .n by COMSEQ_1:def 14;
   then (|.seq.|).n =0 by A2,A3,Lm2;
   then |.seq.n.| = 0 by COMSEQ_1:def 14;
   hence thesis by COMPLEX1:131;
end;

canceled;

theorem
 for seq be Complex_Sequence holds
 ( seq is bounded & sup rng |.seq.| = 0 ) iff
   for n be Nat holds seq.n = 0c by Lm8,Lm11;

registration
cluster CNORMSTR (# the_set_of_BoundedComplexSequences,
  Zero_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Add_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Mult_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Complex_linfty_norm #) -> Abelian add-associative right_zeroed
    right_complementable ComplexLinearSpace-like;
  coherence by Lm6,CSSPACE3:4;
end;

definition
func Complex_linfty_Space -> non empty CNORMSTR equals
 CNORMSTR (# the_set_of_BoundedComplexSequences,
  Zero_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Add_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Mult_(the_set_of_BoundedComplexSequences,Linear_Space_of_ComplexSequences),
  Complex_linfty_norm #);
  coherence;
end;


theorem Th3:
the carrier of Complex_linfty_Space = the_set_of_BoundedComplexSequences &
( for x be set holds
   x is VECTOR of Complex_linfty_Space
    iff x is Complex_Sequence & seq_id x is bounded ) &
0.Complex_linfty_Space = CZeroseq &
( for u be VECTOR of Complex_linfty_Space holds u = seq_id u ) &
( for u,v be VECTOR of Complex_linfty_Space holds u+v =seq_id(u)+seq_id(v) ) &
( for c be Complex, u be VECTOR of Complex_linfty_Space holds
  c*u =c(#)seq_id(u) ) &
( for u be VECTOR of Complex_linfty_Space holds
   -u = -seq_id u & seq_id(-u) = -seq_id u ) &
( for u,v be VECTOR of Complex_linfty_Space holds u-v =seq_id(u)-seq_id(v) ) &
( for v be VECTOR of Complex_linfty_Space holds seq_id v is bounded ) &
( for v be VECTOR of Complex_linfty_Space holds
   ||.v.|| = sup rng |.seq_id v.| )
proof
   set l1 =Complex_linfty_Space;
A1:for x be set holds x is Element of l1
     iff x is Complex_Sequence & seq_id x is bounded
   proof
     let x be set;
      x in the_set_of_ComplexSequences iff x is Complex_Sequence
      by CSSPACE:def 1;
     hence thesis by Def1;
   end;
A2:for u be VECTOR of l1 holds u = seq_id u
   proof
     let u be VECTOR of l1;
        u is VECTOR of Linear_Space_of_ComplexSequences
     by Lm5,CLVECT_1:30;
     hence thesis by CSSPACE:def 2,def 7;
   end;
A3:for u,v be VECTOR of l1 holds u+v =seq_id u+seq_id v
   proof
     let u,v be VECTOR of l1;
     reconsider u1=u, v1 = v as VECTOR of Linear_Space_of_ComplexSequences
      by Lm5,CLVECT_1:30;
     set L1=Linear_Space_of_ComplexSequences;
     set W = the_set_of_BoundedComplexSequences;
      dom (the add of L1)
       = [:the carrier of L1,the carrier of L1:] by FUNCT_2:def 1;
then A4:  dom ((the add of Linear_Space_of_ComplexSequences) | [:W,W:])
       =[:W,W:] by RELAT_1:91;
      u+v =(the add of l1).[u,v] by RLVECT_1:def 3
        .=((the add of Linear_Space_of_ComplexSequences)|[:W,W:]).[u,v]
         by CSSPACE:def 8
        .=(the add of Linear_Space_of_ComplexSequences).[u,v]
         by A4,FUNCT_1:70
        .=u1+v1 by RLVECT_1:def 3;
     hence thesis by CSSPACE:4,def 7;
   end;

A5:for c be Complex, u be VECTOR of l1 holds c*u =c(#)seq_id u
   proof
     let c be Complex;
     let u be VECTOR of l1;
     reconsider u1=u as VECTOR of Linear_Space_of_ComplexSequences
      by Lm5,CLVECT_1:30;
     set L1=Linear_Space_of_ComplexSequences;
     set W = the_set_of_BoundedComplexSequences;
A6:  [:COMPLEX,W:] c= [:COMPLEX,the carrier of L1:] by ZFMISC_1:119;
      dom (the Mult of L1)
        = [:COMPLEX,the carrier of L1:] by FUNCT_2:def 1;
then A7:  dom ((the Mult of Linear_Space_of_ComplexSequences) | [:COMPLEX,W:])
       =[:COMPLEX,W:] by A6,RELAT_1:91;
      c*u =(the Mult of l1).[c,u] by CLVECT_1:def 1
        .=((the Mult of Linear_Space_of_ComplexSequences)|[:COMPLEX,W:]).[c,u]
           by CSSPACE:def 9
        .=(the Mult of Linear_Space_of_ComplexSequences).[c,u]
           by A7,FUNCT_1:70
        .=c*u1 by CLVECT_1:def 1;
     hence thesis by CSSPACE:5,def 7;
   end;

A8:for u be VECTOR of l1 holds -u =-seq_id u & seq_id(-u)=-seq_id u
   proof
     let u be VECTOR of l1;
         -u = (-1r)*u by CLVECT_1:4
        .= (-1r)(#)seq_id u by A5
        .= -seq_id u by COMSEQ_1:14;
     hence thesis by A2;
   end;
A9:for u,v be VECTOR of l1 holds u-v =seq_id u-seq_id v
   proof
     let u,v be VECTOR of l1;
     thus u-v = u+ -v by RLVECT_1:def 11
             .= seq_id u+seq_id(-v) by A3
             .= seq_id u+(-seq_id v) by A8
             .= seq_id u-seq_id v by COMSEQ_1:def 10;
   end;

A10:0.l1 = CZeroseq
   proof
     set W = the_set_of_BoundedComplexSequences;
     thus 0.l1 = Zero_(W,Linear_Space_of_ComplexSequences)
                  by RLVECT_1:def 2
              .= 0.Linear_Space_of_ComplexSequences by CSSPACE:def 10
              .= CZeroseq by CSSPACE:def 7,RLVECT_1:def 2;
   end;
    for v be VECTOR of l1 holds ||.v.|| = sup rng |.seq_id v.|
   proof
     let v be VECTOR of l1;
     thus ||.v.|| = (the norm of l1).v by CLVECT_1:def 9
                 .= sup rng |.seq_id v.| by Def2;
   end;
   hence thesis by A1,A2,A3,A5,A8,A9,A10;
end;

theorem Th4:
for x, y being Point of Complex_linfty_Space, c be Complex holds
  ( ||.x.|| = 0 iff x = 0.Complex_linfty_Space ) &
  0 <= ||.x.|| &
  ||.x+y.|| <= ||.x.|| + ||.y.|| &
  ||. c*x .|| = |.c.| * ||.x.||
proof
   let x, y be Point of Complex_linfty_Space;
   let c be Complex;
A1:now assume
A2:  ||.x.|| = 0;
A3:  ||.x.|| = sup rng |.seq_id x.| by Th3;
      seq_id x is bounded by Th3;
then A4:  for n be Nat holds 0c = (seq_id x).n by A2,A3,Lm11;
      x in the_set_of_ComplexSequences by Def1;
     hence x=0.Complex_linfty_Space by A4,Th3,CSSPACE:def 6;
   end;

A5:now assume
      x=0.Complex_linfty_Space;
then A6:  for n be Nat holds (seq_id x).n=0c by Th3,CSSPACE:def 6;
     thus ||.x.|| = sup rng |.seq_id x.| by Th3 .= 0 by A6,Lm8;
   end;

A7:seq_id x is bounded by Def1;
A8:0 <= sup rng |.seq_id x.|
   proof
A9:  |.seq_id x.| is bounded by A7,Lm9;
      |.seq_id x.| .1 =|.(seq_id x).1 .| by COMSEQ_1:def 14;
     then 0<= |.seq_id x.| .1 by COMPLEX1:132;
     hence 0 <= sup rng |.seq_id x.| by A9,Lm2;
   end;
A10:||.y.|| = sup rng |.seq_id y.| by Th3;
A11:sup rng |.seq_id(x+y).| = ||.x + y.|| by Th3;
A12:for n be Nat holds |.seq_id(x+y).| .n
       = |.(((seq_id x).n) + ((seq_id y).n)).|
   proof
     let n be Nat;
        (|.seq_id(x+y).|).n
         = (|.(seq_id(seq_id x+seq_id y)).|).n by Th3
        .= (|.(seq_id x+seq_id y).|).n by CSSPACE:3
        .= |.((seq_id x+seq_id y).n).| by COMSEQ_1:def 14
        .= |.(((seq_id x).n)+((seq_id y).n)).| by COMSEQ_1:def 4;
     hence thesis;
   end;
A13:for n be Nat holds (|.seq_id(x+y).|).n
        <= (|.seq_id x.|).n + (|.seq_id y.|).n
   proof
     let n be Nat;
      |.(((seq_id x).n)+ ((seq_id y).n)).|
       <= |.((seq_id x).n).| + |.((seq_id y).n).| by COMPLEX1:142;
     then (|.(seq_id(x+y)).|).n
       <= |.((seq_id x).n).| + |.((seq_id y).n).| by A12;
     then (|.seq_id(x+y).|).n
        <= (|.seq_id x.|).n + |.((seq_id y).n).| by COMSEQ_1:def 14;
     hence thesis by COMSEQ_1:def 14;
   end;
A14:for n being Nat holds
     (|.(seq_id(x+y)).|).n <= ((|.seq_id x.|) + (|.seq_id y.|)).n
   proof
     let n be Nat;
        (|.seq_id x.|).n + (|.seq_id y.|).n
        =((|.seq_id x.|) + (|.seq_id y.|)).n by SEQ_1:11;
     hence thesis by A13;
   end;
    seq_id x is bounded by Def1;
then A15:|.seq_id x.| is bounded by Lm9;
    seq_id y is bounded by Def1;
then A16:|.seq_id y.| is bounded by Lm9;
A17:sup rng |.seq_id(x+y).|
     <= sup rng |.seq_id x.| + sup rng |.seq_id y.|
   proof
      now let n be Nat;
A18:   (|.seq_id(x+y).|).n
         <= (|.seq_id x.| + |.seq_id y.|).n by A14;
A19:   (|.seq_id x.| + |.seq_id y.|).n
         = (|.seq_id x.|).n + (|.seq_id y.|).n by SEQ_1:11;
A20:   (|.seq_id x.|).n <=sup rng |.seq_id x.| by A15,Lm2;
        (|.seq_id y.|).n <=sup rng |.seq_id y.| by A16,Lm2;
       then ((|.seq_id x.|) + (|.seq_id y.|)).n
         <= sup rng |.seq_id x.| + sup rng |.seq_id y.|
           by A19,A20,XREAL_1:9;
       hence (|.seq_id(x+y).|).n
         <= sup rng |.seq_id x.| + sup rng |.seq_id y.|
          by A18,XREAL_1:2;
     end;
     hence thesis by Lm1;
   end;

A21:for n be Nat holds
     (|.(c(#)seq_id x).|).n =|.c.|*(|.seq_id x.|).n
   proof
     let n be Nat;
      (|.c(#)seq_id x.|).n
       =|.((c(#)seq_id x).n).| by COMSEQ_1:def 14
      .=|.(c*((seq_id x).n)).| by COMSEQ_1:def 7
      .=|.c.|*(|.((seq_id x).n).|) by COMPLEX1:151
      .=|.c.|*(|.(seq_id x).| .n) by COMSEQ_1:def 14;
     hence thesis;
   end;
    seq_id x is bounded by Def1;
   then |.seq_id x.| is bounded by Lm9;
   then rng |.seq_id x.| is bounded by PSCOMP_1:7;
then A22:rng |.seq_id x.| is bounded_above by SEQ_4:def 3;
A23:0 <= |.c.| by COMPLEX1:132;
    ||. c*x .|| = sup rng |.seq_id(c*x).| by Th3
      .=sup rng |.(seq_id(c(#)seq_id x)).| by Th3
      .=sup(rng |.(c(#)seq_id x).|) by CSSPACE:3
      .=sup(rng (|.c.| (#) (|.seq_id x.|))) by A21,SEQ_1:13
      .=sup(|.c.|*rng |.seq_id x.|) by INTEGRA2:17
      .=|.c.|*sup rng |.seq_id x.| by A22,A23,INTEGRA2:13
      .=|.c.|*||.x.|| by Th3;
   hence thesis by A1,A5,A8,A10,A11,A17,Th3;
end;

registration
  cluster Complex_linfty_Space -> ComplexNormSpace-like ComplexLinearSpace-like
       Abelian add-associative right_zeroed right_complementable;
  coherence by Th4,CLVECT_1:def 10;
end;

reserve CNS for non empty ComplexNormSpace;
reserve seq for sequence of CNS;

Lm12:
for seq1,seq2,seq3 be Complex_Sequence holds
 seq1 = seq2 - seq3 iff
  for n be Nat holds seq1.n = seq2.n - seq3.n
proof
   let seq1,seq2,seq3 be Complex_Sequence;
A1:seq1 = seq2 - seq3 implies
    for n be Nat holds seq1.n = seq2.n - seq3.n
   proof
     assume seq1 = seq2 - seq3;
then A2:  seq1 = seq2 + -seq3 by COMSEQ_1:def 10;
      now let n be Nat;
        seq1.n = seq2.n + (-seq3).n by A2,COMSEQ_1:def 4
             .= seq2.n + -seq3.n by COMSEQ_1:def 9;
       hence seq1.n = seq2.n - seq3.n;
     end;
     hence thesis;
   end;
    (for n be Nat holds seq1.n = seq2.n - seq3.n) implies
    seq1 = seq2 - seq3
   proof
     assume
A3:  for n be Nat holds seq1.n = seq2.n - seq3.n;

      for x be set st x in NAT holds seq1.x = (seq2-seq3).x
     proof
       let x be set;
       assume x in NAT;
       then reconsider x as Nat;
        seq1.x = seq2.x - seq3.x by A3
             .= seq2.x + -seq3.x
             .= seq2.x + (-seq3).x by COMSEQ_1:def 9
             .= (seq2 + (-seq3)).x by COMSEQ_1:def 4;
       hence thesis by COMSEQ_1:def 10;
     end;
     hence thesis by FUNCT_2:18;
   end;
   hence thesis by A1;

end;

theorem Th5:
for vseq be sequence of Complex_linfty_Space st
  vseq is Cauchy_sequence_by_Norm holds vseq is convergent
proof
   let vseq be sequence of Complex_linfty_Space such that
A1:vseq is Cauchy_sequence_by_Norm;

   defpred P[set,set] means
    ex i be Nat st $1=i &
    ex cseqi be Complex_Sequence st
      (for n be Nat holds cseqi.n=(seq_id(vseq.n)).i) &
        cseqi is convergent & $2 = lim cseqi;
A2:for x be set st x in NAT ex y be set st y in COMPLEX & P[x,y]
   proof
     let x be set;
     assume x in NAT;
     then reconsider i=x as Nat;
     deffunc F(Nat) = (seq_id(vseq.$1)).i;
     consider cseqi be Complex_Sequence such that
A3:  for n be Nat holds cseqi.n = F(n) from COMSEQ_1:sch 1;
A4:  cseqi is convergent
     proof
        now
         let e be Real such that
A5:      e > 0;
         thus ex k be Nat st
          for m be Nat st k <= m holds |.(cseqi.m -cseqi.k).| < e
         proof
           consider k be Nat such that
A6:        for n, m be Nat st n >= k & m >= k holds
            ||.(vseq.n) - (vseq.m).|| < e by A1,A5,CSSPACE3:10;
            take k;
            let m be Nat such that
A7:         k<=m;
             ||.(vseq.m) - (vseq.k).||
             =sup rng |.(seq_id((vseq.m) - (vseq.k))).| by Th3;
then A8:         sup rng |.(seq_id((vseq.m) - (vseq.k))).| < e by A6,A7;
             seq_id((vseq.m)-(vseq.k)) is bounded by Def1;
            then |.(seq_id((vseq.m)-(vseq.k))).| is bounded by Lm9;
then A9:         |.(seq_id((vseq.m) - (vseq.k))).| .i
              <= sup rng |.(seq_id((vseq.m) - (vseq.k))).| by Lm2;
             seq_id((vseq.m) - (vseq.k))
              = seq_id(seq_id(vseq.m)-seq_id(vseq.k)) by Th3
             .= seq_id(vseq.m)-seq_id(vseq.k) by CSSPACE:3
             .= seq_id(vseq.m)+-seq_id(vseq.k) by COMSEQ_1:def 10;
            then (seq_id((vseq.m) - (vseq.k))).i
              = (seq_id(vseq.m)).i+(-seq_id(vseq.k)).i by COMSEQ_1:def 4
             .=(seq_id(vseq.m)).i+(-(seq_id(vseq.k)).i) by COMSEQ_1:def 9
             .=(seq_id(vseq.m)).i-(seq_id(vseq.k)).i
             .=cseqi.m -(seq_id(vseq.k)).i by A3
             .=cseqi.m - cseqi.k by A3;
            then |.(cseqi.m-cseqi.k).| = |.(seq_id((vseq.m)-(vseq.k))).| .i
              by COMSEQ_1:def 14;
            hence thesis by A8,A9,XREAL_1:2;
         end;
       end;
       hence thesis by COMSEQ_3:46;
     end;
     take y = lim cseqi;
     thus thesis by A3,A4;
   end;
   consider f be Function of NAT,COMPLEX such that
A10:for x be set st x in NAT holds P[x,f.x] from FUNCT_2:sch 1(A2);
   reconsider tseq=f as Complex_Sequence;
A11:now
     let i be Nat;
     reconsider x=i as set;
        ex i0 be Nat st x=i0 & ex cseqi be Complex_Sequence st
       ( for n be Nat holds cseqi.n=(seq_id(vseq.n)).i0 ) &
      cseqi is convergent & f.x=lim cseqi by A10;
      hence ex cseqi be Complex_Sequence st
       ( for n be Nat holds cseqi.n=(seq_id(vseq.n)).i ) &
       cseqi is convergent & tseq.i=lim cseqi;
   end;
A12:for e be Real st e >0
   ex k be Nat st
    for n be Nat st n >= k holds
     |.(seq_id tseq-seq_id(vseq.n)).| is bounded &
       sup rng |.(seq_id tseq-seq_id(vseq.n)).| <= e
   proof
     let e be Real such that
A13: e > 0;
     consider k be Nat such that
A14: for n, m be Nat st n >= k & m >= k holds
      ||.(vseq.n) - (vseq.m).|| < e by A1,A13,CSSPACE3:10;
A15: for m,n be Nat st n >= k & m >= k holds
      |.seq_id((vseq.n) - (vseq.m)).| is bounded &
        sup rng |.seq_id((vseq.n) - (vseq.m)).| < e
     proof
       let m,n be Nat such that
A16:   n >= k & m >= k;
        ||.(vseq.n) - (vseq.m).|| < e by A14,A16;
then A17:   (the norm of Complex_linfty_Space).((vseq.n) - (vseq.m))
         < e by CLVECT_1:def 9;
        seq_id((vseq.n) - (vseq.m)) is bounded by Def1;
       hence thesis by A17,Def2,Lm9;
     end;
A18: for n be Nat for i be Nat holds
      for rseq be Real_Sequence st
       ( for m be Nat holds rseq.m=|.(seq_id(vseq.m-vseq.n)).| .i ) holds
        ( rseq is convergent &
          lim rseq = |.((seq_id tseq -seq_id(vseq.n))).| .i )
     proof
       let n be Nat;
A19:   for m,k be Nat holds
       seq_id((vseq.m) - (vseq.k)) = seq_id(vseq.m)-seq_id(vseq.k)
       proof
         let m,k be Nat;
            seq_id((vseq.m) - (vseq.k))
            = seq_id(seq_id(vseq.m)-seq_id(vseq.k)) by Th3;
         hence thesis by CSSPACE:3;
       end;
        now let i be Nat;
           consider cseqi be Complex_Sequence such that
    A20:   ( for n be Nat holds cseqi.n=(seq_id(vseq.n)).i ) &
             cseqi is convergent & tseq.i=lim cseqi by A11;
          now let rseq be Real_Sequence such that
A21:       for m be Nat holds rseq.m=|.(seq_id(vseq.m-vseq.n)).| .i;
A22:       now let m be Nat;
A23:         seq_id(vseq.m - vseq.n)
                   = seq_id(vseq.m)-seq_id(vseq.n) by A19;
             thus rseq.m=|.(seq_id(vseq.m-vseq.n)).| .i by A21
                     .=|.((seq_id(vseq.m-vseq.n)).i).| by COMSEQ_1:def 14
                     .=|.((seq_id(vseq.m)).i
                       -(seq_id(vseq.n)).i).| by A23,Lm12
                     .= |.(cseqi.m -(seq_id(vseq.n)).i) .| by A20;
           end;
            |.(tseq.i-(seq_id(vseq.n)).i).|
                   = |.((tseq-(seq_id(vseq.n))).i).| by Lm12
                  .= |.((seq_id tseq-(seq_id(vseq.n))).i).| by CSSPACE:3
                  .= |.((seq_id tseq -seq_id(vseq.n))).| .i by COMSEQ_1:def 14;
           hence rseq is convergent &
            lim rseq = |.(seq_id tseq -seq_id(vseq.n)).| .i
             by A20,A22,CSSPACE3:1;
         end;
         hence for rseq be Real_Sequence st
          ( for m be Nat holds rseq.m=|.(seq_id(vseq.m-vseq.n)).| .i ) holds
            rseq is convergent
            & lim rseq = |.(seq_id tseq -seq_id(vseq.n)).| .i;
       end;
       hence thesis;
     end;
      for n be Nat st n >= k holds
      |.(seq_id tseq-seq_id(vseq.n)).| is bounded &
     sup rng |.(seq_id tseq-seq_id(vseq.n)).| <= e
     proof
       let n be Nat such that
A24:   n >= k;
A25:   for i be Nat holds |.((seq_id tseq -seq_id(vseq.n))).| .i <= e
       proof
         let i be Nat;
         deffunc F(Nat)= |.(seq_id((vseq.$1) - (vseq.n))).| .i;
         consider rseq be Real_Sequence such that
A26:     for m be Nat holds rseq.m = F(m) from SEQ_1:sch 1;
A27:     rseq is convergent by A18,A26;
A28:     lim rseq = |.(seq_id tseq-seq_id(vseq.n)).| .i by A18,A26;
          for m be Nat st m >= k holds rseq.m <= e
         proof
           let m be Nat;
           assume m >= k;
then A29:       |.(seq_id((vseq.m) - (vseq.n))).| is bounded &
             sup rng |. seq_id((vseq.m) - (vseq.n)).| <= e
               by A15,A24;
then A30:       |.(seq_id((vseq.m) - (vseq.n))).| .i
             <=sup rng |.seq_id((vseq.m) - (vseq.n)).| by Lm2;
              rseq.m = |.(seq_id((vseq.m) - (vseq.n))).| .i by A26;
           hence thesis by A29,A30,XREAL_1:2;
         end;
         hence thesis by A27,A28,RSSPACE2:6;
       end;
        |.(seq_id tseq-seq_id(vseq.n)).| is bounded
       proof
A31:     0 + 0 < 1 + e by A13,XREAL_1:10;
A32:     0 + e < 1 + e by XREAL_1:10;
          now let i be Nat;
          |.((seq_id tseq -seq_id(vseq.n))).| .i <= e by A25;
           then |.(((seq_id tseq -seq_id(vseq.n))).i).| <= e by COMSEQ_1:def 14
;
           hence |.(((seq_id tseq -seq_id(vseq.n))).i).|
                <e+1 by A32,XREAL_1:2;
         end;
         then seq_id tseq -seq_id(vseq.n) is bounded by A31,COMSEQ_2:8;
         hence thesis by Lm9;
       end;
       hence thesis by A25,Lm1;
     end;
     hence thesis;
   end;
A33:seq_id tseq is bounded
   proof
     consider m be Nat such that
A34: for n be Nat st n >= m holds
     |.(seq_id tseq -seq_id(vseq.n)).| is bounded &
     sup rng |.(seq_id tseq -seq_id(vseq.n)).| <= 1 by A12;
A35: |.(seq_id tseq -seq_id(vseq.m)).| is bounded by A34;
      seq_id(vseq.m) is bounded by Def1;
then A36: |.seq_id (vseq.m).| is bounded by Lm9;
     set a=|.(seq_id tseq -seq_id(vseq.m)).|;
     set b=|.seq_id(vseq.m).|;
     set d=|.seq_id tseq.|;
A37: a + b is bounded by A35,A36,SEQM_3:71;
A38: for i be Nat holds d.i <= (a+b).i
     proof
       let i be Nat;
A39:   a.i = |.((seq_id tseq -seq_id(vseq.m)).i).| by COMSEQ_1:def 14
          .= |.((seq_id tseq+-seq_id(vseq.m)).i).| by COMSEQ_1:def 10
          .= |.((seq_id tseq).i+(-seq_id(vseq.m)).i).| by COMSEQ_1:def 4
          .= |.((seq_id tseq).i+(-(seq_id(vseq.m)).i)).| by COMSEQ_1:def 9
          .= |.((seq_id tseq).i-(seq_id(vseq.m)).i).|;
A40:   b.i=|.((seq_id(vseq.m)).i).| by COMSEQ_1:def 14;
        d.i=|.((seq_id tseq).i).| by COMSEQ_1:def 14;
       then d.i-b.i <= a.i by A39,A40,COMPLEX1:145;
       then d.i-b.i+b.i<= a.i + b.i by XREAL_1:8;
       hence thesis by SEQ_1:11;
     end;
      d is bounded
     proof
       reconsider r=sup rng (a+b)+1 as real number;
        0 <= sup rng(a+b)
       proof
A41:     (a+b).1 =a.1 + b.1 by SEQ_1:11;
          a.1=|.((seq_id tseq -seq_id(vseq.m)).1).| by COMSEQ_1:def 14;
         then A42:    0<= a.1 by COMPLEX1:132;
          b.1=|.( (seq_id(vseq.m)).1 ).| by COMSEQ_1:def 14;
         then 0<= b.1 by COMPLEX1:132;
         then 0 + 0 <= a.1+ b.1 by A42,XREAL_1:9;
         hence 0<= sup rng(a+b) by A37,A41,Lm2;
       end;
then A43:   0+0 < sup( rng(a+b) )+1 by XREAL_1:10;
A44:   sup( rng(a+b) ) +0 < sup( rng(a+b) )+1 by XREAL_1:10;
        now let i be Nat;
A45:     d.i <= (a+b).i by A38;
          (a+b).i <= sup rng (a+b) by A37,Lm2;
then A46:     d.i <= sup rng (a+b) by A45,XREAL_1:2;
          d.i=|.((seq_id tseq).i).| by COMSEQ_1:def 14;
         hence |.((seq_id tseq).i).| <r by A44,A46,XREAL_1:2;
       end;
       then seq_id tseq is bounded by A43,COMSEQ_2:8;
       hence thesis by Lm9;
     end;
     hence thesis by Lm10;
   end;
A47:tseq in the_set_of_ComplexSequences by CSSPACE:def 1;
   then reconsider tv=tseq as Point of Complex_linfty_Space by A33,Def1;

    ex tv be Point of Complex_linfty_Space st
    for e1 be Real st e1 > 0
     ex m be Nat st
      for n be Nat st n >= m holds ||.(vseq.n) - tv.|| < e1
   proof
     take tv;
     let e1 be Real such that
A48: e1 > 0;
     set e=e1/2;
A49: e > 0 by A48,SEQ_2:3;
A50: e < e1 by A48,XREAL_1:218;
     consider m be Nat such that
A51: for n be Nat st n >= m holds
      |.(seq_id tseq-seq_id(vseq.n)).| is bounded &
     sup rng |.(seq_id tseq-seq_id(vseq.n)).| <= e by A12,A49;
      now let n be Nat such that
A52:   n >= m;
       reconsider u=tseq as VECTOR of Complex_linfty_Space
         by A33,A47,Def1;
A53:   sup rng( |.(seq_id tseq-seq_id(vseq.n)).|) <= e by A51,A52;
       reconsider v=vseq.n as VECTOR of Complex_linfty_Space;
        seq_id(u-v) = u-v by Th3;
       then sup rng |.seq_id(u-v).|
         = sup rng |.(seq_id tseq-seq_id(vseq.n)).| by Th3;
then A54:   (the norm of Complex_linfty_Space).(u-v) <= e by A53,Def2;
        ||.(vseq.n) - tv.|| =||.(vseq.n) +-tv.|| by RLVECT_1:def 11
        .=||.-(tv-(vseq.n)).|| by RLVECT_1:47
        .=||.tv-(vseq.n).|| by CLVECT_1:104;
       then ||.(vseq.n) - tv.|| <= e by A54,CLVECT_1:def 9;
       hence ||.(vseq.n) - tv.|| < e1 by A50,XREAL_1:2;
     end;
     hence thesis;
   end;
   hence thesis by CLVECT_1:def 15;
end;

theorem
 Complex_linfty_Space is ComplexBanachSpace by Th5,CLOPBAN1:def 14;

begin  :: Another example of complex Banach space

definition
  let X be non empty set;
  let Y be ComplexNormSpace;
  let IT be Function of X, the carrier of Y;
  attr IT is bounded means :Def4:
  ex K being Real st 0 <= K &
   for x being Element of X holds
   ||. IT.x .|| <= K;
end;

theorem Th7:
for X be non empty set, Y be ComplexNormSpace,
 f be Function of X,the carrier of Y st
  (for x be Element of X holds f.x=0.Y) holds f is bounded
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f be Function of X,the carrier of Y such that
A1:for x be Element of X holds f.x=0.Y;
A2:now
     let x be Element of X;
     thus ||. f.x .|| = ||. 0.Y .|| by A1
         .=0 by CLVECT_1:103;
   end;
   take 0;
   thus thesis by A2;
end;

registration
  let X be non empty set;
  let Y be ComplexNormSpace;
  cluster bounded Function of X,the carrier of Y;
existence
proof
  set f=X --> 0.Y;
  reconsider f as Function of X,the carrier of Y by FUNCOP_1:58;
  take f;
     for x be Element of X holds f.x =0.Y by FUNCOP_1:13;
  hence thesis by Th7;
end;
end;

definition
  let X be non empty set;
  let Y be ComplexNormSpace;
  func ComplexBoundedFunctions(X,Y) ->
    Subset of ComplexVectSpace(X,Y) means :Def5:
   for x being set holds x in it
     iff
   x is bounded Function of X,the carrier of Y;
existence
proof
   defpred P[set] means $1 is bounded Function of X,the carrier of Y;
   consider IT being set such that
A1:for x being set holds x in IT
    iff x in Funcs(X,the carrier of Y)  & P[x] from XBOOLE_0:sch 1;
A2:IT is Subset of Funcs(X,the carrier of Y)
   proof
      for x be set st x in IT holds x in Funcs(X,the carrier of Y) by A1;
     hence thesis by TARSKI:def 3;
   end;
A3:the carrier of ComplexVectSpace(X,Y) = Funcs(X,the carrier of Y)
   proof
      ComplexVectSpace(X,Y) = CLSStruct(#Funcs(X,the carrier of Y),
              (FuncZero(X,Y)),FuncAdd(X,Y),FuncExtMult(X,Y)#)
              by CLOPBAN1:def 2;
     hence thesis;
   end;
   take IT;
   thus IT is Subset of ComplexVectSpace(X,Y) by A2,A3;
   let x be set;
   thus x in IT implies x is bounded Function of X,the carrier of Y by A1;
   assume
A4:x is bounded Function of X,the carrier of Y;
   then x in Funcs(X,the carrier of Y) by FUNCT_2:11;
   hence thesis by A1,A4;
end;
uniqueness
proof
   let X1,X2 be Subset of ComplexVectSpace(X,Y);
   assume that
A5:for x being set holds x in X1 iff
     x is bounded Function of X,the carrier of Y and
A6:for x being set holds x in X2 iff
     x is bounded Function of X,the carrier of Y;
    for x being set st x in X1 holds x in X2
   proof
     let x be set;
     assume x in X1;
     then x is bounded Function of X,the carrier of Y by A5;
     hence thesis by A6;
   end;
then A7:X1 c= X2 by TARSKI:def 3;
    for x being set st x in X2 holds x in X1
   proof
     let x be set;
     assume x in X2;
     then x is bounded Function of X,the carrier of Y by A6;
     hence thesis by A5;
   end;
   then X2 c= X1 by TARSKI:def 3;
   hence thesis by A7,XBOOLE_0:def 10;
end;
end;

registration
  let X be non empty set;
  let Y be ComplexNormSpace;
  cluster ComplexBoundedFunctions(X,Y) -> non empty;
coherence
proof
   set f=X --> 0.Y;
   reconsider f as  Function of X,the carrier of Y by FUNCOP_1:58;
    f is bounded
   proof
      for x be Element of X holds f.x =0.Y by FUNCOP_1:13;
     hence thesis by Th7;
   end;
   hence thesis by Def5;
end;
