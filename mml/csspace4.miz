canceled;

theorem Th11:
for X be non empty set for  Y be ComplexNormSpace,
 f,g,h be VECTOR of C_VectorSpace_of_BoundedFunctions(X,Y),
  f',g',h' be bounded Function of X,the carrier of Y st
    f'=f & g'=g & h'=h holds
     (h = f+g iff (for x be Element of X holds (h'.x = f'.x + g'.x)) )
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f,g,h be VECTOR of C_VectorSpace_of_BoundedFunctions(X,Y);
   let f',g',h' be bounded Function of X,the carrier of Y such that
A1:f'=f and
A2:g'=g and
A3:h'=h;
    C_VectorSpace_of_BoundedFunctions(X,Y)=
    CLSStruct (# ComplexBoundedFunctions(X,Y),
      Zero_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
      Add_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
      Mult_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y))#);
then A4:C_VectorSpace_of_BoundedFunctions(X,Y) is Subspace of ComplexVectSpace(
X,Y)
     by Th9;
   then reconsider f1=f as VECTOR of ComplexVectSpace(X,Y) by CLVECT_1:30;
   reconsider g1=g as VECTOR of ComplexVectSpace(X,Y) by A4,CLVECT_1:30;
   reconsider h1=h as VECTOR of ComplexVectSpace(X,Y) by A4,CLVECT_1:30;
   reconsider f1'=f', g1'=g', h1'=h' as Element
            of Funcs(X, the carrier of Y) by FUNCT_2:11;
A5:now assume
A6:  h = f+g;
     let x be Element of X;
      h1=f1+g1 by A4,A6,CLVECT_1:33;
     hence h'.x=f'.x+g'.x by A1,A2,A3,CLOPBAN1:12;
   end;
    now assume
      for x be Element of X holds h'.x=f'.x+g'.x;
     then h1=f1+g1 by A1,A2,A3,CLOPBAN1:12;
     hence h =f+g by A4,CLVECT_1:33;
   end;
   hence thesis by A5;
end;

theorem Th12:
for X be non empty set for  Y be ComplexNormSpace,
 f,h be VECTOR of C_VectorSpace_of_BoundedFunctions(X,Y),
  f',h' be bounded Function of X,the carrier of Y st f'=f & h'=h
for c be Complex holds
      h = c*f iff for x be Element of X holds h'.x = c*f'.x
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f,h be VECTOR of C_VectorSpace_of_BoundedFunctions(X,Y);
   let f',h' be bounded Function of X,the carrier of Y such that
A1:f'=f and
A2:h'=h;
   let c be Complex;
    C_VectorSpace_of_BoundedFunctions(X,Y)
    = CLSStruct (# ComplexBoundedFunctions(X,Y),
       Zero_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
       Add_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
       Mult_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)) #);
then A3:C_VectorSpace_of_BoundedFunctions(X,Y) is Subspace of ComplexVectSpace(
X,Y)
     by Th9;
   then reconsider f1=f, h1=h as VECTOR of ComplexVectSpace(X,Y) by CLVECT_1:30
;
   reconsider f1'=f', h1'=h' as Element of Funcs(X, the carrier of Y)
     by FUNCT_2:11;
A4:now assume
A5:  h = c*f;
     let x be Element of X;
      h1=c*f1 by A3,A5,CLVECT_1:34;
     hence h'.x=c*f'.x by A1,A2,CLOPBAN1:13;
   end;
    now assume
      for x be Element of X holds h'.x=c*f'.x;
     then h1=c*f1 by A1,A2,CLOPBAN1:13;
     hence h =c*f by A3,CLVECT_1:34;
   end;
   hence thesis by A4;
 end;

theorem Th13:
for X be non empty set, Y be ComplexNormSpace holds
 0.C_VectorSpace_of_BoundedFunctions(X,Y) = (X -->0.Y)
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
    C_VectorSpace_of_BoundedFunctions(X,Y)
    = CLSStruct (# ComplexBoundedFunctions(X,Y),
        Zero_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
        Add_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
        Mult_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)) #);
then A1:C_VectorSpace_of_BoundedFunctions(X,Y) is Subspace of ComplexVectSpace(
X,Y)
     by Th9;
    0.ComplexVectSpace(X,Y) = (X -->0.Y) by CLOPBAN1:14;
   hence thesis by A1,CLVECT_1:31;
end;

definition
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f be set such that
A1:f in ComplexBoundedFunctions(X,Y);
  func modetrans(f,X,Y)
   -> bounded Function of X,the carrier of Y equals:Def7:
    f;
  coherence by A1,Def5;
end;

definition
   let X be non empty set;
   let Y be ComplexNormSpace;
   let u be Function of X,the carrier of Y;
   func PreNorms(u) -> non empty Subset of REAL equals
     {||.u.t.|| where t is Element of X : not contradiction  };
coherence
proof
A1:now let x be set;
      now assume
        x in {||.u.t.|| where t is Element of X: not contradiction };
       then consider t be Element of X such that
A2:    x=||.u.t.||;
       thus x in REAL by A2;
     end;
     hence x in {||.u.t.|| where t is Element of X: not contradiction }
       implies x in REAL;
   end;
   consider x be Element of X;
    ||.u.x.|| in {||.u.t.|| where t is Element of X: not contradiction };
   hence thesis by A1,TARSKI:def 3;
end;
end;

theorem Th14:
for X be non empty set for Y be ComplexNormSpace,
 g be bounded Function of X,the carrier of Y holds
  PreNorms(g) is non empty bounded_above
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let g be bounded Function of X,the carrier of Y;
    PreNorms(g) is bounded_above
   proof
     consider K be Real such that
      0 <= K and
A1:  for x be Element of X holds ||. g.x .|| <= K by Def4;
A2:  now let r be Real such that
A3:    r in PreNorms(g);
        r in {||.g.t.|| where t is Element of X: not contradiction }
          by A3;
       then consider t be Element of X such that
A4:    r=||.g.t.||;
       thus r <=K by A1,A4;
     end;
     take K;
     thus thesis by A2;
   end;
   hence thesis;
end;

theorem
 for X be non empty set for Y be ComplexNormSpace,
 g be Function of X,the carrier of Y holds
  g is bounded iff PreNorms(g) is bounded_above
proof
   let X be non empty set; let Y be ComplexNormSpace;
   let g be Function of X,the carrier of Y;

    now assume
A1:  PreNorms(g) is bounded_above;
     reconsider K=sup PreNorms(g) as Real;
A2:  0 <= K
     proof
A3:    now let r be Real such that
A4:      r in PreNorms(g);
          r in {||.g.t.|| where t is Element of X : not contradiction  }
           by A4;
         then consider t be Element of X such that
A5:      r=||.g.t.||;
         thus 0 <= r by A5,CLVECT_1:106;
       end;
       consider r0 be set such that
A6:    r0 in PreNorms(g) by XBOOLE_0:def 1;
       reconsider r0 as Real by A6;
        0 <= r0 by A3,A6;
       hence thesis by A1,A6,SEQ_4:def 4;
     end;
A7:  now let t be Element of X;
        ||.g.t.|| in
         {||.g.s.|| where s is Element of X : not contradiction };
       then ||.g.t.|| in PreNorms(g);
       hence ||.g.t.|| <= K by A1,SEQ_4:def 4;
     end;
     take K;
     thus g is bounded by A2,A7,Def4;
   end;
   hence thesis by Th14;
end;

theorem Th16:
for X be non empty set, Y be ComplexNormSpace
  ex NORM be Function of ComplexBoundedFunctions(X,Y),REAL st
    for f be set st f in ComplexBoundedFunctions(X,Y) holds
      NORM.f = sup PreNorms(modetrans(f,X,Y))
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   deffunc F(set) = sup PreNorms(modetrans($1,X,Y));
A1:for z be set st z in ComplexBoundedFunctions(X,Y) holds F(z) in REAL;
      ex f being Function of ComplexBoundedFunctions(X,Y),REAL st
       for x being set st x in ComplexBoundedFunctions(X,Y) holds
       f.x = F(x) from FUNCT_2:sch 2(A1);
   hence thesis;
end;

definition
   let X be non empty set;
   let Y be ComplexNormSpace;
   func ComplexBoundedFunctionsNorm(X,Y)
     -> Function of ComplexBoundedFunctions(X,Y), REAL means :Def9:
  for x be set st x in ComplexBoundedFunctions(X,Y) holds
    it.x = sup PreNorms(modetrans(x,X,Y));
existence by Th16;
uniqueness
proof
   let NORM1,NORM2 be Function of ComplexBoundedFunctions(X,Y), REAL such that
A1: (for x be set st x in ComplexBoundedFunctions(X,Y) holds
      NORM1.x = sup PreNorms(modetrans(x,X,Y))) and
A2: (for x be set st x in ComplexBoundedFunctions(X,Y) holds
      NORM2.x = sup PreNorms(modetrans(x,X,Y)));
A3:dom NORM1 = ComplexBoundedFunctions(X,Y) &
   dom NORM2 = ComplexBoundedFunctions(X,Y) by FUNCT_2:def 1;
    for z be set st z in ComplexBoundedFunctions(X,Y) holds NORM1.z = NORM2.z
   proof
     let z be set such that
A4:  z in ComplexBoundedFunctions(X,Y);
      NORM1.z = sup PreNorms(modetrans(z,X,Y)) by A1,A4;
     hence thesis by A2,A4;
  end;
  hence thesis by A3,FUNCT_1:9;
end;
end;

theorem Th17:
for X be non empty set, Y be ComplexNormSpace,
 f be bounded Function of X,the carrier of Y holds modetrans(f,X,Y)=f
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f be bounded Function of X,the carrier of Y;
    f in ComplexBoundedFunctions(X,Y) by Def5;
   hence thesis by Def7;
end;

theorem Th18:
for X be non empty set, Y be ComplexNormSpace,
 f be bounded Function of X,the carrier of Y holds
   ComplexBoundedFunctionsNorm(X,Y).f = sup PreNorms(f)
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f be bounded Function of X,the carrier of Y;
A1:f in ComplexBoundedFunctions(X,Y) by Def5;
   reconsider f'=f as set;
   thus ComplexBoundedFunctionsNorm(X,Y).f
         = sup PreNorms(modetrans(f',X,Y)) by A1,Def9
        .= sup PreNorms(f) by Th17;
end;

definition
  let X be non empty set;
  let Y be ComplexNormSpace;
func C_NormSpace_of_BoundedFunctions(X,Y) -> non empty CNORMSTR equals
  CNORMSTR (# ComplexBoundedFunctions(X,Y),
    Zero_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
    Add_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
    Mult_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
          ComplexBoundedFunctionsNorm(X,Y) #);
coherence;
end;

theorem Th19:
for X be non empty set, Y be ComplexNormSpace holds
  (X --> 0.Y) = 0.C_NormSpace_of_BoundedFunctions(X,Y)
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
    (X --> 0.Y)
     =0.C_VectorSpace_of_BoundedFunctions(X,Y) by Th13
    .=Zero_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y))
        by RLVECT_1:def 2
    .=0.C_NormSpace_of_BoundedFunctions(X,Y) by RLVECT_1:def 2;
   hence thesis;
end;

theorem Th20:
for X be non empty set, Y be ComplexNormSpace,
 f being Point of C_NormSpace_of_BoundedFunctions(X,Y),
  g be bounded Function of X,the carrier of Y st g=f holds
    for t be Element of X holds ||.g.t.|| <= ||.f.||
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f being Point of C_NormSpace_of_BoundedFunctions(X,Y);
   let g be bounded Function of X,the carrier of Y such that
A1:g=f;
A3:PreNorms(g) is non empty bounded_above by Th14;
    now let t be Element of X;
      ||.g.t.|| in
        {||.g.s.|| where s is Element of X: not contradiction};
     then ||.g.t.|| in PreNorms(g);
     then ||.g.t.|| <= sup PreNorms(g) by A3,SEQ_4:def 4;
     then ||.g.t.|| <= ComplexBoundedFunctionsNorm(X,Y).g by Th18;
     hence ||.g.t.|| <= ||.f.|| by A1,CLVECT_1:def 9;
   end;
   hence thesis;
end;

theorem
 for X be non empty set, Y be ComplexNormSpace,
 f being Point of C_NormSpace_of_BoundedFunctions(X,Y) holds
  0 <= ||.f.||
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f being Point of C_NormSpace_of_BoundedFunctions(X,Y);
C_NormSpace_of_BoundedFunctions(X,Y)
    = CNORMSTR (# ComplexBoundedFunctions(X,Y),
         Zero_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
         Add_(ComplexBoundedFunctions(X,Y), ComplexVectSpace(X,Y)),
         Mult_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
           ComplexBoundedFunctionsNorm(X,Y) #);
   then reconsider g=f as bounded Function of X,the carrier of Y by Def5;
A2:now let r be Real such that
A3:  r in PreNorms(g);
      r in {||.g.t.|| where t is Element of X: not contradiction }
       by A3;
     then consider t be Element of X such that
A4:  r=||.g.t.||;
     thus 0 <= r by A4,CLVECT_1:106;
   end;
A5:PreNorms(g) is non empty bounded_above by Th14;
   consider r0 be set such that
A6:r0 in PreNorms(g) by XBOOLE_0:def 1;
   reconsider r0 as Real by A6;
    0 <= r0 by A2,A6;
then A7:0 <=sup PreNorms(g) by A5,A6,SEQ_4:def 4;
    ComplexBoundedFunctionsNorm(X,Y).f = sup PreNorms(g) by Th18;
   hence thesis by A7,CLVECT_1:def 9;
end;

theorem Th22:
for X be non empty set, Y be ComplexNormSpace,
 f being Point of C_NormSpace_of_BoundedFunctions(X,Y) st
  f = 0.C_NormSpace_of_BoundedFunctions(X,Y) holds 0 = ||.f.||
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f being Point of C_NormSpace_of_BoundedFunctions(X,Y) such that
A1: f = 0.C_NormSpace_of_BoundedFunctions(X,Y);
   thus ||.f.|| = 0
   proof
     set z = X --> 0.Y;
     reconsider z as Function of X, the carrier of Y by FUNCOP_1:58;
     reconsider g=f as bounded Function of X,
     the carrier of Y by Def5;
A3:  z=g by A1,Th19;
A4:  now let r be Real such that
A5:    r in PreNorms(g);
        r in {||.g.t.|| where t is Element of X: not contradiction}
         by A5;
       then consider t be Element of X such that
A6:    r=||.g.t.||;
        ||.g.t.|| = ||.0.Y.|| by A3,FUNCOP_1:13 .= 0 by CLVECT_1:def 10;
       hence 0 <= r & r <=0 by A6;
     end;
A7:  PreNorms(g) is non empty bounded_above by Th14;
     consider r0 be set such that
A8:  r0 in PreNorms(g) by XBOOLE_0:def 1;
     reconsider r0 as Real by A8;
A9:  0 <= r0 & r0<=0 by A4,A8;
      (for s be real number st s in PreNorms(g) holds s <= 0)
      implies sup PreNorms(g) <= 0 by SEQ_4:62;
     then sup PreNorms(g) = 0 by A4,A7,A8,A9,SEQ_4:def 4;
     then ComplexBoundedFunctionsNorm(X,Y).f=0 by Th18;
     hence thesis by CLVECT_1:def 9;
   end;
end;

theorem Th23:
for X be non empty set, Y be ComplexNormSpace,
 f,g,h be Point of C_NormSpace_of_BoundedFunctions(X,Y),
  f',g',h' be bounded Function of X,the carrier of Y st
   f'=f & g'=g & h'=h holds
     ( h = f+g iff (for x be Element of X holds (h'.x = f'.x + g'.x)) )
proof
   let X be non empty set;let Y be ComplexNormSpace;
   let f,g,h be Point of C_NormSpace_of_BoundedFunctions(X,Y);
   let f',g',h' be bounded Function of X,the carrier of Y such that
A1:f'=f and
A2:g'=g and
A3:h'=h;
C_VectorSpace_of_BoundedFunctions(X,Y)
    = CLSStruct (# ComplexBoundedFunctions(X,Y),
       Zero_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
       Add_(ComplexBoundedFunctions(X,Y), ComplexVectSpace(X,Y)),
       Mult_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)) #);
   then reconsider f1=f, g1=g, h1=h as VECTOR of
     C_VectorSpace_of_BoundedFunctions(X,Y);
    h=f+g iff h1=f1+g1
   proof
A6:  now
       assume h=f+g;
       hence h1=Add_(ComplexBoundedFunctions(X,Y),
         ComplexVectSpace(X,Y)).[f,g] by RLVECT_1:def 3
          .=f1+g1 by RLVECT_1:def 3;
     end;
      now
       assume h1=f1+g1;
       hence h=Add_(ComplexBoundedFunctions(X,Y),
         ComplexVectSpace(X,Y))
         .[f1,g1] by RLVECT_1:def 3
      .=f+g by RLVECT_1:def 3;
     end;
     hence thesis by A6;
   end;
   hence thesis by A1,A2,A3,Th11;
end;

theorem Th24:
for X be non empty set, Y be ComplexNormSpace,
 f,h be Point of C_NormSpace_of_BoundedFunctions(X,Y),
  f',h' be bounded Function of X,the carrier of Y st f'=f & h'=h
   for c be Complex holds
      h = c*f iff for x be Element of X holds h'.x = c*f'.x
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f,h be Point of C_NormSpace_of_BoundedFunctions(X,Y);
   let f',h' be bounded Function of X,the carrier of Y such that
A1:f'=f and
A2:h'=h;
   let c be Complex;
C_VectorSpace_of_BoundedFunctions(X,Y)
    = CLSStruct (# ComplexBoundedFunctions(X,Y),
        Zero_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
        Add_(ComplexBoundedFunctions(X,Y), ComplexVectSpace(X,Y)),
        Mult_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)) #);
   then reconsider f1=f as VECTOR of C_VectorSpace_of_BoundedFunctions(X,Y);
   reconsider h1=h as VECTOR of C_VectorSpace_of_BoundedFunctions(X,Y);
    h=c*f iff h1=c*f1
   proof
A5:  now
       assume h=c*f;
       hence h1=Mult_(ComplexBoundedFunctions(X,Y),
         ComplexVectSpace(X,Y)).[c,f] by CLVECT_1:def 1
          .=c*f1 by CLVECT_1:def 1;
     end;
      now
       assume h1=c*f1;
       hence h=Mult_(ComplexBoundedFunctions(X,Y),
         ComplexVectSpace(X,Y))
         .[c,f1] by CLVECT_1:def 1
      .=c*f by CLVECT_1:def 1;
     end;
     hence thesis by A5;
   end;
   hence thesis by A1,A2,Th12;
end;

theorem Th25:
for X be non empty set, Y be ComplexNormSpace,
 f,g being Point of C_NormSpace_of_BoundedFunctions(X,Y),
  c be Complex holds
   ( ||.f.|| = 0 iff
     f = 0.C_NormSpace_of_BoundedFunctions(X,Y) ) &
     ||.c*f.|| = |.c.| * ||.f.|| &
     ||.f+g.|| <= ||.f.|| + ||.g.||
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f, g being Point of C_NormSpace_of_BoundedFunctions(X,Y);
   let c be Complex;
A3:now assume
A4:  ||.f.|| = 0;
     reconsider g=f as bounded Function of X,the carrier of Y by Def5;
     set z = X --> 0.Y;
     reconsider z as Function of X, the carrier of Y by FUNCOP_1:58;
      now let t be Element of X;
        ||.g.t.|| <= ||.f.||  by Th20;
       then ||.g.t.|| = 0 by A4,CLVECT_1:106;
       hence g.t =0.Y by CLVECT_1:def 10
                .=z.t by FUNCOP_1:13;
     end;
     then g=z by FUNCT_2:113;
     hence f=0.C_NormSpace_of_BoundedFunctions(X,Y) by Th19;
   end;

A5:now assume
A6:  f = 0.C_NormSpace_of_BoundedFunctions(X,Y);
     thus ||.f.|| = 0
     proof
       set z = X --> 0.Y;
       reconsider z as Function of X, the carrier of Y by FUNCOP_1:58;
       reconsider g=f as bounded Function of X,the carrier of Y by Def5;
A7:    z=g by A6,Th19;
A8:    now let r be Real such that
A9:      r in PreNorms(g);
          r in {||.g.t.|| where t is Element of X:not contradiction }
           by A9;
         then consider t be Element of X such that
A10:     r=||.g.t.||;
          ||.g.t.|| = ||.0.Y.|| by A7,FUNCOP_1:13
                  .= 0 by CLVECT_1:def 10;
         hence 0 <= r & r <=0 by A10;
       end;
A11:   PreNorms(g) is non empty bounded_above by Th14;
       consider r0 be set such that
A12:   r0 in PreNorms(g) by XBOOLE_0:def 1;
       reconsider r0 as Real by A12;
A13:   0<=r0 by A8,A12;
        (for s be real number st s in PreNorms(g) holds s <= 0)
         implies sup PreNorms(g) <= 0 by SEQ_4:62;
       then sup PreNorms(g) = 0 by A8,A11,A12,A13,SEQ_4:def 4;
       then ComplexBoundedFunctionsNorm(X,Y).f =0 by Th18;
       hence thesis by CLVECT_1:def 9;
     end;
   end;

A14:||.c*f.|| = |.c.| * ||.f.||
   proof
     reconsider f1=f, h1=c*f as bounded Function of X, the carrier of Y
       by Def5;
A15: now let t be Element of  X;
A16: ||.h1.t.||= ||.c*f1.t.|| by Th24;
A17: ||.c*f1.t.|| =|.c.|*||.f1.t.|| by CLVECT_1:def 10;
A18: ||.f1.t.||<= ||.f.|| by Th20;
      0<= |.c.| by COMPLEX1:132;
     hence ||.h1.t.|| <= |.c.|*||.f.|| by A16,A17,A18,XREAL_1:66;
   end;
A19:now let r be Real such that
A20: r in PreNorms(h1);
      r in {||.h1.t.|| where t is Element of  X: not contradiction  }
       by A20;
     then consider t be Element of X such that
A21: r=||.h1.t.||;
     thus r <= |.c.|*||.f.|| by A15,A21;
   end;

A22:
   (for s be real number st s in PreNorms(h1) holds s <= |.c.|*||.f.|| )
     implies sup PreNorms(h1) <=  |.c.|*||.f.|| by SEQ_4:62;
    ComplexBoundedFunctionsNorm(X,Y).(c*f)
    = sup PreNorms(h1) by Th18;
then A23:||.c*f.|| <= |.c.|*||.f.|| by A19,A22,CLVECT_1:def 9;
    now per cases;
     case
A24:   c <> 0c;
A25:   now let t be Element of X;
          h1.t=c*f1.t by Th24;
then A26:     c"*h1.t =( c"* c)*f1.t by CLVECT_1:def 2
               .=1r*f1.t by A24,COMPLEX1:def 7,XCMPLX_0:def 7
               .=f1.t by CLVECT_1:def 2;
A27:     ||.c"*h1.t.|| =|.c".|*||.h1.t.|| by CLVECT_1:def 10;
A28:     ||.h1.t.||<= ||.c*f.|| by Th20;
A29:     0<= |.c".| by COMPLEX1:132;
          |.c".| = |.1r/c.| by COMPLEX1:def 7,XCMPLX_1:217
             .= 1/|.c.| by COMPLEX1:134,153
             .= 1*(|.c.|)" by XCMPLX_0:def 9
             .= |.c.|";
         hence ||.f1.t.|| <= |.c.|"*||.c*f.|| by A26,A27,A28,A29,XREAL_1:66;
       end;
A30:   now let r be Real such that
A31:     r in PreNorms(f1);
          r in {||.f1.t.|| where t is Element of  X: not contradiction }
            by A31;
         then consider t be Element of X such that
A32:     r=||.f1.t.||;
         thus r <= |.c.|"*||.c*f.|| by A25,A32;
       end;
A33:   |.c.| <>0 by A24,COMPLEX1:133;
A34:   (for s be real number st s in PreNorms(f1) holds s <= |.c.|"*||.c*f.|| )
         implies sup PreNorms(f1) <= |.c.|"*||.c*f.|| by SEQ_4:62;
        ComplexBoundedFunctionsNorm(X,Y).(f) = sup PreNorms(f1) by Th18;
then A35:   ||.f.|| <=|.c.|"*||.c*f.|| by A30,A34,CLVECT_1:def 9;
        0 <= |.c.| by COMPLEX1:132;
       then |.c.|*||.f.|| <= |.c.|*(|.c.|"*||.c*f.||) by A35,XREAL_1:66;
       then |.c.|*||.f.|| <=(|.c.|*|.c.|")*||.c*f.||;
       then |.c.|*||.f.|| <=1*||.c*f.|| by A33,XCMPLX_0:def 7;
       hence |.c.|* ||.f.|| <=||.c*f.||;
     end;
     case
A36:   c = 0c;
       reconsider fz=f as VECTOR of C_VectorSpace_of_BoundedFunctions(X,Y);
   c*f =Mult_(ComplexBoundedFunctions(X,Y),
            ComplexVectSpace(X,Y)).[c,f] by CLVECT_1:def 1
        .=c*fz by CLVECT_1:def 1
        .=0.C_VectorSpace_of_BoundedFunctions(X,Y) by A36,CLVECT_1:2
        .=Zero_(ComplexBoundedFunctions(X,Y),
          ComplexVectSpace(X,Y)) by RLVECT_1:def 2
        .=0.C_NormSpace_of_BoundedFunctions(X,Y) by RLVECT_1:def 2;
       hence |.c.|* ||.f.|| =||.c*f.|| by A36,Th22,COMPLEX1:130;
       end;
     end;
     hence thesis by A23,XREAL_1:1;
   end;
    ||.f+g.|| <= ||.f.|| + ||.g.||
   proof
     reconsider f1=f, g1=g, h1=f+g as
        bounded Function of X,the carrier of Y by Def5;
A37: now let t be Element of X;
A38:   ||.h1.t.||= ||.f1.t+g1.t.|| by Th23;
A39:   ||.f1.t+g1.t.|| <=||.f1.t.||+||.g1.t.|| by CLVECT_1:def 10;
A40:   ||.f1.t.||<= ||.f.|| by Th20;
        ||.g1.t.||<= ||.g.|| by Th20;
       then ||.f1.t.||+||.g1.t.|| <= ||.f.|| + ||.g.|| by A40,XREAL_1:9;
       hence ||.h1.t.|| <= ||.f.|| + ||.g.|| by A38,A39,XREAL_1:2;
     end;
A41: now let r be Real such that
A42:   r in PreNorms(h1);
        r in {||.h1.t.|| where t is Element of X: not contradiction}
         by A42;
       then consider t be Element of X such that
A43:   r=||.h1.t.||;
       thus r <= ||.f.|| + ||.g.|| by A37,A43;
     end;
A44: (for s be real number st s in PreNorms(h1) holds s <= ||.f.|| + ||.g.||)
       implies sup PreNorms(h1) <= ||.f.|| + ||.g.|| by SEQ_4:62;
      ComplexBoundedFunctionsNorm(X,Y).(f+g) = sup PreNorms(h1) by Th18;
     hence ||.f+g.|| <=||.f.|| + ||.g.|| by A41,A44,CLVECT_1:def 9;
   end;
   hence thesis by A3,A5,A14;
end;

theorem Th26:
for X be non empty set, Y be ComplexNormSpace holds
 C_NormSpace_of_BoundedFunctions(X,Y) is ComplexNormSpace-like
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
    for x, y being Point of C_NormSpace_of_BoundedFunctions(X,Y)
   for c be Complex holds
    ( ||.x.|| = 0 iff
      x = 0.C_NormSpace_of_BoundedFunctions(X,Y) ) &
    ||.c*x.|| = |.c.| * ||.x.|| &
    ||.x+y.|| <= ||.x.|| +  ||.y.|| by Th25;
   hence thesis by CLVECT_1:def 10;
end;

theorem Th27:
for X be non empty set, Y be ComplexNormSpace holds
  C_NormSpace_of_BoundedFunctions(X,Y) is ComplexNormSpace
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
    CLSStruct (# ComplexBoundedFunctions(X,Y),
        Zero_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)),
        Add_(ComplexBoundedFunctions(X,Y), ComplexVectSpace(X,Y)),
        Mult_(ComplexBoundedFunctions(X,Y),ComplexVectSpace(X,Y)) #)
          is ComplexLinearSpace;
   hence thesis by Th26,CSSPACE3:4;
end;

registration
   let X be non empty set;
   let Y be ComplexNormSpace;
   cluster C_NormSpace_of_BoundedFunctions(X,Y) ->
           ComplexNormSpace-like ComplexLinearSpace-like
           Abelian add-associative right_zeroed right_complementable;
   coherence by Th27;
end;

theorem Th28:
for X be non empty set for Y be ComplexNormSpace,
 f,g,h be Point of C_NormSpace_of_BoundedFunctions(X,Y),
 f',g',h' be bounded Function of X,the carrier of Y st
  f'=f & g'=g & h'=h holds
   (h = f-g iff (for x be Element of X holds (h'.x = f'.x - g'.x)) )
proof
   let X be non empty set;
   let Y be ComplexNormSpace;
   let f,g,h be Point of C_NormSpace_of_BoundedFunctions(X,Y);
   let f',g',h' be bounded Function of X,the carrier of Y such that
A1:f'=f and
A2:g'=g and
A3:h'=h;
A4:now assume h=f-g;
     then h+g=f-(g-g) by RLVECT_1:43;
     then h+g=f-0.C_NormSpace_of_BoundedFunctions(X,Y) by RLVECT_1:28;
then A5:  h+g=f by RLVECT_1:26;
      now let x be Element of  X;
        f'.x=h'.x +  g'.x by A1,A2,A3,A5,Th23;
       then f'.x-g'.x=h'.x + (g'.x-g'.x) by RLVECT_1:42;
       then f'.x-g'.x=h'.x + 0.Y by RLVECT_1:28;
       hence f'.x-g'.x=h'.x by RLVECT_1:10;
     end;
     hence for x be Element of X holds (h'.x = f'.x - g'.x);
   end;
    now assume
A6:  for x be Element of X holds (h'.x = f'.x - g'.x);
        now let x be Element of  X;
          h'.x = f'.x - g'.x by A6;
       then h'.x + g'.x= f'.x - (g'.x- g'.x) by RLVECT_1:43;
       then h'.x + g'.x= f'.x - 0.Y by RLVECT_1:28;
       hence h'.x + g'.x= f'.x by RLVECT_1:26;
     end;
     then f=h+g by A1,A2,A3,Th23;
     then f-g=h+(g-g) by RLVECT_1:42;
     then f-g=h+0.C_NormSpace_of_BoundedFunctions(X,Y) by RLVECT_1:28;
     hence f-g=h by RLVECT_1:10;
   end;
   hence thesis by A4;
end;

Lm13:
for e be Real, seq be Real_Sequence st ( seq is convergent &
 ex k be Nat st (for i be Nat st k <= i holds seq.i <=e ) ) holds
  lim seq <=e
proof
   let e be Real;
   let seq be Real_Sequence such that
A1:seq is convergent and
A2:ex k be Nat st (for i be Nat st k <= i holds seq.i <=e );
   reconsider cseq = NAT --> e as Real_Sequence by FUNCOP_1:57;
A3:for n be Nat holds cseq.n=e by FUNCOP_1:13;
A5:cseq is convergent by SEQ_4:39;
   consider k be Nat such that
A6:for i be Nat st k <= i holds seq.i <=e by A2;
A7:(seq ^\k) is convergent by A1,SEQ_4:33;
A8:lim (seq ^\k)=lim seq by A1,SEQ_4:33;
A9:now let i be Nat;
A10: (seq ^\k).i = seq.(k+i) by SEQM_3:def 9;
        k <= k+i by NAT_1:29;
     then seq.(k+i) <=e by A6;
     hence (seq ^\k) .i <= cseq.i by A3,A10;
  end;
     lim cseq = cseq.0 by SEQ_4:41 .= e by A3;
  hence thesis by A5,A7,A8,A9,SEQ_2:32;
end;

theorem Th29:
for X be non empty set, Y be ComplexNormSpace st Y is complete
 for seq be sequence of C_NormSpace_of_BoundedFunctions(X,Y) st
  seq is Cauchy_sequence_by_Norm holds seq is convergent
proof
   let X be non empty set;
   let Y be ComplexNormSpace such that
A1:Y is complete;
   let vseq be sequence of C_NormSpace_of_BoundedFunctions(X,Y)
    such that
A2:vseq is Cauchy_sequence_by_Norm;
A3:||.vseq.|| is convergent
   proof
      now let e1 be real number such that
A4:    e1 >0;
       reconsider e =e1 as Real by XREAL_0:def 1;
       consider k be Nat such that
A5:    for n, m be Nat st n >= k & m >= k holds
        ||.(vseq.n) - (vseq.m).|| < e by A2,A4,CSSPACE3:10;
A6:    now
         let m be Nat such that
A7:      m >= k;
A8:      abs( ||.vseq.m.||- ||.vseq.k.|| )
           <= ||.(vseq.m) - (vseq.k).|| by CLVECT_1:111;
A9:      ||.vseq.k.||= ||.vseq.||.k by CLVECT_1:def 16;
A10:     ||.vseq.m.||= ||.vseq.||.m by CLVECT_1:def 16;
          ||.(vseq.m) - (vseq.k).|| <e by A5,A7;
         hence abs( ||.vseq.||.m - ||.vseq.||.k ) <e1
           by A8,A9,A10,XREAL_1:2;
       end;
       take k;
       thus for m be Nat st m >= k holds
             abs(||.vseq.||.m - ||.vseq.||.k ) < e1 by A6;
     end;
     hence ||.vseq.|| is convergent by SEQ_4:58;
   end;
   defpred P[set, set] means ex xseq be sequence of Y st
    (for n be Nat holds xseq.n=modetrans((vseq.n),X,Y).$1)
    & xseq is convergent & $2= lim xseq;
A12:for x be Element of X ex y be Element of Y st  P[x,y]
   proof
     let x be Element of X;
     deffunc F(Nat) = modetrans((vseq.$1),X,Y).x;
     consider xseq be sequence of Y such that
A13:  for n be Nat holds xseq.n = F(n) from FUNCT_2:sch 4;
A14: for m,k be Nat holds
       ||.xseq.m-xseq.k.|| <= ||.vseq.m - vseq.k.||
     proof
       let m,k be Nat;
        vseq.m is bounded Function of X,the carrier of Y by Def5;
then A15:   modetrans((vseq.m),X,Y)=vseq.m by Th17;
        vseq.k is bounded Function of X,the carrier of Y by Def5;
then A16:   modetrans((vseq.k),X,Y)=vseq.k by Th17;
       reconsider h1=vseq.m-vseq.k
         as bounded Function of X,the carrier of Y by Def5;
A17:   xseq.m = modetrans((vseq.m),X,Y).x by A13;
        xseq.k = modetrans((vseq.k),X,Y).x by A13;
       then xseq.m - xseq.k = h1.x by A15,A16,A17,Th28;
       hence thesis by Th20;
     end;
A18: xseq is convergent
     proof
        now
         let e be Real such that
A19:     e > 0;
         thus ex k be Nat st for n, m be Nat st n >= k & m >= k holds
           ||.xseq.n -xseq.m.|| < e
         proof
           consider k be Nat such that
A20:        for n, m be Nat st n >= k & m >= k holds
             ||.(vseq.n) - (vseq.m).|| < e by A2,A19,CSSPACE3:10;
           take k;
           thus for n, m be Nat st ( n >= k & m >= k )
                  holds ||.xseq.n-xseq.m.|| < e
           proof
             let n,m be Nat such that
A21:         n >=k & m >= k;
A22:         ||.xseq.n-xseq.m.|| <= ||.(vseq.n) - (vseq.m).|| by A14;
              ||.(vseq.n) - (vseq.m).|| < e by A20,A21;
             hence ||.xseq.n-xseq.m.|| < e by A22,XREAL_1:2;
           end;
         end;
       end;
       then xseq is Cauchy_sequence_by_Norm by CSSPACE3:10;
       hence thesis by A1,CLOPBAN1:def 14;
     end;
     take y = lim xseq;
     thus thesis by A13,A18;
   end;
   consider f be Function of  X,the carrier of Y such that
A23:for x be Element of X holds P[x,f.x] from FUNCT_2:sch 3(A12);
   reconsider tseq=f as Function of X,the carrier of Y;

A24:tseq is bounded
   proof
A25: now
       let x be Element of X;
       consider xseq be sequence of Y such that
A26:   (for n be Nat holds xseq.n=modetrans((vseq.n),X,Y).x)
       & xseq is convergent
       & tseq.x = lim xseq by A23;
A27:   for m be Nat holds ||.xseq.m.|| <= ||.vseq.m.||
       proof
         let m be Nat;
          vseq.m is bounded Function of X,the carrier of Y by Def5;
then A28:     modetrans((vseq.m),X,Y)=vseq.m by Th17;
         reconsider h1=vseq.m as bounded Function of X,the carrier of Y
          by Def5;
          xseq.m =modetrans((vseq.m),X,Y).x by A26;
         hence thesis by A28,Th20;
       end;
A29:   ||.xseq.|| is convergent by A26,CLOPBAN1:45;
A30:   ||.tseq.x.|| = lim ||.xseq.|| by A26,CLOPBAN1:45;
        for n be Nat holds ||.xseq.||.n  <=(||.vseq.||).n
       proof
         let n be Nat;
A31:     ||.xseq.||.n = ||.(xseq.n).|| by CLVECT_1:def 16;
          ||.(xseq.n).|| <= ||.vseq.n.|| by A27;
         hence thesis by A31,CLVECT_1:def 16;
       end;
       hence ||.tseq.x.|| <= lim (||.vseq.|| ) by A3,A29,A30,SEQ_2:32;
     end;
     take K= lim (||.vseq.|| );
      K >=0
     proof
        now let n be Nat;
          ||.vseq.n.|| >=0 by CLVECT_1:106;
         hence ||.vseq.||.n >=0 by CLVECT_1:def 16;
       end;
       hence thesis by A3,SEQ_2:31;
     end;
     hence thesis by A25;
   end;

A32:for e be Real st
    e > 0 ex k be Nat st for n be Nat st n >= k holds
     for x be Element of X holds
      ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e
   proof
     let e be Real such that
A33: e > 0;
     consider k be Nat such that
A34: for n, m be Nat st ( n >= k & m >= k ) holds
      ||.(vseq.n) - (vseq.m).|| < e by A2,A33,CSSPACE3:10;
A35: now let n be Nat such that
A36:   n >= k;
        now let x be Element of X;
         consider xseq be sequence of Y such that
A37:      (for n be Nat holds  xseq.n=modetrans((vseq.n),X,Y).x)
          & xseq is convergent
          & tseq.x = lim xseq by A23;
A38:     for m,k be Nat holds
          ||.xseq.m-xseq.k.|| <= ||.vseq.m - vseq.k.||
         proof
           let m,k be Nat;
            vseq.m is bounded Function of X,the carrier of Y by Def5;
then A39:       modetrans((vseq.m),X,Y)=vseq.m by Th17;
            vseq.k is bounded Function of X,the carrier of Y by Def5;
then A40:       modetrans((vseq.k),X,Y)=vseq.k by Th17;
           reconsider h1=vseq.m-vseq.k
             as bounded Function of X,the carrier of Y by Def5;
A41:       xseq.m =modetrans((vseq.m),X,Y).x by A37;
            xseq.k =modetrans((vseq.k),X,Y).x by A37;
           then xseq.m - xseq.k =h1.x by A39,A40,A41,Th28;
           hence thesis by Th20;
         end;
A42:     for m be Nat st m >=k holds ||.xseq.n-xseq.m.|| <= e
         proof
           let m be Nat such that
A43:       m >=k;
A44:       ||.xseq.n-xseq.m.|| <= ||.vseq.n - vseq.m.|| by A38;
            ||.vseq.n - vseq.m.|| <e by A34,A36,A43;
           hence thesis by A44,XREAL_1:2;
         end;
          ||.xseq.n-tseq.x.|| <= e
         proof
           deffunc F(Nat) = ||.xseq.$1 - xseq.n.||;
           consider rseq be Real_Sequence such that
A45:        for m be Nat holds rseq.m = F(m) from SEQ_1:sch 1;
A46:       rseq is convergent & lim rseq = ||.tseq.x-xseq.n.||
           proof
A47:         xseq - xseq.n is convergent by A37,CLVECT_1:117;
A48:         lim (xseq-xseq.n)= tseq.x - xseq.n by A37,CLVECT_1:123;
              rseq = ||.xseq - xseq.n.||
             proof
                now let x be set such that
A49:             x in NAT;
                 reconsider k=x as Nat by A49;
                 thus rseq.x = ||.xseq.k - xseq.n.|| by A45
                            .= ||.(xseq - xseq.n).k.|| by CLVECT_1:def 13
                            .= ||.(xseq - xseq.n).||.x by CLVECT_1:def 16;
               end;
               hence thesis by SEQ_1:8;
             end;
             hence thesis by A47,A48,CLOPBAN1:22;
           end;
            for m be Nat st m >= k holds rseq.m <= e
           proof
             let m be Nat such that
A50:         m >=k;
              rseq.m = ||.xseq.m-xseq.n.|| by A45
                   .= ||.xseq.n-xseq.m.|| by CLVECT_1:109;
             hence thesis by A42,A50;
           end;
           then lim rseq <= e by A46,Lm13;
           hence thesis by A46,CLVECT_1:109;
         end;
         hence ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e by A37;
       end;
       hence for x be Element of X holds
          ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e;
     end;
     take k;
     thus thesis by A35;
   end;
   reconsider tseq as bounded Function of X,the carrier of Y by A24;
   reconsider tv=tseq as Point of
    C_NormSpace_of_BoundedFunctions(X,Y) by Def5;

A51:for e be Real st
    e > 0 ex k be Nat st for n be Nat st
     n >= k holds ||.vseq.n - tv.|| <= e
   proof
     let e be Real such that
A52: e > 0;
     consider k be Nat such that
A53: for n be Nat st n >= k holds
       for x be Element of X holds
         ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e by A32,A52;
      now let n be Nat such that
A54:   n >= k;
       set f1=modetrans((vseq.n),X,Y);
       set g1=tseq;
       reconsider h1=vseq.n-tv
        as bounded Function of X,the carrier of Y by Def5;
A55:   now
         let t be Element of  X;
          vseq.n is bounded Function of X,the carrier of Y by Def5;
         then modetrans((vseq.n),X,Y)=vseq.n by Th17;
         then ||.h1.t.||= ||.f1.t-g1.t.|| by Th28;
         hence ||.h1.t.|| <=e by A53,A54;
       end;
A56:   now let r be Real such that
A57:     r in PreNorms(h1);
          r in {||.h1.t.|| where t is Element of X:not contradiction }
           by A57;
         then consider t be Element of X such that
A58:     r=||.h1.t.||;
         thus r <=e by A55,A58;
       end;
A59:   (for s be real number st s in PreNorms(h1) holds s <= e)
         implies sup PreNorms(h1) <=e by SEQ_4:62;
        ComplexBoundedFunctionsNorm(X,Y).(vseq.n-tv)
        = sup PreNorms(h1) by Th18;
       hence ||.vseq.n-tv.|| <=e by A56,A59,CLVECT_1:def 9;
     end;
     hence thesis;
   end;
    for e be Real st e > 0 ex m be Nat st for n be Nat st
     n >= m holds ||.(vseq.n) - tv.|| < e
   proof
     let e be Real such that
A60: e > 0;
A61: e/2 > 0 by A60,SEQ_2:3;
A62: e/2<e by A60,XREAL_1:218;
     consider m be Nat such that
A63: for n be Nat st n >= m holds ||.(vseq.n) - tv.|| <= e/2 by A51,A61;
      now let n be Nat such that
A64:   n >= m;
        ||.(vseq.n) - tv.|| <= e/2 by A63,A64;
       hence ||.(vseq.n) - tv.|| < e by A62,XREAL_1:2;
     end;
     hence thesis;
   end;
   hence thesis by CLVECT_1:def 15;
end;

theorem Th30:
for X be non empty set, Y be ComplexBanachSpace holds
 C_NormSpace_of_BoundedFunctions(X,Y) is ComplexBanachSpace
proof
   let X be non empty set;
   let Y be ComplexBanachSpace;
    for seq be sequence of C_NormSpace_of_BoundedFunctions(X,Y)
      st seq is Cauchy_sequence_by_Norm holds seq is convergent by Th29;
    hence thesis by CLOPBAN1:def 14;
end;

registration
  let X be non empty set;
  let Y be ComplexBanachSpace;
  cluster C_NormSpace_of_BoundedFunctions (X,Y) -> complete;
  coherence by Th30;
end;

begin :: Some properties of complex sequences

theorem
 for seq1,seq2 be Complex_Sequence st
 seq1 is bounded & seq2 is bounded holds
  seq1 + seq2 is bounded by Lm3;

theorem
 for c be Complex, seq be Complex_Sequence st seq is bounded holds
 c(#)seq is bounded by Lm4;

theorem
 for seq be Complex_Sequence holds seq is bounded iff
 |.seq.| is bounded by Lm9,Lm10;

theorem
 for seq1,seq2,seq3 be Complex_Sequence holds
 seq1 = seq2 - seq3 iff
  ( for n be Nat holds seq1.n = seq2.n - seq3.n ) by Lm12;
