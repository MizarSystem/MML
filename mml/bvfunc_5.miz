    end;
    end;
      Pj((b 'imp' c) 'imp' ((a 'imp' b) 'imp' (a 'imp' c)),x)
   ='not' Pj(b 'imp' c,x) 'or' Pj((a 'imp' b) 'imp' (a 'imp' c),x)
    by BVFUNC_1:def 11
  .='not' Pj(b 'imp' c,x) 'or' ('not' Pj(a 'imp' b,x) 'or' Pj(a 'imp' c,x))
    by BVFUNC_1:def 11
