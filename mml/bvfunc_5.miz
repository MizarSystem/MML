   .=('not' 'not' Pj(a 'imp' b,x) '&' 'not' Pj(c,x)) 'or'
      ('not' Pj(b,x) 'or' Pj(c,x)) by BINARITH:10
   .=(Pj(a 'imp' b,x) '&' 'not' Pj(c,x)) 'or'
      ('not' Pj(b,x) 'or' Pj(c,x)) by MARGREL1:40
   .=('not' Pj(c,x) '&' ('not' Pj(a,x) 'or' Pj(b,x))) 'or'
