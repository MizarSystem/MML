    then A7:Z c= dom f1 & Z c= dom (2(#)sin) by XBOOLE_1:18;
    for x st x in Z holds ln*f is_differentiable_in x
    proof
      let x;
      assume A8:x in Z;
      then A9:f is_differentiable_in x by FDIFF_1:16,A6;
      f.x >0 by A2,A8;
      hence ln*f is_differentiable_in x by TAYLOR_1:20,A9;
    end;
    then A10:ln*f is_differentiable_on Z by A4,FDIFF_1:16;
    for x st x in Z holds
    (((1/2)(#)(ln*f))`|Z).x =cos.x/(1+2*sin.x)
    proof
      let x;
      assume A11:x in Z;
      then A12:f is_differentiable_in x by FDIFF_1:16,A6;
A13:   f.x>0 by A2,A11;
A14:   f.x=f1.x+(2(#)sin).x by SEQ_1:def 3,A5,A11,A2
      .=1+(2(#)sin).x by A11,A2
      .=1+2*sin.x by SEQ_1:def 6,A7,A11;
      (((1/2)(#)(ln*f))`|Z).x
      =(1/2)*diff((ln*f),x) by A10,A1,FDIFF_1:28,A11
      .=(1/2)*(diff(f,x)/(f.x)) by TAYLOR_1:20,A12,A13
      .=(1/2)*((f`|Z).x/(f.x)) by FDIFF_1:def 8,A6,A11
      .=(1/2)*((2*cos.x)/(f.x)) by A5,A3,Lm6,A11
      .=(1/2)*(2*cos.x)/(f.x) by XCMPLX_1:75
      .=cos.x/(1+2*sin.x) by A14;
