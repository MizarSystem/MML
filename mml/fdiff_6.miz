:: 22 f.x=-ln(1-exp_R.x)
theorem
  Z c= dom -(ln*(f-exp_R)) & (for x st x in Z holds f.x=1
  & (f-exp_R).x>0) implies
      -(ln*(f-exp_R)) is_differentiable_on Z &
  for x st x in Z holds (((-ln*(f-exp_R)))`|Z).x =
      exp_R.x/(1-exp_R.x)
  proof
    assume that A1:Z c= dom -(ln*(f-exp_R)) and
A2: for x st x in Z holds f.x=1 & (f-exp_R).x>0;
A3: Z c= dom ((-1)(#)(ln*(f-exp_R))) by A1,RFUNCT_1:38;
A4: Z c= dom (ln*(f-exp_R)) by SEQ_1:def 7,A1;
    for y st y in Z holds y in dom (f-exp_R) by FUNCT_1:21,A4;
    then A5:Z c= dom (f-exp_R) by TARSKI:def 3;
    then Z c= dom exp_R /\ dom f by SEQ_1:def 4;
    then A6:Z c= dom f by XBOOLE_1:18;
A7: for x st x in Z holds f.x=0*x+1 by A2;
    then A8:f is_differentiable_on Z &
    for x st x in Z holds (f`|Z).x = 0 by FDIFF_1:31,A6;
A9: exp_R is_differentiable_on Z by TAYLOR_1:16,FDIFF_1:34;
    then A10:f-exp_R is_differentiable_on Z by A8,A5,FDIFF_1:27;
A11: for x st x in Z holds ((f-exp_R)`|Z).x =-exp_R.x
     proof
       let x;
       assume A12:x in Z;
       hence ((f-exp_R)`|Z).x =diff(f,x)-diff(exp_R,x) by A9,A8,A5,FDIFF_1:27
       .= diff(f,x)-exp_R.x by SIN_COS:70
       .= (f`|Z).x-exp_R.x by A12,FDIFF_1:def 8,A8
       .=0-exp_R.x by A12,FDIFF_1:31,A6,A7
       .=-exp_R.x;
     end;
     for x st x in Z holds ln*(f-exp_R) is_differentiable_in x
     proof
       let x;
       assume A13:x in Z;
       then A14:f-exp_R is_differentiable_in x by FDIFF_1:16,A10;
       (f-exp_R).x >0 by A2,A13;
       hence thesis by TAYLOR_1:20,A14;
     end;
     then A15:ln*(f-exp_R) is_differentiable_on Z by A4,FDIFF_1:16
;
     then A16:(-1)(#)(ln*(f-exp_R)) is_differentiable_on Z
     by FDIFF_1:28,A3;
     for x st x in Z holds (((-ln*(f-exp_R)))`|Z).x =
       exp_R.x/(1-exp_R.x)
     proof
       let x;
       assume A17:x in Z;
       then A18:(f-exp_R).x=f.x-exp_R.x by SEQ_1:def 4,A5
       .=1-exp_R.x by A2,A17;
A19:    (f-exp_R) is_differentiable_in x by FDIFF_1:16,A10,A17;
A20:   (f-exp_R).x >0 by A17,A2;
       (((-1)(#)(ln*(f-exp_R)))`|Z).x
       =(-1)*diff((ln*(f-exp_R)),x) by FDIFF_1:28,A15,A17,A3
       .=(-1)*(diff((f-exp_R),x)/((f-exp_R).x)) by TAYLOR_1:20,A19,A20
       .=(-1)*(((f-exp_R)`|Z).x/(f-exp_R).x) by A10,A17,FDIFF_1:def 8
       .=(-1)*((-exp_R.x)/(1-exp_R.x)) by A11,A17,A18
       .=((-1)*(-exp_R.x))/(1-exp_R.x) by XCMPLX_1:75
