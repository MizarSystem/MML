         f,f1,f2 for PartFunc of REAL,REAL;

Lm1: 1-cos(2*x)=2*(sin x)^2
proof
  1-cos(2*x)=1-cos(x+x)
    .=1-((cos x) *(cos x)-(sin x) * (sin x)) by SIN_COS:80
    .=(cos x)^2+(sin x)^2-((cos x)^2-(sin x)^2) by SIN_COS:32
    .=2*(sin x)^2;
  hence thesis;
end;

Lm2: 1+cos(2*x)=2*(cos x)^2
proof
  1+cos(2*x)=1+cos(x+x)
    .=1+((cos x)^2-(sin x)^2) by SIN_COS:80
    .=(cos x)^2+(sin x)^2+((cos x)^2-(sin x)^2) by SIN_COS:32
    .=2*(cos x)^2;
  hence thesis;
end;

:: 39 f.x=2*(1-cos.x) #R (1/2)
Lm3: sin.x>0 implies sin.x=((1-cos.x)*(1+cos.x)) #R (1/2)
proof
  assume A1:sin.x>0;
  then sin.x=(sin.x) #R (2*(1/2)) by PREPOWER:86
  .=(sin.x) #R (1+1) #R (1/2) by A1,PREPOWER:105
  .=((sin.x) #R 1 * (sin.x) #R 1) #R (1/2) by PREPOWER:89,A1
  .=(sin.x * (sin.x) #R 1) #R (1/2) by A1,PREPOWER:86
  .=((sin.x)^2+(cos.x)^2-(cos.x)^2) #R (1/2) by A1,PREPOWER:86
  .=(1^2-(cos.x)^2) #R (1/2) by SIN_COS:31
  .=((1-cos.x)*(1+cos.x)) #R (1/2);
  hence thesis;
end;

Lm4: sin.x>0 & cos.x<1 & cos.x>-1 implies
   sin.x/(1-cos.x) #R (1/2) =(1+cos.x) #R (1/2)
proof
   assume that A1:sin.x>0 and A2:cos.x<1 and A3:cos.x>-1;
A4:1-cos.x>1-1 by A2,XREAL_1:17;
   1+cos.x>1+-1 by A3,XREAL_1:10;
   then A5:(1-cos.x)*(1+cos.x)>0 by A4,XREAL_1:131;
   sin.x/(1-cos.x) #R (1/2)
    =((1-cos.x)*(1+cos.x)) #R (1/2)/(1-cos.x) #R (1/2) by Lm3,A1
   .=(((1-cos.x)*(1+cos.x))/(1-cos.x)) #R (1/2) by PREPOWER:94,A4,A5
   .=(1+cos.x) #R (1/2) by A4,XCMPLX_1:90;
   hence thesis;
end;

:: a #R x=exp.(x*ln a)
theorem Th1:
  a>0 implies exp_R.(x*log(number_e,a))=a #R x
  proof
    assume A1:a>0;
    then A2:a #R x >0 by PREPOWER:95;
    number_e>0 & number_e<>1 by TAYLOR_1:11;
    then exp_R.(x*log(number_e,a))
    =exp_R.(log(number_e,a to_power x)) by POWER:63,A1
    .=exp_R.(log(number_e,a #R x)) by POWER:def 2,A1
    .=a #R x by TAYLOR_1:15,A2;
    hence thesis;
  end;

:: 15 a #R (-x)=exp.(-x*ln a)
theorem Th2:
  a>0 implies exp_R.(-x*log(number_e,a))=a #R (-x)
  proof
    assume A1:a>0;
    then A2:a #R (-x) >0 by PREPOWER:95;
A3: number_e>0 & number_e<>1 by TAYLOR_1:11;
    exp_R.(-x*log(number_e,a))
    =exp_R.((-x)*log(number_e,a))
    .=exp_R.(log(number_e,a to_power (-x))) by POWER:63,A1,A3
    .=exp_R.(log(number_e,a #R (-x))) by POWER:def 2,A1
    .=a #R (-x) by TAYLOR_1:15,A2;
    hence thesis;
  end;

:: 40 f.x=-2*(1+cos.x) #R (1/2)
Lm5: sin.x>0 & cos.x<1 & cos.x>-1
     implies sin.x/(1+cos.x) #R (1/2) =(1-cos.x) #R (1/2)
     proof
       assume that A1:sin.x>0 and A2:cos.x<1 and A3:cos.x>-1;
A4:    1-cos.x>1-1 by A2,XREAL_1:17;
A5:    1+cos.x>1+-1 by A3,XREAL_1:10;
       then A6:(1-cos.x)*(1+cos.x)>0 by A4,XREAL_1:131;
       sin.x/(1+cos.x) #R (1/2)
       =((1-cos.x)*(1+cos.x)) #R (1/2)/(1+cos.x) #R (1/2) by Lm3,A1
       .=(((1-cos.x)*(1+cos.x))/(1+cos.x)) #R (1/2) by PREPOWER:94,A5,A6
       .=(1-cos.x) #R (1/2) by A5,XCMPLX_1:90;
       hence thesis;
     end;

:: 1 f.x=a^2-x^2
theorem Th3:
  Z c= dom (f1-f2) & (for x st x in Z holds f1.x=a^2) & f2=#Z 2 implies
  f1-f2 is_differentiable_on Z &
  for x st x in Z holds ((f1-f2)`|Z).x = -2*x
  proof
    assume that A1:Z c= dom (f1-f2) and
A2: (for x st x in Z holds f1.x=a^2) and
A3: f2=#Z 2;
A4: f1+(-1)(#)f2=f1+-f2 by RFUNCT_1:38
    .=f1-f2 by RFUNCT_1:40;
A5: for x st x in Z holds f1.x=a^2+0*x by A2;
    for x st x in Z holds ((f1-f2)`|Z).x = -2*x
    proof
      let x;
      assume x in Z;
      hence ((f1-f2)`|Z).x =0+2*(-1)*x by A4,A1,A3,FDIFF_4:12,A5
      .=-2*x;
    end;
    hence thesis by A4,A1,A3,FDIFF_4:12,A5;
  end;

:: 2 f.x=(a^2+x^2)/(a^2-x^2)
theorem Th4:
  Z c= dom ((f1+f2)/(f1-f2)) & f2=#Z 2 &
  (for x st x in Z holds f1.x=a^2 & (f1-f2).x<>0) implies
  (f1+f2)/(f1-f2) is_differentiable_on Z &
  for x st x in Z holds (((f1+f2)/(f1-f2))`|Z).x = 4*a^2*x/(a^2-x|^2)^2
  proof
    assume that A1:Z c= dom ((f1+f2)/(f1-f2)) and A2:f2=#Z 2 and
A3: (for x st x in Z holds f1.x=a^2 & (f1-f2).x<>0);
    Z c=dom (f1+f2) /\ (dom (f1-f2) \ (f1-f2)"{0}) by A1,RFUNCT_1:def 4;
    then A4:Z c= dom (f1+f2) & Z c= dom (f1-f2) \ (f1-f2)"{0} by XBOOLE_1:18;
    A5:Z c= dom (f1-f2) by XBOOLE_1:1,A4;
A6: for x st x in Z holds f1.x=a^2 by A3;
    then A7:f1+f2 is_differentiable_on Z &
    for x st x in Z holds ((f1+f2)`|Z).x = 2*x by A2,A4,FDIFF_4:17;
A8: f1-f2 is_differentiable_on Z &
    for x st x in Z holds ((f1-f2)`|Z).x = -2*x by Th3,A2,A5,A6;
A9: for x st x in Z holds (f1-f2).x <> 0 by A3;
     then A10:(f1+f2)/(f1-f2) is_differentiable_on Z by A7,A8,FDIFF_2:21;
     for x st x in Z holds (((f1+f2)/(f1-f2))`|Z).x=4*a^2*x/(a^2-x|^2)^2
     proof
       let x;
       assume A11:x in Z;
       then A12:f1+f2 is_differentiable_in x by A7,FDIFF_1:16;
A13:   f1-f2 is_differentiable_in x by A8,FDIFF_1:16,A11;
A14:   f2.x=x #Z 2 by A2,TAYLOR_1:def 1
       .=x |^2 by PREPOWER:46;
A15:    (f1+f2).x=f1.x+f2.x by SEQ_1:def 3,A4,A11
       .=a^2+x |^2 by A11,A3,A14;
A16:    (f1-f2).x=f1.x-f2.x by A5,A11,SEQ_1:def 4
       .=a^2-x |^2 by A3,A11,A14;
       (f1-f2).x<>0 by A3,A11;
       then diff((f1+f2)/(f1-f2),x)
       =(diff(f1+f2,x)*(f1-f2).x - diff(f1-f2,x)*(f1+f2).x)/((f1-f2).x)^2
       by FDIFF_2:14,A12,A13
       .=(((f1+f2)`|Z).x * (f1-f2).x-diff(f1-f2,x) * (f1+f2).x)/((f1-f2).x)^2
       by A7,A11,FDIFF_1:def 8
       .= (((f1+f2)`|Z).x * (f1-f2).x-((f1-f2)`|Z).x * (f1+f2).x)/((f1-f2).x)^2
       by A8,A11,FDIFF_1:def 8
       .=((2*x)*(f1-f2).x-((f1-f2)`|Z).x * (f1+f2).x)/((f1-f2).x)^2
       by A11,A2,A4,FDIFF_4:17,A6
       .=((2*x)*(f1-f2).x-(-2*x) * (f1+f2).x)/((f1-f2).x)^2
       by A11,Th3,A2,A5,A6
       .=4*a^2*x/(a^2-x|^2)^2 by A15,A16;
       hence thesis by A11,A10,FDIFF_1:def 8;
     end;
     hence thesis by A9,A7,A8,FDIFF_2:21;
   end;

:: 3 f.x=ln((a^2+x^2)/(a^2-x^2))
theorem Th5:
  Z c= dom f & f=ln*((f1+f2)/(f1-f2)) & f2=#Z 2 &
  (for x st x in Z holds f1.x=a^2 & (f1-f2).x>0 & a<>0)
  implies f is_differentiable_on Z & for x
  st x in Z holds (f`|Z).x =4*a^2*x/(a|^4-x|^4)
  proof
    assume that A1:Z c= dom f and A2:f=ln*((f1+f2)/(f1-f2)) and
A3: f2=#Z 2 and A4:for x st x in Z holds f1.x=a^2 & (f1-f2).x>0 & a<>0;
    for y st y in Z holds y in dom ((f1+f2)/(f1-f2)) by FUNCT_1:21,A1,A2;
    then A5:Z c= dom ((f1+f2)/(f1-f2)) by TARSKI:def 3;
    then Z c=dom (f1+f2) /\ (dom (f1-f2) \ (f1-f2)"{0}) by RFUNCT_1:def 4;
    then A6:Z c= dom (f1+f2) & Z c= dom (f1-f2) \ (f1-f2)"{0} by XBOOLE_1:18;
    A7:Z c= dom (f1-f2) by XBOOLE_1:1,A6;
A8: for x st x in Z holds f1.x=a^2 & (f1-f2).x<>0 by A4;
     then A9:(f1+f2)/(f1-f2) is_differentiable_on Z & for x st x in Z holds
     (((f1+f2)/(f1-f2))`|Z).x = 4*a^2*x/(a^2-x|^2)^2 by Th4,A5,A3;
A10:  for x st x in Z holds ((f1+f2)/(f1-f2)).x >0
     proof
       let x;
       assume A11:x in Z;
       then A12:(f1-f2).x>0 by A4;
A13:   f2.x=x #Z 2 by A3,TAYLOR_1:def 1
       .=x |^2 by PREPOWER:46;
A14:    (f1+f2).x=f1.x+f2.x by A6,A11,SEQ_1:def 3
       .=a^2+x |^2 by A11,A4,A13;
       a<>0 by A11,A4;
       then A15:a^2>0 by SQUARE_1:74;
A16:    x |^2 = x^2 by NEWTON:100;
       x |^2>=0 by XREAL_1:65,A16;
       then A17:a^2+x |^2>0+0 by A15,XREAL_1:10;
       ((f1+f2)/(f1-f2)).x =(f1+f2).x * ((f1-f2).x)" by A5,A11,RFUNCT_1:def 4
       .=(f1+f2).x / (f1-f2).x by XCMPLX_0:def 9;
       hence thesis by A12,A17,A14,XREAL_1:141;
     end;
A18: for x st x in Z holds ln*((f1+f2)/(f1-f2))
     is_differentiable_in x
     proof
       let x;
       assume A19:x in Z;
       then A20:(f1+f2)/(f1-f2) is_differentiable_in x by FDIFF_1:16,A9;
       ((f1+f2)/(f1-f2)).x >0 by A10,A19;
       hence ln*((f1+f2)/(f1-f2)) is_differentiable_in x
       by TAYLOR_1:20,A20;
     end;
     then A21:f is_differentiable_on Z by A1,A2,FDIFF_1:16;
     for x st x in Z holds (f`|Z).x = 4*a^2*x/(a|^4-x|^4)
     proof
       let x;
       assume A22:x in Z;
       then A23:(f1+f2)/(f1-f2) is_differentiable_in x by FDIFF_1:16,A9;
A24:   f2.x=x #Z 2 by A3,TAYLOR_1:def 1
       .=x |^2 by PREPOWER:46;
A25:    (f1+f2).x=f1.x+f2.x by A6,A22,SEQ_1:def 3
       .=a^2+x |^2 by A22,A4,A24;
A26:    (f1-f2).x=f1.x-f2.x by SEQ_1:def 4,A7,A22
       .=a^2-x |^2 by A4,A22,A24;
A27:   ((f1+f2)/(f1-f2)).x =(f1+f2).x * ((f1-f2).x)" by RFUNCT_1:def 4,A5,A22
       .=(a^2+x |^2)/(a^2-x |^2) by A25,A26,XCMPLX_0:def 9;
A28:    (a^2)^2=(a|^2)*(a^2) by NEWTON:100
       .=(a|^2)*(a|^2) by NEWTON:100
       .=(a|^2)|^2 by WSIERP_1:2
       .=a|^(2*2) by NEWTON:14
       .=a|^4;
A29:    (x |^2)^2=(x |^2)|^2 by WSIERP_1:2
       .=x|^(2*2) by NEWTON:14
       .=x|^4;
A30:   a^2-x |^2>0 by A22,A4,A26;
       ((f1+f2)/(f1-f2)).x >0 by A10,A22;
       then diff(ln*((f1+f2)/(f1-f2)),x)
       =diff(((f1+f2)/(f1-f2)),x)/(((f1+f2)/(f1-f2)).x) by TAYLOR_1:20,A23
       .=(((f1+f2)/(f1-f2))`|Z).x/(((f1+f2)/(f1-f2)).x) by A9,A22,FDIFF_1:def 8
       .= (4*a^2*x/(a^2-x|^2)^2)/((a^2+x |^2)/(a^2-x |^2))
         by A27,A8,Th4,A5,A3,A22
       .=(4*a^2*x/(a^2-x|^2)/(a^2-x|^2))/((a^2+x |^2)/(a^2-x |^2))
       by XCMPLX_1:79
       .=(4*a^2*x/(a^2-x|^2))/((a^2+x |^2)/(a^2-x |^2))/(a^2-x|^2)
       by XCMPLX_1:48
       .=(4*a^2*x)/(a^2+x |^2)/(a^2-x|^2) by A30,XCMPLX_1:55
       .=(4*a^2*x)/((a^2+x |^2)*(a^2-x|^2)) by XCMPLX_1:79
       .=4*a^2*x/(a|^4-x|^4) by A28,A29;
       hence thesis by A22,A21,A2,FDIFF_1:def 8;
     end;
     hence thesis by A18,A1,A2,FDIFF_1:16;
   end;

:: 4 f.x=(1/(4*a^2))*ln((a^2+x^2)/(a^2-x^2))
theorem
  Z c= dom ((1/(4*a^2))(#)f) & f=ln*((f1+f2)/(f1-f2)) & f2=#Z 2 &
  (for x st x in Z holds f1.x=a^2 & (f1-f2).x>0 & a<>0)
  implies (1/(4*a^2))(#)f is_differentiable_on Z & for x
  st x in Z holds (((1/(4*a^2))(#)f)`|Z).x =x/(a|^4-x|^4)
  proof
    assume that A1:Z c= dom ((1/(4*a^2))(#)f) and
A2: f=ln*((f1+f2)/(f1-f2)) and
A3: f2=#Z 2 and
A4: (for x st x in Z holds f1.x=a^2 & (f1-f2).x>0 & a<>0);
A5: Z c= dom f by SEQ_1:def 6,A1;
    then A6:f is_differentiable_on Z & for x
    st x in Z holds (f`|Z).x =4*a^2*x/(a|^4-x|^4) by Th5,A2,A3,A4;
    for x st x in Z holds (((1/(4*a^2))(#)f)`|Z).x =x/(a|^4-x|^4)
    proof
      let x;
      assume A7:x in Z;
      then a<>0 by A4;
      then a^2>0 by SQUARE_1:74;
      then A8:4*a^2>4*0 by XREAL_1:70;
      (((1/(4*a^2))(#)f)`|Z).x
      =(1/(4*a^2))*diff(f,x) by A6,A1,FDIFF_1:28,A7
      .=(1/(4*a^2))*(f`|Z).x by A7,A6,FDIFF_1:def 8
      .=(1/(4*a^2))*(4*a^2*x/(a|^4-x|^4)) by Th5,A2,A3,A4,A5,A7
      .=(1/(4*a^2))*((4*a^2)*(x/(a|^4-x|^4))) by XCMPLX_1:75
      .=(x/(a|^4-x|^4))*(1/(4*a^2)*(4*a^2))
      .=x/(a|^4-x|^4) by XCMPLX_1:109,A8;
      hence thesis;
    end;
    hence thesis by A1,FDIFF_1:28,A6;
  end;

:: 5 f.x=x^2/(1+x^2)
theorem Th7:
  Z c= dom (f1/(f2+f1)) & f1=#Z 2 & (for x st x in Z holds f2.x=1 & x<>0)
  implies f1/(f2+f1) is_differentiable_on Z & for x
  st x in Z holds ((f1/(f2+f1))`|Z).x =2*x/(1+x^2)^2
  proof
    assume that A1:Z c= dom (f1/(f2+f1)) and A2:f1=#Z 2 and
A3: for x st x in Z holds f2.x=1 & x<>0;
A4: for x st x in Z holds f2.x=1^2 by A3;
    Z c= dom f1 /\ (dom (f2+f1) \ (f2+f1)"{0}) by RFUNCT_1:def 4,A1;
    then A5:Z c= dom f1 & Z c= dom (f2+f1) \ (f2+f1)"{0} by XBOOLE_1:18;
    A6:Z c= dom (f2+f1) by XBOOLE_1:1,A5;
    then A7:f2+f1 is_differentiable_on Z &
    for x st x in Z holds ((f2+f1)`|Z).x = 2*x by A2,A4,FDIFF_4:17;
    for x st x in Z holds f1 is_differentiable_in x by A2,TAYLOR_1:2;
    then A8:f1 is_differentiable_on Z by A5,FDIFF_1:16;
A9: for x st x in Z holds (f1`|Z).x=2*x
     proof
       let x;
       assume A10:x in Z;
       2 * (x #Z (2-1))=2 * x by PREPOWER:45;
       then diff(f1,x) =2 * x by TAYLOR_1:2,A2;
       hence thesis by A8,A10,FDIFF_1:def 8;
     end;
A11: for x st x in Z holds (f2+f1).x <> 0
     proof
       let x;
       assume A12:x in Z;
       then A13:(f2+f1).x=f2.x+f1.x by A6,SEQ_1:def 3
       .=1+f1.x by A12,A3
       .=1+x #Z 2 by A2,TAYLOR_1:def 1
       .=1+x |^2 by PREPOWER:46
       .=1+x^2 by NEWTON:100;
       x^2 >=0 by XREAL_1:65;
       then 1+x^2>0+0 by XREAL_1:10;
       hence thesis by A13;
     end;
A14: f1/(f2+f1) is_differentiable_on Z by A7,A8,A11,FDIFF_2:21;
     for x st x in Z holds ((f1/(f2+f1))`|Z).x =2*x/(1+x^2)^2
     proof
       let x;
       assume A15:x in Z;
A16:   f1 is_differentiable_in x by A2,TAYLOR_1:2;
A17:   f2+f1 is_differentiable_in x by A7,FDIFF_1:16,A15;
A18:   f1.x=x #Z 2 by A2,TAYLOR_1:def 1
       .=x |^2 by PREPOWER:46
       .=x^2 by NEWTON:100;
A19:    (f2+f1).x=f2.x+f1.x by A6,A15,SEQ_1:def 3
       .=1+f1.x by A15,A3
       .=1+x #Z 2 by A2,TAYLOR_1:def 1
       .=1+x |^2 by PREPOWER:46
       .=1+x^2 by NEWTON:100;
       (f2+f1).x<>0 by A11,A15;
       then diff(f1/(f2+f1),x)
       =(diff(f1,x)*(f2+f1).x - diff(f2+f1,x)*f1.x)/((f2+f1).x)^2
       by FDIFF_2:14,A16,A17
       .=((f1`|Z).x * (f2+f1).x-diff(f2+f1,x) * f1.x)/((f2+f1).x)^2
       by A8,A15,FDIFF_1:def 8
       .= ((f1`|Z).x * (f2+f1).x-((f2+f1)`|Z).x * f1.x)/((f2+f1).x)^2
       by A7,A15,FDIFF_1:def 8
       .=((2*x)*(f2+f1).x-((f2+f1)`|Z).x * f1.x)/((f2+f1).x)^2
       by A15,A9
       .=((2*x)*(1+x^2)-(2*x)*x^2)/(1+x^2)^2 by A15,A6,A2,A4,FDIFF_4:17,A19,A18
       .=2*x/(1+x^2)^2;
       hence thesis by A15,A14,FDIFF_1:def 8;
     end;
     hence thesis by A7,A8,A11,FDIFF_2:21;
   end;

:: 6 f.x=(1/2)*ln(x^2/(1+x^2))
theorem
  Z c= dom ((1/2)(#)f) & f=ln*(f1/(f2+f1)) & f1=#Z 2 &
  (for x st x in Z holds f2.x=1 & x<>0)
  implies (1/2)(#)f is_differentiable_on Z & for x st x in Z holds
  (((1/2)(#)f)`|Z).x =1/(x*(1+x^2))
  proof
    assume that A1:Z c= dom ((1/2)(#)f) and
A2: f=ln*(f1/(f2+f1)) and
A3: f1=#Z 2 and
A4: (for x st x in Z holds f2.x=1 & x<>0);
A5: Z c= dom f by SEQ_1:def 6,A1;
    for y st y in Z holds y in dom (f1/(f2+f1)) by FUNCT_1:21,A2,A5;
    then A6:Z c= dom (f1/(f2+f1)) by TARSKI:def 3;
    then A7:f1/(f2+f1) is_differentiable_on Z & for x
    st x in Z holds ((f1/(f2+f1))`|Z).x =2*x/(1+x^2)^2 by A3,A4,Th7;
    Z c= dom f1 /\ (dom (f2+f1) \ (f2+f1)"{0}) by RFUNCT_1:def 4,A6;
    then A8:Z c= dom f1 & Z c= dom (f2+f1) \ (f2+f1)"{0} by XBOOLE_1:18;
    A9:Z c= dom (f2+f1) by XBOOLE_1:1,A8;
A10: for x st x in Z holds (f1/(f1+f2)).x >0
     proof
       let x;
       assume A11:x in Z;
A12:    f1.x=x #Z 2 by A3,TAYLOR_1:def 1
       .=x |^2 by PREPOWER:46
       .=x^2 by NEWTON:100;
A13:    (f2+f1).x=f2.x+f1.x by A9,A11,SEQ_1:def 3
       .=1+x^2 by A12,A11,A4;
       x<>0 by A11,A4;
       then A14:f1.x>0 by A12,SQUARE_1:74;
       then A15:1+x^2>0+0 by A12,XREAL_1:10;
       (f1/(f2+f1)).x=f1.x * ((f2+f1).x)" by RFUNCT_1:def 4,A6,A11
       .=f1.x/(f2+f1).x by XCMPLX_0:def 9;
       hence thesis by A14,A15,A13,XREAL_1:141;
     end;
     for x st x in Z holds ln*(f1/(f2+f1))
     is_differentiable_in x
     proof
       let x;
       assume A16:x in Z;
       then A17:f1/(f2+f1) is_differentiable_in x by FDIFF_1:16,A7;
       (f1/(f1+f2)).x >0 by A10,A16;
       hence ln*(f1/(f2+f1)) is_differentiable_in x
       by TAYLOR_1:20,A17;
     end;
     then A18:f is_differentiable_on Z by A5,A2,FDIFF_1:16;
     for x st x in Z holds (((1/2)(#)f)`|Z).x =1/(x*(1+x^2))
     proof
       let x;
       assume A19:x in Z;
A20:    f1.x=x #Z 2 by A3,TAYLOR_1:def 1
       .=x |^2 by PREPOWER:46
       .=x^2 by NEWTON:100;
A21:    (f2+f1).x=f2.x+f1.x by A9,A19,SEQ_1:def 3
       .=1+x^2 by A20,A19,A4;
A22:    (f1/(f2+f1)).x=f1.x * ((f2+f1).x)" by RFUNCT_1:def 4,A6,A19
       .=x^2/(1+x^2) by A20,A21,XCMPLX_0:def 9;
A23:   f1/(f2+f1) is_differentiable_in x by FDIFF_1:16,A7,A19;
A24:   (f1/(f1+f2)).x >0 by A10,A19;
A25:   x<>0 by A19,A4;
       then x^2>0 by SQUARE_1:74;
       then A26:1+x^2>0+0 by XREAL_1:10;
       (((1/2)(#)f)`|Z).x
       =(1/2)*diff(ln*(f1/(f2+f1)),x) by A2,A18,A1,FDIFF_1:28,A19
       .=(1/2)*(diff((f1/(f2+f1)),x)/(f1/(f2+f1)).x) by TAYLOR_1:20,A23,A24
       .=(1/2)*(((f1/(f2+f1))`|Z).x/(f1/(f2+f1)).x) by A19,FDIFF_1:def 8,A7
       .=(1/2)*((2*x/(1+x^2)^2)/(x^2/(1+x^2))) by A22,A6,A3,A4,Th7,A19
       .=(1/2)*((2*x)/(1+x^2)^2)/(x^2/(1+x^2)) by XCMPLX_1:75
       .=((1/2)*(2*x))/(1+x^2)^2/(x^2/(1+x^2)) by XCMPLX_1:75
       .=x/(1+x^2)/(1+x^2)/(x^2/(1+x^2)) by XCMPLX_1:79
       .=x/(1+x^2)/(x^2/(1+x^2))/(1+x^2) by XCMPLX_1:48
       .=x/x^2/(1+x^2) by A26,XCMPLX_1:55
       .=x/x/x/(1+x^2) by XCMPLX_1:79
       .=1/x/(1+x^2) by XCMPLX_1:60,A25
       .=1/(x*(1+x^2)) by XCMPLX_1:79;
       hence thesis;
     end;
     hence thesis by A18,A1,FDIFF_1:28;
   end;

:: 7 f.x=ln(x|^n)
theorem
  Z c= dom (ln*( #Z n)) & (for x st x in Z holds x>0)
  implies ln*( #Z n) is_differentiable_on Z & for x
  st x in Z holds ((ln*( #Z n))`|Z).x =n/x
  proof
    assume that A1:Z c= dom (ln*( #Z n)) and
A2: for x st x in Z holds x > 0;
A3: for x st x in Z holds x #Z n > 0
    proof
      let x;
      assume x in Z;
      then x>0 by A2;
      hence x #Z n > 0 by PREPOWER:49;
    end;
A4: for x st x in Z holds ln*( #Z n) is_differentiable_in x
    proof
      let x;
      assume A5:x in Z;
A6:   #Z n is_differentiable_in x by TAYLOR_1:2;
      #Z n.x=x #Z n by TAYLOR_1:def 1;
      then #Z n.x>0 by A3,A5;
      hence ln*( #Z n) is_differentiable_in x
      by TAYLOR_1:20,A6;
    end;
    then A7:ln*( #Z n) is_differentiable_on Z
    by A1,FDIFF_1:16;
    for x st x in Z holds ((ln*( #Z n))`|Z).x =n/x
    proof
      let x;
      assume A8:x in Z;
      then A9:x>0 by A2;
      then A10:x |^n >0 by NEWTON:102;
A11:   #Z n is_differentiable_in x &
      diff(( #Z n),x) = n * x #Z (n-1) by TAYLOR_1:2;
A12:   #Z n.x=x #Z n by TAYLOR_1:def 1;
      then #Z n.x>0 by A3,A8;
      then diff(ln*( #Z n),x)
      = n * x #Z (n-1)/x #Z n by A12,TAYLOR_1:20,A11
      .=n * x #Z (n-1)/x |^ n by PREPOWER:46
      .=n * (x |^ n/x |^ 1)/x |^ n by PREPOWER:53,A9
      .=n * (x |^ n/x |^ 1/x |^ n) by XCMPLX_1:75
      .=n * (x |^ n/x |^ n/x |^ 1) by XCMPLX_1:48
      .=n * (1/(x |^ 1)) by A10,XCMPLX_1:60
      .=n * (1/x) by NEWTON:10
      .=n*1/x by XCMPLX_1:75
      .=n/x;
      hence thesis by A7,A8,FDIFF_1:def 8;
    end;
    hence thesis by A4,A1,FDIFF_1:16;
  end;

:: 8 f.x=1/x+ln((x-1)/x)
theorem
  Z c= dom (f2^+ln*(f1/f2)) & (for x st x in Z holds f2.x = x
  & f2.x>0 & f1.x=x-1 & f1.x >0 ) implies f2^+ln*(f1/f2)
  is_differentiable_on Z & for x st x in Z holds
  ( (f2^+ln*(f1/f2))`|Z).x= 1/(x^2*(x-1))
  proof
    assume that A1:Z c= dom (f2^+ln*(f1/f2)) and
A2: for x st x in Z holds f2.x = x & f2.x>0 & f1.x=x-1 & f1.x >0;
A3: Z c= dom (f2^) /\ dom (ln*(f1/f2)) by SEQ_1:def 3,A1;
    then A4:Z c= dom (f2^) by XBOOLE_1:18;
    dom (f2^) c= dom f2 by RFUNCT_1:11;
    then A5:Z c= dom f2 by A4,XBOOLE_1:1;
A6: for x st x in Z holds f2.x = 0+x & f2.x<>0 by A2;
    then A7:f2^ is_differentiable_on Z & for x st x in Z holds
    ((f2^)`|Z).x= -1/(0+x)^2 by A5,FDIFF_4:14;
A8: Z c=dom (ln*(f1/f2)) by XBOOLE_1:18,A3;
A9: for x st x in Z holds f1.x=x-1 & f1.x >0 & f2.x = x-0 & f2.x>0 by A2;
     then A10:ln*(f1/f2) is_differentiable_on Z & for x st x in Z
     holds ((ln*(f1/f2))`|Z).x=(1-0)/((x-1)*(x-0))
       by A8,FDIFF_4:24;
     for x st x in Z holds
     ((f2^+ln*(f1/f2))`|Z).x= 1/(x^2*(x-1))
     proof
       let x;
       assume A11:x in Z;
       then A12:f2.x = x & f2.x>0 & f1.x=x-1 & f1.x >0 by A2;
       ((f2^+ln*(f1/f2))`|Z).x
       =diff(f2^,x) + diff(ln*(f1/f2),x)
         by FDIFF_1:26,A7,A10,A1,A11
       .=((f2^)`|Z).x+ diff(ln*(f1/f2),x) by A7,A11,FDIFF_1:def 8
       .=((f2^)`|Z).x+((ln*(f1/f2))`|Z).x by A10,A11,FDIFF_1:def 8
       .= -1/(0+x)^2+((ln*(f1/f2))`|Z).x by A11,A6,A5,FDIFF_4:14
       .=-1/(0+x)^2+(1-0)/((x-1)*(x-0)) by A11,A9,A8,FDIFF_4:24
       .=-(1*(x-1))/(x^2*(x-1))+1/((x-1)*x) by A12,XCMPLX_1:92
       .=-(1*(x-1))/(x^2*(x-1))+(1*x)/((x-1)*x*x) by A12,XCMPLX_1:92
       .=(-(x-1))/(x^2*(x-1))+x/(x^2*(x-1)) by XCMPLX_1:188
       .=((-x+1)+x)/(x^2*(x-1)) by XCMPLX_1:63
       .=1/(x^2*(x-1));
       hence thesis;
     end;
     hence thesis by FDIFF_1:26,A7,A10,A1;
   end;

:: 10 f.x=a #R x=exp.(x*ln a)
theorem Th11:
  Z c= dom (exp_R*f) & (for x st x in Z holds f.x=x*log(number_e,a)) &
  a>0 implies
  exp_R*f is_differentiable_on Z &
  for x st x in Z holds ((exp_R*f)`|Z).x =a #R x*log(number_e,a)
  proof
    assume that A1:Z c= dom (exp_R*f) and
A2: for x st x in Z holds f.x=x*log(number_e,a) and
A3: a>0;
    for y st y in Z holds y in dom f by FUNCT_1:21,A1;
    then A4:Z c= dom f by TARSKI:def 3;
A5: for x st x in Z holds f.x = log(number_e,a)*x+0 by A2;
    then A6:f is_differentiable_on Z &
    for x st x in Z holds (f`|Z).x =log(number_e,a) by FDIFF_1:31,A4;
A7: for x st x in Z holds exp_R*f is_differentiable_in x
    proof
      let x;
      assume x in Z;
      then f is_differentiable_in x by FDIFF_1:16,A6;
      hence exp_R*f is_differentiable_in x by TAYLOR_1:19;
    end;
    then A8:exp_R*f is_differentiable_on Z by A1,FDIFF_1:16;
    for x st x in Z holds ((exp_R*f)`|Z).x = a #R x*log(number_e,a)
    proof
      let x;
      assume A9:x in Z;
      then f is_differentiable_in x by FDIFF_1:16,A6;
      then diff(exp_R*f,x) = exp_R.(f.x)*diff(f,x) by TAYLOR_1:19
      .=exp_R.(f.x)*(f`|Z).x by FDIFF_1:def 8,A9,A6
      .=exp_R.(f.x)*(log(number_e,a)) by A5,FDIFF_1:31,A4,A9
      .=exp_R.(x*log(number_e,a))*(log(number_e,a)) by A9,A2
      .=a #R x*log(number_e,a) by A3,Th1;
      hence thesis by A9,A8,FDIFF_1:def 8;
    end;
    hence thesis by A7,A1,FDIFF_1:16;
  end;

:: 11 f.x=(a #R x)*(x-1/ln a)/ln a
theorem
  Z c= dom ((1/log(number_e,a))(#)((exp_R*f1)(#)f2)) &
  (for x st x in Z holds f1.x=x*log(number_e,a) & f2.x=x-1/log(number_e,a)) &
  a>0 & a<>1 implies
  (1/log(number_e,a))(#)((exp_R*f1)(#)f2) is_differentiable_on Z &
  for x st x in Z holds
  (((1/log(number_e,a))(#)((exp_R*f1)(#)f2))`|Z).x =x * a #R x
  proof
    assume that A1:Z c= dom ((1/log(number_e,a))(#)((exp_R*f1)(#)f2)) and
A2: (for x st x in Z holds f1.x=x*log(number_e,a) & f2.x=x-1/log(number_e,a))
    & a>0 & a<>1;
A3: (for x st x in Z holds f1.x=x*log(number_e,a)) & a>0 by A2;
A4: Z c= dom ((exp_R*f1)(#)f2) by SEQ_1:def 6,A1;
    then Z c= dom (exp_R*f1) /\ dom f2 by SEQ_1:def 5;
    then A5:Z c= dom (exp_R*f1) & Z c= dom f2 by XBOOLE_1:18;
    then A6:exp_R*f1 is_differentiable_on Z & for x st x in Z holds
    ((exp_R*f1)`|Z).x =a #R x*log(number_e,a) by A3,Th11;
A7: for x st x in Z holds f2.x = 1*x+-1/log(number_e,a)
     proof let x;
       assume A8:x in Z;
       1*x+-1/log(number_e,a)=1*x-1/log(number_e,a);
       hence thesis by A2,A8;
     end;
     then A9:f2 is_differentiable_on Z & for x st x in Z holds
     (f2`|Z).x=1 by FDIFF_1:31,A5;
     then A10:((exp_R*f1)(#)f2) is_differentiable_on Z by A4,A6,FDIFF_1:29;
A11:  log(number_e,a)<>0
     proof
       assume log(number_e,a)=0;
       then A12:log(number_e,a)=log(number_e,1) by SIN_COS2:13,TAYLOR_1:13;
A13:    number_e>0 & number_e<>1 by TAYLOR_1:11;
       then a=(number_e) to_power log(number_e,1) by A12,POWER:def 3,A2
       .=1 by POWER:def 3,A13;
       hence contradiction by A2;
     end;
     for x st x in Z holds
     (((1/log(number_e,a))(#)((exp_R*f1)(#)f2))`|Z).x =x * a #R x
     proof
       let x;
       assume A14:x in Z;
       then A15:(exp_R*f1).x=exp_R.(f1.x) by A5,FUNCT_1:22
       .=exp_R.(x*log(number_e,a)) by A14,A2
       .=a #R x by A2,Th1;
       (((1/log(number_e,a))(#)((exp_R*f1)(#)f2))`|Z).x
       =(1/log(number_e,a))*diff((exp_R*f1)(#)f2,x) by A1,A10,FDIFF_1:28,A14
       .=(1/log(number_e,a))*(((exp_R*f1)(#)f2)`|Z).x by FDIFF_1:def 8,A14,A10
       .=(1/log(number_e,a))*(f2.x*diff(exp_R*f1,x)+(exp_R*f1).x*diff(f2,x))
       by A14,A4,A6,A9,FDIFF_1:29
       .=(1/log(number_e,a))*(f2.x*((exp_R*f1)`|Z).x+(exp_R*f1).x*diff(f2,x))
       by A6,A14,FDIFF_1:def 8
       .=(1/log(number_e,a))*(f2.x*((exp_R*f1)`|Z).x+(exp_R*f1).x*(f2`|Z).x)
       by A9,A14,FDIFF_1:def 8
       .=(1/log(number_e,a))*(f2.x*(a #R x*log(number_e,a))
       +(exp_R*f1).x*(f2`|Z).x) by A5,A3,Th11,A14
       .=(1/log(number_e,a))*(f2.x*(a#R x*log(number_e,a))+(exp_R*f1).x*1)
       by A7,FDIFF_1:31,A5,A14
       .=(1/log(number_e,a))*((f2.x*log(number_e,a)+1)*a #R x) by A15
       .=(1/log(number_e,a))*(((x-1/log(number_e,a))*
         log(number_e,a)+1)*a #R x) by A2,A14
       .=(1/log(number_e,a))*(x*log(number_e,a)
       -1/log(number_e,a)*log(number_e,a)+1)*a #R x
       .=(1/log(number_e,a))*(x*log(number_e,a)-1+1)*a #R x by A11,XCMPLX_1:107
       .=1/log(number_e,a)*log(number_e,a)*x*a #R x
       .=1*x*a #R x by A11,XCMPLX_1:107
       .=x*a #R x;
       hence thesis;
     end;
     hence thesis by A1,FDIFF_1:28,A10;
   end;

:: 12 f.x=a #R x*exp(x)/(1+ln a)
theorem
  Z c= dom ((1/(1+log(number_e,a)))(#)((exp_R*f)(#)exp_R)) &
  (for x st x in Z holds f.x=x*log(number_e,a)) & a>0 & a<>1/number_e
  implies
  (1/(1+log(number_e,a)))(#)((exp_R*f)(#)exp_R) is_differentiable_on Z &
  for x st x in Z holds
  (((1/(1+log(number_e,a)))(#)((exp_R*f)(#)exp_R))`|Z).x =a #R x*exp_R.x
  proof
    assume that A1:Z c= dom ((1/(1+log(number_e,a)))(#)((exp_R*f)(#)exp_R)) and
A2: (for x st x in Z holds f.x=x*log(number_e,a)) & a>0 and
A3: a<>1/number_e;
A4: Z c= dom ((exp_R*f)(#)exp_R) by SEQ_1:def 6,A1;
    then Z c= dom (exp_R*f) /\ dom exp_R by SEQ_1:def 5;
    then A5:Z c= dom (exp_R*f) & Z c= dom exp_R by XBOOLE_1:18;
    then A6:exp_R*f is_differentiable_on Z & for x st x in Z holds
    ((exp_R*f)`|Z).x =a #R x*log(number_e,a) by A2,Th11;
A7: exp_R is_differentiable_on Z by TAYLOR_1:16,FDIFF_1:34;
    then A8:(exp_R*f)(#)exp_R is_differentiable_on Z by A4,A6,FDIFF_1:29;
A9: 1+log(number_e,a)<>0
    proof
      assume A10:1+log(number_e,a)=0;
A11:   number_e>0 & number_e<>1 by TAYLOR_1:11;
A12:   number_e*a>0*a by TAYLOR_1:11,XREAL_1:70,A2;
A13:   log(number_e,1)=0 by TAYLOR_1:13,SIN_COS2:13
      .=log(number_e,number_e)+log(number_e,a) by A10,POWER:60,A11
      .=log(number_e,number_e*a) by POWER:61,A11,A2;
      number_e*a=(number_e) to_power log(number_e,1) by A13,POWER:def 3,A11,A12
      .=1 by POWER:def 3,A11;
      hence contradiction by A3,XCMPLX_1:74;
    end;
    for x st x in Z holds
    (((1/(1+log(number_e,a)))(#)((exp_R*f)(#)exp_R))`|Z).x =a #R x*exp_R.x
    proof
      let x;
      assume A14:x in Z;
      then A15:(exp_R*f).x=exp_R.(f.x) by A5,FUNCT_1:22
      .=exp_R.(x*log(number_e,a)) by A14,A2
      .=a #R x by A2,Th1;
      (((1/(1+log(number_e,a)))(#)((exp_R*f)(#)exp_R))`|Z).x
      =(1/(1+log(number_e,a)))*diff((exp_R*f)(#)exp_R,x) by A1,A8,FDIFF_1:28,
A14
      .=(1/(1+log(number_e,a)))*(((exp_R*f)(#)exp_R)`|Z).x
        by FDIFF_1:def 8,A14,A8
      .=(1/(1+log(number_e,a)))*(exp_R.x*diff(exp_R*f,x)+
       (exp_R*f).x*diff(exp_R,x)) by A14,A7,A6,A4,FDIFF_1:29
      .=(1/(1+log(number_e,a)))*(exp_R.x*((exp_R*f)`|Z).x+
         (exp_R*f).x*diff(exp_R,x)) by A6,A14,FDIFF_1:def 8
      .=(1/(1+log(number_e,a)))*(exp_R.x*((exp_R*f)`|Z).x+(exp_R*f).x*exp_R.x)
      by TAYLOR_1:16
      .=(1/(1+log(number_e,a)))*((((exp_R*f)`|Z).x+(exp_R*f).x)*exp_R.x)
      .=(1/(1+log(number_e,a)))*((a #R x*log(number_e,a)+(exp_R*f).x)*exp_R.x)
      by A5,A2,Th11,A14
      .=(1/(1+log(number_e,a)))*(log(number_e,a)+1)*a #R x*exp_R.x by A15
      .=1*a #R x*exp_R.x by A9,XCMPLX_1:107
      .=a #R x*exp_R.x;
      hence thesis;
    end;
    hence thesis by A8,A1,FDIFF_1:28;
  end;

:: 13 f.x=exp_R(-x)
theorem Th14:
  Z c= dom (exp_R*f) & (for x st x in Z holds f.x=-x)
  implies exp_R*f is_differentiable_on Z &
  for x st x in Z holds ((exp_R*f)`|Z).x = -exp_R(-x)
  proof
    assume that A1:Z c= dom (exp_R*f) and
A2: for x st x in Z holds f.x=-x;
    for y st y in Z holds y in dom f by FUNCT_1:21,A1;
    then A3:Z c= dom f by TARSKI:def 3;
A4: for x st x in Z holds f.x=(-1)*x+0
    proof
      let x;
      assume x in Z;
      then f.x=-x by A2
      .=(-1)*x+0;
      hence thesis;
    end;
    then A5:f is_differentiable_on Z &
    for x st x in Z holds (f`|Z).x = -1 by FDIFF_1:31,A3;
A6: for x st x in Z holds exp_R*f is_differentiable_in x
    proof
      let x;
      assume x in Z;
      then f is_differentiable_in x by FDIFF_1:16,A5;
      hence exp_R*f is_differentiable_in x by TAYLOR_1:19;
    end;
    then A7:exp_R*f is_differentiable_on Z by A1,FDIFF_1:16;
    for x st x in Z holds ((exp_R*f)`|Z).x = -exp_R(-x)
    proof
      let x;
      assume A8:x in Z;
      then f is_differentiable_in x by FDIFF_1:16,A5;
      then diff(exp_R*f,x) = exp_R.(f.x)*diff(f,x) by TAYLOR_1:19
      .=exp_R.(f.x)*(f`|Z).x by FDIFF_1:def 8,A8,A5
      .=exp_R.(f.x)*(-1) by A4,FDIFF_1:31,A3,A8
      .=exp_R.(-x)*(-1) by A8,A2
      .=-exp_R.(-x)
      .=-exp_R(-x) by SIN_COS:def 27;
      hence thesis by A8,A7,FDIFF_1:def 8;
    end;
    hence thesis by A6,A1,FDIFF_1:16;
  end;

:: 14 f.x=-(x+1)*exp_R(-x)
theorem
  Z c= dom (f1(#)(exp_R*f2)) & (for x st x in Z holds f1.x=-x-1 & f2.x=-x)
  implies f1(#)(exp_R*f2) is_differentiable_on Z &
  for x st x in Z holds ((f1(#)(exp_R*f2))`|Z).x = x/exp_R(x)
  proof
    assume that A1:Z c= dom (f1(#)(exp_R*f2)) and
A2: (for x st x in Z holds f1.x=-x-1 & f2.x=-x);
A3: for x st x in Z holds f2.x=-x by A2;
    Z c= dom f1 /\ dom (exp_R*f2) by SEQ_1:def 5,A1;
    then A4:Z c= dom f1 & Z c= dom (exp_R*f2) by XBOOLE_1:18;
A5: for x st x in Z holds f1.x=(-1)*x+-1
    proof
      let x;
      assume x in Z;
      then f1.x=-x-1 by A2
      .=(-1)*x+-1;
      hence thesis;
    end;
    then A6:f1 is_differentiable_on Z &
    for x st x in Z holds (f1`|Z).x = -1 by FDIFF_1:31,A4;
A7: exp_R*f2 is_differentiable_on Z &
    for x st x in Z holds ((exp_R*f2)`|Z).x = -exp_R(-x) by A3,A4,Th14;
    for x st x in Z holds ((f1(#)(exp_R*f2))`|Z).x = x/exp_R(x)
    proof
      let x;
      assume A8:x in Z;
      then A9:(exp_R*f2).x=exp_R.(f2.x) by A4,FUNCT_1:22
      .=exp_R.(-x) by A8,A2
      .=exp_R(-x) by SIN_COS:def 27;
      ((f1(#)(exp_R*f2))`|Z).x
      =((exp_R*f2).x)*diff(f1,x) + (f1.x)*diff(exp_R*f2,x)
        by A1,A7,A6,FDIFF_1:29,A8
      .=exp_R(-x)*diff(f1,x) + (-x-1)*diff(exp_R*f2,x) by A9,A8,A2
      .=exp_R(-x)*(f1`|Z).x + (-x-1)*diff(exp_R*f2,x) by A8,A6,FDIFF_1:def 8
      .=exp_R(-x)*(f1`|Z).x + (-x-1)*((exp_R*f2)`|Z).x by A8,A7,FDIFF_1:def 8
      .=exp_R(-x)*(-1) + (-x-1)*((exp_R*f2)`|Z).x by A5,FDIFF_1:31,A4,A8
      .=exp_R(-x)*(-1) + (-x-1)*(-exp_R(-x)) by A3,A4,Th14,A8
      .=x*exp_R(-x)
      .=x*(1/exp_R(x)) by TAYLOR_1:4
      .=x/exp_R(x) by XCMPLX_1:100;
      hence thesis;
    end;
    hence thesis by A1,A6,FDIFF_1:29,A7;
  end;

:: 16 f.x=a #R (-x)=exp_R.(-x*ln a)
theorem Th16:
  Z c= dom -(exp_R*f) & (for x st x in Z holds f.x=-x*log(number_e,a))
  & a>0 implies
  -(exp_R*f) is_differentiable_on Z &
  for x st x in Z holds ((-(exp_R*f))`|Z).x =a #R (-x)*log(number_e,a)
  proof
    assume that A1:Z c= dom -(exp_R*f) and
A2: (for x st x in Z holds f.x=-x*log(number_e,a)) and
A3: a>0;
A4: Z c= dom (exp_R*f) by A1,SEQ_1:def 7;
    for y st y in Z holds y in dom f by FUNCT_1:21,A4;
    then A5:Z c= dom f by TARSKI:def 3;
A6: for x st x in Z holds f.x = (-log(number_e,a))*x+0
    proof
      let x;
      assume x in Z;
      then f.x=-log(number_e,a)*x by A2
      .=(-log(number_e,a))*x+0;
      hence thesis;
    end;
    then A7:f is_differentiable_on Z &
    for x st x in Z holds (f`|Z).x =-log(number_e,a) by FDIFF_1:31,A5;
    for x st x in Z holds exp_R*f is_differentiable_in x
    proof
      let x;
      assume x in Z;
      then f is_differentiable_in x by FDIFF_1:16,A7;
      hence exp_R*f is_differentiable_in x by TAYLOR_1:19;
    end;
    then A8:exp_R*f is_differentiable_on Z by A4,FDIFF_1:16;
A9: Z c= dom ((-1)(#)(exp_R*f)) by A1,RFUNCT_1:38;
    then A10:(-1)(#)(exp_R*f) is_differentiable_on Z & for x st x in Z holds
    (((-1)(#)(exp_R*f))`|Z).x=(-1)*diff((exp_R*f),x) by FDIFF_1:28,A8;
    for x st x in Z holds ((-(exp_R*f))`|Z).x = a #R (-x)*log(number_e,a)
    proof
      let x;
      assume A11:x in Z;
      then A12:f is_differentiable_in x by FDIFF_1:16,A7;
      ((-(exp_R*f))`|Z).x=(((-1)(#)(exp_R*f))`|Z).x by RFUNCT_1:38
      .=(-1)*diff(exp_R*f,x) by A11,FDIFF_1:28,A9,A8
      .=(-1)*(exp_R.(f.x)*diff(f,x)) by TAYLOR_1:19,A12
      .=(-1)*(exp_R.(f.x)*(f`|Z).x ) by FDIFF_1:def 8,A11,A7
      .=(-1)*(exp_R.(f.x)*(-log(number_e,a))) by A6,FDIFF_1:31,A5,A11
      .=(-1)*(exp_R.(-x*log(number_e,a))*(-log(number_e,a))) by A11,A2
      .=(-1)*(a #R (-x)*(-log(number_e,a))) by A3,Th2
      .=a #R (-x)*log(number_e,a);
      hence thesis;
    end;
    hence thesis by RFUNCT_1:38,A10;
  end;

:: 17 f.x=-(a #R -x)*(x+1/ln a)/ln a
theorem
  Z c= dom ((1/log(number_e,a))(#)((-(exp_R*f1))(#)f2)) &
  (for x st x in Z holds f1.x=-x*log(number_e,a) & f2.x=x+1/log(number_e,a)) &
  a>0 & a<>1 implies
  (1/log(number_e,a))(#)((-(exp_R*f1))(#)f2) is_differentiable_on Z &
  for x st x in Z holds
  (((1/log(number_e,a))(#)((-(exp_R*f1))(#)f2))`|Z).x =x /a #R x
  proof
    assume that A1:
    Z c= dom ((1/log(number_e,a))(#)((-(exp_R*f1))(#)f2)) and
A2: (for x st x in Z holds f1.x=-x*log(number_e,a) & f2.x=x+1/log(number_e,a))
    & a>0 & a<>1;
A3: (for x st x in Z holds f1.x=-x*log(number_e,a)) & a>0 by A2;
A4: Z c= dom ((-(exp_R*f1))(#)f2) by SEQ_1:def 6,A1;
    then Z c= dom (-(exp_R*f1)) /\ dom f2 by SEQ_1:def 5;
    then A5:Z c= dom (-(exp_R*f1)) & Z c= dom f2 by XBOOLE_1:18;
    then A6: -(exp_R*f1) is_differentiable_on Z &
    for x st x in Z holds ((-(exp_R*f1))`|Z).x =a #R (-x)*log(number_e,a)
    by A3,Th16;
A7: for x st x in Z holds f2.x = 1*x+1/log(number_e,a) by A2;
     then A8:f2 is_differentiable_on Z & for x st x in Z holds
     (f2`|Z).x=1 by FDIFF_1:31,A5;
     then A9:(-(exp_R*f1))(#)f2 is_differentiable_on Z by A4,A6,FDIFF_1:29;
A10:  log(number_e,a)<>0
     proof
       assume A11:log(number_e,a)=0;
A12:    log(number_e,a)=log(number_e,1) by SIN_COS2:13,A11,TAYLOR_1:13;
A13:    number_e>0 & number_e<>1 by TAYLOR_1:11;
       then a=(number_e) to_power log(number_e,1) by A12,POWER:def 3,A2
       .=1 by POWER:def 3,A13;
       hence contradiction by A2;
     end;
     for x st x in Z holds
     (((1/log(number_e,a))(#)((-(exp_R*f1))(#)f2))`|Z).x =x /a #R x
     proof
       let x;
       assume A14:x in Z;
       then x in dom -(exp_R*f1) by A5;
       then A15:x in dom (exp_R*f1) by SEQ_1:def 7;
A16:    (-(exp_R*f1)).x=-((exp_R*f1).x) by SEQ_1:def 7,A14,A5
       .=-(exp_R.(f1.x)) by FUNCT_1:22,A15
       .=-(exp_R.(-x*log(number_e,a))) by A14,A2
       .=-a #R (-x) by A2,Th2;
       (((1/log(number_e,a))(#)((-(exp_R*f1))(#)f2))`|Z).x
       =(1/log(number_e,a))*diff((-(exp_R*f1))(#)f2,x) by A1,A9,FDIFF_1:28,A14
       .=(1/log(number_e,a))*(((-(exp_R*f1))(#)f2)`|Z).x by FDIFF_1:def 8,A14,
A9
       .=(1/log(number_e,a))*(f2.x*diff(-(exp_R*f1),x)+
        (-(exp_R*f1)).x*diff(f2,x)) by A14,A4,A6,A8,FDIFF_1:29
       .=(1/log(number_e,a))*(f2.x*((-(exp_R*f1))`|Z).x+
        (-(exp_R*f1)).x*diff(f2,x)) by A6,A14,FDIFF_1:def 8
       .=(1/log(number_e,a))*(f2.x*((-(exp_R*f1))`|Z).x+
         (-(exp_R*f1)).x*(f2`|Z).x)
       by A8,A14,FDIFF_1:def 8
       .=(1/log(number_e,a))*(f2.x*(a #R (-x)*log(number_e,a))
       +(-(exp_R*f1)).x*(f2`|Z).x) by A5,A3,Th16,A14
       .=(1/log(number_e,a))*(f2.x*(a #R (-x)*log(number_e,a))
       +(-(exp_R*f1)).x*1) by A7,FDIFF_1:31,A5,A14
       .=(1/log(number_e,a))*((f2.x*log(number_e,a)-1)*a #R (-x)) by A16
       .=(1/log(number_e,a))*(((x+1/log(number_e,a))*
         log(number_e,a)-1)*a #R (-x)) by A2,A14
       .=(1/log(number_e,a))*
       (x*log(number_e,a)+1/log(number_e,a)*log(number_e,a)-1)*a #R (-x)
       .=(1/log(number_e,a))*(x*log(number_e,a)+1-1)*a #R (-x)
         by A10,XCMPLX_1:107
       .=(1/log(number_e,a))*log(number_e,a)*x*a #R (-x)
       .=1*x*a #R (-x) by A10,XCMPLX_1:107
       .=x*(1/a #R x) by PREPOWER:90,A2
       .=x/a #R x by XCMPLX_1:100;
       hence thesis;
     end;
     hence thesis by A9,A1,FDIFF_1:28;
   end;

:: 18 f.x=(1/(ln a-1))*a #R x/exp_R(x)
theorem
  Z c= dom ((1/(log(number_e,a)-1))(#)((exp_R*f)/exp_R)) &
  (for x st x in Z holds f.x=x*log(number_e,a)) & a>0 & a<>number_e
  implies
  (1/(log(number_e,a)-1))(#)((exp_R*f)/exp_R) is_differentiable_on Z &
  for x st x in Z holds
  (((1/(log(number_e,a)-1))(#)((exp_R*f)/exp_R))`|Z).x =a #R x/exp_R.x
  proof
    assume that A1:Z c= dom ((1/(log(number_e,a)-1))(#)((exp_R*f)/exp_R)) and
A2: (for x st x in Z holds f.x=x*log(number_e,a)) & a>0 and
A3: a<>number_e;
    Z c= dom ((exp_R*f)/exp_R) by SEQ_1:def 6,A1;
    then Z c= dom (exp_R*f) /\ (dom exp_R \ (exp_R)"{0}) by RFUNCT_1:def 4;
    then A4:Z c= dom (exp_R*f) by XBOOLE_1:18;
A5: exp_R*f is_differentiable_on Z & for x st x in Z holds
    ((exp_R*f)`|Z).x =a #R x*log(number_e,a) by A2,A4,Th11;
A6: exp_R is_differentiable_on Z by TAYLOR_1:16,FDIFF_1:34;
    for x st x in Z holds exp_R.x<>0 by SIN_COS:59;
    then A7:(exp_R*f)/exp_R is_differentiable_on Z by A5,A6,FDIFF_2:21;
A8: log(number_e,a)-1<>0
     proof
       assume A9:log(number_e,a)-1=0;
A10:    number_e>0 & number_e<>1 by TAYLOR_1:11;
A11:    log(number_e,a)=log(number_e,number_e) by A9,A10,POWER:60;
       a=(number_e) to_power log(number_e,number_e) by A11,POWER:def 3,A10,A2
       .=number_e by POWER:def 3,A10;
       hence contradiction by A3;
     end;
     for x st x in Z holds
     (((1/(log(number_e,a)-1))(#)((exp_R*f)/exp_R))`|Z).x =a #R x/exp_R.x
     proof
       let x;
       assume A12:x in Z;
       then A13:(exp_R*f).x=exp_R.(f.x) by FUNCT_1:22,A4
       .=exp_R.(x*log(number_e,a)) by A12,A2
       .=a #R x by A2,Th1;
A14:    exp_R is_differentiable_in x by SIN_COS:70;
A15:   exp_R*f is_differentiable_in x by A12,A5,FDIFF_1:16;
A16:   exp_R.x <>0 by SIN_COS:59;
       (((1/(log(number_e,a)-1))(#)((exp_R*f)/exp_R))`|Z).x
       =(1/(log(number_e,a)-1))*diff(((exp_R*f)/exp_R),x)
         by A1,A7,FDIFF_1:28,A12
       .=(1/(log(number_e,a)-1))*
       ((diff(exp_R*f,x) * exp_R.x - diff(exp_R,x) *(exp_R*f).x)/(exp_R.x)^2)
       by FDIFF_2:14,A14,A15,A16
       .=(1/(log(number_e,a)-1))*((diff(exp_R*f,x)*
       exp_R.x-exp_R.x*a #R x)/(exp_R.x)^2) by SIN_COS:70,A13
       .=(1/(log(number_e,a)-1))*(((diff(exp_R*f,x)-a #R x)*exp_R.x)/
         ((exp_R.x)*(exp_R.x)))
       .=(1/(log(number_e,a)-1))*((diff(exp_R*f,x)-a #R x)/(exp_R.x))
         by XCMPLX_1:92,A16
       .=(1/(log(number_e,a)-1))*(diff(exp_R*f,x)-a #R x)/(exp_R.x)
          by XCMPLX_1:75
       .=(1/(log(number_e,a)-1))*(((exp_R*f)`|Z).x-a #R x)/(exp_R.x)
       by A5,A12,FDIFF_1:def 8
       .=(1/(log(number_e,a)-1))*(a #R x*log(number_e,a)-a #R x)/(exp_R.x)
       by A2,A4,Th11,A12
       .=(1/(log(number_e,a)-1))*(log(number_e,a)-1)*a #R x/exp_R.x
       .=1* a #R x/exp_R.x by A8,XCMPLX_1:107
       .=a #R x/exp_R.x;
       hence thesis;
     end;
     hence thesis by A7,FDIFF_1:28,A1;
   end;

:: 19 f.x=(1/(1-ln a))*exp_R(x)/a #R x
theorem
  Z c= dom ((1/(1-log(number_e,a)))(#)(exp_R/(exp_R*f))) &
  (for x st x in Z holds f.x=x*log(number_e,a)) & a>0 & a<>number_e
  implies
  (1/(1-log(number_e,a)))(#)(exp_R/(exp_R*f)) is_differentiable_on Z &
  for x st x in Z holds
  (((1/(1-log(number_e,a)))(#)(exp_R/(exp_R*f)))`|Z).x =exp_R.x/a #R x
  proof
    assume that A1:Z c= dom ((1/(1-log(number_e,a)))(#)(exp_R/(exp_R*f))) and
A2: (for x st x in Z holds f.x=x*log(number_e,a)) & a>0 and
A3: a<>number_e;
    Z c= dom (exp_R/(exp_R*f)) by SEQ_1:def 6,A1;
    then Z c= dom exp_R /\ (dom (exp_R*f) \ (exp_R*f)"{0}) by RFUNCT_1:def 4;
    then A4:Z c= dom (exp_R*f) \ (exp_R*f)"{0} by XBOOLE_1:18;
    A5:Z c= dom (exp_R*f) by XBOOLE_1:1,A4;
A6: exp_R is_differentiable_on Z by TAYLOR_1:16,FDIFF_1:34;
A7: exp_R*f is_differentiable_on Z & for x st x in Z holds
    ((exp_R*f)`|Z).x =a #R x*log(number_e,a) by A2,A5,Th11;
A8: for x st x in Z holds (exp_R*f).x<>0
    proof
      let x;
      assume x in Z;
      then (exp_R*f).x=exp_R.(f.x) by FUNCT_1:22,A5;
      hence thesis by SIN_COS:59;
    end;
    then A9:exp_R/(exp_R*f) is_differentiable_on Z by A7,A6,FDIFF_2:21;
A10: 1-log(number_e,a)<>0
     proof
       assume A11:1-log(number_e,a)=0;
A12:    number_e>0 & number_e<>1 by TAYLOR_1:11;
A13:    log(number_e,a)=log(number_e,number_e) by A11,A12,POWER:60;
       a=(number_e) to_power log(number_e,number_e) by A13,POWER:def 3,A12,A2
       .=number_e by POWER:def 3,A12;
       hence contradiction by A3;
     end;
     for x st x in Z holds
     (((1/(1-log(number_e,a)))(#)(exp_R/(exp_R*f)))`|Z).x =exp_R.x/a #R x
     proof
       let x;
       assume A14:x in Z;
       then A15:(exp_R*f).x=exp_R.(f.x) by FUNCT_1:22,A5
       .=exp_R.(x*log(number_e,a)) by A14,A2
       .=a #R x by A2,Th1;
A16:   a #R x >0 by PREPOWER:95,A2;
A17:   exp_R is_differentiable_in x by SIN_COS:70;
A18:   exp_R*f is_differentiable_in x by A14,A7,FDIFF_1:16;
A19:   (exp_R*f).x <>0 by A8,A14;
       (((1/(1-log(number_e,a)))(#)(exp_R/(exp_R*f)))`|Z).x
       =(1/(1-log(number_e,a)))*diff((exp_R/(exp_R*f)),x)
         by A1,A9,FDIFF_1:28,A14
       .=(1/(1-log(number_e,a)))*
       ((diff(exp_R,x) * (exp_R*f).x - diff((exp_R*f),x) *
         exp_R.x)/((exp_R*f).x)^2)
       by FDIFF_2:14,A17,A18,A19
       .=(1/(1-log(number_e,a)))*
       ((exp_R.x * a #R x - diff((exp_R*f),x)*exp_R.x)/(a #R x)^2)
         by SIN_COS:70,A15
       .=(1/(1-log(number_e,a)))*((exp_R.x*(a #R x
          -diff((exp_R*f),x)))/(a #R x)^2)
       .=(1/(1-log(number_e,a)))*((exp_R.x*(a #R x -
          ((exp_R*f)`|Z).x))/(a #R x)^2) by A7,A14,FDIFF_1:def 8
       .=(1/(1-log(number_e,a)))*
       ((exp_R.x*(a #R x -a #R x*log(number_e,a)))/(a #R x)^2) by A2,A5,Th11,
A14
       .=(1/(1-log(number_e,a)))*((1-log(number_e,a))*exp_R.x*a #R x)/
        (a #R x)^2 by XCMPLX_1:75
       .=(1/(1-log(number_e,a)))*(1-log(number_e,a))*exp_R.x*a #R x/(a #R x)^2
       .=1*exp_R.x*a #R x/(a #R x)^2 by A10,XCMPLX_1:107
       .=exp_R.x/a #R x by A16,XCMPLX_1:92;
       hence thesis;
     end;
     hence thesis by A9,FDIFF_1:28,A1;
   end;

:: 20 f.x=ln(exp_R.x+1)
theorem
  Z c= dom (ln*(exp_R+f)) & (for x st x in Z holds f.x=1 )
  implies ln*(exp_R+f) is_differentiable_on Z &
  for x st x in Z holds
   ((ln*(exp_R+f))`|Z).x = exp_R.x/(exp_R.x+1)
  proof
    assume that A1:Z c= dom (ln*(exp_R+f)) and
A2: for x st x in Z holds f.x=1;
    for y st y in Z holds y in dom (exp_R+f) by FUNCT_1:21,A1;
    then A3:Z c= dom (exp_R+f) by TARSKI:def 3;
    then Z c= dom exp_R /\ dom f by SEQ_1:def 3;
    then A4:Z c= dom f by XBOOLE_1:18;
A5: for x st x in Z holds f.x=0*x+1 by A2;
     then A6:f is_differentiable_on Z &
     for x st x in Z holds (f`|Z).x = 0 by FDIFF_1:31,A4;
A7:  exp_R is_differentiable_on Z by TAYLOR_1:16,FDIFF_1:34;
     then A8:exp_R+f is_differentiable_on Z by A6,A3,FDIFF_1:26;
A9: for x st x in Z holds ((exp_R+f)`|Z).x =exp_R.x
     proof
       let x;
       assume A10:x in Z;
       hence ((exp_R+f)`|Z).x = diff(exp_R,x) + diff(f,x)
