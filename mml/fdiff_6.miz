:: 23 f.x=exp_R(2*x)+1
theorem Th23:
  Z c= dom ((( #Z 2)*exp_R)+f) & (for x st x in Z holds f.x=1)
  implies (( #Z 2)*exp_R)+f is_differentiable_on Z & for x st x in Z holds
  (((( #Z 2)*exp_R)+f)`|Z).x =2*exp_R(2*x)
  proof
    assume that A1:Z c= dom ((( #Z 2)*exp_R)+f) and
A2: (for x st x in Z holds f.x=1);
    Z c= dom (( #Z 2)*exp_R) /\ dom f by SEQ_1:def 3,A1;
    then A3:Z c= dom (( #Z 2)*exp_R) & Z c= dom f by XBOOLE_1:18;
A4: for x st x in Z holds f.x=0*x+1 by A2;
    then A5:f is_differentiable_on Z &
    for x st x in Z holds (f`|Z).x = 0 by FDIFF_1:31,A3;
    now let x;
      assume x in Z;
      exp_R is_differentiable_in x by SIN_COS:70;
      hence ( #Z 2)*exp_R is_differentiable_in x by TAYLOR_1:3;
    end;
    then A6:( #Z 2)*exp_R is_differentiable_on Z by A3,FDIFF_1:16;
    for x st x in Z holds (((( #Z 2)*exp_R)+f)`|Z).x =2*exp_R(2*x)
    proof
      let x;
      assume A7:x in Z;
A8:   exp_R is_differentiable_in x by SIN_COS:70;
      (((( #Z 2)*exp_R)+f)`|Z).x
      =diff(( #Z 2)*exp_R,x)+diff(f,x) by A5,A6,FDIFF_1:26,A1,A7
      .=(2*( (exp_R.x) #Z (2-1)) * diff(exp_R,x))+diff(f,x) by A8,TAYLOR_1:3
      .=2*( (exp_R.x) #Z (2-1)) * exp_R.x+diff(f,x) by SIN_COS:70
      .=2* exp_R.x * exp_R.x+diff(f,x) by PREPOWER:45
      .=2*( exp_R.x * exp_R.x)+diff(f,x)
      .=2*( exp_R( x )* exp_R.x)+diff(f,x) by SIN_COS:def 27
      .=2*( exp_R( x )* exp_R(x))+diff(f,x) by SIN_COS:def 27
      .=2*(exp_R(x+x))+diff(f,x) by SIN_COS:55
      .=2* exp_R(2*x)+(f`|Z).x by FDIFF_1:def 8,A7,A5
      .= 2* exp_R(2*x)+0 by A4,FDIFF_1:31,A3,A7
      .=2* exp_R(2*x);
      hence thesis;
    end;
    hence thesis by A6,A5,FDIFF_1:26,A1;
  end;

:: 24 f.x=(1/2)*ln(exp_R(2*x)+1)
theorem
  Z c= dom ((1/2)(#)(ln*f)) & f=(( #Z 2)*exp_R)+f1 &
  (for x st x in Z holds f1.x=1) implies (1/2)(#)(ln*f)
  is_differentiable_on Z & for x st x in Z holds
  (((1/2)(#)(ln*f))`|Z).x =exp_R(x)/(exp_R(x)+exp_R(-x))
  proof
    assume that A1:Z c= dom ((1/2)(#)(ln*f)) and
A2: f=(( #Z 2)*exp_R)+f1 & (for x st x in Z holds f1.x=1);
A3: Z c= dom (ln*f) by SEQ_1:def 6,A1;
    for y st y in Z holds y in dom f by FUNCT_1:21,A3;
    then A4:Z c= dom ((( #Z 2)*exp_R)+f1) by TARSKI:def 3,A2;
    then Z c= dom (( #Z 2)*exp_R) /\ dom f1 by SEQ_1:def 3;
    then A5:Z c= dom (( #Z 2)*exp_R) by XBOOLE_1:18;
A6: (( #Z 2)*exp_R)+f1 is_differentiable_on Z & for x st x in Z holds
    (((( #Z 2)*exp_R)+f1)`|Z).x =2*exp_R(2*x) by A4,A2,Th23;
A7: for x st x in Z holds f.x >0
    proof
      let x;
      assume A8:x in Z;
A9:   ((( #Z 2)*exp_R)+f1).x
      =(( #Z 2)*exp_R).x+f1.x by SEQ_1:def 3,A4,A8
      .=( #Z 2).(exp_R.x)+f1.x by A8,A5,FUNCT_1:22
      .=(exp_R.x) #Z 2+f1.x by TAYLOR_1:def 1
      .=(exp_R.x) #Z 2+1 by A2,A8;
      exp_R.x>0 by SIN_COS:59;
      then (exp_R.x) #Z 2>0 by PREPOWER:49;
      hence thesis by A9,A2,XREAL_1:36;
    end;
    for x st x in Z holds ln*f is_differentiable_in x
    proof
      let x;
      assume A10:x in Z;
      then A11:f is_differentiable_in x by FDIFF_1:16,A6,A2;
      f.x >0 by A7,A10;
      hence ln*f is_differentiable_in x by TAYLOR_1:20,A11;
    end;
    then A12:ln*f is_differentiable_on Z by A3,FDIFF_1:16;
    for x st x in Z holds
    (((1/2)(#)(ln*f))`|Z).x =exp_R(x)/(exp_R(x)+exp_R(-x))
    proof
      let x;
      assume A13:x in Z;
A14:  exp_R(x)>0 by SIN_COS:60;
A15:   ((( #Z 2)*exp_R)+f1).x
      =(( #Z 2)*exp_R).x+f1.x by SEQ_1:def 3,A4,A13
      .=( #Z 2).(exp_R.x)+f1.x by A13,A5,FUNCT_1:22
      .=(exp_R.x) #Z 2+f1.x by TAYLOR_1:def 1
      .=(exp_R.x) #Z 2+1 by A2,A13
      .=(exp_R(x)) #Z (1+1)+1 by SIN_COS:def 27
      .=(exp_R(x)) #Z 1 * (exp_R(x)) #Z 1 +1 by PREPOWER:54,A14
      .=exp_R(x) * (exp_R(x)) #Z 1 +1 by PREPOWER:45
      .=exp_R(x) * exp_R(x) +1 by PREPOWER:45;
A16:  f is_differentiable_in x by FDIFF_1:16,A6,A2,A13;
A17:  f.x >0 by A7,A13;
      (((1/2)(#)(ln*f))`|Z).x
      =(1/2)*diff((ln*f),x) by A12,A1,FDIFF_1:28,A13
      .=(1/2)*(diff(f,x)/f.x) by TAYLOR_1:20,A16,A17
      .=(1/2)*((((( #Z 2)*exp_R)+f1)`|Z).x/((( #Z 2)*exp_R)+f1).x)
      by A13,FDIFF_1:def 8,A6,A2
      .=(1/2)*((2*exp_R(2*x))/(exp_R(x) * exp_R(x) +1)) by A4,A2,Th23,A13,A15
      .=(1/2)*(2*exp_R(2*x))/(exp_R(x) * exp_R(x) +1) by XCMPLX_1:75
      .=(exp_R(x+x)/exp_R(x))/((exp_R(x) * exp_R(x)+1)/exp_R(x))
       by A14,XCMPLX_1:55
      .=((exp_R(x)*exp_R(x))/exp_R(x))/((exp_R(x) * exp_R(x)+1)/exp_R(x))
        by SIN_COS:55
      .=(exp_R(x)*exp_R(x)/exp_R(x))/(exp_R(x) * exp_R(x)/exp_R(x)+1/exp_R(x))
         by XCMPLX_1:63
      .=exp_R(x)/(exp_R(x) * exp_R(x)/exp_R(x)+1/exp_R(x)) by A14,XCMPLX_1:90
      .=exp_R(x)/(exp_R(x)+1/exp_R(x)) by A14,XCMPLX_1:90
      .=exp_R(x)/(exp_R(x)+exp_R(-x)) by TAYLOR_1:4;
      hence thesis;
    end;
    hence thesis by A12,A1,FDIFF_1:28;
  end;

:: 25 f.x=exp_R(2*x)-1
