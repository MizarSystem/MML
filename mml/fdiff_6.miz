       then A13:exp_R-f is_differentiable_in x by FDIFF_1:16,A8;
       (exp_R-f).x >0 by A2,A12;
       hence thesis by TAYLOR_1:20,A13;
     end;
     then A14:ln*(exp_R-f) is_differentiable_on Z by A1,FDIFF_1:16
;
     for x st x in Z holds
      ((ln*(exp_R-f))`|Z).x = exp_R.x/(exp_R.x-1)
     proof
       let x;
       assume A15:x in Z;
       then A16:(exp_R-f).x=exp_R.x - f.x by SEQ_1:def 4,A3
       .=exp_R.x -1 by A2,A15;
A17:    (exp_R-f) is_differentiable_in x by FDIFF_1:16,A8,A15;
A18:   (exp_R-f).x >0 by A15,A2;
       diff(ln*(exp_R-f),x)
       =diff((exp_R-f),x)/((exp_R-f).x) by TAYLOR_1:20,A17,A18
       .=((exp_R-f)`|Z).x/((exp_R-f).x) by A8,A15,FDIFF_1:def 8
       .=exp_R.x/(exp_R.x -1) by A9,A15,A16;
       hence thesis by A15,A14,FDIFF_1:def 8;
