    for x st x in Z holds
    (((1/3)(#)(( #Z 3)*sin))`|Z).x =(sin.x) #Z (3-1) *cos.x by FDIFF_4:54;
A4: sin is_differentiable_on Z by SIN_COS:73,FDIFF_1:34;
    now let x;
      assume A5:x in Z;
      ((sin-((1/3)(#)(( #Z 3)*sin)))`|Z).x
      = diff(sin,x) - diff(((1/3)(#)(( #Z 3)*sin)),x) by A3,A1,FDIFF_1:27,A4,A5
      .=cos.x - diff(((1/3)(#)(( #Z 3)*sin)),x) by SIN_COS:69
      .=cos.x -(((1/3)(#)(( #Z 3)*sin))`|Z).x by FDIFF_1:def 8,A3,A5
      .=cos.x -((sin.x) #Z (3-1) *cos.x) by A2,FDIFF_4:54,A5
      .=cos.x*(1-(sin.x) #Z 2)
      .=cos.x*(1-(sin.x) |^ abs(2) ) by PREPOWER:def 4
      .=cos.x*(1-(sin.x) |^ 2 ) by ABSVALUE:def 1
      .=cos.x*(1-(sin.x)*(sin.x)) by WSIERP_1:2
      .=cos.x*((cos.x)*(cos.x)+(sin.x)*(sin.x)-(sin.x)*(sin.x)) by SIN_COS:31
      .=cos.x*((cos.x)|^2) by WSIERP_1:2
      .=((cos.x)|^(2+1)) by NEWTON:11
      .=(cos.x)|^3;
      hence ((sin-((1/3)(#)(( #Z 3)*sin)))`|Z).x =(cos.x)|^3;
    end;
    hence thesis by A3,A1,FDIFF_1:27,A4;
  end;

:: 49 f.x=sin(ln x)
theorem
  Z c= dom (sin*ln) implies sin*ln
  is_differentiable_on Z & for x st x in Z holds
  ((sin*ln)`|Z).x =cos.(log(number_e,x))/x
  proof
    assume A1: Z c= dom (sin*ln);
    for y st y in Z holds y in dom ln by FUNCT_1:21,A1;
    then A2:Z c= dom ln by TARSKI:def 3;
    then A3:ln is_differentiable_on Z by TAYLOR_1:18,FDIFF_1:34;
A4: for x st x in Z holds sin*ln is_differentiable_in x
    proof
      let x;
      assume x in Z;
      then A5: ln is_differentiable_in x by FDIFF_1:16,A3;
      sin is_differentiable_in ln.x by SIN_COS:69;
      hence sin*ln is_differentiable_in x by A5,FDIFF_2:13;
    end;
    then A6:sin*ln is_differentiable_on Z by FDIFF_1:16,A1;
    for x st x in Z holds ((sin*ln)`|Z).x =cos.(log(number_e,x))/x
    proof
      let x;
      assume A7:x in Z;
      then A8:x in right_open_halfline(0) by TAYLOR_1:18,FUNCT_1:21,A1;
A9:   ln is_differentiable_in x by FDIFF_1:16,A3,A7;
      sin is_differentiable_in ln.x by SIN_COS:69;
      then diff(sin*ln,x)
      = diff(sin,ln.x)*diff(ln,x) by A9,FDIFF_2:13
      .=cos.(ln.x)*diff(ln,x) by SIN_COS:69
      .=cos.(log(number_e,x))*diff(ln,x) by TAYLOR_1:def 2,A8
      .=cos.(log(number_e,x))*(1/x) by A7,A2,TAYLOR_1:18
      .=cos.(log(number_e,x))/x by XCMPLX_1:100;
      hence thesis by A7,A6,FDIFF_1:def 8;
    end;
    hence thesis by A4,FDIFF_1:16,A1;
  end;

:: 50 f.x=-cos(ln x)
theorem
  Z c= dom (-(cos*ln)) implies -(cos*ln)
  is_differentiable_on Z & for x st x in Z holds
  ((-(cos*ln))`|Z).x =sin.(log(number_e,x))/x
  proof
    assume Z c= dom (-(cos*ln));
    then A1:Z c= dom (cos*ln) by SEQ_1:def 7;
    then A2:Z c= dom ((-1)(#)(cos*ln)) by SEQ_1:def 6;
    for y st y in Z holds y in dom ln by FUNCT_1:21,A1;
    then A3:Z c= dom ln by TARSKI:def 3;
    then A4:ln is_differentiable_on Z by TAYLOR_1:18,FDIFF_1:34;
    for x st x in Z holds cos*ln is_differentiable_in x
    proof
      let x;
      assume x in Z;
      then A5:ln is_differentiable_in x by FDIFF_1:16,A4;
      cos is_differentiable_in ln.x by SIN_COS:68;
      hence thesis by A5,FDIFF_2:13;
    end;
    then A6:cos*ln is_differentiable_on Z by FDIFF_1:16,A1;
    then A7:(-1)(#)(cos*ln) is_differentiable_on Z &
    for x st x in Z holds (((-1)(#)(cos*ln))`|Z).x
    =(-1)*diff(cos*ln,x) by FDIFF_1:28,A2;
    for x st x in Z holds
    ((-(cos*ln))`|Z).x =sin.(log(number_e,x))/x
    proof
      let x;
      assume A8:x in Z;
      then A9:x in right_open_halfline(0) by TAYLOR_1:18,FUNCT_1:21,A1;
A10:   ln is_differentiable_in x by FDIFF_1:16,A4,A8;
A11:   cos is_differentiable_in ln.x by SIN_COS:68;
      ((-(cos*ln))`|Z).x
      =(((-1)(#)(cos*ln))`|Z).x by RFUNCT_1:38
      .=(-1)*diff((cos*ln),x) by A6,FDIFF_1:28,A2,A8
      .=(-1)*(diff(cos,ln.x)*diff(ln,x))
      by A11,A10,FDIFF_2:13
      .=(-1)*((-sin.(ln.x))*diff(ln,x)) by SIN_COS:68
      .=(-1)*(-sin.(ln.x))*diff(ln,x)
      .=(-1)*(-sin.(log(number_e,x)))*diff(ln,x)
      by TAYLOR_1:def 2,A9
      .=(-1)*(-sin.(log(number_e,x)))*(1/x) by A8,A3,TAYLOR_1:18
      .=sin.(log(number_e,x))/x by XCMPLX_1:100;
      hence thesis;
    end;
    hence thesis by A7,RFUNCT_1:38;
  end;
