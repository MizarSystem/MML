    27
      .= diff(f,x)-exp_R.x by SIN_COS:70
      .= (f`|Z).x-exp_R.x by A7,A12,FDIFF_1:def 8
      .=0-exp_R.x by A6,A3,A12,FDIFF_1:31
      .=-exp_R.x;
