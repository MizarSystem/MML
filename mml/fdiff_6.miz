       x*(1+a #R x) #R (1/2)) by A28,A1,FDIFF_1:28,A23,A18
       .=(2/(3*log(number_e,a)))*((3*log(number_e,a))/2)
       *a #R x*(1+a #R x) #R (1/2)
       .=1*a #R x*(1+a #R x) #R (1/2) by A29,XCMPLX_1:113
       .=a #R x*(1+a #R x) #R (1/2);
