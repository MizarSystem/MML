A49:      [D,a] in g and
A50:      [J,b] in g;
A51:      x = while(D,J) by A48,ZFMISC_1:33;
A52:      y2 = WhiF(a,b) by A48,ZFMISC_1:33;
A53:      g.D = a by A49,FUNCT_1:8;
