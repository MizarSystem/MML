A40:  [s,C9] in TP by A33,ZFMISC_1:64;
      then
A41:  while(C9,I9) <> IJ or s <> rr by A1,A32,Th75;
A42:  [s, while(C9,I9)] in TP by A34,A35,A37,A40,Def35;
      [s, while(C9,I9)] <> [rr,IJ] by A41,ZFMISC_1:33;
      hence [s, while(C9,I9)] in P by A42,ZFMISC_1:64;
