          and J2 in B|^i and
A41:      [D,a] in g and
A42:      [J1,b] in g and
A43:      [J2,c] in g;
A44:      x = if-then-else(D,J1,J2) by A40,ZFMISC_1:33;
A45:      y2 = IfF(a,b,c) by A40,ZFMISC_1:33;
A46:      g.D = a by A41,FUNCT_1:8;
