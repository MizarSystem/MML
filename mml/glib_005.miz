                      implies y = (the_ELabel_of G).x by C0;
                    let m be odd Nat such that
                E1: m < len P & P.(m+1) = x;
                    1 <= m+1 & m+1 <= len P by E1,NAT_1:29,NAT_1:38; then
                    m+1 <= n+1 & n+1 <= m+1 by C1,C2,E1,GLIB_001:139; then
                    m+1 = n+1 by AXIOMS:21; then
                    m = n by XCMPLX_1:2;
