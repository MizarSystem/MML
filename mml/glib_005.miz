                end; then
                P[x,y] by C1;
                hence ex y being set st P[x,y];                
              end;
              suppose
            D1: not (P.(n+1) DJoins P.n,P.(n+2),G);
                set y =(the_ELabel_of G).(P.(n+1)) - P.tolerance();
