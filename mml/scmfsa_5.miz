environ

 vocabularies SUBSET_1, NUMBERS, AMI_1, SCMFSA_2, AMISTD_2, ARYTM_3, FUNCT_4,
      VALUED_1, XBOOLE_0, FUNCT_1, RELAT_1, TARSKI, FSM_1, CIRCUIT2, CARD_1,
      XXREAL_0, SCMNORM, GRAPHSP, AMI_3, ARYTM_1, INT_1, COMPLEX1, PARTFUN1,
      FINSEQ_1, FINSEQ_2, GLIB_000, STRUCT_0, MSUALG_1, TURING_1, NAT_1,
      ORDINAL1, RELOC, AMISTD_5, PBOOLE;
 notations TARSKI, XBOOLE_0, ENUMSET1, SUBSET_1, NUMBERS, XCMPLX_0, INT_2,
      ORDINAL1, INT_1, NAT_1, NAT_D, PARTFUN1, RELAT_1, FUNCT_1, PBOOLE,
      FINSEQ_1, FINSEQ_2, FUNCT_4, FUNCT_7, VALUED_1, STRUCT_0, COMPOS_1,
      EXTPRO_1, AMI_1, AMISTD_2, SCMFSA_2, XXREAL_0, AMISTD_5;
 constructors DOMAIN_1, XXREAL_0, NAT_D, RELSET_1,
      PRE_POLY, AMISTD_2, SCMFSA_2, SCMFSA_3, AMI_1, AMISTD_5, PBOOLE;
 registrations XBOOLE_0, SETFAM_1, FUNCT_1, RELSET_1, NUMBERS, XREAL_0, INT_1,
      CARD_3, AMI_1, SCMFSA_2, ORDINAL1, FINSET_1, RELAT_1, VALUED_1,
      GRFUNC_1, FUNCT_2, AMISTD_2, SCMFSA10, COMPOS_1, EXTPRO_1, SCMFSA_4,
      SCMFSA_3, AMISTD_5, PBOOLE;
 requirements NUMERALS, SUBSET, BOOLE, ARITHM;
 definitions FUNCOP_1, COMPOS_1, EXTPRO_1, AMI_1, TARSKI, AMISTD_2, AMISTD_5;
 theorems AMI_1, GRFUNC_1, NAT_1, FUNCOP_1, TARSKI, FUNCT_4, FUNCT_1, ZFMISC_1,
      RELAT_1, SCMFSA_2, SCMFSA_3, SCMFSA_4, XBOOLE_0, XBOOLE_1, VALUED_1,
      NAT_D, PBOOLE, PARTFUN1, COMPOS_1, EXTPRO_1, AMISTD_2, AMISTD_5;
 schemes NAT_1;

begin  :: Relocability

reserve j, k, m for Element of NAT;

begin :: Main theorems of relocatability

canceled 11;

theorem Th12:
  for k being Element of NAT, p being autonomic FinPartState of
  SCM+FSA, s1, s2 being State of SCM+FSA st IC SCM+FSA in dom p & p c= s1 &
Relocated(p,k) c= s2
for P1,P2 being (the Instructions of SCM+FSA)-valued ManySortedSet of NAT
  st ProgramPart p c= P1 & Reloc(ProgramPart p,k) c= P2
 for i being Element of NAT holds
  IC Comput(P1,s1,i) + k = IC Comput(P2,s2,i) &
  IncAddr(CurInstr(P1,Comput(P1,s1,i)), k) = CurInstr(P2,Comput(P2,s2,i)) &
  Comput(P1,s1,i)|dom (DataPart p)
   = Comput(P2,s2,i)|dom(DataPart (Relocated(p,k))) &
  DataPart Comput(P1,s1 +* DataPart s2,i) = DataPart Comput(P2,s2,i)
proof
  let k be Element of NAT, p be autonomic FinPartState of SCM+FSA, s1,s2 be
  State of SCM+FSA such that
A1: IC SCM+FSA in dom p and
A2: p c= s1 and
A3: Relocated(p,k) c= s2;
  let P1,P2 being (the Instructions of SCM+FSA)-valued ManySortedSet of NAT
  such that
A4: ProgramPart p c= P1 & Reloc(ProgramPart p,k) c= P2;
A5: Reloc(ProgramPart p,k) c= P2 by A4,COMPOS_1:116;
A6: ProgramPart p c= P1 by A4;
A7: Reloc(ProgramPart p,k) = ProgramPart Relocated(p,k) by COMPOS_1:116;
 then
 Reloc(ProgramPart p,k) c= Relocated(p,k) by RELAT_1:88;
 then
A8: Reloc(ProgramPart p,k) c= s2 by A3,XBOOLE_1:1;
  set s3 = s1 +* DataPart s2;
  defpred Y[Nat] means IC Comput(P1,s1,$1) + k = IC Comput(P2,s2,$1) &
   IncAddr(CurInstr(P1,Comput(P1,s1,$1)), k) = CurInstr(P2,Comput(P2,s2,$1)) &
   Comput(P1,s1,$1)|dom (DataPart p) =
    Comput(P2,s2,$1)|dom (DataPart (Relocated(p,k))) &
    DataPart Comput(P1,s3,$1) = DataPart Comput(P2,s2,$1);
A9: p is non NAT-defined by A1,COMPOS_1:19;
A10: p c= s3 by A2,A3,COMPOS_1:161;
A11: for i being Element of NAT st Y[i] holds Y[i+1]
  proof
    set DPp = DataPart p;
    let i be Element of NAT such that
A12: IC Comput(P1,s1,i) + k = IC Comput(P2,s2,i) and
A13: IncAddr (CurInstr(P1,Comput(P1,s1,i)), k)
 = CurInstr(P2,Comput(P2,s2,i)) and
A14: Comput(P1,s1,i)|dom (DataPart p) = Comput(P2,s2,i
)|dom (
    DataPart (Relocated(p,k))) and
A15: DataPart Comput(P1,s3,i) = DataPart Comput(P2,s2
,i);
    set Cs1i1 = Comput(P1,s1,i+1);
    set Cs1i = Comput(P1,s1,i);
:::TY: P2 = ProgramPart Comput(P2,s2,i+1)
:::by AMI_1:123;
A16: dom Cs1i1 = Data-Locations SCM+FSA \/ {IC SCM+FSA} \/ NAT
    by COMPOS_1:172;
    set Cs3i = Comput(P1,s3,i);
    set Cs2i = Comput(P2,s2,i);
A17: dom Cs1i = Data-Locations SCM+FSA \/ {IC SCM+FSA} \/ NAT
    by COMPOS_1:172;
    set Cs2i1 = Comput(P2,s2,i+1);
A18: dom Cs2i1 = Data-Locations SCM+FSA \/ {IC SCM+FSA} \/ NAT
    by COMPOS_1:172;
A19: dom Cs2i = Data-Locations SCM+FSA \/ {IC SCM+FSA} \/ NAT
    by COMPOS_1:172;
    set I = CurInstr(P1, Cs1i);
A20: Cs2i1 = Following(P2,Cs2i) by EXTPRO_1:4
      .= Exec (CurInstr(P2, Cs2i), Cs2i);
A21: now
      let s be State of SCM+FSA, d be FinSeq-Location;
      d in FinSeq-Locations by SCMFSA_2:10;
      then d in Data-Locations SCM+FSA by XBOOLE_0:def 3,SCMFSA_2:127;
      hence d in dom DataPart s by COMPOS_1:50;
    end;
A22: now
      let d be FinSeq-Location;
A23:  d in dom (DataPart Cs3i) by A21;
      hence Cs3i.d = (DataPart Cs3i).d by FUNCT_1:70
        .= Cs2i.d by A15,A23,FUNCT_1:70;
    end;
    set Cs3i1 = Comput(P1,s3,i+1);
A24: dom DataPart Cs2i = Data-Locations SCM+FSA by COMPOS_1:50
;
A25: dom DataPart Cs3i1 = Data-Locations SCM+FSA by COMPOS_1:50
;
A26: now
      reconsider loc = IC Cs1i1 as Element of NAT;
      assume
A27:  IC Comput(P1,s1,i+1) + k = IC Comput(P2,s2,i+1);
      reconsider ii = loc as Element of NAT;
A28:  ii in dom ProgramPart p by A2,A9,AMISTD_5:def 4,A6;
A29:  loc + k in dom Reloc(ProgramPart p, k) by A28,COMPOS_1:158;
:::Z:  (P2)/.IC Comput(P2,s2,
:::i+1)
::: = Comput(P2,s2,i+1).IC Comput(P2,s2,i+1) by TY,
:::COMPOS_1:38;

A30:   dom P2 = NAT by PARTFUN1:def 4;
      dom P1 = NAT by PARTFUN1:def 4;
      then CurInstr(P1, Cs1i1) = P1.loc by PARTFUN1:def 8
        .= (ProgramPart p).loc by A28,GRFUNC_1:8,A4
        .= (ProgramPart p).loc;
      hence
      IncAddr(CurInstr(P1,Comput(P1,s1,i+1)), k)
       = Reloc(ProgramPart p,k).(loc+k) by A28,COMPOS_1:122
      .= P2.IC Comput(P2,s2,i+1) by A27,A29,A5,GRFUNC_1:8
      .= CurInstr(P2,Comput(P2,s2,i+1)) by A30,PARTFUN1:def 8;
    end;
A31: now
      let s be State of SCM+FSA, d be Int-Location;
      d in Int-Locations by SCMFSA_2:9;
      then d in Data-Locations SCM+FSA by XBOOLE_0:def 3,SCMFSA_2:127;
      hence d in dom DataPart s by COMPOS_1:50;
    end;
A32: now
      let d be Int-Location;
A33:  d in dom DataPart Cs3i by A31;
      hence Cs3i.d = (DataPart Cs3i).d by FUNCT_1:70
        .= Cs2i.d by A15,A33,FUNCT_1:70;
    end;
    dom DPp = dom p /\(Data-Locations SCM+FSA) by RELAT_1:90;
    then
A34: dom DPp c= {IC SCM+FSA} \/ (Data-Locations SCM+FSA) by
XBOOLE_1:10,17;
A35: InsCode I <= 9+1 implies InsCode I <= 8+1 or InsCode I = 10 by NAT_1:8;
A36: InsCode I <= 10+1 implies InsCode I <= 10 or InsCode I = 11 by NAT_1:8;
A37: InsCode I <= 11+1 by SCMFSA_2:35;
A38: dom (Cs1i1|dom DPp) = dom Cs1i1 /\ dom DPp by RELAT_1:90
      .= dom DPp by A16,A34,XBOOLE_1:10,28;
A39: dom DataPart Cs2i1 = Data-Locations SCM+FSA by COMPOS_1:50;
A40: now
      let x be set;
      assume that
A41:  x in dom (Cs3i1|(Data-Locations SCM+FSA)) and
A42:  Cs3i1.x = Cs2i1.x;
      thus (DataPart Cs3i1).x = Cs2i1.x by A41,A42,FUNCT_1:70
        .= (DataPart Cs2i1).x by A25,A39,A41,FUNCT_1:70;
    end;
A43: dom DataPart Cs3i = Data-Locations SCM+FSA by COMPOS_1:50;
A44: now
      let x be set;
      assume that
A45:  x in dom (DataPart Cs3i1) and
A46:  Cs3i1.x = Cs3i.x and
A47:  Cs2i1.x = Cs2i.x;
      (DataPart Cs3i).x = Cs3i.x by A43,A25,A45,FUNCT_1:70;
      hence
      (DataPart Cs3i1).x = (DataPart Cs2i1).x by A15,A24,A25,A40,A45,A46,A47,
FUNCT_1:70;
    end;
A48: Cs3i1 = Following(P1,Cs3i) by EXTPRO_1:4
      .= Exec (CurInstr(P1, Cs1i), Cs3i)
      by A2,A9,A10,AMISTD_5:7,A4;
A49: dom DataPart p = dom DataPart(Relocated(p, k)) by COMPOS_1:115;
    then
A50: dom (Cs2i1|dom DataPart(Relocated(p, k))) = dom Cs2i1 /\ dom DPp by
RELAT_1:90
      .= dom DPp by A18,A34,XBOOLE_1:10,28;
A51: now
      let x,d be set such that
A52:  d = x and
A53:  d in dom DPp and
A54:  Cs1i1.d = Cs2i1.d;
      thus (Cs1i1|dom DPp).x = Cs2i1.d by A38,A52,A53,A54,FUNCT_1:70
        .= (Cs2i1|dom DPp).x by A49,A50,A52,A53,FUNCT_1:70;
    end;
A55: dom (Cs1i|dom DPp) = dom Cs1i /\ dom DPp by RELAT_1:90
      .= dom DPp by A17,A34,XBOOLE_1:10,28;
    reconsider j = IC Cs1i as Element of NAT;
A56: Cs1i1 = Following(P1,Cs1i) by EXTPRO_1:4
      .= Exec (CurInstr(P1, Cs1i), Cs1i);
A57: dom (Cs2i|dom DataPart(Relocated(p, k))) = dom Cs2i /\ dom DPp by A49,
RELAT_1:90
      .= dom DPp by A19,A34,XBOOLE_1:10,28;
A58: now
      let x,d be set such that
A59:  d = x and
A60:  d in dom DPp and
A61:  Cs1i1.d = Cs1i.d and
A62:  Cs2i1.d = Cs2i.d;
A63:  (Cs1i|dom DPp).d = Cs1i.d by A55,A60,FUNCT_1:70;
      (Cs2i|dom DPp).d = Cs2i.d by A49,A57,A60,FUNCT_1:70;
      hence (Cs1i1|dom DPp).x = Cs2i1.d by A14,A49,A38,A59,A60,A61,A62,A63,
FUNCT_1:70
        .= (Cs2i1|dom DPp).x by A49,A50,A59,A60,FUNCT_1:70;
    end;
A64: succ (IC Cs1i + k) = (j+k+1) by NAT_1:39
      .= (j+1) + k
      .= ((succ IC Cs1i) qua Element of NAT) + k by NAT_1:39;
    per cases by A37,A36,A35,NAT_1:8,33;
    suppose
      InsCode I = 0;
      then
A65:  I = halt SCM+FSA by SCMFSA_2:122;
      thus IC Comput(P1,s1,i+1) + k = IC Cs1i + k by A56,A65,
EXTPRO_1:def 3
        .= IC Comput(P2,s2,i+1)
         by A12,A20,A65,A13,EXTPRO_1:def 3;
      hence
      IncAddr(CurInstr(P1,Comput(P1,s1,i+1)), k)
       = CurInstr(P2,Comput(P2,s2,i+1)) by A26;
A66:  Cs2i1 = Cs2i by A20,A65,A13,EXTPRO_1:def 3;
      hence Comput(P1,s1,i+1)|dom (DataPart p) = Comput(
P2,s2,i+1)|dom (
      DataPart (Relocated(p,k))) by A14,A56,A65,EXTPRO_1:def 3;
      thus thesis by A15,A48,A65,A66,EXTPRO_1:def 3;
    end;
    suppose
      InsCode I = 1;
      then consider da, db being Int-Location such that
A67:  I = da := db by SCMFSA_2:54;
A68:  IncAddr(I, k) = da := db by A67,COMPOS_1:92;
A69:  Exec(I, Cs1i).IC SCM+FSA = succ IC Cs1i by A67,SCMFSA_2:89;
      hence
      IC Comput(P1,s1,i+1) + k = IC Comput(P2,s2,i+1)
by A12,A13,A56,A20,A64,A68,SCMFSA_2:89;
      thus IncAddr(CurInstr(P1,
      Comput(P1,s1,i+1)), k)
       = CurInstr(P2,Comput(P2,s2,i+1))
       by A12,A13,A26,A56,A20,A64,A68,A69,SCMFSA_2:89;
A70:  Cs3i.db = Cs2i.db by A32;
      now
        let x be set;
A71:    dom DPp c= Data-Locations SCM+FSA by RELAT_1:87;
        assume
A72:    x in dom (Cs1i1|dom DPp);
        per cases by A38,A72,A71,XBOOLE_0:def 3,SCMFSA_2:127;
        suppose
          x in FinSeq-Locations;
          then reconsider d = x as FinSeq-Location by SCMFSA_2:12;
A73:      Cs1i1.d = Cs1i.d by A56,A67,SCMFSA_2:89;
          Cs2i1.d = Cs2i.d by A13,A20,A68,SCMFSA_2:89;
          hence (Cs1i1|dom DPp).x = (Cs2i1|dom DPp).x by A38,A58,A72,A73;
        end;
        suppose
A74:      da = x;
          DPp c= p by RELAT_1:88;
          then dom DPp c= dom p by GRFUNC_1:8;
          then
A75:      Cs3i.db = Cs1i.db by A2,A9,A10,A38,A67,A72,A74,SCMFSA_3:19,A4;
A76:      Cs1i1.x = Cs1i.db by A56,A67,A74,SCMFSA_2:89;
          Cs2i1.x = Cs2i.db by A13,A20,A68,A74,SCMFSA_2:89;
          hence (Cs1i1|dom DPp).x = (Cs2i1|dom DPp).x by A38,A51,A70,A72,A76
,A75;
        end;
        suppose
A77:      da <> x & x in Int-Locations;
          then reconsider d = x as Int-Location by SCMFSA_2:11;
A78:      Cs1i1.d = Cs1i.d by A56,A67,A77,SCMFSA_2:89;
          Cs2i1.d = Cs2i.d by A13,A20,A68,A77,SCMFSA_2:89;
          hence (Cs1i1|dom DPp).x = (Cs2i1|dom DPp).x by A38,A58,A72,A78;
        end;
      end;
      then (Cs1i1|dom DPp) c= (Cs2i1|dom DPp) by A49,A38,A50,GRFUNC_1:8;
      hence Comput(P1,s1,i+1)|dom (DataPart p) = Comput(
P2,s2,i+1)|dom (
      DataPart (Relocated(p,k))) by A49,A38,A50,GRFUNC_1:9;
      now
        let x be set;
        assume
A79:    x in dom (DataPart Cs3i1);
        per cases by A25,A79,XBOOLE_0:def 3,SCMFSA_2:127;
        suppose
          x in FinSeq-Locations;
          then reconsider f = x as FinSeq-Location by SCMFSA_2:12;
A80:      Cs3i1.f = Cs3i.f by A48,A67,SCMFSA_2:89;
          Cs2i1.f = Cs2i.f by A13,A20,A68,SCMFSA_2:89;
          hence (DataPart Cs3i1).x = (DataPart Cs2i1).x by A44,A79,A80;
        end;
        suppose
A81:      da = x;
A82:      Cs3i1.da=Cs3i.db by A48,A67,SCMFSA_2:89;
          Cs2i1.da = Cs2i.db by A13,A20,A68,SCMFSA_2:89;
          hence (DataPart Cs3i1).x = (DataPart Cs2i1).x by A32,A40,A79,A81,A82;
        end;
        suppose
A83:      da <> x & x in Int-Locations;
          then reconsider d = x as Int-Location by SCMFSA_2:11;
A84:      Cs3i1.d = Cs3i.d by A48,A67,A83,SCMFSA_2:89;
          Cs2i1.d = Cs2i.d by A13,A20,A68,A83,SCMFSA_2:89;
          hence (DataPart Cs3i1).x = (DataPart Cs2i1).x by A44,A79,A84;
        end;
      end;
      then DataPart Cs3i1 c= DataPart Comput(P2,s2,i+1) by A25,A39
,GRFUNC_1:8
;
      hence thesis by A25,A39,GRFUNC_1:9;
    end;
    suppose
      InsCode I = 2;
      then consider da, db being Int-Location such that
A85:  I = AddTo(da, db) by SCMFSA_2:55;
A86:  Cs3i.db = Cs2i.db by A32;
A87:  IncAddr(I, k) = AddTo(da, db) by A85,COMPOS_1:92;
A88:  Exec(I, Cs1i).IC SCM+FSA = succ IC Cs1i by A85,SCMFSA_2:90;
      hence
      IC Comput(P1,s1,i+1) + k = IC Comput(P2,s2,i+1)
by A12,A13,A56,A20,A64,A87,SCMFSA_2:90;
      thus IncAddr(CurInstr(P1,
      Comput(P1,s1,i+1)), k)
       = CurInstr(P2,Comput(P2,s2,i+1))
       by A12,A13,A26,A56,A20,A64,A87,A88,SCMFSA_2:90;
A89:  Cs3i.da = Cs2i.da by A32;
      now
A90:    dom DPp c= Data-Locations SCM+FSA by RELAT_1:87;
        let x be set such that
A91:    x in dom (Cs1i1|dom DPp);
        per cases by A38,A91,A90,XBOOLE_0:def 3,SCMFSA_2:127;
        suppose
          x in FinSeq-Locations;
          then reconsider d = x as FinSeq-Location by SCMFSA_2:12;
A92:      Cs1i1.d=Cs1i.d by A56,A85,SCMFSA_2:90;
          Cs2i1.d = Cs2i.d by A13,A20,A87,SCMFSA_2:90;
          hence (Cs1i1|dom DPp).x = (Cs2i1|dom DPp).x by A38,A58,A91,A92;
        end;
        suppose
A93:      da = x;
          DPp c= p by RELAT_1:88;
          then
A94:      dom DPp c= dom p by GRFUNC_1:8;
A95:      Cs1i1.x = Cs1i.da + Cs1i.db by A56,A85,A93,SCMFSA_2:90;
          Cs2i1.x = Cs2i.da + Cs2i.db by A13,A20,A87,A93,SCMFSA_2:90;
          then Cs1i1.x = Cs2i1.x by A2,A9,A10,A38,A85,A89,A86,A91,A93,A95,A94,
SCMFSA_3:20,A4;
          hence (Cs1i1|dom DPp).x = (Cs2i1|dom DPp).x by A38,A51,A91;
        end;
        suppose
A96:      da <> x & x in Int-Locations;
          then reconsider d = x as Int-Location by SCMFSA_2:11;
A97:      Cs1i1.d=Cs1i.d by A56,A85,A96,SCMFSA_2:90;
          Cs2i1.d = Cs2i.d by A13,A20,A87,A96,SCMFSA_2:90;
          hence (Cs1i1|dom DPp).x = (Cs2i1|dom DPp).x by A38,A58,A91,A97;
        end;
      end;
      then (Cs1i1|dom DPp) c= (Cs2i1|dom DPp) by A49,A38,A50,GRFUNC_1:8;
      hence Comput(P1,s1,i+1)|dom (DataPart p) = Comput(
P2,s2,i+1)|dom (
      DataPart (Relocated(p,k))) by A49,A38,A50,GRFUNC_1:9;
      now
        let x be set;
        assume
A98:    x in dom (DataPart Cs3i1);
        per cases by A25,A98,XBOOLE_0:def 3,SCMFSA_2:127;
        suppose
          x in FinSeq-Locations;
          then reconsider d = x as FinSeq-Location by SCMFSA_2:12;
A99:      Cs3i1.d = Cs3i.d by A48,A85,SCMFSA_2:90;
          Cs2i1.d = Cs2i.d by A13,A20,A87,SCMFSA_2:90;
          hence (DataPart Cs3i1).x = (DataPart Cs2i1).x by A44,A98,A99;
        end;
        suppose
A100:      da = x;
          then
A101:      Cs3i1.x = Cs3i.da + Cs3i.db by A48,A85,SCMFSA_2:90;
          Cs2i1.x = Cs2i.da + Cs2i.db by A13,A20,A87,A100,SCMFSA_2:90;
          hence (DataPart Cs3i1).x = (DataPart Cs2i1).x by A40,A89,A86,A98,A101
;
        end;
        suppose
A102:      da <> x & x in Int-Locations;
          then reconsider d = x as Int-Location by SCMFSA_2:11;
A103:      Cs3i1.d = Cs3i.d by A48,A85,A102,SCMFSA_2:90;
          Cs2i1.d = Cs2i.d by A13,A20,A87,A102,SCMFSA_2:90;
          hence (DataPart Cs3i1).x = (DataPart Cs2i1).x by A44,A98,A103;
        end;
      end;
      then DataPart Cs3i1 c= DataPart Comput(P2,s2,i+1) by A25,A39
,GRFUNC_1:8
;
      hence thesis by A25,A39,GRFUNC_1:9;
    end;
    suppose
      InsCode I = 3;
