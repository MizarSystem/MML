    y = xx2 by A9,A7;
    hence thesis by A9,A8;
  end;
  func x`3 -> Element of X3 means
  :Def7:
  x = [x1,x2,x3] implies it = x3;
  existence
  proof
    consider xx1,xx2,xx3 such that
A10: x = [xx1,xx2,xx3] by A1,Lm2;
