                      .= [:{[x1,x2],[x1,y2]},{x3}:] by ZFMISC_1:36
                      .= {[[x1,x2],x3],[[x1,y2],x3]} by ZFMISC_1:36
                      .= { [x1,x2,x3],[[x1,y2],x3] } by Def3
