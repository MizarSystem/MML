 registrations XBOOLE_0, SUBSET_1, FUNCT_1, RELSET_1, FINSET_1, NUMBERS,
      STRUCT_0, RLVECT_1, RLSUB_1, ORDINAL1, REAL_1, XREAL_0, RLVECT_2;
 requirements NUMERALS, BOOLE, SUBSET, ARITHM;
