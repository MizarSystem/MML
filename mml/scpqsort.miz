      end;
         t1.intpos n1=t.intpos n1 & t1.intpos n2=t.intpos n2 or
        n1 >= n0+1 & n2 >= n0+1 & n1 <= n0+n & n2 <= n0+n &
        t1.intpos n1=t.intpos n2 & t1.intpos n2=t.intpos n1
        proof
          per cases by A44;
          suppose t1.intpos n1=t.intpos n1 & t1.intpos n2=t.intpos n2;
            hence thesis;
          end;
          suppose A60:t.a3 <= n1 & n2 <= t.a4 &
              t1.intpos n1=t.intpos n2 & t1.intpos n2=t.intpos n1;
          then A61: n1 >= n0+1 by A34,AXIOMS:22;
               n2 <= n0+n by A34,A60,AXIOMS:22;
           hence thesis by A47,A60,A61,AXIOMS:22;
      end;
      end;
 then A62:  g,f3 are_fiberwise_equipotent by A35,A37,A49,A50,A58,Lm24;
             now
          let i be Nat;
          assume i in Seg n;
           then A63: 1 <= i & i <= n by FINSEQ_1:3;
        hence f4.i=t2.intpos (n0+i) by A51,A57
            .=g1.i by A36,A37,A63,SCPISORT:def 1;
      end;
      then f4=g1 by A37,A51,FINSEQ_2:10;
    hence g,g1 are_fiberwise_equipotent by A56,A62,RFINSEQ:2;
            n1 < t1.a3 by A44,INT_1:26;
   then md < t1.a3 by A53,AXIOMS:22;
   then A64: t2.intpos md = t1.intpos md by A1,A33,A43,A44,A46,A48,A49,A50,A51
,A52,A53,A54,A55,A57;
   A65: t1.a3=n1+1 by A44;
      hereby
          let i be Nat;
          assume A66: t.a3<=i & i <= t2.a4;
          then A67: n0+1 <= i by A34,AXIOMS:22;
          hereby
            per cases;
            suppose A68: i < t1.a3;
            then A69: i <= n1 by A65,INT_1:20;
                 t2.intpos i = t1.intpos i by A33,A43,A44,A46,A48,A49,A50,A51
,A52,A53,A54,A55,A57,A67,A68;
             hence t2.intpos md >= t2.intpos i by A44,A64,A66,A69;
            end;
            suppose i >= t1.a3;
            hence t2.intpos md >= t2.intpos i by A33,A43,A44,A46,A48,A49,A50
,A51,A52,A53,A54,A55,A57,A66;
        end;
        end;
      end;
      hereby
          let i be Nat;
          assume A70: t2.a4 < i & i <= t.a4;
          hereby
            per cases;
            suppose A71: i > t1.a4;
            then A72: i >= n2 by A44,INT_1:20;
                     t1.a4 > n1+0 by A43,A44,A48,REAL_1:53
;
                then n2 > n1+1 by A44,REAL_1:53;
                then n2 >= n0+1 by A44,A54,AXIOMS:22;
            then i >= n0+1 by A72,AXIOMS:22;
            then t2.intpos i = t1.intpos i by A33,A43,A44,A46,A48,A49,A50,A51
,A52,A53,A54,A55,A57,A71;
             hence t2.intpos md <= t2.intpos i by A44,A64,A70,A72;
            end;
            suppose i <= t1.a4;
            hence t2.intpos md <= t2.intpos i by A33,A43,A44,A46,A48,A49,A50
,A51,A52,A53,A54,A55,A57,A70;
        end;
        end;
      end;
      hereby
          let i be Nat;
          assume A73: i >= n0+1 & (i < t.a3 or i > t.a4);
                n0+1 >= 7+1 by A4,AXIOMS:24;
          then i >= 8 by A73,AXIOMS:22;
      then A74: t1.intpos i = t.intpos i by A44,A47,A73,Lm25;
          per cases by A73;
          suppose i < t.a3;
      then i < t1.a3 by A43,AXIOMS:22;
           hence t2.intpos i =t.intpos i by A33,A43,A44,A46,A48,A49,A50,A51,A52
,A53,A54,A55,A57,A73,A74;
