           hence t2.intpos i =t.intpos i by A33,A43,A44,A46,A48,A49,A50,A51,A52
,A53,A54,A55,A57,A73,A74;
     end;
     end;

     end;
     suppose A75: t1.a5 <= 0;
A76:   now let x;
        thus t2.x =IExec(WH3,t1).x by A34,A38,A39,Lm17,Th7
