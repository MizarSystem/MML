;
        thus t2.a4 <= t.a4 by A43,A56,A57,AXIOMS:22;
   A58:  now let i be Nat;
         assume A59: i >= n0+1 & i<>n1 & i<>n2;
                n0+1 >= 7+1 by A4,AXIOMS:24;
       then i >= 8 by A59,AXIOMS:22;
