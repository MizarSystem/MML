    .=2+(card TR +1 +2) by SCMPDS_5:6
    .=2+(card (l3 ';' l4 ';' Pt)+ card L5+1+2) by AFINSQ_1:20
    .=2+(card (l3 ';' l4) + card Pt + card L5+1+2) by AFINSQ_1:20
