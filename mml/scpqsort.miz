    thus s3.a4=s3.a3-1+s3.a5 by A4,A25,A34,A35,A36;
    take n1=m4;
    take n2=m4+1;
A37: mE=0 by A25;
    hence n1=s3.a3-1 by A4,A34;
    thus n2=s3.a4+1 by A35;
A38: now
      let x;
      thus s3.x=IExec(IF3,s12).x by A33,A32,SCPISORT:7
