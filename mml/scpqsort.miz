      hence t2.a=0 by A43;
      thus t2.a1=t.a1 by A43,A76;
      thus t2.a2=md by A43,A76;
             t2.a4 >= n3 by A43,A76;
      hence t2.a4 >= md by A34,AXIOMS:22;
