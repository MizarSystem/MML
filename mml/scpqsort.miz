 for s be State of SCMPDS,md,m3 be Nat st s.GBP=0 & s.a5 > 0 &
 s.a4=m3+s.a5 & m3=s.a3-1 & s.a2=md & md >= 8 & md <= m3
 holds WB3 is_closed_on s & WB3 is_halting_on s &
  IExec(WB3,s).GBP=0 & IExec(WB3,s).a1=s.a1 &
  IExec(WB3,s).a2=md & IExec(WB3,s).a3 >= s.a3 &
  IExec(WB3,s).a4 <= s.a4 & IExec(WB3,s).a4 >= m3 &
  IExec(WB3,s).a5 < s.a5 & IExec(WB3,s).a5 >= -1 &
  IExec(WB3,s).a4=IExec(WB3,s).a3-1+IExec(WB3,s).a5 &
  ex n1,n2 be Nat st n1=IExec(WB3,s).a3-1 & n2=IExec(WB3,s).a4+1 &
   (for i be Nat st i >= 8 & i <>n1 & i<> n2 holds
       IExec(WB3,s).intpos i=s.intpos i) &
  (IExec(WB3,s).intpos n1=s.intpos n1 & IExec(WB3,s).intpos n2=s.intpos n2 or
  n1 >= s.a3 & n2 <= s.a4 &
  IExec(WB3,s).intpos n1=s.intpos n2 & IExec(WB3,s).intpos n2=s.intpos n1) &
  (for i be Nat st s.a3<=i & i <= n1 holds
   IExec(WB3,s).intpos md >= IExec(WB3,s).intpos i) &
   (for i be Nat st n2<=i & i <= s.a4 holds
   IExec(WB3,s).intpos md <= IExec(WB3,s).intpos i)
proof
   let s be State of SCMPDS,md,m3 be Nat;
   set a=GBP;
   assume A1: s.a=0 & s.a5 > 0 & s.a4=m3+s.a5 & m3=s.a3-1 &
       s.a2=md & md >= 8 & md <= m3;
     set s1=IExec(WH1,s);
A2:  m3 >= 8 by A1,AXIOMS:22;
then A3:  s1.a=0 & s1.a1=s.a1 & s1.a5=0 & s1.a2=s.a2 & s1.a3=s.a3 &
     (for i be Nat st i >= 8 holds s1.intpos i=s.intpos i) by A1,Lm9;
     consider mE be Nat such that
A4:  mE=s1.a7 & s1.a4=m3+mE & mE <= s.a5 &
     (for i be Nat st m3+mE < i & i <=s.a4 holds s1.intpos md < s1.intpos i)
     & (mE = 0 or s1.intpos md >= s1.intpos(m3+mE)) by A1,A2,Lm9;
A5:  WH1 is_closed_on s & WH1 is_halting_on s by A1,A2,Lm10;
     set s2=IExec(WH2,s1),
         sm=m3+1+mE,
         s12=IExec(WH1 ';' WH2,s),
         s3=IExec(WB3,s),
         m4=m3+mE;
  s1.a3=m3+1 by A1,A2,Lm9;
     then m3 < s1.a3 by REAL_1:69;
then A6:  s1.a3 >= 8 by A2,AXIOMS:22;
A7:  s1.a3+s1.a7=sm by A1,A2,A4,Lm9;
     A8: m3+mE <= m3+s.a5 by A4,AXIOMS:24;
A9:  m4 >= 8 by A2,NAT_1:37;

     per cases;
     suppose A10: mE <= 0;
       A11: DataLoc(s1.a,7)=intpos(0+7) by A3,SCMP_GCD:5;
then A12:    WH2 is_closed_on s1 & WH2 is_halting_on s1 by A4,A10,SCMPDS_8:20;
then A13:    WH1 ';' WH2 is_closed_on s & WH1 ';' WH2 is_halting_on s
         by A5,SCMPDS_7:43;
    hence WB3 is_closed_on s & WB3 is_halting_on s by SCPISORT:9;
       A14: mE >= 0 by NAT_1:18;
A15:    mE=0 by A10,NAT_1:18;
A16:    s12.a=s2.a by A5,A12,SCMPDS_7:49
       .=0 by A3,A4,A10,A11,SCMPDS_8:23;
A17:    s12.a5=s2.a5 by A5,A12,SCMPDS_7:49
       .=0 by A3,A4,A10,A11,SCMPDS_8:23;
       A18: DataLoc(s12.a,5)=intpos(0+5) by A16,SCMP_GCD:5;
     thus IExec(WB3,s).a=IExec(IF3,s12).a by A13,SCPISORT:7
             .=0 by A16,A17,A18,SCMPDS_6:98;
     thus IExec(WB3,s).a1=IExec(IF3,s12).a1 by A13,SCPISORT:7
             .= s12.a1 by A17,A18,SCMPDS_6:98
             .= s2.a1 by A5,A12,SCMPDS_7:49
             .= s.a1 by A3,A4,A10,A11,SCMPDS_8:23;
     thus s3.a2=IExec(IF3,s12).a2 by A13,SCPISORT:7
             .= s12.a2 by A17,A18,SCMPDS_6:98
             .= s2.a2 by A5,A12,SCMPDS_7:49
             .= md by A1,A3,A4,A10,A11,SCMPDS_8:23;
     A19:   s3.a3=IExec(IF3,s12).a3 by A13,SCPISORT:7
             .= s12.a3 by A17,A18,SCMPDS_6:98
             .= s2.a3 by A5,A12,SCMPDS_7:49
             .= s.a3 by A3,A4,A10,A11,SCMPDS_8:23;
         hence s3.a3 >= s.a3;
     A20:   s3.a4=IExec(IF3,s12).a4 by A13,SCPISORT:7
             .= s12.a4 by A17,A18,SCMPDS_6:98
             .= s2.a4 by A5,A12,SCMPDS_7:49
             .= m4 by A4,A10,A11,SCMPDS_8:23;
         hence s3.a4 <= s.a4 by A1,A4,AXIOMS:24;
         thus s3.a4 >= m3 by A20,NAT_1:37;
     A21:   s3.a5=IExec(IF3,s12).a5 by A13,SCPISORT:7
             .=0 by A17,A18,SCMPDS_6:98;
         hence s3.a5 < s.a5 by A1;
         thus s3.a5 >= -1 by A21;
     thus s3.a4=s3.a3-1+s3.a5 by A1,A10,A14,A19,A20,A21,AXIOMS:21;
          take n1=m4;
          take n2=m4+1;
       thus n1=s3.a3-1 by A1,A15,A19;
       thus n2=s3.a4+1 by A20;
     A22: now
            let x;
            thus s3.x=IExec(IF3,s12).x by A13,SCPISORT:7
              .=s12.x by A17,A18,SCMPDS_6:98
              .=s2.x by A5,A12,SCMPDS_7:49;
         end;
     A23: now
            let i be Nat;
            assume A24: i >= 8;
            thus s3.intpos i=s2.intpos i by A22
              .=s1.intpos i by A4,A10,A11,SCMPDS_8:23
              .=s.intpos i by A1,A2,A24,Lm9;
         end;
         hence for i be Nat st i >= 8 & i <>n1 & i<> n2 holds
             s3.intpos i=s.intpos i;
      A25: m4 < n2 by REAL_1:69;
          then 8 < n2 by A9,AXIOMS:22;
     hence s3.intpos n1=s.intpos n1 & s3.intpos n2=s.intpos n2 or
        n1 >= s.a3 & n2 <= s.a4 &
        s3.intpos n1=s.intpos n2 & s3.intpos n2=s.intpos n1 by A9,A23;
     hereby
         let i be Nat;
         assume A26: s.a3<=i & i <= n1;
         assume not s3.intpos md >= s3.intpos i;
                n1 < s.a3 by A1,A15,INT_1:26;
          hence contradiction by A26,AXIOMS:22;
     end;
     hereby
         let i be Nat;
         assume A27: n2<=i & i <= s.a4;
         then A28: m4 < i by A25,AXIOMS:22;
        A29: s3.intpos md=s2.intpos md by A22
            .=s1.intpos md by A4,A10,A11,SCMPDS_8:23;
                 s3.intpos i=s2.intpos i by A22
            .=s1.intpos i by A4,A10,A11,SCMPDS_8:23;
         hence s3.intpos md <= s3.intpos i by A4,A27,A28,A29;
     end;

     end;
     suppose A30: mE > 0;
     then A31: m4 > m3+0 by REAL_1:53;
then A32:  m4 >= m3+1 by INT_1:20;
A33: s2.a=0 & s2.a1=s1.a1 & s2.a7=0 & s2.a2=s1.a2 & s2.a4=s1.a4 &
    (for i be Nat st i >= 8 holds s2.intpos i=s1.intpos i)
      by A1,A3,A4,A6,A7,A30,Lm15;
     consider m5,mE3 be Nat such that
A34:  m5=s2.a5 & s2.a3=mE3 & mE3+m5=sm & m5 <= s1.a7 &
     (for i be Nat st s1.a3 <= i & i < mE3 holds s2.intpos md > s2.intpos i)
     & (m5 = 0 or s2.intpos md <= s2.intpos mE3)
       by A1,A3,A4,A6,A30,Lm15;
A35:  WH2 is_closed_on s1 & WH2 is_halting_on s1 by A1,A3,A4,A6,A30,Lm16;
then A36:  WH1 ';' WH2 is_closed_on s & WH1 ';' WH2 is_halting_on s
      by A5,SCMPDS_7:43;
    hence WB3 is_closed_on s & WB3 is_halting_on s by SCPISORT:9;

A37:  s12.a=0 by A5,A33,A35,SCMPDS_7:49;
      A38: mE3+m5 <= mE3+s1.a7 by A34,AXIOMS:24;
     A39: s1.a3+s1.a7 <= mE3+s1.a7 by A1,A3,A4,A34,AXIOMS:24;
A40:  s1.a3 <= mE3 by A1,A3,A4,A34,A38,REAL_1:53;
A41:  s.a3 <= mE3 by A3,A39,REAL_1:53;
A42:  s12.a3=mE3 by A5,A34,A35,SCMPDS_7:49;
A43:  mE3 >= 8 by A3,A6,A41,AXIOMS:22;
then A44:  mE3 > 6 by AXIOMS:22;
A45:  s12.a4=m4 by A4,A5,A33,A35,SCMPDS_7:49;
A46:  m4 > 6 by A9,AXIOMS:22;
A47:  s12.a2=md by A1,A3,A5,A33,A35,SCMPDS_7:49;
A48:  s12.a1=s.a1 by A3,A5,A33,A35,SCMPDS_7:49;
A49:  s12.a5=m5 by A5,A34,A35,SCMPDS_7:49;
     hereby
        per cases;
        suppose A50: s12.a5 > 0;
then A51:      IExec(IF3,s12).a=0 & IExec(IF3,s12).a1=s12.a1 &
         IExec(IF3,s12).a2=s12.a2 &
         IExec(IF3,s12).intpos mE3=s12.intpos m4 &
         IExec(IF3,s12).intpos m4=s12.intpos mE3 &
         IExec(IF3,s12).a3=s12.a3+1 & IExec(IF3,s12).a4=s12.a4-1 &
         IExec(IF3,s12).a5=s12.a5-2 &
          for i be Nat st i >= 8 & i <> mE3 & i <> m4 holds
          IExec(IF3,s12).intpos i=s12.intpos i by A37,A42,A44,A45,A46,Lm19;
         hence IExec(WB3,s).a=0 by A36,SCPISORT:7;
         thus IExec(WB3,s).a1=s.a1 by A36,A48,A51,SCPISORT:7;
         thus s3.a2=md by A36,A47,A51,SCPISORT:7;
   A52:    s3.a3=mE3+1 by A36,A42,A51,SCPISORT:7;
             then mE3 < s3.a3 by REAL_1:69;
         hence s3.a3 >= s.a3 by A3,A40,AXIOMS:22;
   A53:    s3.a4=m4-1 by A36,A45,A51,SCPISORT:7;
             then s3.a4 < m4 by INT_1:26;
         hence s3.a4 <= s.a4 by A1,A8,AXIOMS:22;
         thus s3.a4 >= m3 by A32,A53,REAL_1:84;
  A54:      s3.a5 =s12.a5-2 by A36,A51,SCPISORT:7;
            m5 <= s.a5 by A4,A34,AXIOMS:22;
           then m5-2 < s.a5-0 by REAL_1:92;
         hence s3.a5 < s.a5 by A5,A34,A35,A54,SCMPDS_7:49;
               s12.a5 >=0+1 by A50,INT_1:20;
          then A55: s12.a5-2 >= 1-2 by REAL_1:49;
       hence
           s3.a5 >= -1 by A36,A51,SCPISORT:7;
     A56:  s3.a4=m3+mE+1-(1+1) by A36,A45,A51,SCPISORT:7
            .=mE3+s3.a5 by A34,A49,A54,XCMPLX_1:29;
      hence s3.a4=s3.a3-1+s3.a5 by A52;

          take n1=mE3;
          take n2=m4;
          thus n1=s3.a3-1 by A52;
          thus
     A57:   n2=m4+-1+1
             .=s3.a4+1 by A36,A45,A51,SCPISORT:7;
     A58:   now
            let i be Nat;
            assume A59: i >= 8;
            thus s12.intpos i=s2.intpos i by A5,A35,SCMPDS_7:49
              .=s1.intpos i by A1,A3,A4,A6,A7,A30,A59,Lm15
              .=s.intpos i by A1,A2,A59,Lm9;
          end;
     A60:  now
            let i be Nat;
            assume A61: i >= 8 & i <>n1 & i<> n2;
            thus s3.intpos i=IExec(IF3,s12).intpos i by A36,SCPISORT:7
              .=s12.intpos i by A37,A42,A44,A45,A46,A50,A61,Lm19
              .=s.intpos i by A58,A61;
          end;
      hence for i be Nat st i >= 8 & i <>n1 & i<> n2 holds
           s3.intpos i=s.intpos i;
      A62: s3.intpos n1=s12.intpos n2 by A36,A51,SCPISORT:7
          .=s.intpos n2 by A9,A58;
      A63: s3.intpos n2=s12.intpos n1 by A36,A51,SCPISORT:7
          .=s.intpos n1 by A43,A58;
       hence s3.intpos n1=s.intpos n1 & s3.intpos n2=s.intpos n2 or
          s.a3 <= n1 & n2 <= s.a4 &
          s3.intpos n1=s.intpos n2 & s3.intpos n2=s.intpos n1 by A1,A3,A4,A39
,A62,AXIOMS:24,REAL_1:53;
               m3 < m3+1 by REAL_1:69;
          then m3 < n1 by A1,A3,A40,AXIOMS:22;
      then A64: md < n1 by A1,AXIOMS:22;
      A65: n2=n1+(s3.a5+1) by A56,A57,XCMPLX_1:1;
               s3.a5+1 >= -1+1 by A54,A55,AXIOMS:24;
      then A66: n2 >= n1+0 by A65,AXIOMS:24;
      then md < n2 by A64,AXIOMS:22;
      then A67: n2 >= 8 by A1,AXIOMS:22;
      A68: n1 >= 8 by A1,A64,AXIOMS:22;
      A69: s3.intpos md=s.intpos md by A1,A60,A64,A66;
      A70: s2.intpos md=s1.intpos md by A1,A3,A4,A6,A7,A30,Lm15
          .=s3.intpos md by A1,A2,A69,Lm9;
      A71: s1.intpos md=s3.intpos md by A1,A2,A69,Lm9;
      hereby
             let i be Nat;
             assume A72: s.a3<=i & i <= n1;
          then A73: s1.a3 <= i by A1,A2,Lm9;
          A74: i >= 8 by A3,A6,A72,AXIOMS:22;
           per cases;
           suppose A75: i < n1;
                      s2.intpos i=s1.intpos i by A1,A3,A4,A6,A7,A30,A74,Lm15
                 .=s.intpos i by A1,A2,A74,Lm9
                 .=s3.intpos i by A60,A66,A74,A75;
             hence s3.intpos md >= s3.intpos i by A34,A70,A73,A75;
          end;
          suppose i >= n1;
             then i=n1 by A72,AXIOMS:21;
              hence s3.intpos md >= s3.intpos i by A1,A2,A4,A30,A62,A67,A71,Lm9
;
       end;
       end;

       hereby let i be Nat;
            assume A76: n2<=i & i <= s.a4;
          then A77: i >= 8 by A67,AXIOMS:22;
          per cases;
           suppose A78: n2 < i;
                      s1.intpos i=s.intpos i by A1,A2,A77,Lm9
                 .=s3.intpos i by A60,A66,A77,A78;
             hence s3.intpos md <= s3.intpos i by A4,A71,A76,A78;
          end;
          suppose n2 >= i;
             then i=n2 by A76,AXIOMS:21;
                then s3.intpos i=s1.intpos n1 by A1,A2,A63,A68,Lm9
                  .=s2.intpos n1 by A1,A3,A4,A6,A7,A30,A68,Lm15;
              hence s3.intpos md <= s3.intpos i by A5,A34,A35,A50,A70,
SCMPDS_7:49;
       end;
       end;

       end;
       suppose A79: s12.a5 <= 0;
     A80:  DataLoc(s12.a,5)=intpos(0+5) by A37,SCMP_GCD:5;
         thus IExec(WB3,s).a=IExec(IF3,s12).a by A36,SCPISORT:7
             .=0 by A37,A79,A80,SCMPDS_6:98;
         thus IExec(WB3,s).a1=IExec(IF3,s12).a1 by A36,SCPISORT:7
             .= s.a1 by A48,A79,A80,SCMPDS_6:98;
         thus s3.a2=IExec(IF3,s12).a2 by A36,SCPISORT:7
             .= md by A47,A79,A80,SCMPDS_6:98;
     A81:   s3.a3=IExec(IF3,s12).a3 by A36,SCPISORT:7
             .= mE3 by A42,A79,A80,SCMPDS_6:98;
         hence s3.a3 >= s.a3 by A1,A4,A34,A38,REAL_1:53;
     A82:   s3.a4=IExec(IF3,s12).a4 by A36,SCPISORT:7
             .= m4 by A45,A79,A80,SCMPDS_6:98;
         hence s3.a4 <= s.a4 by A1,A4,AXIOMS:24;
         thus s3.a4 >= m3 by A31,A82;
        A83:  s12.a5=0 by A49,A79,NAT_1:18;
        A84:  s3.a5=IExec(IF3,s12).a5 by A36,SCPISORT:7
             .=0 by A80,A83,SCMPDS_6:98;
         hence s3.a5 < s.a5 by A1;
         thus s3.a5 >= -1 by A84;
     A85:  m4=m3+mE+1-1
          .=mE3+0-1 by A5,A34,A35,A83,SCMPDS_7:49;
        hence s3.a4=s3.a3-1+s3.a5 by A81,A82,A84;

          take n1=m4;
          take n2=m4+1;
        thus n1=s3.a3-1 by A81,A85;
        thus n2=s3.a4+1 by A82;
     A86: now
            let x;
            thus s3.x=IExec(IF3,s12).x by A36,SCPISORT:7
              .=s12.x by A79,A80,SCMPDS_6:98
              .=s2.x by A5,A35,SCMPDS_7:49;
         end;
     A87: now
            let i be Nat;
            assume A88: i >= 8;
            thus s3.intpos i=s2.intpos i by A86
              .=s1.intpos i by A1,A3,A4,A6,A7,A30,A88,Lm15
              .=s.intpos i by A1,A2,A88,Lm9;
         end;
         hence for i be Nat st i >= 8 & i <>n1 & i<> n2 holds
             s3.intpos i=s.intpos i;
      A89: m4 < n2 by REAL_1:69;
          then 8 < n2 by A9,AXIOMS:22;
     hence s3.intpos n1=s.intpos n1 & s3.intpos n2=s.intpos n2 or
        s.a3 <= n1 & n2 <= s.a4 &
        s3.intpos n1=s.intpos n2 & s3.intpos n2=s.intpos n1 by A9,A87;
     hereby
         let i be Nat;
         assume A90: s.a3<=i & i <= n1;
                   mE3-1 < mE3 by INT_1:26;
              then i < mE3 by A85,A90,AXIOMS:22;
          then A91: s2.intpos md > s2.intpos i by A3,A34,A90;
               s3.intpos md=s2.intpos md by A86;
          hence s3.intpos md >= s3.intpos i by A86,A91;
     end;
     hereby
         let i be Nat;
         assume A92: n2<=i & i <= s.a4;
        then A93: m4 < i by A89,AXIOMS:22;
        A94: s3.intpos md=s2.intpos md by A86
            .=s1.intpos md by A1,A3,A4,A6,A7,A30,Lm15;
        A95: i >= 8 by A9,A93,AXIOMS:22;
                 s3.intpos i=s2.intpos i by A86
            .=s1.intpos i by A1,A3,A4,A6,A7,A30,A95,Lm15;
         hence s3.intpos md <= s3.intpos i by A4,A92,A93,A94;
     end;
   end;
   end;
end;
end;

Lm21:
 for i be Integer st i >= -1 & i <= 0 holds i=-1 or i=0
proof
   let i be Integer;
   assume A1: i >= -1 & i <= 0;
   per cases;
   suppose i <= -1;
    hence thesis by A1,AXIOMS:21;
   end;
   suppose i > -1;
    then i >= -1+1 by INT_1:20;
    hence thesis by A1;
end;
end;

Lm22:
  for i1,i2 be Integer,n1,n2,i be Nat st i1 >= -1 & i1 <= 0 & n2=i2+1 &
  i2=n1+i1 & i < n2 holds i <= n1
proof
   let i1,i2 be Integer,n1,n2,i be Nat;
    assume A1: i1 >= -1 & i1 <= 0 & n2=i2+1 & i2=n1+i1 & i < n2;
    per cases by A1,Lm21;
    suppose i1=0;
     hence i <= n1 by A1,NAT_1:38;
    end;
    suppose i1=-1;
     hence i <= n1 by A1;
end;
end;

Lm23:
  for i1,i2 be Integer,n1,n2 be Nat st i1 >= -1 & n2=i2+1 & i2=n1+i1
  holds n1 <= n2
proof
    let i1,i2 be Integer,n1,n2 be Nat;
    assume A1: i1 >= -1 & n2=i2+1 & i2=n1+i1;
    then n1+i1 >= n1+ -1 by AXIOMS:24;
    then i2+1 >= n1+ -1+1 by A1,AXIOMS:24;
   hence n1 <= n2 by A1;
end;

Lm24:
 for s,s1 be State of SCMPDS,n0,n1,n2,n be Nat,f,f1 be FinSequence of INT st
 f is_FinSequence_on s,n0 & f1 is_FinSequence_on s1,n0 & len f=n &
 len f1 = n &
  (for i be Nat st i >= n0+1 & i<>n1 & i<>n2 holds s1.intpos i=s.intpos i) &
  (s1.intpos n1=s.intpos n1 & s1.intpos n2=s.intpos n2 or
  n1 >= n0+1 & n2 >= n0+1 & n1 <= n0+n & n2 <= n0+n &
  s1.intpos n1=s.intpos n2 & s1.intpos n2=s.intpos n1)
  holds f,f1 are_fiberwise_equipotent
proof
   let s,s1 be State of SCMPDS,n0,n1,n2,n be Nat,
   f,f1 be FinSequence of INT;
   assume A1: f is_FinSequence_on s,n0 & f1 is_FinSequence_on s1,n0;
   assume A2: len f=n & len f1 = n;
   assume A3: for i be Nat st i >= n0+1 & i<>n1 & i<>n2 holds
              s1.intpos i=s.intpos i;
   assume A4: s1.intpos n1=s.intpos n1 & s1.intpos n2=s.intpos n2 or
              n1 >= n0+1 & n2 >= n0+1 & n1 <= n0+n & n2 <= n0+n &
              s1.intpos n1=s.intpos n2 & s1.intpos n2=s.intpos n1;
    per cases by A4;
    suppose A5:s1.intpos n1=s.intpos n1 & s1.intpos n2=s.intpos n2;
          now
        let i be Nat;
        assume i in Seg n;
     then A6: 1 <= i & i <= n by FINSEQ_1:3;
     then A7: n0+1 <= n0+i & n0+i <= n0+n by AXIOMS:24;
        per cases;
        suppose A8: n0+i<>n1 & n0+i<>n2;
            thus f1.i=s1.intpos (n0+i) by A1,A2,A6,SCPISORT:def 1
              .=s.intpos (n0+i) by A3,A7,A8
              .=f.i by A1,A2,A6,SCPISORT:def 1;
        end;
        suppose A9: not (n0+i<>n1 & n0+i<>n2);
           hereby
                per cases by A9;
                suppose n0+i=n1;
                hence f1.i=s.intpos (n0+i) by A1,A2,A5,A6,SCPISORT:def 1
                 .=f.i by A1,A2,A6,SCPISORT:def 1;
                end;
                suppose n0+i=n2;
                hence f1.i=s.intpos (n0+i) by A1,A2,A5,A6,SCPISORT:def 1
                 .=f.i by A1,A2,A6,SCPISORT:def 1;
           end;
           end;
      end;
      end;
      hence thesis by A2,FINSEQ_2:10;
    end;
    suppose A10: n1 >= n0+1 & n2 >= n0+1 & n1 <= n0+n & n2 <= n0+n &
      s1.intpos n1=s.intpos n2 & s1.intpos n2=s.intpos n1;
then A11:  n1-n0 >= 1 by REAL_1:84;
     then n1-n0 > 0;
     then reconsider m1=n1-n0 as Nat by INT_1:16;
A12:  n2-n0 >= 1 by A10,REAL_1:84;
     then n2-n0 > 0;
     then reconsider m2=n2-n0 as Nat by INT_1:16;
A13:  m1 <= len f by A2,A10,REAL_1:86;
A14:  m2 <= len f1 by A2,A10,REAL_1:86;
A15:  m2 <= len f by A2,A10,REAL_1:86;
A16:  n1=m1+n0;
A17:  n2=m2+n0;
A18:  f.m1=s1.intpos n2 by A1,A10,A11,A13,A16,SCPISORT:def 1
     .=f1.m2 by A1,A12,A14,A17,SCPISORT:def 1;
A19:  f.m2=s1.intpos n1 by A1,A10,A12,A15,A17,SCPISORT:def 1
     .=f1.m1 by A1,A2,A11,A13,A16,SCPISORT:def 1;
          now
        let k be Nat;
        assume A20: k <> m1 & k <> m2 & 1 <= k & k <= len f;
          then A21: k+n0 <> m1+n0 & k+n0 <> m2+n0 by XCMPLX_1:2;
    A22:   n0+1 <= n0+k by A20,AXIOMS:24;
     thus f.k=s.intpos(k+n0) by A1,A20,SCPISORT:def 1
       .=s1.intpos (k+n0) by A3,A21,A22
       .=f1.k by A1,A2,A20,SCPISORT:def 1;
     end;
     hence thesis by A2,A11,A12,A13,A15,A18,A19,SCPISORT:4;
end;
end;

Lm25:
   for s,s1 be State of SCMPDS,n0,n1,n2 be Nat,c1,c2 be Integer st
   (for i be Nat st i >= n0 & i <>n1 & i<> n2 holds s1.intpos i=s.intpos i) &
    n1 <= n2 & (s1.intpos n1=s.intpos n1 & s1.intpos n2=s.intpos n2 or
 c1 <= n1 & n2 <= c2 & s1.intpos n1=s.intpos n2 & s1.intpos n2=s.intpos n1)
holds
   for i be Nat st i >= n0 & (i < c1 or i > c2) holds s1.intpos i=s.intpos i
proof
    let s,s1 be State of SCMPDS,n0,n1,n2 be Nat,c1,c2 be Integer;
    assume
A1:  for i be Nat st i >= n0 & i<>n1 & i<>n2 holds s1.intpos i=s.intpos i;
    assume A2: n1 <= n2;
    assume A3: s1.intpos n1=s.intpos n1 & s1.intpos n2=s.intpos n2 or
     c1 <= n1 & n2 <= c2 & s1.intpos n1=s.intpos n2 & s1.intpos n2=s.intpos n1;
    per cases by A3;
    suppose A4: s1.intpos n1=s.intpos n1 & s1.intpos n2=s.intpos n2;
     hereby
          let i be Nat;
          assume A5: i >= n0 & (i < c1 or i > c2);
         per cases;
         suppose i<>n1 & i<>n2;
            hence s1.intpos i=s.intpos i by A1,A5;
         end;
         suppose A6: not (i<>n1 & i<>n2);
           hereby
              per cases by A6;
              suppose i=n1;
                hence s1.intpos i=s.intpos i by A4;
              end;
              suppose i=n2;
                hence s1.intpos i=s.intpos i by A4;
           end;
           end;
     end;
     end;
     end;
     suppose A7: c1 <= n1 & n2 <= c2 &
         s1.intpos n1=s.intpos n2 & s1.intpos n2=s.intpos n1;
     hereby
         let i be Nat;
         assume A8: i >= n0 & (i < c1 or i > c2);
         per cases by A8;
         suppose i < c1;
      then i < n1 by A7,AXIOMS:22;
        hence s1.intpos i=s.intpos i by A1,A2,A8;
         end;
         suppose i > c2;
      then i > n2 by A7,AXIOMS:22;
        hence s1.intpos i=s.intpos i by A1,A2,A8;
    end;
    end;
end;
end;

Lm26:
 for s be State of SCMPDS,md,m3,n0 be Nat,f,f1 be FinSequence of INT
 st s.GBP=0 & s.a5 > 0 & s.a4=m3+s.a5 & m3=s.a3-1 & s.a2=md &
 md >= n0+1 & md <= m3 & n0+1 <= s.a3 & s.a4 <= n0+n &
 f is_FinSequence_on s,n0 &
 f1 is_FinSequence_on IExec(WH3,s),n0 & n0 >= 7 & len f=n & len f1=n
holds IExec(WH3,s).GBP=0 & IExec(WH3,s).a1=s.a1 & IExec(WH3,s).a2=md &
   IExec(WH3,s).a4 >= md & IExec(WH3,s).a4 <= s.a4 &
   f,f1 are_fiberwise_equipotent &
   (for i be Nat st s.a3<=i & i <= IExec(WH3,s).a4 holds
   IExec(WH3,s).intpos md >= IExec(WH3,s).intpos i) &
   (for i be Nat st IExec(WH3,s).a4 < i & i <= s.a4 holds
   IExec(WH3,s).intpos md <= IExec(WH3,s).intpos i) &
   (for i be Nat st i >= n0+1 & (i < s.a3 or i > s.a4) holds
         IExec(WH3,s).intpos i = s.intpos i)
proof
    let s be State of SCMPDS,md,m3,n0 be Nat,f,f1 be FinSequence of INT;
    set a=GBP;
    assume A1: s.a=0 & s.a5 > 0 & s.a4=m3+s.a5 & m3=s.a3-1 &
         s.a2=md & md >= n0+1 & md <= m3 & n0+1 <= s.a3 & s.a4 <= n0+n;
    assume A2: f is_FinSequence_on s,n0;
    assume A3: f1 is_FinSequence_on IExec(WH3,s),n0;
    assume A4: n0 >= 7 & len f=n & len f1=n;
        then A5: n0+1 >= 7+1 by AXIOMS:24;
then A6: md >= 8 by A1,AXIOMS:22;
    defpred P[Nat] means
      for t be State of SCMPDS,n3 be Nat,g,g1 be FinSequence of INT st
     t.a=0 & t.a5 > 0 & t.a5<= $1+1 & t.a4=n3+t.a5 & n3=t.a3-1 &
     t.a2=md & md <= n3 & n0+1 <= t.a3 & t.a4 <= n0+n &
     g is_FinSequence_on t,n0 &
         g1 is_FinSequence_on IExec(WH3,t),n0 & len g=n & len g1=n
       holds IExec(WH3,t).a=0 & IExec(WH3,t).a1=t.a1 &
       IExec(WH3,t).a2=md & IExec(WH3,t).a4 >= md &
       IExec(WH3,t).a4 <= t.a4 & g,g1 are_fiberwise_equipotent &
       (for i be Nat st t.a3<=i & i <= IExec(WH3,t).a4 holds
         IExec(WH3,t).intpos md >= IExec(WH3,t).intpos i) &
       (for i be Nat st IExec(WH3,t).a4 < i & i <= t.a4 holds
         IExec(WH3,t).intpos md <= IExec(WH3,t).intpos i) &
       (for i be Nat st i >= n0+1 & (i < t.a3 or i > t.a4) holds
         IExec(WH3,t).intpos i = t.intpos i);
A7: P[0]
    proof
      let t be State of SCMPDS,n3 be Nat,g,g1 be FinSequence of INT;
       assume A8: t.a=0 & t.a5 > 0 & t.a5<= 0+1 & t.a4=n3+t.a5 &
                  n3=t.a3-1 & t.a2=md & md <= n3 & n0+1 <= t.a3 & t.a4 <= n0+n;
       assume A9: g is_FinSequence_on t,n0;
       assume A10: g1 is_FinSequence_on IExec(WH3,t),n0;
       assume A11: len g=n & len g1=n;
       set t1=IExec(WB3,t);

        A12: DataLoc(t.a,5)=intpos(0+5) by A8,SCMP_GCD:5;
A13:  now
         let v be State of SCMPDS;
         assume A14: v.a4=v.a3-1+v.a5 & md <= v.a3-1 &
            v.a2=t.a2 & v.a=t.a & v.DataLoc(t.a,5) > 0;
            then v.a3-1 > 0 by A1,A5;
            then reconsider mm=v.a3-1 as Nat by INT_1:16;
      A15:  DataLoc(t.a,5)=intpos(0+5) by A8,SCMP_GCD:5;
      A16:  v.a4=mm+v.a5 & md <= mm by A14;
           set Iv=IExec(WB3,v);
        thus Iv.a=v.a by A6,A8,A14,A15,A16,Lm20;
        thus WB3 is_closed_on v & WB3 is_halting_on v
          by A6,A8,A14,A15,A16,Lm20;
        thus Iv.DataLoc(t.a,5) < v.DataLoc(t.a,5) by A6,A8,A14,A15,A16,Lm20;
        thus Iv.a4=Iv.a3-1+Iv.a5 by A6,A8,A14,A15,A16,Lm20;
               Iv.a3 >= v.a3 by A6,A8,A14,A15,A16,Lm20;
          then Iv.a3-1 >= v.a3-1 by REAL_1:49;
        hence md <= Iv.a3-1 by A14,AXIOMS:22;
        thus Iv.a2=v.a2 by A6,A8,A14,A15,A16,Lm20;
     end;
A17: t1.a=0 & t1.a1=t.a1 & t1.a2=md & t1.a3 >= t.a3 & t1.a4 <= t.a4 &
    t1.a4 >= n3 & t1.a5 < t.a5 & t1.a5 >= -1 & t1.a4=t1.a3-1+t1.a5
    by A6,A8,Lm20;
    consider n1,n2 be Nat such that
A18: n1=t1.a3-1 & n2=t1.a4+1 &
    (for i be Nat st i >= 8 & i <>n1 & i<> n2 holds t1.intpos i=t.intpos i) &
    (t1.intpos n1=t.intpos n1 & t1.intpos n2=t.intpos n2 or
t.a3 <= n1 & n2 <= t.a4 & t1.intpos n1=t.intpos n2 &
   t1.intpos n2=t.intpos n1) &
    (for i be Nat st t.a3<=i & i <= n1 holds t1.intpos md >= t1.intpos i) &
    (for i be Nat st n2<=i & i <= t.a4 holds t1.intpos md <= t1.intpos i)
     by A6,A8,Lm20;
A19:  DataLoc(t1.a,5)=intpos(0+5) by A17,SCMP_GCD:5;
          t1.a5+1 <= t.a5 by A17,INT_1:20;
     then t1.a5+1 <= 0+1 by A8,AXIOMS:22;
then A20:  t1.a5 <= 0 by REAL_1:53;
     set t2=IExec(WH3,t);
A21:  now let x;
        thus t2.x =IExec(WH3,t1).x by A8,A12,A13,Lm17,Th7
        .=t1.x by A19,A20,SCMPDS_8:23;
     end;
     hence t2.a=0 by A17;
     thus t2.a1=t.a1 by A17,A21;
     thus t2.a2=md by A17,A21;
               t2.a4 >= n3 by A17,A21;
     hence t2.a4 >= md by A8,AXIOMS:22;
     thus t2.a4 <= t.a4 by A17,A21;
A22:  now let i be Nat;
         assume A23: i >= n0+1 & i<>n1 & i<>n2;
                n0+1 >= 7+1 by A4,AXIOMS:24;
       then A24: i >= 8 by A23,AXIOMS:22;
          thus t2.intpos i=t1.intpos i by A21
            .=t.intpos i by A18,A23,A24;
     end;
A25:  n1 <= n2 by A17,A18,Lm23;
A26:  t2.intpos n1=t.intpos n1 & t2.intpos n2=t.intpos n2 or
     t.a3 <= n1 & n2 <= t.a4 &
     t2.intpos n1=t.intpos n2 & t2.intpos n2=t.intpos n1
     proof
          per cases by A18;
          suppose t1.intpos n1=t.intpos n1 & t1.intpos n2=t.intpos n2;
            hence thesis by A21;
          end;
          suppose t.a3 <= n1 & n2 <= t.a4 &
              t1.intpos n1=t.intpos n2 & t1.intpos n2=t.intpos n1;
           hence thesis by A21;
     end;
     end;
      t2.intpos n1=t.intpos n1 & t2.intpos n2=t.intpos n2 or
     n1 >= n0+1 & n2 >= n0+1 & n1 <= n0+n & n2 <= n0+n &
     t2.intpos n1=t.intpos n2 & t2.intpos n2=t.intpos n1
     proof
          per cases by A26;
          suppose t2.intpos n1=t.intpos n1 & t2.intpos n2=t.intpos n2;
            hence thesis;
          end;
          suppose A27:t.a3 <= n1 & n2 <= t.a4 &
              t2.intpos n1=t.intpos n2 & t2.intpos n2=t.intpos n1;
          then A28: n1 >= n0+1 by A8,AXIOMS:22;
               n2 <= n0+n by A8,A27,AXIOMS:22;
           hence thesis by A25,A27,A28,AXIOMS:22;
     end;
     end;
     hence g,g1 are_fiberwise_equipotent by A9,A10,A11,A22,Lm24;
A29:  t2.a4=n2-1 by A18,A21;
     hereby let i be Nat;
        assume A30: t.a3 <= i & i <= t2.a4;
             t2.a4 < n2 by A29,INT_1:26;
        then i < n2 by A30,AXIOMS:22;
        then i <= n1 by A17,A18,A20,Lm22;
        then t1.intpos md >= t1.intpos i by A18,A30;
        then t2.intpos md >= t1.intpos i by A21;
        hence t2.intpos md >= t2.intpos i by A21;
     end;
     hereby let i be Nat;
        assume A31: t2.a4 < i & i <= t.a4;
             t2.a4+1=n2 by A18,A21;
        then n2 <= i by A31,INT_1:20;
        then t1.intpos md <= t1.intpos i by A18,A31;
        then t2.intpos md <= t1.intpos i by A21;
      hence t2.intpos md <= t2.intpos i by A21;
     end;
     thus for i be Nat st i >= n0+1 & (i < t.a3 or i > t.a4) holds
         t2.intpos i = t.intpos i by A22,A25,A26,Lm25;
   end;
A32:now
       let k be Nat;
       assume A33: P[k];
            now
          let t be State of SCMPDS,n3 be Nat,g,g1 be FinSequence of INT;
          assume A34: t.a=0 & t.a5 > 0 & t.a5<= (k+1)+1 & t.a4=n3+t.a5 &
                  n3=t.a3-1 & t.a2=md & md <= n3 & n0+1 <= t.a3 & t.a4 <= n0+n;
          assume A35: g is_FinSequence_on t,n0;
          assume A36: g1 is_FinSequence_on IExec(WH3,t),n0;
          assume A37: len g=n & len g1=n;
          set t1=IExec(WB3,t);
             A38: DataLoc(t.a,5)=intpos(0+5) by A34,SCMP_GCD:5;
A39:     now
         let v be State of SCMPDS;
         assume A40: v.a4=v.a3-1+v.a5 & md <= v.a3-1 &
            v.a2=t.a2 & v.a=t.a & v.DataLoc(t.a,5) > 0;
            then v.a3-1 > 0 by A1,A5;
            then reconsider mm=v.a3-1 as Nat by INT_1:16;
      A41:  DataLoc(t.a,5)=intpos(0+5) by A34,SCMP_GCD:5;
      A42:  v.a4=mm+v.a5 & md <= mm by A40;
           set Iv=IExec(WB3,v);
        thus Iv.a=v.a by A6,A34,A40,A41,A42,Lm20;
        thus WB3 is_closed_on v & WB3 is_halting_on v
          by A6,A34,A40,A41,A42,Lm20;
        thus Iv.DataLoc(t.a,5) < v.DataLoc(t.a,5) by A6,A34,A40,A41,A42,Lm20;
        thus Iv.a4=Iv.a3-1+Iv.a5 by A6,A34,A40,A41,A42,Lm20;
               Iv.a3 >= v.a3 by A6,A34,A40,A41,A42,Lm20;
          then Iv.a3-1 >= v.a3-1 by REAL_1:49;
        hence md <= Iv.a3-1 by A40,AXIOMS:22;
        thus Iv.a2=v.a2 by A6,A34,A40,A41,A42,Lm20;
      end;
A43:   t1.a=0 & t1.a1=t.a1 & t1.a2=md & t1.a3 >= t.a3 & t1.a4 <= t.a4 &
      t1.a4 >= n3 & t1.a5 < t.a5 & t1.a5 >= -1 & t1.a4=t1.a3-1+t1.a5
      by A6,A34,Lm20;
      consider n1,n2 be Nat such that
A44:   n1=t1.a3-1 & n2=t1.a4+1 &
    (for i be Nat st i >= 8 & i <>n1 & i<> n2 holds t1.intpos i=t.intpos i) &
    (t1.intpos n1=t.intpos n1 & t1.intpos n2=t.intpos n2 or
 t.a3 <= n1 & n2 <= t.a4 & t1.intpos n1=t.intpos n2 &
 t1.intpos n2=t.intpos n1) &
    (for i be Nat st t.a3<=i & i <= n1 holds t1.intpos md >= t1.intpos i) &
    (for i be Nat st n2<=i & i <= t.a4 holds t1.intpos md <= t1.intpos i)
     by A6,A34,Lm20;
A45:  DataLoc(t1.a,5)=intpos(0+5) by A43,SCMP_GCD:5;
          t1.a5+1 <= t.a5 by A43,INT_1:20;
     then t1.a5+1 <= (k+1)+1 by A34,AXIOMS:22;
then A46:  t1.a5 <= k+1 by REAL_1:53;
A47:  n1 <= n2 by A43,A44,Lm23;
     set t2=IExec(WH3,t);

     per cases;
     suppose A48: t1.a5 > 0;
        set t3=IExec(WH3,t1);
        consider f3 be FinSequence of INT such that
   A49:  len f3=n & for i be Nat st 1<=i & i <= len f3 holds
          f3.i=t1.intpos(n0+i) by SCPISORT:2;
   A50:  f3 is_FinSequence_on t1,n0 by A49,SCPISORT:def 1;
        consider f4 be FinSequence of INT such that
   A51:  len f4=n & for i be Nat st 1<=i & i <= len f4 holds
          f4.i=t3.intpos(n0+i) by SCPISORT:2;
   A52:  f4 is_FinSequence_on t3,n0 by A51,SCPISORT:def 1;
             t1.a3-1 >= t.a3-1 by A43,REAL_1:49;
   then A53:  md <= n1 by A34,A44,AXIOMS:22;
   A54:  n0+1 <= t1.a3 by A34,A43,AXIOMS:22;
   A55:  t1.a4 <= n0+n by A34,A43,AXIOMS:22;
   then A56:  t3.a=0 & t3.a1=t1.a1 & t3.a2=md & t3.a4 >= md & t3.a4 <= t1.a4 &
        f3,f4 are_fiberwise_equipotent &
       (for i be Nat st t1.a3<=i & i <= t3.a4 holds
         t3.intpos md >= t3.intpos i) &
       (for i be Nat st t3.a4 < i & i <= t1.a4 holds
         t3.intpos md <= t3.intpos i) &
         (for i be Nat st i >= n0+1 & (i < t1.a3 or i > t1.a4) holds
         t3.intpos i = t1.intpos i)
           by A33,A43,A44,A46,A48,A49,A50,A51,A52,A53,A54;
   A57:  t2 =t3 by A34,A38,A39,Lm17,Th7;
        hence t2.a=0 by A33,A43,A44,A46,A48,A49,A50,A51,A52,A53,A54,A55;
        thus t2.a1=t.a1 by A33,A43,A44,A46,A48,A49,A50,A51,A52,A53,A54,A55,A57
;
        thus t2.a2=md by A33,A43,A44,A46,A48,A49,A50,A51,A52,A53,A54,A55,A57;
