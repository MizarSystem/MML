        .=t.intpos i by SCMPDS_5:40;
     end;
then A17: t6.a=0 by A9,SCMP_GCD:def 2;
          mp+3 > 1 by A13,AXIOMS:22;
then A18: t6.SBP=m1 by A9,A16,SCMP_GCD:def 3;
     A19: mp+3 > 4 by A13,AXIOMS:22;
then A20: t6.a4=t.a4 by A16;
          mp+0 <> mp+3 by XCMPLX_1:2;
then A21: t6.intpos mp=t.intpos mp by A16;
A22: t6.intpos (mp+3)=t0.DataLoc(t0.SBP,pn+1) by A15,SCMPDS_2:59
     .=t0.intpos(m1+(pn+1)) by A14,SCMP_GCD:5
     .=t.intpos (mp+1) by SCMPDS_5:40;

A23:  DataLoc(t6.SBP,pn+1)=intpos (m1+(pn+1)) by A18,SCMP_GCD:5
     .=intpos (mp+1);
A24:  now
        let i be Nat;
