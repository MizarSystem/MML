A44:  a1<>DataLoc(t2.a,3) by A42,SCMP_GCD:4;
   thus t1.a1 =Exec(k4, t2).a1 by SCMPDS_5:46
       .=s.a1 by A31,A44,SCMPDS_2:60;
A45:  a2<>DataLoc(t2.a,3) by A42,SCMP_GCD:4;
