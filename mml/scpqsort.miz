       .=s.a4-s.a2 by A38,A47,SCMPDS_2:60;
   hereby let i be Nat;
        assume A48:i>=8;
        then i > 3 by AXIOMS:22;
    then A49: intpos i <> DataLoc(t2.a,3) by A42,SCMP_GCD:4;
        thus t1.intpos i=Exec(k4, t2).intpos i by SCMPDS_5:46
             .=t2.intpos i by A49,SCMPDS_2:60
