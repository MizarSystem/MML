        IExec(IF3,s12).a1=s12.a1 by A64,A66,A57,A62,A68,A71,Lm19,X4,X5,X7,X8,X9
,X12;
        hence IExec(WB3,s).a1=s.a1 by A60,A67,A61,SCPISORT:7;
        IExec(IF3,s12).a2=s12.a2 by A64,A66,A57,A62,A68,A71,Lm19,X4,X6,X7,X8,X9
,X12;
        hence s3.a2=md by A60,A67,A63,SCPISORT:7;
        IExec(IF3,s12).a3=s12.a3+1 by A64,A66,A57,A62,A68,A71,Lm19,X4,X7,X8,X9
,X12;
        then
A72:    s3.a3=mE3+1 by A60,A67,A66,SCPISORT:7;
        then mE3 < s3.a3 by XREAL_1:31;
