A77:   now let i be Nat;
         assume A78: i >= n0+1 & i<>n1 & i<>n2;
                n0+1 >= 7+1 by A4,AXIOMS:24;
       then A79: i >= 8 by A78,AXIOMS:22;
          thus t2.intpos i=t1.intpos i by A76
            .=t.intpos i by A44,A78,A79;
