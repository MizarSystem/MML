  end;
A35: now
    let k be Element of NAT;
    assume
A36: P[k];
    now

      let t be State of SCMPDS,f1 be FinSequence of INT, k1,k2,y1,ym,yn be
      Element of NAT;

      assume that
A37:  t.a=c and
A38:  2*k1+1=t.b and
A39:  k2=m+n+2*k1+1 and
A40:  1 <= y1 & yn <= n or y1 >= yn and
A41:  m+y1=t.intpos k2 and
A42:  m+yn=t.intpos(k2+1) and
A43:  yn-y1<=k+1 and
A44:  f1 is_FinSequence_on t,m and
A45:  len f1=n;
      per cases;
      suppose
        yn-y1<=0;

        hence
        ex kk be Element of NAT,f2 be FinSequence of INT st
         Initialize Comput(ProgramPart(Initialize t+* sWH),
           Initialize t+*sWH,kk)+*sWH
    = Comput(ProgramPart(Initialize t+*sWH),Initialize t+*sWH,kk) &
    f2 is_FinSequence_on
     Comput(ProgramPart(Initialize t+*sWH),Initialize t+*sWH,kk),m &
    len f2=n & f1,f2 are_fiberwise_equipotent & f2 is_non_decreasing_on

y1,yn & (for j be Element of NAT st y1<yn & (1<=j & j<y1 or yn<j & j<= n) holds

f2.j= t.intpos(m+j)) & (for j be Element of NAT st y1>=yn & 1<=j & j<= n holds

f2.j= t.intpos(m+j)) & (for j be Element of NAT st (1<=j & j<2*k1+1) holds

Comput(ProgramPart(Initialize t+*sWH),Initialize t+*sWH,kk).intpos(m+n+j)
=t.intpos(m+n+j)) &
 Comput(ProgramPart(Initialize t+*sWH),Initialize t+*sWH,kk).b=
 t.b-2 & Comput(ProgramPart(Initialize t+*sWH),Initialize t+*sWH,kk).a=c
  by A13,A37,A38,A39,A40,A41,A42,A44,A45;

      end;
      suppose
A46:    yn-y1>0;
        set m1=LifeSpan(ProgramPart(Initialize t +* stop I),
                 Initialize t +* stop I)+2;
        set t1= Comput(ProgramPart(Initialize t +*sWH),Initialize t +*sWH,m1);
A47:    I is_halting_on t by A12,A37,A38,A39,A40,A41,A42;
        y1-1 >= 0 by A40,A46,XREAL_1:49,50;
        then reconsider y0=y1-1 as Element of NAT by INT_1:16;
