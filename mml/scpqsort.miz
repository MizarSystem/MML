               n2 <= n0+n by A34,A81,AXIOMS:22;
           hence thesis by A47,A81,A82,AXIOMS:22;
     end;
     end;
      hence g,g1 are_fiberwise_equipotent by A35,A36,A37,A77,Lm24;
A83:   t2.a4=n2-1 by A44,A76;
      hereby let i be Nat;
        assume A84: t.a3 <= i & i <= t2.a4;
             t2.a4 < n2 by A83,INT_1:26;
        then i < n2 by A84,AXIOMS:22;
        then i <= n1 by A43,A44,A75,Lm22;
        then t1.intpos md >= t1.intpos i by A44,A84;
        then t2.intpos md >= t1.intpos i by A76;
        hence t2.intpos md >= t2.intpos i by A76;
      end;
      hereby let i be Nat;
        assume A85: t2.a4 < i & i <= t.a4;
             t2.a4+1=n2 by A44,A76;
        then n2 <= i by A85,INT_1:20;
        then t1.intpos md <= t1.intpos i by A44,A85;
        then t2.intpos md <= t1.intpos i by A76;
       hence t2.intpos md <= t2.intpos i by A76;
      end;
      thus for i be Nat st i >= n0+1 & (i < t.a3 or i > t.a4) holds
         t2.intpos i = t.intpos i by A47,A77,A80,Lm25;
     end;
     end;
     hence P[k+1];
   end;
A86: for k be Nat holds P[k] from NAT_1:sch 1(A7,A32);
         s.a5 >=1+0 by A1,INT_1:20;
