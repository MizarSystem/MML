      KURATO_2, PROB_1;
 notations TARSKI, XBOOLE_0, ENUMSET1, SUBSET_1, ORDINAL1, XCMPLX_0, XREAL_0,
      COMPLEX1, REAL_1, INT_1, NAT_1, SQUARE_1, FUNCT_1, FUNCT_2, LIMFUNC1,
      FINSEQ_1, FINSEQ_4, FINSEQ_5, MATRIX_1, FINSEQ_6, POWER, TOPRNS_1,
