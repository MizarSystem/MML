               p in rng LS by A223,A225,PARTFUN2:4;
           then p = Gik by A35,A226,A227,FINSEQ_5:10;
           then Gik`1 = Ebo by A225,JORDAN1G:40;
           then Gik`1 = G*(len G,j)`1 by A10,A17,JORDAN1A:92;
