         then A177: i1 <= len G-'1 by A176,BINARITH:def 3;
         len G-'1 <= len G by GOBOARD9:2;
         then Gij`1 <= G*(len G-'1,1)`1 by A6,A10,A17,A21,A177,JORDAN1A:39;
