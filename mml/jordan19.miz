         then Index(Gik,LS)+1 = t by A212,A213,JORDAN3:45;
         then A214: len L_Cut(LS,Gik) = len LS-Index(Gik,LS)
                                                  by A8,A212,JORDAN3:61;
         set tt = Index(p,do)+(Gik..LS)-'1;
         A215: 1<=Index(Gik,LS) & 0+Index(Gik,LS) < len LS by A8,JORDAN3:41;
         then A216: len LS-Index(Gik,LS) > 0 by REAL_1:86;
         Index(p,do) < len LS-'Index(Gik,LS) by A209,A214,BINARITH:def 3;
         then Index(p,do)+1 <= len LS-'Index(Gik,LS) by NAT_1:38;
         then Index(p,do) <= len LS-'Index(Gik,LS)-1 by REAL_1:84;
         then Index(p,do) <= len LS-Index(Gik,LS)-1 by A216,BINARITH:def 3;
         then A217: Index(p,do) <= len LS-Gik..LS by A210,XCMPLX_1:36;
         then len LS-Gik..LS >= 1 by A209,AXIOMS:22;
         then len LS-Gik..LS >= 0;
         then Index(p,do) <= len LS-'Gik..LS by A217,BINARITH:def 3;
         then Index(p,do) < len LS-'(Gik..LS)+1 by NAT_1:38;
         then A218: LSeg(mid(LS,Gik..LS,len LS),Index(p,do)) =
              LSeg(LS,Index(p,do)+(Gik..LS)-'1) by A207,A208,A209,JORDAN4:31;
         A219: 1+1 <= Gik..LS by A210,A215,REAL_1:55;
         then Index(p,do)+Gik..LS >= 1+1+1 by A209,REAL_1:55;
         then A220: Index(p,do)+Gik..LS-1 >= 1+1+1-1 by REAL_1:49;
         then A221: Index(p,do)+Gik..LS-1 >= 0;
         A222: tt >= 1+1 by A220,BINARITH:def 3;
         A223: 2 in dom LS by A177,FINSEQ_3:27;
             now per cases by A222,REAL_1:def 5;
          suppose tt > 1+1;
           then LSeg(LS,1) misses LSeg(LS,tt) by TOPREAL1:def 9;
           hence contradiction by A204,A205,A206,A218,XBOOLE_0:3;
          end;
          suppose A224: tt = 1+1;
           then LSeg(LS,1) /\ LSeg(LS,tt) = {LS/.2} by A24,TOPREAL1:def 8
;
           then p in {LS/.2} by A204,A205,A206,A218,XBOOLE_0:def 3;
           then A225: p = LS/.2 by TARSKI:def 1;
           then A226: p..LS = 2 by A223,FINSEQ_5:44;
               1+1 = Index(p,do)+(Gik..LS)-1 by A221,A224,BINARITH:def 3;
           then 1+1+1 = Index(p,do)+(Gik..LS);
