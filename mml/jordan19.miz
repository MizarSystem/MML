         A201: 1 <= ii & ii <= len G & 1 <= jj+1 & jj+1 <= width G
                                                           by A193,GOBOARD5:1;
         A202: 1 <= ii & ii <= len G & 1 <= jj & jj <= width G
                                                           by A194,GOBOARD5:1;
         A203: ii+1 <> ii by NAT_1:38;
