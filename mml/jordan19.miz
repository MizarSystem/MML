         A223: 1<=Index(Gik,LS) & 0+Index(Gik,LS) < len LS by A8,JORDAN3:41;
         then A224: len LS-Index(Gik,LS) > 0 by REAL_1:86;
         then Index(p,do) < len LS-'Index(Gik,LS) by A217,A222,BINARITH:def 3;
