         then A174: p`1 = Ebo by A158,JORDAN1A:104;
         A175: (E-max C)`2 = p`2 by A172,JORDAN1A:def 3;
         set RC = Rotate(Cage(C,n),Emax);
         A176: E-max C in right_cell(RC,1) by JORDAN1I:9;
