         then p in LSeg(RC/.1,RC/.(1+1))
                              by A174,A187,A188,A192,A200,A202,A203,GOBOARD7:8;
         then A204: p in LSeg(LS,1) by A88,A178,A180,TOPREAL1:def 5;
