theorem  for f,i1,i2 st i1<=i2 holds
  (f|i1)|i2=f|i1 & (f|i2)|i1=f|i1
proof let f,i1,i2;assume A1:i1<=i2;
    len (f|i1)<=i1 by Th19;
 then len (f|i1)<=i2 by A1,XXREAL_0:2;
 hence (f|i1)|i2=f|i1 by FINSEQ_1:79;
     Seg i1 c= Seg i2 by A1,FINSEQ_1:7;
 hence thesis by FUNCT_1:82;
end;

theorem  (<*>D)|i=<*>D
proof len (<*>D)=0 by FINSEQ_1:32;
  then len (<*>D)<=i by NAT_1:18;
 hence thesis by FINSEQ_1:79;
end;

theorem  Rev (<*>D)=<*>D;

