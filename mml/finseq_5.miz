theorem
  for i1,i2 be Nat st i1<=i2 holds (f|i1)|i2=f|i1 & (f|i2)|i1=f|i1
proof
  let i1,i2 be Nat;
  assume
A1: i1<=i2;
  len (f|i1)<=i1 by Th19;
  hence (f|i1)|i2=f|i1 by A1,FINSEQ_1:79,XXREAL_0:2;
  Seg i1 c= Seg i2 by A1,FINSEQ_1:7;
  hence thesis by FUNCT_1:82;
end;

theorem
  for i be Nat holds (<*>D)|i=<*>D;

theorem
  Rev <*>D = <*>D;
 
:: from AMISTD_1, 2006.03,15, A.T.

registration
  cluster non trivial FinSequence;
  existence
  proof
    take <*0,0*>;
    thus thesis;
  end;
end;
 
:: from JORDAN3, 2007.03.09, A.T

theorem
  for f being FinSequence of D,l1,l2 being Nat holds (f/^l1)|(l2-'l1)=(f
  |l2)/^l1
proof
  let f be FinSequence of D,l1,l2 be Nat;
  per cases;
  suppose
A1: l1<=l2;
    per cases;
    suppose
A2:   l2<=len f;
      then
A3:   l2-l1<=len f-l1 by XREAL_1:11;
A4:   l1<=len (f|l2) by A1,A2,FINSEQ_1:80;
      then
A5:   len ((f|l2)/^l1)=len (f|l2)-l1 by RFINSEQ:def 2
        .=l2-l1 by A2,FINSEQ_1:80
        .=l2-'l1 by A1,XREAL_1:235;
A6:   l1<=len f by A1,A2,XXREAL_0:2;
      then len (f/^l1)=len f-l1 by RFINSEQ:def 2;
      then
A7:   l2-'l1<=len (f/^l1) by A1,A3,XREAL_1:235;
A8:   for k being Nat st 1<=k & k<=len ((f|l2)/^l1) holds ((f/^l1)|(l2-'
      l1)).k=((f|l2)/^l1).k
      proof
        let k be Nat such that
A9:     1<=k and
A10:    k<=len ((f|l2)/^l1);
A11:    k in dom ((f|l2)/^l1) by A9,A10,FINSEQ_3:27;
        k<=l2-l1 by A1,A5,A10,XREAL_1:235;
        then
A12:    k+l1<=l2-l1+l1 by XREAL_1:8;
        k<=len (f/^l1) by A7,A5,A10,XXREAL_0:2;
        then
A13:    k in dom (f/^l1) by A9,FINSEQ_3:27;
        k in NAT by ORDINAL1:def 13;
        then k in Seg (l2-'l1) by A5,A9,A10;
        then ((f/^l1)|(l2-'l1)).k =(f/^l1).k by FUNCT_1:72
          .=f.(k+l1) by A6,A13,RFINSEQ:def 2
          .=(f|l2).(k+l1) by A12,FINSEQ_3:121
          .=((f|l2)/^l1).k by A4,A11,RFINSEQ:def 2;
        hence thesis;
      end;
      len ((f/^l1)|(l2-'l1)) = l2-'l1 by A7,FINSEQ_1:80;
      hence thesis by A5,A8,FINSEQ_1:18;
    end;
    suppose
A14:  l2>len f;
A15:  len (f/^l1)=len f-'l1 by RFINSEQ:42;
      f|l2=f by A14,FINSEQ_1:79;
      hence thesis by A14,A15,FINSEQ_1:79,NAT_D:42;
    end;
  end;
  suppose
A16: l1>l2;
    reconsider l19=l1,l29=l2 as Element of NAT by ORDINAL1:def 13;
    l1-l1>l2-l1 by A16,XREAL_1:11;
    then l2-'l1=0 by XREAL_0:def 2;
    then dom ((f/^l1)|(l2-'l1)) =dom (f/^l1) /\ {} NAT .={} NAT;
    then
A17: (f/^l1)|(l2-'l1)=<*>(D);
    per cases;
    suppose
      l1<=len f;
      then l19>len (f|l29) by A16,FINSEQ_1:80,XXREAL_0:2;
      hence thesis by A17,Th35;
    end;
    suppose
A18:  l1>len f;
      len (f|l29)<=len f by Th18;
      hence thesis by A17,A18,Th35,XXREAL_0:2;
    end;
  end;
end;
 
:: from JORDAN8, 2007.03.18, A.T.

reserve D for set,
  f for FinSequence of D;

theorem
  len f >= 2 implies f|2 = <*f/.1,f/.2*>
proof
  set d1 = f/.1, d2 = f/.2;
  assume
A1: len f >= 2;
  then
A2: len(f|2) = 2 by FINSEQ_1:80;
  now
    assume
A3: D is empty;
    rng f c= D by FINSEQ_1:def 4;
    then f = {} by A3;
    hence contradiction by A1;
  end;
  then reconsider D as non empty set;
  reconsider f as FinSequence of D;
  2 in dom(f|2) by A2,FINSEQ_3:27;
  then
A4: d2 = (f|2)/.2 by FINSEQ_4:85
    .= (f|2).2 by A2,FINSEQ_4:24;
  1 in dom(f|2) by A2,FINSEQ_3:27;
  then d1 = (f|2)/.1 by FINSEQ_4:85
    .= (f|2).1 by A2,FINSEQ_4:24;
  hence thesis by A2,A4,FINSEQ_1:61;
end;

theorem Th85:
  k+1 <= len f implies f|(k+1) = f|k^<*f/.(k+1)*>
proof
A1: 1 <= k+1 by NAT_1:12;
  assume k+1 <= len f;
  then
A2: k+1 in dom f by A1,FINSEQ_3:27;
  then f|Seg(k+1) = (f|k)^<*f.(k+1)*> by Th11;
  hence thesis by A2,PARTFUN1:def 8;
end;
 
:: from JORDAN3, 2007.03.18, A.T.

theorem Th86:
  for D being set, p be FinSequence of D for i be Nat st i < len p
  holds p|(i+1) = p|i ^ <*p.(i+1)*>
proof
  let D be set, p be FinSequence of D;
  let i be Nat;
  assume i < len p;
  then
A1: i+1 <= len p by NAT_1:13;
  1 <= i+1 by NAT_1:11;
  then
A2: i+1 in dom p by A1,FINSEQ_3:27;
  p|(i+1) = p|i ^ <*p/.(i+1)*> by A1,Th85;
  hence thesis by A2,PARTFUN1:def 8;
end;
 
:: from POLYNOM4, 2007.03.18, A.T.

theorem
  for D be non empty set for p be FinSequence of D for n be Nat st 1 <=
  n & n <= len p holds p = (p|(n-'1))^<*p.n*>^(p/^n)
proof
  let D be non empty set;
  let p be FinSequence of D;
  let n be Nat;
  assume that
A1: 1 <= n and
A2: n <= len p;
  len p >= n-'1+1 by A1,A2,XREAL_1:237;
  then
A3: len p > n-'1 by NAT_1:13;
  p|n = p|(n-'1+1) by A1,XREAL_1:237
    .= p|(n-'1)^<*p.(n-'1+1)*> by A3,Th86
    .= p|(n-'1)^<*p.n*> by A1,XREAL_1:237;
  hence thesis by RFINSEQ:21;
end;
