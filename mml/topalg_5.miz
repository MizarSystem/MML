A415:       a in dom f and
A416:       f.a = b by FUNCT_1:def 5;
        a in union D by A415,A414,A386,TARSKI:def 4;
        then CircleMap.a in Ui by A375,FUNCT_2:46;
        hence b in Ui by A415,A416,A414,FUNCT_1:72;
