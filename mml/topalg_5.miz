A355: now
    let i be Nat such that
A356:   0 <= TT.i & TT.(i+1) <= 1;
    thus [.TT.i,TT.(i+1).] c= I
      proof
        let a be set;
        assume
A357:       a in [.TT.i,TT.(i+1).];
