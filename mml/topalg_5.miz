A422:      a in dom (Fn1 | [:xy,ZZ:]) and
A423:      (Fn1 | [:xy,ZZ:]).a = b by FUNCT_1:def 5;
        Fn1.a = b by A419,A423,A422,FUNCT_1:72;
        then b in Fn1.:xZZ by A333,A419,A422,FUNCT_1:def 12;
        hence b in dom f by A414,A399;
