  cluster JUMP ((a,k1):=(b,k2)) -> empty;
  coherence
  proof
    for l being Element of NAT holds NIC((a,k1):=(b,k2),l)
    ={succ l} by Th14;
    hence thesis by Th8;
  end;
end;

