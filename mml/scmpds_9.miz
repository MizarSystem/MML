       .= abs(m1-2+2*k2)+2 by A8,A28,SCMPDS_2:68;
       x = Exec(i,s2).IC SCMPDS by A10,A11,A12,Lm1
       .= Next n2 by A11,A29,A30,SCMPDS_2:68
