 vocabularies FINSET_1, REALSET1, RELAT_1, BOOLE, SUBSET_1, VECTSP_1, BINOP_1,
      GROUP_1, FUNCT_1, RLSUB_1, CARD_1, TARSKI, SETFAM_1, FINSUB_1, SETWISEO,
      ARYTM_3, GROUP_2, NAT_1;
 notations TARSKI, XBOOLE_0, ZFMISC_1, SUBSET_1, SETFAM_1, FINSUB_1, FINSET_1,
      REALSET1, XXREAL_0, NUMBERS, STRUCT_0, GROUP_1, WELLORD2, CARD_1,
      BINOP_1, SETWISEO, DOMAIN_1, RELAT_1, FUNCT_1, RELSET_1, NAT_1, NAT_D,
      MCART_1;
 constructors SETFAM_1, PARTFUN1, WELLORD2, BINOP_1, SETWISEO, XXREAL_0, NAT_1,
      NAT_D, MEMBERED, REALSET1, GROUP_1;
 registrations XBOOLE_0, SUBSET_1, RELSET_1, FINSET_1, FINSUB_1, MEMBERED,
      REALSET1, STRUCT_0, GROUP_1;
 requirements NUMERALS, REAL, BOOLE, SUBSET, ARITHM;
 definitions FUNCT_1, GROUP_1, STRUCT_0, TARSKI, XBOOLE_0, BINOP_1, REALSET1,
      SUBSET_1;
 theorems CARD_1, CARD_2, ENUMSET1, FINSET_1, FINSUB_1, FUNCT_1, FUNCT_2,
      GROUP_1, TARSKI, WELLORD2, ZFMISC_1, RELAT_1, RELSET_1, XBOOLE_0,
      XBOOLE_1, XCMPLX_1, NAT_D, STRUCT_0;
 schemes FUNCT_1, SUBSET_1, SETWISEO;

begin

reserve x for set;
reserve G for non empty 1-sorted;
reserve A for Subset of G;

 Lm1:
 x in A implies x is Element of G;

canceled 2;

theorem Th3:
 G is finite implies A is finite
  proof assume G is finite;
    then A c= the carrier of G & the carrier of G is finite
                                                    by STRUCT_0:def 11;
   hence thesis by FINSET_1:13;
  end;

reserve y,y1,y2,Y,Z for set;
reserve k for Element of NAT;
reserve G for Group;
reserve a,g,h for Element of G;
reserve A for Subset of G;

definition let G,A;
 func A" -> Subset of G equals
   {g" : g in A};
 coherence
  proof set F = {g" : g in A};
       F c= the carrier of G
     proof let x;
       assume x in F;
        then ex g st x = g" & g in A;
      hence thesis;
     end;
   hence thesis;
  end;
end;

canceled;

theorem Th5:
 x in A" iff ex g st x = g" & g in A;

theorem
    {g}" = {g"}
  proof
   thus {g}" c= {g"}
    proof let x;
      assume x in {g}";
       then consider h such that A1: x = h" & h in {g};
           h = g by A1,TARSKI:def 1;
     hence thesis by A1,TARSKI:def 1;
    end;
   let x;
    assume x in {g"};
     then x = g" & g in {g} by TARSKI:def 1;
   hence thesis;
  end;

theorem
    {g,h}" = {g",h"}
  proof
   thus {g,h}" c= {g",h"}
    proof let x;
      assume x in {g,h}";
       then consider a such that A1: x = a" & a in {g,h};
           a = g or a = h by A1,TARSKI:def 2;
     hence thesis by A1,TARSKI:def 2;
    end;
   let x;
    assume x in {g",h"};
     then (x = g" or x = h") & g in {g,h} & h in {g,h} by TARSKI:def 2;
   hence thesis;
  end;

theorem
    ({}(the carrier of G))" = {}
  proof
   thus ({}(the carrier of G))" c= {}
    proof let x;
      assume x in ({}(the carrier of G))";
       then consider a such that x = a" and A1: a in {}
(the carrier of G);
     thus thesis by A1;
    end;
   thus thesis by XBOOLE_1:2;
  end;

theorem
    ([#](the carrier of G))" = the carrier of G
  proof
   thus ([#](the carrier of G))" c= the carrier of G;
   let x;
    assume x in the carrier of G;
     then reconsider a = x as Element of G;
      a"" in ([#](the carrier of G))";
   hence thesis by GROUP_1:19;
  end;

theorem
    A <> {} iff A" <> {}
  proof
   thus A <> {} implies A" <> {}
    proof assume
A1:   A <> {};
      consider x being Element of A;
      reconsider x as Element of G by A1,Lm1;
          x" in A" by A1;
     hence thesis;
    end;
   assume
A2:  A" <> {};
    consider x being Element of A";
        ex a st x = a" & a in A by A2,Th5;
   hence thesis;
  end;

reserve G for non empty HGrStr,A,B,C for Subset of G;
reserve a,b,g,g1,g2,h,h1,h2 for Element of G;

definition let G; let A,B;
 func A * B -> Subset of G equals
    {g * h : g in A & h in B};
 coherence
  proof set S = {g * h : g in A & h in B};
       S c= the carrier of G
     proof let x;
       assume x in S;
        then ex g,h st x = g * h & g in A & h in B;
      hence thesis;
     end;
   hence thesis;
  end;
end;

canceled;

theorem Th12:
 x in A * B iff ex g,h st x = g * h & g in A & h in B;

theorem Th13:
 A <> {} & B <> {} iff A * B <> {}
  proof
   thus A <> {} & B <> {} implies A * B <> {}
    proof assume
A1:   A <> {};
       consider x being Element of A;
       reconsider x as Element of G by A1,TARSKI:def 3;
      assume
A2:     B <> {};
       consider y being Element of B;
       reconsider y as Element of G by A2,TARSKI:def 3;
           x * y in A * B by A1,A2;
     hence thesis;
    end;
    assume
A3:   A * B <> {};
     consider x being Element of A * B;
         ex a,b st x = a * b & a in A & b in B by A3,Th12;
   hence thesis;
  end;

theorem Th14:
 G is associative implies A * B * C = A * (B * C)
  proof assume A1: G is associative;
   thus A * B * C c= A * (B * C)
    proof let x;
      assume x in A * B * C;
       then consider g,h such that
       A2: x = g * h and A3: g in A * B and A4: h in C;
       consider g1,g2 such that A5: g = g1 * g2 and A6: g1 in A and A7: g2 in B
                                         by A3;
      x = g1 * (g2 * h) & g2 * h in B * C by A1,A2,A4,A5,A7,GROUP_1:def 4;
     hence thesis by A6;
    end;
   let x;
    assume x in A * (B * C);
     then consider g,h such that A8: x = g * h and A9: g in A and A10: h in
 B * C;
     consider g1,g2 such that A11: h = g1 * g2 and A12: g1 in B and A13: g2 in
 C by A10;
 x = g * g1 * g2 & g * g1 in A * B by A1,A8,A9,A11,A12,GROUP_1:def 4;
   hence thesis by A13;
  end;

theorem
    for G being Group, A,B being Subset of G
   holds (A * B)" = B" * A"
  proof let G be Group, A,B be Subset of G;
   thus (A * B)" c= B" * A"
    proof let x;
      assume x in (A * B)";
       then consider g being Element of G such that
       A1: x = g" & g in A * B;
       consider g1,g2 being Element of G such that
       A2: g = g1 * g2 & g1 in A & g2 in B by A1;
           x = g2" * g1" & g1" in A" & g2" in B" by A1,A2,GROUP_1:25;
     hence thesis;
    end;
   let x;
    assume x in B" * A";
     then consider g1,g2 being Element of G such that
     A3: x = g1 * g2 & g1 in B" & g2 in A";
     consider a being Element of G such that
     A4: g1 = a" & a in B by A3;
     consider b being Element of G such that
A5:    g2 = b" & b in A by A3;
         x = (b * a)" & b * a in A * B by A3,A4,A5,GROUP_1:25;
   hence thesis;
  end;

theorem
    A * (B \/ C) = A * B \/ A * C
  proof
   thus A * (B \/ C) c= A * B \/ A * C
    proof let x;
      assume x in A * (B \/ C);
       then consider g1,g2 such that A1: x = g1 * g2 & g1 in A & g2 in B \/ C;
           g2 in B or g2 in C by A1,XBOOLE_0:def 2;
        then x in A * B or x in A * C by A1;
     hence thesis by XBOOLE_0:def 2;
    end;
   let x;
    assume A2: x in A * B \/ A * C;
        now per cases by A2,XBOOLE_0:def 2;
      suppose x in A * B;
        then consider g1,g2 such that A3: x = g1 * g2 & g1 in A & g2 in
 B;
            g2 in B \/ C by A3,XBOOLE_0:def 2;
       hence thesis by A3;
      end;
      suppose x in A * C;
        then consider g1,g2 such that A4: x = g1 * g2 & g1 in A & g2 in
 C;
            g2 in B \/ C by A4,XBOOLE_0:def 2;
       hence thesis by A4;
     end;
     end;
   hence thesis;
  end;

theorem
    (A \/ B) * C = A * C \/ B * C
  proof
   thus (A \/ B) * C c= A * C \/ B * C
    proof let x;
      assume x in (A \/ B) * C;
       then consider g1,g2 such that A1: x = g1 * g2 & g1 in A \/ B & g2 in C;
           g1 in A or g1 in B by A1,XBOOLE_0:def 2;
        then x in A * C or x in B * C by A1;
     hence thesis by XBOOLE_0:def 2;
    end;
   let x;
    assume A2: x in A * C \/ B * C;
        now per cases by A2,XBOOLE_0:def 2;
      suppose x in A * C;
        then consider g1,g2 such that A3: x = g1 * g2 & g1 in A & g2 in
 C;
            g1 in A \/ B by A3,XBOOLE_0:def 2;
       hence thesis by A3;
      end;
      suppose x in B * C;
        then consider g1,g2 such that A4: x = g1 * g2 & g1 in B & g2 in
 C;
            g1 in A \/ B by A4,XBOOLE_0:def 2;
       hence thesis by A4;
     end;
     end;
   hence thesis;
  end;

theorem
    A * (B /\ C) c= (A * B) /\ (A * C)
  proof let x;
    assume x in A * (B /\ C);
     then consider g1,g2 such that A1: x = g1 * g2 & g1 in A & g2 in B /\ C;
         g2 in B & g2 in C by A1,XBOOLE_0:def 3;
      then x in A * B & x in A * C by A1;
   hence thesis by XBOOLE_0:def 3;
  end;

theorem
    (A /\ B) * C c= (A * C) /\ (B * C)
  proof let x;
    assume x in (A /\ B) * C;
     then consider g1,g2 such that A1: x = g1 * g2 & g1 in A /\ B & g2 in C;
         g1 in A & g1 in B by A1,XBOOLE_0:def 3;
      then x in A * C & x in B * C by A1;
   hence thesis by XBOOLE_0:def 3;
  end;

theorem Th20:
 {}(the carrier of G) * A = {} & A * {}(the carrier of G) = {}
  proof
    A1: now assume
A2:     {}(the carrier of G) * A <> {};
     consider x being Element of {}(the carrier of G) * A;
      consider g1,g2 such that x = g1 * g2 and
       A3: g1 in {}(the carrier of G) and g2 in A by A2,Th12;
     thus contradiction by A3;
    end;
       now assume
A4:    A * {}(the carrier of G) <> {};
     consider x being Element of A * {}(the carrier of G);
      consider g1,g2 such that x = g1 * g2 and
          g1 in A and A5: g2 in {}(the carrier of G) by A4,Th12;
     thus contradiction by A5;
    end;
   hence thesis by A1;
  end;

theorem Th21:
 for G being Group, A being Subset of G holds
 A <> {} implies [#](the carrier of G) * A = the carrier of G &
  A * [#](the carrier of G) = the carrier of G
   proof let G be Group, A be Subset of G;
    set B = [#](the carrier of G);
     assume A1: A <> {};
    thus [#](the carrier of G) * A = the carrier of G
     proof
      thus [#](the carrier of G) * A c= the carrier of G;
      let x;
       assume x in the carrier of G;
        then reconsider a = x as Element of G;
        consider y being Element of A;
        reconsider y as Element of G by A1,Lm1;
         A2: (a * y") * y = a * (y" * y) by GROUP_1:def 4
                         .= a * 1_G by GROUP_1:def 6
                         .= a by GROUP_1:def 5;
      thus thesis by A1,A2;
     end;
    thus A * [#](the carrier of G) c= the carrier of G;
    let x;
     assume x in the carrier of G;
      then reconsider a = x as Element of G;
      consider y being Element of A;
      reconsider y as Element of G by A1,Lm1;
       A3: y * (y" * a) = (y * y") * a by GROUP_1:def 4
                       .= 1_G * a by GROUP_1:def 6
                       .= a by GROUP_1:def 5;
    thus thesis by A1,A3;
   end;

theorem Th22:
 {g} * {h} = {g * h}
  proof
   thus {g} * {h} c= {g * h}
    proof let x;
      assume x in {g} * {h};
       then consider g1,g2 such that
       A1: x = g1 * g2 and A2: g1 in {g} & g2 in {h};
           g1 = g & g2 = h by A2,TARSKI:def 1;
     hence thesis by A1,TARSKI:def 1;
    end;
   let x;
    assume x in {g * h};
     then x = g * h & g in {g} & h in {h} by TARSKI:def 1;
   hence thesis;
  end;

theorem
    {g} * {g1,g2} = {g * g1,g * g2}
  proof
   thus {g} * {g1,g2} c= {g * g1,g * g2}
    proof let x;
      assume x in {g} * {g1,g2};
       then consider h1,h2 such that A1: x = h1 * h2 and
                                     A2: h1 in {g} & h2 in {g1,g2};
           h1 = g & (h2 = g1 or h2 = g2) by A2,TARSKI:def 1,def 2;
     hence thesis by A1,TARSKI:def 2;
    end;
   let x;
    assume x in {g * g1,g * g2};
     then (x = g * g1 or x = g * g2) & g in {g} & g1 in {g1,g2} & g2 in {g1,g2
}                                         by TARSKI:def 1,def 2;
   hence thesis;
  end;

theorem
    {g1,g2} * {g} = {g1 * g,g2 * g}
  proof
   thus {g1,g2} * {g} c= {g1 * g,g2 * g}
    proof let x;
      assume x in {g1,g2} * {g};
       then consider h1,h2 such that A1: x = h1 * h2 and
                                     A2: h1 in {g1,g2} & h2 in {g};
           h2 = g & (h1 = g1 or h1 = g2) by A2,TARSKI:def 1,def 2;
     hence thesis by A1,TARSKI:def 2;
    end;
   let x;
    assume x in {g1 * g,g2 * g};
     then (x = g1 * g or x = g2 * g) & g in {g} & g1 in {g1,g2} & g2 in {g1,g2
}                                   by TARSKI:def 1,def 2;
   hence thesis;
  end;

theorem
    {g,h} * {g1,g2} = {g * g1, g * g2, h * g1, h * g2}
  proof set A = {g,h} * {g1,g2}; set B = {g * g1, g * g2, h * g1, h * g2};
   thus A c= B
    proof let x;
      assume x in A;
       then consider h1,h2 such that A1: x = h1 * h2 and
                                     A2: h1 in {g,h} & h2 in {g1,g2};
           (h1 = g or h1 = h) & (h2 = g1 or h2 = g2) by A2,TARSKI:def 2;
     hence thesis by A1,ENUMSET1:def 2;
    end;
   let x;
    assume x in B;
     then (x = g * g1 or x = g * g2 or x = h * g1 or x = h * g2) &
          g in {g,h} & h in {g,h} & g1 in {g1,g2} & g2 in {g1,g2}
                                                  by ENUMSET1:def 2
,TARSKI:def 2;
   hence thesis;
  end;

theorem Th26:
 for G being Group, A being Subset of G holds
 (for g1,g2 being Element of G st g1 in A & g2 in A
    holds g1 * g2 in A) &
 (for g being Element of G st g in A holds g" in A)
      implies A * A = A
  proof let G be Group, A be Subset of G such that
     A1: for g1,g2 being Element of G st g1 in A & g2 in A
          holds g1 * g2 in A and
     A2: for g being Element of G st g in A holds g" in A;
   thus A * A c= A
    proof let x;
      assume x in A * A;
       then ex g1,g2 being Element of G
         st x = g1 * g2 & g1 in A & g2 in A;
     hence thesis by A1;
    end;
   let x;
    assume A3: x in A;
     then reconsider a = x as Element of G;
         a" in A by A2,A3;
      then a" * a in A by A1,A3;
      then 1_G in A & 1_G * a = a by GROUP_1:def 5,def 6;
   hence thesis by A3;
  end;

theorem Th27:
 for G being Group, A being Subset of G holds
 (for g being Element of G st g in A holds g" in
 A) implies A" = A
  proof let G be Group, A be Subset of G;
   assume A1: for g being Element of G st g in A holds g" in A;
   thus A" c= A
    proof let x;
      assume x in A";
       then ex g being Element of G st x = g" & g in A;
     hence thesis by A1;
    end;
   let x;
    assume A2: x in A;
     then reconsider a = x as Element of G;
         a" in A & x = a"" by A1,A2,GROUP_1:19;
   hence thesis;
  end;

theorem
    (for a,b st a in A & b in B holds a * b = b * a) implies A * B = B * A
  proof assume A1: for a,b st a in A & b in B holds a * b = b * a;
   thus A * B c= B * A
    proof let x;
      assume x in A * B;
       then consider a,b such that A2: x = a * b & a in A & b in B;
           x = b * a by A1,A2;
     hence thesis by A2;
    end;
   let x;
    assume x in B * A;
     then consider b,a such that A3: x = b * a & b in B & a in A;
         x = a * b by A1,A3;
   hence thesis by A3;
  end;

Lm2: for A be commutative Group, a, b be Element of A
     holds a * b = b * a;

theorem Th29:
 G is commutative Group implies A * B = B * A
  proof assume A1: G is commutative Group;
   thus A * B c= B * A
    proof let x;
      assume x in A * B;
       then consider g,h such that A2: x = g * h and A3: g in A & h in
 B;
           x = h * g by A1,A2,Lm2;
     hence thesis by A3;
    end;
   let x;
    assume x in B * A;
     then consider g,h such that A4: x = g * h and A5: g in B & h in A;
         x = h * g by A1,A4,Lm2;
   hence thesis by A5;
  end;

theorem
    for G being commutative Group, A,B being Subset of G holds
  (A * B)" = A" * B"
  proof let G be commutative Group, A,B be Subset of G;
   thus (A * B)" c= A" * B"
    proof let x;
      assume x in (A * B)";
       then consider g being Element of G such that
       A1: x = g" & g in A * B;
       consider g1,g2 being Element of G such that
       A2: g = g1 * g2 & g1 in A & g2 in B by A1;
           x = g1" * g2" & g1" in A" & g2" in B" by A1,A2,GROUP_1:94;
     hence thesis;
    end;
   let x;
    assume x in A" * B";
     then consider g1,g2 being Element of G such that
     A3: x = g1 * g2 & g1 in A" & g2 in B";
     consider a being Element of G such that
     A4: g1 = a" & a in A by A3;
     consider b being Element of G such that
     A5: g2 = b" & b in B by A3;
         x = (a * b)" & a * b in A * B by A3,A4,A5,GROUP_1:94;
   hence thesis;
  end;

definition let G,g,A;
 func g * A -> Subset of G equals
    {g} * A;
 correctness;
 func A * g -> Subset of G equals
    A * {g};
 correctness;
end;

canceled 2;

theorem Th33:
 x in g * A iff ex h st x = g * h & h in A
  proof
   thus x in g * A implies ex h st x = g * h & h in A
    proof assume x in g * A;
       then x in {g} * A;
      then consider g1,g2 such that
      A1: x = g1 * g2 & g1 in {g} & g2 in A;
          g1 = g by A1,TARSKI:def 1;
     hence thesis by A1;
    end;
    given h such that A2: x = g * h & h in A;
        g in {g} by TARSKI:def 1;
     then x in {g} * A by A2;
   hence thesis;
  end;

theorem Th34:
 x in A * g iff ex h st x = h * g & h in A
  proof
   thus x in A * g implies ex h st x = h * g & h in A
    proof assume x in A * g;
       then x in A * {g};
      then consider g1,g2 such that
      A1: x = g1 * g2 & g1 in A & g2 in {g};
          g2 = g by A1,TARSKI:def 1;
     hence thesis by A1;
    end;
    given h such that A2: x = h * g & h in A;
        g in {g} by TARSKI:def 1;
     then x in A * {g} by A2;
   hence thesis;
  end;

theorem
    G is associative implies g * A * B = g * (A * B) by Th14;

theorem
    G is associative implies A * g * B = A * (g * B) by Th14;

theorem
    G is associative implies A * B * g = A * (B * g) by Th14;

theorem Th38:
 G is associative implies g * h * A = g * (h * A)
  proof assume A1: G is associative;
   thus g * h * A = {g} * {h} * A by Th22
                 .= g * (h * A) by A1,Th14;
  end;

theorem Th39:
 G is associative implies g * A * h = g * (A * h) by Th14;

theorem Th40:
 G is associative implies A * g * h = A * (g * h)
  proof assume A1: G is associative;
   thus A * g * h = A * ({g} * {h}) by A1,Th14
                 .= A * (g * h) by Th22;
  end;

theorem
    {}(the carrier of G) * a = {} & a * {}(the carrier of G) = {} by Th20;

reserve G for Group-like (non empty HGrStr);
reserve h,g,g1,g2 for Element of G;
reserve A for Subset of G;

theorem Th42:
for G being Group, a being Element of G holds
 [#](the carrier of G) * a = the carrier of G &
  a * [#](the carrier of G) = the carrier of G by Th21;

theorem Th43:
 1_G * A = A & A * 1_G = A
  proof
   thus 1_G * A = A
    proof
     thus 1_G * A c= A
      proof let x;
        assume x in 1_G * A;
         then ex h st x = 1_G * h & h in A by Th33;
       hence thesis by GROUP_1:def 5;
      end;
     let x;
      assume A1: x in A;
       then reconsider a = x as Element of G;
           1_G * a = a by GROUP_1:def 5;
     hence thesis by A1,Th33;
    end;
   thus A * 1_G c= A
    proof let x;
      assume x in A * 1_G;
       then ex h st x = h * 1_G & h in A by Th34;
     hence thesis by GROUP_1:def 5;
    end;
   let x;
    assume A2: x in A;
     then reconsider a = x as Element of G;
         a * 1_G = a by GROUP_1:def 5;
   hence thesis by A2,Th34;
  end;

