             y" = b" * a" & a" in carr H2 & b" in carr H1
                                                      by A20,Th90,GROUP_1:25;
        hence contradiction by A15,A21;
       end;
      then y"" in carr H by Th90;
      then x in carr H by GROUP_1:19;
    hence thesis by A15;
   end;

