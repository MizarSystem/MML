     let x;
      assume x in carr(H);
        then A4: x in H by Lm3;
        then x in G by Th49;
       then reconsider b = x as Element of G by STRUCT_0:def 5;
           a" in H by A1,Th60;
        then A5: b * a" in H by A4,Th59;
           (b * a") * a = b * (a" * a) by GROUP_1:def 4
                    .= b * 1_G by GROUP_1:def 6
                    .= x by GROUP_1:def 5;
     hence thesis by A5,Th126;
    end;
    assume A6: H * a = carr(H);
        1_G * a = a & 1_G in H by Th55,GROUP_1:def 5;
