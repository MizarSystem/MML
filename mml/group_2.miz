     let x;
      assume x in carr(H);
        then A4: x in H by Lm3;
        then x in G by Th49;
       then reconsider b = x as Element of G by STRUCT_0:def 5;
           a" in H by A1,Th60;
        then A5: a" * b in H by A4,Th59;
           a * (a" * b) = a * a" * b by GROUP_1:def 4
                    .= 1_G * b by GROUP_1:def 6
                    .= x by GROUP_1:def 5;
     hence thesis by A5,Th125;
