    =('not' (z 'imp' (y 'imp' x))) 'or' (y 'imp' (z 'imp' x))
     by BVFUNC_1:def 1
   .=('not' ('not' z 'or' (y 'imp' x))) 'or' (y 'imp' (z 'imp' x))
     by BVFUNC_1:def 1
   .=('not' ('not' z 'or' ('not' y 'or' x))) 'or' (y 'imp' (z 'imp' x))
     by BVFUNC_1:def 1
