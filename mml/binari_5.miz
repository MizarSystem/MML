  .=((TRUE 'or' w) '&' (TRUE 'or' y)) '&'
    ((TRUE 'or' z) '&' (TRUE 'or' x)) by A4,A5,A6,A7,BINARITH:20
  .=(TRUE '&' (TRUE 'or' y)) '&'
    ((TRUE 'or' z) '&' (TRUE 'or' x)) by BINARITH:19
  .=(TRUE '&' TRUE) '&'
    ((TRUE 'or' z) '&' (TRUE 'or' x)) by BINARITH:19
  .=(TRUE '&' TRUE) '&'
    (TRUE '&' (TRUE 'or' x)) by BINARITH:19
  .=(TRUE '&' TRUE) '&'
    (TRUE '&' TRUE) by BINARITH:19
