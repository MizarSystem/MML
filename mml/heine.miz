 notations TARSKI, XBOOLE_0, SUBSET_1, ORDINAL1, NUMBERS, XCMPLX_0, XREAL_0,
      COMPLEX1, REAL_1, NAT_1, RELAT_1, FUNCT_2, METRIC_1, FINSET_1, BINOP_1,
      STRUCT_0, PRE_TOPC, COMPTS_1, PCOMPS_1, SEQ_1, SEQ_2, SEQM_3, LIMFUNC1,
      POWER, RCOMP_1, NEWTON, TOPMETR, XXREAL_0;
 constructors SETFAM_1, REAL_1, NAT_1, SEQ_2, SEQM_3, SEQ_4, RCOMP_1, LIMFUNC1,
      NEWTON, POWER, COMPTS_1, TOPMETR;
 registrations XBOOLE_0, SUBSET_1, ORDINAL1, RELSET_1, FINSET_1, NUMBERS,
      XXREAL_0, XREAL_0, MEMBERED, SEQM_3, PRE_TOPC, METRIC_1;
 requirements REAL, NUMERALS, SUBSET, BOOLE, ARITHM;
 definitions TARSKI, SEQ_2, LIMFUNC1;
 theorems ABSVALUE, FINSET_1, FUNCT_1, FUNCT_2, TOPMETR, LIMFUNC1, METRIC_1,
      NAT_1, NEWTON, POWER, PCOMPS_1, RCOMP_1, REAL_2, SEQ_1, SEQ_2, SEQ_4,
      SEQM_3, TARSKI, ZFMISC_1, XREAL_0, ORDINAL1, XBOOLE_1, XCMPLX_0,
