    +s3*s3)+3*a3*a3/8 by A4
      .= -2*(-2*p)+3*a3*a3/8 by A6,A5,A9,Th17
      .= 4*(8*a2-3*a3|^2)/32+3*a3*a3/8
      .= 4*(8*a2-3*(a3*a3))/32+3*a3*a3/8 by Th1
      .= a2;
  end;
