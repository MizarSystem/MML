A5:   len f<=len f+1 by NAT_1:29;
A6:   len f+1<>0 & len f+1=len(f^<*x*>) by A3,FINSEQ_1:35,NAT_1:21;
      A7:(f^<*x*>)/.len f=f/.len f by A4,Lm7;
         (f^<*x*>)/.(len f+1)=x by Lm6;
      hence thesis by A2,A4,A5,A6,A7,Lm18;
    end;

