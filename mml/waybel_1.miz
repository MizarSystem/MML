     then reconsider X' = X as Subset of L by XBOOLE_1:1;
     assume
A10:   ex_sup_of X,L;
         p preserves_sup_of X' by A8,WAYBEL_0:def 33;
then A11:    ex_sup_of p.:X,L & sup (p.:X') = p.(sup X') by A10,WAYBEL_0:def 31
;
          sup X' is_>=_than p.:X'
       proof let y be Element of L;
