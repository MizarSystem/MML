W1:   x = JumpPart I and
W2:   InsCode I = T;
     consider a,b such that
W3:   I = AddTo(a,b) by A1,W2,Th18;
     x = {} by W1,W3,RECDEF_2:def 2;
    hence x in {{}} by TARSKI:def 1;
  end;
  set a = the Data-Location of R;
  let x be set;
  assume x in {{}};
   then x = {} by TARSKI:def 1;
   then
X:  x = JumpPart AddTo(a,a) by RECDEF_2:def 2;
