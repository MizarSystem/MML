    end;
    thus SCM R is with_explicit_jumps
    proof
      let I be Instruction of SCM R;
      thus JUMP I c= rng JumpPart I
      proof
      let f be set such that
A26:  f in JUMP I;
      per cases by SCMRING2:8;
      suppose
A27:    I = [0,{},{}];
        JUMP halt SCM R is empty;
