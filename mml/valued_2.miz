theorem Th35:
  dom(f<->g) = dom f /\ dom g
  proof
    thus dom(f<->g) = dom f /\ dom -g by Def40
    .= dom f /\ dom g by VALUED_1:8;
  end;

theorem Th36:
  x in dom(f<->g) implies (f<->g).x = f.x - g.x
  proof
    assume x in dom(f<->g);
    hence (f<->g).x = f.x + (-g).x by Def40
    .= f.x - g.x by VALUED_1:8;
  end;

