theorem
  f1 <--> f2 = <-> (f2 <--> f1)
  proof
    set f = f2<-->f1;
A2: dom(f1<-->f2) = dom f1 /\ dom f2 by Def45;
A3: dom(f2<-->f1) = dom f2 /\ dom f1 by Def45;
    hence
A4: dom(f1<-->f2) = dom<->f by A2,Def32;
    let x;
    assume
A5: x in dom(f1<-->f2);
    hence (f1<-->f2).x = f1.x-f2.x by Def45
    .= -(f2.x-f1.x) by Th13
    .= -f.x by A2,A3,A5,Def45
    .= (<->f).x by A5,A4,Def32;
  end;

theorem
  <-> (f1 <--> f2) = (<->f1) <++> f2
  proof
    set f3 = f1 <--> f2, f4 = <->f1;
a4: dom(f1 <--> f2) = dom f1 /\ dom f2 by Def45;
a5: dom <->f1 = dom f1 by Def32;
a7: dom <->f3 = dom f3 by Def32;
    hence
A1: dom(<->f3) = dom(f4<++>f2) by a4,a5,Def44;
    let x;
    assume
A2: x in dom(<->f3);
    then
a8: x in dom f4 by a4,a5,a7,XBOOLE_0:def 4;
    thus (<->f3).x = -f3.x by A2,Def32
    .= -(f1.x-f2.x) by a7,A2,Def45
    .= -f1.x--f2.x by Th13a
    .= f4.x + f2.x by a8,Def32
    .= (f4<++>f2).x by A2,A1,Def44;
  end;

theorem
  f <++> f1 <--> f2 = f <++> (f1<-->f2)
  proof
    set f3 = f<++>f1, f4 = f1<-->f2;
A3: dom f3 = dom f /\ dom f1 by Def44;
A4: dom f4 = dom f1 /\ dom f2 by Def45;
A5: dom(f3<-->f2) = dom f3 /\ dom f2 by Def45;
A6: dom(f<++>f4) = dom f /\ dom f4 by Def44;
    hence
A7: dom(f3<-->f2) = dom(f<++>f4) by A3,A5,A4,XBOOLE_1:16;
    let x;
    assume
A8: x in dom(f3<-->f2);
    then
A9: x in dom f3 by A5,XBOOLE_0:def 4;
A10: x in dom f4 by A6,A8,A7,XBOOLE_0:def 4;
    thus (f3<-->f2).x = f3.x - f2.x by A8,Def45
    .= f.x + f1.x - f2.x by A9,Def44
    .= f.x + (f1.x - f2.x) by RFUNCT_1:19
    .= f.x + f4.x by A10,Def45
    .= (f<++>f4).x by A8,A7,Def44;
  end;

theorem
  f <--> f1 <++> f2 = f <--> (f1 <--> f2)
  proof
    set f3 = f<-->f1, f4 = f1<-->f2;
A3: dom f3 = dom f /\ dom f1 by Def45;
A4: dom f4 = dom f1 /\ dom f2 by Def45;
A5: dom(f3<++>f2) = dom f3 /\ dom f2 by Def44;
A6: dom(f<-->f4) = dom f /\ dom f4 by Def45;
    hence
A7: dom(f3<++>f2) = dom(f<-->f4) by A3,A5,A4,XBOOLE_1:16;
    let x;
    assume
A8: x in dom(f3<++>f2);
    then
A9: x in dom f3 by A5,XBOOLE_0:def 4;
A10: x in dom f4 by A6,A8,A7,XBOOLE_0:def 4;
    thus (f3<++>f2).x = f3.x + f2.x by A8,Def44
    .= f.x - f1.x + f2.x by A9,Def45
    .= f.x - (f1.x - f2.x) by RFUNCT_1:34
    .= f.x - f4.x by A10,Def45
    .= (f<-->f4).x by A8,A7,Def45;
  end;

theorem
  f <--> f1 <--> f2 = f <--> (f1 <++> f2)
  proof
    set f3 = f<-->f1, f4 = f1<++>f2;
A3: dom f3 = dom f /\ dom f1 by Def45;
A4: dom f4 = dom f1 /\ dom f2 by Def44;
A5: dom(f3<-->f2) = dom f3 /\ dom f2 by Def45;
A6: dom(f<-->f4) = dom f /\ dom f4 by Def45;
    hence
A7: dom(f3<-->f2) = dom(f<-->f4) by A3,A5,A4,XBOOLE_1:16;
