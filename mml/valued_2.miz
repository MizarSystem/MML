  end;
end;

theorem Th49:
  f1 <##> f2 = f2 <##> f1
  proof
    dom(f1<##>f2) = dom f1 /\ dom f2 by Def46;
    hence
A1: dom(f1<##>f2) = dom(f2<##>f1) by Def46;
    let x;
    assume
A2: x in dom(f1<##>f2);
    hence (f1<##>f2).x = f1.x (#) f2.x by Def46
    .= (f2<##>f1).x by A2,A1,Def46;
  end;

theorem
  (f <##> f1) <##> f2 = f <##> (f1 <##> f2) 
  proof
    set f3 = f<##>f1, f4 = f1<##>f2;
A3: dom f3 = dom f /\ dom f1 by Def46;
A4: dom f4 = dom f1 /\ dom f2 by Def46;
A5: dom(f3<##>f2) = dom f3 /\ dom f2 by Def46;
A6: dom(f<##>f4) = dom f /\ dom f4 by Def46;
    hence
A7: dom(f3<##>f2) = dom(f<##>f4) by A3,A5,A4,XBOOLE_1:16;
    let x;
    assume
A8: x in dom(f3<##>f2);
    then
A9: x in dom f3 by A5,XBOOLE_0:def 4;
A10: x in dom f4 by A6,A8,A7,XBOOLE_0:def 4;
    thus (f3<##>f2).x = f3.x (#) f2.x by A8,Def46
    .= f.x (#) f1.x (#) f2.x by A9,Def46
    .= f.x (#) (f1.x (#) f2.x) by RFUNCT_1:21
    .= f.x (#) f4.x by A10,Def46
    .= (f<##>f4).x by A8,A7,Def46;
  end;

theorem
  (<->f1) <##> f2 = <-> (f1<##>f2)
  proof
    set f3 = f1<##>f2, f4 = <->f1;
A4: dom f3 = dom f1 /\ dom f2 by Def46;
A5: dom f4 = dom f1 by Def32;
    dom(f4<##>f2) = dom f4 /\ dom f2 by Def46;
    hence
A8: dom(f4<##>f2) = dom(<->f3) by A4,A5,Def32;
    let x;
    assume
A9: x in dom(f4<##>f2);
    then
A10: x in dom f3 by A4,A5,Def46;
    then
A11: x in dom <->f1 by A4,A5,XBOOLE_0:def 4;
    thus (f4<##>f2).x = f4.x (#) f2.x by A9,Def46
    .= (-(f1.x)) (#) f2.x by A11,Def32
    .= -(f1.x) (#) f2.x by Th13b
    .= -f3.x by A10,Def46
    .= (<->f3).x by A9,A8,Def32;
  end;

theorem
  f1 <##> <->f2 = <-> (f1<##>f2)
  proof
    set f3 = f1<##>f2, f4 = <->f2;
A4: dom f3 = dom f1 /\ dom f2 by Def46;
A5: dom f4 = dom f2 by Def32;
    dom(f1<##>f4) = dom f1 /\ dom f4 by Def46;
    hence
A8: dom(f1<##>f4) = dom(<->f3) by A4,A5,Def32;
    let x;
    assume
A9: x in dom(f1<##>f4);
    then
A10: x in dom f3 by A4,A5,Def46;
    then
A11: x in dom <->f2 by A4,A5,XBOOLE_0:def 4;
    thus (f1<##>f4).x = f1.x (#) f4.x by A9,Def46
    .= f1.x (#) -f2.x by A11,Def32
    .= -(f1.x) (#) f2.x by Th13b
    .= -f3.x by A10,Def46
    .= (<->f3).x by A9,A8,Def32;
  end;

theorem Th51:
  f <##> (f1<++>f2) = (f<##>f1) <++> (f<##>f2)
  proof
    set f3 = f<##>f1, f4 = f<##>f2, f5 = f1<++>f2;
A4: dom f3 = dom f /\ dom f1 by Def46;
A5: dom f4 = dom f /\ dom f2 by Def46;
A6: dom(f<##>f5) = dom f /\ dom f5 by Def46;
A7: dom(f3<++>f4) = dom f3 /\ dom f4 by Def44;
    dom f5 = dom f1 /\ dom f2 by Def44;
    hence
A8: dom(f<##>f5) = dom(f3<++>f4) by A7,A4,A5,A6,Th1;
    let x;
    assume
A9: x in dom(f<##>f5);
    then
A10: x in dom f3 by A7,A8,XBOOLE_0:def 4;
A11: x in dom f4 by A7,A9,A8,XBOOLE_0:def 4;
A12: x in dom f5 by A6,A9,XBOOLE_0:def 4;
    thus (f<##>f5).x = f.x (#) f5.x by A9,Def46
    .= f.x (#) (f1.x + f2.x) by A12,Def44
    .= f.x (#) f1.x + f.x (#) f2.x by RFUNCT_1:22
    .= f3.x + f.x (#) f2.x by A10,Def46
    .= f3.x + f4.x by A11,Def46
    .= (f3<++>f4).x by A9,A8,Def44;
  end;

theorem
  (f1<++>f2) <##> f = (f1<##>f) <++> (f2<##>f)
  proof
    set f3 = f1<##>f, f4 = f2<##>f, f5 = f1<++>f2;
A2: f1<##>f = f<##>f1 & f2<##>f = f<##>f2 & f1<++>f2 = f2<++>f1 by Th43,Th49;
    thus f5 <##> f = f <##> f5 by Th49
    .= f3 <++> f4 by A2,Th51;
  end;

theorem Th53:
  f <##> (f1<-->f2) = (f<##>f1) <--> (f<##>f2)
  proof
    set f3 = f<##>f1, f4 = f<##>f2, f5 = f1<-->f2;
A4: dom f3 = dom f /\ dom f1 by Def46;
A5: dom f4 = dom f /\ dom f2 by Def46;
A6: dom(f<##>f5) = dom f /\ dom f5 by Def46;
A7: dom(f3<-->f4) = dom f3 /\ dom f4 by Def45;
    dom f5 = dom f1 /\ dom f2 by Def45;
    hence
A8: dom(f<##>f5) = dom(f3<-->f4) by A7,A4,A5,A6,Th1;
    let x;
    assume
A9: x in dom(f<##>f5);
    then
A10: x in dom f3 by A7,A8,XBOOLE_0:def 4;
A11: x in dom f4 by A7,A9,A8,XBOOLE_0:def 4;
A12: x in dom f5 by A6,A9,XBOOLE_0:def 4;
    thus (f<##>f5).x = f.x (#) f5.x by A9,Def46
    .= f.x (#) (f1.x - f2.x) by A12,Def45
    .= f.x (#) f1.x - f.x (#) f2.x by RFUNCT_1:27
    .= f3.x - f.x (#) f2.x by A10,Def46
    .= f3.x - f4.x by A11,Def46
    .= (f3<-->f4).x by A9,A8,Def45;
  end;

theorem
  (f1<-->f2) <##> f = (f1<##>f) <--> (f2<##>f)
  proof
    set f3 = f1<##>f, f4 = f2<##>f, f5 = f1<-->f2;
A2: f1<##>f = f<##>f1 & f2<##>f = f<##>f2 by Th49;
    thus f5 <##> f = f <##> f5 by Th49
    .= f3 <--> f4 by A2,Th53;
  end;

definition
  let X1, X2 be set;
  let Y1, Y2 be complex-functions-membered set;
  let f be PartFunc of X1,Y1;
  let g be PartFunc of X2,Y2;
  deffunc F(set) = f.$1/"g.$1;
  func f<//>g -> Function means
:Def47:
  dom it = dom f /\ dom g &
  for x being set st x in dom it holds it.x = f.x /" g.x;
  existence
  proof
    ex F being Function st dom F = dom f /\ dom g &
    for x being set st x in dom f /\ dom g holds F.x = F(x) from FUNCT_1:sch 3;
    hence thesis;
  end;
  uniqueness
  proof
    let F,G be Function such that
A1: dom F = dom f /\ dom g and
A2: for x being set st x in dom F holds F.x = F(x) and
A3: dom G = dom f /\ dom g and
A4: for x being set st x in dom G holds G.x = F(x);
    thus dom F = dom G by A1,A3;
    let x;
    assume
A5: x in dom F;
    hence F.x = F(x) by A2
    .= G.x by A1,A3,A4,A5;
  end;
end;

definition
  let X1, X2 be set;
  let Y1, Y2 be complex-functions-membered set;
  let f be PartFunc of X1,Y1;
  let g be PartFunc of X2,Y2;
  redefine func f<//>g -> PartFunc of X1 /\ X2, C_PFuncs(DOMS(Y1)/\DOMS(Y2));
  coherence
  proof
    set h = f<//>g;
a2: dom h = dom f /\ dom g by Def47;
    rng h c= C_PFuncs(DOMS(Y1)/\DOMS(Y2))
    proof
      let y be set;
      assume y in rng h;
      then consider x such that
a3:   x in dom h and
a4:   h.x = y by FUNCT_1:def 5;
a7:   h.x = f.x /" g.x by a3,Def47;
      then reconsider y as Function by a4;
b2:   dom y = dom(f.x) /\ dom(g.x) by a4,a7,VALUED_1:16;
      x in dom f & x in dom g by a2,a3,XBOOLE_0:def 4;
      then f.x in Y1 & g.x in Y2 by PARTFUN1:27;
      then dom(f.x) in {dom m where m is Element of Y1: not contradiction} &
      dom(g.x) in {dom m where m is Element of Y2: not contradiction};
      then
a5:   dom(f.x) c= DOMS(Y1) & dom(g.x) c= DOMS(Y2) by ZFMISC_1:92;
      rng y c= COMPLEX
      proof
        let b be set;
        thus thesis by a4,a7,XCMPLX_0:def 2;
      end;
      then y is PartFunc of DOMS(Y1)/\DOMS(Y2),COMPLEX
      by a5,b2,XBOOLE_1:27,RELSET_1:11;
      hence thesis by Def7;
    end;
    hence thesis by a2,XBOOLE_1:27,RELSET_1:11;
  end;
end;

