theorem Th1:
  C_Funcs(X) is Subset of C_PFuncs(X)
proof
  C_Funcs(X) c= C_PFuncs(X)
  proof
    let x;
    assume x in C_Funcs(X);
    then x is Function of X,COMPLEX by Def9;
    hence thesis by Def8;
  end;
  hence thesis;
end;

