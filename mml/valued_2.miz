A4: dom f3 = dom f1 /\ dom f by Def47;
A5: dom f4 = dom f2 /\ dom f by Def47;
A6: dom(f5<//>f) = dom f /\ dom f5 by Def47;
A7: dom(f3<-->f4) = dom f3 /\ dom f4 by Def45;
    dom f5 = dom f1 /\ dom f2 by Def45;
    hence
A8: dom(f5<//>f) = dom(f3<-->f4) by A7,A4,A5,A6,Th1;
    let x;
    assume
A9: x in dom(f5<//>f);
    then
A10: x in dom f3 by A7,A8,XBOOLE_0:def 4;
A11: x in dom f4 by A7,A9,A8,XBOOLE_0:def 4;
A12: x in dom f5 by A6,A9,XBOOLE_0:def 4;
    thus (f5<//>f).x = f5.x /" f.x by A9,Def47
    .= (f1.x-f2.x) /" f.x by A12,Def45
    .= f1.x /" f.x - f2.x /" f.x by RFUNCT_1:26
    .= f3.x - f2.x /" f.x by A10,Def47
    .= f3.x - f4.x by A11,Def47
    .= (f3<-->f4).x by A9,A8,Def45;
  end;
