  :16 ,def 4;
  hence dom(g (#) h /" k) = dom(g (#) (h /" k)) by A1,XBOOLE_1:16;
  let x;
  assume x in dom(g (#) h /" k);
  thus (g (#) h /" k).x = (g (#) h).x / k.x by VALUED_1:17
    .= g.x * h.x / k.x by VALUED_1:5
