theorem Th71:
  dom(f</>g) = dom f /\ dom g
proof
  thus dom(f</>g) = dom f /\ dom(g") by Def43
    .= dom f /\ dom g by VALUED_1:def 7;
end;

theorem Th72:
  x in dom(f</>g) implies (f</>g).x = f.x (/) g.x
proof
  assume x in dom(f</>g);
  hence (f</>g).x = f.x (#) (g").x by Def43
    .= f.x (/) g.x by VALUED_1:10;
end;

