theorem Th61:
  dom(f<->g) = dom f /\ dom g
proof
  thus dom(f<->g) = dom f /\ dom -g by Def41
    .= dom f /\ dom g by VALUED_1:8;
end;

theorem Th62:
  x in dom(f<->g) implies (f<->g).x = f.x - g.x
proof
  assume x in dom(f<->g);
  hence (f<->g).x = f.x + (-g).x by Def41
    .= f.x - g.x by VALUED_1:8;
end;

