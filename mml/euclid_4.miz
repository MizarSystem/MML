theorem Th48: :: AFF_1:27
  q1 in Line(p1,p2) & q2 in Line(p1,p2) implies Line(q1,q2) c= Line(p1,p2)
proof
  assume
A1: q1 in Line(p1,p2) & q2 in Line(p1,p2);
  (ex x1,x2 be Element of REAL n st x1 = p1 & x2 = p2 & Line(x1,x2)=Line(
  p1, p2) )& ex y1,y2 be Element of REAL n st y1 = q1 & y2 = q2 & Line(y1,y2)=
  Line(q1,q2) by Lm8;
  hence thesis by A1,Th11;
end;

theorem Th49: :: AFF_1:28
  q1 in Line(p1,p2) & q2 in Line(p1,p2) & q1<>q2 implies Line(p1,
  p2) c= Line(q1,q2)
proof
  assume
A1: q1 in Line(p1,p2) & q2 in Line(p1,p2) & q1<>q2;
  (ex x1,x2 be Element of REAL n st x1 = p1 & x2 = p2 & Line(x1,x2)=Line(
  p1, p2) )& ex y1,y2 be Element of REAL n st y1 = q1 & y2 = q2 & Line(y1,y2)=
  Line(q1,q2) by Lm8;
  hence thesis by A1,Th12;
end;

definition
  let n;
  let A be Subset of TOP-REAL n;
  attr A is being_line means :: AFF_1:def 3
  :Def8:
  ex p1,p2 st p1<>p2 & A=Line(p1,p2);
end;

Lm9: for A be Subset of TOP-REAL n,p1,p2 holds A is being_line & p1 in A & p2
in A & p1<>p2 implies A=Line(p1,p2)
proof
  let A be Subset of TOP-REAL n;
  let p1,p2;
  assume that
A1: A is being_line and
A2: p1 in A & p2 in A & p1<>p2;
  ex q1,q2 st q1<>q2 & A=Line(q1,q2) by A1,Def8;
  then Line(p1,p2) c= A & A c= Line(p1,p2) by A2,Th48,Th49;
  hence thesis by XBOOLE_0:def 10;
end;

theorem :: AFF_1:30
  for A,C being Subset of TOP-REAL n holds A is being_line & C is
  being_line & p1 in A & p2 in A & p1 in C & p2 in C implies p1=p2 or A=C
proof
  let A,C be Subset of TOP-REAL n;
  assume that
A1: A is being_line and
A2: C is being_line and
A3: p1 in A & p2 in A and
A4: p1 in C & p2 in C;
  assume
A5: p1<>p2;
  then A = Line(p1,p2) by A1,A3,Lm9;
  hence thesis by A2,A4,A5,Lm9;
end;

theorem Th51: :: AFF_1:31
  for A being Subset of TOP-REAL n st A is being_line holds ex p1,
  p2 st p1 in A & p2 in A & p1<>p2
proof
  let A be Subset of TOP-REAL n;
  assume A is being_line;
  then consider p1,p2 such that
A1: p1<>p2 and
A2: A = Line(p1,p2) by Def8;
  p1 in A & p2 in A by A2,Th47;
  hence thesis by A1;
end;

theorem :: AFF_1:32
  for A being Subset of TOP-REAL n st A is being_line holds ex p2 st p1
  <>p2 & p2 in A
proof
  let A be Subset of TOP-REAL n;
  assume A is being_line;
  then consider q1,q2 such that
A1: q1 in A and
A2: q2 in A & q1<>q2 by Th51;
  p1 = q1 implies p1<>q2 & q2 in A by A2;
  hence thesis by A1;
end;

definition
  let n;
  let p be Point of TOP-REAL n;
  func TPn2Rn (p) -> Element of REAL n equals
  p;
  correctness by EUCLID:25;
end;

theorem
  for R being Subset of REAL,p1,p2,q1 being Point of TOP-REAL n st R={|.
  q1-p .| where p is Point of TOP-REAL n: p in Line(p1,p2)} holds ex q2 being
Point of TOP-REAL n st q2 in Line(p1,p2) & |. q1-q2 .| =lower_bound R & p1-p2,
  q1-q2 are_orthogonal
proof
  let R being Subset of REAL,p1,p2,q1 being Point of TOP-REAL n;
  reconsider y1 = q1 as Element of REAL n by EUCLID:25;
  consider x1,x2 being Element of REAL n such that
A1: p1=x1 & p2=x2 and
A2: Line(x1,x2)=Line(p1,p2) by Lm8;
A3: x1-x2 = p1-p2 by A1,EUCLID:73;
  consider y2 being Element of REAL n such that
A4: y2 in Line(x1,x2) and
A5: x1-x2,y1-y2 are_orthogonal and
A6: for x being Element of REAL n st x in Line(x1,x2) holds |.y1 - y2.|
  <= |.y1 - x.| by Lm7;
  reconsider q2 = y2 as Point of TOP-REAL n by EUCLID:25;
  assume
A7: R={|. q1-p .| where p is Point of TOP-REAL n: p in Line(p1,p2)};
A8: for s being real number st 0<s holds ex r being real number st r in R &
  r < |.q1-q2.|+s
  proof
    let s be real number;
    assume
A9: 0<s;
    take |.q1- q2.|;
    thus thesis by A7,A2,A4,A9,XREAL_1:31;
  end;
  p1 in Line(p1,p2) by Th47;
  then
A10: |.q1-p1.| in R by A7;
A11: for r being real number st r in R holds |.q1-q2.| <=r
  proof
    let r be real number;
    assume r in R;
    then consider p0 being Point of TOP-REAL n such that
A12: r= |.q1-p0.| & p0 in Line(p1,p2) by A7;
    the carrier of Euclid n = the carrier of TOP-REAL n by EUCLID:71;
    then reconsider x = p0 as Element of REAL n;
    y1 - y2 = q1-q2 & y1 - x = q1-p0 by EUCLID:73;
    hence |.q1-q2.| <=r by A2,A6,A12;
  end;
   R is bounded_below
  proof
   take |.q1-q2.|;
    let r be ext-real number;
    assume r in R;
    then consider p0 being Point of TOP-REAL n such that
A12: r= |.q1-p0.| & p0 in Line(p1,p2) by A7;
    the carrier of Euclid n = the carrier of TOP-REAL n by EUCLID:71;
    then reconsider x = p0 as Element of REAL n;
    y1 - y2 = q1-q2 & y1 - x = q1-p0 by EUCLID:73;
    hence |.q1-q2.| <=r by A2,A6,A12;
  end;
  then y1-y2 = q1-q2 & |.q1-q2.| = lower_bound R by A10,A8,A11,EUCLID:73
,SEQ_4:def 5;
  hence thesis by A2,A4,A5,A3;
end;
