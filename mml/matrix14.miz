A63:    1<k;
        ((Base_FinSeq(K,n,1))/.k)=((Base_FinSeq(K,n,1)).k) by A56,A57,A58,
FINSEQ_4:24;
        then
A64:    (a")*(Base_FinSeq(K,n,1))/.k = a"*(0.K) by A57,A63,Th25
          .= 0.K by VECTSP_1:36;
