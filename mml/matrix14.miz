                              .=1.K by S,C0,XA,B1;
  end;
 hereby assume C0: i=1 & j=1;
    [j,i] in Indices (SwapDiagonal(K,n,i0)) by B2,A5,ZFMISC_1:106;
   hence (SwapDiagonal(K,n,i0))@ *(i,j)=(SwapDiagonal(K,n,i0)) *(j,i)
                                     by MATRIX_1:def 7
                              .= 0.K by S,C0,XA,A1,B1;
  end;
 hereby assume C0: i=i0 & j=i0;
    [j,i] in Indices (SwapDiagonal(K,n,i0)) by B2,A5,ZFMISC_1:106;
