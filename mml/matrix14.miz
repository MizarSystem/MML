        end;
       for k2 be Nat st k2 in dom (Col(G2,j-'1)) holds
         ((Col(B4,j-'1+1)).(len (<* 0.K *>) + k2) = (Col(G2,j-'1)).k2)
        proof let k2 be Nat;
         assume F3a: k2 in dom (Col(G2,j-'1));
