theorem Th1:
  i1 divides i2 & i1 divides i3 implies i1 divides i2-i3
proof
  assume that
A1: i1 divides i2 and
A2: i1 divides i3;
  consider i4 such that
A3: i2=i1*i4 by A1,INT_1:def 9;
  consider i5 such that
A4: i3=i1*i5 by A2,INT_1:def 9;
  i2-i3=i1*(i4-i5) by A3,A4;
  hence thesis by INT_1:def 9;
end;

theorem Th2:
  i divides a & i divides (a-b) implies i divides b
proof
  assume that
A1: i divides a and
A2: i divides (a-b);
A3: b=-(a-b)+a;
  i divides -(a-b) by A2,INT_2:14;
  hence thesis by A1,A3,WSIERP_1:9;
end;

Lm1: (x divides y implies y mod x = 0) & (x<>0 & y mod x = 0 implies x divides
y)
proof
A1: x divides y implies y mod x = 0
  proof
    assume x divides y;
    then consider i such that
A2: y = x * i by INT_1:def 9;
    y mod x = (x * i + 0) mod x by A2
      .= 0 mod x by EULER_1:13
      .= 0 by INT_4:12;
    hence thesis;
  end;
  x<>0 & y mod x = 0 implies x divides y
  proof
    assume that
A3: x<>0 and
A4: y mod x = 0;
    y = (y div x) * x + (y mod x) by A3,INT_1:86
      .= (y div x) * x by A4;
    hence thesis by INT_1:def 9;
  end;
  hence thesis by A1;
end;

definition
  let fp;
  func Poly-INT fp -> Function of INT,INT means
  :Def1:
  for x being Element of
  INT holds ex fr being FinSequence of INT st len fr = len fp & (for d st d in
  dom fr holds fr.d = (fp.d) * x|^(d-'1)) & it.x = Sum fr;
  existence
  proof
    defpred F[Element of INT,set] means ex fr being FinSequence of INT st len
fr = len fp & (for d st d in dom fr holds fr.d = (fp.d) * $1|^(d-'1)) & $2 =
    Sum fr;
A1: for x being Element of INT ex y being Element of INT st F[x,y]
    proof
      let x be Element of INT;
      deffunc G(Nat) = (fp.$1) * x|^($1-'1);
      consider fr be FinSequence such that
A2:   len fr = len fp & for d being Nat st d in dom fr holds fr.d = G(
      d) from FINSEQ_1:sch 2;
      for d being Nat st d in dom fr holds fr.d in INT
      proof
        let d be Nat;
        assume d in dom fr;
        then fr.d = (fp.d) * x|^(d-'1) by A2;
        hence thesis by INT_1:def 2;
      end;
      then reconsider fr as FinSequence of INT by FINSEQ_2:14;
      take Sum fr;
      take fr;
      thus thesis by A2;
    end;
    consider f being Function of INT,INT such that
A3: for x being Element of INT holds F[x,f.x] from FUNCT_2:sch 3(A1);
    take f;
    thus thesis by A3;
  end;
  uniqueness
  proof
    let f1 be Function of INT,INT;
    let f2 be Function of INT,INT;
    assume that
A4: for x being Element of INT holds ex fr1 being FinSequence of INT
st len fr1 = len fp & (for d st d in dom fr1 holds fr1.d = (fp.d) * x|^(d-'1))
    & f1.x = Sum fr1 and
A5: for x being Element of INT holds ex fr2 being FinSequence of INT
st len fr2 = len fp & (for d st d in dom fr2 holds fr2.d = (fp.d) * x|^(d-'1))
    & f2.x = Sum fr2;
    for x being Element of INT holds f1.x = f2.x
    proof
      let x be Element of INT;
      consider fr1 being FinSequence of INT such that
A6:   len fr1 = len fp and
A7:   for d st d in dom fr1 holds fr1.d = (fp.d) * x|^(d-'1) and
A8:   f1.x = Sum fr1 by A4;
      consider fr2 being FinSequence of INT such that
A9:   len fr2 = len fp and
A10:  for d st d in dom fr2 holds fr2.d = (fp.d) * x|^(d-'1) and
A11:  f2.x = Sum fr2 by A5;
A12:  dom fr1 = dom fr2 by A6,A9,FINSEQ_3:31;
      for d being Nat st d in dom fr1 holds fr1.d = fr2.d
      proof
        let d be Nat;
        assume
A13:    d in dom fr1;
        hence fr2.d = (fp.d) * x|^(d-'1) by A10,A12
          .= fr1.d by A7,A13;
      end;
      hence thesis by A8,A11,A12,FINSEQ_1:17;
    end;
    hence thesis by FUNCT_2:113;
  end;
end;

registration
  let fp be FinSequence of INT,x be Integer;
  cluster (Poly-INT fp).x -> integer;
  coherence;
end;

theorem Th3:
  len fp = 1 implies (Poly-INT fp) = INT --> fp.1
proof
  assume
A1: len fp = 1;
  for x being set st x in dom Poly-INT fp holds (Poly-INT fp).x = fp.1
  proof
    let x be set;
    assume x in dom Poly-INT fp;
    then reconsider x as Element of INT;
    consider fr being FinSequence of INT such that
A2: len fr = len fp and
A3: for d st d in dom fr holds fr.d = (fp.d) * x|^(d-'1) and
A4: (Poly-INT fp).x = Sum fr by Def1;
    1 in dom fr by A1,A2,FINSEQ_3:27;
    then
A5: fr.1 = (fp.1) * x|^(1-'1) by A3
      .= fp.1 * x|^0 by XREAL_1:234
      .= fp.1 * 1 by NEWTON:9;
    fr = <*fr.1*> by A1,A2,FINSEQ_1:57;
    hence thesis by A4,A5,RVSUM_1:103;
  end;
  then Poly-INT fp = dom Poly-INT fp --> fp.1 by FUNCOP_1:17;
  hence thesis by FUNCT_2:def 1;
end;

theorem
  len fp = 1 implies for x being Element of INT holds (Poly-INT fp).x = fp.1
proof
  assume
A1: len fp = 1;
  let x be Element of INT;
  consider fr being FinSequence of INT such that
A2: len fr = len fp and
A3: for d st d in dom fr holds fr.d = (fp.d) * x|^(d-'1) and
A4: (Poly-INT fp).x = Sum fr by Def1;
  1 in dom fr by A1,A2,FINSEQ_3:27;
  then
A5: fr.1 = (fp.1) * x|^(1-'1) by A3
    .= fp.1 * x|^0 by XREAL_1:234
    .= fp.1 * 1 by NEWTON:9;
  fr = <*fr.1*> by A1,A2,FINSEQ_1:57;
  hence thesis by A4,A5,RVSUM_1:103;
end;

reserve fr for FinSequence of REAL;

theorem Th5:
  for f,f1,f2 st len f = n+1 & len f1 = len f & len f2 = len f & (
for d st d in dom f holds f.d = f1.d - f2.d) holds ex fr st len fr = len f - 1
& (for d st d in dom fr holds fr.d = f1.d - f2.(d + 1)) & Sum f = Sum fr + f1.(
  n+1) - f2.1
proof
  defpred P[Nat] means for f,f1,f2 st len f = $1 + 1 & len f1 = len f & len f2
= len f & (for d st d in dom f holds f.d = f1.d - f2.d) holds ex fr st len fr =
len f - 1 & (for d st d in dom fr holds fr.d = f1.d - f2.(d + 1)) & Sum f = Sum
  fr + f1.($1+1) - f2.1;
A1: for n st P[n] holds P[n+1]
  proof
    let n;
    assume
A2: P[n];
    let f,f1,f2;
    assume that
A3: len f = (n+1)+1 and
A4: len f1 = len f and
A5: len f2 = len f and
A6: for d st d in dom f holds f.d = f1.d - f2.d;
    set ff1 = f1|Seg(n+1);
    reconsider ff1 as FinSequence of REAL by FINSEQ_1:23;
A7: len ff1 = n+1 by A3,A4,FINSEQ_3:59;
    set ff2 = f2|Seg(n+1);
    reconsider ff2 as FinSequence of REAL by FINSEQ_1:23;
A8: f2 = ff2^<*f2.((n+1)+1)*> by A3,A5,FINSEQ_3:61;
A9: len ff2 = n+1 by A3,A5,FINSEQ_3:59;
    then ff2 <> {};
    then 1 in dom ff2 by FINSEQ_5:6;
    then
A10: ff2.1 = f2.1 by A8,FINSEQ_1:def 7;
A11: f1 = ff1^<*f1.((n+1)+1)*> by A3,A4,FINSEQ_3:61;
    (n+1)+1 in Seg ((n+1)+1) by FINSEQ_1:6;
    then ((n+1)+1) in dom f by A3,FINSEQ_1:def 3;
    then
A12: f.((n+1)+1) = f1.((n+1)+1) - f2.((n+1)+1) by A6;
    set f3 = f|Seg(n+1);
    reconsider f3 as FinSequence of REAL by FINSEQ_1:23;
A13: dom f3 = Seg(n+1) by A3,FINSEQ_3:60;
    then
A14: len f3 = n+1 by FINSEQ_1:def 3;
A15: f = f3^<*f.(n+1+1)*> by A3,FINSEQ_3:61;
A16: for d st d in dom f3 holds f3.d = ff1.d - ff2.d
    proof
      let d;
A17:  dom f3 c= dom f by A15,FINSEQ_1:39;
      assume
A18:  d in dom f3;
      then
A19:  d in dom ff2 by A13,A9,FINSEQ_1:def 3;
      d in dom ff1 by A13,A7,A18,FINSEQ_1:def 3;
      then
A20:  f1.d = ff1.d by A11,FINSEQ_1:def 7;
      f3.d = f.d by A15,A18,FINSEQ_1:def 7
        .= f1.d - f2.d by A6,A18,A17;
      hence thesis by A8,A19,A20,FINSEQ_1:def 7;
    end;
    ff1 <> {} by A7;
    then (n+1) in dom ff1 by A7,FINSEQ_5:6;
    then ff1.(n+1) = f1.(n+1) by A11,FINSEQ_1:def 7;
    then consider f4 being FinSequence of REAL such that
A21: len f4 = len f3 - 1 and
A22: for d st d in dom f4 holds f4.d=ff1.d - ff2.(d + 1) and
A23: Sum f3 = Sum f4 + f1.(n+1) - f2.1 by A2,A14,A7,A9,A16,A10;
    take f5 = f4^<*f1.(n+1) - f2.(n+2)*>;
    f1.(n+1) - f2.(n+2) is Element of REAL by XREAL_0:def 1;
    then <*f1.(n+1) - f2.(n+2)*> is FinSequence of REAL by FINSEQ_1:95;
    then reconsider f5 as FinSequence of REAL by FINSEQ_1:96;
A24: Sum f = Sum f4 + f1.(n+1) - f2.1 + f.(n+1+1) by A15,A23,RVSUM_1:104
      .= Sum f4 + (f1.(n+1) - f2.(n+2)) + f1.((n+1)+1) - f2.1 by A12
      .= Sum f5 + f1.((n+1)+1) - f2.1 by RVSUM_1:104;
A25: len f4 + 1 = n + 1 by A13,A21,FINSEQ_1:def 3;
A26: for d st d in dom f5 holds f5.d = f1.d - f2.(d + 1)
    proof
      let d;
      assume d in dom f5;
      then d in Seg len f5 by FINSEQ_1:def 3;
      then d in Seg (len f4 + 1) by FINSEQ_2:19;
      then d in Seg len f4 \/{len f4 + 1} by FINSEQ_1:11;
      then
A27:  d in Seg len f4 or d in {len f4 + 1} by XBOOLE_0:def 3;
      per cases by A27,TARSKI:def 1;
      suppose
A28:    d in Seg len f4;
        then d+1 in Seg(len f4 + 1) by FINSEQ_1:81;
        then d+1 in Seg len ff2 by A3,A5,A14,A21,FINSEQ_3:59;
        then
A29:    d+1 in dom ff2 by FINSEQ_1:def 3;
A30:    d in dom f4 by A28,FINSEQ_1:def 3;
        len f4 <= len ff1 by A14,A7,A21,XREAL_1:149;
        then dom f4 c= dom ff1 by FINSEQ_3:32;
        then
A31:    f1.d = ff1.d by A11,A30,FINSEQ_1:def 7;
        f5.d = f4.d by A30,FINSEQ_1:def 7
          .= ff1.d - ff2.(d+1) by A22,A30;
        hence thesis by A8,A31,A29,FINSEQ_1:def 7;
      end;
      suppose
A32:    d = len f4 + 1;
        1 in Seg 1 by FINSEQ_1:4,TARSKI:def 1;
        then 1 in dom <*f1.(n+1) - f2.(n+2)*> by FINSEQ_1:55;
        then f5.d = <*f1.(n+1) - f2.(n+2)*>.1 by A32,FINSEQ_1:def 7
          .= f1.d - f2.(d+1) by A25,A32,FINSEQ_1:57;
        hence thesis;
      end;
    end;
    len f5 = len f4 + 1 by FINSEQ_2:19
      .= len f - 1 by A3,A13,A21,FINSEQ_1:def 3;
    hence thesis by A26,A24;
  end;
A33: P[0]
  proof
    let f,f1,f2;
    assume that
A34: len f = 0+1 and
    len f1 = len f and
    len f2 = len f and
A35: for d st d in dom f holds f.d = f1.d - f2.d;
    take <*>REAL;
    0+1 in Seg (0+1) by FINSEQ_1:6;
    then 1 in dom f by A34,FINSEQ_1:def 3;
    then f.1 = f1.1 - f2.1 by A35;
    then f = <*f1.1 - f2.1*> by A34,FINSEQ_1:57;
    hence thesis by A34,RVSUM_1:102,103;
  end;
  for n holds P[n] from NAT_1:sch 2(A33,A1);
  hence thesis;
end;

theorem Th6:
  len fp = n+2 implies for a being Integer holds ex fr being
  FinSequence of INT, r being Integer st len fr = n+1 & (for x being Element of
  INT holds (Poly-INT fp).x = (x-a)*(Poly-INT fr).x + r) & fp.(n+2) = fr.(n+1)
proof
  assume
A1: len fp = n+2;
  (n+1)+1 in Seg ((n+1)+1) by FINSEQ_1:6;
  then n+2 in dom fp by A1,FINSEQ_1:def 3;
  then reconsider A = fp.(n+2) as Element of INT by FINSEQ_2:13;
  reconsider n1=n+1 as Element of NAT;
  let a be Integer;
  defpred P[Nat,Integer,set] means $3 = fp.(n+2-$1) +a*$2;
A2: for d being Element of NAT st 1 <= d & d < n1 holds for x being Element
  of INT ex y being Element of INT st P[d,x,y]
  proof
    let d be Element of NAT;
    assume that
    1 <= d and
    d < n1;
    let x be Element of INT;
    set y = fp.(n+2-d) +a*x;
    reconsider y as Element of INT by INT_1:def 2;
    take y;
    thus thesis;
  end;
  consider p being FinSequence of INT such that
A3: len p = n1 & (p.1 = A or n1 = 0) & for d being Element of NAT st 1
  <= d & d < n1 holds P[d,p.d,p.(d+1)] from RECDEF_1:sch 4(A2);
  take fr = Rev p;
  take r = fp.1 + a*fr.1;
A4: len fr = n+1 by A3,FINSEQ_5:def 3;
  for x being Element of INT holds (Poly-INT fp).x = (x-a)*(Poly-INT fr).x + r
  proof
    let x be Element of INT;
    deffunc F(Nat) = (fr.$1)*x|^$1;
    deffunc FF(Nat) = a*(fr.$1) * x|^($1-'1);
    consider f1 being FinSequence of INT such that
A5: len f1 = len fp and
A6: for d st d in dom f1 holds f1.d = (fp.d) * x|^(d-'1) and
A7: (Poly-INT fp).x = Sum f1 by Def1;
A8: f1 <> {} by A1,A5;
    then n+2 in dom f1 by A1,A5,FINSEQ_5:6;
    then f1.(n+2)=(fp.(n+2))*x|^((n+1)+1-'1) by A6;
    then
A9: f1.(n+2)=(fp.(n+2))*x|^(n+1) by NAT_D:34;
    f1.1 = (fp.1)*x|^(1-'1) by A6,A8,FINSEQ_5:6;
    then f1.1=(fp.1)*x|^0 by XREAL_1:234;
    then
A10: f1.1 = (fp.1)*1 by NEWTON:9;
    reconsider n as Element of NAT by ORDINAL1:def 13;
    consider f4 being FinSequence such that
A11: len f4 = n+1 & for d being Nat st d in dom f4 holds f4.d=F(d)
    from FINSEQ_1:sch 2;
A12: for d being Nat st d in dom f4 holds f4.d in INT
    proof
      let d be Nat;
      reconsider d1 = d as Element of NAT by ORDINAL1:def 13;
      assume d in dom f4;
      then f4.d1 = (fr.d1)*x|^d1 by A11;
      hence thesis by INT_1:def 2;
    end;
    f4 <> {} by A11;
    then n+1 in dom f4 by A11,FINSEQ_5:6;
    then f4.(n+1)=(fr.(n+1))*x|^(n+1) by A11;
    then
A13: f4.(n+1) = (fp.(n+2))*x|^(n+1) by A3,FINSEQ_5:65;
    reconsider f4 as FinSequence of INT by A12,FINSEQ_2:14;
    consider f5 being FinSequence such that
A14: len f5 = n+1 & for d being Nat st d in dom f5 holds f5.d=FF(d)
    from FINSEQ_1:sch 2;
A15: for d being Nat st d in dom f5 holds f5.d in INT
    proof
      let d be Nat;
      assume d in dom f5;
      then f5.d = a*(fr.d) * x|^(d-'1) by A14;
      hence thesis by INT_1:def 2;
    end;
    f5 <> {} by A14;
    then 1 in dom f5 by FINSEQ_5:6;
    then f5.1 = a*(fr.1) * x|^(1-'1) by A14;
    then f5.1 = a*(fr.1)*x|^0 by XREAL_1:234;
    then
A16: f5.1 = a*(fr.1)*1 by NEWTON:9;
    reconsider f5 as FinSequence of INT by A15,FINSEQ_2:14;
A17: f4 is FinSequence of REAL by FINSEQ_3:126;
    consider f2 being FinSequence of INT such that
A18: len f2 = len fr and
A19: for d st d in dom f2 holds f2.d = (fr.d) * x|^(d-'1) and
A20: (Poly-INT fr).x = Sum f2 by Def1;
    set f3 = (x-a)*f2;
A21: dom f3 = dom f2 by VALUED_1:def 5;
    then
A22: len f3 = len f2 by FINSEQ_3:31;
A23: dom f3 = dom f4 by A4,A18,A11,A21,FINSEQ_3:31;
A24: for k being Element of NAT st k in dom f3 holds f3.k = (fr.k)*x|^k -
    a*(fr.k) * x|^(k-'1)
    proof
      let k be Element of NAT;
      assume
A25:  k in dom f3;
      then
A26:  k >= 1 by FINSEQ_3:27;
A27:  k in dom f2 by A25,VALUED_1:def 5;
      thus f3.k = (x-a)*(f2.k) by A25,VALUED_1:def 5
        .= (x-a)*((fr.k) * x|^(k-'1)) by A19,A27
        .= (fr.k) * (x|^(k-'1)*x) - a*(fr.k) * x|^(k-'1)
        .= (fr.k)*x|^(k-'1+1) - a*(fr.k) * x|^(k-'1) by NEWTON:11
        .= (fr.k)*x|^k - a*(fr.k) * x|^(k-'1) by A26,XREAL_1:237;
    end;
A28: dom f3 = dom f5 by A4,A18,A14,A21,FINSEQ_3:31;
A29: for d st d in dom f3 holds f3.d = f4.d - f5.d
    proof
      let d;
      assume
A30:  d in dom f3;
      then
A31:  f5.d = a*(fr.d) * x|^(d-'1) by A14,A28;
      f4.d = (fr.d)*x|^d by A11,A23,A30;
      hence thesis by A24,A30,A31;
    end;
    f5 is FinSequence of REAL by FINSEQ_3:126;
    then consider f6 being FinSequence of REAL such that
A32: len f6 = len f3 - 1 and
A33: for d st d in dom f6 holds f6.d = f4.d - f5.(d + 1) and
A34: Sum f3 = Sum f6 + f4.(n+1) - f5.1 by A4,A18,A11,A14,A22,A29,A17,Th5;
A35: len f6 <= len f3 by A4,A18,A22,A32,XREAL_1:147;
    then
A36: dom f6 c= dom f3 by FINSEQ_3:32;
A37: for d being Element of NAT st d in dom f6 holds f6.d = f1.(d+1)
    proof
      let d be Element of NAT;
A38:  dom f6 c= dom p by A3,A4,A18,A22,A35,FINSEQ_3:32;
      assume
A39:  d in dom f6;
      then
A40:  d in Seg n by A4,A18,A22,A32,FINSEQ_1:def 3;
      then
A41:  d <= n by FINSEQ_1:3;
      then
A42:  n-d >= 0 by XREAL_1:50;
      then reconsider d9=n-d+1 as Element of NAT by INT_1:16;
      d >= 1 by A40,FINSEQ_1:3;
      then n-d <= n-1 by XREAL_1:12;
      then d9 <= (n-1)+1 by XREAL_1:8;
      then
A43:  d9 < n+1 by XREAL_1:147;
      d9 >= 0+1 by A42,XREAL_1:8;
      then
A44:  p.(d9+1) = fp.(n+2-d9) +a*(p.d9) by A3,A43;
      d < n+1 by A41,XREAL_1:147;
      then
A45:  d+1 in Seg (n+1) by FINSEQ_3:12;
      then
A46:  d+1 in dom f5 by A14,FINSEQ_1:def 3;
      d+0 < n+2 by A41,XREAL_1:10;
      then d+1 in Seg (n+2) by FINSEQ_3:12;
      then
A47:  d+1 in dom f1 by A1,A5,FINSEQ_1:def 3;
A48:  d+1 in dom p by A3,A45,FINSEQ_1:def 3;
      thus f6.d = f4.d - f5.(d + 1) by A33,A39
        .= (fr.d)*x|^d - f5.(d+1) by A11,A23,A36,A39
        .= (fr.d)*x|^d - a*fr.(d+1) * x|^(d+1-'1) by A14,A46
        .= (fr.d)*x|^d - a*(fr.(d+1)) * x|^d by NAT_D:34
        .= ((fr.d) - a*fr.(d+1))* x|^d
        .= (p.((n+1)-d+1) - a*fr.(d+1))* x|^d by A3,A39,A38,FINSEQ_5:61
        .= (p.((n-d+1)+1)-a*p.((n+1)-(d+1)+1))* x|^d by A3,A48,FINSEQ_5:61
        .= fp.(d+1)* x|^(d+1-'1) by A44,NAT_D:34
        .= f1.(d+1) by A6,A47;
    end;
    f1 = <*f1.1*>^f6^<*f1.(n+2)*>
    proof
      set K = <*f1.1*>^f6^<*f1.(n+2)*>;
A49:  for d being Nat st d in dom f1 holds f1.d = K.d
      proof
        let d be Nat;
        assume
A50:    d in dom f1;
        then
A51:    d>=1 by FINSEQ_3:27;
A52:    d<=n+2 by A1,A5,A50,FINSEQ_3:27;
        per cases by A51,A52,XXREAL_0:1;
        suppose
A53:      d=1;
          hence K.d = (<*f1.1*>^(f6^<*f1.(n+2)*>)).1 by FINSEQ_1:45
            .= f1.d by A53,FINSEQ_1:58;
        end;
        suppose
A54:      d>1 & d<n+2;
          then d-1>0 by XREAL_1:52;
          then reconsider w=d-1 as Element of NAT by INT_1:16;
          d-1<n+2-1 by A54,XREAL_1:11;
          then
A55:      d-1<=n+1-1 by INT_1:20;
          d-1>=0+1 by A54,INT_1:20,XREAL_1:52;
          then w in Seg n by A55,FINSEQ_1:3;
          then
A56:      w in dom f6 by A4,A18,A22,A32,FINSEQ_1:def 3;
          then
A57:      w in dom (f6^<*f1.(n+2)*>) by FINSEQ_2:18;
          thus K.d = (<*f1.1*>^(f6^<*f1.(n+2)*>)).(w+1) by FINSEQ_1:45
            .= (f6^<*f1.(n+2)*>).w by A57,FINSEQ_3:112
            .= f6.w by A56,FINSEQ_1:def 7
            .= f1.(w+1) by A37,A56
            .= f1.d;
        end;
        suppose
A58:      d=n+2;
          set K1 = <*f1.1*>^f6;
          thus K.d = K.((n+1)+1) by A58
            .= K.(len K1 +1) by A4,A18,A22,A32,FINSEQ_5:8
            .= f1.d by A58,FINSEQ_1:59;
        end;
      end;
      len K = len (<*f1.1*>^(f6^<*f1.(n+2)*>)) by FINSEQ_1:45
        .= 1+len (f6^<*f1.(n+2)*>) by FINSEQ_5:8
        .= 1+len f6 +1 by FINSEQ_2:19
        .= len f1 by A1,A4,A5,A18,A22,A32;
      then dom f1 = dom K by FINSEQ_3:31;
      hence thesis by A49,FINSEQ_1:17;
    end;
    then Sum f1 = Sum (<*f1.1*>^(f6^<*f1.(n+2)*>)) by FINSEQ_1:45
      .= f1.1 + Sum (f6^<*f1.(n+2)*>) by RVSUM_1:106
      .= f1.1 + (Sum f6 + f1.(n+2)) by RVSUM_1:104
      .= Sum ((x-a)*f2) + r by A10,A9,A13,A16,A34
      .= (x-a) * (Poly-INT fr).x + r by A20,RVSUM_1:117;
    hence thesis by A7;
  end;
  hence thesis by A3,FINSEQ_5:65,def 3;
end;

theorem Th7:
  p divides i*j implies p divides i or p divides j
proof
  assume
A1: p divides i*j;
  per cases;
  suppose
    i>=0 & j>=0;
    then reconsider i,j as Element of NAT by INT_1:16;
    p divides i*j by A1;
    hence thesis by NEWTON:98;
  end;
  suppose
    i>=0 & j<0;
    then reconsider i,j9=-j as Element of NAT by INT_1:16;
    p divides -(i*j) by A1,INT_2:14;
    then p divides i*j9;
    then p divides i or p divides j9 by NEWTON:98;
    hence thesis by INT_2:14;
  end;
  suppose
    i<0 & j>=0;
    then reconsider i9=-i,j as Element of NAT by INT_1:16;
    p divides -(i*j) by A1,INT_2:14;
    then p divides i9*j;
    then p divides i9 or p divides j by NEWTON:98;
    hence thesis by INT_2:14;
  end;
  suppose
    i<0 & j<0;
    then reconsider i9=-i,j9=-j as Element of NAT by INT_1:16;
    p divides i9*j9 by A1;
    then p divides i9 or p divides j9 by NEWTON:98;
    hence thesis by INT_2:14;
  end;
end;

reserve fr,f for FinSequence of INT;

theorem Th8:
  for fp st len fp = n+1 & p>2 & not p divides fp.(n+1) holds for
fr st (for d st d in dom fr holds (Poly-INT fp).(fr.d) mod p=0) & (for d,e st d
  in dom fr & e in dom fr & d<>e holds not fr.d,fr.e are_congruent_mod p) holds
  len fr <= n
proof
  defpred P[Nat] means for fp st len fp = $1+1 & p>2 & not p divides fp.($1+1)
holds for fr st (for d st d in dom fr holds (Poly-INT fp).(fr.d) mod p = 0) & (
  for d,e st d in dom fr & e in dom fr & d<>e holds not fr.d,fr.e
  are_congruent_mod p) holds len fr <= $1;
A1: for n st P[n] holds P[n+1]
  proof
    let n;
    assume
A2: P[n];
    let fp;
    assume that
A3: len fp = (n+1)+1 and
A4: p>2 and
A5: not p divides fp.(n+1+1);
    per cases;
    suppose
A6:   for x holds (Poly-INT fp).x mod p <> 0;
      assume ex fr st (for d st d in dom fr holds (Poly-INT fp).(fr.d) mod p
      =0) & (for d,e st d in dom fr & e in dom fr & d<>e holds not fr.d,fr.e
      are_congruent_mod p) & len fr > n+1;
      then consider fr such that
A7:   for d st d in dom fr holds (Poly-INT fp).(fr.d) mod p=0 and
      for d,e st d in dom fr & e in dom fr & d<>e holds not fr.d,fr.e
      are_congruent_mod p and
A8:   len fr > n+1;
      fr <> {} by A8;
      then (Poly-INT fp).(fr.1) mod p=0 by A7,FINSEQ_5:6;
      hence contradiction by A6;
    end;
    suppose
      ex a being Integer st (Poly-INT fp).a mod p = 0;
      then consider a being Integer such that
A9:   (Poly-INT fp).a mod p=0;
      assume ex f st (for d st d in dom f holds (Poly-INT fp).(f.d) mod p =
      0) & (for d,e st d in dom f & e in dom f & d<>e holds not f.d,f.e
      are_congruent_mod p) & len f > n+1;
      then consider f such that
A10:  for d st d in dom f holds (Poly-INT fp).(f.d) mod p = 0 and
A11:  for d,e st d in dom f & e in dom f & d<>e holds not f.d,f.e
      are_congruent_mod p and
A12:  len f > n+1;
      consider fk,r such that
A13:  len fk = n+1 and
A14:  for x being Element of INT holds (Poly-INT fp).x =(x-a)*(
      Poly-INT fk).x +r and
A15:  fp.(n+2)=fk.(n+1) by A3,Th6;
      a is Element of INT by INT_1:def 2;
      then
A16:  (Poly-INT fp).a mod p =((a-a)*(Poly-INT fk).a +r) mod p by A14
        .= r mod p;
A17:  for d being Element of NAT st d in dom f holds p divides (f.d -a)*(
      Poly-INT fk).(f.d)
      proof
        let d be Element of NAT;
        f.d is Element of INT by INT_1:def 2;
        then
A18:    (Poly-INT fp).(f.d) mod p = (((f.d-a)*(Poly-INT fk).(f.d)) +r)
        mod p by A14
          .=(((f.d-a)*(Poly-INT fk).(f.d)) mod p + (r mod p)) mod p by INT_3:14
          .= ((f.d-a)*(Poly-INT fk).(f.d)) mod p by A9,A16,INT_3:13;
        assume d in dom f;
        then ((f.d-a)*(Poly-INT fk).(f.d)) mod p = 0 by A10,A18;
        hence thesis by INT_1:89;
      end;
      per cases;
      suppose
A19:    for d st d in dom f holds not p divides (f.d - a);
        for d st d in dom f holds (Poly-INT fk).(f.d) mod p = 0
        proof
          let d;
          assume
A20:      d in dom f;
          then p divides (f.d -a)*(Poly-INT fk).(f.d) by A17;
          then p divides (f.d -a) or p divides (Poly-INT fk).(f.d) by Th7;
          hence thesis by A19,A20,INT_1:89;
        end;
        then len f <= n by A2,A4,A5,A13,A15,A11;
        hence contradiction by A12,XREAL_1:147;
      end;
      suppose
        ex d st d in dom f & p divides (f.d - a);
        then consider d9 being Element of NAT such that
A21:    d9 in dom f and
A22:    p divides (f.d9 - a);
        set f9 = f - {f.d9};
A23:    for d st d in dom f9 holds not p divides (f9.d - a)
        proof
          given k being Nat such that
A24:      k in dom f9 and
A25:      p divides (f9.k -a);
          f9.k in rng f9 by A24,FUNCT_1:12;
          then
A26:      f9.k in rng f \ {f.d9} by FINSEQ_3:72;
          then f9.k in rng f by XBOOLE_0:def 5;
          then consider w being set such that
A27:      w in dom f and
A28:      f.w = f9.k by FUNCT_1:def 5;
          reconsider w as Element of NAT by A27;
          p divides ((f.w - a)-(f.d9 - a)) by A22,A25,A28,Th1;
          then p divides (f.w - f.d9);
          then
A29:      f.w,f.d9 are_congruent_mod p by INT_2:19;
          not f9.k in {f.d9} by A26,XBOOLE_0:def 5;
          then w <> d9 by A28,TARSKI:def 1;
          hence contradiction by A11,A21,A27,A29;
        end;
A30:    for d st d in dom f9 holds (Poly-INT fk).(f9.d) mod p = 0
        proof
          let d;
          assume
A31:      d in dom f9;
          then f9.d in rng f9 by FUNCT_1:12;
          then f9.d in rng f \ {f.d9} by FINSEQ_3:72;
          then f9.d in rng f by XBOOLE_0:def 5;
          then ex w being set st w in dom f & f.w = f9.d by FUNCT_1:def 5;
          then p divides (f9.d -a)*(Poly-INT fk).(f9.d) by A17;
          then p divides (f9.d -a) or p divides (Poly-INT fk).(f9.d) by Th7;
          hence thesis by A23,A31,INT_1:89;
        end;
A32:    f is one-to-one
        proof
          let x1,x2 be set;
          assume that
A33:      x1 in dom f and
A34:      x2 in dom f and
A35:      f.x1 = f.x2 and
A36:      x1 <> x2;
          thus contradiction by A11,A33,A34,A35,A36,INT_1:32;
        end;
        then
A37:    f9 is one-to-one by FINSEQ_3:94;
A38:    for d,e st d in dom f9 & e in dom f9 & d<>e holds not f9.d,f9.e
        are_congruent_mod p
        proof
          let d,e;
          assume that
A39:      d in dom f9 and
A40:      e in dom f9 and
A41:      d<>e;
          f9.e in rng f9 by A40,FUNCT_1:12;
          then f9.e in rng f \ {f.d9} by FINSEQ_3:72;
          then f9.e in rng f by XBOOLE_0:def 5;
          then consider w2 being set such that
A42:      w2 in dom f and
A43:      f9.e = f.w2 by FUNCT_1:def 5;
          f9.d in rng f9 by A39,FUNCT_1:12;
          then f9.d in rng f \ {f.d9} by FINSEQ_3:72;
          then f9.d in rng f by XBOOLE_0:def 5;
          then consider w1 being set such that
A44:      w1 in dom f and
A45:      f9.d = f.w1 by FUNCT_1:def 5;
          reconsider w1,w2 as Element of NAT by A44,A42;
          w1 <> w2 by A37,A39,A40,A41,A45,A43,FUNCT_1:def 8;
          hence thesis by A11,A44,A45,A42,A43;
        end;
        f.d9 in rng f by A21,FUNCT_1:12;
        then len f9 = len f - 1 by A32,FINSEQ_3:97;
        then len f9 > (n+1)-1 by A12,XREAL_1:11;
        hence contradiction by A2,A4,A5,A13,A15,A30,A38;
      end;
    end;
  end;
A46: P[0]
  proof
    let fp;
    assume that
A47: len fp = 0+1 and
    p>2 and
A48: not p divides fp.(0+1);
    assume ex fr st (for d st d in dom fr holds (Poly-INT fp).(fr.d) mod p=0)
    & (for d,e st d in dom fr & e in dom fr & d<>e holds not fr.d,fr.e
    are_congruent_mod p) & len fr > 0;
    then consider fr such that
A49: for d st d in dom fr holds (Poly-INT fp).(fr.d) mod p=0 and
    for d,e st d in dom fr & e in dom fr & d<>e holds not fr.d,fr.e
    are_congruent_mod p and
A50: len fr > 0;
    fr <> {} by A50;
    then
A51: (Poly-INT fp).(fr.1) mod p=0 by A49,FINSEQ_5:6;
A52: fr.1 in INT by INT_1:def 2;
    (Poly-INT fp).(fr.1) = (INT --> fp.1).(fr.1) by A47,Th3
      .= fp.1 by A52,FUNCOP_1:13;
    hence contradiction by A48,A51,Lm1;
  end;
  for n holds P[n] from NAT_1:sch 2(A46,A1);
  hence thesis;
end;

definition
  let a be Integer, m be Nat;
  pred a is_quadratic_residue_mod m means
  :Def2:
  ex x being Integer st (x^2 - a) mod m = 0;
end;

reserve b,m for Nat;

theorem Th9:
  a^2 is_quadratic_residue_mod m
proof
  (a^2 - a^2) mod m = 0 by INT_4:12;
  hence thesis by Def2;
end;

theorem
  1 is_quadratic_residue_mod 2
proof
  1^2 is_quadratic_residue_mod 2 by Th9;
  hence thesis;
end;

theorem Th11:
  i is_quadratic_residue_mod m & i,j are_congruent_mod m implies j
  is_quadratic_residue_mod m
proof
  assume that
A1: i is_quadratic_residue_mod m and
A2: i,j are_congruent_mod m;
  consider x being Integer such that
A3: (x^2 - i) mod m = 0 by A1,Def2;
  m divides (i - j) by A2,INT_2:19;
  then
A4: (i - j) mod m = 0 by Lm1;
  (x^2 - j) mod m = ((x^2 - i) + (i - j)) mod m
    .= (((x^2 - i) mod m) + ((i - j) mod m)) mod m by INT_3:14
    .= 0 by A3,A4,INT_3:13;
  hence thesis by Def2;
end;

Lm2: i,p are_relative_prime or p divides i
proof
  per cases;
  suppose
    i>=0;
    then reconsider i as Element of NAT by INT_1:16;
    i,p are_relative_prime or (i gcd p) = p by PEPIN:2;
    hence thesis by NAT_D:def 5;
  end;
  suppose
A1: i<0;
    then reconsider m = -i as Element of NAT by INT_1:16;
A2: m,p are_relative_prime or (m gcd p) = p by PEPIN:2;
    per cases by A2,NAT_D:def 5;
    suppose
A3:   m,p are_relative_prime;
      m = abs i by A1,ABSVALUE:def 1;
      then i gcd p = m gcd abs(p) by INT_2:51
        .= m gcd p by ABSVALUE:def 1
        .= 1 by A3,INT_2:def 4;
      hence thesis by INT_2:def 4;
    end;
    suppose
      p divides m;
      then consider t being Nat such that
A4:   m = p * t by NAT_D:def 3;
      i = p * (-t) by A4;
      hence thesis by INT_1:def 9;
    end;
  end;
end;

theorem Th12:
  i divides j implies i gcd j = abs(i)
proof
  assume i divides j;
  then abs(i) divides abs(j) by INT_2:21;
  then abs i gcd abs j = abs(i) by NEWTON:62;
  hence thesis by INT_2:51;
end;

theorem Th13:
  for i,j,m being Integer st i mod m = j mod m holds i|^n mod m = j|^n mod m
proof
  let i,j,m be Integer;
  defpred P[Nat] means i|^$1 mod m = j|^$1 mod m;
  assume
A1: i mod m = j mod m;
A2: for n being Element of NAT st P[n] holds P[n+1]
  proof
    let n be Element of NAT;
    assume
A3: P[n];
    thus i|^(n+1) mod m = ((i|^n)*i) mod m by NEWTON:11
      .= (((j|^n) mod m)*(j mod m)) mod m by A1,A3,INT_3:15
      .= ((j|^n)*j) mod m by INT_3:15
      .= j|^(n+1) mod m by NEWTON:11;
  end;
  i|^0 = 1 by NEWTON:9;
  then
A4: P[0] by NEWTON:9;
A5: for n being Element of NAT holds P[n] from NAT_1:sch 1(A4,A2);
  for n holds P[n]
  proof
    let n;
    n in NAT by ORDINAL1:def 13;
    hence thesis by A5;
  end;
  hence thesis;
end;

theorem Th14:
  a gcd p = 1 & (x^2 - a) mod p = 0 implies x,p are_relative_prime
proof
  assume that
A1: a gcd p = 1 and
A2: (x^2 - a) mod p = 0;
  assume not x,p are_relative_prime;
  then
A3: p divides x^2 by Lm2,INT_2:2;
  p divides (x^2 - a) by A2,Lm1;
  then p divides (x^2 - (x^2 - a)) by A3,Th1;
  then p gcd a = abs(p) by Th12
    .= p by ABSVALUE:def 1;
  hence contradiction by A1,INT_2:def 5;
end;

theorem
  p > 2 & a gcd p = 1 & a is_quadratic_residue_mod p implies ex x,y
  being Integer st (x^2 - a) mod p = 0 & (y^2 - a) mod p = 0 & not x,y
  are_congruent_mod p
proof
  assume that
A1: p > 2 and
A2: a gcd p = 1 and
A3: a is_quadratic_residue_mod p;
  consider x such that
A4: (x^2 - a) mod p = 0 by A3,Def2;
  take x;
  take -x;
  not x,(-x) are_congruent_mod p
  proof
    assume x,(-x) are_congruent_mod p;
    then p divides (x - (-x)) by INT_2:19;
    then
A5: p divides 2*x;
    2,p are_relative_prime by A1,INT_2:44,47;
    then 2 gcd p = 1 by INT_2:def 4;
    then p divides x by A5,WSIERP_1:36;
    then consider i being Integer such that
A6: x = p * i by INT_1:def 9;
    x gcd p = p*i gcd p*1 by A6
      .= p*(i gcd 1) by EULER_1:16
      .= p*1 by WSIERP_1:13;
    then x gcd p <> 1 by INT_2:def 5;
    then not x,p are_relative_prime by INT_2:def 4;
    hence contradiction by A2,A4,Th14;
  end;
  hence thesis by A4;
end;

registration
  let fp be FinSequence of NAT, d;
  cluster fp.d -> natural;
  coherence;
end;

theorem Th16:
  p>2 implies ex fp being FinSequence of NAT st len fp = (p-'1)
  div 2 & (for d st d in dom fp holds fp.d gcd p = 1) & (for d st d in dom fp
holds fp.d is_quadratic_residue_mod p) & for d,e st d in dom fp & e in dom fp &
  d<>e holds not fp.d,fp.e are_congruent_mod p
proof
  deffunc F(Nat) = $1^2;
  consider fp being FinSequence such that
A1: len fp = (p-'1) div 2 & for d being Nat st d in dom fp holds fp.d =
  F(d) from FINSEQ_1:sch 2;
  for d being Nat st d in dom fp holds fp.d in NAT
  proof
    let d be Nat;
    assume d in dom fp;
    then fp.d = d^2 by A1;
    hence thesis;
  end;
  then reconsider fp as FinSequence of NAT by FINSEQ_2:14;
A2: p>1 by INT_2:def 5;
  then
A3: p-'1 = p-1 by XREAL_1:235;
  assume p > 2;
  then p is odd by PEPIN:17;
  then p-1 is even by HILBERT3:2;
  then 2 divides (p-'1) by A3,PEPIN:22;
  then (p-'1) mod 2 = 0 by PEPIN:6;
  then
A4: (p-'1) div 2 = (p-'1)/2 by PEPIN:68;
A5: for d,e st d in dom fp & e in dom fp & d<>e holds not fp.d,fp.e
  are_congruent_mod p
  proof
    p-1>0 by A2,XREAL_1:52;
    then (p-1)/2 < (p-1)/1 by XREAL_1:78;
    then (p-'1) div 2 < p by A3,A4,XREAL_1:149;
    then
A6: ((p-'1) div 2) - 1 < p by XREAL_1:149;
    let d,e;
    assume that
A7: d in dom fp and
A8: e in dom fp and
A9: d<>e;
A10: e in Seg ((p-'1) div 2) by A1,A8,FINSEQ_1:def 3;
    then
A11: e <= (p-'1) div 2 by FINSEQ_1:3;
A12: d in Seg ((p-'1) div 2) by A1,A7,FINSEQ_1:def 3;
    then
A13: d >= 1 by FINSEQ_1:3;
    then 1-((p-'1) div 2) <= d-e by A11,XREAL_1:15;
    then
A14: -(((p-'1) div 2) - 1) <= d-e;
A15: d <= (p-'1) div 2 by A12,FINSEQ_1:3;
    then d+e <= (p-'1) div 2 + ((p-'1) div 2) by A11,XREAL_1:9;
    then d+e < p by A3,A4,XREAL_1:149;
    then (d+e),p are_relative_prime by A13,EULER_1:3;
    then
A16: (d+e) gcd p = 1 by INT_2:def 4;
    assume fp.d,fp.e are_congruent_mod p;
    then p divides (fp.d - fp.e) by INT_2:19;
    then p divides (d^2 - fp.e) by A1,A7;
    then p divides (d^2 - e^2) by A1,A8;
    then
A17: p divides (d - e)*(d+e);
    d-e <> 0 by A9;
    then abs(p) <= abs(d-e) by A16,A17,INT_4:6,WSIERP_1:36;
    then
A18: p <= abs(d-e) by ABSVALUE:def 1;
    e >= 1 by A10,FINSEQ_1:3;
    then d-e <= ((p-'1) div 2) - 1 by A15,XREAL_1:15;
    then abs(d-e) <= ((p-'1) div 2) - 1 by A14,ABSVALUE:12;
    hence contradiction by A18,A6,XXREAL_0:2;
  end;
A19: for d st d in dom fp holds d gcd p = 1
  proof
    let d;
A20: 1*d <= 2*d by XREAL_1:66;
    assume d in dom fp;
    then
A21: d in Seg ((p-'1) div 2) by A1,FINSEQ_1:def 3;
    then d <= (p-'1) div 2 by FINSEQ_1:3;
    then 2*d <= (p-'1)/2*2 by A4,XREAL_1:66;
    then d <= p-'1 by A20,XXREAL_0:2;
    then
A22: d < p by A3,XREAL_1:149;
    d >= 1 by A21,FINSEQ_1:3;
    then d,p are_relative_prime by A22,EULER_1:3;
    hence thesis by INT_2:def 4;
  end;
A23: for d st d in dom fp holds fp.d gcd p = 1
  proof
    let d;
    assume
A24: d in dom fp;
    then d gcd p = 1 by A19;
    then d^2 gcd p = 1 by WSIERP_1:12;
    hence thesis by A1,A24;
  end;
  take fp;
  for d st d in dom fp holds fp.d is_quadratic_residue_mod p
  proof
    let d;
    assume
A25: d in dom fp;
    d^2 is_quadratic_residue_mod p by Th9;
    hence thesis by A1,A25;
  end;
  hence thesis by A1,A23,A5;
end;
 
::Euler Criterion

theorem Th17:
  p > 2 & a gcd p = 1 & a is_quadratic_residue_mod p implies a|^((
  p-'1) div 2) mod p = 1
proof
  assume that
A1: p > 2 and
A2: a gcd p = 1 and
A3: a is_quadratic_residue_mod p;
  consider s being Integer such that
A4: (s^2 - a) mod p = 0 by A3,Def2;
A5: p>1 by INT_2:def 5;
  p is odd by A1,PEPIN:17;
  then p-1 is even by HILBERT3:2;
  then p-'1 is even by A5,XREAL_1:235;
  then 2 divides (p-'1) by PEPIN:22;
  then
A6: p-'1 = 2*((p-'1) div 2) by NAT_D:3;
  p divides (s^2 - a) by A4,INT_1:89;
  then s^2,a are_congruent_mod p by INT_2:19;
  then a mod p = s^2 mod p by INT_3:12;
  then
A7: a|^((p-'1) div 2) mod p = (s^2)|^((p-'1) div 2) mod p by Th13
    .= (s|^2)|^((p-'1) div 2) mod p by NEWTON:100
    .= s|^(p-'1) mod p by A6,NEWTON:14;
A8: s,p are_relative_prime by A2,A4,Th14;
  per cases;
  suppose
    s>=0;
    then reconsider s as Element of NAT by INT_1:16;
    s,p are_relative_prime by A2,A4,Th14;
    hence thesis by A7,PEPIN:38;
  end;
  suppose
A9: s<0;
    then reconsider s9 = -s as Element of NAT by INT_1:16;
A10: abs p = p by ABSVALUE:def 1;
    s9 gcd p = abs(s) gcd p by A9,ABSVALUE:def 1
      .= s gcd p by A10,INT_2:51
      .= 1 by A8,INT_2:def 4;
    then s9,p are_relative_prime by INT_2:def 4;
    then
A11: s9|^(p-'1) mod p = 1 by PEPIN:38;
    s|^(p-'1) mod p = (s|^2)|^((p-'1) div 2) mod p by A6,NEWTON:14
      .= ((-s)|^2)|^((p-'1) div 2) mod p by WSIERP_1:2
      .= 1 by A6,A11,NEWTON:14;
    hence thesis by A7;
  end;
end;

theorem Th18:
  p>2 & b gcd p = 1 & not b is_quadratic_residue_mod p implies b|^
  ((p-'1) div 2) mod p = p - 1
proof
  assume that
A1: p>2 and
A2: b gcd p = 1 and
A3: not b is_quadratic_residue_mod p;
  reconsider b as Element of NAT by ORDINAL1:def 13;
A4: p>1 by INT_2:def 5;
  then
A5: 1 mod p = 1 by NAT_D:14;
  p is odd by A1,PEPIN:17;
  then p-1 is even by HILBERT3:2;
  then p-'1 is even by A4,XREAL_1:235;
  then 2 divides (p-'1) by PEPIN:22;
  then p-'1 = 2*((p-'1) div 2) by NAT_D:3;
  then
A6: b|^(p-'1) - 1 = (b|^((p-'1) div 2))|^2 - 1 by NEWTON:14
    .= (b|^((p-'1) div 2))^2 - 1 by NEWTON:100
    .= (b|^((p-'1) div 2) +1)*(b|^((p-'1) div 2)-1);
  b,p are_relative_prime by A2,INT_2:def 4;
  then (b|^(p-'1)) mod p = 1 by PEPIN:38;
  then (b|^(p-'1) - 1) mod p = 0 by A5,INT_4:22;
  then
A7: p divides (b|^((p-'1) div 2) +1)*(b|^((p-'1) div 2)-1) by A6,Lm1;
  p-1 > 2-1 by A1,XREAL_1:11;
  then p-1 >= 1 + 1 by INT_1:20;
  then p-'1 >= 2 by A4,XREAL_1:235;
  then (p-'1) div 2 >= 2 div 2 by NAT_2:26;
  then
A8: (p-'1) div 2 >= 1 by PEPIN:48;
  per cases by A8,XXREAL_0:1;
  suppose
A9: (p-'1) div 2 = 1;
A10: now
      assume p divides (b - 1);
      then p divides -(b - 1) by INT_2:14;
      then (1^2 - b) mod p = 0 by Lm1;
      hence contradiction by A3,Def2;
    end;
    p divides (b +1)*(b|^1 -1) by A7,A9,NEWTON:10;
    then p divides (b +1)*(b -1) by NEWTON:10;
    then p divides (b - (-1)) by A10,Th7;
    then b,(-1) are_congruent_mod p by INT_2:19;
    then
A11: b mod p = (-1) mod p by INT_3:12;
    -p < -2 by A1,XREAL_1:26;
    then -p < -2+1 by XREAL_1:41;
    then b mod p = p - 1 by A11,INT_3:10;
    hence thesis by A9,NEWTON:10;
  end;
  suppose
A12: (p-'1) div 2 > 1;
    set l = (p-'1) div 2;
    0 is Element of INT by INT_1:def 2;
    then
A13: (l-'1) |-> 0 is FinSequence of INT by FINSEQ_2:77;
    set K1 = <*-1*>^((l-'1) |-> 0);
A14: len ((l-'1) |-> 0) = l-'1 by FINSEQ_1:def 18;
    then
A15: len K1 = 1 + (l-'1) by FINSEQ_5:8
      .= l by A12,XREAL_1:237;
    set fs = <*-1*>^((l-'1) |-> 0)^<*1*>;
    1 is Element of INT by INT_1:def 2;
    then
A16: <*1*> is FinSequence of INT by FINSEQ_1:95;
    -1 is Element of INT by INT_1:def 2;
    then <*-1*> is FinSequence of INT by FINSEQ_1:95;
    then <*-1*>^((l-'1) |-> 0) is FinSequence of INT by A13,FINSEQ_1:96;
    then reconsider fs as FinSequence of INT by A16,FINSEQ_1:96;
A17: len fs = len (<*-1*>^(((l-'1) |-> 0)^<*1*>)) by FINSEQ_1:45
      .= 1 + len (((l-'1) |-> 0)^<*1*>) by FINSEQ_5:8
      .= 1 + len ((l-'1) |-> 0) + 1 by FINSEQ_2:19
      .= 1 + ((l-'1) +1) by FINSEQ_1:def 18
      .= 1 + l by A12,XREAL_1:237;
A18: fs.1 = (<*-1*>^(((l-'1) |-> 0)^<*1*>)).1 by FINSEQ_1:45
      .= -1 by FINSEQ_1:58;
A19: for x being Element of INT holds (Poly-INT fs).x = x|^l -1
    proof
      let x be Element of INT;
      consider fr such that
A20:  len fr = len fs and
A21:  for d st d in dom fr holds fr.d = (fs.d) * x|^(d-'1) and
A22:  (Poly-INT fs).x = Sum fr by Def1;
      fr = <*-1*>^((l-'1) |-> 0)^<*x|^l*>
      proof
        set K = <*-1*>^((l-'1) |-> 0)^<*x|^l*>;
A23:    for d being Nat st d in dom fr holds fr.d = K.d
        proof
          let d be Nat;
          assume
A24:      d in dom fr;
          then
A25:      d in Seg(l + 1) by A17,A20,FINSEQ_1:def 3;
          then
A26:      d>=1 by FINSEQ_1:3;
A27:      d<=l+1 by A25,FINSEQ_1:3;
          per cases by A26,A27,XXREAL_0:1;
          suppose
A28:        d=1;
            then
A29:        fr.1 = (fs.1) * x|^(1-'1) by A21,A24
              .= (fs.1) * x|^0 by XREAL_1:234
              .= (fs.1) * 1 by NEWTON:9
              .= -1 by A18;
            K.1 = (<*-1*>^(((l-'1) |-> 0)^<*x|^l*>)).1 by FINSEQ_1:45
              .= fr.1 by A29,FINSEQ_1:58;
            hence thesis by A28;
          end;
          suppose
A30:        d>1 & d<l+1;
            then d-1>0 by XREAL_1:52;
            then reconsider w = d-1 as Element of NAT by INT_1:16;
            d-1<l+1-1 by A30,XREAL_1:11;
            then
A31:        w <= l-'1 by NAT_D:49;
A32:        w >= 0+1 by A30,INT_1:20,XREAL_1:52;
            then
A33:        ((l-'1) |-> 0).w = 0 by A31,GOBRD10:def 3;
            w in Seg(l-'1) by A31,A32,FINSEQ_1:3;
            then
A34:        w in dom ((l-'1) |-> 0) by A14,FINSEQ_1:def 3;
            then
A35:        w in dom (((l-'1) |-> 0)^<*1*>) by FINSEQ_2:18;
A36:        w in dom (((l-'1) |-> 0)^<*x|^l*>) by A34,FINSEQ_2:18;
A37:        fs.d = (<*-1*>^(((l-'1) |-> 0)^<*1*>)).(w+1) by FINSEQ_1:45
              .= (((l-'1) |-> 0)^<*1*>).w by A35,FINSEQ_3:112
              .= 0 by A33,A34,FINSEQ_1:def 7;
            thus K.d = (<*-1*>^(((l-'1) |-> 0)^<*x|^l*>)).(w+1) by FINSEQ_1:45
              .= (((l-'1) |-> 0)^<*x|^l*>).w by A36,FINSEQ_3:112
              .= (fs.d)*x|^(d-'1) by A33,A34,A37,FINSEQ_1:def 7
              .= fr.d by A21,A24;
          end;
          suppose
A38:        d=l+1;
            then d in dom fs by A17,FINSEQ_5:6;
            then
A39:        d in dom fr by A20,FINSEQ_3:31;
            fs.d = 1 by A15,A38,FINSEQ_1:59;
            hence fr.d = 1 * x|^(l+1-'1) by A21,A38,A39
              .= x|^l by NAT_D:34
              .= K.d by A15,A38,FINSEQ_1:59;
          end;
        end;
        len K = len (<*-1*>^(((l-'1) |-> 0)^<*x|^l*>)) by FINSEQ_1:45
          .= 1 + len (((l-'1) |-> 0)^<*x|^l*>) by FINSEQ_5:8
          .= 1 + len ((l-'1) |-> 0) + 1 by FINSEQ_2:19
          .= 1 + ((l-'1) +1) by FINSEQ_1:def 18
          .= len fr by A12,A17,A20,XREAL_1:237;
        then dom fr = dom K by FINSEQ_3:31;
        hence thesis by A23,FINSEQ_1:17;
      end;
      then Sum fr = Sum (<*-1*>^(((l-'1) |-> 0)^<*x|^l*>)) by FINSEQ_1:45
        .= -1 + Sum (((l-'1) |-> 0)^<*x|^l*>) by RVSUM_1:106
        .= -1 + (Sum ((l-'1) |-> 0) + x|^l) by RVSUM_1:104
        .= -1 + ((l-'1)*0 + x|^l) by RVSUM_1:110;
      hence thesis by A22;
    end;
    consider fp being FinSequence of NAT such that
A40: len fp = l and
A41: for d st d in dom fp holds fp.d gcd p = 1 and
A42: for d st d in dom fp holds fp.d is_quadratic_residue_mod p and
A43: for d,e st d in dom fp & e in dom fp & d<>e holds not fp.d,fp.e
    are_congruent_mod p by A1,Th16;
A44: fs.(l+1) = 1 by A15,FINSEQ_1:59;
    now
      assume p divides (b|^l - 1);
      then
A45:  (b|^l - 1) mod p = 0 by Lm1;
      reconsider b as Element of INT by INT_1:def 2;
      set f = fp^<*b*>;
      <*b*> is FinSequence of NAT by FINSEQ_1:95;
      then reconsider f as FinSequence of NAT by FINSEQ_1:96;
A46:  len f = l+1 by A40,FINSEQ_2:19;
A47:  for d,e st d in dom f & e in dom f & d<>e holds not f.d,f.e
      are_congruent_mod p
      proof
        let d,e;
        assume that
A48:    d in dom f and
A49:    e in dom f and
A50:    d<>e;
A51:    e>=1 by A49,FINSEQ_3:27;
A52:    d<= l+1 by A46,A48,FINSEQ_3:27;
A53:    e<=l+1 by A46,A49,FINSEQ_3:27;
        per cases by A48,A52,FINSEQ_3:27,XXREAL_0:1;
        suppose
A54:      d>=1 & d<l+1;
          then d<=l by NAT_1:13;
          then
A55:      d in dom fp by A40,A54,FINSEQ_3:27;
          then
A56:      f.d = fp.d by FINSEQ_1:def 7;
          per cases by A49,A53,FINSEQ_3:27,XXREAL_0:1;
          suppose
A57:        e>=1 & e<l+1;
            then e<=l by NAT_1:13;
            then
A58:        e in dom fp by A40,A57,FINSEQ_3:27;
            then not fp.d,fp.e are_congruent_mod p by A43,A50,A55;
            hence thesis by A56,A58,FINSEQ_1:def 7;
          end;
          suppose
A59:        e=l+1;
            not f.d,b are_congruent_mod p
            proof
              f.d is_quadratic_residue_mod p by A42,A55,A56;
              then consider j being Integer such that
A60:          (j^2 - f.d) mod p = 0 by Def2;
              assume f.d,b are_congruent_mod p;
              then
A61:          p divides (f.d - b) by INT_2:19;
              p divides (j^2 - f.d) by A60,INT_1:89;
              then p divides ((j^2 - f.d)+(f.d - b)) by A61,WSIERP_1:9;
              then (j^2 - b) mod p = 0 by INT_1:89;
              hence contradiction by A3,Def2;
            end;
            hence thesis by A40,A59,FINSEQ_1:59;
          end;
        end;
        suppose
A62:      d=l+1;
          then e<=l by A50,A53,NAT_1:8;
          then
A63:      e in dom fp by A40,A51,FINSEQ_3:27;
          then f.e = fp.e by FINSEQ_1:def 7;
          then f.e is_quadratic_residue_mod p by A42,A63;
          then consider j being Integer such that
A64:      (j^2 - f.e) mod p = 0 by Def2;
A65:      p divides (j^2 - f.e) by A64,INT_1:89;
          not b,f.e are_congruent_mod p
          proof
            assume b,f.e are_congruent_mod p;
            then p divides (b - f.e) by INT_2:19;
            then p divides ((j^2 - f.e) - (b - f.e)) by A65,Th1;
            then (j^2 - b) mod p = 0 by INT_1:89;
            hence contradiction by A3,Def2;
          end;
          hence thesis by A40,A62,FINSEQ_1:59;
        end;
      end;
A66:  (Poly-INT fs).b mod p = 0 by A19,A45;
A67:  for d st d in dom f holds (Poly-INT fs).(f.d) mod p = 0
      proof
        let d;
        assume d in dom f;
        then
A68:    d in Seg (l+1) by A46,FINSEQ_1:def 3;
        then
A69:    d<=l+1 by FINSEQ_1:3;
        per cases by A68,A69,FINSEQ_1:3,XXREAL_0:1;
        suppose
A70:      d>=1 & d<l+1;
          reconsider k = fp.d as Element of INT by INT_1:def 2;
          d<=l by A70,NAT_1:13;
          then
A71:      d in dom fp by A40,A70,FINSEQ_3:27;
          then fp.d gcd p = 1 by A41;
          then (fp.d)|^l mod p = 1 mod p by A1,A5,A42,A71,Th17;
          then (k|^l - 1) mod p = 0 by INT_4:22;
          then (Poly-INT fs).k mod p = 0 by A19;
          hence thesis by A71,FINSEQ_1:def 7;
        end;
        suppose
          d=l+1;
          hence thesis by A40,A66,FINSEQ_1:59;
        end;
      end;
      reconsider f as FinSequence of INT by FINSEQ_2:27,NUMBERS:17;
      not p divides fs.(l+1) by A4,A44,NAT_D:7;
      then len f <= l by A1,A17,A67,A47,Th8;
      hence contradiction by A46,XREAL_1:31;
    end;
    then p divides (b|^l + 1) by A7,Th7;
    then consider k being Nat such that
A72: (b|^l + 1) = p*k by NAT_D:def 3;
    -p < -1 by A4,XREAL_1:26;
    then
A73: (-1) mod p = (-1) + p by INT_3:10;
    b|^l mod p = (p*k + (- 1)) mod p by A72
      .= p - 1 by A73,INT_3:8;
    hence thesis;
  end;
end;

theorem Th19:
  p>2 & a gcd p = 1 & not a is_quadratic_residue_mod p implies a|^
  ((p-'1) div 2) mod p = p - 1
proof
  assume that
A1: p>2 and
A2: a gcd p = 1 and
A3: not a is_quadratic_residue_mod p;
  set l = a mod p;
  reconsider l as Element of NAT by INT_1:16,84;
A4: l mod p = a mod p by INT_3:13;
  then
A5: l,a are_congruent_mod p by INT_3:12;
  then l gcd p = 1 by A2,INT_4:14;
  then l|^((p-'1) div 2) mod p = p - 1 by A1,A3,A5,Th11,Th18;
  hence thesis by A4,Th13;
end;

theorem Th20:
  p > 2 & a gcd p = 1 & a is_quadratic_residue_mod p implies (a|^(
  (p-'1) div 2) - 1) mod p = 0
proof
  assume that
A1: p > 2 and
A2: a gcd p = 1 and
A3: a is_quadratic_residue_mod p;
A4: p>1 by INT_2:def 5;
  a|^((p-'1) div 2) mod p = 1 by A1,A2,A3,Th17;
  then a|^((p-'1) div 2) mod p = 1 mod p by A4,NAT_D:14;
  then a|^((p-'1) div 2),1 are_congruent_mod p by INT_3:12;
  then p divides (a|^((p-'1) div 2) - 1) by INT_2:19;
  hence thesis by INT_1:89;
end;

theorem Th21:
  p > 2 & a gcd p = 1 & not a is_quadratic_residue_mod p implies (
  a|^((p-'1) div 2) + 1) mod p = 0
proof
  assume that
A1: p > 2 and
A2: a gcd p = 1 and
A3: not a is_quadratic_residue_mod p;
A4: p-1 > 2-1 by A1,XREAL_1:11;
A5: p-1<p by XREAL_1:148;
  a|^((p-'1) div 2) mod p = p-1 by A1,A2,A3,Th19;
  then a|^((p-'1) div 2) mod p = (p-1) mod p by A4,A5,INT_3:10;
  then a|^((p-'1) div 2),(p-1) are_congruent_mod p by INT_3:12;
  then p divides (a|^((p-'1) div 2) - (p-1)) by INT_2:19;
  then p divides -((a|^((p-'1) div 2)+1) - p) by INT_2:14;
  then p divides (p-(a|^((p-'1) div 2)+1));
  then p divides (a|^((p-'1) div 2)+1) by Th2;
  hence thesis by INT_1:89;
end;

reserve b for Integer;

theorem
  a is_quadratic_residue_mod p & b is_quadratic_residue_mod p implies a*
  b is_quadratic_residue_mod p
proof
  assume that
A1: a is_quadratic_residue_mod p and
A2: b is_quadratic_residue_mod p;
  consider i being Integer such that
A3: (i^2 - a) mod p =0 by A1,Def2;
  consider j being Integer such that
A4: (j^2 - b) mod p =0 by A2,Def2;
  p divides (j^2 - b) by A4,INT_1:89;
  then
A5: j^2,b are_congruent_mod p by INT_2:19;
  p divides (i^2 - a) by A3,INT_1:89;
  then i^2,a are_congruent_mod p by INT_2:19;
  then (i^2)*(j^2),a*b are_congruent_mod p by A5,INT_1:39;
  then p divides ((i*j)^2 - a*b) by INT_2:19;
  then ((i*j)^2 - a*b) mod p = 0 by INT_1:89;
  hence thesis by Def2;
end;

theorem
  p>2 & a gcd p = 1 & b gcd p = 1 & a is_quadratic_residue_mod p & not b
  is_quadratic_residue_mod p implies not a*b is_quadratic_residue_mod p
proof
  assume that
A1: p>2 and
A2: a gcd p = 1 and
A3: b gcd p = 1 and
A4: a is_quadratic_residue_mod p and
A5: not b is_quadratic_residue_mod p;
A6: a*b gcd p = 1 by A2,A3,WSIERP_1:11;
  set l = (p-'1) div 2;
  (b|^l + 1) mod p = 0 by A1,A3,A5,Th21;
  then
A7: p divides (b|^l + 1) by INT_1:89;
A8: (a|^l -1)*(b|^l +1) = a|^l * b|^l +a|^l*1 -1*b|^l -1*1
    .= (a*b)|^l +a|^l*1 -1*b|^l -1*1 by NEWTON:12
    .= ((a*b)|^l -1) +(a|^l - 1) -(b|^l - 1);
  (a|^l -1) mod p = 0 by A1,A2,A4,Th20;
  then
A9: p divides (a|^l -1) by INT_1:89;
  then
A10: p divides (a|^l -1)*(b|^l +1) by INT_2:2;
    assume a*b is_quadratic_residue_mod p;
    then ((a*b)|^l -1) mod p = 0 by A1,A6,Th20;
    then p divides ((a*b)|^l -1) by INT_1:89;
    then p divides ((a*b)|^l -1) +(a|^l - 1) by A9,WSIERP_1:9;
    then p divides (b|^l - 1) by A10,A8,Th2;
    then p divides ((b|^l +1) - (b|^l -1)) by A7,Th1;
    hence contradiction by A1,NAT_D:7;
end;

theorem
  p>2 & a gcd p = 1 & b gcd p = 1 & not a is_quadratic_residue_mod p &
  not b is_quadratic_residue_mod p implies a*b is_quadratic_residue_mod p
proof
  assume that
A1: p>2 and
A2: a gcd p = 1 and
A3: b gcd p = 1 and
A4: not a is_quadratic_residue_mod p and
A5: not b is_quadratic_residue_mod p;
A6: a*b gcd p = 1 by A2,A3,WSIERP_1:11;
  set l = (p-'1) div 2;
  (b|^l + 1) mod p = 0 by A1,A3,A5,Th21;
  then
A7: p divides (b|^l + 1) by INT_1:89;
A8: (a|^l +1)*(b|^l +1) = a|^l * b|^l +a|^l*1 +1*b|^l +1*1
    .= (a*b)|^l +a|^l + b|^l +1 by NEWTON:12
    .= ((a*b)|^l +1) +(a|^l + 1) - (1- b|^l);
  (a|^l +1) mod p = 0 by A1,A2,A4,Th21;
  then
A9: p divides (a|^l +1) by INT_1:89;
  then
A10: p divides (a|^l +1)*(b|^l +1) by INT_2:2;
  now
    assume not a*b is_quadratic_residue_mod p;
    then ((a*b)|^l +1) mod p = 0 by A1,A6,Th21;
    then p divides ((a*b)|^l +1) by INT_1:89;
    then p divides ((a*b)|^l +1) +(a|^l + 1) by A9,WSIERP_1:9;
    then p divides (1- b|^l) by A10,A8,Th2;
    then p divides ((b|^l +1) + (1- b|^l)) by A7,WSIERP_1:9;
    hence contradiction by A1,NAT_D:7;
  end;
  hence thesis;
end;

definition
  let a be Integer, p be Prime;
  func Lege (a,p) -> Integer equals
  :Def3:
  1 if a is_quadratic_residue_mod p
  otherwise  -1;
  coherence;
  consistency;
end;

theorem Th25:
  Lege (a,p) = 1 or Lege (a,p) = -1 by Def3;

theorem Th26:
  Lege (a^2,p) = 1
proof
  a^2 is_quadratic_residue_mod p by Th9;
  hence thesis by Def3;
end;

theorem
  Lege (1,p) = 1
proof
  Lege (1^2,p) = 1 by Th26;
  hence thesis;
end;

theorem Th28:
  p>2 & a gcd p =1 implies Lege (a,p),a|^((p-'1) div 2) are_congruent_mod p
proof
  assume that
A1: p>2 and
A2: a gcd p = 1;
A3: p>1 by INT_2:def 5;
  then -p < -1 by XREAL_1:26;
  then
A4: (-1) mod p = p+(-1) by INT_3:10;
  per cases;
  suppose
A5: a is_quadratic_residue_mod p;
    then a|^((p-'1) div 2) mod p = 1 by A1,A2,Th17;
    then a|^((p-'1) div 2) mod p = 1 mod p by A3,NAT_D:14;
    then a|^((p-'1) div 2) mod p = Lege (a,p) mod p by A5,Def3;
    hence thesis by INT_3:12;
  end;
  suppose
A6: not a is_quadratic_residue_mod p;
    then a|^((p-'1) div 2) mod p = p-1 by A1,A2,Th19
      .= Lege (a,p) mod p by A4,A6,Def3;
    hence thesis by INT_3:12;
  end;
end;

theorem
  p>2 & a gcd p =1 & a,b are_congruent_mod p implies Lege (a,p) = Lege ( b,p)
proof
  assume that
A1: p>2 and
A2: a gcd p =1 and
A3: a,b are_congruent_mod p;
  Lege (a,p),a|^((p-'1) div 2) are_congruent_mod p by A1,A2,Th28;
  then
A4: Lege (a,p) mod p = a|^((p-'1) div 2) mod p by INT_3:12;
  b gcd p = 1 by A2,A3,INT_4:14;
  then Lege (b,p),b|^((p-'1) div 2) are_congruent_mod p by A1,Th28;
  then
A5: Lege (b,p) mod p = b|^((p-'1) div 2) mod p by INT_3:12;
  a mod p = b mod p by A3,INT_3:12;
  then Lege (a,p) mod p = Lege (b,p) mod p by A4,A5,Th13;
  then Lege (a,p),Lege (b,p) are_congruent_mod p by INT_3:12;
  then
A6: p divides (Lege (a,p) - Lege (b,p)) by INT_2:19;
  per cases by Th25;
  suppose
A7: Lege (a,p) = 1;
    then Lege (b,p) <> -1 by A1,A6,NAT_D:7;
    hence thesis by A7,Th25;
  end;
  suppose
A8: Lege (a,p) = -1;
    now
      assume Lege (b,p) = 1;
      then p divides -2 by A6,A8;
      then p divides 2 by INT_2:14;
      hence contradiction by A1,NAT_D:7;
    end;
    hence thesis by A8,Th25;
  end;
end;

theorem
  p>2 & a gcd p=1 & b gcd p=1 implies Lege(a*b,p)=Lege(a,p)*Lege(b,p)
proof
  assume that
A1: p>2 and
A2: a gcd p=1 and
A3: b gcd p=1;
A4: Lege(b,p),b|^((p-'1) div 2) are_congruent_mod p by A1,A3,Th28;
  Lege(a,p),a|^((p-'1) div 2) are_congruent_mod p by A1,A2,Th28;
  then Lege(a,p)*Lege(b,p),(a|^((p-'1) div 2))*(b|^((p-'1) div 2))
  are_congruent_mod p by A4,INT_1:39;
  then Lege(a,p)*Lege(b,p),(a*b)|^((p-'1) div 2) are_congruent_mod p by
NEWTON:12;
  then
A5: (a*b)|^((p-'1) div 2),Lege(a,p)*Lege(b,p) are_congruent_mod p by INT_1:35;
  a*b gcd p = 1 by A2,A3,WSIERP_1:11;
  then Lege(a*b,p),(a*b)|^((p-'1) div 2) are_congruent_mod p by A1,Th28;
  then Lege(a*b,p),Lege(a,p)*Lege(b,p) are_congruent_mod p by A5,INT_1:36;
  then
A6: p divides (Lege(a*b,p)-Lege(a,p)*Lege(b,p)) by INT_2:19;
A7: Lege(b,p) = 1 or Lege(b,p) = -1 by Th25;
A8: Lege(a,p) = 1 or Lege(a,p) = -1 by Th25;
  per cases by Th25;
  suppose
    Lege(a*b,p) = 1;
    hence thesis by A1,A6,A8,A7,NAT_D:7;
  end;
  suppose
A9: Lege(a*b,p) = -1;
    now
      assume Lege(a,p)*Lege(b,p) <> -1;
      then p divides (-2) by A6,A8,A7,A9;
      then p divides 2 by INT_2:14;
      hence contradiction by A1,NAT_D:7;
    end;
    hence thesis by A9;
  end;
end;

theorem Th31:
  (for d st d in dom fr holds fr.d = 1 or fr.d = -1) implies
  Product fr = 1 or Product fr = -1
proof
  defpred P[FinSequence of INT] means (for d st d in dom $1 holds $1.d = 1 or
  $1.d = -1) implies Product $1 = 1 or Product $1 = -1;
A1: for p being FinSequence of INT, n being Element of INT st P[p] holds P[p
  ^<*n*>]
  proof
    let p be FinSequence of INT,i be Element of INT;
    set p1 = p^<*i*>;
    assume
A2: P[p];
    P[p1]
    proof
      assume
A3:   for d st d in dom p1 holds p1.d = 1 or p1.d = -1;
A4:   for d st d in dom p holds p.d = 1 or p.d = -1
      proof
        let d;
        assume
A5:     d in dom p;
        then p1.d = 1 or p1.d = -1 by A3,FINSEQ_2:18;
        hence thesis by A5,FINSEQ_1:def 7;
      end;
A6:   len p1 in dom p1 by FINSEQ_5:6;
A7:   Product p1 = (Product p)*i by RVSUM_1:126;
      len p1 =len p +1 by FINSEQ_2:19;
      then
A8:   p1.(len p + 1) = 1 or p1.(len p + 1) = -1 by A3,A6;
      per cases by A2,A4,A8,FINSEQ_1:59;
      suppose
        Product p = 1 & i =1;
        hence thesis by A7;
      end;
      suppose
        Product p = 1 & i = -1;
        hence thesis by A7;
      end;
      suppose
        Product p = -1 & i =1;
        hence thesis by A7;
      end;
      suppose
        Product p = -1 & i = -1;
        hence thesis by A7;
      end;
    end;
    hence thesis;
  end;
A9: P[<*>INT] by RVSUM_1:124;
  for p being FinSequence of INT holds P[p] from FINSEQ_2:sch 2(A9,A1 );
  hence thesis;
end;

reserve m for Integer;

theorem Th32:
  for f,fr st len f = len fr & (for d st d in dom f holds f.d,fr.d
  are_congruent_mod m) holds Product f,Product fr are_congruent_mod m
proof
  defpred P[Nat] means for f,fr st len f =$1 & len f=len fr & (for d st d in
  dom f holds f.d,fr.d are_congruent_mod m) holds Product f,Product fr
  are_congruent_mod m;
A1: for n be Element of NAT st P[n] holds P[n+1]
  proof
    let n be Element of NAT;
    assume
A2: P[n];
    P[n+1]
    proof
      let f,fr;
      assume that
A3:   len f = n+1 and
A4:   len f = len fr and
A5:   for d st d in dom f holds f.d,fr.d are_congruent_mod m;
      consider fr1 being FinSequence of INT,b being Element of INT such that
A6:   fr = fr1^<*b*> by A3,A4,FINSEQ_2:22;
      f <> {} by A3;
      then
A7:   (n+1) in dom f by A3,FINSEQ_5:6;
      consider f1 being FinSequence of INT,a being Element of INT such that
A8:   f = f1^<*a*> by A3,FINSEQ_2:22;
A9:   n+1 = len fr1 +1 by A3,A4,A6,FINSEQ_2:19;
      then
A10:  fr.(n+1) = b by A6,FINSEQ_1:59;
A11:  n+1 = len f1 +1 by A3,A8,FINSEQ_2:19;
      then
A12:  dom f1 = dom fr1 by A9,FINSEQ_3:31;
      for d st d in dom f1 holds f1.d,fr1.d are_congruent_mod m
      proof
        let d;
        assume
A13:    d in dom f1;
        then
A14:    f.d = f1.d by A8,FINSEQ_1:def 7;
        fr.d = fr1.d by A6,A12,A13,FINSEQ_1:def 7;
        hence thesis by A5,A8,A13,A14,FINSEQ_2:18;
      end;
      then
A15:  Product f1,Product fr1 are_congruent_mod m by A2,A11,A9;
      f.(n+1) = a by A8,A11,FINSEQ_1:59;
      then a,b are_congruent_mod m by A5,A7,A10;
      then (Product f1)*a,(Product fr1)*b are_congruent_mod m by A15,INT_1:39;
      then Product f,(Product fr1)*b are_congruent_mod m by A8,RVSUM_1:126;
      hence thesis by A6,RVSUM_1:126;
    end;
    hence thesis;
  end;
A16: P[0]
  proof
    let f,fr;
    assume that
A17: len f = 0 and
A18: len f = len fr;
A19: f = <*>INT by A17;
    fr = <*>INT by A17,A18;
    hence thesis by A19,INT_1:32;
  end;
  for n be Element of NAT holds P[n] from NAT_1:sch 1(A16,A1);
  hence thesis;
end;

theorem Th33:
  for f,fr st len f = len fr & (for d st d in dom f holds f.d,-fr.
  d are_congruent_mod m) holds Product f,((-1)|^(len f))*Product fr
  are_congruent_mod m
proof
  defpred P[Nat] means for f,fr st len f =$1 & len f=len fr & (for d st d in
  dom f holds f.d,-fr.d are_congruent_mod m) holds Product f,((-1)|^(len f))*
  Product fr are_congruent_mod m;
A1: for n be Element of NAT st P[n] holds P[n+1]
  proof
    let n be Element of NAT;
    assume
A2: P[n];
    P[n+1]
    proof
      let f,fr;
      assume that
A3:   len f = n+1 and
A4:   len f = len fr and
A5:   for d st d in dom f holds f.d,-fr.d are_congruent_mod m;
      consider fr1 be FinSequence of INT,b be Element of INT such that
A6:   fr = fr1^<*b*> by A3,A4,FINSEQ_2:22;
      f <> {} by A3;
      then
A7:   (n+1) in dom f by A3,FINSEQ_5:6;
      consider f1 be FinSequence of INT,a be Element of INT such that
A8:   f = f1^<*a*> by A3,FINSEQ_2:22;
A9:   n+1 = len fr1 +1 by A3,A4,A6,FINSEQ_2:19;
      then
A10:  fr.(n+1) = b by A6,FINSEQ_1:59;
A11:  n+1 = len f1 +1 by A3,A8,FINSEQ_2:19;
      then
A12:  dom f1 = dom fr1 by A9,FINSEQ_3:31;
      for d st d in dom f1 holds f1.d,-fr1.d are_congruent_mod m
      proof
        let d;
        assume
A13:    d in dom f1;
        then
A14:    f.d = f1.d by A8,FINSEQ_1:def 7;
        fr.d = fr1.d by A6,A12,A13,FINSEQ_1:def 7;
        hence thesis by A5,A8,A13,A14,FINSEQ_2:18;
      end;
      then
A15:  Product f1,((-1)|^(len f1))*Product fr1 are_congruent_mod m by A2,A11,A9;
      f.(n+1) = a by A8,A11,FINSEQ_1:59;
      then a,-b are_congruent_mod m by A5,A7,A10;
      then (Product f1)*a,((-1)|^(len f1))*(Product fr1)*(-b)
      are_congruent_mod m by A15,INT_1:39;
      then
      Product f,((-1)|^(len f1))*(-1)*((Product fr1)*b) are_congruent_mod
      m by A8,RVSUM_1:126;
      then Product f,((-1)|^(len f1 + 1))*((Product fr1)*b) are_congruent_mod
      m by NEWTON:11;
      hence thesis by A3,A6,A11,RVSUM_1:126;
    end;
    hence thesis;
  end;
A16: P[0]
  proof
    let f,fr;
    assume that
A17: len f = 0 and
A18: len f = len fr;
A19: f = <*>INT by A17;
A20: (-1)|^(len f) = 1 by A17,NEWTON:9;
    fr = <*>INT by A17,A18;
    hence thesis by A19,A20,INT_1:32;
  end;
  for n be Element of NAT holds P[n] from NAT_1:sch 1(A16,A1);
  hence thesis;
end;

reserve fp for FinSequence of NAT;

theorem Th34:
  p>2 & (for d st d in dom fp holds fp.d gcd p = 1) implies ex fr
being FinSequence of INT st len fr = len fp & (for d st d in dom fr holds fr.d
  = Lege (fp.d,p)) & Lege (Product fp,p) = Product fr
proof
  assume
A1: p>2;
  deffunc F(Nat) = Lege (fp.$1,p);
  set k = (p-'1) div 2;
  assume
A2: for d st d in dom fp holds fp.d gcd p = 1;
  set f = fp|^k;
  reconsider f as FinSequence of INT by FINSEQ_2:27,NUMBERS:17;
  consider fr being FinSequence such that
A3: len fr = len fp & for d being Nat st d in dom fr holds fr.d = F(d)
  from FINSEQ_1:sch 2;
  for d being Nat st d in dom fr holds fr.d in INT
  proof
    let d be Nat;
    assume d in dom fr;
    then fr.d = Lege (fp.d,p) by A3;
    hence thesis by INT_1:def 2;
  end;
  then reconsider fr as FinSequence of INT by FINSEQ_2:14;
A4: fp is FinSequence of REAL by FINSEQ_2:27;
A5: len f = len fp by NAT_3:def 1;
  for d st d in dom fr holds fr.d,f.d are_congruent_mod p
  proof
    let d;
    assume
A6: d in dom fr;
    then d in dom fp by A3,FINSEQ_3:31;
    then fp.d gcd p = 1 by A2;
    then Lege (fp.d,p),(fp.d)|^k are_congruent_mod p by A1,Th28;
    then
A7: fr.d,(fp.d)|^k are_congruent_mod p by A3,A6;
    d in dom f by A3,A5,A6,FINSEQ_3:31;
    hence thesis by A7,NAT_3:def 1;
  end;
  then
A8: Product f,Product fr are_congruent_mod p by A3,A5,Th32,INT_1:35;
  Product(fp) gcd p = 1 by A2,WSIERP_1:43;
  then
  Lege (Product fp,p),(Product fp)|^((p-'1) div 2) are_congruent_mod p by A1
,Th28;
  then Lege (Product fp,p),Product f are_congruent_mod p by A4,NAT_3:15;
  then Lege (Product fp,p),Product fr are_congruent_mod p by A8,INT_1:36;
  then
A9: p divides (Lege (Product fp,p) - Product fr) by INT_2:19;
  take fr;
A10: for d st d in dom fr holds fr.d = 1 or fr.d = -1
  proof
    let d;
    assume d in dom fr;
    then fr.d = Lege (fp.d,p) by A3;
    hence thesis by Th25;
  end;
  per cases by Th25;
  suppose
A11: Lege (Product fp,p) = 1;
    then Product fr <> -1 by A1,A9,NAT_D:7;
    hence thesis by A3,A10,A11,Th31;
  end;
  suppose
A12: Lege (Product fp,p) = -1;
    now
      assume Product fr = 1;
      then p divides -2 by A9,A12;
      then p divides 2 by INT_2:14;
      hence contradiction by A1,NAT_D:7;
    end;
    hence thesis by A3,A10,A12,Th31;
  end;
end;

theorem
  p > 2 & d gcd p = 1 & e gcd p = 1 implies Lege((d^2)*e,p) = Lege(e,p)
proof
  assume that
A1: p > 2 and
A2: d gcd p = 1 and
A3: e gcd p = 1;
  reconsider d2=d^2, e as Element of NAT by ORDINAL1:def 13;
  set fp = <*d2,e*>;
  reconsider fp as FinSequence of NAT by FINSEQ_2:15;
A4: Lege(d^2,p) = 1 by Th26;
  reconsider p as prime Element of NAT by ORDINAL1:def 13;
  for k st k in dom fp holds fp.k gcd p = 1
  proof
    let k;
    assume k in dom fp;
    then k in Seg(len fp) by FINSEQ_1:def 3;
    then
A5: k in Seg 2 by FINSEQ_1:61;
    per cases by A5,FINSEQ_1:4,TARSKI:def 2;
    suppose
      k = 1;
      then fp.k = d^2 by FINSEQ_1:61;
      hence thesis by A2,WSIERP_1:12;
    end;
    suppose
      k = 2;
      hence thesis by A3,FINSEQ_1:61;
    end;
  end;
  then consider fr be FinSequence of INT such that
A6: len fr = len fp and
A7: for k be Nat st k in dom fr holds fr.k = Lege (fp.k,p) and
A8: Lege (Product fp,p) = Product fr by A1,Th34;
A9: len fr = 2 by A6,FINSEQ_1:61;
  then 2 in dom fr by FINSEQ_3:27;
  then fr.2 = Lege(fp.2,p) by A7;
  then
A10: fr.2 = Lege(e,p) by FINSEQ_1:61;
  1 in dom fr by A9,FINSEQ_3:27;
  then fr.1 = Lege(fp.1,p) by A7;
  then fr.1 = Lege(d^2,p) by FINSEQ_1:61;
  then fr = <*1,Lege(e,p)*> by A4,A9,A10,FINSEQ_1:61;
  then Product fr = 1 * Lege(e,p) by RVSUM_1:129;
  hence thesis by A8,RVSUM_1:129;
end;

theorem Th36:
  p>2 implies Lege (-1,p) = (-1)|^((p-'1) div 2)
proof
  assume
A1: p>2;
  abs((-1)|^((p-'1) div 2)) = 1 by SERIES_2:1;
  then
A2: (-1)|^((p-'1) div 2) =1 or -(-1)|^((p-'1) div 2) =1 by ABSVALUE:1;
  (-1) gcd p = (-1)|^1 gcd p by NEWTON:10
    .= abs((-1)|^1) gcd abs(p) by INT_2:51
    .= 1 gcd abs(p) by SERIES_2:1
    .= 1 by NEWTON:64;
  then
A3: Lege (-1,p),(-1)|^((p-'1) div 2) are_congruent_mod p by A1,Th28;
  per cases by A2;
  suppose
A4: (-1)|^((p-'1) div 2) =1;
    then
A5: p divides (Lege (-1,p) - 1) by A3,INT_2:19;
    now
      assume Lege(-1,p) = -1;
      then p divides -2 by A5;
      then p divides 2 by INT_2:14;
      hence contradiction by A1,NAT_D:7;
    end;
    hence thesis by A4,Th25;
  end;
  suppose
A6: (-1)|^((p-'1) div 2) = -1;
    then p divides (Lege (-1,p) - (-1)) by A3,INT_2:19;
    then Lege(-1,p) <> 1 by A1,NAT_D:7;
    hence thesis by A6,Th25;
  end;
end;

theorem
  p>2 & p mod 4 = 1 implies (-1) is_quadratic_residue_mod p
proof
  assume that
A1: p>2 and
A2: p mod 4 = 1;
  p>1 by INT_2:def 5;
  then
A3: p-'1 = p-1 by XREAL_1:235;
  p = (p div 4)*4 + 1 by A2,NAT_D:2;
  then p-'1 = 2*(2*(p div 4)) by A3;
  then (-1)|^((p-'1) div 2) = (-1)|^(2*(p div 4)) by NAT_D:18
    .= ((-1)|^2)|^(p div 4) by NEWTON:14
    .= (1|^2)|^(p div 4) by WSIERP_1:2
    .= 1^2|^(p div 4) by NEWTON:100
    .= 1 by NEWTON:15;
  then Lege(-1,p) = 1 by A1,Th36;
  hence thesis by Def3;
end;

theorem
  p>2 & p mod 4 = 3 implies not (-1) is_quadratic_residue_mod p
proof
  assume that
A1: p>2 and
A2: p mod 4 = 3;
  p>1 by INT_2:def 5;
  then
A3: p-'1 = p-1 by XREAL_1:235;
  p = (p div 4)*4 + 3 by A2,NAT_D:2;
  then p-'1 = 2*(2*(p div 4) + 1) by A3;
  then (-1)|^((p-'1) div 2) = (-1)|^(2*(p div 4) + 1) by NAT_D:18
    .= (-1)|^(2*(p div 4)) * (-1) by NEWTON:11
    .= ((-1)|^2)|^(p div 4) *(-1) by NEWTON:14
    .= (1|^2)|^(p div 4) *(-1) by WSIERP_1:2
    .= 1^2|^(p div 4) *(-1) by NEWTON:100
    .= 1 *(-1) by NEWTON:15;
  then Lege(-1,p) = -1 by A1,Th36;
  hence thesis by Def3;
end;

begin :: Gauss Lemma and Law of Quadratic Reciprocity

theorem Th39:
  for D being non empty set,f being FinSequence of D, i,j being
  Nat holds f is one-to-one iff Swap(f,i,j) is one-to-one
proof
  let D be non empty set,f be FinSequence of D,i,j be Nat;
  thus f is one-to-one implies Swap(f,i,j) is one-to-one
  proof
    set ff = Swap(f,i,j);
A1: rng ff = rng f by FINSEQ_7:24;
    assume f is one-to-one;
    then
A2: card(rng f) = len f by FINSEQ_4:77;
    len ff = len f by FINSEQ_7:20;
    hence thesis by A2,A1,FINSEQ_4:77;
  end;
  assume Swap(f,i,j) is one-to-one;
  then card(rng Swap(f,i,j)) = len Swap(f,i,j) by FINSEQ_4:77;
  then card(rng f) = len Swap(f,i,j) by FINSEQ_7:24;
  then card(rng f) = len f by FINSEQ_7:20;
  hence thesis by FINSEQ_4:77;
end;

theorem Th40:
  for f being FinSequence of NAT st len f = n & (for d st d in dom
  f holds f.d>0 & f.d<=n) & f is one-to-one holds rng f = Seg n
proof
  defpred P[Nat] means for f being FinSequence of NAT st len f = $1 & (for d
  st d in dom f holds f.d>0 & f.d<=$1) & f is one-to-one holds rng f = Seg $1;
A1: for n be Element of NAT st P[n] holds P[n+1]
  proof
    let n be Element of NAT;
    assume
A2: P[n];
    P[n+1]
    proof
      let f be FinSequence of NAT;
      assume that
A3:   len f = n+1 and
A4:   for d st d in dom f holds f.d>0 & f.d<=n+1 and
A5:   f is one-to-one;
A6:   f <> {} by A3;
      then
A7:   n+1 in dom f by A3,FINSEQ_5:6;
      then
A8:   f.(n+1) > 0 by A4;
      consider f1 being FinSequence of NAT,a being Element of NAT such that
A9:   f = f1^<*a*> by A6,HILBERT2:4;
A10:  f1 is one-to-one by A5,A9,FINSEQ_3:98;
A11:  len f = len f1 + 1 by A9,FINSEQ_2:19;
      f.(n+1) <= n+1 by A4,A7;
      then
A12:  a<=n+1 by A3,A9,A11,FINSEQ_1:59;
      per cases by A3,A9,A11,A8,A12,FINSEQ_1:59,XXREAL_0:1;
      suppose
A13:    a = n+1;
        for d st d in dom f1 holds f1.d>0 & f1.d<=n
        proof
          let d;
          assume
A14:      d in dom f1;
          then
A15:      d in dom f by A9,FINSEQ_2:18;
A16:      now
            d <= n by A3,A11,A14,FINSEQ_3:27;
            then d < n+1 by XREAL_1:147;
            then f.d <> f.(n+1) by A5,A7,A15,FUNCT_1:def 8;
            then
A17:        f1.d <> f.(n+1) by A9,A14,FINSEQ_1:def 7;
            assume f1.d = n+1;
            hence contradiction by A3,A9,A11,A13,A17,FINSEQ_1:59;
          end;
          f.d<=n+1 by A4,A15;
          then f1.d<=n+1 by A9,A14,FINSEQ_1:def 7;
          then
A18:      f1.d < n+1 by A16,XXREAL_0:1;
          f.d>0 by A4,A15;
          hence thesis by A9,A14,A18,FINSEQ_1:def 7,NAT_1:13;
        end;
        then rng f1 = Seg n by A2,A3,A11,A10;
        then rng f1 \/ {a} = Seg (n+1) by A13,FINSEQ_1:11;
        then rng f1 \/ rng <*a*> = Seg (n+1) by FINSEQ_1:55;
        hence thesis by A9,FINSEQ_1:44;
      end;
      suppose
A19:    a > 0 & a < n+1;
        ex d st d in dom f1 & f1.d = n+1
        proof
          assume
A20:      for d st d in dom f1 holds f1.d <> n+1;
          for d being Nat st d in dom f holds f.d in Seg n
          proof
            let d be Nat;
            assume
A21:        d in dom f;
            then
A22:        d in Seg(n+1) by A3,FINSEQ_1:def 3;
            then
A23:        d<=n+1 by FINSEQ_1:3;
            per cases by A22,A23,FINSEQ_1:3,XXREAL_0:1;
            suppose
              d=n+1;
              then
A24:          f.d = a by A3,A9,A11,FINSEQ_1:59;
              then
A25:          f.d<=n by A19,NAT_1:13;
              f.d>=0+1 by A19,A24,NAT_1:13;
              hence thesis by A25,FINSEQ_1:3;
            end;
            suppose
A26:          d>=1 & d<n+1;
              then d<=n by NAT_1:13;
              then d in Seg n by A26,FINSEQ_1:3;
              then
A27:          d in dom f1 by A3,A11,FINSEQ_1:def 3;
              then f1.d <> n+1 by A20;
              then
A28:          f.d <> n+1 by A9,A27,FINSEQ_1:def 7;
              f.d <=n+1 by A4,A21;
              then f.d <n+1 by A28,XXREAL_0:1;
              then
A29:          f.d<=n by NAT_1:13;
              f.d>0 by A4,A21;
              then f.d>=0+1 by NAT_1:13;
              hence thesis by A29,FINSEQ_1:3;
            end;
          end;
          then f is FinSequence of Seg n by FINSEQ_2:14;
          then rng f c= Seg n by FINSEQ_1:def 4;
          then card rng f <= card Seg n by NAT_1:44;
          then n+1 <= card Seg n by A3,A5,FINSEQ_4:77;
          then n+1 <= n+0 by FINSEQ_1:78;
          hence contradiction by XREAL_1:8;
        end;
        then consider d1 being Element of NAT such that
A30:    d1 in dom f1 and
A31:    f1.d1 = n+1;
        d1<=n by A3,A11,A30,FINSEQ_3:27;
        then
A32:    d1 <= len f by A3,NAT_1:13;
A33:    0+1 <= n+1 by XREAL_1:8;
        set f2 = Swap(f,d1,n+1);
A34:    len f2 = n+1 by A3,FINSEQ_7:20;
        then
A35:    f2 <> {};
        then consider
        f3 being FinSequence of NAT,b being Element of NAT such that
A36:    f2 = f3^<*b*> by HILBERT2:4;
A37:    n+1 = len f3 +1 by A34,A36,FINSEQ_2:19;
A38:    1 <= d1 by A30,FINSEQ_3:27;
        then f2/.(n+1) = f/.d1 by A3,A32,A33,FINSEQ_7:33;
        then f2/.(n+1) = f.d1 by A38,A32,FINSEQ_4:24;
        then f2.(n+1) = f.d1 by A34,A33,FINSEQ_4:24;
        then
A39:    f2.(n+1) = n+1 by A9,A30,A31,FINSEQ_1:def 7;
        then
A40:    b = n+1 by A36,A37,FINSEQ_1:59;
A41:    f2 is one-to-one by A5,Th39;
A42:    for d st d in dom f3 holds f3.d>0 & f3.d<=n
        proof
          let d;
          assume
A43:      d in dom f3;
          then
A44:      d in dom f2 by A36,FINSEQ_2:18;
A45:      now
            d<=n by A37,A43,FINSEQ_3:27;
            then
A46:        d < n+1 by XREAL_1:147;
            assume f3.d=n+1;
            then
A47:        f2.d = n+1 by A36,A43,FINSEQ_1:def 7;
            n+1 in dom f2 by A34,A35,FINSEQ_5:6;
            hence contradiction by A39,A41,A44,A47,A46,FUNCT_1:def 8;
          end;
          f2.d in rng f2 by A44,FUNCT_1:12;
          then f2.d in rng f by FINSEQ_7:24;
          then
A48:      ex e being Nat st e in dom f & f2.d = f.e by FINSEQ_2:11;
          then f2.d<=n+1 by A4;
          then f3.d<=n+1 by A36,A43,FINSEQ_1:def 7;
          then
A49:      f3.d<n+1 by A45,XXREAL_0:1;
          f2.d >0 by A4,A48;
          hence thesis by A36,A43,A49,FINSEQ_1:def 7,NAT_1:13;
        end;
        f3 is one-to-one by A36,A41,FINSEQ_3:98;
        then
A50:    rng f3 = Seg n by A2,A37,A42;
        rng f2 = rng f3 \/ rng <*b*> by A36,FINSEQ_1:44
          .= Seg n \/ {n+1} by A40,A50,FINSEQ_1:55
          .= Seg (n+1) by FINSEQ_1:11;
        hence thesis by FINSEQ_7:24;
      end;
    end;
    hence thesis;
  end;
A51: P[0]
  proof
    let f be FinSequence of NAT;
    assume len f = 0;
    then f = {};
    hence thesis;
  end;
  for n be Element of NAT holds P[n] from NAT_1:sch 1(A51,A1);
  hence thesis;
end;

reserve a,m for Nat;

theorem Th41:
  for f be FinSequence of NAT st p>2 & a gcd p = 1 & f = a*(idseq
((p-'1) div 2)) & m=card{k where k is Element of NAT:k in rng (f mod p) & k > p
  /2} holds Lege(a,p) = (-1)|^m
proof
  let f be FinSequence of NAT;
  assume that
A1: p>2 and
A2: a gcd p = 1 and
A3: f = a*(idseq ((p-'1) div 2)) and
A4: m=card{k where k is Element of NAT:k in rng (f mod p) & k > p/2};
  set f1 = f mod p;
A5: len f1 = len f by EULER_2:def 1;
  set X = {k where k is Element of NAT:k in rng f1 & k > p/2};
  for x being set st x in X holds x in rng f1
  proof
    let x be set;
    assume x in X;
    then ex k being Element of NAT st x = k & k in rng f1 & k > p/2;
    hence thesis;
  end;
  then
A6: X c= rng f1 by TARSKI:def 3;
  then reconsider X as finite set;
A7: rng f1 c= NAT by FINSEQ_1:def 4;
  then reconsider X as finite Subset of NAT by A6,XBOOLE_1:1;
  card X is Element of NAT;
  then reconsider m as Element of NAT by A4;
A8: rng f1 \ X c= rng f1 by XBOOLE_1:36;
  then reconsider Y = rng f1 \ X as finite Subset of NAT by A7,XBOOLE_1:1;
A9: a|^((p-'1) div 2),Lege(a,p) are_congruent_mod p by A1,A2,Th28,INT_1:35;
  set f2 = Sgm rng f1;
  (Product f1) mod p = (Product f) mod p by EULER_2:26;
  then
A10: Product f1,Product f are_congruent_mod p by INT_3:12;
A11: p>1 by INT_2:def 5;
  then
A12: p-'1 = p-1 by XREAL_1:235;
  then
A13: p-'1>0 by A11,XREAL_1:52;
  set p9 = (p-'1) div 2;
A14: rng idseq p9 = Seg p9 by RELAT_1:71;
  then reconsider I = idseq p9 as FinSequence of NAT by FINSEQ_1:def 4;
  dom f = dom I by A3,VALUED_1:def 5;
  then
A15: len f = len I by FINSEQ_3:31
    .= p9 by FINSEQ_1:def 18;
  p >= 2+1 by A1,NAT_1:13;
  then p-1 >= 3-1 by XREAL_1:11;
  then f1 <> {} by A15,A12,A5,NAT_2:15;
  then rng f1 is non empty Subset of NAT by FINSEQ_1:def 4;
  then consider n1 being Element of NAT such that
A16: rng f1 c= Seg n1 \/ {0} by HEYTING3:3;
  I is Element of p9-tuples_on REAL by FINSEQ_2:129;
  then
A17: Product f = (Product (p9|->a))*(Product I) by A3,RVSUM_1:138
    .= (a|^p9) * (Product I) by NEWTON:def 1;
  p is odd by A1,PEPIN:17;
  then
A18: p-'1 is even by A12,HILBERT3:2;
  then
A19: p9 = ((p-'1)+1) div 2 by NAT_2:28
    .= p div 2 by A11,XREAL_1:237;
  2 divides (p-'1) by A18,PEPIN:22;
  then
A20: p-'1 = 2*p9 by NAT_D:3;
  then p9 divides (p-'1) by NAT_D:def 3;
  then p9 <= (p-'1) by A13,NAT_D:7;
  then
A21: p9 < p by A12,XREAL_1:148,XXREAL_0:2;
  for d being Nat st d in dom I holds I.d gcd p = 1
  proof
    let d be Nat;
    assume d in dom I;
    then
A22: d in Seg len I by FINSEQ_1:def 3;
    then
A23: d in Seg p9 by FINSEQ_1:def 18;
    then
A24: I.d = d by FINSEQ_2:57;
    d <= p9 by A23,FINSEQ_1:3;
    then
A25: d < p by A21,XXREAL_0:2;
    d >= 1 by A22,FINSEQ_1:3;
    then d,p are_relative_prime by A25,EULER_1:3;
    hence thesis by A24,INT_2:def 4;
  end;
  then
A26: (Product I) gcd p = 1 by WSIERP_1:43;
A27: for d st d in dom f holds f.d = a*d
  proof
    let d;
    assume
A28: d in dom f;
    then d in dom I by A3,VALUED_1:def 5;
    then d in Seg len I by FINSEQ_1:def 3;
    then
A29: d is Element of Seg p9 by FINSEQ_1:def 18;
    thus f.d = a * I.d by A3,A28,VALUED_1:def 5
      .= a*d by A29,FINSEQ_2:57;
  end;
A30: for d,e being Element of NAT st 1<=d & d<e & e<=len f1 holds f1.d <> f1 .e
  proof
    let d,e be Element of NAT;
    assume that
A31: 1<=d and
A32: d<e and
A33: e<=len f1;
A34: e<=len f by A33,EULER_2:def 1;
    1<=e by A31,A32,XXREAL_0:2;
    then
A35: e in dom f by A34,FINSEQ_3:27;
    then
A36: f1.e = (f.e) mod p by EULER_2:def 1;
    d<len f by A32,A34,XXREAL_0:2;
    then
A37: d in dom f by A31,FINSEQ_3:27;
    then
A38: f1.d = (f.d) mod p by EULER_2:def 1;
    now
      assume f1.d = f1.e;
      then f.e,f.d are_congruent_mod p by A38,A36,INT_3:12;
      then p divides (f.e - f.d) by INT_2:19;
      then p divides (a*e - f.d) by A27,A35;
      then p divides (a*e - a*d) by A27,A37;
      then
A39:  p divides a*(e-d);
A40:  p9-1<p by A21,XREAL_1:149;
      reconsider dd = e-d as Element of NAT by A32,NAT_1:21;
A41:  abs p = p by ABSVALUE:def 1;
A42:  abs dd = dd by ABSVALUE:def 1;
A43:  dd <= p9 - 1 by A15,A5,A31,A33,XREAL_1:15;
      dd <> 0 by A32;
      then p <= dd by A2,A39,A41,A42,INT_4:6,WSIERP_1:36;
      hence contradiction by A43,A40,XXREAL_0:2;
    end;
    hence thesis;
  end;
  then
A44: len f1 = card rng f1 by GRAPH_5:10;
  then
A45: f1 is one-to-one by FINSEQ_4:77;
A46: dom f1 = dom f by A5,FINSEQ_3:31;
  not 0 in rng f1
  proof
    reconsider a as Element of NAT by ORDINAL1:def 13;
    assume 0 in rng f1;
    then consider n be Nat such that
A47: n in dom f1 and
A48: f1.n = 0 by FINSEQ_2:11;
    0 = (f.n) mod p by A46,A47,A48,EULER_2:def 1
      .= (a*n) mod p by A27,A46,A47;
    then
A49: p divides (a*n) by PEPIN:6;
    n >= 1 by A47,FINSEQ_3:27;
    then
A50: p <= n by A2,A49,NAT_D:7,WSIERP_1:37;
    n <= p9 by A15,A5,A47,FINSEQ_3:27;
    hence contradiction by A21,A50,XXREAL_0:2;
  end;
  then
A51: {0} misses rng f1 by ZFMISC_1:56;
  then
A52: f2 is one-to-one by A16,FINSEQ_3:99,XBOOLE_1:73;
A53: rng f1 c= Seg n1 by A16,A51,XBOOLE_1:73;
  then
A54: X c= Seg n1 by A6,XBOOLE_1:1;
  len f = card rng f1 by A5,A30,GRAPH_5:10;
  then reconsider n = p9 - m as Element of NAT by A4,A15,A6,NAT_1:21,44;
A55: Y c= Seg n1 by A53,A8,XBOOLE_1:1;
A56: rng f1 = rng f2 by A53,FINSEQ_1:def 13;
  then
A57: Product f1 = Product f2 by A45,A52,EULER_2:25,RFINSEQ:39;
    set f3 = (len (f2/^n) |-> p) - (f2/^n);
    set f4 = (f2|n) ^ f3;
A64: f2/^n is FinSequence of INT by FINSEQ_2:27,NUMBERS:17;
A65: dom f3 = dom(len (f2/^n) |-> p)/\dom(f2/^n) by VALUED_1:12
      .= (Seg len (len(f2/^n) |-> p))/\dom(f2/^n) by FINSEQ_1:def 3
      .= (Seg len(f2/^n))/\dom(f2/^n) by FINSEQ_1:def 18
      .= dom(f2/^n) /\ dom(f2/^n) by FINSEQ_1:def 3
      .= dom(f2/^n);
    then
A66: len f3 = len(f2/^n) by FINSEQ_3:31;
    for k,l being Element of NAT st k in Y & l in X holds k < l
    proof
      let k,l be Element of NAT;
      assume that
A67:  k in Y and
A68:  l in X;
A69:  not k in X by A67,XBOOLE_0:def 5;
A70:  ex l1 being Element of NAT st l1 = l & l1 in rng f1 & l1>p/2 by A68;
      k in rng f1 by A67,XBOOLE_0:def 5;
      then k <= p/2 by A69;
      hence thesis by A70,XXREAL_0:2;
    end;
    then Sgm (Y\/X) = (Sgm Y)^(Sgm X) by A54,A55,FINSEQ_3:48;
    then Sgm (rng f1 \/ X) = (Sgm Y)^(Sgm X) by XBOOLE_1:39;
    then
A71: f2 = (Sgm Y)^(Sgm X) by A6,XBOOLE_1:12;
A72: for d st d in dom f3 holds f3.d = p - (f2/^n).d
    proof
      let d;
      assume
A73:  d in dom f3;
      then d in Seg len(f2/^n) by A65,FINSEQ_1:def 3;
      then (len (f2/^n) |-> p).d = p by FINSEQ_2:71;
      hence thesis by A73,VALUED_1:13;
    end;
A74: len Sgm Y = card Y by A53,A8,FINSEQ_3:44,XBOOLE_1:1
      .= p9 - m by A4,A15,A5,A6,A44,CARD_2:63;
    then
A75: f2/^n = Sgm X by A71,FINSEQ_5:40;
A76: for d st d in dom f3 holds f3.d > 0 & f3.d <= p9
    proof
      let d;
      reconsider w = f3.d as Element of INT by INT_1:def 2;
      assume
A77:  d in dom f3;
      then (Sgm X).d in rng Sgm X by A75,A65,FUNCT_1:12;
      then (Sgm X).d in X by A54,FINSEQ_1:def 13;
      then
A78:  ex ll be Element of NAT st ll = (Sgm X).d & ll in rng f1 & ll > p/2;
      then consider e being Nat such that
A79:  e in dom f1 and
A80:  f1.e = (f2/^n).d by A75,FINSEQ_2:11;
      (f2/^n).d = f.e mod p by A46,A79,A80,EULER_2:def 1;
      then
A81:  (f2/^n).d < p by NAT_D:1;
A82:  f3.d = p - (f2/^n).d by A72,A77;
      then w < p - p/2 by A75,A78,XREAL_1:12;
      hence thesis by A19,A82,A81,INT_1:81,XREAL_1:52;
    end;
    for d being Nat st d in dom f3 holds f3.d in NAT
    proof
      let d be Nat;
      assume d in dom f3;
      then f3.d > 0 by A76;
      hence thesis by INT_1:16;
    end;
    then reconsider f3 as FinSequence of NAT by FINSEQ_2:14;
    abs ((-1)|^m) = 1 by SERIES_2:1;
    then
A83: (-1)|^m = 1 or -((-1)|^m) = 1 by ABSVALUE:1;
    f3 is FinSequence of NAT;
    then reconsider f4 as FinSequence of NAT by FINSEQ_1:96;
A84: f2|n = Sgm Y by A71,A74,FINSEQ_3:122,FINSEQ_6:12;
A85: for d st d in dom f4 holds f4.d>0 & f4.d <= p9
    proof
      let d;
      assume
A86:  d in dom f4;
      per cases by A86,FINSEQ_1:38;
      suppose
A87:    d in dom(f2|n);
        reconsider d as Element of NAT by ORDINAL1:def 13;
        (f2|n).d in rng Sgm Y by A84,A87,FUNCT_1:12;
        then
A88:    (f2|n).d in Y by A55,FINSEQ_1:def 13;
        then
A89:    (f2|n).d in rng f1 by XBOOLE_0:def 5;
        not (f2|n).d in X by A88,XBOOLE_0:def 5;
        then (f2|n).d <= p/2 by A89;
        then
A90:    (f2|n).d <= p9 by A19,INT_1:81;
        not (f2|n).d in {0} by A51,A89,XBOOLE_0:3;
        then (f2|n).d <> 0 by TARSKI:def 1;
        hence thesis by A87,A90,FINSEQ_1:def 7;
      end;
      suppose
        ex l being Nat st l in dom f3 & d=len(f2|n)+ l;
        then consider l be Element of NAT such that
A91:    l in dom f3 and
A92:    d = len(f2|n)+ l;
        f4.d = f3.l by A91,A92,FINSEQ_1:def 7;
        hence thesis by A76,A91;
      end;
    end;
A93: f2 = (f2|n)^(f2/^n) by RFINSEQ:21;
    then
A94: f2/^n is one-to-one by A52,FINSEQ_3:98;
    for d,e being Element of NAT st 1<=d & d<e & e<=len f3 holds f3.d <> f3.e
    proof
      let d,e be Element of NAT;
      assume that
A95:  1<=d and
A96:  d<e and
A97:  e<=len f3;
      1<=e by A95,A96,XXREAL_0:2;
      then
A98:  e in dom f3 by A97,FINSEQ_3:27;
      then
A99:  f3.e = p - (f2/^n).e by A72;
      d<len f3 by A96,A97,XXREAL_0:2;
      then
A100: d in dom f3 by A95,FINSEQ_3:27;
      then f3.d = p - (f2/^n).d by A72;
      hence thesis by A94,A65,A96,A100,A98,A99,FUNCT_1:def 8;
    end;
    then len f3 = card rng f3 by GRAPH_5:10;
    then
A101: f3 is one-to-one by FINSEQ_4:77;
A102: len f2 = p9 by A15,A5,A16,A51,A44,FINSEQ_3:44,XBOOLE_1:73;
    then
A103: n <= len f2 by XREAL_1:45;
A104: rng(f2|n) misses rng f3
    proof
      assume rng(f2|n) meets rng f3;
      then consider x be set such that
A105: x in rng(f2|n) and
A106: x in rng f3 by XBOOLE_0:3;
      consider e being Nat such that
A107: e in dom f3 and
A108: f3.e = x by A106,FINSEQ_2:11;
      x = p - (f2/^n).e by A72,A107,A108;
      then
A109: x = p - f2.(e+n) by A103,A65,A107,RFINSEQ:def 2;
      e+n in dom f2 by A65,A107,FINSEQ_5:29;
      then consider e1 be Nat such that
A110: e1 in dom f1 and
A111: f1.e1 = f2.(e+n) by A56,FINSEQ_2:11,FUNCT_1:12;
A112: e1 in dom f by A5,A110,FINSEQ_3:31;
A113: e1 <= p9 by A15,A5,A110,FINSEQ_3:27;
      rng(f2|n) c= rng f2 by FINSEQ_5:21;
      then consider d1 be Nat such that
A114: d1 in dom f1 and
A115: f1.d1 = x by A56,A105,FINSEQ_2:11;
      d1 <= p9 by A15,A5,A114,FINSEQ_3:27;
      then d1+e1 <= p9+p9 by A113,XREAL_1:9;
      then
A116: d1+e1 < p by A12,A20,XREAL_1:148,XXREAL_0:2;
      x = f.d1 mod p by A46,A114,A115,EULER_2:def 1;
      then (f.d1 mod p) + f2.(e+n) = p by A109;
      then (f.d1 mod p) + (f.e1 mod p) = p by A111,A112,EULER_2:def 1;
      then ((f.d1 mod p)+(f.e1 mod p)) mod p = 0 by NAT_D:25;
      then (f.d1 + f.e1) mod p = 0 by EULER_2:8;
      then p divides (f.d1 + f.e1) by PEPIN:6;
      then p divides (d1*a + f.e1) by A27,A46,A114;
      then p divides (d1*a + e1*a) by A27,A112;
      then
A117: p divides (d1+e1)*a;
      d1 >= 1 by A114,FINSEQ_3:27;
      hence contradiction by A2,A117,A116,NAT_D:7,WSIERP_1:37;
    end;
    f2|n is one-to-one by A52,A93,FINSEQ_3:98;
    then
A118: f4 is one-to-one by A101,A104,FINSEQ_3:98;
A119: for d st d in dom f3 holds f3.d,-(f2/^n).d are_congruent_mod p
    proof
      let d;
      assume d in dom f3;
      then f3.d mod p = (p - (f2/^n).d) mod p by A72
        .= (1*p + (-(f2/^n).d)) mod p
        .= (-(f2/^n).d) mod p by EULER_1:13;
      hence thesis by INT_3:12;
    end;
A120: len(f2/^n) = (len f2 -' n) by RFINSEQ:42
      .= len f2 - n by A102,XREAL_1:45,235
      .= m by A102;
    len(f2|n) = n by A102,FINSEQ_1:80,XREAL_1:45;
    then len f4 = n + m by A66,A120,FINSEQ_1:35
      .= len f by A15;
    then rng f4 = rng I by A14,A15,A118,A85,Th40;
    then Product f4 = Product I by A118,EULER_2:25,RFINSEQ:39;
    then
A121: (Product(f2|n)) * (Product f3) = Product I by RVSUM_1:127;
    f3 is FinSequence of INT by FINSEQ_2:27,NUMBERS:17;
    then (Product f3)*(Product(f2|n)),(-1)|^m * Product(f2/^n)*(Product(f2|n)
    ) are_congruent_mod p by A66,A120,A64,A119,Th33,INT_4:11;
    then (Product f3)*(Product(f2|n)),(-1)|^m * ((Product(f2|n))*Product(f2/^
    n ) ) are_congruent_mod p;
    then Product I,(-1)|^m * Product((f2|n)^(f2/^n)) are_congruent_mod p by
A121,RVSUM_1:127;
    then
A122: Product I,(-1)|^m * Product f1 are_congruent_mod p by A57,RFINSEQ:21;
    (-1)|^m * Product f1,(-1)|^m * Product f are_congruent_mod p by A10,
INT_4:11;
    then Product I,(-1)|^m * (a|^p9) * (Product I) are_congruent_mod p by A17
,A122,INT_1:36;
    then p divides (1*Product I-((-1)|^m * (a|^p9)) * (Product I)) by INT_2:19;
    then p divides (1-((-1)|^m * (a|^p9)))* Product I;
    then p divides (1-((-1)|^m * (a|^p9))) by A26,WSIERP_1:36;
    then p divides (-1)|^m * (1-((-1)|^m * (a|^p9))) by INT_2:2;
    then
A123: p divides ((-1)|^m - (-1)|^m * (-1)|^m * (a|^p9));
    (-1)|^m * (-1)|^m = (-1)|^(m+m) by NEWTON:13
      .= (-1)|^(2*m)
      .= ((-1)|^2)|^m by NEWTON:14
      .= (1|^2)|^m by WSIERP_1:2
      .= 1^2|^m by NEWTON:100
      .= 1 by NEWTON:15;
    then (-1)|^m,a|^p9 are_congruent_mod p by A123,INT_2:19;
    then
A124: (-1)|^m,Lege(a,p) are_congruent_mod p by A9,INT_1:36;
    per cases by A83;
    suppose
A125: (-1)|^m = 1;
      now
        assume Lege(a,p) = -1;
        then p divides (1-(-1)) by A124,A125,INT_2:19;
        hence contradiction by A1,NAT_D:7;
      end;
      hence thesis by A125,Th25;
    end;
    suppose
A126: (-1)|^m = -1;
      now
        assume Lege(a,p) = 1;
        then p divides (-1-1) by A124,A126,INT_2:19;
        then p divides (-2);
        then p divides 2 by INT_2:14;
        hence contradiction by A1,NAT_D:7;
      end;
      hence thesis by A126,Th25;
    end;
end;

theorem Th42:
  p>2 implies Lege(2,p) = (-1)|^((p^2 -'1) div 8)
proof
  set p9 = (p-'1) div 2;
  set I = idseq p9;
  set fp = 2 * I;
  set nn = p div 8;
A1: p>1 by INT_2:def 5;
  then
A2: p-1 = p-'1 by XREAL_1:235;
A3: for d st d in dom fp holds fp.d = 2*d
  proof
    let d;
    assume
A4: d in dom fp;
    then d in dom I by VALUED_1:def 5;
    then d in Seg len I by FINSEQ_1:def 3;
    then
A5: d is Element of Seg p9 by FINSEQ_1:def 18;
    thus fp.d = 2 * I.d by A4,VALUED_1:def 5
      .= 2 * d by A5,FINSEQ_2:57;
  end;
  for d being Nat st d in dom fp holds fp.d in NAT
  proof
    let d be Nat;
    assume d in dom fp;
    then fp.d = 2*d by A3;
    hence thesis;
  end;
  then reconsider fp as FinSequence of NAT by FINSEQ_2:14;
  set f = fp mod p;
  set X = {k where k is Element of NAT:k in rng f & k > p/2};
  set m = card X;
  dom fp = dom I by VALUED_1:def 5;
  then
A6: len fp = len I by FINSEQ_3:31
    .= p9 by FINSEQ_1:def 18;
  set Y = {d where d is Element of NAT:d in dom f & f.d > p/2};
  for x be set st x in Y holds x in dom f
  proof
    let x be set;
    assume x in Y;
    then ex k be Element of NAT st x = k & k in dom f & f.k > p/ 2;
    hence thesis;
  end;
  then Y c= dom f by TARSKI:def 3;
  then reconsider Y as finite Subset of NAT by XBOOLE_1:1;
  set Z = seq((p div 4),p9-'(p div 4));
A7: p mod 8 <= 8-1 by INT_1:79,NAT_D:1;
  8 = 2*4;
  then
A8: 2 divides 8 by NAT_D:def 3;
A9: now
    assume p mod 8 = 0;
    then 8 divides p by PEPIN:6;
    then p = 8 by INT_2:def 5;
    hence contradiction by A8,NAT_4:12;
  end;
  for x being set st x in X holds x in rng f
  proof
    let x be set;
    assume x in X;
    then ex k being Element of NAT st x = k & k in rng f & k > p /2;
    hence thesis;
  end;
  then
A10: X c= rng f by TARSKI:def 3;
  then reconsider X as finite set;
  card X is Element of NAT;
  then reconsider m as Element of NAT;
A11: len f = len fp by EULER_2:def 1;
  then
A12: dom f = dom fp by FINSEQ_3:31;
  assume
A13: p>2;
  then 2,p are_relative_prime by EULER_1:3;
  then
A14: 2 gcd p = 1 by INT_2:def 4;
  then
A15: Lege(2,p) = (-1)|^m by A13,Th41;
  p is odd by A13,PEPIN:17;
  then
A16: p-1 is even by HILBERT3:2;
  then
A17: p9 = ((p-'1)+1) div 2 by A2,NAT_2:28
    .= p div 2 by A1,XREAL_1:237;
  then
A18: f <> {} by A13,A6,A11,NAT_2:15;
  then reconsider U=dom f as non empty finite Subset of NAT;
  2 divides (p-'1) by A16,A2,PEPIN:22;
  then
A19: p-'1 = 2*p9 by NAT_D:3;
A20: for d st d in dom f holds f.d = 2*d
  proof
    let d;
    assume
A21: d in dom f;
    then d<= p9 by A6,A11,FINSEQ_3:27;
    then 2*d<=(p-'1) by A19,XREAL_1:66;
    then 2*d<p by NAT_2:11,XXREAL_0:2;
    hence 2*d = 2*d mod p by NAT_D:24
      .= fp.d mod p by A3,A12,A21
      .= f.d by A12,A21,EULER_2:def 1;
  end;
A22: for d1,d2,k1,k2 be Nat st 1 <= d1 & d1 < d2 & d2 <= len f & k1=f.d1 &
  k2=f.d2 holds k1 < k2
  proof
    let d1,d2,k1,k2 be Nat;
    assume that
A23: 1 <= d1 and
A24: d1 < d2 and
A25: d2 <= len f and
A26: k1=f.d1 and
A27: k2=f.d2;
    1 <= d2 by A23,A24,XXREAL_0:2;
    then d2 in dom f by A25,FINSEQ_3:27;
    then
A28: k2 = 2*d2 by A20,A27;
    d1 <= len f by A24,A25,XXREAL_0:2;
    then d1 in dom f by A23,FINSEQ_3:27;
    then k1 = 2*d1 by A20,A26;
    hence thesis by A24,A28,XREAL_1:70;
  end;
  rng f is non empty Subset of NAT by A18,FINSEQ_1:def 4;
  then consider n1 be Element of NAT such that
A29: rng f c= Seg n1 \/ {0} by HEYTING3:3;
  rng f c= NAT by FINSEQ_1:def 4;
  then reconsider X as finite Subset of NAT by A10,XBOOLE_1:1;
  Z, ((p-'1) div 2)-'(p div 4) are_equipotent by CALCUL_2:6;
  then
A30: card Z = card(((p-'1) div 2)-'(p div 4)) by CARD_1:21
    .= ((p-'1) div 2)-'(p div 4) by CARD_1:def 5;
  not 0 in rng f
  proof
    assume 0 in rng f;
    then consider n be Nat such that
A31: n in dom f and
A32: f.n = 0 by FINSEQ_2:11;
    2*n =0 by A20,A31,A32;
    hence contradiction by A31,FINSEQ_3:27;
  end;
  then
A33: {0} misses rng f by ZFMISC_1:56;
  then rng f c= Seg n1 by A29,XBOOLE_1:73;
  then
A34: Sgm rng f = f by A22,FINSEQ_1:def 13;
A35: X,Y are_equipotent
  proof
    deffunc F(Element of U) = f.$1;
    set YY = {d where d is Element of U:F(d) in X};
A36: now
      let x be set;
      assume x in X;
      then consider y be Element of NAT such that
A37:  y=x and
A38:  y in rng f and
      y > p/2;
      consider d being Nat such that
A39:  d in U and
A40:  f.d = y by A38,FINSEQ_2:11;
      reconsider d as Element of U by A39;
      take d;
      thus x=F(d) by A37,A40;
    end;
A41: Y c= YY
    proof
      let x be set;
      assume x in Y;
      then
A42:  ex d be Element of NAT st d = x & d in dom f & f.d > p/ 2;
      then reconsider x as Element of U;
      reconsider f as FinSequence of NAT qua set;
      f.x in rng f by FUNCT_1:12;
      then F(x) in X by A42;
      hence thesis;
    end;
    now
      let x be set;
      assume x in YY;
      then consider d be Element of U such that
A43:  d = x and
A44:  f.d in X;
      ex k be Element of NAT st k = f.d & k in rng f & k > p/ 2 by A44;
      hence x in Y by A43;
    end;
    then
A45: YY c= Y by TARSKI:def 3;
A46: for d1,d2 being Element of U st F(d1) = F(d2) holds d1 = d2
    proof
      let d1,d2 be Element of U;
      assume
A47:  F(d1)=F(d2);
      f is one-to-one by A29,A33,A34,FINSEQ_3:99,XBOOLE_1:73;
      hence thesis by A47,FUNCT_1:def 8;
    end;
    X,YY are_equipotent from FUNCT_7:sch 3(A36,A46);
    hence thesis by A41,A45,XBOOLE_0:def 10;
  end;
  p div 2 < p by INT_1:83;
  then (p div 2) div 2 <= p div 2 by NAT_2:26;
  then
A48: p div (2*2) <= p div 2 by NAT_2:29;
A49: Z c= Y
  proof
    let x be set;
    assume
A50: x in Z;
    then reconsider x as Element of NAT;
A51: x>=(p div 4)+1 by A50,CALCUL_2:1;
    then (p div 4)+x >= (p div 4)+1 by NAT_1:12;
    then
A52: x >= 1 by XREAL_1:8;
    x<=((p-'1) div 2)-'(p div 4)+(p div 4) by A50,CALCUL_2:1;
    then x<=((p-'1) div 2) by A17,A48,XREAL_1:237;
    then
A53: x in dom f by A6,A11,A52,FINSEQ_3:27;
    x > p/4 by A51,INT_1:52,XXREAL_0:2;
    then 2*x > 2*(p/4) by XREAL_1:70;
    then f.x > p/2 by A20,A53;
    hence thesis by A53;
  end;
  now
    let x be set;
A54: p/4 >= [\p/4/] by INT_1:def 4;
    assume x in Y;
    then consider x1 be Element of NAT such that
A55: x1=x and
A56: x1 in dom f and
A57: f.x1 >p/2;
    2*x1>p/2 by A20,A56,A57;
    then x1 > (p/2)/2 by XREAL_1:85;
    then x1 > [\p/4/] by A54,XXREAL_0:2;
    then
A58: x1 >= (p div 4) + 1 by NAT_1:13;
    x1 <= p9 by A6,A11,A56,FINSEQ_3:27;
    then x1 <= (p9-'(p div 4) + (p div 4)) by A17,A48,XREAL_1:237;
    hence x in Z by A55,A58;
  end;
  then Y c= Z by TARSKI:def 3;
  then Y = Z by A49,XBOOLE_0:def 10;
  then
A59: m = ((p-'1) div 2)-'(p div 4) by A30,A35,CARD_1:21;
A60: now
    assume p mod 8 = 2;
    then 8 divides (p - 2) by PEPIN:8;
    then 2 divides (p - 2) by A8,INT_2:13;
    then 2 divides -(p-2) by INT_2:14;
    then 2 divides (2 - p);
    then 2 divides p by Th2;
    hence contradiction by A13,NAT_4:12;
  end;
A61: now
    assume p mod 8 = 4;
    then 8 divides (p - 4) by PEPIN:8;
    then 2 divides (p - 4) by A8,INT_2:13;
    then 2 divides -(p - 4) by INT_2:14;
    then
A62: 2 divides (4 - p);
    4 = 2*2;
    then 2 divides 4 by NAT_D:def 3;
    then 2 divides p by A62,Th2;
    hence contradiction by A13,NAT_4:12;
  end;
A63: now
    assume p mod 8 = 6;
    then 8 divides (p - 6) by PEPIN:8;
    then 2 divides (p - 6) by A8,INT_2:13;
    then 2 divides -(p - 6) by INT_2:14;
    then
A64: 2 divides (6-p);
    6=2*3;
    then 2 divides 6 by NAT_D:def 3;
    then 2 divides p by A64,Th2;
    hence contradiction by A13,NAT_4:12;
  end;
  per cases by A9,A60,A61,A63,A7,NAT_1:32;
  suppose
    p mod 8 = 1;
    then
A65: p=8*nn+1 by NAT_D:2;
    then p-'1 = 2*(4*nn) by A2;
    then
A66: (p-'1) div 2 = 4*nn by NAT_D:18;
    p div 4 = (4*(2*nn)+1) div 4 by A65
      .= 2*nn+(1 div 4) by INT_3:8
      .= 2*nn+0 by NAT_D:27;
    then m = 4*nn - 2*nn by A59,A66,XREAL_1:66,235
      .= 2*nn;
    then
A67: Lege(2,p) =((-1)|^2)|^nn by A15,NEWTON:14
      .= (1|^2)|^nn by WSIERP_1:2
      .= 1^2|^nn by NEWTON:100
      .= 1 by NEWTON:15;
    (p^2 -'1) div 8 = (((8*nn)^2 + 2*(8*nn)) + 1-'1) div 8 by A65
      .= 8*(8*nn^2 + 2*nn) div 8 by NAT_D:34
      .= 8*nn^2 + 2*nn by NAT_D:18;
    hence (-1)|^((p^2 -'1) div 8) = (-1)|^(2*(4*nn^2 + nn))
      .= ((-1)|^2)|^(4*nn^2 + nn) by NEWTON:14
      .= (1|^2)|^(4*nn^2 + nn) by WSIERP_1:2
      .= 1|^(4*nn^2 + nn) by NEWTON:15
      .= Lege(2,p) by A67,NEWTON:15;
  end;
  suppose
    p mod 8 = 3;
    then
A68: p=8*nn+3 by NAT_D:2;
    then p-'1 = 2*(4*nn+1) by A2;
    then
A69: (p-'1) div 2 = 4*nn+1 by NAT_D:18;
A70: 4*nn>=2*nn by XREAL_1:66;
    p div 4 = (4*(2*nn)+3) div 4 by A68
      .= 2*nn+(3 div 4) by INT_3:8
      .= 2*nn+0 by NAT_D:27;
    then m = 4*nn+1-2*nn by A59,A69,A70,NAT_1:12,XREAL_1:235
      .= 2*nn+1;
    then
A71: Lege(2,p) = (-1)|^(2*nn)*(-1) by A15,NEWTON:11
      .= ((-1)|^2)|^nn*(-1) by NEWTON:14
      .= (1|^2)|^nn*(-1) by WSIERP_1:2
      .= 1|^nn*(-1) by NEWTON:15
      .= 1*(-1) by NEWTON:15
      .= -1;
    (p^2 -'1) div 8 = (8*(8*nn^2)+8*(6*nn)+3*3-1) div 8 by A68,NAT_1:12
,XREAL_1:235
      .= 8*(8*nn^2+6*nn+1) div 8
      .= 8*nn^2+6*nn+1 by NAT_D:18;
    hence (-1)|^((p^2 -'1) div 8) = (-1)|^(2*(4*nn^2+3*nn))*(-1) by NEWTON:11
      .= ((-1)|^2)|^(4*nn^2+3*nn)*(-1) by NEWTON:14
      .= (1|^2)|^(4*nn^2+3*nn)*(-1) by WSIERP_1:2
      .= 1|^(4*nn^2+3*nn)*(-1) by NEWTON:15
      .= 1*(-1) by NEWTON:15
      .= Lege(2,p) by A71;
  end;
  suppose
    p mod 8 = 5;
    then
A72: p=8*nn+5 by NAT_D:2;
    then p-'1 = 2*(4*nn+2) by A2;
    then
A73: (p-'1) div 2 = 4*nn+2 by NAT_D:18;
A74: 4*nn>=2*nn by XREAL_1:66;
    p div 4 = (4*(2*nn+1)+1) div 4 by A72
      .= 2*nn+1+(1 div 4) by INT_3:8
      .= 2*nn+1+0 by NAT_D:27;
    then m = 4*nn+2-(2*nn+1) by A59,A73,A74,XREAL_1:9,235
      .= 2*nn+1;
    then
A75: Lege(2,p) = (-1)|^(2*nn)*(-1) by A15,NEWTON:11
      .= ((-1)|^2)|^nn*(-1) by NEWTON:14
      .= (1|^2)|^nn*(-1) by WSIERP_1:2
      .= 1|^nn*(-1) by NEWTON:15
      .= 1*(-1) by NEWTON:15
      .= -1;
    (p^2 -'1) div 8=(8*(8*nn^2)+8*(10*nn)+25-1) div 8 by A72,NAT_1:12
,XREAL_1:235
      .= 8*(8*nn^2+10*nn+3) div 8
      .= 8*nn^2+10*nn+3 by NAT_D:18;
    hence (-1)|^((p^2 -'1) div 8) = (-1)|^(2*(4*nn^2)+2*(5*nn)+2*1+1)
      .= (-1)|^(2*(4*nn^2+5*nn+1))*(-1) by NEWTON:11
      .= ((-1)|^2)|^(4*nn^2+5*nn+1)*(-1) by NEWTON:14
      .= (1|^2)|^(4*nn^2+5*nn+1)*(-1) by WSIERP_1:2
      .= 1|^(4*nn^2+5*nn+1)*(-1) by NEWTON:15
      .= 1*(-1) by NEWTON:15
      .= Lege(2,p) by A75;
  end;
  suppose
    p mod 8 = 7;
    then
A76: p=8*nn+7 by NAT_D:2;
    then p-'1 = 2*(4*nn+3) by A2;
    then
A77: (p-'1) div 2 = 4*nn+3 by NAT_D:18;
A78: 4*nn>=2*nn by XREAL_1:66;
    p div 4 = (4*(2*nn+1)+3) div 4 by A76
      .= 2*nn+1+(3 div 4) by INT_3:8
      .= 2*nn+1+0 by NAT_D:27;
    then m = 4*nn+3-(2*nn+1) by A59,A77,A78,XREAL_1:9,235
      .= 2*nn+2;
    then
A79: Lege(2,p) = (-1)|^(2*(nn+1)) by A13,A14,Th41
      .= ((-1)|^2)|^(nn+1) by NEWTON:14
      .= (1|^2)|^(nn+1) by WSIERP_1:2
      .= 1|^(nn+1) by NEWTON:15
      .= 1 by NEWTON:15;
    (p^2 -'1) div 8=(8*(8*nn^2)+8*(14*nn)+49-1) div 8 by A76,NAT_1:12
,XREAL_1:235
      .= 8*(8*nn^2+14*nn+6) div 8
      .= 8*nn^2+14*nn+6 by NAT_D:18;
    hence (-1)|^((p^2 -'1) div 8) = (-1)|^(2*(4*nn^2+7*nn+3))
      .= ((-1)|^2)|^(4*nn^2+7*nn+3) by NEWTON:14
      .= (1|^2)|^(4*nn^2+7*nn+3) by WSIERP_1:2
      .= 1|^(4*nn^2+7*nn+3) by NEWTON:15
      .= Lege(2,p) by A79,NEWTON:15;
  end;
end;

theorem
  p>2 & (p mod 8 = 1 or p mod 8 = 7) implies 2 is_quadratic_residue_mod p
proof
  assume that
A1: p>2 and
A2: p mod 8 = 1 or p mod 8 = 7;
  set nn = p div 8;
  per cases by A2;
  suppose
    p mod 8 = 1;
    then p = 8*nn+1 by NAT_D:2;
    then (p^2 -'1) div 8 = (((8*nn)^2 + 2*(8*nn)) + 1-'1) div 8
      .= 8*(8*nn^2 + 2*nn) div 8 by NAT_D:34
      .= 2*(4*nn^2 + nn) by NAT_D:18;
    then Lege(2,p) = (-1)|^(2*(4*nn^2 + nn)) by A1,Th42
      .= ((-1)|^2)|^(4*nn^2 + nn) by NEWTON:14
      .= (1|^2)|^(4*nn^2 + nn) by WSIERP_1:2
      .= 1|^(4*nn^2 + nn) by NEWTON:15
      .= 1 by NEWTON:15;
    hence thesis by Def3;
  end;
  suppose
    p mod 8 = 7;
    then p = 8*nn+7 by NAT_D:2;
    then (p^2 -'1) div 8=(8*(8*nn^2)+8*(14*nn)+49-1) div 8 by NAT_1:12
,XREAL_1:235
      .= 8*(8*nn^2+14*nn+6) div 8
      .= 2*(4*nn^2+7*nn+3) by NAT_D:18;
    then Lege(2,p) = (-1)|^(2*(4*nn^2+7*nn+3)) by A1,Th42
      .= ((-1)|^2)|^(4*nn^2+7*nn+3) by NEWTON:14
      .= (1|^2)|^(4*nn^2+7*nn+3) by WSIERP_1:2
      .= 1|^(4*nn^2+7*nn+3) by NEWTON:15
      .= 1 by NEWTON:15;
    hence thesis by Def3;
  end;
end;

theorem
  p>2 & (p mod 8 = 3 or p mod 8 = 5) implies not 2 is_quadratic_residue_mod p
proof
  assume that
A1: p>2 and
A2: p mod 8 = 3 or p mod 8 = 5;
  set nn = p div 8;
  per cases by A2;
  suppose
    p mod 8 = 3;
    then p = 8*nn+3 by NAT_D:2;
    then (p^2 -'1) div 8 = (8*(8*nn^2)+8*(6*nn)+3*3-1) div 8 by NAT_1:12
,XREAL_1:235
      .= 8*(8*nn^2+6*nn+1) div 8
      .= 8*nn^2+6*nn+1 by NAT_D:18;
    then Lege(2,p) = (-1)|^(8*nn^2+6*nn+1) by A1,Th42
      .= (-1)|^(2*(4*nn^2+3*nn))*(-1) by NEWTON:11
      .= ((-1)|^2)|^(4*nn^2+3*nn)*(-1) by NEWTON:14
      .= (1|^2)|^(4*nn^2+3*nn)*(-1) by WSIERP_1:2
      .= 1|^(4*nn^2+3*nn)*(-1) by NEWTON:15
      .= 1*(-1) by NEWTON:15
      .= -1;
    hence thesis by Def3;
  end;
  suppose
    p mod 8 = 5;
    then p = 8*nn+5 by NAT_D:2;
    then (p^2 -'1) div 8=(8*(8*nn^2)+8*(10*nn)+25-1) div 8 by NAT_1:12
,XREAL_1:235
      .= 8*(8*nn^2+10*nn+3) div 8
      .= 8*nn^2+10*nn+3 by NAT_D:18;
    then Lege(2,p) = (-1)|^(2*(4*nn^2)+2*(5*nn)+2*1+1) by A1,Th42
      .= (-1)|^(2*(4*nn^2+5*nn+1))*(-1) by NEWTON:11
      .= ((-1)|^2)|^(4*nn^2+5*nn+1)*(-1) by NEWTON:14
      .= (1|^2)|^(4*nn^2+5*nn+1)*(-1) by WSIERP_1:2
      .= 1|^(4*nn^2+5*nn+1)*(-1) by NEWTON:15
      .= 1*(-1) by NEWTON:15
      .= -1;
    hence thesis by Def3;
  end;
end;

theorem Th45:
  for a,b be Nat st a mod 2 = b mod 2 holds (-1)|^a = (-1)|^b
proof
  let a,b be Nat;
  assume a mod 2 = b mod 2;
  then a,b are_congruent_mod 2 by INT_3:12;
  then
A1: 2 divides (a-b) by INT_2:19;
  per cases;
  suppose
    a>=b;
    then reconsider l=a-b as Element of NAT by NAT_1:21;
    consider n be Nat such that
A2: l=2*n by A1,NAT_D:def 3;
    (-1)|^a = (-1)|^(b + (2*n)) by A2
      .= ((-1)|^b) * ((-1)|^(2*n)) by NEWTON:13
      .= (-1)|^b * ((-1)|^2)|^n by NEWTON:14
      .= (-1)|^b * (1|^2)|^n by WSIERP_1:2
      .= (-1)|^b * 1|^n by NEWTON:15
      .= (-1)|^b * 1 by NEWTON:15;
    hence thesis;
  end;
  suppose
    a<b;
    then reconsider l=b-a as Element of NAT by NAT_1:21;
    2 divides -(a-b) by A1,INT_2:14;
    then consider n be Nat such that
A3: l=2*n by NAT_D:def 3;
    (-1)|^b = (-1)|^(a+2*n) by A3
      .= (-1)|^a * (-1)|^(2*n) by NEWTON:13
      .= (-1)|^a * ((-1)|^2)|^n by NEWTON:14
      .= (-1)|^a * (1|^2)|^n by WSIERP_1:2
      .= (-1)|^a * 1|^n by NEWTON:15
      .= (-1)|^a * 1 by NEWTON:15;
    hence thesis;
  end;
end;

reserve f,g,h,k for FinSequence of REAL;

theorem Th46:
  len f = len h & len g = len k implies f^g-h^k = (f-h)^(g-k)
proof
  assume that
A1: len f = len h and
A2: len g = len k;
A3: len(f-h) = len f by A1,TOPREAL7:8;
  len(f^g) = len h + len k by A1,A2,FINSEQ_1:35;
  then len(f^g) = len(h^k) by FINSEQ_1:35;
  then
A4: len(f^g-h^k) = len(f^g) by TOPREAL7:8;
A5: len(g-k) = len g by A2,TOPREAL7:8;
  then len((f-h)^(g-k)) = len f + len g by A3,FINSEQ_1:35;
  then len(f^g-h^k) = len((f-h)^(g-k)) by A4,FINSEQ_1:35;
  then
A6: dom(f^g-h^k) = dom((f-h)^(g-k)) by FINSEQ_3:31;
  for d being Nat st d in dom((f-h)^(g-k)) holds ((f-h)^(g-k)).d = (f^g-h ^k).d
  proof
    let d be Nat;
    assume
A7: d in dom((f-h)^(g-k));
    per cases by A7,FINSEQ_1:38;
    suppose
A8:   d in dom(f-h);
      then
A9:   ((f-h)^(g-k)).d = (f-h).d by FINSEQ_1:def 7
        .= f.d - h.d by A8,VALUED_1:13;
A10:  dom f = dom(f-h) by A1,TOPREAL7:8;
A11:  dom h = dom(f-h) by A1,A3,FINSEQ_3:31;
      (f^g-h^k).d = (f^g).d - (h^k).d by A6,A8,FINSEQ_2:18,VALUED_1:13
        .= f.d - (h^k).d by A8,A10,FINSEQ_1:def 7
        .= f.d - h.d by A8,A11,FINSEQ_1:def 7;
      hence thesis by A9;
    end;
    suppose
      ex e being Nat st e in dom(g-k) & d = len(f-h) + e;
      then consider e such that
A12:  e in dom(g-k) and
A13:  d = len(f-h) + e;
      e in dom g by A2,A12,TOPREAL7:8;
      then
A14:  (f^g).d = g.e by A3,A13,FINSEQ_1:def 7;
      e in dom k by A2,A5,A12,FINSEQ_3:31;
      then
A15:  (h^k).d = k.e by A1,A3,A13,FINSEQ_1:def 7;
      ((f-h)^(g-k)).d = (g-k).e by A12,A13,FINSEQ_1:def 7
        .=g.e - k.e by A12,VALUED_1:13;
      hence thesis by A6,A12,A13,A14,A15,FINSEQ_1:41,VALUED_1:13;
    end;
  end;
  hence thesis by A6,FINSEQ_1:17;
end;

theorem Th47:
  for f be FinSequence of REAL,m be Real holds Sum(((len f) |-> m)
  - f) = (len f)*m - Sum f
proof
  defpred P[Nat] means for f be FinSequence of REAL,m be Real st len f = $1
  holds Sum(($1|-> m) - f) = $1 * m - Sum f;
A1: for n be Element of NAT st P[n] holds P[n+1]
  proof
    let n be Element of NAT;
    assume
A2: P[n];
    P[n+1]
    proof
      let f be FinSequence of REAL,m be Real;
A3:   len<*m*> = 1 by FINSEQ_1:56;
      assume
A4:   len f = n+1;
      then f <> {};
      then consider f1 be FinSequence of REAL,x be Element of REAL such that
A5:   f = f1^<*x*> by HILBERT2:4;
A6:   n + 1 = len f1 + 1 by A4,A5,FINSEQ_2:19;
      then
A7:   len(n|-> m)=len f1 by FINSEQ_1:def 18;
A8:   len<*x*> = 1 by FINSEQ_1:56;
      ((n+1)|-> m)-f = (n|-> m)^<*m*> - f1^<*x*> by A5,FINSEQ_2:74
        .= ((n|-> m)-f1) ^ (<*m*>-<*x*>) by A7,A8,A3,Th46
        .= ((n|-> m)-f1) ^ <*m-x*> by RVSUM_1:50;
      hence Sum(((n+1)|-> m)-f) = Sum((n|-> m)-f1) + (m-x) by RVSUM_1:104
        .= n*m - Sum f1 + (m - x) by A2,A6
        .= (n+1)*m - (Sum f1 + x)
        .= (n+1)*m - Sum f by A5,RVSUM_1:104;
    end;
    hence thesis;
  end;
A9: P[0]
  proof
    let f be FinSequence of REAL,m be Real;
    assume len f = 0;
    then Sum f = 0 by PROB_3:67;
    hence thesis by RVSUM_1:49,102;
  end;
  for n be Element of NAT holds P[n] from NAT_1:sch 1(A9,A1);
  hence thesis;
end;

reserve X for finite set,
  F for FinSequence of bool X;

definition
  let X, F;
  redefine func Card F -> Cardinal-yielding FinSequence of NAT;
  coherence
  proof
    rng Card F c= NAT
    proof
      let y be set;
      assume y in rng Card F;
      then consider x being set such that
A1:   x in dom Card F and
A2:   y = (Card F).x by FUNCT_1:def 5;
A3:   x in dom F by A1,CARD_3:def 2;
      then F.x in rng F by FUNCT_1:12;
      then reconsider Fx = F.x as finite set;
      y = card Fx by A2,A3,CARD_3:def 2;
      hence thesis;
    end;
    hence thesis by FINSEQ_1:def 4;
  end;
end;

theorem Th48:
  for f be FinSequence of bool X st len f = n & (for d,e st d in
  dom f & e in dom f & d<>e holds f.d misses f.e) holds card union rng f = Sum
  Card f
proof
  defpred P[Nat] means for f be FinSequence of bool X st len f = $1 & (for d,e
st d in dom f & e in dom f & d<>e holds f.d misses f.e) holds card union rng f
  = Sum Card f;
A1: for n be Element of NAT st P[n] holds P[n+1]
  proof
    let n be Element of NAT;
    assume
A2: P[n];
    P[n+1]
    proof
      let f be FinSequence of bool X;
      assume that
A3:   len f = n+1 and
A4:   for d,e st d in dom f & e in dom f & d<>e holds f.d misses f.e;
A5:   f <> {} by A3;
      then consider
      f1 be FinSequence of bool X,Y be Element of bool X such that
A6:   f = f1^<*Y*> by HILBERT2:4;
      Union f1 is finite;
      then reconsider F1 = union(rng f1) as finite set;
A7:   union(rng f) = union((rng f1) \/ (rng <*Y*>)) by A6,FINSEQ_1:44
        .= union((rng f1) \/ {Y}) by FINSEQ_1:55
        .= F1 \/ union {Y} by ZFMISC_1:96
        .= F1 \/ Y by ZFMISC_1:31;
A8:   n+1 = len f1 +1 by A3,A6,FINSEQ_2:19;
      F1 misses Y
      proof
A9:     n+1 in dom f by A3,A5,FINSEQ_5:6;
        assume F1 meets Y;
        then consider x be set such that
A10:    x in F1 /\ Y by XBOOLE_0:4;
        x in F1 by A10,XBOOLE_0:def 4;
        then consider Z be set such that
A11:    x in Z and
A12:    Z in rng f1 by TARSKI:def 4;
        consider k be Nat such that
A13:    k in dom f1 and
A14:    f1.k = Z by A12,FINSEQ_2:11;
        k <= n by A8,A13,FINSEQ_3:27;
        then
A15:    k < n + 1 by NAT_1:13;
        k in dom f by A6,A13,FINSEQ_2:18;
        then f.(n+1) misses f.k by A4,A15,A9;
        then Y misses f.k by A6,A8,FINSEQ_1:59;
        then
A16:    Y misses Z by A6,A13,A14,FINSEQ_1:def 7;
        x in Y \/ Z by A11,XBOOLE_0:def 3;
        then not x in Y by A11,A16,XBOOLE_0:5;
        hence contradiction by A10,XBOOLE_0:def 4;
      end;
      then
A17:  card F1 + card Y = card(F1\/Y) by CARD_2:53;
      reconsider gg = <*card Y*> as FinSequence of NAT;
A18:  Card f = Card f1 ^ Card<*Y*> by A6,PRE_POLY:25
        .= Card f1 ^ gg by PRE_POLY:24;
      for d,e st d in dom f1 & e in dom f1 & d<>e holds f1.d misses f1.e
      proof
        let d,e;
        assume that
A19:    d in dom f1 and
A20:    e in dom f1 and
A21:    d<>e;
A22:    f.e = f1.e by A6,A20,FINSEQ_1:def 7;
A23:    e in dom f by A6,A20,FINSEQ_2:18;
A24:    d in dom f by A6,A19,FINSEQ_2:18;
        f.d = f1.d by A6,A19,FINSEQ_1:def 7;
        hence thesis by A4,A21,A22,A24,A23;
      end;
      then card union rng f1 = Sum Card f1 by A2,A8;
      hence thesis by A17,A18,A7,RVSUM_1:104;
    end;
    hence thesis;
  end;
A25: P[0]
  proof
    let f be FinSequence of bool X;
    assume that
A26: len f = 0 and
    for d,e st d in dom f & e in dom f & d<>e holds f.d misses f.e;
A27: Card {} = {};
    f = {} by A26;
    hence thesis by A27,CARD_1:47,RVSUM_1:102,ZFMISC_1:2;
  end;
  for n be Element of NAT holds P[n] from NAT_1:sch 1(A25,A1);
  hence thesis;
end;

Lm3: Sum(fp) is Element of NAT;

reserve q for Prime;

theorem Th49:
  p>2 & q>2 & p<>q implies Lege(p,q)*Lege(q,p)=(-1)|^(((p-'1) div
  2)*((q-'1) div 2))
proof
  assume that
A1: p>2 and
A2: q>2 and
A3: p<>q;
A4: q,p are_relative_prime by A3,INT_2:47;
  then
A5: q gcd p = 1 by INT_2:def 4;
  reconsider p,q as prime Element of NAT by ORDINAL1:def 13;
  set p9 = (p-'1) div 2;
A6: p>1 by INT_2:def 5;
  then
A7: p-'1 = p-1 by XREAL_1:235;
  then
A8: p-'1>0 by A6,XREAL_1:52;
  p is odd by A1,PEPIN:17;
  then
A9: p-'1 is even by A7,HILBERT3:2;
  then
A10: 2 divides (p-'1) by PEPIN:22;
  then
A11: p-'1 = 2*p9 by NAT_D:3;
  then p9 divides (p-'1) by NAT_D:def 3;
  then p9 <= (p-'1) by A8,NAT_D:7;
  then
A12: p9 < p by A7,XREAL_1:148,XXREAL_0:2;
  set f1 = q*idseq p9;
A13: for d st d in dom f1 holds f1.d = q*d
  proof
    let d;
    assume
A14: d in dom f1;
    then d in dom idseq p9 by VALUED_1:def 5;
    then d in Seg len idseq p9 by FINSEQ_1:def 3;
    then
A15: d is Element of Seg p9 by FINSEQ_1:def 18;
    f1.d = q * (idseq p9).d by A14,VALUED_1:def 5;
    hence thesis by A15,FINSEQ_2:57;
  end;
A16: for d being Nat st d in dom f1 holds f1.d in NAT
  proof
    let d be Nat;
    assume d in dom f1;
    then f1.d = q*d by A13;
    hence thesis;
  end;
  dom f1 = dom idseq p9 by VALUED_1:def 5;
  then
A17: len f1 = len idseq p9 by FINSEQ_3:31;
  then
A18: len f1 = p9 by FINSEQ_1:def 18;
  set q9 = (q-'1) div 2;
  set g1 = p*idseq q9;
A19: for d st d in dom g1 holds g1.d = p*d
  proof
    let d;
    assume
A20: d in dom g1;
    then d in dom idseq q9 by VALUED_1:def 5;
    then d in Seg len idseq q9 by FINSEQ_1:def 3;
    then
A21: d is Element of Seg q9 by FINSEQ_1:def 18;
    g1.d = p * (idseq q9).d by A20,VALUED_1:def 5;
    hence thesis by A21,FINSEQ_2:57;
  end;
A22: for d being Nat st d in dom g1 holds g1.d in NAT
  proof
    let d be Nat;
    assume d in dom g1;
    then g1.d = p*d by A19;
    hence thesis;
  end;
  dom g1 = dom idseq q9 by VALUED_1:def 5;
  then len g1 = len idseq q9 by FINSEQ_3:31;
  then
A23: len g1 = q9 by FINSEQ_1:def 18;
  reconsider g1 as FinSequence of NAT by A22,FINSEQ_2:14;
  set g3 = g1 mod q;
  set g4 = Sgm rng g3;
A24: len g3 = len g1 by EULER_2:def 1;
  then
A25: dom g1 = dom g3 by FINSEQ_3:31;
  set XX = {k where k is Element of NAT:k in rng g4 & k>q/2};
  for x being set st x in XX holds x in rng g4
  proof
    let x be set;
    assume x in XX;
    then ex k being Element of NAT st x = k & k in rng g4 & k > q/2;
    hence thesis;
  end;
  then
A26: XX c= rng g4 by TARSKI:def 3;
  reconsider f1 as FinSequence of NAT by A16,FINSEQ_2:14;
  deffunc F(Nat) = f1.$1 div p;
  consider f2 be FinSequence such that
A27: len f2 = p9 & for d being Nat st d in dom f2 holds f2.d = F(d) from
  FINSEQ_1:sch 2;
A28: q>1 by INT_2:def 5;
  then
A29: q-'1 = q-1 by XREAL_1:235;
  then
A30: q-'1 >0 by A28,XREAL_1:52;
  q >= 2+1 by A2,NAT_1:13;
  then q-1 >= 3-1 by XREAL_1:11;
  then
A31: q9 >= 1 by A29,NAT_2:15;
  then len g3 >=1 by A23,EULER_2:def 1;
  then g3 <> {};
  then rng g3 is finite non empty Subset of NAT by FINSEQ_1:def 4;
  then consider n2 be Element of NAT such that
A32: rng g3 c= Seg n2 \/ {0} by HEYTING3:3;
  deffunc F(Nat) = g1.$1 div q;
  consider g2 be FinSequence such that
A33: len g2 = q9 & for d being Nat st d in dom g2 holds g2.d = F(d)
  from FINSEQ_1:sch 2;
  for d being Nat st d in dom g2 holds g2.d in NAT
  proof
    let d be Nat;
    assume d in dom g2;
    then g2.d = g1.d div q by A33;
    hence thesis;
  end;
  then reconsider g2 as FinSequence of NAT by FINSEQ_2:14;
A34: dom g1 = dom g2 by A23,A33,FINSEQ_3:31;
A35: for d st d in dom g1 holds g1.d = g2.d * q + g3.d
  proof
    let d;
    assume
A36: d in dom g1;
    then
A37: g3.d = g1.d mod q by EULER_2:def 1;
    g2.d = g1.d div q by A33,A34,A36;
    hence thesis by A37,NAT_D:2;
  end;
  q is odd by A2,PEPIN:17;
  then
A38: q-'1 is even by A29,HILBERT3:2;
  then
A39: 2 divides (q-'1) by PEPIN:22;
  then
A40: q-'1 = 2*q9 by NAT_D:3;
  then q9 divides (q-'1) by NAT_D:def 3;
  then q9 <= (q-'1) by A30,NAT_D:7;
  then
A41: q9 < q by A29,XREAL_1:148,XXREAL_0:2;
  not 0 in rng g3
  proof
    assume 0 in rng g3;
    then consider a be Nat such that
A42: a in dom g3 and
A43: g3.a = 0 by FINSEQ_2:11;
    a in dom g1 by A24,A42,FINSEQ_3:31;
    then
A44: g1.a = g2.a * q + 0 by A35,A43;
    a in dom g1 by A24,A42,FINSEQ_3:31;
    then p*a = g2.a * q by A19,A44;
    then
A45: q divides p*a by NAT_D:def 3;
    a >= 1 by A42,FINSEQ_3:27;
    then
A46: q <= a by A4,A45,NAT_D:7,PEPIN:3;
    a <= q9 by A23,A24,A42,FINSEQ_3:27;
    hence contradiction by A41,A46,XXREAL_0:2;
  end;
  then
A47: {0} misses rng g3 by ZFMISC_1:56;
  then
