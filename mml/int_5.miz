A191: dom Pr = Seg p9 & for d being Nat st d in Seg p9 holds G[d,Pr.d]
    from FINSEQ_1:sch 5(A190);
A192: dom Card Pr = dom Pr by CARD_3:def 2
      .= dom f2 by A27,A191,FINSEQ_1:def 3;
    for d being Nat st d in dom Card Pr holds (Card Pr).d = f2.d
    proof
      let d be Nat;
      assume
A193: d in dom Card Pr;
      then d in Seg p9 by A27,A192,FINSEQ_1:def 3;
      then consider m be Element of A such that
A194: m = d and
A195: Pr.d = {[m,y] where y is Element of B:z.(m,y)>0} by A191;
      Pr.d = [:{m},Seg(f2.m):]
      proof
        set L = [:{m},Seg(f2.m):];
A196:   L c= Pr.d
        proof
          now
            assume q mod p = 0;
            then
A197:       p divides q by PEPIN:6;
            then p <= q by NAT_D:7;
            then p < q by A3,XXREAL_0:1;
            hence contradiction by A6,A197,NAT_4:12;
          end;
          then
A198:     -(q div p) = (-q) div p + 1 by WSIERP_1:49;
          2 divides (p-'1)*q by A10,NAT_D:9;
          then (p-'1)*q mod 2 = 0 by PEPIN:6;
          then ((p-'1)*q) div 2 = (p-'1)*q/2 by REAL_3:4;
          then
A199:     (p9*q) div p = ((p-1)*q) div (2*p) by A7,A11,NAT_2:29
            .= ((p*q - q) div p) div 2 by PRE_FF:5
            .= (q+(-(q div p)-1)) div 2 by A198,INT_3:8
            .= (2*q9+(-(q div p))) div 2 by A29,A40
            .= q9+((-(q div p)) div 2) by INT_3:8;
A200:     (p9*q) div p <= q9
          proof
            per cases;
            suppose
              (q div p) mod 2 = 0;
              then (-(q div p)) div 2 = -((q div p) div 2) by WSIERP_1:50
                .= -(q div (2*p)) by NAT_2:29;
              then (p9*q) div p = q9-(q div (2*p)) by A199;
              hence thesis by XREAL_1:45;
            end;
            suppose
              (q div p) mod 2 <> 0;
              then -((q div p) div 2) = (-(q div p)) div 2 +1 by WSIERP_1:49;
              then (-(q div p)) div 2 = -((q div p) div 2)-1
                .= -(q div (2*p)) - 1 by NAT_2:29;
              then (p9*q) div p = q9 -((q div (2*p)) + 1) by A199;
              hence thesis by XREAL_1:45;
            end;
          end;
          m <= p9 by FINSEQ_1:3;
          then m*q <= p9*q by XREAL_1:66;
          then (m*q) div p <= (p9*q) div p by NAT_2:26;
          then
A201:     (m*q) div p <= q9 by A200,XXREAL_0:2;
          m in Seg p9;
          then
A202:     m in dom f1 by A18,FINSEQ_1:def 3;
          then
A203:     f2.m = f1.m div p by A27,A99
            .= (m*q) div p by A13,A202;
          now
            assume m*q/p is integer;
            then
A204:       p divides m*q by WSIERP_1:22;
A205:       m <= p9 by FINSEQ_1:3;
            0+1 <= m by FINSEQ_1:3;
            then p <= m by A5,A204,NAT_D:7,WSIERP_1:37;
            hence contradiction by A12,A205,XXREAL_0:2;
          end;
          then
A206:     [\m*q/p/] < m*q/p by INT_1:48;
          let l be set;
          assume l in L;
          then consider x,y be set such that
A207:     x in {m} and
A208:     y in Seg(f2.m) and
A209:     l = [x,y] by ZFMISC_1:def 2;
          reconsider y as Element of NAT by A208;
A210:     1 <= y by A208,FINSEQ_1:3;
          y <= f2.m by A208,FINSEQ_1:3;
          then y <= q9 by A201,A203,XXREAL_0:2;
          then reconsider y as Element of B by A210,FINSEQ_1:3;
          y <= [\m*q/p/] by A208,A203,FINSEQ_1:3;
          then y < m*q/p by A206,XXREAL_0:2;
          then y*p < (m*q)/p*p by XREAL_1:70;
          then y*p < m*q by XCMPLX_1:88;
          then y/q < m/p by XREAL_1:108;
          then m/p - y/q > 0 by XREAL_1:52;
          then z.(m,y) > 0 by A189;
          then [m,y] in Pr.d by A195;
          hence thesis by A207,A209,TARSKI:def 1;
        end;
        Pr.d c= L
        proof
          let l be set;
A211:     m in {m} by TARSKI:def 1;
          m in Seg p9;
          then
A212:     m in dom f1 by A18,FINSEQ_1:def 3;
          assume l in Pr.d;
          then consider y1 be Element of B such that
A213:     l = [m,y1] and
A214:     z.(m,y1) > 0 by A195;
          m/p - y1/q > 0 by A189,A214;
          then m/p - y1/q + y1/q > 0 + y1/q by XREAL_1:8;
          then m/p*q > y1/q*q by XREAL_1:70;
          then (m*q)/p > y1 by XCMPLX_1:88;
          then ((m*q) div p) >= y1 by INT_1:81;
          then ((f1.m) div p) >= y1 by A13,A212;
          then
A215:     y1 <= f2.m by A27,A99,A212;
          y1 >= 1 by FINSEQ_1:3;
          then y1 in Seg(f2.m) by A215,FINSEQ_1:3;
          hence thesis by A213,A211,ZFMISC_1:def 2;
        end;
        hence thesis by A196,XBOOLE_0:def 10;
      end;
      then card(Pr.d) = card [:Seg(f2.m),{m}:] by CARD_2:11
        .= card Seg(f2.m) by CARD_2:13;
      then
A216: card(Pr.d) = card(f2.d) by A194,FINSEQ_1:76
        .= f2.d by CARD_1:def 5;
      d in dom Pr by A193,CARD_3:def 2;
      hence thesis by A216,CARD_3:def 2;
    end;
    then
A217: Card Pr = f2 by A192,FINSEQ_1:17;
    defpred K[set,set] means ex y be Element of B st $1=y & $2 = {[x,y] where
    x is Element of A:z.(x,y)<0};
A218: for d being Nat st d in Seg q9 ex x1 be Element of bool dom z st K[d ,x1]
    proof
      let d be Nat;
      assume d in Seg q9;
      then reconsider d as Element of B;
      take x1 = {[x,d] where x is Element of A:z.(x,d)<0};
      x1 c= dom z
      proof
        let l be set;
        assume l in x1;
        then ex xx be Element of A st [xx,d] = l & z.(xx,d) < 0;
        then l in [:A,B:];
        hence thesis by FUNCT_2:def 1;
      end;
      hence thesis;
    end;
    consider Pk be FinSequence of bool dom z such that
A219: dom Pk = Seg q9 & for d being Nat st d in Seg q9 holds K[d,Pk.d]
    from FINSEQ_1:sch 5(A218);
A220: dom Card Pk = Seg(len g2) by A33,A219,CARD_3:def 2
      .= dom g2 by FINSEQ_1:def 3;
A221: for d being Nat st d in dom Card Pk holds (Card Pk).d = g2.d
    proof
      let d be Nat;
      assume
A222: d in dom Card Pk;
      then d in Seg q9 by A33,A220,FINSEQ_1:def 3;
      then consider n be Element of B such that
A223: n = d and
A224: Pk.d = {[x,n] where x is Element of A:z.(x,n)<0} by A219;
      Pk.d = [:Seg(g2.n),{n}:]
      proof
        set L = [:Seg(g2.n),{n}:];
A225:   L c= Pk.d
        proof
          now
            assume p mod q = 0;
            then
A226:       q divides p by PEPIN:6;
            then q <= p by NAT_D:7;
            then q < p by A3,XXREAL_0:1;
            hence contradiction by A28,A226,NAT_4:12;
          end;
          then
A227:     -(p div q) = (-p) div q + 1 by WSIERP_1:49;
          2 divides (q-'1)*p by A39,NAT_D:9;
          then (q-'1)*p mod 2 = 0 by PEPIN:6;
          then ((q-'1)*p) div 2 = (q-'1)*p/2 by REAL_3:4;
          then
A228:     (q9*p) div q = ((q-1)*p) div (2*q) by A29,A40,NAT_2:29
            .= ((q*p - p) div q) div 2 by PRE_FF:5
            .= (p+(-(p div q)-1)) div 2 by A227,INT_3:8
            .= (2*p9-(p div q)) div 2 by A7,A11
            .= p9+((-(p div q)) div 2) by INT_3:8;
A229:     (q9*p) div q <= p9
          proof
            per cases;
            suppose
              (p div q) mod 2 = 0;
              then (-(p div q)) div 2 = -((p div q) div 2) by WSIERP_1:50
                .= -(p div (2*q)) by NAT_2:29;
              then (q9*p) div q = p9-(p div (2*q)) by A228;
              hence thesis by XREAL_1:45;
            end;
            suppose
              (p div q) mod 2 <> 0;
              then -((p div q) div 2) = (-(p div q)) div 2 +1 by WSIERP_1:49;
              then (-(p div q)) div 2 = -((p div q) div 2)-1
                .= -(p div (2*q)) - 1 by NAT_2:29;
              then (q9*p) div q = p9 -((p div (2*q)) + 1) by A228;
              hence thesis by XREAL_1:45;
            end;
          end;
          n in Seg q9;
          then
A230:     n in dom g1 by A23,FINSEQ_1:def 3;
          then
A231:     g2.n = g1.n div q by A33,A34
            .= (n*p) div q by A19,A230;
          let l be set;
          assume l in L;
          then consider x,y be set such that
A232:     x in Seg(g2.n) and
A233:     y in {n} and
A234:     l = [x,y] by ZFMISC_1:def 2;
          reconsider x as Element of NAT by A232;
A235:     x <= g2.n by A232,FINSEQ_1:3;
          n <= q9 by FINSEQ_1:3;
          then n*p <= q9*p by XREAL_1:66;
          then (n*p) div q <= (q9*p) div q by NAT_2:26;
          then (n*p) div q <= p9 by A229,XXREAL_0:2;
          then
A236:     x <= p9 by A231,A235,XXREAL_0:2;
          1 <= x by A232,FINSEQ_1:3;
          then reconsider x as Element of A by A236,FINSEQ_1:3;
          now
            assume n*p/q is integer;
            then
A237:       q divides n*p by WSIERP_1:22;
A238:       n <= q9 by FINSEQ_1:3;
            0+1 <= n by FINSEQ_1:3;
            then q <= n by A5,A237,NAT_D:7,WSIERP_1:37;
            hence contradiction by A41,A238,XXREAL_0:2;
          end;
          then [\n*p/q/] < n*p/q by INT_1:48;
          then x < n*p/q by A231,A235,XXREAL_0:2;
          then x*q < (n*p)/q*q by XREAL_1:70;
          then x*q < n*p by XCMPLX_1:88;
          then x/p - n/q < 0 by XREAL_1:51,108;
          then z.(x,n) < 0 by A189;
          then [x,n] in Pk.d by A224;
          hence thesis by A233,A234,TARSKI:def 1;
        end;
        Pk.d c= L
        proof
          let l be set;
A239:     n in {n} by TARSKI:def 1;
          n in Seg q9;
          then
A240:     n in dom g1 by A23,FINSEQ_1:def 3;
          assume l in Pk.d;
          then consider x be Element of A such that
A241:     l = [x,n] and
A242:     z.(x,n) < 0 by A224;
          x/p - n/q < 0 by A189,A242;
          then x/p - n/q + n/q < 0 + n/q by XREAL_1:8;
          then x/p*p < n/q*p by XREAL_1:70;
          then x < (n*p)/q by XCMPLX_1:88;
          then x <= (n*p) div q by INT_1:81;
          then ((g1.n) div q) >= x by A19,A240;
          then
A243:     x <= g2.n by A33,A34,A240;
          x >= 1 by FINSEQ_1:3;
          then x in Seg(g2.n) by A243,FINSEQ_1:3;
          hence thesis by A241,A239,ZFMISC_1:def 2;
        end;
        hence thesis by A225,XBOOLE_0:def 10;
      end;
      then card(Pk.d) = card Seg(g2.n) by CARD_2:13;
      then
A244: card(Pk.d) = card(g2.d) by A223,FINSEQ_1:76
        .= g2.d by CARD_1:def 5;
      d in dom Pk by A222,CARD_3:def 2;
      hence thesis by A244,CARD_3:def 2;
    end;
    reconsider U1 = union rng Pr, U2 = union rng Pk as finite Subset of dom z
    by PROB_3:53;
    dom z c= U1 \/ U2
    proof
      let l be set;
      assume l in dom z;
      then consider x,y be set such that
A245: x in A and
A246: y in B and
A247: l = [x,y] by ZFMISC_1:def 2;
      reconsider y as Element of B by A246;
      reconsider x as Element of A by A245;
A248: z.(x,y) <> 0
      proof
        assume z.(x,y) = 0;
        then x/p - y/q = 0 by A189;
        then x*q = y*p by XCMPLX_1:96;
        then
A249:   p divides x*q by NAT_D:def 3;
A250:   x <= p9 by FINSEQ_1:3;
        x >= 0 + 1 by FINSEQ_1:3;
        then p <= x by A5,A249,NAT_D:7,WSIERP_1:37;
        hence contradiction by A12,A250,XXREAL_0:2;
      end;
      per cases by A248;
      suppose
A251:   z.(x,y) > 0;
        G[x,Pr.x] by A191;
        then l in Pr.x by A247,A251;
        then l in Union Pr by A191,PROB_3:54;
        hence thesis by XBOOLE_0:def 3;
      end;
      suppose
A252:   z.(x,y) < 0;
        K[y,Pk.y] by A219;
        then l in Pk.y by A247,A252;
        then l in Union Pk by A219,PROB_3:54;
        hence thesis by XBOOLE_0:def 3;
      end;
    end;
    then
A253: U1 \/ U2 = dom z by XBOOLE_0:def 10;
A254: U1 misses U2
    proof
      assume U1 meets U2;
      then consider l be set such that
A255: l in U1 and
A256: l in U2 by XBOOLE_0:3;
      l in Union Pk by A256;
      then consider k2 be Nat such that
A257: k2 in dom Pk and
A258: l in Pk.k2 by PROB_3:54;
      l in Union Pr by A255;
      then consider k1 be Nat such that
A259: k1 in dom Pr and
A260: l in Pr.k1 by PROB_3:54;
      reconsider k1,k2 as Element of NAT by ORDINAL1:def 13;
      consider n1 be Element of B such that
      n1 = k2 and
A261: Pk.k2 = {[x,n1] where x is Element of A:z.(x,n1)<0} by A219,A257;
      consider n2 be Element of A such that
A262: l = [n2,n1] and
A263: z.(n2,n1) < 0 by A258,A261;
      consider m1 be Element of A such that
      m1 = k1 and
A264: Pr.k1 = {[m1,y] where y is Element of B:z.(m1,y)>0} by A191,A259;
A265: ex m2 be Element of B st l = [m1,m2] & z.(m1,m2) > 0 by A260,A264;
      then m1 = n2 by A262,ZFMISC_1:33;
      hence contradiction by A265,A262,A263,ZFMISC_1:33;
    end;
A266: for d,e st d in dom Pk & e in dom Pk & d<>e holds Pk.d misses Pk.e
    proof
      let d,e;
      assume that
A267: d in dom Pk and
A268: e in dom Pk and
A269: d<>e;
      consider y2 be Element of B such that
A270: y2=e and
A271: Pk.e = {[x,y2] where x is Element of A:z.(x,y2)<0} by A219,A268;
      consider y1 be Element of B such that
A272: y1=d and
A273: Pk.d = {[x,y1] where x is Element of A:z.(x,y1)<0} by A219,A267;
      now
        assume not Pk.d misses Pk.e;
        then consider l be set such that
A274:   l in Pk.d and
A275:   l in Pk.e by XBOOLE_0:3;
A276:   ex x2 be Element of A st l = [x2,y2] & z.(x2,y2) < 0 by A271,A275;
        ex x1 be Element of A st l = [x1,y1] & z.(x1,y1) < 0 by A273,A274;
        hence contradiction by A269,A272,A270,A276,ZFMISC_1:33;
      end;
      hence thesis;
    end;
    len Pk = q9 by A219,FINSEQ_1:def 3;
    then
A277: card union rng Pk = Sum Card Pk by A266,Th48;
A278: for d,e st d in dom Pr & e in dom Pr & d<>e holds Pr.d misses Pr.e
    proof
      let d,e;
      assume that
A279: d in dom Pr and
A280: e in dom Pr and
A281: d<>e;
      consider x2 be Element of A such that
A282: x2=e and
A283: Pr.e = {[x2,y] where y is Element of B:z.(x2,y)>0} by A191,A280;
      consider x1 be Element of A such that
A284: x1=d and
A285: Pr.d = {[x1,y] where y is Element of B:z.(x1,y)>0} by A191,A279;
      now
        assume not Pr.d misses Pr.e;
        then consider l be set such that
A286:   l in Pr.d and
A287:   l in Pr.e by XBOOLE_0:3;
A288:   ex y2 be Element of B st l = [x2,y2] & z.(x2,y2) > 0 by A283,A287;
        ex y1 be Element of B st l = [x1,y1] & z.(x1,y1) > 0 by A285,A286;
        hence contradiction by A281,A284,A282,A288,ZFMISC_1:33;
      end;
      hence thesis;
    end;
    len Pr = p9 by A191,FINSEQ_1:def 3;
    then card union rng Pr = Sum Card Pr by A278,Th48;
    then card(U1 \/ U2) = (Sum Card Pr) + (Sum Card Pk) by A277,A254,CARD_2:53;
    then (Sum Card Pr) + (Sum Card Pk) = card [:A,B:] by A253,FUNCT_2:def 1
      .= card A * card B by CARD_2:65
      .= p9* card B by FINSEQ_1:78
      .= p9 * q9 by FINSEQ_1:78;
    hence thesis by A217,A220,A221,FINSEQ_1:17;
  end;
  dom(p*f2) = dom f2 by VALUED_1:def 5;
  then
A289: len(p*f2) = p9 by A27,FINSEQ_3:31;
  p*f2 is Element of NAT* by FINSEQ_1:def 11;
  then p*f2 in p9-tuples_on NAT by A289;
  then
A290: p*f2 is Element of p9-tuples_on REAL by FINSEQ_2:129;
A291: p9 = ((p-'1)+1) div 2 by A9,NAT_2:28
    .= p div 2 by A6,XREAL_1:237;
  reconsider X as finite Subset of NAT by A185,XBOOLE_1:1;
  set m = card X;
  reconsider Y = rng f4 \ X as finite Subset of NAT;
A292: f3 is Element of NAT* by FINSEQ_1:def 11;
  len f3 = p9 by A17,A95,FINSEQ_1:def 18;
  then f3 in p9-tuples_on NAT by A292;
  then
A293: f3 is Element of p9-tuples_on REAL by FINSEQ_2:129;
A294: rng f3 c= Seg n1 by A98,A107,XBOOLE_1:73;
  then
A295: rng f4 = rng f3 by FINSEQ_1:def 13;
  then
A296: X c= Seg n1 by A294,A185,XBOOLE_1:1;
A297: dom (p*f2+f3) = dom (p*f2) /\ dom f3 by VALUED_1:def 1
    .= dom f2 /\ dom f3 by VALUED_1:def 5
    .= dom f1 by A96,A99;
  for d being Nat st d in dom f1 holds f1.d = (p*f2+f3).d
  proof
    let d be Nat;
    assume
A298: d in dom f1;
    then
A299: d in dom (p*f2) by A99,VALUED_1:def 5;
    (p*f2+f3).d = (p*f2).d + f3.d by A297,A298,VALUED_1:def 1;
    hence (p*f2+f3).d = p * f2.d + f3.d by A299,VALUED_1:def 5
      .= f1.d by A100,A298;
  end;
  then f1 = p*f2 + f3 by A297,FINSEQ_1:17;
  then
A300: Sum f1 = Sum(p*f2) + Sum f3 by A290,A293,RVSUM_1:119
    .= p*(Sum f2) + Sum f3 by RVSUM_1:117;
A302: rng f4 \ X c= rng f4 by XBOOLE_1:36;
  then
A303: Y c= Seg n1 by A294,A295,XBOOLE_1:1;
A304: len f3 = card rng f4 by A126,A295,FINSEQ_4:77;
  then reconsider n = p9 - m as Element of NAT by A18,A95,A185,NAT_1:21,44;
A305: f4 = (f4|n)^(f4/^n) by RFINSEQ:21;
  then
A306: f4/^n is one-to-one by A108,FINSEQ_3:98;
  f4 is FinSequence of REAL by FINSEQ_2:27;
  then
A307: Sum f4 = Sum f3 by A126,A295,A108,RFINSEQ:22,39;
  for k,l being Element of NAT st k in Y & l in X holds k < l
  proof
    let k,l be Element of NAT;
    assume that
A308: k in Y and
A309: l in X;
A310: not k in X by A308,XBOOLE_0:def 5;
A311: ex l1 being Element of NAT st l1 = l & l1 in rng f4 & l1>p/2 by A309;
    k in rng f4 by A308,XBOOLE_0:def 5;
    then k <= p/2 by A310;
    hence thesis by A311,XXREAL_0:2;
  end;
  then Sgm (Y\/X) = (Sgm Y)^(Sgm X) by A296,A303,FINSEQ_3:48;
  then Sgm (rng f4 \/ X) = (Sgm Y)^(Sgm X) by XBOOLE_1:39;
  then
A312: f4 = (Sgm Y)^(Sgm X) by A295,A185,XBOOLE_1:12;
  then Sum f4 = Sum(Sgm Y) + Sum(Sgm X) by RVSUM_1:105;
  then
A313: q*(Sum idseq p9)=p*(Sum f2)+Sum(Sgm Y) + Sum(Sgm X) by A300,A307,
RVSUM_1:117;
A314: len Sgm Y = card Y by A294,A295,A302,FINSEQ_3:44,XBOOLE_1:1
    .= p9 - m by A18,A95,A185,A304,CARD_2:63;
  then
A315: f4/^n = Sgm X by A312,FINSEQ_5:40;
A316: f4|n = Sgm Y by A312,A314,FINSEQ_3:122,FINSEQ_6:12;
A317: f4|n is one-to-one by A108,A305,FINSEQ_3:98;
  Lege(q,p) = (-1)|^(Sum f2)
  proof
      set f5 = (m|->p)-(f4/^n);
      set f6 = (f4|n)^f5;
A324: rng idseq p9 = Seg p9 by RELAT_1:71;
A325: f4/^n is FinSequence of REAL by FINSEQ_2:27;
A326: len(f4|n) = n by A127,FINSEQ_1:80,XREAL_1:45;
A327: len(f4/^n) = (len f4 -' n) by RFINSEQ:42
        .= len f4 - n by A127,XREAL_1:45,235
        .= m by A127;
A328: dom f5 = dom(m |-> p) /\ dom(f4/^n) by VALUED_1:12
        .= (Seg len (m |-> p)) /\ dom(f4/^n) by FINSEQ_1:def 3
        .= (Seg len(f4/^n))/\dom(f4/^n) by A327,FINSEQ_1:def 18
        .= dom(f4/^n) /\ dom(f4/^n) by FINSEQ_1:def 3
        .= dom(f4/^n);
      then
A329: len f5 = len(f4/^n) by FINSEQ_3:31;
A330: for d st d in dom f5 holds f5.d = p - (f4/^n).d
      proof
        let d;
        assume
A331:   d in dom f5;
        then d in Seg m by A327,A328,FINSEQ_1:def 3;
        then (m |-> p).d = p by FINSEQ_2:71;
        hence thesis by A331,VALUED_1:13;
      end;
A332: for d st d in dom f5 holds f5.d > 0 & f5.d <= p9
      proof
        let d;
        reconsider w = f5.d as Element of INT by INT_1:def 2;
        assume
A333:   d in dom f5;
        then (Sgm X).d in rng Sgm X by A315,A328,FUNCT_1:12;
        then (Sgm X).d in X by A296,FINSEQ_1:def 13;
        then
A334:   ex ll be Element of NAT st ll = (Sgm X).d & ll in rng f3 &
        ll > p/2 by A295;
        then consider e being Nat such that
A335:   e in dom f3 and
A336:   f3.e = (f4/^n).d by A315,FINSEQ_2:11;
        (f4/^n).d = f1.e mod p by A96,A335,A336,EULER_2:def 1;
        then
A337:   (f4/^n).d < p by NAT_D:1;
A338:   f5.d = p - (f4/^n).d by A330,A333;
        then w < p - p/2 by A315,A334,XREAL_1:12;
        hence thesis by A291,A338,A337,INT_1:81,XREAL_1:52;
      end;
      for d being Nat st d in dom f5 holds f5.d in NAT
      proof
        let d be Nat;
        assume d in dom f5;
        then f5.d > 0 by A332;
        hence thesis by INT_1:16;
      end;
      then reconsider f5 as FinSequence of NAT by FINSEQ_2:14;
      f5 is FinSequence of NAT;
      then reconsider f6 as FinSequence of NAT by FINSEQ_1:96;
A339: f6 is FinSequence of REAL by FINSEQ_2:27;
A340: n <= len f4 by A127,XREAL_1:45;
A341: rng(f4|n) misses rng f5
      proof
        assume not rng(f4|n) misses rng f5;
        then consider x be set such that
A342:   x in rng(f4|n) and
A343:   x in rng f5 by XBOOLE_0:3;
        consider e being Nat such that
A344:   e in dom f5 and
A345:   f5.e = x by A343,FINSEQ_2:11;
        x = p - (f4/^n).e by A330,A344,A345;
        then
A346:   x = p - f4.(e+n) by A340,A328,A344,RFINSEQ:def 2;
        e+n in dom f4 by A328,A344,FINSEQ_5:29;
        then consider e1 be Nat such that
A347:   e1 in dom f3 and
A348:   f3.e1 = f4.(e+n) by A295,FINSEQ_2:11,FUNCT_1:12;
A349:   e1 <= p9 by A18,A95,A347,FINSEQ_3:27;
        rng(f4|n) c= rng f4 by FINSEQ_5:21;
        then consider d1 be Nat such that
A350:   d1 in dom f3 and
A351:   f3.d1 = x by A295,A342,FINSEQ_2:11;
        d1 <= p9 by A18,A95,A350,FINSEQ_3:27;
        then d1+e1 <= p9+p9 by A349,XREAL_1:9;
        then
A352:   d1+e1 < p by A7,A11,XREAL_1:148,XXREAL_0:2;
        x = f1.d1 mod p by A96,A350,A351,EULER_2:def 1;
        then (f1.d1 mod p) + f4.(e+n) = p by A346;
        then (f1.d1 mod p) + (f1.e1 mod p) = p by A96,A347,A348,EULER_2:def 1;
        then ((f1.d1 mod p)+(f1.e1 mod p)) mod p = 0 by NAT_D:25;
        then (f1.d1 + f1.e1) mod p = 0 by EULER_2:8;
        then p divides (f1.d1 + f1.e1) by PEPIN:6;
        then p divides (d1*q + f1.e1) by A13,A96,A350;
        then p divides (d1*q + e1*q) by A13,A96,A347;
        then
A353:   p divides (d1+e1)*q;
        d1 >= 1 by A350,FINSEQ_3:27;
        hence contradiction by A4,A353,A352,NAT_D:7,PEPIN:3;
      end;
      for d,e being Element of NAT st 1<=d & d<e & e<=len f5 holds f5.d
      <> f5. e
      proof
        let d,e be Element of NAT;
        assume that
A354:   1<=d and
A355:   d<e and
A356:   e<=len f5;
        1<=e by A354,A355,XXREAL_0:2;
        then
A357:   e in dom f5 by A356,FINSEQ_3:27;
        then
A358:   f5.e = p - (f4/^n).e by A330;
        d<len f5 by A355,A356,XXREAL_0:2;
        then
A359:   d in dom f5 by A354,FINSEQ_3:27;
        then f5.d = p - (f4/^n).d by A330;
        hence thesis by A306,A328,A355,A359,A357,A358,FUNCT_1:def 8;
      end;
      then len f5 = card rng f5 by GRAPH_5:10;
      then f5 is one-to-one by FINSEQ_4:77;
      then
A360: f6 is one-to-one by A317,A341,FINSEQ_3:98;
A361: for d st d in dom f6 holds f6.d>0 & f6.d <= p9
      proof
        let d;
        assume
A362:   d in dom f6;
        per cases by A362,FINSEQ_1:38;
        suppose
A363:     d in dom(f4|n);
          then (f4|n).d in rng Sgm Y by A316,FUNCT_1:12;
          then
A364:     (f4|n).d in Y by A303,FINSEQ_1:def 13;
          then
A365:     (f4|n).d in rng f4 by XBOOLE_0:def 5;
          not (f4|n).d in X by A364,XBOOLE_0:def 5;
          then (f4|n).d <= p/2 by A365;
          then
A366:     (f4|n).d <= p9 by A291,INT_1:81;
          not (f4|n).d in {0} by A107,A295,A365,XBOOLE_0:3;
          then (f4|n).d <> 0 by TARSKI:def 1;
          hence thesis by A363,A366,FINSEQ_1:def 7;
        end;
        suppose
          ex l being Nat st l in dom f5 & d=len(f4|n)+ l;
          then consider l be Element of NAT such that
A367:     l in dom f5 and
A368:     d = len(f4|n)+ l;
          f6.d = f5.l by A367,A368,FINSEQ_1:def 7;
          hence thesis by A332,A367;
        end;
      end;
      len f6 = len(f4|n) + len f5 by FINSEQ_1:35
        .= p9 by A326,A327,A329;
      then rng f6 = rng idseq p9 by A324,A360,A361,Th40;
      then M = Sum f6 by A360,A339,RFINSEQ:22,39
        .= Sum(f4|n) + Sum f5 by RVSUM_1:105
        .= Sum(f4|n) + (m*p - Sum(f4/^n)) by A327,A325,Th47
        .= Sum(f4|n) + m*p - Sum(f4/^n);
      then (q-1)*M = p*(Sum f2) + 2*Sum(Sgm X) - m*p by A313,A315,A316;
      then
A369: (q-'1)*M mod 2 = ((p*(Sum f2)-m*p) + 2*Sum(Sgm X)) mod 2 by A28,
XREAL_1:235
        .= (p*(Sum f2)-m*p) mod 2 by EULER_1:13;
      2 divides (q-'1)*M by A39,NAT_D:9;
      then (q-'1)*M mod 2 = 0 by PEPIN:6;
      then 2 divides (p*((Sum f2)-m)) by A369,Lm1;
      then 2 divides ((Sum f2) - m) by A184,INT_2:40;
      then (Sum f2),m are_congruent_mod 2 by INT_2:19;
      then (Sum f2) mod 2 = m mod 2 by INT_3:12;
      then (-1)|^(Sum f2) = (-1)|^m by Th45;
      hence thesis by A1,A5,A295,Th41;
    end;
  hence thesis by A130,A187,NEWTON:13;
end;

theorem
  p>2 & q>2 & p<>q & p mod 4 = 3 & q mod 4 = 3 implies Lege(p,q) = -Lege (q,p)
proof
  assume that
A1: p>2 and
A2: q>2 and
A3: p<>q and
A4: p mod 4 = 3 and
A5: q mod 4 = 3;
  q>1 by INT_2:def 5;
  then
A6: q-'1 = q-1 by XREAL_1:235;
  q = 4*(q div 4)+3 by A5,NAT_D:2;
  then q-'1 = 2*(2*(q div 4)+1) by A6;
  then
A7: (q-'1) div 2=2*(q div 4)+1 by NAT_D:18;
  p>1 by INT_2:def 5;
  then
A8: p-'1 = p-1 by XREAL_1:235;
  p=4*(p div 4)+3 by A4,NAT_D:2;
  then p-'1 = 2*(2*(p div 4)+1) by A8;
  then (p-'1) div 2=2*(p div 4)+1 by NAT_D:18;
  then
A9: Lege(p,q)*Lege(q,p) =(-1)|^((2*(p div 4)+1)*(2*(q div 4)+1)) by A1,A2,A3,A7
,Th49
    .=((-1)|^(2*(p div 4)+1))|^(2*(q div 4)+1) by NEWTON:14
    .= ((-1)|^(2*(p div 4))*(-1))|^(2*(q div 4)+1) by NEWTON:11
    .= ((((-1)|^2)|^(p div 4))*(-1))|^(2*(q div 4)+1) by NEWTON:14
    .= ((1|^2)|^(p div 4)*(-1))|^(2*(q div 4)+1) by WSIERP_1:2
    .= (1^2|^(p div 4)*(-1))|^(2*(q div 4)+1) by NEWTON:100
    .= (1*(-1))|^(2*(q div 4)+1) by NEWTON:15
    .= (-1)|^(2*(q div 4))*(-1) by NEWTON:11
    .= ((-1)|^2)|^(q div 4) * (-1) by NEWTON:14
    .= (1|^2)|^(q div 4) *(-1) by WSIERP_1:2
    .= 1^2|^(q div 4) *(-1) by NEWTON:100
    .= 1*(-1) by NEWTON:15;
  per cases by Th25;
  suppose
    Lege(p,q) = 1;
    hence thesis by A9;
  end;
  suppose
    Lege(p,q) = -1;
    hence thesis by A9;
  end;
end;

theorem
  p>2 & q>2 & p<>q & (p mod 4 = 1 or q mod 4 = 1) implies Lege(p,q) = Lege(q,p)
proof
  assume that
A1: p>2 and
A2: q>2 and
A3: p<>q and
A4: p mod 4 = 1 or q mod 4 = 1;
  p>1 by INT_2:def 5;
  then
A5: p-'1 = p-1 by XREAL_1:235;
  q>1 by INT_2:def 5;
  then
A6: q-'1 = q-1 by XREAL_1:235;
  per cases by A4;
  suppose
    p mod 4 = 1;
    then p=4*(p div 4)+1 by NAT_D:2;
    then p-'1 = 2*(2*(p div 4)) by A5;
    then (p-'1) div 2 = 2*(p div 4) by NAT_D:18;
    then
A7: Lege(p,q)*Lege(q,p) = (-1)|^((2*(p div 4))*((q-'1) div 2)) by A1,A2,A3,Th49
      .= ((-1)|^(2*(p div 4)))|^((q-'1) div 2) by NEWTON:14
      .= (((-1)|^2)|^(p div 4))|^((q-'1) div 2) by NEWTON:14
      .= ((1|^2)|^(p div 4))|^((q-'1) div 2) by WSIERP_1:2
      .= (1^2|^(p div 4))|^((q-'1) div 2) by NEWTON:100
      .= 1|^((q-'1) div 2) by NEWTON:15
      .= 1 by NEWTON:15;
    per cases by Th25;
    suppose
      Lege(p,q)=1;
      hence thesis by A7;
    end;
    suppose
      Lege(p,q)=-1;
      hence thesis by A7;
    end;
  end;
  suppose
    q mod 4 = 1;
    then q=4*(q div 4)+1 by NAT_D:2;
    then q-'1 = 2*(2*(q div 4)) by A6;
    then (q-'1) div 2 = 2*(q div 4) by NAT_D:18;
    then
A8: Lege(p,q)*Lege(q,p) = (-1)|^((2*(q div 4))*((p-'1) div 2)) by A1,A2,A3,Th49
      .= ((-1)|^(2*(q div 4)))|^((p-'1) div 2) by NEWTON:14
      .= (((-1)|^2)|^(q div 4))|^((p-'1) div 2) by NEWTON:14
      .= ((1|^2)|^(q div 4))|^((p-'1) div 2) by WSIERP_1:2
      .= (1^2|^(q div 4))|^((p-'1) div 2) by NEWTON:100
      .= 1|^((p-'1) div 2) by NEWTON:15
      .= 1 by NEWTON:15;
    per cases by Th25;
    suppose
      Lege(p,q)=1;
      hence thesis by A8;
    end;
    suppose
      Lege(p,q)=-1;
      hence thesis by A8;
    end;
  end;
end;
