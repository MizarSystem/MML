                  then 1>=j-len f by NAT_D:39;
                  then
A183:             1+len f>=j-len f+len f by XREAL_1:8;
                  j>=len f+1 by A142,NAT_1:13;
                  then
A184:             j=len f +1 by A183,XXREAL_0:1;
                  LSeg(g,j-'len f+1)<>{} by A174,A179;
                  then 1+2<=len g by A184,A177,TOPREAL1:def 5;
