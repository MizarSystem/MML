      ARYTM_3, JORDAN3, FINSEQ_4, ORDINAL2, ARYTM;
 notations TARSKI, XBOOLE_0, SUBSET_1, ORDINAL1, NUMBERS, XCMPLX_0, REAL_1,
      NAT_1, RFINSEQ, BINARITH, RELAT_1, FUNCT_1, PARTFUN1, FINSEQ_1, FINSEQ_4,
      FINSEQ_5, SEQ_4, STRUCT_0, TOPREAL1, PRE_TOPC, EUCLID, SPPOL_2, XXREAL_0;
 constructors PARTFUN1, XXREAL_0, REAL_1, NAT_1, SEQ_4, FINSEQ_4, REALSET1,
      RFINSEQ, BINARITH, FINSEQ_5, TOPREAL1;
 registrations XBOOLE_0, SUBSET_1, FUNCT_1, ORDINAL1, RELSET_1, XXREAL_0,
      XREAL_0, NAT_1, MEMBERED, FINSEQ_1, FINSEQ_5, STRUCT_0, EUCLID, SPPOL_2;
