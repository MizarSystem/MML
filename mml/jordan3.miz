      NAT_1, REAL_1, REAL_2, BINARITH, TOPREAL3, SPPOL_2, FINSEQ_5, EUCLID,
      FINSEQ_6, ZFMISC_1, FINSEQ_3, RELAT_1, SEQ_4, PARTFUN2, XBOOLE_0,
