                        .= H * a" * a by A1,GROUP_2:135
                        .= H * (a" * a) by GROUP_2:129
                        .= H * 1.G by GROUP_1:def 6
                        .= carr H by GROUP_2:132
                        .= the carrier of H by GROUP_2:def 9;
   hence thesis by GROUP_2:68;
  end;

