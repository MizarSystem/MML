theorem Th69:
 for F being non empty initial NAT-defined Function holds 0 in dom F
proof
 let F be non empty initial NAT-defined Function;
  consider x such that
A1: x in dom F by XBOOLE_0:def 1;
  dom F c= NAT by RELAT_1:def 18;
  then reconsider x as Element of NAT by A1;
  x = 0 or 0 < x;
 hence 0 in dom F by A1,Def13;
end;

registration
 cluster initial finite NAT-defined -> T-Sequence-like Function;
 coherence
  proof let F be Function;
   assume
A1:   F is initial finite NAT-defined;
    then
A2:   dom F c= NAT by RELAT_1:def 18;
   thus dom F is epsilon-transitive
    proof let x;
     assume
A3:    x in dom F;
      then reconsider i = x as Nat by A2;
     let y;
     assume
A4:   y in x;
      x in NAT by A3,A2;
      then x c= NAT by ORDINAL1:def 2;
      then y in NAT by A4;
      then reconsider j = y as Nat;
      j < i by A4,NAT_1:45;
     hence y in dom F by A1,A3,Def13;
    end;
  let x,y;
  assume x in dom F & y in dom F;
    then reconsider x,y as Ordinal by A2;
    x in y or x = y or y in x by ORDINAL1:24;
   hence thesis;
  end;
end;

theorem Th70:
 for F being finite initial NAT-defined Function
 for n being Nat holds
  n in dom F iff n < card F by NAT_1:45;

:: from AMISTD_2, 2010.04.16, A.T.

theorem
  for F being initial NAT-defined Function,
  G being NAT-defined Function st dom F = dom G holds G is initial
proof
  let F be initial NAT-defined Function,
  G be NAT-defined Function;
  assume dom F = dom G;
  hence for m,l being Nat st l in dom G & m < l holds m in dom G by Def13;
end;

theorem
  for F being initial NAT-defined finite Function
  holds dom F = { k where k is Element of NAT: k < card F }
proof
  let F be initial NAT-defined finite Function;
  hereby
    let x be set;
    assume
A1: x in dom F;
    then reconsider f = x as Element of NAT;
    f < card F by A1,Th70;
    hence x in { k where k is Element of NAT: k < card F };
  end;
  let x be set;
  assume x in { k where k is Element of NAT: k < card F };
  then ex k being Element of NAT st x = k & k < card F;
  hence thesis by Th70;
end;

theorem
  for F being initial non empty NAT-defined finite Function, G being non
  empty NAT-defined finite Function holds F c= G & LastLoc F = LastLoc G
  implies F = G
proof
  let F be initial non empty NAT-defined finite Function, G be non empty NAT
  -defined finite Function such that
A1: F c= G and
A2: LastLoc F = LastLoc G;
  dom F = dom G
  proof
    thus dom F c= dom G by A1,GRFUNC_1:8;
    let x be set;
    assume
A3: x in dom G;
    dom G c= NAT by RELAT_1:def 18;
    then reconsider x as Element of NAT by A3;
A4: LastLoc F in dom F by VALUED_1:31;
    x <= LastLoc F by A2,A3,VALUED_1:33;
    then x < LastLoc F or x = LastLoc F by XXREAL_0:1;
    hence thesis by A4,Def13;
  end;
  hence thesis by A1,GRFUNC_1:9;
end;

theorem
  for F being initial non empty NAT-defined finite Function holds
  LastLoc F = card F -' 1
proof
  let F be initial non empty NAT-defined finite Function;
  consider k being natural number such that
A1: LastLoc F = k;
  reconsider k as Element of NAT by ORDINAL1:def 13;
  LastLoc F in dom F by VALUED_1:31;
  then k < card F by A1,Th70;
  then
A2: k <= card F -' 1 by NAT_D:49;
  per cases by A2,XXREAL_0:1;
  suppose
    k < card F -' 1;
    then k+1 < card F -' 1 + 1 by XREAL_1:8;
    then k+1 < card F by NAT_1:14,XREAL_1:237;
    then k+1 in dom F by Th70;
    then
A3: k+1 <= k by A1,VALUED_1:33;
    k <= k+1 by NAT_1:11;
    then k+0 = k+1 by A3,XXREAL_0:1;
    hence thesis;
  end;
  suppose
    k = card F -' 1;
    hence thesis by A1;
  end;
end;

theorem
  for F being initial non empty NAT-defined finite Function holds
  FirstLoc F = 0
proof
  let F be initial non empty NAT-defined finite Function;
  0 in dom F by Th69;
  then FirstLoc F <= 0 by VALUED_1:36;
  hence thesis;
end;

registration
  let F be initial non empty NAT-defined finite Function;
  cluster CutLastLoc F -> initial;
  coherence
  proof
    set G = CutLastLoc F;
    per cases;
    suppose G is empty;
      then reconsider H = G as empty finite Function;
      H is initial;
      hence thesis;
    end;
    suppose G is non empty;
      then reconsider G as non empty finite Function;
      G is initial
      proof
        let m,l be Nat such that
A1:     l in dom G and
A2:     m < l;
        set M = dom F;
        reconsider R = {[LastLoc F, F.LastLoc F]} as Relation;
        R = LastLoc F .--> (F.LastLoc F) by FUNCT_4:87;
        then
A3:     dom R = {LastLoc F} by FUNCOP_1:19;
        then
A4:     dom F \ dom R = dom G by VALUED_1:37;
        then
       l in dom F by A1,XBOOLE_0:def 5;
        then
A5:     m in dom F by A2,Def13;
        l in M by A4,A1,XBOOLE_0:def 5;
        then m <> LastLoc F by A2,XXREAL_2:def 8;
        then not m in {LastLoc F} by TARSKI:def 1;
        hence thesis by A3,A4,A5,XBOOLE_0:def 5;
      end;
      hence thesis;
    end;
  end;
end;

