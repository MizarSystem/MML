reserve r,r1,r2,g,g1,g2,x0 for Real;
reserve f1,f2 for PartFunc of REAL,REAL;

Lm1: 0 < g implies r-g<r & r<r+g
proof
  assume
A1: 0<g;
  then r-g<r-0 by XREAL_1:17;
  hence r-g<r;
  r+0<r+g by A1,XREAL_1:10;
  hence thesis;
end;

Lm2: for s be Real_Sequence st rng s c=dom(f2*f1) holds rng s c=dom f1 &
rng(f1/*s)c=dom f2
proof
  let s be Real_Sequence such that
A1: rng s c=dom(f2*f1);
A2: dom(f2*f1)c=dom f1 by RELAT_1:44;
  hence rng s c=dom f1 by A1,XBOOLE_1:1;
  let x be set;
  assume x in rng(f1/*s);
  then consider n be Element of NAT such that
A3: (f1/*s).n=x by FUNCT_2:190;
  s.n in rng s by VALUED_0:28;
  then f1.(s.n) in dom f2 by A1,FUNCT_1:21;
  hence thesis by A1,A2,A3,FUNCT_2:185,XBOOLE_1:1;
end;

theorem Th1:
  for s be Real_Sequence,X be set st rng s c= dom(f2*f1) /\ X holds
  rng s c= dom(f2*f1) & rng s c= X & rng s c= dom f1 & rng s c= dom f1 /\ X &
  rng(f1/*s) c= dom f2
proof
  let s be Real_Sequence,X be set such that
A1: rng s c=dom(f2*f1)/\X; thus
A2: rng s c=dom(f2*f1) & rng s c=X by A1,XBOOLE_1:18;
A3: dom(f2*f1)c=dom f1 by RELAT_1:44;
  hence rng s c=dom f1 by A2,XBOOLE_1:1;
  hence rng s c=dom f1/\X by A2,XBOOLE_1:19;
  let x be set;
  assume x in rng(f1/*s);
  then consider n be Element of NAT such that
A4: (f1/*s).n=x by FUNCT_2:190;
  s.n in rng s by VALUED_0:28;
  then f1.(s.n) in dom f2 by A2,FUNCT_1:21;
  hence thesis by A2,A3,A4,FUNCT_2:185,XBOOLE_1:1;
end;

theorem Th2:
  for s be Real_Sequence,X be set st rng s c= dom(f2*f1) \ X holds
  rng s c= dom(f2*f1) & rng s c= dom f1 & rng s c= dom f1 \ X &
  rng(f1/*s) c= dom f2
proof
  let s be Real_Sequence,X be set such that
A1: rng s c=dom(f2*f1)\X;
  dom(f2*f1)\X c=dom(f2*f1) by XBOOLE_1:36;
  hence
A2: rng s c=dom(f2*f1) by A1,XBOOLE_1:1;
A3: dom(f2*f1)c=dom f1 by RELAT_1:44;
  hence
A4: rng s c=dom f1 by A2,XBOOLE_1:1;
  thus rng s c=dom f1\X
  proof
    let x be set;
    assume
A5: x in rng s;
    then not x in X by A1,XBOOLE_0:def 5;
    hence thesis by A4,A5,XBOOLE_0:def 5;
  end;
  let x be set;
  assume x in rng(f1/*s);
  then consider n be Element of NAT such that
A6: (f1/*s).n=x by FUNCT_2:190;
  s.n in rng s by VALUED_0:28;
  then f1.(s.n) in dom f2 by A2,FUNCT_1:21;
  hence thesis by A2,A3,A6,FUNCT_2:185,XBOOLE_1:1;
end;

theorem
  f1 is divergent_in+infty_to+infty & f2 is divergent_in+infty_to+infty &
  (for r ex g st r<g & g in
  dom(f2*f1)) implies f2*f1 is divergent_in+infty_to+infty
proof
  assume
A1: f1 is divergent_in+infty_to+infty & f2 is divergent_in+infty_to+infty &
  for r ex g st r<g & g in dom(f2*f1);
  now
    let s be Real_Sequence;
    assume
A2: s is divergent_to+infty & rng s c=dom(f2*f1);
    then
A3: rng s c=dom f1 & rng(f1/*s)c=dom f2 by Lm2;
    then f1/*s is divergent_to+infty by A1,A2,LIMFUNC1:def 7;
    then f2/*(f1/*s) is divergent_to+infty by A1,A3,LIMFUNC1:def 7;
    hence (f2*f1)/*s is divergent_to+infty by A2,VALUED_0:31;
  end;
  hence thesis by A1,LIMFUNC1:def 7;
end;

