  now
    let s be Real_Sequence;
    assume that
A4: s is convergent & lim s=x0 and
A5: rng s c=dom(f2*f1)/\left_open_halfline(x0);
A6: rng s c=dom(f2*f1) by A5,Th1;
    rng s c=dom f1/\left_open_halfline(x0) by A5,Th1;
