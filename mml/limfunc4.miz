    end;
A19: f2/*(f1/*q)=f2/*((f1/*s)^\k) by A10,VALUED_0:27
      .=(f2/*(f1/*s))^\k by A13,VALUED_0:27
      .=((f2*f1)/*s)^\k by A7,VALUED_0:31;
    L=L;
    then
A20: f2/*(f1/*q) is convergent by A2,A12,A14,LIMFUNC3:def 4;
    hence (f2*f1)/*s is convergent by A19,SEQ_4:35;
    lim(f2/*(f1/*q))=L by A2,A12,A14,LIMFUNC3:def 4;
    hence lim((f2*f1)/*s)=L by A20,A19,SEQ_4:36;
  end;
