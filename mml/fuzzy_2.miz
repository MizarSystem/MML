      RCOMP_1, INTEGRA1, FUZZY_2, ARYTM, FUZZY_3, FUNCT_2, FUNCT_3, SEQ_4;
 notations TARSKI, XBOOLE_0, SUBSET_1, ZFMISC_1, NUMBERS, XXREAL_0, SQUARE_1,
      REAL_1, RELSET_1, PARTFUN1, FUNCT_2, FUNCT_3, INTEGRA1, SEQ_1, RCOMP_1,
      SEQ_4, FUZZY_1;
