A3:     min(f.c,g.c) = f.c & min(f.c,h.c) = f.c;
        min(f.c,1 - ((1 - g.c)*(1 - h.c))) <=
        min(f.c,g.c) + min(f.c,h.c) - (min(f.c,g.c))*(min(f.c,h.c))
        proof
          f c= f ++ f by Th33;
          then min(f.c,1 - ((1 - g.c)*(1 - h.c))) <= f.c & (f ++ f).c >= f.c
          by FUZZY_1:def 3,XXREAL_0:17;
          then min(f.c,1 - ((1 - g.c)*(1 - h.c))) <= (f ++ f).c by XXREAL_0:2;
          hence thesis by A3,Def3;
        end;
        hence thesis;
      end;
      suppose
A4:     min(f.c,g.c) = f.c & min(f.c,h.c) = h.c;
        min(f.c,1 - ((1 - g.c)*(1 - h.c))) <=
        min(f.c,g.c) + min(f.c,h.c) - (min(f.c,g.c))*(min(f.c,h.c))
        proof
          (1_minus f).c >= 0 & h.c >= 0 by Th1;
          then 1 - f.c >= 0 & h.c >= 0 by FUZZY_1:def 6;
          then 0*(h.c) <= (h.c)*(1-f.c) by XREAL_1:66;
          then 0 + f.c <= (h.c)*(1-f.c) + f.c by XREAL_1:8;

then f.c <= f.c + h.c -(h.c)*(f.c) & min(f.c,1 - ((1 - g.c)*(1 - h.c))) <= f.c
          by XXREAL_0:17;
          hence thesis by A4,XXREAL_0:2;
        end;
        hence thesis;
      end;
      suppose
A5:     min(f.c,g.c) = g.c & min(f.c,h.c) = f.c;
        min(f.c,1 - ((1 - g.c)*(1 - h.c))) <=
        min(f.c,g.c) + min(f.c,h.c) - (min(f.c,g.c))*(min(f.c,h.c))
        proof
          (1_minus f).c >= 0 & g.c >= 0 by Th1;
          then 1 - f.c >= 0 & g.c >= 0 by FUZZY_1:def 6;
          then 0*(g.c) <= (g.c)*(1-f.c) by XREAL_1:66;
          then 0 + f.c <= (g.c)*(1-f.c) + f.c by XREAL_1:8;

then f.c <= f.c + g.c -(g.c)*(f.c) & min(f.c,1 - ((1 - g.c)*(1 - h.c))) <= f.c
          by XXREAL_0:17;
          hence thesis by A5,XXREAL_0:2;
        end;
        hence thesis;
      end;
      suppose min(f.c,g.c) = g.c & min(f.c,h.c) = h.c;
        hence thesis by XXREAL_0:17;
      end;
    end;
    hence thesis;
  end;
  hence thesis by A1,A2;
end;

begin :: Addenda, moved from FUZZY_3

reserve C1,C2 for non empty set;

registration
  let C be non empty set;
  cluster -> quasi_total Membership_Func of C;
  coherence;
end;

definition
  let C1,C2 be non empty set;
  mode RMembership_Func of C1,C2 is Membership_Func of [:C1,C2:];
end;

reserve f,g for RMembership_Func of C1,C2;

definition
  let C1,C2 be non empty set;
  func Zmf(C1,C2) -> RMembership_Func of C1,C2 equals

  chi({},[:C1,C2:]);
  correctness by FUZZY_1:13;
  func Umf(C1,C2) -> RMembership_Func of C1,C2 equals
  chi([:C1,C2:],[:C1,C2:]);
  correctness by FUZZY_1:2;
end;

theorem
  for x be Element of [:C1,C2:],h be RMembership_Func of C1,C2 holds
  (Zmf(C1,C2)).x <= h.x & h.x <= (Umf(C1,C2)).x
proof
  Zmf(C1,C2) = EMF [:C1,C2:] & Umf(C1,C2) = UMF [:C1,C2:];
  hence thesis by FUZZY_1:17;
end;

theorem
  max(f,Umf(C1,C2)) = Umf(C1,C2) & min(f,Umf(C1,C2)) = f &
  max(f,Zmf(C1,C2)) = f & min(f,Zmf(C1,C2)) = Zmf(C1,C2)
proof
  Zmf(C1,C2) = EMF [:C1,C2:] & Umf(C1,C2) = UMF [:C1,C2:];
  hence thesis by FUZZY_1:19;
end;

theorem
  1_minus(Zmf(C1,C2)) = Umf(C1,C2) & 1_minus(Umf(C1,C2)) = Zmf(C1,C2)
proof
  Zmf(C1,C2) = EMF [:C1,C2:] & Umf(C1,C2) = UMF [:C1,C2:];
  hence thesis by FUZZY_1:44;
end;

theorem
  f\g = Zmf(C1,C2) implies f c= g
proof Zmf(C1,C2) = EMF [:C1,C2:];
  hence thesis by Th31;
end;

theorem
  min(f,g) = Zmf(C1,C2) implies f\g = f
proof Zmf(C1,C2) = EMF [:C1,C2:];
  hence thesis by Th32;
end;

