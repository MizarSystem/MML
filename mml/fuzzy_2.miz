begin :: Basic Properties of Membership Function and Difference Set

reserve x,y,y1,y2 for set;
reserve C for non empty set;
reserve c for Element of C;
reserve f,h,g,h1 for Membership_Func of C;

theorem Th1:
  for x be Element of C,h be Membership_Func of C holds 0 <= h.x & h.x <= 1
proof
  let x be Element of C,h be Membership_Func of C;
  (EMF C).x = 0 by FUNCT_3:def 3;
  hence 0 <= h.x by FUZZY_1:17;
  (UMF C).x = 1 by FUNCT_3:def 3;
  hence thesis by FUZZY_1:17;
end;

theorem
  f c= h implies max(f,min(g,h)) = min(max(f,g),h)
proof
  assume
A1: f c= h;
  thus max(f,min(g,h)) = min(max(f,g),max(f,h)) by FUZZY_1:10
    .= min(max(f,g),h) by A1,FUZZY_1:23;
end;

definition
  let C be non empty set;
  let f,g be Membership_Func of C;
  func f\g -> Membership_Func of C equals

  min(f,1_minus g);
  correctness;
end;

theorem
  1_minus (f\g) = max(1_minus f,g)
proof
  thus 1_minus (f\g) = max(1_minus f,1_minus(1_minus g)) by FUZZY_1:12
    .= max(1_minus f,g);
end;

theorem Th4:
  f c= g implies f\h c= g\h
proof
  assume
A1: f.c <= g.c;
  let c;
  f.c <= g.c by A1;
  then min(f.c,(1_minus h).c) <= min(g.c,(1_minus h).c) by XXREAL_0:18;
  then (f\h).c <= min(g.c,(1_minus h).c) by FUZZY_1:6;
  hence thesis by FUZZY_1:6;
end;

theorem
  f c= g implies h\g c= h\f
proof
  assume
A1: f.c <= g.c;
  let c;
  f.c <= g.c by A1;
  then 1 - f.c >= 1 - g.c by XREAL_1:12;
  then (1_minus g).c <= 1- f.c by FUZZY_1:def 6;
  then (1_minus g).c <= (1_minus f).c by FUZZY_1:def 6;
  then min((1_minus g).c,h.c) <= min((1_minus f).c,h.c) by XXREAL_0:18;
  then (h\g).c <= min((1_minus f).c,h.c) by FUZZY_1:6;
  hence thesis by FUZZY_1:6;
end;

theorem
  f c= g & h c= h1 implies f\h1 c= g\h
proof
  assume that
A1: f.c <= g.c and
A2: h.c <= h1.c;
  let c;
  f.c <= g.c & h.c <= h1.c by A1,A2;
  then f.c <= g.c & 1-h.c >= 1-h1.c by XREAL_1:12;
  then min(f.c,1-h1.c) <= min(g.c,1- h.c) by XXREAL_0:18;
  then min(f.c,(1_minus h1).c) <= min(g.c,1- h.c) by FUZZY_1:def 6;
  then min(f.c,(1_minus h1).c) <= min(g.c,(1_minus h).c) by FUZZY_1:def 6;
  then min(f,1_minus h1).c <= min(g.c,(1_minus h).c) by FUZZY_1:6;
  hence thesis by FUZZY_1:6;
end;

canceled 2;

theorem
  f\EMF(C) = f
proof
  thus f\EMF(C) = min(f,UMF(C)) by FUZZY_1:44
    .= f by FUZZY_1:19;
end;

canceled;

theorem
  f\g c= f\min(f,g)
proof
  f\min(f,g) = min(f,max(1_minus f,1_minus g)) by FUZZY_1:12
    .=max(min(f,1_minus f),f\g) by FUZZY_1:10;
  hence thesis by FUZZY_1:18;
end;

theorem
  max(min(f,g),f\g) c= f
proof
  min(f,g) c= f & f\g c= f by FUZZY_1:18;
  hence thesis by FUZZY_1:20;
end;

theorem
  max(f,g\f) c= max(f,g)
proof
  max(f,g\f) = min(max(f,g),max(f,1_minus f)) by FUZZY_1:10;
  hence thesis by FUZZY_1:18;
end;

theorem
  f\(g\h) = max(f\g,min(f,h))
proof
  f\(g\h) = min(f,max(1_minus g,1_minus(1_minus h))) by FUZZY_1:12
    .= max(min(f,1_minus g),min(f,h)) by FUZZY_1:10;
  hence thesis;
end;

theorem
  min(f,g) c= f\(f\g)
proof
  f\(f\g) = min(f,max(1_minus f,1_minus(1_minus g))) by FUZZY_1:12
    .= max(min(f,1_minus f),min(f,g)) by FUZZY_1:10;
  hence thesis by FUZZY_1:18;
end;

theorem
  f\g c= max(f,g)\g
proof
  f c= max(f,g) by FUZZY_1:18;
  hence thesis by Th4;
end;

theorem
  f\max(g,h) = min(f\g,f\h)
proof
  thus f\max(g,h) = min(min(f,f),min(1_minus g,1_minus h)) by FUZZY_1:12
    .= min(f,min(f,min(1_minus g,1_minus h))) by FUZZY_1:8
    .= min(f,min(min(f,1_minus g),1_minus h)) by FUZZY_1:8
    .= min(f\g,f\h) by FUZZY_1:8;
end;

theorem
  f\min(g,h) = max(f\g,f\h)
proof
  thus f\min(g,h) = min(f,max(1_minus g,1_minus h)) by FUZZY_1:12
    .= max(f\g,f\h) by FUZZY_1:10;
end;

theorem
  (f\g)\h = f\max(g,h)
proof
  thus (f\g)\h = min(f,min(1_minus g,1_minus h)) by FUZZY_1:8
    .= f\max(g,h) by FUZZY_1:12;
end;

theorem
  f \+\ g c= max(f,g)\min(f,g)
proof
  let c;
  (max(f,g)\min(f,g)).c =
  min(max(f,g),max(1_minus(f),1_minus(g))).c by FUZZY_1:12
    .= max(min(max(f,g),1_minus f),min(max(f,g),1_minus g)).c by FUZZY_1:10
    .= max(max(min(1_minus f,f),min(1_minus f,g)),min(1_minus g,max(f,g))).c
  by FUZZY_1:10
    .= max(max(min(1_minus f,f),min(1_minus f,g))
  ,max(min(1_minus g,f),min(1_minus g,g))).c by FUZZY_1:10
    .= max(max(max(min(1_minus f,f),min(1_minus f,g)),min(1_minus g,f)),
  min(1_minus g,g)).c by FUZZY_1:8
    .= max(max(min(1_minus f,f),max(min(1_minus f,g),min(1_minus g,f))),
  min(1_minus g,g)).c by FUZZY_1:8
    .= max(max(min(1_minus f,g),min(1_minus g,f)),
  max(min(1_minus f,f),min(1_minus g,g))).c by FUZZY_1:8
    .= max((f \+\ g).c,
  max(min(1_minus f,f),min(1_minus g,g)).c) by FUZZY_1:6;
  hence thesis by XXREAL_0:25;
end;

canceled;

theorem
  f\g c= h & g\f c= h implies f \+\ g c= h
proof
  assume that
A1: (f\g).c <= h.c and
A2: (g\f).c <= h.c;
  let c;
  min(f,1_minus g).c <= h.c & min(g,1_minus f).c <= h.c by A1,A2;
  then max(min(f,1_minus g).c,min(g,1_minus f).c) <= max(h.c,h.c)
  by XXREAL_0:26;
  hence thesis by FUZZY_1:6;
end;

canceled;

theorem
  min(f,g\h) c= min(f,g)\min(f,h)
proof
  let c;
A1: min(f,g\h).c = min(min(f,g),1_minus h).c by FUZZY_1:8
    .= min(min(f,g).c,(1_minus h).c) by FUZZY_1:6;
  (1_minus h).c <= max((1_minus f).c,(1_minus h).c) by XXREAL_0:25;
  then min(min(f,g).c,(1_minus h).c)
  <= min(min(f,g).c,max((1_minus f).c,(1_minus h).c)) by XXREAL_0:18;
  then min(min(f,g).c,(1_minus h).c)
  <= min(min(f,g).c,max(1_minus f,1_minus h).c) by FUZZY_1:6;
  then min(min(f,g).c,(1_minus h).c)
  <= min(min(f,g),max(1_minus f,1_minus h)).c by FUZZY_1:6;
  hence thesis by A1,FUZZY_1:12;
end;

theorem Th25:
  f \+\ g c= max(f,g)\min(f,g)
proof
  let c;
  (max(f,g)\min(f,g)).c =
  min(max(f,g),max(1_minus f,1_minus g)).c by FUZZY_1:12
    .= max(min(max(f,g),1_minus f),min(max(f,g),1_minus g)).c by FUZZY_1:10
    .= max(max(min(1_minus f,f),min(1_minus f,g)),min(1_minus g,max(f,g))).c
  by FUZZY_1:10
    .= max(max(min(1_minus f,f),min(1_minus f,g)),
  max(min(1_minus g,f),min(1_minus g,g))).c by FUZZY_1:10
    .= max(max(max(min(1_minus f,f),min(1_minus f,g)),min(1_minus g,g))
  ,min(1_minus g,f)).c by FUZZY_1:8
    .= max(max(max(min(1_minus f,f),min(1_minus g,g)),min(1_minus f,g))
  ,min(1_minus g,f)).c by FUZZY_1:8
    .= max(max(min(1_minus f,f),min(1_minus g,g)),max(min(1_minus f,g)
  ,min(1_minus g,f))).c by FUZZY_1:8
    .= max(max(min(1_minus f,f),min(1_minus g,g)).c,
  (f \+\ g).c) by FUZZY_1:6;
  hence thesis by XXREAL_0:25;
end;

theorem Th26:
  max(min(f,g),1_minus max(f,g)) c= 1_minus (f \+\ g)
proof
  f \+\ g c= max(f,g)\min(f,g) by Th25;
  then 1_minus (max(f,g)\min(f,g)) c= 1_minus (f \+\ g) by FUZZY_1:39;
  then max(1_minus max(f,g),1_minus(1_minus min(f,g))) c= 1_minus (f \+\ g)
  by FUZZY_1:12;
  hence thesis;
end;

theorem
  (f \+\ g)\h = max(f\max(g,h),g\max(f,h))
proof
  set f1 = 1_minus f, g1 = 1_minus g, h1 = 1_minus h;
  max(min(f,g1),min(f1,g))\h =
  max(min(h1,min(f,g1)),min(h1,min(f1,g))) by FUZZY_1:10
    .= max(min(min(h1,g1),f),min(h1,min(f1,g))) by FUZZY_1:8
    .= max(min(min(h1,g1),f),min(min(h1,f1),g)) by FUZZY_1:8
    .= max(min(f,1_minus max(h,g)),min(g,min(h1,f1))) by FUZZY_1:12
    .= max(f\max(h,g),g\max(f,h)) by FUZZY_1:12;
  hence thesis;
end;

theorem
  max(f\ max(g,h),min(min(f,g),h)) c= f\(g \+\ h)
proof
  max(f\ max(g,h),min(min(f,g),h))
  = max(min(f,1_minus max(g,h)),min(f,min(g,h))) by FUZZY_1:8
    .= min(f,max(1_minus max(g,h),min(g,h))) by FUZZY_1:10;
  hence thesis by Th26,FUZZY_1:28;
end;

theorem
  f c= g implies max(f,g\f) c= g
proof
  assume
A1: f.c <= g.c;
  let c;
A2: f.c <= g.c by A1;
  max(f,g\f).c = min(max(f,g),max(f,1_minus f)).c by FUZZY_1:10
    .= min(max(f,g).c,max(f,1_minus f).c) by FUZZY_1:6
    .= min(max(f.c,g.c),max(f,1_minus f).c) by FUZZY_1:6
    .= min(g.c,max(f,1_minus f).c) by A2,XXREAL_0:def 10;
  hence thesis by XXREAL_0:17;
end;

theorem
  max(f \+\ g,min(f,g)) c= max(f,g)
proof
  set f1 = 1_minus f, g1 = 1_minus g;
  let c;
  max(f \+\ g,min(f,g)) = max(min(f,g1),max(min(f1,g),min(f,g))) by FUZZY_1:8
    .= max(min(f,g1),min(max(min(f1,g),f),max(min(f1,g),g))) by FUZZY_1:10
    .= max(min(f,g1),min(max(min(f1,g),f),g)) by FUZZY_1:9
    .= min(max(min(f,g1),max(min(f1,g),f)),max(min(f,g1),g)) by FUZZY_1:10;
  then max(f \+\ g,min(f,g)).c =
  min(max(min(f,g1),max(min(f1,g),f)).c,max(min(f,g1),g).c) by FUZZY_1:6;
  then
A1: max(f \+\ g, min(f,g)).c <= max(min(f,g1),g).c by XXREAL_0:17;
  max(min(f,g1),g).c = min(max(g,f),max(g,g1)).c by FUZZY_1:10
    .= min(max(f,g).c,max(g,g1).c) by FUZZY_1:6;
  then max(min(f,g1),g).c <= max(f,g).c by XXREAL_0:17;
  hence thesis by A1,XXREAL_0:2;
end;

theorem Th31:
  f\g = EMF(C) implies f c= g
proof
  assume
A1: f\g = EMF(C);
  let c;
A2: min(f.c,(1_minus g).c) = (EMF(C)).c by A1,FUZZY_1:6;
  per cases by A2,XXREAL_0:15;
  suppose f.c = (EMF(C)).c;
    hence thesis by FUZZY_1:17;
  end;
  suppose
A3: (1_minus g).c = (EMF(C)).c;
    f.c <= g.c
    proof
      g.c = (1_minus (1_minus g)).c
        .= 1 - (1_minus g).c by FUZZY_1:def 6
        .= (1_minus EMF(C)).c by A3,FUZZY_1:def 6
        .= (UMF(C)).c by FUZZY_1:44;
      hence thesis by FUZZY_1:17;
    end;
    hence thesis;
  end;
end;

theorem Th32:
  min(f,g) = EMF(C) implies f\g = f
proof
  assume
A1: min(f,g) = EMF(C);
  min(f,1_minus g) = f
  proof
A2: C = dom min(f,1_minus g) & C = dom f by FUNCT_2:def 1;
    for x being Element of C st x in C holds min(f,1_minus g).x = f.x
    proof
      let x be Element of C;
A3:   (EMF C).x = min(f.x,g.x) by A1,FUZZY_1:6;
      per cases by A3,XXREAL_0:15;
      suppose
A4:     f.x = (EMF(C)).x;
        min(f,1_minus g).x = f.x
        proof
          min(f,1_minus g).x = min(f.x,(1_minus g).x) by FUZZY_1:6
            .= min(1_minus g,EMF(C)).x by A4,FUZZY_1:6
            .= (EMF(C)).x by FUZZY_1:19;
          hence thesis by A4;
        end;
        hence thesis;
      end;
      suppose
A5:     g.x = (EMF(C)).x;
        min(f,1_minus g).x = f.x
        proof
          min(f,1_minus g).x = min(f.x,(1_minus g).x) by FUZZY_1:6
            .= min(f.x,1 - (EMF(C)).x) by A5,FUZZY_1:def 6
            .= min(f.x,(1_minus EMF(C)).x) by FUZZY_1:def 6
            .= min(f.x,(UMF(C)).x) by FUZZY_1:44
            .= min(f,UMF(C)).x by FUZZY_1:6;
          hence thesis by FUZZY_1:19;
        end;
        hence thesis;
      end;
    end;
    hence thesis by A2,PARTFUN1:34;
  end;
  hence thesis;
end;

begin :: Algebraic Product and Algebraic Sum

definition
  let C be non empty set;
  let h,g be Membership_Func of C;
  func h*g -> Membership_Func of C means
  :Def2:
  for c being Element of C holds it.c = (h.c)*(g.c);
  existence
  proof
    defpred P[set,set] means $2 = (h.$1)*(g.$1);
A1: for x,y st x in C & P[x,y] holds y in REAL;
A2: for x,y1,y2 st x in C & P[x,y1] & P[x,y2] holds y1=y2;
    consider IT being PartFunc of C,REAL such that
A3: (for x holds x in dom IT iff x in C & ex y st P[x,y]) &
    (for x st x in dom IT holds P[x,IT.x]) from PARTFUN1:sch 2(A1,A2);
A4: dom IT = C & rng IT c= [.0,1.]
    proof
A5:   dom IT = C
      proof
        C c= dom IT
        proof
          for x being set st x in C holds x in dom IT
          proof
            let x be set;
            assume
A6:         x in C;
            ex y st y = (h.x)*(g.x);
            hence thesis by A3,A6;
          end;
          hence thesis by TARSKI:def 3;
        end;
        hence thesis by XBOOLE_0:def 10;
      end;
      rng IT c= [.0,1.]
      proof
A7:     rng h c= [.0,1.] & rng g c= [.0,1.] by RELAT_1:def 19;
        for y being set st y in rng IT holds y in [.0,1.]
        proof
          let y be set;
          assume y in rng IT;
          then consider x being Element of C such that
A8:       x in dom IT & y = IT.x by PARTFUN1:26;

reconsider A=[.0,1.] as closed-interval Subset of REAL by INTEGRA1:def 1;
          A = [. lower_bound A, upper_bound A .] by INTEGRA1:5;
          then
A9:       0=lower_bound A & 1=upper_bound A by INTEGRA1:6;
          x in C;
          then x in dom h & x in dom g by FUNCT_2:def 1;
          then h.x in rng h & g.x in rng g by FUNCT_1:def 5;
          then 0 <= h.x & h.x <= 1 & 0 <= g.x & g.x <= 1 by A7,A9,INTEGRA2:1;
          then 0 <= (h.x)*(g.x) & (h.x)*(g.x) <= 1 by XREAL_1:129,162;
          then 0 <= IT.x & IT.x <= 1 by A3,A8;
          hence thesis by A8,A9,INTEGRA2:1;
        end;
        hence thesis by TARSKI:def 3;
      end;
      hence thesis by A5;
    end;
    then IT is Function of C,REAL by FUNCT_2:def 1;
    then
A10: IT is Membership_Func of C by RELAT_1:def 19,A4;
    for c being Element of C holds IT.c = (h.c)*(g.c) by A3,A4;
    hence thesis by A10;
  end;
  uniqueness
  proof
    let F,G be Membership_Func of C;
    assume that
A11: for c being Element of C holds F.c = (h.c)*(g.c) and
A12: for c being Element of C holds G.c = (h.c)*(g.c);
A13: C = dom F & C = dom G by FUNCT_2:def 1;
    for c being Element of C st c in C holds F.c = G.c
    proof
      let c be Element of C;
      F.c = (h.c)*(g.c) & G.c = (h.c)*(g.c) by A11,A12;
      hence thesis;
    end;
    hence thesis by A13,PARTFUN1:34;
  end;
  commutativity;
end;

definition
  let C be non empty set;
  let h,g be Membership_Func of C;
  func h ++ g -> Membership_Func of C means
  :Def3:
  for c being Element of C holds it.c = h.c + g.c -(h.c)*(g.c);
  existence
  proof
    defpred P[set,set] means $2 = h.$1 + g.$1 -(h.$1)*(g.$1);
A1: for x,y st x in C & P[x,y] holds y in REAL;
A2: for x,y1,y2 st x in C & P[x,y1] & P[x,y2] holds y1=y2;
    consider IT being PartFunc of C,REAL such that
A3: (for x holds x in dom IT iff x in C & ex y st P[x,y]) &
    (for x st x in dom IT holds P[x,IT.x]) from PARTFUN1:sch 2(A1,A2);
A4: dom IT = C & rng IT c= [.0,1.]
    proof
A5:   dom IT = C
      proof
        C c= dom IT
        proof
          for x being set st x in C holds x in dom IT
          proof
            let x be set;
            assume
A6:         x in C;
            ex y st y = h.x + g.x -(h.x)*(g.x);
            hence thesis by A3,A6;
          end;
          hence thesis by TARSKI:def 3;
        end;
        hence thesis by XBOOLE_0:def 10;
      end;
      rng IT c= [.0,1.]
      proof
        for y being set st y in rng IT holds y in [.0,1.]
        proof
          let y be set;
          assume y in rng IT;
          then consider x being Element of C such that
A7:       x in dom IT & y = IT.x by PARTFUN1:26;

reconsider A=[.0,1.] as closed-interval Subset of REAL by INTEGRA1:def 1;
          A = [. lower_bound A, upper_bound A .] by INTEGRA1:5;
          then
A8:       0=lower_bound A & 1=upper_bound A by INTEGRA1:6;
          0 <= h.x + g.x -(h.x)*(g.x) & h.x + g.x -(h.x)*(g.x) <= 1
          proof
            0 <= (1_minus h).x & (1_minus h).x <= 1 &
            0 <= (1_minus g).x & (1_minus g).x <= 1 by Th1;

then 0 <= 1-h.x & 1-h.x <= 1 & 0 <= 1- g.x & 1-g.x <= 1 by FUZZY_1:def 6;
            then 0 <= (1-h.x)*(1-g.x) & (1-h.x)*(1-g.x) <= 1
            by XREAL_1:129,162;
            then 1 - (1-h.x)*(1-g.x) >= 1 - 1 & 1-0 >= 1 -(1-h.x)*(1-g.x) by
XREAL_1:12;
            hence thesis;
          end;
          then 0 <= IT.x & IT.x <= 1 by A3,A7;
          hence thesis by A7,A8,INTEGRA2:1;
        end;
        hence thesis by TARSKI:def 3;
      end;
      hence thesis by A5;
    end;
    then IT is Function of C,REAL by FUNCT_2:def 1;
    then
A9: IT is Membership_Func of C by RELAT_1:def 19,A4;
    for c being Element of C holds IT.c = h.c + g.c -(h.c)*(g.c) by A3,A4;
    hence thesis by A9;
  end;
  uniqueness
  proof
    let F,G be Membership_Func of C;
    assume that
A10: for c being Element of C holds F.c = h.c + g.c -(h.c)*(g.c) and
A11: for c being Element of C holds G.c = h.c + g.c -(h.c)*(g.c);
A12: C = dom F & C = dom G by FUNCT_2:def 1;
    for c being Element of C st c in C holds F.c = G.c
    proof
      let c be Element of C;
      F.c = h.c + g.c -(h.c)*(g.c) & G.c = h.c + g.c -(h.c)*(g.c) by A10,A11;
      hence thesis;
    end;
    hence thesis by A12,PARTFUN1:34;
  end;
  commutativity;
end;

theorem Th33:
  f*f c= f & f c= f ++ f
proof
  thus f*f c= f
  proof
    let c;
    f.c <= 1 & 0 <= f.c by Th1;
    then (f.c)*(f.c) <= 1*(f.c) by XREAL_1:66;
    hence thesis by Def2;
  end;
  let c;
  0 <= (1_minus f).c & 0 <= f.c by Th1;
  then 0*(f.c) <= (f.c)*((1_minus f).c) by XREAL_1:66;
  then 0 <= (f.c)*(1 - f.c) by FUZZY_1:def 6;
  then 0 + f.c <= f.c - (f.c)*(f.c) + f.c by XREAL_1:8;
  then f.c <= f.c + f.c - (f.c)*(f.c);
  hence thesis by Def3;
end;

theorem Th34:
  (f*g)*h = f*(g*h)
proof
A1: C = dom ((f*g)*h) & C = dom (f*(g*h)) by FUNCT_2:def 1;
  for c being Element of C st c in C holds ((f*g)*h).c = (f*(g*h)).c
  proof
    let c;
    ((f*g)*h).c = ((f*g).c)*(h.c) by Def2
      .= ((f.c)*(g.c))*(h.c) by Def2
      .= (f.c)*((g.c)*(h.c))
      .= (f.c)*((g*h).c) by Def2;
    hence thesis by Def2;
  end;
  hence thesis by A1,PARTFUN1:34;
end;

theorem
  (f ++ g) ++ h = f ++ (g ++ h)
proof
A1: C = dom((f ++ g) ++ h) & C = dom(f ++ (g ++ h))
 by FUNCT_2:def 1;
  for c being Element of C st c in C holds
  ((f ++ g) ++ h).c = (f ++ (g ++ h)).c
  proof
    let c;
    set x = f.c, y = g.c, z = h.c;
A2: ((f ++ g) ++ h).c = (f ++ g).c + h.c - ((f ++ g).c)*(h.c) by Def3
      .= f.c + g.c -(f.c)*(g.c) + h.c - ((f ++ g).c)*(h.c) by Def3
      .= (-x*y + (x + y)) + z - (x + y -x*y)*z by Def3
      .= -x*y - x*z - y*z +x*y*z + (x + y + z);
    (f ++ (g ++ h)).c = x + (g ++ h).c -x*((g ++ h).c) by Def3
      .= x + (y + z -y*z) -x*((g ++ h).c) by Def3
      .= x + (y + z) -y*z -x*(y + z -y*z) by Def3
      .= -y*z -x*y - x*z +x*y*z + (x + y + z);
    hence thesis by A2;
  end;
  hence thesis by A1,PARTFUN1:34;
end;

theorem
  f*(f ++ g) c= f & f c= f ++ (f*g)
proof
  thus f*(f ++ g) c= f
  proof
    let c;
    0 <= f.c & (f ++ g).c <= 1 by Th1;
    then ((f ++ g).c)*(f.c) <= 1*(f.c) by XREAL_1:66;
    hence thesis by Def2;
  end;
  let c;
  (f*g).c >= 0 & (1_minus f).c >= 0 by Th1;
  then 0*((f*g).c) <= ((1_minus f).c)*((f*g).c) by XREAL_1:66;
  then 0 <= (1 - f.c)*((f*g).c) by FUZZY_1:def 6;
  then 0+f.c <= (f*g).c - (f.c)*((f*g).c) + f.c by XREAL_1:8;
  then f.c <= f.c + (f*g).c - (f.c)*((f*g).c);
  hence thesis by Def3;
end;

theorem
  f*(g ++ h) c= (f*g) ++ (f*h)
proof
  let c;
  set x = f.c, y = g.c, z = h.c;
  f*f c= f by Th33;
  then (f*f).c <= f.c & (g*h).c >= 0 by Th1,FUZZY_1:def 3;
  then ((f*f).c)*((g*h).c) <= (f.c)*((g*h).c) by XREAL_1:66;
  then ((f*g).c + (f*h).c ) - ((f*f).c)*((g*h).c) >=
  ((f*g).c + (f*h).c) - (f.c)*((g*h).c) by XREAL_1:12;
  then ((f*g).c + (f*h).c ) - ((f*f)*(g*h)).c >=
  ((f*g).c + (f*h).c) - (f.c)*((g*h).c) by Def2;
  then ((f*g).c + (f*h).c ) - ((f*f)*(g*h)).c >=
  (x*y + (f*h).c) - (f.c)*((g*h).c) by Def2;
  then ((f*g).c + (f*h).c ) - ((f*f)*(g*h)).c >= x*y+x*z - (f.c)*((g*h).c)
  by Def2;
  then (f*g).c + (f*h).c - ((f* (f*(g*h)))).c >= x*y+x*z - (f.c)*((g*h).c)
  by Th34;
  then (f*g).c + (f*h).c - ((f*((g*f)*h))).c >= (y + z - (g*h).c)*x by Th34;
  then (f*g).c + (f*h).c - (((f*g)*(f*h))).c >= (y + z - (g*h).c)*x by Th34;
  then (f*g).c + (f*h).c - (((f*g)*(f*h))).c >= x*(y + z - y*z) by Def2;
  then (f*g).c + (f*h).c - ((f*g).c)*((f*h).c) >= x*(y + z - y*z) by Def2;
  then ((f*g) ++(f*h)).c >= x*(y + z - y*z) by Def3;
  then ((f*g) ++(f*h)).c >= x*((g ++ h).c) by Def3;
  hence thesis by Def2;
end;

theorem
  (f ++ g)*(f ++ h) c= f ++ (g*h)
proof
  let c;
  set x = f.c, y = g.c, z = h.c;
A1: ((f ++ g)*(f ++ h)).c - (f ++ (g*h)).c
  = ((f ++ g).c)*((f ++ h).c) - (f ++ (g*h)).c by Def2
    .= (x + y - x*y)*((f ++ h).c) - (f ++ (g*h)).c by Def3
    .= (x + y - x*y)*(x + z - x*z) - (f ++ (g*h)).c by Def3
    .= (x + y - x*y)*(x + z - x*z) - (x + (g*h).c - x*((g*h).c)) by Def3
    .= (x + y - x*y)*(x + z - x*z) - (x + y*z - x*((g*h).c)) by Def2
    .= (x - x*y + y)*(x + z - x*z) - (x + y*z - (y*z)*x) by Def2
    .= x*(y +z -y*z -1)+( - y+y*z -z +1)*(x*x)
    .= x*((g++h).c -1)+( - (y+z-y*z) +1)*(x*x) by Def3
    .= x*(-(-(g++h).c +1))+( - (g++h).c +1)*(x*x) by Def3
    .= (1-(g++h).c)*(-x+ x*x)
    .=((1_minus (g++h)).c)*(-x+ x*x) by FUZZY_1:def 6;
  f*f c= f by Th33;
  then (f*f).c <= f.c by FUZZY_1:def 3;
  then x*x <= x by Def2;
  then
A2: x*x - x <= 0 by XREAL_1:49;
  0<=(1_minus (g++h)).c by Th1;
  then ((1_minus (g++h)).c)*(-x+ x*x) <= 0*(-x+ x*x) by A2,XREAL_1:67;
  hence thesis by A1,XREAL_1:52;
end;

theorem
  1_minus (f*g) = (1_minus f) ++ (1_minus g)
proof
A1: C = dom 1_minus (f*g) &
  C = dom ((1_minus f) ++ (1_minus g)) by FUNCT_2:def 1;
  for c being Element of C st c in C holds
  (1_minus (f*g)).c = ((1_minus f) ++ (1_minus g)).c
  proof
    let c;
    ((1_minus f) ++ (1_minus g)).c
    = (1_minus f).c + (1_minus g).c - ((1_minus f).c)*((1_minus g).c) by Def3

.= 1 - f.c + (1_minus g).c - ((1_minus f).c)*((1_minus g).c) by FUZZY_1:def 6
      .= 1 - f.c + (1- g.c) - ((1_minus f).c)*((1_minus g).c) by FUZZY_1:def 6
      .= 1 - f.c + (1- g.c) - (1 - f.c)*((1_minus g).c) by FUZZY_1:def 6
      .= 1 - f.c + (1- g.c) - (1 - f.c)*(1- g.c) by FUZZY_1:def 6
      .= g.c- g.c + 1 - f.c*g.c
      .= 1 - (f*g).c by Def2;
    hence thesis by FUZZY_1:def 6;
  end;
  hence thesis by A1,PARTFUN1:34;
end;

theorem
  1_minus(f ++ g) = (1_minus f)*(1_minus g)
proof
A1: C = dom 1_minus(f ++ g) &
  C = dom ((1_minus f)*(1_minus g)) by FUNCT_2:def 1;
  for c being Element of C st c in C holds
  (1_minus(f ++ g)).c = ((1_minus f)*(1_minus g)).c
  proof
    let c;
    ((1_minus f)*(1_minus g)).c = ((1_minus f).c)*((1_minus g).c) by Def2
      .= (1 - f.c)*((1_minus g).c) by FUZZY_1:def 6
      .= (1 - f.c)*(1- g.c) by FUZZY_1:def 6
      .= 1 - (f.c+ g.c - f.c*g.c)
      .= 1 - (f ++ g).c by Def3;
    hence thesis by FUZZY_1:def 6;
  end;
  hence thesis by A1,PARTFUN1:34;
end;

theorem Th41:
  f ++ g = 1_minus((1_minus f)*(1_minus g))
proof
A1: C = dom (f ++ g) &
  C = dom 1_minus((1_minus f)*(1_minus g)) by FUNCT_2:def 1;
  for c being Element of C st c in C holds
  (f ++ g).c = (1_minus((1_minus f)*(1_minus g))).c
  proof
    let c;
    (1_minus((1_minus f)*(1_minus g))).c
    = 1 - ((1_minus f)*(1_minus g)).c by FUZZY_1:def 6
      .= 1 - ((1_minus f).c)*((1_minus g).c) by Def2
      .= 1 - (1 - f.c)*((1_minus g).c) by FUZZY_1:def 6
      .= 1 - (1 - f.c)*(1 - g.c) by FUZZY_1:def 6
      .= f.c + g.c - f.c*g.c;
    hence thesis by Def3;
  end;
  hence thesis by A1,PARTFUN1:34;
end;

theorem
  f*(EMF(C)) = EMF(C) & f*(UMF(C)) = f
proof
A1: f*(EMF C) = EMF C
  proof
A2: C = dom EMF(C) & C = dom (f*EMF(C)) by FUNCT_2:def 1;
    for c being Element of C st c in C holds (f*(EMF C)).c = (EMF C).c
    proof
      let c;
      (f*EMF(C)).c = (f.c)*((EMF C).c) by Def2
        .= (f.c)*0 by FUNCT_3:def 3;
      hence thesis by FUNCT_3:def 3;
    end;
    hence thesis by A2,PARTFUN1:34;
  end;
  f*(UMF(C)) = f
  proof
A3: C = dom f & C = dom (f*UMF C) by FUNCT_2:def 1;
    for c being Element of C st c in C holds (f*(UMF(C))).c = f.c
    proof
      let c;
      (f*(UMF(C))).c = (f.c)*((UMF(C)).c) by Def2
        .= (f.c)*1 by FUNCT_3:def 3;
      hence thesis;
    end;
    hence thesis by A3,PARTFUN1:34;
  end;
  hence thesis by A1;
end;

theorem
  f ++ EMF(C) = f & f ++ UMF(C) = UMF(C)
proof
A1: f ++ EMF(C) = f
  proof
A2: C = dom f & C = dom (f ++ EMF(C)) by FUNCT_2:def 1;
    for c being Element of C st c in C holds (f ++ EMF(C)).c = f.c
    proof
      let c;
      (f ++ EMF(C)).c = f.c + (EMF(C)).c - (f.c)*((EMF(C)).c) by Def3
        .= f.c + 0 - (f.c)*((EMF(C)).c) by FUNCT_3:def 3
        .= f.c - (f.c)*0 by FUNCT_3:def 3;
      hence thesis;
    end;
    hence thesis by A2,PARTFUN1:34;
  end;
  f ++ UMF(C) = UMF(C)
  proof
A3: C = dom UMF(C) & C = dom (f ++ UMF(C)) by FUNCT_2:def 1;
    for c being Element of C st c in C holds (f ++ UMF(C)).c = (UMF(C)).c
    proof
      let c;
      (f ++ UMF(C)).c = f.c + (UMF(C)).c - (f.c)*((UMF(C)).c) by Def3
        .= (UMF(C)).c+f.c - (f.c)*1 by FUNCT_3:def 3
        .= (UMF(C)).c + (f.c - f.c);
      hence thesis;
    end;
    hence thesis by A3,PARTFUN1:34;
  end;
  hence thesis by A1;
end;

theorem
  f*g c= min(f,g)
proof
  let c;
A1: min(f,g).c = min(f.c,g.c) by FUZZY_1:6;
  per cases by A1,XXREAL_0:15;
  suppose
A2: min(f,g).c = f.c;
    (f*g).c <= min(f,g).c
    proof
      g.c <= 1 & f.c >= 0 by Th1;
      then (g.c)*(f.c) <= 1*(f.c) by XREAL_1:66;
      hence thesis by A2,Def2;
    end;
    hence thesis;
  end;
  suppose
A3: min(f,g).c = g.c;
    (f*g).c <= min(f,g).c
    proof
      f.c <= 1 & g.c >= 0 by Th1;
      then (f.c)*(g.c) <= 1*(g.c) by XREAL_1:66;
      hence thesis by A3,Def2;
    end;
    hence thesis;
  end;
end;

theorem
  max(f,g) c= f ++ g
proof
  let c;
A1: max(f,g).c = max(f.c,g.c) by FUZZY_1:6;
  per cases by A1,XXREAL_0:16;
  suppose
A2: max(f,g).c = f.c;
    (f ++ g).c >= max(f,g).c
    proof
      g.c >= 0 & (1_minus f).c >= 0 by Th1;
      then 0*(g.c) <= (g.c)*((1_minus f).c) by XREAL_1:66;
      then 0 <= (g.c)*(1 - f.c) by FUZZY_1:def 6;
      then 0 + f.c <= g.c - (f.c)*(g.c) + f.c by XREAL_1:8;
      then f.c <= f.c + g.c - (f.c)*(g.c);
      hence thesis by A2,Def3;
    end;
    hence thesis;
  end;
  suppose
A3: max(f,g).c = g.c;
    (f ++ g).c >= max(f,g).c
    proof
      f.c >= 0 & (1_minus g).c >= 0 by Th1;
      then 0*(f.c) <= (f.c)*((1_minus g).c) by XREAL_1:66;
      then 0 <= (f.c)*(1 - g.c) by FUZZY_1:def 6;
      then 0 + g.c <= f.c - (f.c)*(g.c) + g.c by XREAL_1:8;
      then g.c <= f.c + g.c - (f.c)*(g.c);
      hence thesis by A3,Def3;
    end;
    hence thesis;
  end;
end;

Lm1: for a,b,c be Real st 0 <= c holds c*min(a,b) = min(c*a,c*b)
proof
  let a,b,c be Element of REAL;
  assume
A1: 0 <= c;
  per cases by XXREAL_0:15;
  suppose
A2: min(a,b) = a;
    c*min(a,b) = min(c*a,c*b)
    proof
      a <= b by A2,XXREAL_0:def 9;
      then a*c <= b*c by A1,XREAL_1:66;
      hence thesis by A2,XXREAL_0:def 9;
    end;
    hence thesis;
  end;
  suppose
A3: min(a,b) = b;
    c*min(a,b) = min(c*a,c*b)
    proof
      a >= b by A3,XXREAL_0:def 9;
      then a*c >= b*c by A1,XREAL_1:66;
      hence thesis by A3,XXREAL_0:def 9;
    end;
    hence thesis;
  end;
end;

Lm2: for a,b,c be Real st 0 <= c holds c*max(a,b) = max(c*a,c*b)
proof
  let a,b,c be Element of REAL;
  assume
A1: 0 <= c;
  per cases by XXREAL_0:16;
  suppose
A2: max(a,b) = b;
    c*max(a,b) = max(c*a,c*b)
    proof
      a <= b by A2,XXREAL_0:def 10;
      then a*c <= c*b by A1,XREAL_1:66;
      hence thesis by A2,XXREAL_0:def 10;
    end;
    hence thesis;
  end;
  suppose
A3: max(a,b) = a;
    c*max(a,b) = max(c*a,c*b)
    proof
      a >= b by A3,XXREAL_0:def 10;
      then a*c >= b*c by A1,XREAL_1:66;
      hence thesis by A3,XXREAL_0:def 10;
    end;
    hence thesis;
  end;
end;

theorem
  for a,b,c be Real st 0 <= c holds
  c*max(a,b) = max(c*a,c*b) & c*min(a,b) = min(c*a,c*b) by Lm1,Lm2;

theorem
  for a,b,c be Real holds
  c + max(a,b) = max(c+a,c+b) & c + min(a,b) = min(c+a,c+b)
proof
  let a,b,c be Element of REAL;
A1: c + max(a,b) = max(c+a,c+b)
  proof
    per cases by XXREAL_0:16;
    suppose
A2:   max(a,b) = b;
      c + max(a,b) = max(c+a,c+b)
      proof
        a <= b by A2,XXREAL_0:def 10;
        then c+a <= c+b by XREAL_1:8;
        hence thesis by A2,XXREAL_0:def 10;
      end;
      hence thesis;
    end;
    suppose
A3:   max(a,b) = a;
      c + max(a,b) = max(c+a,c+b)
      proof
        a >= b by A3,XXREAL_0:def 10;
        then a+c >= b+c by XREAL_1:8;
        hence thesis by A3,XXREAL_0:def 10;
      end;
      hence thesis;
    end;
  end;
  c + min(a,b) = min(c+a,c+b)
  proof
    per cases by XXREAL_0:15;
    suppose
A4:   min(a,b) = a;
      c + min(a,b) = min(c+a,c+b)
      proof
        a <= b by A4,XXREAL_0:def 9;
        then a+c <= c+b by XREAL_1:8;
        hence thesis by A4,XXREAL_0:def 9;
      end;
      hence thesis;
    end;
    suppose
A5:   min(a,b) = b;
      c + min(a,b) = min(c+a,c+b)
      proof
        a >= b by A5,XXREAL_0:def 9;
        then a+c >= b+c by XREAL_1:8;
        hence thesis by A5,XXREAL_0:def 9;
      end;
      hence thesis;
    end;
  end;
  hence thesis by A1;
end;

theorem
  f*max(g,h) = max(f*g,f*h)
proof
A1: C = dom (f*max(g,h)) & C = dom max(f*g,f*h) by FUNCT_2:def 1;
  for c being Element of C st c in C holds (f*max(g,h)).c = max(f*g,f*h).c
  proof
    let c;
A2: f.c >= 0 by Th1;
    (f*max(g,h)).c = (f.c)*(max(g,h).c) by Def2
      .= (f.c)*max(g.c,h.c) by FUZZY_1:6
      .= max((f.c)*g.c,(f.c)*(h.c)) by A2,Lm2
      .= max((f*g).c,(f.c)*(h.c)) by Def2
      .= max((f*g).c,(f*h).c) by Def2;
    hence thesis by FUZZY_1:6;
  end;
  hence thesis by A1,PARTFUN1:34;
end;

theorem
  f*min(g,h) = min(f*g,f*h)
proof
A1: C = dom (f*min(g,h)) & C = dom min(f*g,f*h) by FUNCT_2:def 1;
  for c being Element of C st c in C holds (f*min(g,h)).c = min(f*g,f*h).c
  proof
    let c;
A2: f.c >= 0 by Th1;
    (f*min(g,h)).c = (f.c)*(min(g,h).c) by Def2
      .= (f.c)*min(g.c,h.c) by FUZZY_1:6
      .= min((f.c)*g.c,(f.c)*(h.c)) by A2,Lm1
      .= min((f*g).c,(f.c)*(h.c)) by Def2
      .= min((f*g).c,(f*h).c) by Def2;
    hence thesis by FUZZY_1:6;
  end;
  hence thesis by A1,PARTFUN1:34;
end;

theorem
  f ++ max(g,h) = max((f ++ g),(f ++ h))
proof
A1: C = dom (f ++ max(g,h)) & C = dom max((f ++ g),(f ++ h))
 by FUNCT_2:def 1;
  for c being Element of C st c in C holds
  (f ++ max(g,h)).c = (max((f ++ g),(f ++ h))).c
  proof
    let c;
A2: (f ++ max(g,h)).c = f.c + max(g,h).c - (f.c)*(max(g,h).c) by Def3
      .= f.c + max(g.c,h.c) - (f.c)*(max(g,h).c) by FUZZY_1:6;
    per cases by XXREAL_0:16;
    suppose
A3:   max(g.c,h.c) = g.c;
