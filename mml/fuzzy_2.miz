      proof
        per cases by XXREAL_0:16;
        suppose
A9:       max(f.c,h.c) = f.c;
A10:      1 >= g.c by Th1;
          f.c <= max(f.c,(g*h).c) by XXREAL_0:25;
          then
A11:      f.c <= max(f,(g*h)).c by FUZZY_1:6;
          f.c >= 0 by Th1;
          then (f.c)*(g.c) <= (f.c)*1 by A10,XREAL_1:66;
          hence thesis by A8,A9,A11,XXREAL_0:2;
        end;
