      end;
      suppose
        min(f.c,g.c) = g.c & min(f.c,h.c) = h.c;
        hence thesis by XXREAL_0:17;
      end;
    end;
    hence thesis;
  end;
  min(f,g ++ h).c = min(f.c,(g ++ h).c) by FUZZY_1:6
    .= min(f.c,1 - ((1 - g.c)*(1 - h.c))) by Th54;
  hence thesis by A1,A2;
end;

