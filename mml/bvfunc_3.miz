      hence contradiction by A1,MARGREL1:45;
    end;
        for x being Element of Y st x in EqClass(z,CompF(PA,G)) holds
         (a 'imp' 'not' b).x=TRUE
    proof
      let x be Element of Y;
      assume A4:x in EqClass(z,CompF(PA,G));
      then (c 'imp' 'not' a).x=TRUE by A2;
      then A5:('not' c.x) 'or' ('not' a).x=TRUE by BVFUNC_1:def 11;
A6:  ('not' c.x)=TRUE or ('not' c.x)=FALSE by XBOOLEAN:def 3;
          (b 'imp' c).x=TRUE by A3,A4;
      then A7:('not' b.x) 'or' c.x=TRUE by BVFUNC_1:def 11;
A8:  ('not' b.x)=TRUE or ('not' b.x)=FALSE by XBOOLEAN:def 3;
      per cases by A5,A6,A7,A8,BINARITH:7;
      suppose ('not' c.x)=TRUE & ('not' b.x)=TRUE;
      then ('not' b).x=TRUE by MARGREL1:def 20;
      hence (a 'imp' 'not' b).x =('not' a.x) 'or' TRUE by BVFUNC_1:def 11
      .=TRUE by BINARITH:19;
      end;
      suppose ('not' c.x)=TRUE & c.x=TRUE;
      hence thesis by MARGREL1:41;
      end;
      suppose ('not' a).x=TRUE & ('not' b.x)=TRUE;
      then ('not' b).x=TRUE by MARGREL1:def 20;
      hence (a 'imp' 'not' b).x =('not' a.x) 'or' TRUE by BVFUNC_1:def 11
      .=TRUE by BINARITH:19;
      end;
      suppose ('not' a).x=TRUE & c.x=TRUE;
      then 'not' a.x=TRUE by MARGREL1:def 20;
      hence (a 'imp' 'not' b).x =TRUE 'or' ('not' b).x by BVFUNC_1:def 11
      .=TRUE by BINARITH:19;
    end;
    end;
    then B_INF(a 'imp' 'not' b,CompF(PA,G)).z = TRUE by BVFUNC_1:def 19;
    hence thesis by BVFUNC_2:def 9;
end;

theorem
      Ex(b,PA,G) '&' All(b 'imp' c,PA,G) '&' All(c 'imp' a,PA,G)
   '<' Ex(a '&' b,PA,G)
proof
    let z be Element of Y;
    assume
        (Ex(b,PA,G) '&' All(b 'imp' c,PA,G) '&' All(c 'imp' a,PA,G)).z=TRUE;
then A1:(Ex(b,PA,G) '&' All(b 'imp' c,PA,G)).z '&' All(c 'imp' a,PA,G).z=
TRUE
    by MARGREL1:def 21;
    then Ex(b,PA,G).z '&' All(b 'imp' c,PA,G).z '&'
       All(c 'imp' a,PA,G).z=TRUE by MARGREL1:def 21;
then A2:(Ex(b,PA,G).z '&' All(b 'imp' c,PA,G).z)=TRUE &
       All(c 'imp' a,PA,G).z=TRUE by MARGREL1:45;
        now assume not (ex x being Element of Y st
            x in EqClass(z,CompF(PA,G)) & b.x=TRUE);
      then B_SUP(b,CompF(PA,G)).z = FALSE by BVFUNC_1:def 20;
      then Ex(b,PA,G).z=FALSE by BVFUNC_2:def 10;
      hence contradiction by A2,MARGREL1:45;
    end;
    then consider x1 being Element of Y such that
A3:  x1 in EqClass(z,CompF(PA,G)) & b.x1=TRUE;
        now assume
          not (for x being Element of Y st x in EqClass(z,CompF(PA,G)) holds
         (c 'imp' a).x=TRUE);
      then B_INF(c 'imp' a,CompF(PA,G)).z = FALSE by BVFUNC_1:def 19;
      then All(c 'imp' a,PA,G).z=FALSE by BVFUNC_2:def 9;
      hence contradiction by A1,MARGREL1:45;
    end;
    then (c 'imp' a).x1=TRUE by A3;
    then A4:('not' c.x1) 'or' a.x1=TRUE by BVFUNC_1:def 11;
A5:('not' c.x1)=TRUE or ('not' c.x1)=FALSE by XBOOLEAN:def 3;
        now assume
          not (for x being Element of Y st x in EqClass(z,CompF(PA,G)) holds
         (b 'imp' c).x=TRUE);
      then B_INF(b 'imp' c,CompF(PA,G)).z = FALSE by BVFUNC_1:def 19;
      then All(b 'imp' c,PA,G).z=FALSE by BVFUNC_2:def 9;
      hence contradiction by A2,MARGREL1:45;
    end;
    then (b 'imp' c).x1=TRUE by A3;
    then A6:('not' b.x1) 'or' c.x1=TRUE by BVFUNC_1:def 11;
A7:('not' b.x1)=TRUE or ('not' b.x1)=FALSE by XBOOLEAN:def 3;
    per cases by A4,A5,A6,A7,BINARITH:7;
    suppose ('not' c.x1)=TRUE & ('not' b.x1)=TRUE;
    hence thesis by A3,MARGREL1:41;
    end;
    suppose ('not' c.x1)=TRUE & c.x1=TRUE;
    hence thesis by MARGREL1:41;
    end;
    suppose a.x1=TRUE & ('not' b.x1)=TRUE;
    hence thesis by A3,MARGREL1:41;
    end;
    suppose a.x1=TRUE & c.x1=TRUE;
    then (a '&' b).x1
     =TRUE '&' TRUE by A3,MARGREL1:def 21
    .=TRUE;
    then B_SUP(a '&' b,CompF(PA,G)).z = TRUE by A3,BVFUNC_1:def 20;
    hence thesis by BVFUNC_2:def 10;
end;
end;

theorem
      Ex(c,PA,G) '&' All(b 'imp' 'not' c,PA,G) '&' All(c 'imp' a,PA,G)
   '<' Ex(a '&' 'not' b,PA,G)
proof
    let z be Element of Y;
    assume
        (Ex(c,PA,G) '&' All(b 'imp' 'not'
c,PA,G) '&' All(c 'imp' a,PA,G)).z=TRUE;
then A1:   (Ex(c,PA,G) '&' All(b 'imp' 'not' c,PA,G)).z '&'
    All(c 'imp' a,PA,G).z=TRUE by MARGREL1:def 21;
    then Ex(c,PA,G).z '&' All(b 'imp' 'not' c,PA,G).z '&'
       All(c 'imp' a,PA,G).z=TRUE by MARGREL1:def 21;
then A2:Ex(c,PA,G).z '&' All(b 'imp' 'not' c,PA,G).z=TRUE &
       All(c 'imp' a,PA,G).z=TRUE by MARGREL1:45;
        now assume not (ex x being Element of Y st
            x in EqClass(z,CompF(PA,G)) & c.x=TRUE);
      then B_SUP(c,CompF(PA,G)).z = FALSE by BVFUNC_1:def 20;
      then Ex(c,PA,G).z=FALSE by BVFUNC_2:def 10;
      hence contradiction by A2,MARGREL1:45;
    end;
    then consider x1 being Element of Y such that
A3:  x1 in EqClass(z,CompF(PA,G)) & c.x1=TRUE;
        now assume
          not (for x being Element of Y st x in EqClass(z,CompF(PA,G)) holds
         (c 'imp' a).x=TRUE);
      then B_INF(c 'imp' a,CompF(PA,G)).z = FALSE by BVFUNC_1:def 19;
      then All(c 'imp' a,PA,G).z=FALSE by BVFUNC_2:def 9;
      hence contradiction by A1,MARGREL1:45;
    end;
    then (c 'imp' a).x1=TRUE by A3;
    then A4:('not' c.x1) 'or' a.x1=TRUE by BVFUNC_1:def 11;
A5:('not' c.x1)=TRUE or ('not' c.x1)=FALSE by XBOOLEAN:def 3;
        now assume
          not (for x being Element of Y st x in EqClass(z,CompF(PA,G)) holds
         (b 'imp' 'not' c).x=TRUE);
      then B_INF(b 'imp' 'not' c,CompF(PA,G)).z = FALSE by BVFUNC_1:def 19
;
      then All(b 'imp' 'not' c,PA,G).z=FALSE by BVFUNC_2:def 9;
      hence contradiction by A2,MARGREL1:45;
    end;
    then (b 'imp' 'not' c).x1=TRUE by A3;
    then A6:('not' b.x1) 'or' ('not' c).x1=TRUE by BVFUNC_1:def 11;
A7:('not' b.x1)=TRUE or
    ('not' b.x1)=FALSE by XBOOLEAN:def 3;
    per cases by A4,A5,A6,A7,BINARITH:7;
    suppose ('not' c.x1)=TRUE & ('not' b.x1)=TRUE;
    hence thesis by A3,MARGREL1:41;
    end;
    suppose ('not' c.x1)=TRUE & ('not' c).x1=TRUE;
    hence thesis by A3,MARGREL1:41;
    end;
    suppose A8:a.x1=TRUE & ('not' b.x1)=TRUE;
        (a '&' 'not' b).x1 =a.x1 '&' ('not' b).x1 by MARGREL1:def 21
    .=TRUE '&' TRUE by A8,MARGREL1:def 20
    .=TRUE;
    then B_SUP(a '&' 'not' b,CompF(PA,G)).z=TRUE by A3,BVFUNC_1:def 20;
    hence thesis by BVFUNC_2:def 10;
    end;
    suppose a.x1=TRUE & ('not' c).x1=TRUE;
    then 'not' c.x1=TRUE by MARGREL1:def 20;
    hence thesis by A3,MARGREL1:41;
end;
end;

