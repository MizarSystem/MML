         (a 'imp' 'not' c).x=TRUE);
      then B_INF(a 'imp' 'not' c,CompF(PA,G)).z = FALSE by BVFUNC_1:def 19
;
      then All(a 'imp' 'not' c,PA,G).z=FALSE by BVFUNC_2:def 9;
      hence contradiction by A1,MARGREL1:45;
    end;
        for x being Element of Y st x in EqClass(z,CompF(PA,G)) holds
         (a 'imp' 'not' b).x=TRUE
    proof
      let x be Element of Y;
      assume A4:x in EqClass(z,CompF(PA,G));
      then (b 'imp' c).x=TRUE by A2;
      then A5:('not' b.x) 'or' c.x=TRUE by BVFUNC_1:def 11;
A6:  ('not' b.x)=TRUE or ('not' b.x)=FALSE by XBOOLEAN:def 3;
          (a 'imp' 'not' c).x=TRUE by A3,A4;
      then A7:('not' a.x) 'or' ('not' c).x=TRUE by BVFUNC_1:def 11;
A8:  ('not' a.x)=TRUE or ('not' a.x)=FALSE by XBOOLEAN:def 3;
      per cases by A5,A6,A7,A8,BINARITH:7;
      suppose A9:('not' b.x)=TRUE & ('not' a.x)=TRUE;
      then ('not' b).x=TRUE by MARGREL1:def 20;
      hence (a 'imp' 'not' b).x =TRUE 'or' TRUE by A9,BVFUNC_1:def 11
      .=TRUE;
      end;
      suppose ('not' b.x)=TRUE & ('not' c).x=TRUE;
      then ('not' b).x=TRUE by MARGREL1:def 20;
      hence (a 'imp' 'not' b).x =('not' a.x) 'or' TRUE by BVFUNC_1:def 11
      .=TRUE by BINARITH:19;
      end;
      suppose c.x=TRUE & ('not' a.x)=TRUE;
      hence (a 'imp' 'not' b).x =TRUE 'or' ('not' b).x by BVFUNC_1:def 11
      .=TRUE by BINARITH:19;
      end;
      suppose A10:c.x=TRUE & ('not' c).x=TRUE;
      then 'not' c.x=TRUE by MARGREL1:def 20;
      hence thesis by A10,MARGREL1:41;
    end;
    end;
    then B_INF(a 'imp' 'not' b,CompF(PA,G)).z = TRUE by BVFUNC_1:def 19;
    hence thesis by BVFUNC_2:def 9;
end;

theorem
      All(b 'imp' 'not' c,PA,G) '&' Ex(a '&' c,PA,G) '<' Ex(a '&' 'not' b,PA,G)
proof
    let z be Element of Y;
    assume (All(b 'imp' 'not' c,PA,G) '&' Ex(a '&' c,PA,G)).z=TRUE;
then A1:    All(b 'imp' 'not' c,PA,G).z '&' Ex(a '&' c,PA,G).z=TRUE
       by MARGREL1:def 21;
A2:    now assume
          not (for x being Element of Y st x in EqClass(z,CompF(PA,G)) holds
         (b 'imp' 'not' c).x=TRUE);
      then B_INF(b 'imp' 'not' c,CompF(PA,G)).z = FALSE by BVFUNC_1:def 19
;
      then All(b 'imp' 'not' c,PA,G).z=FALSE by BVFUNC_2:def 9;
      hence contradiction by A1,MARGREL1:45;
    end;
        now assume not (ex x being Element of Y st
            x in EqClass(z,CompF(PA,G)) & (a '&' c).x=TRUE);
      then B_SUP(a '&' c,CompF(PA,G)).z = FALSE by BVFUNC_1:def 20;
      then Ex(a '&' c,PA,G).z=FALSE by BVFUNC_2:def 10;
      hence contradiction by A1,MARGREL1:45;
    end;
    then consider x1 being Element of Y such that
      A3:x1 in EqClass(z,CompF(PA,G)) & (a '&' c).x1=TRUE;
        (b 'imp' 'not' c).x1=TRUE by A2,A3;
    then A4:('not' b.x1) 'or' ('not' c).x1=TRUE by BVFUNC_1:def 11;
A5:('not' b.x1)=TRUE or ('not' b.x1)=FALSE by XBOOLEAN:def 3;
A6:    a.x1 '&' c.x1=TRUE by A3,MARGREL1:def 21;
    per cases by A4,A5,A6,BINARITH:7,MARGREL1:45;
    suppose A7:(a.x1=TRUE & c.x1=TRUE) & ('not' b.x1)=TRUE;
        (a '&' 'not' b).x1
     =a.x1 '&' ('not' b).x1 by MARGREL1:def 21
    .=TRUE '&' TRUE by A7,MARGREL1:def 20
    .=TRUE;
    then B_SUP(a '&' 'not' b,CompF(PA,G)).z = TRUE by A3,BVFUNC_1:def 20;
    hence thesis by BVFUNC_2:def 10;
    end;
    suppose A8:(a.x1=TRUE & c.x1=TRUE) & ('not' c).x1=TRUE;
    then 'not' c.x1=TRUE by MARGREL1:def 20;
    hence thesis by A8,MARGREL1:41;
end;
end;

theorem
      All(b 'imp' c,PA,G) '&' Ex(a '&' 'not' c,PA,G) '<' Ex(a '&' 'not' b,PA,G)
proof
    let z be Element of Y;
    assume (All(b 'imp' c,PA,G) '&' Ex(a '&' 'not' c,PA,G)).z=TRUE;
