      FUNCT_4, NUMBERS, FUNCT_2, STRUCT_0, GRAPH_1, CAT_1, OPPCAT_1;
 constructors PARTFUN1, OPPCAT_1, FUNCOP_1, RELSET_1;
 registrations XBOOLE_0, RELSET_1, STRUCT_0;
 requirements SUBSET, BOOLE;
