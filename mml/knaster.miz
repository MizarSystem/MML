      suppose
        O1 <> O2 & O2 <> succ O4;
        then O2 c< succ O4 by A16,XBOOLE_0:def 8;
        then O2 in succ O4 by ORDINAL1:21;
        then O2 c= O4 by ORDINAL1:34;
        hence thesis by A14,A15;
      end;
      suppose
        O1 = O2 & O2 = succ O4;
        hence thesis;
      end;
      suppose
A17:    O1 <> O2 & O2 = succ O4;
A18:    (f, succ O4)-.a [= (f, O4)-.a
        proof
          per cases by ORDINAL1:42;
          suppose
A19:        O4 is limit_ordinal;
            thus thesis
            proof
              per cases;
              suppose
                O4 = {};
                then (f, O4)-.a = a by Th17;
