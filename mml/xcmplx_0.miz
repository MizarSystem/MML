registration let x be non zero (complex number);
  cluster -x -> non zero;
  coherence
   proof
    assume
A1:   -x = 0;
         x + -x = 0 by Def6;
     then consider x1,x2,y1,y2 being Element of REAL such that
A2:   x = [*x1,x2*] and
A3:   -x = [*y1,y2*] and
A4:   0 = [*+(x1,y1),+(x2,y2)*] by Def4;
A5:   +(x2,y2) = 0 by A4,ARYTM_0:26;
     then A6:    +(x1,y1) = 0 by A4,ARYTM_0:def 7;
      y2 = 0 by A1,A3,ARYTM_0:26;
     then A8:     y1 = 0 by A1,A3,ARYTM_0:def 7;
         x2 = 0 by A5,A1,A3,ARYTM_0:13,26;
     then x = x1 by A2,ARYTM_0:def 7 .= 0 by A6,A8,ARYTM_0:13;
    hence contradiction;
   end;
  cluster x" -> non zero;
  coherence
   proof
    assume
A9:   x" = 0;
         x*x" = 1 by Def7;
     then consider x1,x2,y1,y2 being Element of REAL such that
          x = [*x1,x2*] and
A10:   x" = [*y1,y2*] and
A11:   1 = [* +(*(x1,y1),opp*(x2,y2)), +(*(x1,y2),*(x2,y1)) *] by Def5;
       y2 = 0 by A9,A10,ARYTM_0:26;
     then A13:   y1 = 0 by A9,A10,ARYTM_0:def 7;
         +(*(x1,y2),*(x2,y1)) = 0 by A11,ARYTM_0:26;
     then 1 = +(*(x1,y1),opp*(x2,y2)) by A11,ARYTM_0:def 7
          .= +(*(x1,y1),*(opp x2,y2)) by ARYTM_0:17
          .= +(0,*(opp x2,y2)) by A13,ARYTM_0:14
          .= *(opp x2,y2) by ARYTM_0:13;
    hence contradiction by A9,A10,ARYTM_0:14,26;
   end;
  let y be non zero (complex number);
  cluster x*y -> non zero;
  coherence
   proof
         1 in REAL;
     then reconsider j =1 as complex number by Def2,Lm4;
     consider u1,u2,v1,v2 being Element of REAL such that
A14:   j = [*u1,u2*] and
A15:   y = [*v1,v2*] and
A16:   j*y = [* +(*(u1,v1),opp*(u2,v2)), +(*(u1,v2),*(u2,v1)) *] by Def5;
A17:   u2 = 0 by A14,ARYTM_0:26;
     then A18:    +(*(u1,v2),*(u2,v1)) = *(u1,v2) by ARYTM_0:13,14;
A19:   u1 = 1 by A14,A17,ARYTM_0:def 7;
         +(0,opp 0) = 0 by ARYTM_0:def 4;
     then A20:   opp 0 = 0 by ARYTM_0:13;
A21:   +(*(u1,v1),opp*(u2,v2)) = +(v1,opp*(u2,v2)) by A19,ARYTM_0:21
         .= +(v1,*(opp u2,v2)) by ARYTM_0:17
         .= +(v1,*(0,v2)) by A14,A20,ARYTM_0:26
         .= +(v1,0) by ARYTM_0:14
         .= v1 by ARYTM_0:13;
      0 in REAL;
     then reconsider z =0 as complex number by Def2,Lm4;
     consider u1,u2,v1,v2 being Element of REAL such that
          x" = [*u1,u2*] and
A22:   z = [*v1,v2*] and
A23:   x"*z = [* +(*(u1,v1),opp*(u2,v2)), +(*(u1,v2),*(u2,v1)) *] by Def5;
        v2 = 0 by A22,ARYTM_0:26;
      then A25:    v1 = 0 by A22,ARYTM_0:def 7;
      then A26:    +(*(u1,v1),opp*(u2,v2)) = +(0,opp*(u2,v2)) by ARYTM_0:14
         .= opp*(u2,v2) by ARYTM_0:13
         .= 0 by A20,A22,ARYTM_0:14,26;
A27:  +(*(u1,v2),*(u2,v1)) = +(0,*(u2,v1)) by A22,ARYTM_0:14,26
         .= *(u2,v1) by ARYTM_0:13
         .= 0 by A25,ARYTM_0:14;
    assume
A28: x*y = 0;
A29: x"*x*y = x"*(x*y) by Lm3 .= 0 by A23,A26,A27,A28,ARYTM_0:def 7;
      x"*x*y = j * y by Def7 .= y by A15,A16,A18,A19,A21,ARYTM_0:21;
    hence contradiction by A29;
   end;
end;

