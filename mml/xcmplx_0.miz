registration
  let x be non zero (complex number);
  cluster -x -> non zero;
  coherence
  proof
    assume
A1: -x = 0;
A2: x + -x = 0 by Def6;
    consider x1,x2,y1,y2 being Element of REAL such that
A3: x = [*x1,x2*] and
A4: -x = [*y1,y2*] and
A5: 0 = [*+(x1,y1),+(x2,y2)*] by A2,Def4;
A6: +(x2,y2) = 0 by A5,ARYTM_0:26;
A7: +(x1,y1) = 0 by A5,A6,ARYTM_0:def 7;
A8: y2 = 0 by A1,A4,ARYTM_0:26;
A9: y1 = 0 by A1,A4,A8,ARYTM_0:def 7;
A10: x2 = 0 by A1,A4,A6,ARYTM_0:13,26;
A11: x = x1 by A3,A10,ARYTM_0:def 7
      .= 0 by A7,A9,ARYTM_0:13;
    thus contradiction by A11;
  end;
  cluster x" -> non zero;
  coherence
  proof
    assume
A12: x" = 0;
A13: x*x" = 1 by Def7;
    consider x1,x2,y1,y2 being Element of REAL such that
    x = [*x1,x2*] and
A14: x" = [*y1,y2*] and
A15: 1 = [* +(*(x1,y1),opp*(x2,y2)), +(*(x1,y2),*(x2,y1)) *] by A13,Def5;
A16: y2 = 0 by A12,A14,ARYTM_0:26;
A17: y1 = 0 by A12,A14,A16,ARYTM_0:def 7;
A18: +(*(x1,y2),*(x2,y1)) = 0 by A15,ARYTM_0:26;
A19: 1 = +(*(x1,y1),opp*(x2,y2)) by A15,A18,ARYTM_0:def 7
      .= +(*(x1,y1),*(opp x2,y2)) by ARYTM_0:17
      .= *(opp x2,y2) by A17,ARYTM_0:13,14;
    thus contradiction by A12,A14,A19,ARYTM_0:14,26;
  end;
  let y be non zero (complex number);
  cluster x*y -> non zero;
  coherence
  proof
A20: 1 in REAL;
    reconsider j =1 as complex number by A20,Def2,Lm4;
    consider u1,u2,v1,v2 being Element of REAL such that
A21: j = [*u1,u2*] and
    A22: y
 = [*v1,v2*] & j*y = [* +(*(u1,v1),opp*(u2,v2)), +(*(u1,v2),*(u2,v1)) *]
    by Def5;
A23: u2 = 0 by A21,ARYTM_0:26;
A24: +(*(u1,v2),*(u2,v1)) = *(u1,v2) by A23,ARYTM_0:13,14;
A25: u1 = 1 by A21,A23,ARYTM_0:def 7;
A26: +(0,opp 0) = 0 by ARYTM_0:def 4;
A27: opp 0 = 0 by A26,ARYTM_0:13;
A28: +(*(u1,v1),opp*(u2,v2)) = +(v1,opp*(u2,v2)) by A25,ARYTM_0:21
      .= +(v1,*(opp u2,v2)) by ARYTM_0:17
      .= +(v1,*(0,v2)) by A21,A27,ARYTM_0:26
      .= v1 by ARYTM_0:13,14;
A29: 0 in REAL;
    reconsider z =0 as complex number by A29,Def2,Lm4;
    consider u1,u2,v1,v2 being Element of REAL such that
    x" = [*u1,u2*] and
A30: z = [*v1,v2*] and
A31: x"*z = [* +(*(u1,v1),opp*(u2,v2)), +(*(u1,v2),*(u2,v1)) *] by Def5;
A32: v2 = 0 by A30,ARYTM_0:26;
A33: v1 = 0 by A30,A32,ARYTM_0:def 7;
A34: +(*(u1,v1),opp*(u2,v2)) = opp*(u2,v2) by A33,ARYTM_0:13,14
      .= 0 by A27,A30,ARYTM_0:14,26;
A35: +(*(u1,v2),*(u2,v1)) = +(0,*(u2,v1)) by A30,ARYTM_0:14,26
      .= *(u2,v1) by ARYTM_0:13
      .= 0 by A33,ARYTM_0:14;
    assume
A36: x*y = 0;
A37: x"*x*y = x"*(x*y) by Lm3
      .= 0 by A31,A34,A35,A36,ARYTM_0:def 7;
A38: x"*x*y = j * y by Def7
      .= y by A22,A24,A25,A28,ARYTM_0:21;
    thus contradiction by A37,A38;
  end;
end;

