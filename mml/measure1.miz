registration
  let X be set;
  cluster non empty compl-closed sigma-additive Subset-Family of X;
  existence
  proof
    reconsider S = {{},X} as non empty Subset-Family of X by PROB_1:14;
    take S;
    thus S is non empty;
    thus for A being set holds A in S implies X\A in S
    proof
      let A be set;
A1:   A = {} implies X\A in S by TARSKI:def 2;
A2:   A = X implies X\A in S
      proof
        assume A = X;
        then X\A = {} by XBOOLE_1:37;
        hence thesis by TARSKI:def 2;
      end;
      assume A in S;
      hence thesis by A1,A2,TARSKI:def 2;
    end;
    let M be N_Sub_set_fam of X;
    assume
A3: M c= S;
A4: not X in M implies union M in S
    proof
      assume not X in M;
      then for A being set st A in M holds A c= {} by A3,TARSKI:def 2;
      then union M c= {} by ZFMISC_1:94;
      then union M = {};
      hence thesis by TARSKI:def 2;
    end;
    X in M implies union M in S
    proof
      assume X in M;
      then X c= union M by ZFMISC_1:92;
      then X = union M by XBOOLE_0:def 10;
      hence thesis by TARSKI:def 2;
    end;
    hence thesis by A4;
  end;
end;

registration
  let X;
  cluster compl-closed sigma-multiplicative -> sigma-additive Subset-Family of
    X;
  coherence
  proof
    let F be Subset-Family of X such that
A1: F is compl-closed and
A2: F is sigma-multiplicative;
    let M be non empty countable Subset-Family of X such that
A3: M c= F;
    consider f being SetSequence of X such that
A4: M = rng f by SUPINF_2:def 14;
    for n being Nat holds (Complement f).n in F
    proof
      let n be Nat;
      reconsider n as Element of NAT by ORDINAL1:def 13;
      f.n in M & (Complement f).n = (f.n)` by A4,FUNCT_2:6,PROB_1:def 4;
      hence thesis by A1,A3,PROB_1:def 1;
    end;
    then rng Complement f c= F by NAT_1:53;
    then
A5: Intersection Complement f in F by A2,PROB_1:def 8;
    (Intersection Complement f)` = union M by A4,CARD_3:def 4;
    hence thesis by A1,A5,PROB_1:def 1;
  end;
  cluster compl-closed sigma-additive -> sigma-multiplicative Subset-Family of
    X;
  coherence
  proof
    let F be Subset-Family of X such that
A6: F is compl-closed and
A7: F is sigma-additive;
    let f be SetSequence of X such that
A8: rng f c= F;
    dom Complement f = NAT by FUNCT_2:def 1;
    then reconsider
    M = rng Complement f as non empty countable Subset-Family of X
    by CARD_3:127,RELAT_1:65;
    M c= F
    proof
      let e be set;
      assume e in M;
      then consider n being set such that
A9:   n in NAT and
A10:  e = (Complement f).n by FUNCT_2:17;
      reconsider n as Element of NAT by A9;
A11:  f.n in rng f by NAT_1:52;
      e = (f.n)` by A10,PROB_1:def 4;
      hence thesis by A6,A8,A11,PROB_1:def 1;
    end;
    then Intersection f = (union M)` & union M in F by A7,
    Def9,CARD_3:def
    4;
    hence thesis by A6,PROB_1:def 1;
  end;
end;

