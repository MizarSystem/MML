      per cases;
      suppose
        n in dom F;
        hence thesis by A1;
      end;
      suppose
        not n in dom F;
        hence thesis by FUNCT_1:def 4;
      end;
    end;
    thus for x,y being set st x <> y holds F.x misses F.y
    proof
      let x,y be set;
      F.x = {} by A2;
      hence thesis by XBOOLE_1:65;
    end;
