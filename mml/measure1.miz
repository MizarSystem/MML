begin :: Some useful theorems about sets and R_eal numbers

reserve X for set;

theorem Th1:
  for X,Y being set holds union {X,Y,{}} = union {X,Y}
proof
  let X,Y be set;
  thus union {X,Y,{}} = union ({X,Y} \/ {{}}) by ENUMSET1:43
    .= union {X,Y} \/ union {{}} by ZFMISC_1:96
    .= union {X,Y} \/ {} by ZFMISC_1:31
    .= union {X,Y};
end;

canceled 5;

theorem
  for A,B being Subset of X holds {A,B} is Subset-Family of X
proof
  let A,B be Subset of X;
  set C = {A,B};
  C c= bool X
  proof
    let x be set;
    assume x in C;
    then x = A or x = B by TARSKI:def 2;
    hence thesis;
  end;
  hence thesis;
end;

theorem
  for A,B,C being Subset of X holds {A,B,C} is Subset-Family of X
proof
  let A,B,C be Subset of X;
  set D = {A,B,C};
  D c= bool X
  proof
    let x be set;
    assume x in D;
    then x = A or x = B or x = C by ENUMSET1:def 1;
    hence thesis;
  end;
  hence thesis;
end;

scheme
  DomsetFamEx {A() -> set,P[set]}: ex F being non empty Subset-Family of A()
  st for B being set holds B in F iff B c= A() & P[B]
provided
A1: ex B being set st B c= A() & P[B]
proof
  defpred X[set] means ex Z being set st $1 = Z & P[Z];
  consider X being set such that
A2: for x being set holds x in X iff x in bool A() & X[x] from XBOOLE_0:
  sch 1;
  X c= bool A()
  proof
    let x be set;
    assume x in X;
    hence thesis by A2;
  end;
  then reconsider X as non empty Subset-Family of A() by A1,A2;
  take X;
  for B being set holds B in X iff B c= A() & P[B]
  proof
    let B be set;
    thus B in X implies B c= A() & P[B]
    proof
      assume
A3:   B in X;
      then ex Z being set st B = Z & P[Z] by A2;
      hence thesis by A3;
    end;
    assume B c= A() & P[B];
    hence thesis by A2;
  end;
  hence thesis;
end;

notation
  let X be set;
  let S be non empty Subset-Family of X;
  synonym X\S for COMPLEMENT S;
end;

registration
  let X be set;
  let S be non empty Subset-Family of X;
  cluster X\S -> non empty;
  coherence
  proof
    consider x being Subset of X such that
A1: x in S by SUBSET_1:10;
    x`` in S by A1;
    hence thesis by SETFAM_1:def 8;
  end;
end;

canceled 6;

theorem Th15:
  for S being non empty Subset-Family of X holds meet S = X \
  union (X \ S) & union S = X \ meet (X \ S)
proof
  let S be non empty Subset-Family of X;
A1: X \ union (X \ S) c= meet S
  proof
    let x be set;
    assume
A2: x in X \ union (X \ S);
    then
A3: not x in union (X \ S) by XBOOLE_0:def 5;
    for Y being set st Y in S holds x in Y
    proof
      let Y be set;
      assume that
A4:   Y in S and
A5:   not x in Y;
      reconsider Y as Subset of X by A4;
      Y`` in S by A4;
      then
A6:   Y` in X \ S by SETFAM_1:def 8;
      x in Y` by A2,A5,SUBSET_1:50;
      hence contradiction by A3,A6,TARSKI:def 4;
    end;
    hence thesis by SETFAM_1:def 1;
  end;
  meet S c= X \ union (X \ S)
  proof
    let x be set;
    assume
A7: x in meet S;
    not x in union (X \ S)
    proof
      assume x in union (X \ S);
      then consider Z being set such that
A8:   x in Z and
A9:   Z in X \ S by TARSKI:def 4;
      reconsider Z as Subset of X by A9;
      Z` in S & not x in Z` by A8,A9,SETFAM_1:def 8,XBOOLE_0:def 5;
      hence thesis by A7,SETFAM_1:def 1;
    end;
    hence thesis by A7,XBOOLE_0:def 5;
  end;
  hence meet S = X \ union (X \ S) by A1,XBOOLE_0:def 10;
  thus union S c= X \ meet (X \ S)
  proof
    let x be set;
    assume x in union S;
    then consider Y being set such that
A10: x in Y and
A11: Y in S by TARSKI:def 4;
    reconsider Y as Subset of X by A11;
    not x in meet (X \ S)
    proof
      Y`` in S by A11;
      then
A12:  Y` in X \ S by SETFAM_1:def 8;
      assume
A13:  x in meet (X \ S);
      not x in Y` by A10,XBOOLE_0:def 5;
      hence thesis by A13,A12,SETFAM_1:def 1;
    end;
    hence thesis by A10,A11,XBOOLE_0:def 5;
  end;
  let x be set;
  assume
A14: x in X \ meet (X \ S);
  then not x in meet (X \ S) by XBOOLE_0:def 5;
  then consider Z being set such that
A15: Z in X \ S and
A16: not x in Z by SETFAM_1:def 1;
  reconsider Z as Subset of X by A15;
A17: Z` in S by A15,SETFAM_1:def 8;
  x in Z` by A14,A16,SUBSET_1:50;
  hence thesis by A17,TARSKI:def 4;
end;
 
::
::        Field Subset of X and nonnegative measure
::

definition
  let X be set;
  let IT be Subset-Family of X;
  canceled 2;
  redefine attr IT is compl-closed means
  :Def3:
  for A being set holds A in IT implies X\A in IT;
  compatibility
  proof
    hereby
      assume
A1:   IT is compl-closed;
      let A be set;
      assume
A2:   A in IT;
      then reconsider A9 = A as Subset of X;
      A9` in IT by A1,A2,PROB_1:def 1;
      hence X\A in IT;
    end;
    assume
A3: for A being set holds A in IT implies X\A in IT;
    let A be Subset of X;
    assume A in IT;
    hence thesis by A3;
  end;
end;

registration
  let X be set;
  cluster cup-closed compl-closed -> cap-closed Subset-Family of X;
  coherence
proof
  let S be Subset-Family of X;
  assume
A1: S is cup-closed compl-closed;
  let A,B be set;
  assume that
A2: A in S and
A3: B in S;
  X \ A in S & X \ B in S by A1,A2,A3,Def3;
  then
A4: (X \ A) \/ (X \ B) in S by A1,FINSUB_1:def 1;
  A /\ B c= A by XBOOLE_1:17;
  then A /\ B = X /\ (A /\ B) by A2,XBOOLE_1:1,28
    .= X \ (X \ (A /\ B)) by XBOOLE_1:48
    .= X \ ((X \ A) \/ (X \ B)) by XBOOLE_1:54;
  hence thesis by A1,A4,Def3;
end;
  cluster cap-closed compl-closed -> cup-closed Subset-Family of X;
  coherence
  proof
    let S be Subset-Family of X;
    assume
A1: S is cap-closed compl-closed;
    let A,B be set;
    assume
A2: A in S & B in S;
    then X \ A in S & X \ B in S by A1,Def3;
    then
A3: (X \ A) /\ (X \ B) in S by A1,FINSUB_1:def 2;
    A \/ B = X /\ (A \/ B) by A2,XBOOLE_1:8,28
      .= X \ (X \ (A \/ B)) by XBOOLE_1:48
      .= X \ ((X \ A) /\ (X \ B)) by XBOOLE_1:53;
    hence thesis by A1,A3,Def3;
  end;
end;

canceled;

theorem
  for S being Field_Subset of X holds S = X \ S
proof
  let S be Field_Subset of X;
  for A being set holds A in S iff A in X \ S
  proof
    let A be set;
    hereby
      assume
A1:   A in S;
      then reconsider B = A as Subset of X;
      B` in S by A1,PROB_1:def 1;
      hence A in X \ S by SETFAM_1:def 8;
    end;
    assume
A2: A in X \ S;
    then reconsider B = A as Subset of X;
    B` in S by A2,SETFAM_1:def 8;
    then B`` in S by PROB_1:def 1;
    hence thesis;
  end;
  hence thesis by TARSKI:2;
end;

canceled 2;

registration let X;
 cluster compl-closed cap-closed -> diff-closed Subset-Family of X;
 coherence
proof
  let S be Subset-Family of X such that
Z1: S is compl-closed and
Z2: S is cap-closed;
  let A,B be set;
  assume that
A1: A in S and
A2: B in S;
A3: A /\ (X \ B) = (A /\ X) \ B by XBOOLE_1:49
    .= A \ B by A1,XBOOLE_1:28;
   X \ B in S by A2,Z1,Def3;
  hence thesis by A1,A3,Z2,FINSUB_1:def 2;
end;
end;

theorem
  for S being Field_Subset of X holds for A,B being set st A in S
  & B in S holds A \ B in S by FINSUB_1:def 3;

theorem
  for S being Field_Subset of X holds {} in S & X in S by PROB_1:10,11;

definition
  let X be non empty set, F be Function of X,ExtREAL;
  redefine attr F is nonnegative means
  :Def4:
  for A being Element of X holds 0. <= F.A;
  compatibility by SUPINF_2:58;
end;

canceled 2;

definition
  let X be set, S be Field_Subset of X;
  let F be Function of S,ExtREAL;
  attr F is additive means
  :Def5:
  for A,B being Element of S st A misses B holds F.(A \/ B) = F.A + F.B;
end;

registration
  let X be set, S be Field_Subset of X;
  cluster nonnegative additive zeroed Function of S,ExtREAL;
  existence
  proof
    reconsider M = S --> 0. as Function of S,ExtREAL;
A1: for A,B being Element of S st A misses B holds M.(A \/ B) = M.A + M.B
    proof
      let A,B be Element of S;
      assume A misses B;
A2:   M.A = 0. & M.B = 0. by FUNCOP_1:13;
      reconsider A,B as set;
      reconsider A,B as Element of S;
      0.= M.A + M.B by A2,XXREAL_3:4;
      hence thesis by FUNCOP_1:13;
    end;
    take M;
    ( for x being Element of S holds 0.<= M.x)& M.{} = 0. by FUNCOP_1:13 ,
    PROB_1:10;
    hence thesis by A1,Def4,Def5,VALUED_0:def 19;
  end;
end;

definition
  let X be set, S be Field_Subset of X;
  mode Measure of S is nonnegative additive zeroed Function of S,ExtREAL;
end;

canceled;

theorem Th25:
  for S being Field_Subset of X, M being Measure of S, A,B being
  Element of S holds A c= B implies M.A <= M.B
proof
  let S be Field_Subset of X, M be Measure of S, A,B be Element of S;
  reconsider C = B \ A as Element of S;
A1: 0.<= M.C by Def4;
  A misses C by XBOOLE_1:79;
  then
A2: M.(A \/ C) = M.A + M.C by Def5;
  assume A c= B;
  then M.B = M.A + M.C by A2,XBOOLE_1:45;
  hence thesis by A1,XXREAL_3:42;
end;

theorem Th26:
  for S being Field_Subset of X, M being Measure of S, A,B being
  Element of S holds A c= B & M.A < +infty implies M.(B \ A) = M.B - M.A
proof
  let S be Field_Subset of X, M be Measure of S, A,B be Element of S;
  reconsider C = B \ A as Element of S;
  assume that
A1: A c= B and
A2: M.A < +infty;
A3: 0.<= M.A by Def4;
  A misses C by XBOOLE_1:79;
  then M.(A \/ C) = M.A + M.C by Def5;
  then M.B = M.A + M.C by A1,XBOOLE_1:45;
  hence thesis by A2,A3,XXREAL_3:29;
end;

registration
  let X be set;
  cluster non empty compl-closed cap-closed Subset-Family of X;
  existence
  proof
    consider S being non empty compl-closed cap-closed Subset-Family of X;
    take S;
    thus thesis;
  end;
end;

definition
  let X be set, S be non empty cup-closed Subset-Family of X, A,B be Element
  of S;
  redefine func A \/ B -> Element of S;
  coherence by FINSUB_1:def 1;
end;

definition
  let X be set, S be Field_Subset of X, A,B be Element of S;
  redefine func A /\ B -> Element of S;
  coherence by FINSUB_1:def 2;
  redefine func A \ B -> Element of S;
  coherence by FINSUB_1:def 3;
end;

theorem Th27:
  for S being Field_Subset of X, M being Measure of S, A,B being
  Element of S holds M.(A \/ B) <= M.A + M.B
proof
  let S be Field_Subset of X, M be Measure of S, A,B be Element of S;
  set C = B \ A;
A1: A misses C by XBOOLE_1:79;
A2: M.C <= M.B by Th25,XBOOLE_1:36;
  M.(A \/ B) = M.(A \/ C) by XBOOLE_1:39
    .= M.A + M.C by A1,Def5;
  hence thesis by A2,XXREAL_3:38;
end;

canceled;

theorem
  for S being Field_Subset of X, M being Measure of S holds {} in S & X
in S & for A,B being set st A in S & B in S holds X \ A in S & A \/ B in S & A
  /\ B in S by FINSUB_1:def 1, def 2,PROB_1:10,11, Def3;

definition
  canceled;
  let X be set, S be Field_Subset of X, M be Measure of S;
  mode measure_zero of M -> Element of S means
    :Def7:
    M.it = 0.;
  existence
  proof
    reconsider A = {} as Element of S by PROB_1:10;
    take A;
    thus thesis by VALUED_0:def 19;
  end;
end;

canceled;

theorem
  for S being Field_Subset of X, M being Measure of S, A being Element
  of S, B being measure_zero of M st A c= B holds A is measure_zero of M
proof
  let S be Field_Subset of X, M be Measure of S, A be Element of S, B be
  measure_zero of M;
  assume A c= B;
  then M.A <= M.B by Th25;
  then
A1: M.A <= 0. by Def7;
  0.<= M.A by Def4;
  then M.A = 0.by A1,XXREAL_0:1;
  hence thesis by Def7;
end;

theorem
  for S being Field_Subset of X, M being Measure of S, A,B being
measure_zero of M holds A \/ B is measure_zero of M & A /\ B is measure_zero of
  M & A \ B is measure_zero of M
proof
  let S be Field_Subset of X, M be Measure of S, A,B be measure_zero of M;
A1: 0.<= M.(A /\ B) by Def4;
A2: 0.<= M.(A \ B) by Def4;
A3: M.A = 0. by Def7;
  then M.(A \ B) <= 0.by Th25,XBOOLE_1:36;
  then
A4: M.(A \ B) = 0.by A2,XXREAL_0:1;
  M.B = 0. by Def7;
  then M.(A \/ B) <= 0.+ 0.by A3,Th27;
  then
A5: M.(A \/ B) <= 0.by XXREAL_3:4;
  0.<= M.(A \/ B) by Def4;
  then
A6: M.(A \/ B) = 0.by A5,XXREAL_0:1;
  M.(A /\ B) <= 0.by A3,Th25,XBOOLE_1:17;
  then M.(A /\ B) = 0.by A1,XXREAL_0:1;
  hence thesis by A6,A4,Def7;
end;

theorem
  for S being Field_Subset of X, M being Measure of S, A being Element
of S, B being measure_zero of M holds M.(A \/ B) = M.A & M.(A /\ B) = 0.& M.(A
  \ B) = M.A
proof
  let S be Field_Subset of X, M be Measure of S, A be Element of S, B be
  measure_zero of M;
A1: M.A = M.((A /\ B) \/ (A \ B)) by XBOOLE_1:51;
A2: M.B = 0.by Def7;
  then
A3: M.(A /\ B) <= 0.by Th25,XBOOLE_1:17;
A4: 0.<= M.(A /\ B) by Def4;
  then M.(A /\ B) = 0.by A3,XXREAL_0:1;
  then M.A <= 0.+ M.(A \ B) by A1,Th27;
  then
A5: M.A <= M.(A \ B) by XXREAL_3:4;
  M.(A \/ B) <= M.A + 0.by A2,Th27;
  then
A6: M.(A \/ B) <= M.A by XXREAL_3:4;
  M.A <= M.(A \/ B) & M.(A \ B) <= M.A by Th25,XBOOLE_1:7,36;
  hence thesis by A4,A6,A3,A5,XXREAL_0:1;
end;
 
::
::       sigma_Field Subset of X and sigma_additive nonnegative measure
::

theorem Th34:
  for A being Subset of X ex F being Function of NAT,bool X st rng F = {A}
proof
  let A be Subset of X;
  reconsider F = NAT --> A as Function of NAT, bool X;
  take F;
  thus thesis by FUNCOP_1:14;
end;

theorem Th35:
  for A being set ex F being Function of NAT,{A} st for n being
  Element of NAT holds F.n = A
proof
  let A be set;
  reconsider F = NAT --> A as Function of NAT, {A};
  take F;
  thus thesis by TARSKI:def 1;
end;

registration
  let X be set;
  cluster non empty countable Subset-Family of X;
  existence
  proof
    reconsider S = {{}} as Subset-Family of X by SETFAM_1:61;
    take S;
    thus S is non empty;
    {} is Subset of X by XBOOLE_1:2;
    hence S is empty or ex F being Function of NAT,bool X st S = rng F by
    Th34;
  end;
end;

definition
  let X be set;
  mode N_Sub_set_fam of X is non empty countable Subset-Family of X;
end;

canceled 2;

theorem Th38:
  for A,B,C being Subset of X ex F being Function of NAT,bool X st
rng F = {A,B,C} & F.0 = A & F.1 = B & for n being Element of NAT st 1 < n holds
  F.n = C
proof
  let A,B,C be Subset of X;
  defpred P0[Element of NAT,set] means ($1 = 0 implies $2 = A) & ($1 = 1
  implies $2 = B) & (1 < $1 implies $2 = C);
  defpred P[set] means ex n being Element of NAT st ex X being set st ($1 = [n
  ,X] & P0[n,X]);
  ex GRAF being set st for x being set holds x in GRAF iff x in [:NAT,bool
  X:] & P[x] from XBOOLE_0:sch 1;
  then consider GRAF being set such that
A1: for x being set holds x in GRAF iff x in [:NAT,bool X:] & P[x];
A2: GRAF is Function-like
  proof
    let x,y1,y2 be set;
    assume that
A3: [x,y1] in GRAF and
A4: [x,y2] in GRAF;
A5: P[[x,y1]] by A1,A3;
A6: [x,y2] in [:NAT,bool X:] by A1,A4;
A7: P[[x,y2]] by A1,A4;
    reconsider x as Element of NAT by A6,ZFMISC_1:106;
    per cases by NAT_1:26;
    suppose
A8:   x = 0;
      then y1 = A by A5,ZFMISC_1:33;
      hence thesis by A7,A8,ZFMISC_1:33;
    end;
    suppose
A9:   x = 1;
      then y1 = B by A5,ZFMISC_1:33;
      hence thesis by A7,A9,ZFMISC_1:33;
    end;
    suppose
A10:  x > 1;
      then y1 = C by A5,ZFMISC_1:33;
      hence thesis by A7,A10,ZFMISC_1:33;
    end;
  end;
A11: for x being set holds x in GRAF iff (x = [0,A] or x = [1,B] or ex n
  being Element of NAT st x = [n,C] & 1 < n )
  proof
    let x be set;
A12: (x = [0,A] or x = [1,B] or ex n being Element of NAT st x = [n,C] & 1
    < n ) implies x in GRAF
    proof
A13:  (ex n being Element of NAT st (x = [n,C] & 1 < n)) implies x in GRAF
      proof
        given n being Element of NAT such that
A14:    x = [n,C] and
A15:    1 < n;
        ( P0[n,C])& x in [:NAT,bool X:] by A14,A15,ZFMISC_1:106;
        hence thesis by A1,A14;
      end;
A16:  x = [1,B] implies x in GRAF
      proof
        assume
A17:    x = [1,B];
        then x in [:NAT,bool X:] by ZFMISC_1:106;
        hence thesis by A1,A17;
      end;
A18:  x = [0,A] implies x in GRAF
      proof
        assume
A19:    x = [0,A];
        then x in [:NAT,bool X:] by ZFMISC_1:106;
        hence thesis by A1,A19;
      end;
      assume
      x = [0,A] or x = [1,B] or ex n being Element of NAT st x = [n,C] & 1 < n;
      hence thesis by A18,A16,A13;
    end;
    x in GRAF implies (x = [0,A] or x = [1,B] or ex n being Element of NAT
    st x = [n,C] & 1 < n )
    proof
      assume x in GRAF;
      then
      ex n being Element of NAT st ex X0 being set st x = [n,X0] & P0[n,X0]
      by A1;
      hence thesis by NAT_1:26;
    end;
    hence thesis by A12;
  end;
  GRAF is Relation-like
  proof
    let p be set;
    assume p in GRAF;
    then p = [0,A] or p = [1,B] or ex n being Element of NAT st p = [n,C] & 1
    < n by A11;
    hence thesis;
  end;
  then reconsider F = GRAF as Function by A2;
A20: for x being set holds x in NAT iff ex y being set st [x,y] in F
  proof
    let x be set;
    thus x in NAT implies ex y being set st [x,y] in F
    proof
      assume x in NAT;
      then reconsider x as Element of NAT;
A21:  x = 1 implies ex y being set st [x,y] in F
      proof
        assume
A22:    x = 1;
        take B;
        thus thesis by A11,A22;
      end;
A23:  1 < x implies ex y being set st [x,y] in F
      proof
        assume
A24:    1 < x;
        take C;
        thus thesis by A11,A24;
      end;
      x = 0 implies ex y being set st [x,y] in F
      proof
        assume
A25:    x = 0;
        take A;
        thus thesis by A11,A25;
      end;
      hence thesis by A21,A23,NAT_1:26;
    end;
    given y being set such that
A26: [x,y] in F;
    [x,y] in [:NAT,bool X:] by A1,A26;
    hence thesis by ZFMISC_1:106;
  end;
  then
A27: dom F = NAT by RELAT_1:def 4;
A28: for y being set holds y in {A,B,C} iff ex x being set st x in dom F & y
  = F.x
  proof
    let y be set;
    thus y in {A,B,C} implies ex x being set st x in dom F & y = F.x
    proof
      assume
A29:  y in {A,B,C};
      per cases by A29,ENUMSET1:def 1;
      suppose
A30:    y = A;
        take 0;
        [0,y] in F by A11,A30;
        hence thesis by FUNCT_1:8;
      end;
      suppose
A31:    y = B;
        take 1;
        [1,y] in F by A11,A31;
        hence thesis by FUNCT_1:8;
      end;
      suppose
A32:    y = C;
        take 2;
        [2,y] in F by A11,A32;
        hence thesis by FUNCT_1:8;
      end;
    end;
    given x being set such that
A33: x in dom F and
A34: y = F.x;
    reconsider x as Element of NAT by A20,A33,RELAT_1:def 4;
    per cases by NAT_1:26;
    suppose
A35:  x = 0;
      [0,A] in F by A11;
      then A = F.x by A35,FUNCT_1:8;
      hence thesis by A34,ENUMSET1:def 1;
    end;
    suppose
A36:  x = 1;
      [1,B] in F by A11;
      then B = F.x by A36,FUNCT_1:8;
      hence thesis by A34,ENUMSET1:def 1;
    end;
    suppose
      1 < x;
      then [x,C] in F by A11;
      then y = C by A34,FUNCT_1:8;
      hence thesis by ENUMSET1:def 1;
    end;
  end;
  then
A37: rng F = {A,B,C} by FUNCT_1:def 5;
  rng F c= bool X
  proof
    let a be set;
    assume a in rng F;
    then a = A or a = B or a = C by A37,ENUMSET1:def 1;
    hence thesis;
  end;
  then reconsider F as Function of NAT,bool X by A27,FUNCT_2:4;
A38: for n being Element of NAT st 1 < n holds F.n = C
  proof
    let n be Element of NAT;
    assume 1 < n;
    then [n,C] in F by A11;
    hence thesis by FUNCT_1:8;
  end;
  take F;
  [0,A] in F & [1,B] in F by A11;
  hence thesis by A28,A38,FUNCT_1:8,def 5;
end;

theorem
  for A,B being Subset of X holds {A,B,{}} is N_Sub_set_fam of X
proof
  let A,B be Subset of X;
  {} is Subset of X by XBOOLE_1:2;
  then consider C being Subset of X such that
A1: C = {};
  ex F being Function of NAT,bool X st rng F = {A,B,C} & F.0 = A & F.1 = B
  & for n being Element of NAT st 1 < n holds F.n = C by Th38;
  hence thesis by A1,SUPINF_2:def 14;
end;

theorem Th40:
  for A,B being Subset of X ex F being Function of NAT,bool X st
  rng F = {A,B} & F.0 = A & for n being Element of NAT st 0 < n holds F.n = B
proof
  let A,B be Subset of X;
  consider F being Function of NAT,bool X such that
A1: rng F = {A,B,B} & F.0 = A &( F.1 = B & for n being Element of NAT st
  1 < n holds F.n = B ) by Th38;
  take F;
  {A,B,B} = {B,B,A} by ENUMSET1:100
    .= {A,B} by ENUMSET1:70;
  hence thesis by A1,NAT_1:26;
end;

theorem
  for A,B being Subset of X holds {A,B} is N_Sub_set_fam of X
proof
  let A,B be Subset of X;
  ex F being Function of NAT,bool X st (rng F = {A,B} & F.0 = A & for n
  being Element of NAT st 0 < n holds F.n = B) by Th40;
  hence thesis by SUPINF_2:def 14;
end;

theorem Th42:
  for S being N_Sub_set_fam of X holds X \ S is N_Sub_set_fam of X
proof
  let S be N_Sub_set_fam of X;
  consider F being Function of NAT,bool X such that
A1: S = rng F by SUPINF_2:def 14;
  defpred P[set,set] means ex V being Subset of X st V = F.$1 & $2 = V`;
A2: for n being set st n in NAT ex y being set st y in X\S & P[n,y]
  proof
    let n be set;
    assume n in NAT;
    then consider V being set such that
A3: V in S and
A4: V=F.n by A1,FUNCT_2:6;
    reconsider V as Subset of X by A3;
    take V`;
    V`` in S by A3;
    hence thesis by A4,SETFAM_1:def 8;
  end;
A5: ex G being Function of NAT,X \ S st for n being set st n in NAT holds P[
  n,G.n] from FUNCT_2:sch 1(A2);
A6: NAT = dom F by FUNCT_2:def 1;
  ex G being Function of NAT,bool X st X \ S = rng G
  proof
    consider G being Function of NAT,X \ S such that
A7: for n being set st n in NAT holds ex V being Subset of X st V = F
    .n & G.n = V` by A5;
A8: dom G = NAT by FUNCT_2:def 1;
A9: X \ S c= rng G
    proof
      let x be set;
      assume
A10:  x in X \ S;
      then reconsider B = x as Subset of X;
      B` in S by A10,SETFAM_1:def 8;
      then consider n being set such that
A11:  n in NAT and
A12:  F.n = B` by A1,A6,FUNCT_1:def 5;
      ex V being Subset of X st V = F.n & G.n = V` by A7,A11;
      hence thesis by A8,A11,A12,FUNCT_1:def 5;
    end;
    reconsider G as Function of NAT,bool X by FUNCT_2:9;
    take G;
    thus thesis by A9,XBOOLE_0:def 10;
  end;
  hence thesis by SUPINF_2:def 14;
end;

