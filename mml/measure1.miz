theorem Th60:
  for S being SigmaField of X, M being sigma_Measure of S holds M
  is Measure of S
proof
  let S be SigmaField of X, M be sigma_Measure of S;
  for A,B being Element of S holds (A misses B implies M.(A \/ B) = M.A + M.B)
  proof
    set n2 = 1 + 1;
    let A,B be Element of S;
    assume
A1: A misses B;
A2: A in S & B in S;
    reconsider A,B as Subset of X;
    {} is Subset of X by XBOOLE_1:2;
    then consider F being Function of NAT,bool X such that
A3: rng F = {A,B,{}} and
A4: F.0 = A & F.1 = B and
A5: for n being Element of NAT st 1 < n holds F.n = {} by Th38;
    {} in S by PROB_1:10;
    then for x being set holds x in {A,B,{}} implies x in S by A2,
    ENUMSET1:
    def 1;
    then {A,B,{}} c= S by TARSKI:def 3;
    then reconsider F as Function of NAT,S by A3,FUNCT_2:8;
A6: for n,m being Element of NAT st n <> m holds F.n misses F.m
    proof
      let n,m be Element of NAT;
A7:   n = 0 or n = 1 or 1 < n by NAT_1:26;
A8:   m = 0 or m = 1 or 1 < m by NAT_1:26;
A9:   1 < n & m = 1 implies F.n misses F.m
      proof
        assume that
A10:    1 < n and
        m = 1;
        F.n = {} by A5,A10;
        then F.n /\ F.m = {};
        hence thesis by XBOOLE_0:def 7;
      end;
A11:  1 < n & m = 0 implies F.n misses F.m
      proof
        assume that
A12:    1 < n and
        m = 0;
        F.n = {} by A5,A12;
        then F.n /\ F.m = {};
        hence thesis by XBOOLE_0:def 7;
      end;
A13:  1 < n & 1 < m implies F.n misses F.m
      proof
        assume that
A14:    1 < n and
        1 < m;
        F.n = {} by A5,A14;
        then F.n /\ F.m = {};
        hence thesis by XBOOLE_0:def 7;
      end;
A15:  n = 1 & 1 < m implies F.n misses F.m
      proof
        assume that
        n = 1 and
A16:    1 < m;
        F.m = {} by A5,A16;
        then F.n /\ F.m = {};
        hence thesis by XBOOLE_0:def 7;
      end;
A17:  n = 0 & 1 < m implies F.n misses F.m
      proof
        assume that
        n = 0 and
A18:    1 < m;
        F.m = {} by A5,A18;
        then F.n /\ F.m = {};
        hence thesis by XBOOLE_0:def 7;
      end;
      assume n <> m;
      hence thesis by A1,A4,A7,A8,A17,A15,A11,A9,A13;
    end;
    for m,n being set st m <> n holds F.m misses F.n
    proof
      let m,n be set;
      assume
A19:  m <> n;
      per cases;
      suppose
        m in NAT & n in NAT;
        hence thesis by A6,A19;
      end;
      suppose
        not m in NAT or not n in NAT;
        then not m in dom F or not n in dom F;
        then F.m = {} or F.n = {} by FUNCT_1:def 4;
        hence thesis by XBOOLE_1:65;
      end;
    end;
    then reconsider F as disjoint_valued Function of NAT, S by PROB_2:def 3;
    union {A,B,{}} = union {A,B} by Th1;
    then
A20: union rng F = A \/ B by A3,ZFMISC_1:93;
A21: for r being Element of NAT holds (M*F).0 = M.A & (M*F).1 = M.B & (1+1
    <= r implies (M*F).r = 0.)
    proof
      let r be Element of NAT;
A22:  for r being Element of NAT holds (M*F).r = M.(F.r)
      proof
        let r be Element of NAT;
        dom (M*F) = NAT by FUNCT_2:def 1;
        hence thesis by FUNCT_1:22;
      end;
      1 + 1 <= r implies (M*F).r = 0.
      proof
        assume 1 + 1 <= r;
        then 1 < r by NAT_1:13;
        then F.r = {} by A5;
        then (M*F).r = M.{} by A22;
        hence thesis by VALUED_0:def 19;
      end;
      hence thesis by A4,A22;
    end;
    set H = M*F;
A23: 0 + 1 = 0 + 1;
    set y1 = Ser H.1;
A24: H is nonnegative by Th54;
    reconsider A,B as Element of S;
    Ser H.n2 = y1 + H.n2 by SUPINF_2:63;
    then Ser H.n2 = y1 + 0.by A21
      .= Ser H.1 by XXREAL_3:4
      .= Ser H.0 + H.1 by A23,SUPINF_2:63
      .= M.A + M.B by A21,SUPINF_2:63;
    then SUM(M*F) = M.A + M.B by A21,A24,SUPINF_2:67;
    hence thesis by A20,Def11;
  end;
  hence thesis by Def5;
end;

theorem
  for S being SigmaField of X, M being sigma_Measure of S, A,B being
  Element of S st A misses B holds M.(A \/ B) = M.A + M.B
proof
  let S be SigmaField of X, M be sigma_Measure of S, A,B be Element of S;
  assume
A1: A misses B;
  M is Measure of S by Th60;
  hence thesis by A1,Def5;
end;

theorem Th62:
  for S being SigmaField of X, M being sigma_Measure of S, A,B
  being Element of S st A c= B holds M.A <= M.B
proof
  let S be SigmaField of X, M be sigma_Measure of S, A,B be Element of S;
  assume
A1: A c= B;
  M is Measure of S by Th60;
  hence thesis by A1,Th25;
end;

theorem
  for S being SigmaField of X, M being sigma_Measure of S, A,B being
  Element of S st A c= B & M.A < +infty holds M.(B \ A) = M.B - M.A
proof
  let S be SigmaField of X, M be sigma_Measure of S, A,B be Element of S;
  assume
A1: A c= B & M.A < +infty;
  M is Measure of S by Th60;
  hence thesis by A1,Th26;
end;

theorem Th64:
  for S being SigmaField of X, M being sigma_Measure of S, A,B
  being Element of S holds M.(A \/ B) <= M.A + M.B
proof
  let S be SigmaField of X, M be sigma_Measure of S, A,B be Element of S;
  M is Measure of S by Th60;
  hence thesis by Th27;
end;

