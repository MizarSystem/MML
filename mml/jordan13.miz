    assume m <> 1;
then A608:  1 < m by A395,REAL_1:def 5;
A609: for n st 1 <= n & n <= m-'1 holds not f/.n in L~g
     proof let n such that
  A610: 1 <= n and
  A611: n <= m-'1;
  A612: n <= len f by A400,A611,XXREAL_0:2;

    set p = f/.n;
    assume p in L~g;
    then consider j such that
  A613: m-'1+1 <= j and
  A614: j+1 <= len f and
  A615: p in LSeg(f,j) by A400,JORDAN9:9;

  A616: n < m-'1+1 by A611,NAT_1:38;
  then A617: n < j by A613,XXREAL_0:2;

  A618: m-'1+1=m by A395,BINARITH:53;
  then A619: 1 < j by A608,A613,XXREAL_0:2;
  A620: j+1 <= k by A92,A614;
  A621: j < k by A393,A614,NAT_1:38;
  A622: 2 <= len G & 2 <= width G by A2,NAT_1:37;

  A623: p in Values G by A394,A610,A612,JORDAN9:8;
    per cases by A394,A614,A615,A619,A622,A623,JORDAN9:25;
    suppose
    A624: p = f/.j;
          n <= len(F.j) by A92,A617;
    then A625: n in dom(F.j) by A610,FINSEQ_3:27;
    A626: n <> len(F.j) by A92,A613,A616;
          (F.j)/.n = (F.n)/.n by A561,A610,A617
              .= p by A393,A561,A610,A612
              .= (F.j)/.j by A561,A619,A621,A624
              .= (F.j)/.len(F.j) by A92;
      hence contradiction by A389,A619,A621,A625,A626;

    end;
    suppose
    A627: p = f/.(j+1);
          now
        per cases by A620,REAL_1:def 5;
        suppose
        A628: j+1 = k;
              n <= len(F.m) by A92,A616,A618;
        then A629: n in dom(F.m) by A610,FINSEQ_3:27;
        A630: n <> len(F.m) by A92,A616,A618;
              (F.m)/.n = (F.n)/.n by A561,A610,A616,A618
                  .= p by A393,A561,A610,A612
                  .= (F.m)/.m by A392,A393,A395,A561,A627,A628
                  .= (F.m)/.len(F.m) by A92;
          hence contradiction by A389,A393,A395,A396,A629,A630;
        end;
        suppose
        A631: j+1 < k;
          set l = j+1;
        A632: 1 <= l by NAT_1:29;
          A633: n < n+1 & n+1 < l by A617,XREAL_1:8,XREAL_1:31;
        then A634: n < l by XXREAL_0:2;
          then n <= len(F.l) by A92;
        then A635: n in dom(F.l) by A610,FINSEQ_3:27;
        A636: n <> len(F.l) by A92,A633;
              (F.l)/.n = (F.n)/.n by A561,A610,A634
                  .= p by A393,A561,A610,A612
                  .= (F.l)/.l by A561,A627,A631,A632
                  .= (F.l)/.len(F.l) by A92;
          hence contradiction by A389,A631,A632,A635,A636;
      end;
      end;
      hence contradiction;
     end;
     end;
A637: for n st 1 <= n holds n-'1+2 = n+1
proof
  let n;
  assume 1 <= n;
  hence n-'1+2 = n+2-'1 by BINARITH:56
            .= n+1+1 - 1 by BINARITH:55
            .= n+1;
end;

    C meets LeftComp Rev g
  proof
   set rg = Rev g;

  A638: rg is_sequence_on G by A404,JORDAN9:7;

  A639: 1+1 <= len rg by A557,FINSEQ_5:def 3;
    then 1+1-'1 <= len rg-'1 by JORDAN3:5;
  then A640: 1 <= len rg -'1 by BINARITH:39;
        1 < len rg by A639,NAT_1:38;
  then A641: len rg -'1+1 = len rg by BINARITH:53;

    set p = rg/.1, q = rg/.2;
    set a = f/.(m-'1);
    set l = (m-'1)+(len g-'1);

  A642: p = f/.m by A392,A403,FINSEQ_5:68;
  A643: m-'1+1 = m by A395,BINARITH:53;
  then A644: 1 <= m-'1 by A608,NAT_1:38;

        1+1-'1 <= len g-'1 by A557,JORDAN3:5;
  then A645: 1 <= len g-'1 by BINARITH:39;
B646:   1+1 - 1 <= len g - 1 by A557,XREAL_1:11;
    A647: 1 - 1 < m - 1 by A608,XREAL_1:11;

       A648: l = m+(len g -'1)-'1 by A395,BINARITH:56
       .= (len g -'1)+m - 1 by A645,BINARITH:55
       .= (len g - 1)+m - 1 by B646,BINARITH:def 3
       .= ((k - (m - 1)) - 1)+m - 1 by A393,A401,A647,BINARITH:def 3
       .= k - 1;
  then A649: k = l+1;
  then A650: l < k by XREAL_1:31;
        len g-'1 <= l by NAT_1:29;
  then A651: 1 <= l by A645,XXREAL_0:2;
        len g-'1 <= len g by JORDAN3:7;
  then A652: len g-'1 in dom g by A645,FINSEQ_3:27;
        1 <= len g by A558,SPPOL_1:5;
  then A653: len g-'1+2 = len g+1 by A637;
  then A654: q = g/.(len g -'1) by A652,FINSEQ_5:69
       .= f/.l by A652,FINSEQ_5:30;
  A655: p = f/.(l+1) by A393,A403,A648,FINSEQ_5:68;

  A656: left_cell(f,m-'1,G) meets C by A246,A395,A643,A644;
  A657: left_cell(f,l,G) meets C by A246,A393,A649,A651;

        not a in L~g by A609,A644;
  then A658: not a in L~rg by SPPOL_2:22;

  A659: m-'1+2 = m+1 by A395,A637;

  A660: m-'1 <= l by NAT_1:29;
    then m-'1 <= len(F.l) by A92;
  then A661: m-'1 in dom(F.l) by A644,FINSEQ_3:27;
        (m-'1)+1 <= l by A645,XREAL_1:8;
    then m-'1 < l by NAT_1:38;
  then A662: m-'1 <> len(F.l) by A92;

    consider p1,p2,q1,q2 being Element of NAT such that
  A663: [p1,p2] in Indices G and
  A664: p = G*(p1,p2) and
  A665: [q1,q2] in Indices G and
  A666: q = G*(q1,q2) and
  A667: p1 = q1 & p2+1 = q2 or p1+1 = q1 & p2 = q2 or
        p1 = q1+1 & p2 = q2 or p1 = q1 & p2 = q2+1 by A638,A639,JORDAN8:6;

  A668: 1 <= p1 & p1 <= len G & 1 <= p2 & p2 <= width G by A663,GOBOARD5:1;

    per cases by A667;
    suppose ::A
    A669: p1 = q1 & p2+1 = q2;
      consider a1,a2,p'1,p'2 being Element of NAT such that
    A670: [a1,a2] in Indices G and
    A671: a = G*(a1,a2) and
    A672: [p'1,p'2] in Indices G and
    A673: p = G*(p'1,p'2) and
    A674: a1 = p'1 & a2+1 = p'2 or a1+1 = p'1 & a2 = p'2 or
          a1 = p'1+1 & a2 = p'2 or a1 = p'1 & a2 = p'2+1
            by A394,A395,A642,A643,A644,JORDAN8:6;
    A675: 1 <= a1 & a1 <= len G & 1 <= a2 & a2 <= width G by A670,GOBOARD5:1;

      thus thesis
      proof
        per cases by A674;
        suppose ::I
              a1 = p'1 & a2+1 = p'2;
        then A676: a1 = p1 & a2+1 = p2 by A663,A664,A672,A673,GOBOARD1:21;
        A677: F.m is_sequence_on G by A246;
        A678: m-'1+1 <= len (F.m) by A92,A643;
        A679: m-'1 <= m by A643,NAT_1:29;
        A680: F.k|(m+1)=F.(m+1) by A322,A393,A399;
          A681: p2+1 > a2+1 & a2+1 > a2 by A676,NAT_1:38;
        A682: f/.(m-'1) = (F.(m-'1))/.(m-'1) by A393,A400,A561,A644
                     .= (F.m)/.(m-'1) by A561,A644,A679;
        A683: f/.(m-'1+1) = (F.m)/.m by A393,A395,A561,A643;
              left_cell(f,l,G) = cell(G,p1,p2)
                 by A393,A394,A648,A651,A654,A655,A663,A664,A665,A666,A669,
GOBRD13:28
            .= front_right_cell(F.m,m-'1,G) by A642,A643,A644,A663,A664,A670
,A671,A676,A677,A678,A682,A683,GOBRD13:36;
          then F.(m+1) turns_right m-'1,G by A323,A608,A657;
          then f turns_right m-'1,G by A399,A644,A659,A680,GOBRD13:44;
        then A684: f/.(m+1) = G*(p1+1,p2) & [p1+1,p2] in Indices G
            by A642,A643,A659,A663,A664,A670,A671,A681,GOBRD13:def 6;
        then A685: 1 <= p1+1 & p1+1 <= len G by GOBOARD5:1;
          set rc = left_cell(rg,len rg-'1,G)\L~rg;
        A686: rc c= LeftComp rg by A638,A640,A641,JORDAN9:29;
        A687: p2-'1 = a2 & p1-'1+1 = p1 by A668,A676,BINARITH:53,BINARITH:39;
        A688: 2 in dom g by A557,FINSEQ_3:27;
               len rg-'1+2 = len g +1 by A653,FINSEQ_5:def 3;
        then A689: rg/.(len rg -' 1) = g/.2 by A688,FINSEQ_5:69
                           .= f/.(m+1) by A659,A688,FINSEQ_5:30;
            p = g/.1 by A392,A402,A403,FINSEQ_5:68
              .= rg/.len g by FINSEQ_5:68
              .= rg/.len rg by FINSEQ_5:def 3;
          then left_cell(rg,len rg-'1,G) = cell(G,p1,a2)
            by A638,A640,A641,A663,A664,A684,A687,A689,GOBRD13:26;
          then a in left_cell(rg,len rg-'1,G)
            by A668,A671,A675,A676,A685,JORDAN9:22;
          then A690: a in rc by A658,XBOOLE_0:def 4;
        A691: L~rg c= L~f by A576,SPPOL_2:22;
              LeftComp rg is_a_component_of (L~rg)` by GOBOARD9:def 1;
          hence C meets LeftComp Rev g by A395,A578,A643,A644,A658,A686,A690
,A691;

        end;
        suppose ::II
          A692: a1+1 = p'1 & a2 = p'2;
        then A693: a1+1 = p1 & a2 = p2 by A663,A664,A672,A673,GOBOARD1:21;
        then q1-'1 = a1 by A669,BINARITH:39;
          then right_cell(f,l,G) = cell(G,a1,a2)
                 by A393,A394,A648,A651,A654,A655,A663,A664,A665,A666,A669,A693
,GOBRD13:29
            .= left_cell(f,m-'1,G)
                 by A394,A395,A642,A643,A644,A670,A671,A672,A673,A692,
GOBRD13:24;
          hence C meets LeftComp Rev g by A246,A393,A649,A651,A656;

        end;
        suppose ::III
              a1 = p'1+1 & a2 = p'2;
        then a1 = p1+1 & a2 = p2 by A663,A664,A672,A673,GOBOARD1:21;
          then right_cell(f,m-'1,G) = cell(G,p1,p2)
                 by A394,A395,A642,A643,A644,A663,A664,A670,A671,GOBRD13:27
            .= left_cell(f,l,G)
