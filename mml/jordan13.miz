        end;
        suppose n = k+1 & m <= k;
         hence n = m by A373,A377,A378,A381,A382;
        end;
        suppose n <= k & m = k+1;
         hence n = m by A373,A377,A378,A381,A382;
        end;
        suppose n = k+1 & m = k+1;
