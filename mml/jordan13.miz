A560: for j,k st 1 <= j & j <= k holds (F.k)/.j = (F.j)/.j
  proof
    let j,k;
    assume that
  A561: 1 <= j and
  A562: j <= k;
  A563: (F.k)|j = F.j by A321,A562;
           j <= len(F.k) by A92,A562;
  then len(F.k|j) = j by FINSEQ_1:80;
      then j in dom((F.k)|j) by A561,FINSEQ_3:27;
      hence (F.k)/.j = (F.j)/.j by A563,FINSEQ_4:85;
  end;
    reconsider Lg' = (L~g)` as Subset of TOP-REAL 2;
A564:   C c= Lg'
    proof
     let c be set;
     assume that
A565:   c in C and
A566:   not c in Lg';
     reconsider c as Point of TOP-REAL 2 by A565;
          c in L~g by A566,SUBSET_1:50;
     then consider i such that
A567:   1 <= i & i+1 <= len g and
A568:   c in LSeg(g/.i,g/.(i+1)) by SPPOL_2:14;
          i in dom g & i+1 in dom g by A567,GOBOARD2:3;
then A569:   g/.i = f/.(i+(m-'1)) & g/.(i+1) = f/.(i+1+(m-'1)) by FINSEQ_5:30;
A570:   1 <= i+(m-'1) by A567,NAT_1:12;
    i+1+(m-'1) = i+(m-'1)+1;
     then A571: i+(m-'1)+1 <= (len g)+(m-'1) by A567,XREAL_1:8;
      then c in LSeg(f,i+(m-'1)) by A400,A568,A569,A570,TOPREAL1:def 5;
     then c in right_cell(f,i+(m-'1),G) /\ left_cell(f,i+(m-'1),G)
      by A393,A400,A570,A571,GOBRD13:30;
     then c in right_cell(f,i+(m-'1),G) by XBOOLE_0:def 3;
      then right_cell(f,i+(m-'1),G) meets C by A565,XBOOLE_0:3;
     hence contradiction by A245,A400,A570,A571;
    end;
A572:   TOP-REAL 2 = TopSpaceMetr (Euclid 2) by EUCLID:def 8;
         L~g is closed by SPPOL_1:49;
    then (L~g)` is open by TOPS_1:29;
then A573:  (L~g)` = Int (L~g)` by TOPS_1:55;
A574:  L~g c= L~f by JORDAN3:75;
A575:  LeftComp g is_a_component_of (L~g)` &
       RightComp g is_a_component_of (L~g)` by GOBOARD9:def 1,def 2;
A576: for h being standard non constant special_circular_sequence st L~h c= L~f
    for Comp being Subset of TOP-REAL 2 st Comp is_a_component_of (L~h)`
    for n st 1 <= n & n+1 <= len f & f/.n in Comp & not f/.n in L~h
      holds
     C meets Comp
  proof
    let h be standard non constant special_circular_sequence such that
  A577: L~h c= L~f;
    let Comp be Subset of TOP-REAL 2 such that
  A578: Comp is_a_component_of (L~h)`;
    let n such that
  A579: 1 <= n & n+1 <= len f and
  A580: f/.n in Comp and
  A581: not f/.n in L~h;

    set rc=left_cell(f,n,G)\L~h;
    reconsider rc as Subset of TOP-REAL 2;
  A582: rc c= left_cell(f,n,G) by XBOOLE_1:36;
  A583: rc = left_cell(f,n,G) /\ (L~h)` by SUBSET_1:32;

  A584: rc meets C
  proof
         left_cell(f,n,G) meets C by A245,A579;
    then consider p being set such that
  A585: p in left_cell(f,n,G) & p in C by XBOOLE_0:3;
    reconsider p as Element of TOP-REAL 2 by A585;
         now take p;
         now
     assume p in L~h;
      then consider j such that
    A586: 1 <= j and
    A587: j+1 <= len f and
    A588: p in LSeg(f,j) by A577,SPPOL_2:13;
           p in right_cell(f,j,G) /\ left_cell(f,j,G) by A393,A586,A587,A588,
GOBRD13:30;
      then A589: p in right_cell(f,j,G) by XBOOLE_0:def 3;
           right_cell(f,j,G) misses C by A245,A586,A587;
      hence contradiction by A585,A589,XBOOLE_0:3;
    end;
    hence p in rc by A585,XBOOLE_0:def 4;
    thus p in C by A585;
    end; hence thesis by XBOOLE_0:3;
  end;
  A590: Int left_cell(f,n,G) is connected by A393,A579,JORDAN9:12;
         Int left_cell(f,n,G) misses L~f by A393,A579,JORDAN9:17;
    then Int left_cell(f,n,G) misses L~h by A577,XBOOLE_1:63;
  then A591: Int left_cell(f,n,G) c= (L~h)` by SUBSET_1:43;
         Int left_cell(f,n,G) c= left_cell(f,n,G) by TOPS_1:44;
  then A592: Int left_cell(f,n,G) c= rc by A583,A591,XBOOLE_1:19;
         rc c= Cl Int left_cell(f,n,G) by A393,A579,A582,JORDAN9:13;
  then A593: rc is connected by A590,A592,CONNSP_1:19;
         f/.n in left_cell(f,n,G) by A393,A579,JORDAN9:10;
    then f/.n in rc by A581,XBOOLE_0:def 4;
  then A594: rc meets Comp by A580,XBOOLE_0:3;
         rc c= (L~h)` by A583,XBOOLE_1:17;
    then rc c= Comp by A578,A593,A594,GOBOARD9:6;
    hence C meets Comp by A584,XBOOLE_1:63;
  end;

A595:
   C meets LeftComp g
   proof
         left_cell(f,m,G) meets C by A245,A394,A398;
    then consider p being set such that
A596:  p in left_cell(f,m,G) & p in C by XBOOLE_0:3;
    reconsider p as Element of TOP-REAL 2 by A596;
         now take p;
    thus p in C by A596;
    reconsider u = p as Element of Euclid 2 by TOPREAL3:13;
    consider r being real number such that
A597:   r > 0 and
A598:   Ball(u,r) c= (L~g)` by A564,A573,A596,GOBOARD6:8;
    reconsider r as Real by XREAL_0:def 1;
A599:   p in Ball(u,r) by A597,TBSP_1:16;
    reconsider B = Ball(u,r) as non empty Subset of TOP-REAL 2
       by A572,A597,TBSP_1:16,TOPMETR:16;
A600:   B is connected by SPRECT_3:17;
         left_cell(g,1,G) c= left_cell(g,1) by A403,A557,GOBRD13:34;
then A601:   Int left_cell(g,1,G) c= Int left_cell(g,1) by TOPS_1:48;
         Int left_cell(g,1) c= LeftComp g by A557,GOBOARD9:24;
    then Int left_cell(g,1,G) c= LeftComp g by A601,XBOOLE_1:1;
    then Int left_cell(f,m-'1+1,G) c= LeftComp g by A393,A399,A557,GOBRD13:33;
    then A602:  Int left_cell(f,m,G) c= LeftComp g by A394,BINARITH:53;
A603:   left_cell(f,m,G) = Cl Int left_cell(f,m,G) by A394,A398,A442;
         B is open by GOBOARD6:6;
then A604:   Int left_cell(f,m,G) meets B by A596,A599,A603,TOPS_1:39;
A605:   p in B by A597,TBSP_1:16;
         B c= LeftComp g by A575,A598,A600,A602,A604,GOBOARD9:6;
    hence p in LeftComp g by A605;
    end; hence thesis by XBOOLE_0:3;
   end;

      m = 1
