        end;
        end;
        hence contradiction;
       end;
       suppose
A553:      i1 = i2 & j1 = j2+1 & i1' = i2'+1 & j1' = j2';
             now per cases by A512,A517,A519,A521,A522,A523,A525,A527,A529,A530
,A531,A553,GOBOARD7:23;
         suppose i1 = i2' & j2 = j1';
          hence contradiction by A428,A460,A510,A513,A519,A527,A553;
         end;
         suppose i1 = i2' & j2+1 = j1';
          hence contradiction by A436,A463,A510,A517,A527,A553;
         end;
         suppose i1 = i2'+1 & j2 = j1';
          hence contradiction by A405,A457,A461,A462,A511,A515,A519,A525,A553;
         end;
         suppose i1 = i2'+1 & j2+1 = j1';
          hence contradiction by A405,A457,A459,A462,A510,A514,A515,A517,A525
,A553;
        end;
        end;
        hence contradiction;
       end;
       suppose
A554:      i1 = i2 & j1 = j2+1 & i1' = i2' & j1' = j2'+1;
then A555:      i1 = i1' by A512,A517,A519,A521,A522,A523,A525,A527,A529,A530
,A531,GOBOARD7:21;
             now per cases by A512,A517,A519,A521,A522,A523,A525,A527,A529,A530
,A531,A554,GOBOARD7:24;
         suppose j2 = j2';
          hence contradiction by A428,A460,A510,A513,A519,A527,A554,A555;
