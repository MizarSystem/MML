            assume [i1',j1'] in Indices G & [i2',j2'] in Indices G &
              (F.k)/.len(F.k) = G*(i1',j1') & G*(i2+1,j2) = G*(i2',j2');
              then i2 = i1' & j2 = j1' & i2+1 = i2' & j2 = j2'
