         end;
         suppose i1 = i1' & j2+1 = j1';
          hence contradiction by A405,A457,A459,A462,A510,A514,A515,A517,A525
,A552;
         end;
         suppose i1 = i1'+1 & j2 = j1';
          hence contradiction by A428,A460,A510,A513,A519,A527,A552;
         end;
         suppose i1 = i1'+1 & j2+1 = j1';
