A807:  m+1 > 1 by A803,NAT_1:13;
A808:  front_right_cell(F.(m+1),m,G) = front_right_cell(f,m,G) &
       front_left_cell(F.(m+1),m,G) = front_left_cell(f,m,G)
        by A393,A803,A804,A805,GOBRD13:43;
A809:  F.(m+1+1) = f|(m+1+1) by A321,A392,A803;
   hereby assume front_right_cell(f,m,G) misses C &
          front_left_cell(f,m,G) misses C;
    then F.(m+1+1) turns_left m,G by A322,A806,A807,A808;
    hence f turns_left m,G by A803,A809,GOBRD13:45;
   end;
   hereby assume front_right_cell(f,m,G) misses C &
          front_left_cell(f,m,G) meets C;
    then F.(m+1+1) goes_straight m,G by A322,A806,A807,A808;
    hence f goes_straight m,G by A803,A809,GOBRD13:46;
   end;
   assume front_right_cell(f,m,G) meets C;
    then F.(m+1+1) turns_right m,G by A322,A806,A807,A808;
   hence f turns_right m,G by A803,A809,GOBRD13:44;
 end;
uniqueness
proof
  let f1,f2 be clockwise_oriented
     (standard non constant special_circular_sequence)
   such that
A810:   f1 is_sequence_on Gauge(C,n) and
A811: f1/.1 = Gauge(C,n)*(X-SpanStart(C,n),Y-SpanStart(C,n)) and
A812: f1/.2 = Gauge(C,n)*(X-SpanStart(C,n)-'1,Y-SpanStart(C,n)) and
A813: for k st 1 <= k & k+2 <= len f1 holds
      (front_right_cell(f1,k,Gauge(C,n)) misses C &
         front_left_cell(f1,k,Gauge(C,n)) misses C
       implies f1 turns_left k,Gauge(C,n)) &
      (front_right_cell(f1,k,Gauge(C,n)) misses C &
        front_left_cell(f1,k,Gauge(C,n)) meets C
       implies f1 goes_straight k,Gauge(C,n)) &
      (front_right_cell(f1,k,Gauge(C,n)) meets C
       implies f1 turns_right k,Gauge(C,n)) and
A814:   f2 is_sequence_on Gauge(C,n) and
A815: f2/.1 = Gauge(C,n)*(X-SpanStart(C,n),Y-SpanStart(C,n)) and
A816: f2/.2 = Gauge(C,n)*(X-SpanStart(C,n)-'1,Y-SpanStart(C,n)) and
A817: for k st 1 <= k & k+2 <= len f2 holds
      (front_right_cell(f2,k,Gauge(C,n)) misses C &
         front_left_cell(f2,k,Gauge(C,n)) misses C
       implies f2 turns_left k,Gauge(C,n)) &
      (front_right_cell(f2,k,Gauge(C,n)) misses C &
        front_left_cell(f2,k,Gauge(C,n)) meets C
       implies f2 goes_straight k,Gauge(C,n)) &
      (front_right_cell(f2,k,Gauge(C,n)) meets C
       implies f2 turns_right k,Gauge(C,n));
       defpred P[Element of NAT] means f1|$1 = f2|$1;
A818: P[0];
A819: for k st P[k] holds P[k+1]
     proof let k such that
A820:    f1|k = f2|k;
A821:    f1|1 = <*f1/.1 *> & f2|1 = <*f2/.1 *> by FINSEQ_5:23;
A822:    len f1 > 4 & len f2 > 4 by GOBOARD7:36;
      per cases by NAT_1:26;
      suppose k = 0;
       hence f1|(k+1) = f2|(k+1) by A811,A815,A821;
      end;
      suppose
A823:      k = 1;
            len f1 > 2 & len f2 > 2 by A822,XXREAL_0:2;
       then f1|2 = <*f1/.1,f1/.2*> & f2|2 = <*f2/.1,f2/.2*>
        by JORDAN8:1;
       hence f1|(k+1) = f2|(k+1) by A811,A812,A815,A816,A823;
      end;
      suppose
A824:      k > 1;
A825:      f1/.1 = f1/.len f1 & f2/.1 = f2/.len f2 by FINSEQ_6:def 1;
            now
        per cases;
        suppose
A826:       len f1 > k;
         set m = k-'1;
A827:       1 <= m by A824,BINARITH:67;
then A828:       m+1 = k by BINARITH:61;
A829:      now assume
A830:        len f2 <= k;
then A831:        f2 = f2|k by FINSEQ_1:79;
          then 1 in dom(f2|k) by FINSEQ_5:6;
then A832:        (f1|k)/.1 = f1/.1 by A820,FINSEQ_4:85;
               len f2 in dom(f2|k) by A831,FINSEQ_5:6;
then A833:        (f1|k)/.len f2 = f1/.len f2 by A820,FINSEQ_4:85;
               1 < len f2 & len f2 <= len f1 by A820,A822,A831,FINSEQ_5:18
,XXREAL_0:2;
          hence contradiction by A820,A825,A826,A830,A831,A832,A833,GOBOARD7:40
;
         end;
then A834:       k+1 <= len f2 by NAT_1:13;
A835:       k+1 <= len f1 by A826,NAT_1:13;
A836:       front_right_cell(f1,m,Gauge(C,n))
              = front_right_cell(f1|k,m,Gauge(C,n))
              by A810,A826,A827,A828,GOBRD13:43;
A837:       front_left_cell(f1,m,Gauge(C,n))
              = front_left_cell(f1|k,m,Gauge(C,n))
              by A810,A826,A827,A828,GOBRD13:43;
A838:       front_right_cell(f2,m,Gauge(C,n))
              = front_right_cell(f2|k,m,Gauge(C,n))
              by A814,A827,A828,A829,GOBRD13:43;
A839:       front_left_cell(f2,m,Gauge(C,n))
              = front_left_cell(f2|k,m,Gauge(C,n))
              by A814,A827,A828,A829,GOBRD13:43;
A840:      m+(1+1) = k+1 by A828;
              now per cases;
          suppose front_right_cell(f1,m,Gauge(C,n)) misses C &
             front_left_cell(f1,m,Gauge(C,n)) misses C;
           then f1 turns_left m,Gauge(C,n) & f2 turns_left m,Gauge(C,n)
            by A813,A817,A820,A827,A834,A835,A836,A837,A838,A839,A840;
          hence f1|(k+1) = f2|(k+1) by A814,A820,A824,A834,A835,GOBRD13:48;
         end;
         suppose front_right_cell(f1,m,Gauge(C,n)) misses C &
              front_left_cell(f1,m,Gauge(C,n)) meets C;
            then f1 goes_straight m,Gauge(C,n) & f2 goes_straight m,Gauge(C,n)
             by A813,A817,A820,A827,A834,A835,A836,A837,A838,A839,A840;
            hence f1|(k+1) = f2|(k+1) by A814,A820,A824,A834,A835,GOBRD13:49;
         end;
         suppose front_right_cell(f1,m,Gauge(C,n)) meets C;
           then f1 turns_right m,Gauge(C,n) & f2 turns_right m,Gauge(C,n)
             by A813,A817,A820,A827,A834,A835,A836,A838,A840;
           hence f1|(k+1) = f2|(k+1) by A814,A820,A824,A834,A835,GOBRD13:47;
         end;
         end;
         hence f1|(k+1) = f2|(k+1);
        end;
        suppose
A841:       k >= len f1;
then A842:       f1 = f1|k by FINSEQ_1:79;
         then 1 in dom(f1|k) by FINSEQ_5:6;
then A843:       (f2|k)/.1 = f2/.1 by A820,FINSEQ_4:85;
              len f1 in dom(f1|k) by A842,FINSEQ_5:6;
then A844:       (f2|k)/.len f1 = f2/.len f1 by A820,FINSEQ_4:85;
              1 < len f1 & len f1 <= len f2 by A820,A822,A842,FINSEQ_5:18
,XXREAL_0:2
;
         then len f2 = len f1 by A820,A825,A842,A843,A844,GOBOARD7:40;
         hence f1|(k+1) = f2|(k+1) by A820,A841,A842,FINSEQ_1:79;
        end;
        end;
       hence f1|(k+1) = f2|(k+1);
     end;
     end;
        for k holds P[k] from NAT_1:sch 1(A818,A819);
   hence f1 = f2 by JORDAN9:4;
 end;
end;

