FINSEQ_1:25;
A395: f is_sequence_on G by A245;
A396: 1 <= m & m <= len f by A391,FINSEQ_3:27;
then A397: m < len f by A392,REAL_1:def 5;
   then 1 < len f by A396,XXREAL_0:2;
then A398: len f >= 1+1 by NAT_1:13;
then A399: k >= 2 by A92;
  reconsider f as non constant special unfolded non empty
    FinSequence of TOP-REAL 2 by A370,A395,A398,JORDAN8:7,8;
  set g = f/^(m-'1);
A400: m+1 <= len f by A397,NAT_1:13;
