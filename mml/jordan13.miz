A387:  K[k] and
A388:  for l be Nat st K[l] holds k <= l from NAT_1:sch 5(A386);
  reconsider k as Element of NAT by ORDINAL1:def 13;
   consider w being Element of NAT such that
A389: w = k and
 k >= 1 and
A390: ex m st m in dom(F.w) & m <> len(F.w) &
       (F.w)/.m = (F.w)/.len(F.w) by A387;
  consider m such that
A391:  m in dom(F.k) and
A392:  m <> len(F.k) and
A393:  (F.k)/.m = (F.k)/.len(F.k) by A389,A390;
A394:  len(F.k) = k by A92;
