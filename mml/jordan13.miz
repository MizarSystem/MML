              end;
              suppose
                i1 = i19 & j2+1 = j19;
                hence
                contradiction by A710,A752,A756,A754,A817,A836,A833,A838,A819
,A871;
              end;
              suppose
                i1 = i19+1 & j2 = j19;
                hence contradiction by A738,A757,A817,A832,A840,A821,A871;
              end;
              suppose
                i1 = i19+1 & j2+1 = j19;
