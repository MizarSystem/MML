  take f;
  thus f is_sequence_on G by A245;
  thus f/.1 = G*(XS,YS) by A786;
  thus f/.2 = G*(XS-'1,YS) by A787;
   let m such that
A805:  1 <= m & m+2 <= len f;
        m+1 < m+2 by XREAL_1:8;
then A806:  m+1 <= len f by A805,XXREAL_0:2;
then A807:  f|(m+1) = F.(m+1) by A321,A394;
A808:  m = m+1-'1 by BINARITH:39;
A809:  m+1 > 1 by A805,NAT_1:13;
A810:  front_right_cell(F.(m+1),m,G) = front_right_cell(f,m,G) &
       front_left_cell(F.(m+1),m,G) = front_left_cell(f,m,G)
        by A395,A805,A806,A807,GOBRD13:43;
A811:  F.(m+1+1) = f|(m+1+1) by A321,A394,A805;
   hereby assume front_right_cell(f,m,G) misses C &
          front_left_cell(f,m,G) misses C;
    then F.(m+1+1) turns_left m,G by A322,A808,A809,A810;
    hence f turns_left m,G by A805,A811,GOBRD13:45;
   end;
   hereby assume front_right_cell(f,m,G) misses C &
          front_left_cell(f,m,G) meets C;
    then F.(m+1+1) goes_straight m,G by A322,A808,A809,A810;
    hence f goes_straight m,G by A805,A811,GOBRD13:46;
   end;
   assume front_right_cell(f,m,G) meets C;
    then F.(m+1+1) turns_right m,G by A322,A808,A809,A810;
   hence f turns_right m,G by A805,A811,GOBRD13:44;
 end;
uniqueness
proof
  let f1,f2 be clockwise_oriented
     (standard non constant special_circular_sequence)
   such that
A812:   f1 is_sequence_on Gauge(C,n) and
A813: f1/.1 = Gauge(C,n)*(X-SpanStart(C,n),Y-SpanStart(C,n)) and
A814: f1/.2 = Gauge(C,n)*(X-SpanStart(C,n)-'1,Y-SpanStart(C,n)) and
A815: for k st 1 <= k & k+2 <= len f1 holds
      (front_right_cell(f1,k,Gauge(C,n)) misses C &
         front_left_cell(f1,k,Gauge(C,n)) misses C
       implies f1 turns_left k,Gauge(C,n)) &
      (front_right_cell(f1,k,Gauge(C,n)) misses C &
        front_left_cell(f1,k,Gauge(C,n)) meets C
       implies f1 goes_straight k,Gauge(C,n)) &
      (front_right_cell(f1,k,Gauge(C,n)) meets C
       implies f1 turns_right k,Gauge(C,n)) and
A816:   f2 is_sequence_on Gauge(C,n) and
A817: f2/.1 = Gauge(C,n)*(X-SpanStart(C,n),Y-SpanStart(C,n)) and
A818: f2/.2 = Gauge(C,n)*(X-SpanStart(C,n)-'1,Y-SpanStart(C,n)) and
A819: for k st 1 <= k & k+2 <= len f2 holds
      (front_right_cell(f2,k,Gauge(C,n)) misses C &
         front_left_cell(f2,k,Gauge(C,n)) misses C
       implies f2 turns_left k,Gauge(C,n)) &
      (front_right_cell(f2,k,Gauge(C,n)) misses C &
        front_left_cell(f2,k,Gauge(C,n)) meets C
       implies f2 goes_straight k,Gauge(C,n)) &
      (front_right_cell(f2,k,Gauge(C,n)) meets C
       implies f2 turns_right k,Gauge(C,n));
       defpred P[Element of NAT] means f1|$1 = f2|$1;
A820: P[0];
A821: for k st P[k] holds P[k+1]
     proof let k such that
A822:    f1|k = f2|k;
A823:    f1|1 = <*f1/.1 *> & f2|1 = <*f2/.1 *> by FINSEQ_5:23;
A824:    len f1 > 4 & len f2 > 4 by GOBOARD7:36;
      per cases by NAT_1:26;
      suppose k = 0;
       hence f1|(k+1) = f2|(k+1) by A813,A817,A823;
      end;
      suppose
A825:      k = 1;
            len f1 > 2 & len f2 > 2 by A824,XXREAL_0:2;
       then f1|2 = <*f1/.1,f1/.2*> & f2|2 = <*f2/.1,f2/.2*>
        by JORDAN8:1;
       hence f1|(k+1) = f2|(k+1) by A813,A814,A817,A818,A825;
      end;
      suppose
A826:      k > 1;
A827:      f1/.1 = f1/.len f1 & f2/.1 = f2/.len f2 by FINSEQ_6:def 1;
            now
        per cases;
        suppose
A828:       len f1 > k;
         set m = k-'1;
A829:       1 <= m by A826,BINARITH:67;
then A830:       m+1 = k by BINARITH:61;
A831:      now assume
A832:        len f2 <= k;
then A833:        f2 = f2|k by FINSEQ_1:79;
          then 1 in dom(f2|k) by FINSEQ_5:6;
then A834:        (f1|k)/.1 = f1/.1 by A822,FINSEQ_4:85;
               len f2 in dom(f2|k) by A833,FINSEQ_5:6;
then A835:        (f1|k)/.len f2 = f1/.len f2 by A822,FINSEQ_4:85;
               1 < len f2 & len f2 <= len f1 by A822,A824,A833,FINSEQ_5:18
,XXREAL_0:2;
          hence contradiction by A822,A827,A828,A832,A833,A834,A835,GOBOARD7:40
;
         end;
then A836:       k+1 <= len f2 by NAT_1:13;
A837:       k+1 <= len f1 by A828,NAT_1:13;
A838:       front_right_cell(f1,m,Gauge(C,n))
              = front_right_cell(f1|k,m,Gauge(C,n))
              by A812,A828,A829,A830,GOBRD13:43;
A839:       front_left_cell(f1,m,Gauge(C,n))
              = front_left_cell(f1|k,m,Gauge(C,n))
              by A812,A828,A829,A830,GOBRD13:43;
A840:       front_right_cell(f2,m,Gauge(C,n))
              = front_right_cell(f2|k,m,Gauge(C,n))
              by A816,A829,A830,A831,GOBRD13:43;
A841:       front_left_cell(f2,m,Gauge(C,n))
              = front_left_cell(f2|k,m,Gauge(C,n))
              by A816,A829,A830,A831,GOBRD13:43;
A842:      m+(1+1) = k+1 by A830;
              now per cases;
          suppose front_right_cell(f1,m,Gauge(C,n)) misses C &
             front_left_cell(f1,m,Gauge(C,n)) misses C;
           then f1 turns_left m,Gauge(C,n) & f2 turns_left m,Gauge(C,n)
            by A815,A819,A822,A829,A836,A837,A838,A839,A840,A841,A842;
          hence f1|(k+1) = f2|(k+1) by A816,A822,A826,A836,A837,GOBRD13:48;
         end;
         suppose front_right_cell(f1,m,Gauge(C,n)) misses C &
              front_left_cell(f1,m,Gauge(C,n)) meets C;
            then f1 goes_straight m,Gauge(C,n) & f2 goes_straight m,Gauge(C,n)
             by A815,A819,A822,A829,A836,A837,A838,A839,A840,A841,A842;
            hence f1|(k+1) = f2|(k+1) by A816,A822,A826,A836,A837,GOBRD13:49;
         end;
         suppose front_right_cell(f1,m,Gauge(C,n)) meets C;
           then f1 turns_right m,Gauge(C,n) & f2 turns_right m,Gauge(C,n)
             by A815,A819,A822,A829,A836,A837,A838,A840,A842;
           hence f1|(k+1) = f2|(k+1) by A816,A822,A826,A836,A837,GOBRD13:47;
         end;
         end;
         hence f1|(k+1) = f2|(k+1);
        end;
        suppose
A843:       k >= len f1;
then A844:       f1 = f1|k by FINSEQ_1:79;
         then 1 in dom(f1|k) by FINSEQ_5:6;
then A845:       (f2|k)/.1 = f2/.1 by A822,FINSEQ_4:85;
              len f1 in dom(f1|k) by A844,FINSEQ_5:6;
then A846:       (f2|k)/.len f1 = f2/.len f1 by A822,FINSEQ_4:85;
              1 < len f1 & len f1 <= len f2 by A822,A824,A844,FINSEQ_5:18
,XXREAL_0:2
;
         then len f2 = len f1 by A822,A827,A844,A845,A846,GOBOARD7:40;
         hence f1|(k+1) = f2|(k+1) by A822,A843,A844,FINSEQ_1:79;
        end;
        end;
       hence f1|(k+1) = f2|(k+1);
     end;
     end;
        for k holds P[k] from NAT_1:sch 1(A820,A821);
   hence f1 = f2 by JORDAN9:4;
 end;
end;

