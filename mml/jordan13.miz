A387:  K[k] and
A388:  for l be Nat st K[l] holds k <= l from NAT_1:sch 5(A386);
  reconsider k as Element of NAT by ORDINAL1:def 13; 
   consider w being Element of NAT such that
A1000: w = k and
A1002: k >= 1 and
A1001: ex m st m in dom(F.w) & m <> len(F.w) &
       (F.w)/.m = (F.w)/.len(F.w) by A387;
  consider m such that
A389:  m in dom(F.k) and
A390:  m <> len(F.k) and
A391:  (F.k)/.m = (F.k)/.len(F.k) by A1001,A1000;
A392:  len(F.k) = k by A92;
  then reconsider f = F.k as non empty FinSequence of TOP-REAL 2 by A387,
FINSEQ_1:25;
A393: f is_sequence_on G by A245;
A394: 1 <= m & m <= len f by A389,FINSEQ_3:27;
then A395: m < len f by A390,REAL_1:def 5;
   then 1 < len f by A394,XXREAL_0:2;
then A396: len f >= 1+1 by NAT_1:13;
then A397: k >= 2 by A92;
  reconsider f as non constant special unfolded non empty
    FinSequence of TOP-REAL 2 by A370,A393,A396,JORDAN8:7,8;
  set g = f/^(m-'1);
A398: m+1 <= len f by A395,NAT_1:13;
