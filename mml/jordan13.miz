         end;
         suppose i1 = i1' & j2+1 = j1';
          hence contradiction by A406,A458,A460,A463,A511,A515,A516,A518,A526
,A553;
         end;
         suppose i1 = i1'+1 & j2 = j1';
          hence contradiction by A429,A461,A511,A514,A520,A528,A553;
         end;
         suppose i1 = i1'+1 & j2+1 = j1';
          hence contradiction by A437,A464,A511,A518,A528,A553;
        end;
        end;
        hence contradiction;
       end;
       suppose
A554:      i1 = i2 & j1 = j2+1 & i1' = i2'+1 & j1' = j2';
            now per cases by A513,A518,A520,A522,A523,A524,A526,A528,A530,A531
,A532,A554,GOBOARD7:23;
         suppose i1 = i2' & j2 = j1';
          hence contradiction by A429,A461,A511,A514,A520,A528,A554;
         end;
         suppose i1 = i2' & j2+1 = j1';
          hence contradiction by A437,A464,A511,A518,A528,A554;
         end;
         suppose i1 = i2'+1 & j2 = j1';
          hence contradiction by A406,A458,A462,A463,A512,A516,A520,A526,A554;
         end;
         suppose i1 = i2'+1 & j2+1 = j1';
          hence contradiction by A406,A458,A460,A463,A511,A515,A516,A518,A526
,A554;
        end;
        end;
        hence contradiction;
       end;
       suppose
A555:      i1 = i2 & j1 = j2+1 & i1' = i2' & j1' = j2'+1;
then A556:      i1 = i1' by A513,A518,A520,A522,A523,A524,A526,A528,A530,A531
,A532,GOBOARD7:21;
            now per cases by A513,A518,A520,A522,A523,A524,A526,A528,A530,A531
,A532,A555,GOBOARD7:24;
         suppose j2 = j2';
          hence contradiction by A429,A461,A511,A514,A520,A528,A555,A556;
         end;
         suppose j2 = j2'+1;
          hence contradiction by A406,A458,A462,A463,A512,A516,A520,A526,A555
,A556;
         end;
         suppose j2+1 = j2';
          hence contradiction by A437,A464,A511,A518,A528,A555,A556;
        end;
        end;
        hence contradiction;
      end;
      end;
     hence contradiction;
   end;
   end;
