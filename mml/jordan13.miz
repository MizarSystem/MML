then A606:  1 < m by A394,REAL_1:def 5;
A607: for n st 1 <= n & n <= m-'1 holds not f/.n in L~g
     proof let n such that
  A608: 1 <= n and
  A609: n <= m-'1;
  A610: n <= len f by A399,A609,XXREAL_0:2;

    set p = f/.n;
    assume p in L~g;
    then consider j such that
  A611: m-'1+1 <= j and
  A612: j+1 <= len f and
  A613: p in LSeg(f,j) by A399,JORDAN9:9;

  A614: n < m-'1+1 by A609,NAT_1:13;
  then A615: n < j by A611,XXREAL_0:2;

  A616: m-'1+1=m by A394,BINARITH:53;
  then A617: 1 < j by A606,A611,XXREAL_0:2;
  A618: j+1 <= k by A92,A612;
  A619: j < k by A392,A612,NAT_1:13;
  A620: 2 <= len G & 2 <= width G by A2,NAT_1:12;

  A621: p in Values G by A393,A608,A610,JORDAN9:8;
    per cases by A393,A612,A613,A617,A620,A621,JORDAN9:25;
    suppose
    A622: p = f/.j;
           n <= len(F.j) by A92,A615;
    then A623: n in dom(F.j) by A608,FINSEQ_3:27;
    A624: n <> len(F.j) by A92,A611,A614;
           (F.j)/.n = (F.n)/.n by A560,A608,A615
              .= p by A392,A560,A608,A610
              .= (F.j)/.j by A560,A617,A619,A622
              .= (F.j)/.len(F.j) by A92;
      hence contradiction by A388,A617,A619,A623,A624;

    end;
    suppose
    A625: p = f/.(j+1);
           now
        per cases by A618,REAL_1:def 5;
        suppose
        A626: j+1 = k;
               n <= len(F.m) by A92,A614,A616;
        then A627: n in dom(F.m) by A608,FINSEQ_3:27;
        A628: n <> len(F.m) by A92,A614,A616;
               (F.m)/.n = (F.n)/.n by A560,A608,A614,A616
                  .= p by A392,A560,A608,A610
                  .= (F.m)/.m by A391,A392,A394,A560,A625,A626
                  .= (F.m)/.len(F.m) by A92;
          hence contradiction by A388,A392,A394,A395,A627,A628;
        end;
        suppose
        A629: j+1 < k;
          set l = j+1;
        A630: 1 <= l by NAT_1:11;
          A631: n < n+1 & n+1 < l by A615,XREAL_1:8,31;
        then A632: n < l by XXREAL_0:2;
          then n <= len(F.l) by A92;
        then A633: n in dom(F.l) by A608,FINSEQ_3:27;
        A634: n <> len(F.l) by A92,A631;
               (F.l)/.n = (F.n)/.n by A560,A608,A632
                  .= p by A392,A560,A608,A610
                  .= (F.l)/.l by A560,A625,A629,A630
                  .= (F.l)/.len(F.l) by A92;
          hence contradiction by A388,A629,A630,A633,A634;
      end;
      end;
      hence contradiction;
     end;
     end;
A635: for n st 1 <= n holds n-'1+2 = n+1
proof
  let n;
  assume 1 <= n;
  hence n-'1+2 = n+2-'1 by BINARITH:56
            .= n+1+1 - 1 by BINARITH:55
            .= n+1;
end;

     C meets LeftComp Rev g
  proof
   set rg = Rev g;

  A636: rg is_sequence_on G by A403,JORDAN9:7;

  A637: 1+1 <= len rg by A556,FINSEQ_5:def 3;
    then 1+1-'1 <= len rg-'1 by BINARITH:60;
  then A638: 1 <= len rg -'1 by BINARITH:39;
         1 < len rg by A637,NAT_1:13;
  then A639: len rg -'1+1 = len rg by BINARITH:53;

    set p = rg/.1, q = rg/.2;
    set a = f/.(m-'1);
    set l = (m-'1)+(len g-'1);

  A640: p = f/.m by A391,A402,FINSEQ_5:68;
  A641: m-'1+1 = m by A394,BINARITH:53;
  then A642: 1 <= m-'1 by A606,NAT_1:13;

         1+1-'1 <= len g-'1 by A556,BINARITH:60;
  then A643: 1 <= len g-'1 by BINARITH:39;
A644:   1+1 - 1 <= len g - 1 by A556,XREAL_1:11;
    A645: 1 - 1 < m - 1 by A606,XREAL_1:11;

       A646: l = m+(len g -'1)-'1 by A394,BINARITH:56
       .= (len g -'1)+m - 1 by A643,BINARITH:55
       .= (len g - 1)+m - 1 by A644,BINARITH:def 3
       .= ((k - (m - 1)) - 1)+m - 1 by A392,A400,A645,BINARITH:def 3
       .= k - 1;
  then A647: k = l+1;
  then A648: l < k by XREAL_1:31;
         len g-'1 <= l by NAT_1:11;
  then A649: 1 <= l by A643,XXREAL_0:2;
         len g-'1 <= len g by BINARITH:62;
  then A650: len g-'1 in dom g by A643,FINSEQ_3:27;
         1 <= len g by A557,SPPOL_1:5;
  then A651: len g-'1+2 = len g+1 by A635;
  then A652: q = g/.(len g -'1) by A650,FINSEQ_5:69
       .= f/.l by A650,FINSEQ_5:30;
  A653: p = f/.(l+1) by A392,A402,A646,FINSEQ_5:68;

  A654: left_cell(f,m-'1,G) meets C by A245,A394,A641,A642;
  A655: left_cell(f,l,G) meets C by A245,A392,A647,A649;

         not a in L~g by A607,A642;
  then A656: not a in L~rg by SPPOL_2:22;

  A657: m-'1+2 = m+1 by A394,A635;

  A658: m-'1 <= l by NAT_1:11;
    then m-'1 <= len(F.l) by A92;
  then A659: m-'1 in dom(F.l) by A642,FINSEQ_3:27;
         (m-'1)+1 <= l by A643,XREAL_1:8;
    then m-'1 < l by NAT_1:13;
  then A660: m-'1 <> len(F.l) by A92;

    consider p1,p2,q1,q2 being Element of NAT such that
  A661: [p1,p2] in Indices G and
  A662: p = G*(p1,p2) and
  A663: [q1,q2] in Indices G and
  A664: q = G*(q1,q2) and
  A665: p1 = q1 & p2+1 = q2 or p1+1 = q1 & p2 = q2 or
        p1 = q1+1 & p2 = q2 or p1 = q1 & p2 = q2+1 by A636,A637,JORDAN8:6;

  A666: 1 <= p1 & p1 <= len G & 1 <= p2 & p2 <= width G by A661,GOBOARD5:1;

    per cases by A665;
    suppose ::A
    A667: p1 = q1 & p2+1 = q2;
      consider a1,a2,p'1,p'2 being Element of NAT such that
    A668: [a1,a2] in Indices G and
    A669: a = G*(a1,a2) and
    A670: [p'1,p'2] in Indices G and
    A671: p = G*(p'1,p'2) and
    A672: a1 = p'1 & a2+1 = p'2 or a1+1 = p'1 & a2 = p'2 or
          a1 = p'1+1 & a2 = p'2 or a1 = p'1 & a2 = p'2+1
            by A393,A394,A640,A641,A642,JORDAN8:6;
    A673: 1 <= a1 & a1 <= len G & 1 <= a2 & a2 <= width G by A668,GOBOARD5:1;

      thus thesis
      proof
        per cases by A672;
        suppose ::I
               a1 = p'1 & a2+1 = p'2;
        then A674: a1 = p1 & a2+1 = p2 by A661,A662,A670,A671,GOBOARD1:21;
        A675: F.m is_sequence_on G by A245;
        A676: m-'1+1 <= len (F.m) by A92,A641;
        A677: m-'1 <= m by A641,NAT_1:11;
        A678: F.k|(m+1)=F.(m+1) by A321,A392,A398;
          A679: p2+1 > a2+1 & a2+1 > a2 by A674,NAT_1:13;
        A680: f/.(m-'1) = (F.(m-'1))/.(m-'1) by A392,A399,A560,A642
                     .= (F.m)/.(m-'1) by A560,A642,A677;
        A681: f/.(m-'1+1) = (F.m)/.m by A392,A394,A560,A641;
               left_cell(f,l,G) = cell(G,p1,p2)
                 by A392,A393,A646,A649,A652,A653,A661,A662,A663,A664,A667,
GOBRD13:28
            .= front_right_cell(F.m,m-'1,G) by A640,A641,A642,A661,A662,A668
,A669,A674,A675,A676,A680,A681,GOBRD13:36;
          then F.(m+1) turns_right m-'1,G by A322,A606,A655;
          then f turns_right m-'1,G by A398,A642,A657,A678,GOBRD13:44;
        then A682: f/.(m+1) = G*(p1+1,p2) & [p1+1,p2] in Indices G
            by A640,A641,A657,A661,A662,A668,A669,A679,GOBRD13:def 6;
        then A683: 1 <= p1+1 & p1+1 <= len G by GOBOARD5:1;
          set rc = left_cell(rg,len rg-'1,G)\L~rg;
        A684: rc c= LeftComp rg by A636,A638,A639,JORDAN9:29;
        A685: p2-'1 = a2 & p1-'1+1 = p1 by A666,A674,BINARITH:39,53;
        A686: 2 in dom g by A556,FINSEQ_3:27;
                len rg-'1+2 = len g +1 by A651,FINSEQ_5:def 3;
        then A687: rg/.(len rg -' 1) = g/.2 by A686,FINSEQ_5:69
                           .= f/.(m+1) by A657,A686,FINSEQ_5:30;
             p = g/.1 by A391,A401,A402,FINSEQ_5:68
              .= rg/.len g by FINSEQ_5:68
              .= rg/.len rg by FINSEQ_5:def 3;
          then left_cell(rg,len rg-'1,G) = cell(G,p1,a2)
            by A636,A638,A639,A661,A662,A682,A685,A687,GOBRD13:26;
          then a in left_cell(rg,len rg-'1,G)
            by A666,A669,A673,A674,A683,JORDAN9:22;
          then A688: a in rc by A656,XBOOLE_0:def 4;
        A689: L~rg c= L~f by A574,SPPOL_2:22;
               LeftComp rg is_a_component_of (L~rg)` by GOBOARD9:def 1;
          hence C meets LeftComp Rev g by A394,A576,A641,A642,A656,A684,A688
,A689;

        end;
        suppose ::II
          A690: a1+1 = p'1 & a2 = p'2;
        then A691: a1+1 = p1 & a2 = p2 by A661,A662,A670,A671,GOBOARD1:21;
        then q1-'1 = a1 by A667,BINARITH:39;
          then right_cell(f,l,G) = cell(G,a1,a2)
                 by A392,A393,A646,A649,A652,A653,A661,A662,A663,A664,A667,A691
,GOBRD13:29
            .= left_cell(f,m-'1,G)
                 by A393,A394,A640,A641,A642,A668,A669,A670,A671,A690,
GOBRD13:24;
          hence C meets LeftComp Rev g by A245,A392,A647,A649,A654;

        end;
        suppose ::III
               a1 = p'1+1 & a2 = p'2;
