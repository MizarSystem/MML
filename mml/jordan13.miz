  A616: n < m-'1+1 by A611,NAT_1:13;
  then A617: n < j by A613,XXREAL_0:2;

  A618: m-'1+1=m by A396,BINARITH:53;
  then A619: 1 < j by A608,A613,XXREAL_0:2;
  A620: j+1 <= k by A92,A614;
  A621: j < k by A394,A614,NAT_1:13;
  A622: 2 <= len G & 2 <= width G by A2,NAT_1:12;

  A623: p in Values G by A395,A610,A612,JORDAN9:8;
    per cases by A395,A614,A615,A619,A622,A623,JORDAN9:25;
