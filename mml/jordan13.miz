A371: for k holds P[k] from NAT_1:sch 1(A328,A329);
   defpred K[Element of NAT] means $1 >= 1 &
   ex m st m in dom(F.$1) & m <> len(F.$1) & (F.$1)/.m = (F.$1)/.len(F.$1);
  now assume
A372:   for k st k >= 1 holds
        for m st m in dom(F.k) & m <> len(F.k)
         holds (F.k)/.m <> (F.k)/.len(F.k);
         defpred P[Element of NAT] means F.$1 is one-to-one;
A373:    P[0] by A77;
A374:    for k st P[k] holds P[k+1]
     proof let k; assume
A375: F.k is one-to-one;
          now let n,m such that
A376:     n in dom(F.(k+1)) & m in dom(F.(k+1)) and
A377:     (F.(k+1))/.n = (F.(k+1))/.m;
       consider i,j such that
            [i,j] in Indices G and
A378:     F.(k+1) = (F.k)^<*G*(i,j)*> by A300;
A379:     1 <= n & n <= len(F.(k+1)) & 1 <= m & m <= len(F.(k+1))
         by A376,FINSEQ_3:27;
A380:     len(F.k) = k & len(F.(k+1)) = k+1 by A92;
A381:     1 <= k+1 by NAT_1:37;
       per cases by A379,A380,NAT_1:26;
        suppose n <= k & m <= k;
then A382:        n in dom(F.k) & m in dom(F.k) by A379,A380,FINSEQ_3:27;
         then (F.(k+1))/.n = (F.k)/.n & (F.(k+1))/.m = (F.k)/.m
          by A378,FINSEQ_4:83;
         hence n = m by A375,A377,A382,PARTFUN2:17;
        end;
        suppose n = k+1 & m <= k;
         hence n = m by A372,A376,A377,A380,A381;
        end;
        suppose n <= k & m = k+1;
         hence n = m by A372,A376,A377,A380,A381;
        end;
        suppose n = k+1 & m = k+1;
         hence n = m;
      end;
      end;
      hence F.(k+1) is one-to-one by PARTFUN2:16;
     end;
A383:   for k holds P[k] from NAT_1:sch 1(A373,A374);
A384:   for k holds card rng(F.k) = k
     proof let k;
          F.k is one-to-one by A383;
      hence card rng(F.k) = len(F.k) by FINSEQ_4:77
                         .= k by A92;
     end;
     reconsider k = (len G)*(width G)+1 as Element of NAT;
A385:   k > (len G)*(width G) by NAT_1:38;
A386:   card Values G <= (len G)*(width G) by GOBRD13:8;
        F.k is_sequence_on G by A246;
    then rng(F.k) c= Values G by GOBRD13:9;
    then card rng(F.k) <= card Values G by CARD_1:80;
    then card rng(F.k) <= (len G)*(width G) by A386,XXREAL_0:2;
    hence contradiction by A384,A385;
   end;
   then A387: ex k st K[k];
   consider k such that
A388:  K[k] and
A389:  for l st K[l]
       holds k <= l from NAT_1:sch 5(A387);

   consider m such that
A390:  m in dom(F.k) and
A391:  m <> len(F.k) and
A392:  (F.k)/.m = (F.k)/.len(F.k) by A388;
A393:  len(F.k) = k by A92;
  then reconsider f = F.k as non empty FinSequence of TOP-REAL 2 by A388,
FINSEQ_1:25;
A394: f is_sequence_on G by A246;
A395: 1 <= m & m <= len f by A390,FINSEQ_3:27;
then A396: m < len f by A391,REAL_1:def 5;
   then 1 < len f by A395,XXREAL_0:2;
then A397: len f >= 1+1 by NAT_1:38;
then A398: k >= 2 by A92;
  reconsider f as non constant special unfolded non empty
    FinSequence of TOP-REAL 2 by A371,A394,A397,JORDAN8:7,8;
  set g = f/^(m-'1);
A399: m+1 <= len f by A396,NAT_1:38;
      m-'1 <= m & m < m+1 by JORDAN3:7,NAT_1:38;
  then m-'1 < m+1 by XXREAL_0:2;
then A400: m-'1 < len f by A399,XXREAL_0:2;
then A401: len g = len f - (m-'1) by RFINSEQ:def 2;
  then (m-'1)-(m-'1) < len g by A400,XREAL_1:11;
  then reconsider g as non empty FinSequence of TOP-REAL 2
   by FINSEQ_1:25;
      1 in dom g by FINSEQ_5:6;
then A402:  g/.1 = f/.(m-'1+1) by FINSEQ_5:30
