          then n <= len(F.l) by A92;
        then A635: n in dom(F.l) by A610,FINSEQ_3:27;
        A636: n <> len(F.l) by A92,A633;
               (F.l)/.n = (F.n)/.n by A562,A610,A634
                  .= p by A394,A562,A610,A612
                  .= (F.l)/.l by A562,A627,A631,A632
                  .= (F.l)/.len(F.l) by A92;
          hence contradiction by A388,A631,A632,A635,A636;
      end;
      end;
      hence contradiction;
     end;
     end;
A637: for n st 1 <= n holds n-'1+2 = n+1
proof
  let n;
  assume 1 <= n;
  hence n-'1+2 = n+2-'1 by BINARITH:56
            .= n+1+1 - 1 by BINARITH:55
            .= n+1;
end;

     C meets LeftComp Rev g
  proof
   set rg = Rev g;

  A638: rg is_sequence_on G by A405,JORDAN9:7;

  A639: 1+1 <= len rg by A558,FINSEQ_5:def 3;
    then 1+1-'1 <= len rg-'1 by BINARITH:60;
  then A640: 1 <= len rg -'1 by BINARITH:39;
         1 < len rg by A639,NAT_1:13;
  then A641: len rg -'1+1 = len rg by BINARITH:53;

    set p = rg/.1, q = rg/.2;
    set a = f/.(m-'1);
    set l = (m-'1)+(len g-'1);

  A642: p = f/.m by A393,A404,FINSEQ_5:68;
  A643: m-'1+1 = m by A396,BINARITH:53;
  then A644: 1 <= m-'1 by A608,NAT_1:13;

         1+1-'1 <= len g-'1 by A558,BINARITH:60;
  then A645: 1 <= len g-'1 by BINARITH:39;
A646:   1+1 - 1 <= len g - 1 by A558,XREAL_1:11;
    A647: 1 - 1 < m - 1 by A608,XREAL_1:11;

       A648: l = m+(len g -'1)-'1 by A396,BINARITH:56
       .= (len g -'1)+m - 1 by A645,BINARITH:55
       .= (len g - 1)+m - 1 by A646,BINARITH:def 3
       .= ((k - (m - 1)) - 1)+m - 1 by A394,A402,A647,BINARITH:def 3
       .= k - 1;
  then A649: k = l+1;
  then A650: l < k by XREAL_1:31;
         len g-'1 <= l by NAT_1:11;
  then A651: 1 <= l by A645,XXREAL_0:2;
         len g-'1 <= len g by BINARITH:62;
  then A652: len g-'1 in dom g by A645,FINSEQ_3:27;
         1 <= len g by A559,SPPOL_1:5;
  then A653: len g-'1+2 = len g+1 by A637;
  then A654: q = g/.(len g -'1) by A652,FINSEQ_5:69
       .= f/.l by A652,FINSEQ_5:30;
  A655: p = f/.(l+1) by A394,A404,A648,FINSEQ_5:68;

  A656: left_cell(f,m-'1,G) meets C by A245,A396,A643,A644;
  A657: left_cell(f,l,G) meets C by A245,A394,A649,A651;

         not a in L~g by A609,A644;
  then A658: not a in L~rg by SPPOL_2:22;

  A659: m-'1+2 = m+1 by A396,A637;

  A660: m-'1 <= l by NAT_1:11;
    then m-'1 <= len(F.l) by A92;
  then A661: m-'1 in dom(F.l) by A644,FINSEQ_3:27;
         (m-'1)+1 <= l by A645,XREAL_1:8;
    then m-'1 < l by NAT_1:13;
  then A662: m-'1 <> len(F.l) by A92;

    consider p1,p2,q1,q2 being Element of NAT such that
  A663: [p1,p2] in Indices G and
  A664: p = G*(p1,p2) and
  A665: [q1,q2] in Indices G and
  A666: q = G*(q1,q2) and
  A667: p1 = q1 & p2+1 = q2 or p1+1 = q1 & p2 = q2 or
        p1 = q1+1 & p2 = q2 or p1 = q1 & p2 = q2+1 by A638,A639,JORDAN8:6;

  A668: 1 <= p1 & p1 <= len G & 1 <= p2 & p2 <= width G by A663,GOBOARD5:1;

    per cases by A667;
    suppose ::A
    A669: p1 = q1 & p2+1 = q2;
      consider a1,a2,p'1,p'2 being Element of NAT such that
    A670: [a1,a2] in Indices G and
    A671: a = G*(a1,a2) and
    A672: [p'1,p'2] in Indices G and
    A673: p = G*(p'1,p'2) and
    A674: a1 = p'1 & a2+1 = p'2 or a1+1 = p'1 & a2 = p'2 or
          a1 = p'1+1 & a2 = p'2 or a1 = p'1 & a2 = p'2+1
            by A395,A396,A642,A643,A644,JORDAN8:6;
    A675: 1 <= a1 & a1 <= len G & 1 <= a2 & a2 <= width G by A670,GOBOARD5:1;

      thus thesis
      proof
        per cases by A674;
        suppose ::I
               a1 = p'1 & a2+1 = p'2;
        then A676: a1 = p1 & a2+1 = p2 by A663,A664,A672,A673,GOBOARD1:21;
        A677: F.m is_sequence_on G by A245;
        A678: m-'1+1 <= len (F.m) by A92,A643;
        A679: m-'1 <= m by A643,NAT_1:11;
        A680: F.k|(m+1)=F.(m+1) by A321,A394,A400;
          A681: p2+1 > a2+1 & a2+1 > a2 by A676,NAT_1:13;
        A682: f/.(m-'1) = (F.(m-'1))/.(m-'1) by A394,A401,A562,A644
                     .= (F.m)/.(m-'1) by A562,A644,A679;
        A683: f/.(m-'1+1) = (F.m)/.m by A394,A396,A562,A643;
               left_cell(f,l,G) = cell(G,p1,p2)
                 by A394,A395,A648,A651,A654,A655,A663,A664,A665,A666,A669,
GOBRD13:28
            .= front_right_cell(F.m,m-'1,G) by A642,A643,A644,A663,A664,A670
,A671,A676,A677,A678,A682,A683,GOBRD13:36;
          then F.(m+1) turns_right m-'1,G by A322,A608,A657;
          then f turns_right m-'1,G by A400,A644,A659,A680,GOBRD13:44;
        then A684: f/.(m+1) = G*(p1+1,p2) & [p1+1,p2] in Indices G
            by A642,A643,A659,A663,A664,A670,A671,A681,GOBRD13:def 6;
        then A685: 1 <= p1+1 & p1+1 <= len G by GOBOARD5:1;
          set rc = left_cell(rg,len rg-'1,G)\L~rg;
        A686: rc c= LeftComp rg by A638,A640,A641,JORDAN9:29;
        A687: p2-'1 = a2 & p1-'1+1 = p1 by A668,A676,BINARITH:39,53;
        A688: 2 in dom g by A558,FINSEQ_3:27;
                len rg-'1+2 = len g +1 by A653,FINSEQ_5:def 3;
        then A689: rg/.(len rg -' 1) = g/.2 by A688,FINSEQ_5:69
                           .= f/.(m+1) by A659,A688,FINSEQ_5:30;
             p = g/.1 by A393,A403,A404,FINSEQ_5:68
              .= rg/.len g by FINSEQ_5:68
              .= rg/.len rg by FINSEQ_5:def 3;
          then left_cell(rg,len rg-'1,G) = cell(G,p1,a2)
            by A638,A640,A641,A663,A664,A684,A687,A689,GOBRD13:26;
          then a in left_cell(rg,len rg-'1,G)
            by A668,A671,A675,A676,A685,JORDAN9:22;
          then A690: a in rc by A658,XBOOLE_0:def 4;
        A691: L~rg c= L~f by A576,SPPOL_2:22;
               LeftComp rg is_a_component_of (L~rg)` by GOBOARD9:def 1;
          hence C meets LeftComp Rev g by A396,A578,A643,A644,A658,A686,A690
,A691;

        end;
        suppose ::II
          A692: a1+1 = p'1 & a2 = p'2;
        then A693: a1+1 = p1 & a2 = p2 by A663,A664,A672,A673,GOBOARD1:21;
