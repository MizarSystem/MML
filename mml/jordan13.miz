    A626: n <> len(F.j) by A92,A613,A616;
           (F.j)/.n = (F.n)/.n by A562,A610,A617
              .= p by A394,A562,A610,A612
              .= (F.j)/.j by A562,A619,A621,A624
              .= (F.j)/.len(F.j) by A92;
      hence contradiction by A388,A619,A621,A625,A626;

    end;
    suppose
    A627: p = f/.(j+1);
           now
        per cases by A620,REAL_1:def 5;
        suppose
        A628: j+1 = k;
               n <= len(F.m) by A92,A616,A618;
        then A629: n in dom(F.m) by A610,FINSEQ_3:27;
        A630: n <> len(F.m) by A92,A616,A618;
               (F.m)/.n = (F.n)/.n by A562,A610,A616,A618
                  .= p by A394,A562,A610,A612
                  .= (F.m)/.m by A393,A394,A396,A562,A627,A628
                  .= (F.m)/.len(F.m) by A92;
          hence contradiction by A388,A394,A396,A397,A629,A630;
        end;
        suppose
        A631: j+1 < k;
          set l = j+1;
        A632: 1 <= l by NAT_1:11;
          A633: n < n+1 & n+1 < l by A617,XREAL_1:8,31;
