          by A377,FINSEQ_4:83;
         hence n = m by A374,A376,A381,PARTFUN2:17;
        end;
        suppose n = k+1 & m <= k;
         hence n = m by A371,A375,A376,A379,A380;
        end;
        suppose n <= k & m = k+1;
         hence n = m by A371,A375,A376,A379,A380;
        end;
        suppose n = k+1 & m = k+1;
         hence n = m;
      end;
      end;
      hence F.(k+1) is one-to-one by PARTFUN2:16;
     end;
A382:   for k holds P[k] from NAT_1:sch 1(A372,A373);
A383:   for k holds card rng(F.k) = k
     proof let k;
           F.k is one-to-one by A382;
