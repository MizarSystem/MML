then A405: g is standard by JORDAN8:7;
A406: for i st 1 <= i & i < len g & 1 <= j & j < len g & g/.i = g/.j
      holds i = j
   proof let i such that
A407:  1 <= i & i < len g and
A408:  1 <= j & j < len g and
A409:  g/.i = g/.j and
A410:  i <> j;
         i in dom g by A407,FINSEQ_3:27;
then A411:  g/.i = f/.(m-'1+i) by FINSEQ_5:30;
         j in dom g by A408,FINSEQ_3:27;
then A412:  g/.j = f/.(m-'1+j) by FINSEQ_5:30;
    per cases by A410,REAL_1:def 5;
    suppose
A413:    i < j;
     set l = m-'1+j, m'= m-'1+i;
A414:    m' < l by A413,XREAL_1:8;
A415:    l < k by A393,A401,A408,XREAL_1:22;
then A416:    f|l = F.l by A322;
         0+j <= l by XREAL_1:8;
then A417:    1 <= l by A408,XXREAL_0:2;
A418:    len(F.l) = l by A92;
         0+i <= m' by XREAL_1:8;
     then 1 <= m' by A407,XXREAL_0:2;
then A419:    m' in dom(F.l) by A414,A418,FINSEQ_3:27;
then A420:    (F.l)/.m' = f/.m' by A416,FINSEQ_4:85;
