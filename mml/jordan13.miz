A276: (len(F.k))-'1 in dom(F.k) & len(F.k) in dom(F.k)
       by A267,A270,A272,A273,FINSEQ_3:27;
A277: (len(F.k))-'1+(1+1) = (len(F.k))+1 by A274;
A278: i1+1 > i1 & i2+1 > i2 & j1+1 > j1 & j2+1 > j2 by NAT_1:38;
   hereby assume front_right_cell(F.k,(len(F.k))-'1,G) misses C &
        front_left_cell(F.k,(len(F.k))-'1,G) meets C;
    then consider i,j such that
A279:   (F.k)^<*G*(i,j)*> goes_straight (len(F.k))-'1,G and
A280:   F.(k+1) = (F.k)^<*G*(i,j)*> by A78,A267,A271,A272,A275;
A281:   (F.(k+1))/.(len(F.k)-'1) = G*(i1,j1) & (F.(k+1))/.len(F.k) = G*(i2,j2)
        by A268,A269,A276,A280,FINSEQ_4:83;
A282:   (F.(k+1))/.(len(F.k)+1) = G*(i,j) by A280,FINSEQ_4:82;
    thus F.(k+1) goes_straight (len(F.k))-'1,G by A279,A280;
    thus i1 = i2 & j1+1 = j2 implies
      [i2,j2+1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2+1)*>
       by A268,A269,A274,A277,A278,A279,A280,A281,A282,GOBRD13:def 8;
     thus i1+1 = i2 & j1 = j2 implies
      [i2+1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2+1,j2)*>
       by A268,A269,A274,A277,A278,A279,A280,A281,A282,GOBRD13:def 8;
    thus i1 = i2+1 & j1 = j2 implies
      [i2-'1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2-'1,j2)*>
       by A268,A269,A274,A277,A278,A279,A280,A281,A282,GOBRD13:def 8;
    thus i1 = i2 & j1 = j2+1 implies
      [i2,j2-'1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2-'1)*>
       by A268,A269,A274,A277,A278,A279,A280,A281,A282,GOBRD13:def 8;
   end;
 end;

A283: for k,i1,i2,j1,j2 st k > 1 &
        [i1,j1] in Indices G & (F.k)/.(len(F.k)-'1) = G*(i1,j1) &
        [i2,j2] in Indices G & (F.k)/.len(F.k) = G*(i2,j2)
      holds
       (front_right_cell(F.k,(len(F.k))-'1,G) meets C
         implies F.(k+1) turns_right (len(F.k))-'1,G &
          (i1 = i2 & j1+1 = j2 implies [i2+1,j2] in Indices G &
            F.(k+1) = (F.k)^<*G*(i2+1,j2)*>) &
          (i1+1 = i2 & j1 = j2 implies [i2,j2-'1] in Indices G &
            F.(k+1) = (F.k)^<*G*(i2,j2-'1)*>) &
          (i1 = i2+1 & j1 = j2 implies [i2,j2+1] in Indices G &
            F.(k+1) = (F.k)^<*G*(i2,j2+1)*>)&
          (i1 = i2 & j1 = j2+1 implies [i2-'1,j2] in Indices G &
            F.(k+1) = (F.k)^<*G*(i2-'1,j2)*>))
  proof let k,i1,i2,j1,j2 such that
A284:  k > 1 and
A285:  [i1,j1] in Indices G & (F.k)/.(len(F.k)-'1) = G*(i1,j1) and
A286:  [i2,j2] in Indices G & (F.k)/.len(F.k) = G*(i2,j2);
A287:  (len(F.k))-'1 <= len(F.k) by BINARITH:52;
A288:   F.k is_sequence_on G by A246;
A289:   len(F.k) = k by A92;
then A290:  1 <= (len(F.k))-'1 by A284,JORDAN3:12;
A291:  (len(F.k)) -'1 +1 = len(F.k) by A284,A289,BINARITH:53;
then A292: left_cell(F.k,(len(F.k))-'1,G) meets C by A246,A290;
A293: (len(F.k))-'1 in dom(F.k) & len(F.k) in dom(F.k)
       by A284,A287,A289,A290,FINSEQ_3:27;
A294: (len(F.k))-'1+(1+1) = (len(F.k))+1 by A291;
A295: i1+1 > i1 & i2+1 > i2 & j1+1 > j1 & j2+1 > j2 by NAT_1:38;
   assume front_right_cell(F.k,(len(F.k))-'1,G) meets C;
   then consider i,j such that
A296:   (F.k)^<*G*(i,j)*> turns_right (len(F.k))-'1,G and
A297:   F.(k+1) = (F.k)^<*G*(i,j)*> by A78,A284,A288,A289,A292;
A298:   (F.(k+1))/.(len(F.k)-'1) = G*(i1,j1) & (F.(k+1))/.len(F.k) = G*(i2,j2)
        by A285,A286,A293,A297,FINSEQ_4:83;
A299:   (F.(k+1))/.(len(F.k)+1) = G*(i,j) by A297,FINSEQ_4:82;
   thus F.(k+1) turns_right (len(F.k))-'1,G by A296,A297;
   thus i1 = i2 & j1+1 = j2 implies
    [i2+1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2+1,j2)*>
       by A285,A286,A291,A294,A295,A296,A297,A298,A299,GOBRD13:def 6;
   thus i1+1 = i2 & j1 = j2 implies
    [i2,j2-'1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2-'1)*>
       by A285,A286,A291,A294,A295,A296,A297,A298,A299,GOBRD13:def 6;
   thus i1 = i2+1 & j1 = j2 implies
    [i2,j2+1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2+1)*>
       by A285,A286,A291,A294,A295,A296,A297,A298,A299,GOBRD13:def 6;
   thus i1 = i2 & j1 = j2+1 implies
    [i2-'1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2-'1,j2)*>
       by A285,A286,A291,A294,A295,A296,A297,A298,A299,GOBRD13:def 6;
 end;

A300:
  for k holds ex i,j st [i,j] in Indices G & F.(k+1) = (F.k)^<*G*(i,j)*>
   proof let k;
A301:  F.k is_sequence_on G by A246;
A302:  len(F.k) = k by A92;
    per cases by REAL_1:def 5;
    suppose k < 1;
then A303:   k = 0 by NAT_1:39;
      take X-SpanStart(C,n),Y-SpanStart(C,n);
      thus [X-SpanStart(C,n),Y-SpanStart(C,n)] in Indices G by A1,JORDAN11:8;
      thus thesis by A77,A247,A303,FINSEQ_1:47;
    end;
    suppose
A304:   k = 1;
     take X-SpanStart(C,n)-'1,Y-SpanStart(C,n);
     thus [X-SpanStart(C,n)-'1,Y-SpanStart(C,n)] in Indices G
       by A1,JORDAN11:9;
     thus thesis by A247,A248,A304,FINSEQ_1:def 9;
    end;
    suppose
A305:   k > 1;
then A306:   1 <= (len(F.k))-'1 by A302,JORDAN3:12;
          (len(F.k)) -'1 +1 = len(F.k) by A302,A305,BINARITH:53;
     then consider i1,j1,i2,j2 being Element of NAT such that
A307:   [i1,j1] in Indices G & (F.k)/.(len(F.k)-'1) = G*(i1,j1) and
A308:   [i2,j2] in Indices G & (F.k)/.len(F.k) = G*(i2,j2) and
A309:   i1 = i2 & j1+1 = j2 or i1+1 = i2 & j1 = j2 or
        i1 = i2+1 & j1 = j2 or i1 = i2 & j1 = j2+1 by A301,A306,JORDAN8:6;
         now per cases;
       suppose
A310:       front_right_cell(F.k,(len(F.k))-'1,G) misses C &
          front_left_cell(F.k,(len(F.k))-'1,G) misses C;
             now per cases by A309;
          suppose i1 = i2 & j1+1 = j2;
           then [i2-'1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2-'1,j2)*>
             by A249,A305,A307,A308,A310;
            hence thesis;
          end;
          suppose i1+1 = i2 & j1 = j2;
           then [i2,j2+1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2+1)*>
             by A249,A305,A307,A308,A310;
            hence thesis;
          end;
          suppose i1 = i2+1 & j1 = j2;
           then [i2,j2-'1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2-'1)*>
             by A249,A305,A307,A308,A310;
            hence thesis;
          end;
          suppose i1 = i2 & j1 = j2+1;
           then [i2+1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2+1,j2)*>
             by A249,A305,A307,A308,A310;
            hence thesis;
         end;
         end;
        hence thesis;
       end;
       suppose
A311:       front_right_cell(F.k,(len(F.k))-'1,G) misses C &
          front_left_cell(F.k,(len(F.k))-'1,G) meets C;
             now per cases by A309;
          suppose i1 = i2 & j1+1 = j2;
           then [i2,j2+1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2+1)*>
             by A266,A305,A307,A308,A311;
            hence thesis;
          end;
          suppose i1+1 = i2 & j1 = j2;
           then [i2+1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2+1,j2)*>
             by A266,A305,A307,A308,A311;
            hence thesis;
          end;
          suppose i1 = i2+1 & j1 = j2;
           then [i2-'1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2-'1,j2)*>
             by A266,A305,A307,A308,A311;
            hence thesis;
          end;
          suppose i1 = i2 & j1 = j2+1;
           then [i2,j2-'1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2-'1)*>
             by A266,A305,A307,A308,A311;
            hence thesis;
         end;
         end;
        hence thesis;
       end;
       suppose
A312:       front_right_cell(F.k,(len(F.k))-'1,G) meets C;
             now per cases by A309;
          suppose i1 = i2 & j1+1 = j2;
           then [i2+1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2+1,j2)*>
             by A283,A305,A307,A308,A312;
            hence thesis;
          end;
          suppose i1+1 = i2 & j1 = j2;
           then [i2,j2-'1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2-'1)*>
             by A283,A305,A307,A308,A312;
            hence thesis;
          end;
          suppose i1 = i2+1 & j1 = j2;
           then [i2,j2+1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2+1)*>
             by A283,A305,A307,A308,A312;
            hence thesis;
          end;
          suppose i1 = i2 & j1 = j2+1;
           then [i2-'1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2-'1,j2)*>
             by A283,A305,A307,A308,A312;
            hence thesis;
         end;
         end;
        hence thesis;
     end;
     end;
     hence thesis;
   end;
   end;
defpred P[Element of NAT] means for m st m <= $1 holds (F.$1)|m = F.m;
A313: P[0]
   proof let m;
    assume
A314:  m <= 0;
   0 = m by A314;
    then (F.0)|m = (F.m)|Seg 0 by FINSEQ_1:def 15;
    then (F.0)|m = {} by FINSEQ_1:4,RELAT_1:110;
    hence thesis by A77,A314,XXREAL_0:1;
   end;
A315: for k st P[k] holds P[k+1]
   proof let k such that
A316:   for m st m <= k holds (F.k)|m = F.m;
    let m such that
A317:   m <= k+1;
    per cases by A317,REAL_1:def 5;
    suppose m < k+1;
then A318:    m <= k by NAT_1:38;
A319:    len(F.k) = k by A92;
     consider i,j such that
           [i,j] in Indices G and
A320:    F.(k+1) = F.k^<*G*(i,j)*> by A300;
         (F.(k+1))|m = (F.k)|m by A318,A319,A320,FINSEQ_5:25;
     hence (F.(k+1))|m = F.m by A316,A318;
    end;
    suppose
A321:    m = k+1;
         len(F.(k+1)) = k+1 by A92;
     hence (F.(k+1))|m = F.m by A321,FINSEQ_1:79;
   end;
   end;

A322: for k holds P[k] from NAT_1:sch 1(A313,A315);
A323: for k st k > 1 holds
       (front_right_cell(F.k,k-'1,Gauge(C,n)) misses C &
          front_left_cell(F.k,k-'1,Gauge(C,n)) misses C
         implies F.(k+1) turns_left k-'1,Gauge(C,n)) &
       (front_right_cell(F.k,k-'1,Gauge(C,n)) misses C &
          front_left_cell(F.k,k-'1,Gauge(C,n)) meets C
         implies F.(k+1) goes_straight k-'1,Gauge(C,n)) &
       (front_right_cell(F.k,k-'1,Gauge(C,n)) meets C
         implies F.(k+1) turns_right k-'1,Gauge(C,n))
   proof
     let k such that
A324:  k > 1;
A325:  F.k is_sequence_on G by A246;
A326:  len(F.k) = k by A92;
then A327:  1 <= (len(F.k))-'1 by A324,JORDAN3:12;
        (len(F.k)) -'1 +1 = len(F.k) by A324,A326,BINARITH:53;
    then ex i1,j1,i2,j2 being Element of NAT st
         [i1,j1] in Indices G & (F.k)/.(len(F.k)-'1) = G*(i1,j1) &
         [i2,j2] in Indices G & (F.k)/.len(F.k) = G*(i2,j2) &
         (i1 = i2 & j1+1 = j2 or i1+1 = i2 & j1 = j2 or
      i1 = i2+1 & j1 = j2 or i1 = i2 & j1 = j2+1) by A325,A327,JORDAN8:6;
    hence thesis by A249,A266,A283,A324,A326;
   end;
   defpred P[Element of NAT] means F.$1 is unfolded;
       len(F.0) = 0 by A77,FINSEQ_1:25;
then A328: P[0] by SPPOL_2:27;
A329: for k st P[k] holds P[k+1]
   proof let k such that
A330:  F.k is unfolded;
A331:  F.k is_sequence_on G by A246;
    per cases;
    suppose k <= 1;
     then k+1 <= 1+1 by XREAL_1:8;
     then len(F.(k+1)) <= 2 by A92;
     hence F.(k+1) is unfolded by SPPOL_2:27;
    end;
    suppose
A332:   k > 1;
    set m = k-'1;
A333:   1 <= m by A332,JORDAN3:12;
A334:   m+1 = k by A332,BINARITH:53;
A335:   len(F.k) = k by A92;
     then consider i1,j1,i2,j2 being Element of NAT such that
A336:   [i1,j1] in Indices G & (F.k)/.m = G*(i1,j1) and
A337:   [i2,j2] in Indices G & (F.k)/.len(F.k) = G*(i2,j2) and
A338:   i1 = i2 & j1+1 = j2 or i1+1 = i2 & j1 = j2 or
        i1 = i2+1 & j1 = j2 or i1 = i2 & j1 = j2+1 by A331,A333,A334,JORDAN8:6;
A339:  1 <= i1 & i1 <= len G & 1 <= j1 & j1 <= width G by A336,GOBOARD5:1;
A340:  1 <= i2 & i2 <= len G & 1 <= j2 & j2 <= width G by A337,GOBOARD5:1;
then A341:  (i2-'1)+1 = i2 by BINARITH:53;
A342:  (j2-'1)+1 = j2 by A340,BINARITH:53;
A343:   LSeg(F.k,m) = LSeg(G*(i1,j1),G*(i2,j2)) by A333,A334,A335,A336,A337,
TOPREAL1:def 5;
        now per cases;
     suppose
A344:     front_right_cell(F.k,(len(F.k))-'1,G) misses C &
          front_left_cell(F.k,(len(F.k))-'1,G) misses C;
          now per cases by A338;
       suppose
A345:       i1 = i2 & j1+1 = j2;
then A346:       [i2-'1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2-'1,j2)*>
           by A249,A332,A335,A336,A337,A344;
         then 1 <= i2-'1 by GOBOARD5:1;
         then LSeg(F.k,m) /\ LSeg((F.k)/.len(F.k),G*(i2-'1,j2)) =
         {(F.k)/.len(F.k)}
          by A337,A339,A340,A341,A343,A345,GOBOARD7:18;
        hence F.(k+1) is unfolded by A330,A334,A335,A346,SPPOL_2:31;
       end;
       suppose
A347:       i1+1 = i2 & j1 = j2;
then A348:       [i2,j2+1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2+1)*>
           by A249,A332,A335,A336,A337,A344;
         then j2+1 <= width G &
          LSeg((F.k)/.len(F.k),G*(i2,j2+1))=LSeg(G*(i2,j2+1),(F.k)/.len(F.k))
           by GOBOARD5:1;
         then LSeg(F.k,m) /\ LSeg((F.k)/.len(F.k),G*(i2,j2+1)) = {(F.k)/.len(F
.k)}
           by A337,A339,A340,A343,A347,GOBOARD7:20;
        hence F.(k+1) is unfolded by A330,A334,A335,A348,SPPOL_2:31;
       end;
       suppose
A349:       i1 = i2+1 & j1 = j2;
then A350:       [i2,j2-'1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2-'1)*>
           by A249,A332,A335,A336,A337,A344;
         then 1 <= j2-'1 by GOBOARD5:1;
         then LSeg(F.k,m) /\ LSeg((F.k)/.len(F.k),G*(i2,j2-'1)) =
           {(F.k)/.len(F.k)}
          by A337,A339,A340,A342,A343,A349,GOBOARD7:17;
        hence F.(k+1) is unfolded by A330,A334,A335,A350,SPPOL_2:31;
       end;
       suppose
A351:       i1 = i2 & j1 = j2+1;
then A352:       [i2+1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2+1,j2)*>
           by A249,A332,A335,A336,A337,A344;
         then i2+1 <= len G by GOBOARD5:1;
         then LSeg(F.k,m) /\ LSeg((F.k)/.len(F.k),G*(i2+1,j2)) = {(F.k)/.len(F
.k)}
          by A337,A339,A340,A343,A351,GOBOARD7:19;
        hence F.(k+1) is unfolded by A330,A334,A335,A352,SPPOL_2:31;
      end;
      end;
      hence F.(k+1) is unfolded;
     end;
     suppose
A353:     front_right_cell(F.k,(len(F.k))-'1,G) misses C &
              front_left_cell(F.k,(len(F.k))-'1,G) meets C;
          now per cases by A338;
       suppose
A354:       i1 = i2 & j1+1 = j2;
then A355:       [i2,j2+1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2+1)*>
           by A266,A332,A335,A336,A337,A353;
         then 1 <= j2+1 & j2+1 <= width G & j2+1 = j1+(1+1)
          by A354,GOBOARD5:1;
         then LSeg(F.k,m) /\ LSeg((F.k)/.len(F.k),G*(i2,j2+1)) = {(F.k)/.len(F
.k)}
          by A337,A339,A343,A354,GOBOARD7:15;
        hence F.(k+1) is unfolded by A330,A334,A335,A355,SPPOL_2:31;
       end;
       suppose
A356:       i1+1 = i2 & j1 = j2;
then A357:       [i2+1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2+1,j2)*>
           by A266,A332,A335,A336,A337,A353;
         then 1 <= i2+1 & i2+1 <= len G & i2+1 = i1+(1+1)
          by A356,GOBOARD5:1;
         then LSeg(F.k,m) /\ LSeg((F.k)/.len(F.k),G*(i2+1,j2)) = {(F.k)/.len(F
.k)}
          by A337,A339,A343,A356,GOBOARD7:16;
        hence F.(k+1) is unfolded by A330,A334,A335,A357,SPPOL_2:31;
       end;
       suppose
A358:       i1 = i2+1 & j1 = j2;
then A359:       [i2-'1,j2] in Indices G & F.(k+1) = (F.k)^<*G*(i2-'1,j2)*>
           by A266,A332,A335,A336,A337,A353;
         then 1 <= i2-'1 & i2-'1+1+1 = i2-'1+(1+1)
          by GOBOARD5:1;
         then LSeg(F.k,m) /\ LSeg((F.k)/.len(F.k),G*(i2-'1,j2)) =
          {(F.k)/.len(F.k)}
          by A337,A339,A341,A343,A358,GOBOARD7:16;
        hence F.(k+1) is unfolded by A330,A334,A335,A359,SPPOL_2:31;
       end;
       suppose
A360:       i1 = i2 & j1 = j2+1;
then A361:       [i2,j2-'1] in Indices G & F.(k+1) = (F.k)^<*G*(i2,j2-'1)*>
           by A266,A332,A335,A336,A337,A353;
         then 1 <= j2-'1 & j2-'1+1+1 = j2-'1+(1+1)
          by GOBOARD5:1;
         then LSeg(F.k,m) /\ LSeg((F.k)/.len(F.k),G*(i2,j2-'1)) =
         {(F.k)/.len(F.k)}
          by A337,A339,A342,A343,A360,GOBOARD7:15;
        hence F.(k+1) is unfolded by A330,A334,A335,A361,SPPOL_2:31;
      end;
      end;
      hence F.(k+1) is unfolded;
     end;
     suppose
A362:     front_right_cell(F.k,(len(F.k))-'1,G) meets C;
