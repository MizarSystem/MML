  A644: p = f/.m by A393,A404,FINSEQ_5:68;
  A645: m-'1+1 = m by A396,AMI_5:4;
  then A646: 1 <= m-'1 by A610,NAT_1:38;

        1+1-'1 <= len g-'1 by A559,JORDAN3:5;
  then A647: 1 <= len g-'1 by BINARITH:39;
        1+1 - 1 <= len g - 1 by A559,REAL_1:49;
  then A648: 0 <= len g - 1 by AXIOMS:22;
    A649: 1 - 1 < m - 1 by A610,REAL_1:54;

       A650: l = m+(len g -'1)-'1 by A396,JORDAN4:3
       .= (len g -'1)+m - 1 by A647,JORDAN4:2
       .= (len g - 1)+m - 1 by A648,BINARITH:def 3
       .= ((k - (m - 1)) - 1)+m - 1 by A394,A402,A649,BINARITH:def 3
       .= k - 1;
  then A651: k = l+1;
  then A652: l < k by REAL_1:69;
        len g-'1 <= l by NAT_1:29;
  then A653: 1 <= l by A647,AXIOMS:22;
        len g-'1 <= len g by JORDAN3:7;
  then A654: len g-'1 in dom g by A647,FINSEQ_3:27;
        1 <= len g by A560,SPPOL_1:5;
  then A655: len g-'1+2 = len g+1 by A639;
  then A656: q = g/.(len g -'1) by A654,FINSEQ_5:69
       .= f/.l by A654,FINSEQ_5:30;
  A657: p = f/.(l+1) by A394,A404,A650,FINSEQ_5:68;

  A658: left_cell(f,m-'1,G) meets C by A246,A396,A645,A646;
  A659: left_cell(f,l,G) meets C by A246,A394,A651,A653;

        not a in L~g by A611,A646;
  then A660: not a in L~rg by SPPOL_2:22;

  A661: m-'1+2 = m+1 by A396,A639;

  A662: m-'1 <= l by NAT_1:29;
    then m-'1 <= len(F.l) by A92;
  then A663: m-'1 in dom(F.l) by A646,FINSEQ_3:27;
        (m-'1)+1 <= l by A647,AXIOMS:24;
    then m-'1 < l by NAT_1:38;
  then A664: m-'1 <> len(F.l) by A92;

    consider p1,p2,q1,q2 being Nat such that
  A665: [p1,p2] in Indices G and
  A666: p = G*(p1,p2) and
  A667: [q1,q2] in Indices G and
  A668: q = G*(q1,q2) and
  A669: p1 = q1 & p2+1 = q2 or p1+1 = q1 & p2 = q2 or
        p1 = q1+1 & p2 = q2 or p1 = q1 & p2 = q2+1 by A640,A641,JORDAN8:6;

  A670: 1 <= p1 & p1 <= len G & 1 <= p2 & p2 <= width G by A665,GOBOARD5:1;

    per cases by A669;
    suppose ::A
    A671: p1 = q1 & p2+1 = q2;
      consider a1,a2,p'1,p'2 being Nat such that
    A672: [a1,a2] in Indices G and
    A673: a = G*(a1,a2) and
    A674: [p'1,p'2] in Indices G and
    A675: p = G*(p'1,p'2) and
    A676: a1 = p'1 & a2+1 = p'2 or a1+1 = p'1 & a2 = p'2 or
          a1 = p'1+1 & a2 = p'2 or a1 = p'1 & a2 = p'2+1
            by A395,A396,A644,A645,A646,JORDAN8:6;
    A677: 1 <= a1 & a1 <= len G & 1 <= a2 & a2 <= width G by A672,GOBOARD5:1;

      thus thesis
      proof
        per cases by A676;
        suppose ::I
              a1 = p'1 & a2+1 = p'2;
        then A678: a1 = p1 & a2+1 = p2 by A665,A666,A674,A675,GOBOARD1:21;
        A679: F.m is_sequence_on G by A246;
        A680: m-'1+1 <= len (F.m) by A92,A645;
        A681: m-'1 <= m by A645,NAT_1:29;
        A682: F.k|(m+1)=F.(m+1) by A323,A394,A400;
          A683: p2+1 > a2+1 & a2+1 > a2 by A678,NAT_1:38;
        A684: f/.(m-'1) = (F.(m-'1))/.(m-'1) by A394,A401,A563,A646
                     .= (F.m)/.(m-'1) by A563,A646,A681;
        A685: f/.(m-'1+1) = (F.m)/.m by A394,A396,A563,A645;
              left_cell(f,l,G) = cell(G,p1,p2)
                 by A394,A395,A650,A653,A656,A657,A665,A666,A667,A668,A671,
GOBRD13:28
            .= front_right_cell(F.m,m-'1,G) by A644,A645,A646,A665,A666,A672,
A673,A678,A679,A680,A684,A685,GOBRD13:36;
          then F.(m+1) turns_right m-'1,G by A324,A610,A659;
          then f turns_right m-'1,G by A400,A646,A661,A682,GOBRD13:44;
        then A686: f/.(m+1) = G*(p1+1,p2) & [p1+1,p2] in Indices G
            by A644,A645,A661,A665,A666,A672,A673,A683,GOBRD13:def 6;
        then A687: 1 <= p1+1 & p1+1 <= len G by GOBOARD5:1;
          set rc = left_cell(rg,len rg-'1,G)\L~rg;
        A688: rc c= LeftComp rg by A640,A642,A643,JORDAN9:29;
        A689: p2-'1 = a2 & p1-'1+1 = p1 by A670,A678,AMI_5:4,BINARITH:39;
        A690: 2 in dom g by A559,FINSEQ_3:27;
               len rg-'1+2 = len g +1 by A655,FINSEQ_5:def 3;
        then A691: rg/.(len rg -' 1) = g/.2 by A690,FINSEQ_5:69
                           .= f/.(m+1) by A661,A690,FINSEQ_5:30;
            p = g/.1 by A393,A403,A404,FINSEQ_5:68
              .= rg/.len g by FINSEQ_5:68
              .= rg/.len rg by FINSEQ_5:def 3;
          then left_cell(rg,len rg-'1,G) = cell(G,p1,a2)
            by A640,A642,A643,A665,A666,A686,A689,A691,GOBRD13:26;
          then a in left_cell(rg,len rg-'1,G)
            by A670,A673,A677,A678,A687,JORDAN9:22;
          then A692: a in rc by A660,XBOOLE_0:def 4;
        A693: L~rg c= L~f by A578,SPPOL_2:22;
              LeftComp rg is_a_component_of (L~rg)` by GOBOARD9:def 1;
          hence C meets LeftComp Rev g by A396,A580,A645,A646,A660,A688,A692,
A693;

        end;
        suppose ::II
          A694: a1+1 = p'1 & a2 = p'2;
        then A695: a1+1 = p1 & a2 = p2 by A665,A666,A674,A675,GOBOARD1:21;
        then q1-'1 = a1 by A671,BINARITH:39;
          then right_cell(f,l,G) = cell(G,a1,a2)
                 by A394,A395,A650,A653,A656,A657,A665,A666,A667,A668,A671,A695
