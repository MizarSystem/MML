A561: for j,k st 1 <= j & j <= k holds (F.k)/.j = (F.j)/.j
  proof
    let j,k;
    assume that
  A562: 1 <= j and
  A563: j <= k;
  A564: (F.k)|j = F.j by A322,A563;
          j <= len(F.k) by A92,A563;
  then len(F.k|j) = j by FINSEQ_1:80;
      then j in dom((F.k)|j) by A562,FINSEQ_3:27;
      hence (F.k)/.j = (F.j)/.j by A564,FINSEQ_4:85;
  end;
    reconsider Lg' = (L~g)` as Subset of TOP-REAL 2;
A565:   C c= Lg'
    proof
     let c be set;
     assume that
A566:   c in C and
A567:   not c in Lg';
     reconsider c as Point of TOP-REAL 2 by A566;
         c in L~g by A567,SUBSET_1:50;
     then consider i such that
A568:   1 <= i & i+1 <= len g and
A569:   c in LSeg(g/.i,g/.(i+1)) by SPPOL_2:14;
         i in dom g & i+1 in dom g by A568,GOBOARD2:3;
then A570:   g/.i = f/.(i+(m-'1)) & g/.(i+1) = f/.(i+1+(m-'1)) by FINSEQ_5:30;
A571:   1 <= i+(m-'1) by A568,NAT_1:37;
   i+1+(m-'1) = i+(m-'1)+1;
     then A572: i+(m-'1)+1 <= (len g)+(m-'1) by A568,XREAL_1:8;
      c in LSeg(f,i+(m-'1)) by A401,A569,A570,A571,A572,TOPREAL1:def 5;
     then c in right_cell(f,i+(m-'1),G) /\ left_cell(f,i+(m-'1),G)
      by A394,A571,A572,A401,GOBRD13:30;
     then c in right_cell(f,i+(m-'1),G) by XBOOLE_0:def 3;
      then right_cell(f,i+(m-'1),G) meets C by A566,XBOOLE_0:3;
     hence contradiction by A246,A571,A572,A401;
    end;
A574:   TOP-REAL 2 = TopSpaceMetr (Euclid 2) by EUCLID:def 8;
        L~g is closed by SPPOL_1:49;
    then (L~g)` is open by TOPS_1:29;
then A575:  (L~g)` = Int (L~g)` by TOPS_1:55;
A576:  L~g c= L~f by JORDAN3:75;
A577:  LeftComp g is_a_component_of (L~g)` &
       RightComp g is_a_component_of (L~g)` by GOBOARD9:def 1,def 2;
A578: for h being standard non constant special_circular_sequence st L~h c= L~f
    for Comp being Subset of TOP-REAL 2 st Comp is_a_component_of (L~h)`
    for n st 1 <= n & n+1 <= len f & f/.n in Comp & not f/.n in L~h
      holds
     C meets Comp
  proof
    let h be standard non constant special_circular_sequence such that
  A579: L~h c= L~f;
    let Comp be Subset of TOP-REAL 2 such that
  A580: Comp is_a_component_of (L~h)`;
    let n such that
  A581: 1 <= n & n+1 <= len f and
  A582: f/.n in Comp and
  A583: not f/.n in L~h;

    set rc=left_cell(f,n,G)\L~h;
    reconsider rc as Subset of TOP-REAL 2;
  A584: rc c= left_cell(f,n,G) by XBOOLE_1:36;
  A585: rc = left_cell(f,n,G) /\ (L~h)` by SUBSET_1:32;

  A586: rc meets C
  proof
        left_cell(f,n,G) meets C by A246,A581;
    then consider p being set such that
  A587: p in left_cell(f,n,G) & p in C by XBOOLE_0:3;
    reconsider p as Element of TOP-REAL 2 by A587;
        now take p;
        now
     assume p in L~h;
      then consider j such that
    A588: 1 <= j and
    A589: j+1 <= len f and
    A590: p in LSeg(f,j) by A579,SPPOL_2:13;
          p in right_cell(f,j,G) /\ left_cell(f,j,G) by A394,A588,A589,A590,
GOBRD13:30;
      then A591: p in right_cell(f,j,G) by XBOOLE_0:def 3;
          right_cell(f,j,G) misses C by A246,A588,A589;
      hence contradiction by A587,A591,XBOOLE_0:3;
    end;
    hence p in rc by A587,XBOOLE_0:def 4;
    thus p in C by A587;
    end; hence thesis by XBOOLE_0:3;
  end;
  A592: Int left_cell(f,n,G) is connected by A394,A581,JORDAN9:12;
        Int left_cell(f,n,G) misses L~f by A394,A581,JORDAN9:17;
    then Int left_cell(f,n,G) misses L~h by A579,XBOOLE_1:63;
  then A593: Int left_cell(f,n,G) c= (L~h)` by SUBSET_1:43;
        Int left_cell(f,n,G) c= left_cell(f,n,G) by TOPS_1:44;
  then A594: Int left_cell(f,n,G) c= rc by A585,A593,XBOOLE_1:19;
        rc c= Cl Int left_cell(f,n,G) by A394,A581,A584,JORDAN9:13;
  then A595: rc is connected by A592,A594,CONNSP_1:19;
        f/.n in left_cell(f,n,G) by A394,A581,JORDAN9:10;
    then f/.n in rc by A583,XBOOLE_0:def 4;
  then A596: rc meets Comp by A582,XBOOLE_0:3;
        rc c= (L~h)` by A585,XBOOLE_1:17;
    then rc c= Comp by A580,A595,A596,GOBOARD9:6;
    hence C meets Comp by A586,XBOOLE_1:63;
  end;

A597:
   C meets LeftComp g
   proof
        left_cell(f,m,G) meets C by A246,A395,A399;
    then consider p being set such that
A598:  p in left_cell(f,m,G) & p in C by XBOOLE_0:3;
    reconsider p as Element of TOP-REAL 2 by A598;
        now take p;
    thus p in C by A598;
    reconsider u = p as Element of Euclid 2 by TOPREAL3:13;
    consider r being real number such that
A599:   r > 0 and
A600:   Ball(u,r) c= (L~g)` by A565,A575,A598,GOBOARD6:8;
    reconsider r as Real by XREAL_0:def 1;
A601:   p in Ball(u,r) by A599,TBSP_1:16;
    reconsider B = Ball(u,r) as non empty Subset of TOP-REAL 2
       by A574,A599,TBSP_1:16,TOPMETR:16;
A602:   B is connected by SPRECT_3:17;
        left_cell(g,1,G) c= left_cell(g,1) by A404,A558,GOBRD13:34;
then A603:   Int left_cell(g,1,G) c= Int left_cell(g,1) by TOPS_1:48;
        Int left_cell(g,1) c= LeftComp g by A558,GOBOARD9:24;
    then Int left_cell(g,1,G) c= LeftComp g by A603,XBOOLE_1:1;
    then Int left_cell(f,m-'1+1,G) c= LeftComp g by A394,A400,A558,GOBRD13:33;
    then A604:  Int left_cell(f,m,G) c= LeftComp g by A395,BINARITH:53;
A605:   left_cell(f,m,G) = Cl Int left_cell(f,m,G) by A395,A399,A443;
        B is open by GOBOARD6:6;
then A606:   Int left_cell(f,m,G) meets B by A598,A601,A605,TOPS_1:39;
A607:   p in B by A599,TBSP_1:16;
        B c= LeftComp g by A577,A600,A602,A604,A606,GOBOARD9:6;
    hence p in LeftComp g by A607;
    end; hence thesis by XBOOLE_0:3;
   end;

     m = 1
