    end;
    suppose
A421:    j < i;
     set l = m-'1+i, m'= m-'1+j;
A422:    m' < l by A421,XREAL_1:8;
A423:    l < k by A393,A401,A407,XREAL_1:22;
then A424:    f|l = F.l by A322;
