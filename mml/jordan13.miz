          hence contradiction by A405,A457,A459,A462,A510,A514,A515,A517,A525
,A550,A551;
        end;
        end;
        hence contradiction;
       end;
       suppose
A552:      i1 = i2 & j1 = j2+1 & i1'+1 = i2' & j1' = j2';
