         l in dom(F.l) by A425,A426,FINSEQ_3:27;
     then (F.l)/.l = f/.l by A424,FINSEQ_4:85;
     hence contradiction by A389,A409,A411,A412,A422,A423,A425,A426,A427,A428;
   end;
   end;
