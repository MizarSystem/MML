      C meets LeftComp Rev g
      proof
        1 <= len g by A703,XREAL_1:147;
        then
A930:   len g-'1+2 = len g+1 by A899;
A931:   1 - 1 < m - 1 by A900,XREAL_1:11;
A932:   m-'1+2 = m+1 by A651,A899;
