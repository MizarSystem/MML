      .=(-F.x) + G.(-x) by A1,A6,A7,A8,Def6
      .=(-F.x) + (-G.x) by A2,A5,A7,A8,Def6
      .=-(F.x + G.x)
      .=-(F + G).x by A7,VALUED_1:def 1;
