      .=-(-F.x) by A2,A1,A3,Def6
      .=-(-F).x by VALUED_1:8;
    hence thesis;
  end;
  then -F is with_symmetrical_domain quasi_odd by A2,A1,Def2,Def6;
  hence thesis;
end;

