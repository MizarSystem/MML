theorem Th26:
  for X, Y be RealNormSpace holds
  BoundedLinearOperators(X,Y) is lineary-closed
proof
  let X, Y be RealNormSpace;
  set W = BoundedLinearOperators(X,Y);
A1:  R_VectorSpace_of_LinearOperators(X,Y) =
  RLSStruct (# LinearOperators(X,Y),
             Zero_(LinearOperators(X,Y),RealVectSpace(the carrier of X,Y)),
           Add_(LinearOperators(X,Y), RealVectSpace(the carrier of X,Y)),
           Mult_(LinearOperators(X,Y), RealVectSpace(the carrier of X,Y)) #)
 by Def8;
A2:for v,u be VECTOR of R_VectorSpace_of_LinearOperators(X, Y) st
    v in W & u in W
    holds v + u in W
   proof
     let v,u be VECTOR of R_VectorSpace_of_LinearOperators(X, Y) such that
     A3: v in W & u in W;
     reconsider f=v+u as LinearOperator of X,Y by A1,Def7;
        f is bounded
     proof
     reconsider u1=u as bounded LinearOperator of X,Y by A3,Def10;
     reconsider v1=v as bounded LinearOperator of X,Y by A3,Def10;
     consider K1 be Real such that
       A4: 0 <= K1 &
     for x be VECTOR of X holds ||. u1.x .|| <= K1*||. x .|| by Def9;
     consider K2 be Real such that
       A5: 0 <= K2 &
     for x be VECTOR of X holds ||. v1.x .|| <= K2*||. x .|| by Def9;
     take K3=K1+K2;
A6:         0 + 0 <= K1+K2 by A4,A5,REAL_1:55;
         now let x be VECTOR of X;
        A7: ||. f.x .|| =||. u1.x+v1.x .|| by Th20;
        A8: ||. u1.x+v1.x .|| <= ||. u1.x .||+ ||. v1.x .|| by NORMSP_1:def 2;
        A9:||. u1.x .|| <= K1*||. x .|| by A4;
         ||. v1.x .|| <= K2*||. x .|| by A5;
        then ||. u1.x .|| + ||. v1.x .|| <= K1*||. x .|| +K2*||. x .||
      by A9,REAL_1:55;
       hence ||. f.x .|| <= K3*||. x .|| by A7,A8,AXIOMS:22;
        end;
     hence thesis by A6;
     end;
     hence thesis by Def10;
  end;

  for a be Real for v be VECTOR of R_VectorSpace_of_LinearOperators(X,Y)
    st v in W holds a * v in W
    proof
     let a be Real;
     let v be VECTOR of R_VectorSpace_of_LinearOperators(X,Y) such that
           A10: v in W;
          reconsider f=a*v as LinearOperator of X,Y by A1,Def7;
            f is bounded
          proof
     reconsider v1=v as bounded LinearOperator of X,Y by A10,Def10;
     consider K be Real such that
A11: 0 <= K &
        for x be VECTOR of X holds ||. v1.x .|| <= K*||. x .|| by Def9;
      take K1=abs(a)*K;
A12:      0 <= K & 0 <=abs(a) by A11,ABSVALUE:5;
        now let x be VECTOR of X;
        A13: ||. a*v1.x .|| = abs(a)* ||. v1.x .|| by NORMSP_1:def 2;
        A14:||. v1.x .|| <= K*||. x .|| by A11;
         0 <=abs(a) by ABSVALUE:5;
         then abs(a)* ||. v1.x .|| <= abs(a)* (K*||. x .||) by A14,AXIOMS:25;
       hence ||. f.x .|| <= abs(a)* K*||. x .|| by A13,Th21;
     end;
     hence thesis by A12,REAL_2:121;
     end;
     hence thesis by Def10;
     end;
 hence thesis by A2,RLSUB_1:def 1;
end;

theorem Th27:
  for X, Y be RealNormSpace holds
RLSStruct (# BoundedLinearOperators(X,Y),
        Zero_(BoundedLinearOperators(X,Y),
        R_VectorSpace_of_LinearOperators(X,Y)),
         Add_(BoundedLinearOperators(X,Y),
