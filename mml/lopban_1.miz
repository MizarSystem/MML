theorem Th29:
  for X, Y be RealNormSpace
  for f,g,h be VECTOR of R_VectorSpace_of_BoundedLinearOperators(X,Y) holds
      h = f+g iff for x be VECTOR of X holds h.x = f.x + g.x
proof
     let X, Y be RealNormSpace;
     let f,g,h be VECTOR of R_VectorSpace_of_BoundedLinearOperators(X,Y);
A1:  R_VectorSpace_of_BoundedLinearOperators(X,Y) =
  RLSStruct (# BoundedLinearOperators(X,Y),
        Zero_(BoundedLinearOperators(X,Y),
        R_VectorSpace_of_LinearOperators(X,Y)),
         Add_(BoundedLinearOperators(X,Y),
         R_VectorSpace_of_LinearOperators(X,Y)),
          Mult_(BoundedLinearOperators(X,Y),
          R_VectorSpace_of_LinearOperators(X,Y)) #) by Def11;
      then reconsider f'=f,g'=g,h'=h as bounded LinearOperator of X,Y by Def10;
      A2:R_VectorSpace_of_BoundedLinearOperators(X,Y) is
           Subspace of R_VectorSpace_of_LinearOperators(X,Y) by A1,Th27;
        then reconsider f1=f as VECTOR
           of R_VectorSpace_of_LinearOperators(X,Y) by RLSUB_1:18;
       reconsider g1=g as VECTOR
          of R_VectorSpace_of_LinearOperators(X,Y) by A2,RLSUB_1:18;
       reconsider h1=h as VECTOR
         of R_VectorSpace_of_LinearOperators(X,Y) by A2,RLSUB_1:18;
       reconsider f1'=f' as Element
            of Funcs(the carrier of X, the carrier of Y) by FUNCT_2:11;
       reconsider g1'=g' as Element
            of Funcs(the carrier of X, the carrier of Y) by FUNCT_2:11;
       reconsider h1'=h' as Element
            of Funcs(the carrier of X, the carrier of Y) by FUNCT_2:11;
      hereby assume
            A3: h = f+g;
             let x be Element of X;
                    h1=f1+g1 by A2,A3,RLSUB_1:21;
                       hence h.x=f.x+g.x by Th20;
    end;
    assume
              for x be Element of X holds h.x=f.x+g.x;
           then h1=f1+g1 by Th20;
           hence h =f+g by A2,RLSUB_1:21;
 end;

theorem Th30:
  for X, Y be RealNormSpace
  for f,h be VECTOR of R_VectorSpace_of_BoundedLinearOperators(X,Y)
  for a be Real holds
      h = a*f iff for x be VECTOR of X holds h.x = a * f.x
  proof
     let X, Y be RealNormSpace;
     let f,h be VECTOR of R_VectorSpace_of_BoundedLinearOperators(X,Y);
     let a be Real;
A1:  R_VectorSpace_of_BoundedLinearOperators(X,Y) =
  RLSStruct (# BoundedLinearOperators(X,Y),
        Zero_(BoundedLinearOperators(X,Y),
        R_VectorSpace_of_LinearOperators(X,Y)),
         Add_(BoundedLinearOperators(X,Y),
         R_VectorSpace_of_LinearOperators(X,Y)),
          Mult_(BoundedLinearOperators(X,Y),
         R_VectorSpace_of_LinearOperators(X,Y)) #) by Def11;
     then reconsider f'=f,h'=h as bounded LinearOperator of X,Y by Def10;
      A2:R_VectorSpace_of_BoundedLinearOperators(X,Y) is
           Subspace of R_VectorSpace_of_LinearOperators(X,Y)
        by A1,Th27;
        then reconsider f1=f as VECTOR
                   of R_VectorSpace_of_LinearOperators(X,Y) by RLSUB_1:18;
       reconsider h1=h as VECTOR
                of R_VectorSpace_of_LinearOperators(X,Y) by A2,RLSUB_1:18;
       reconsider f1'=f' as Element
            of Funcs(the carrier of X, the carrier of Y) by FUNCT_2:11;
       reconsider h1'=h' as Element
            of Funcs(the carrier of X, the carrier of Y) by FUNCT_2:11;
      hereby assume
            A3: h = a*f;
             let x be Element of X;
               h1=a*f1 by A2,A3,RLSUB_1:22;
             hence h.x=a*f.x by Th21;
    end;
    assume
              for x be Element of X holds h.x=a*f.x;
           then h1=a*f1 by Th21;
           hence h =a*f by A2,RLSUB_1:22;
 end;

