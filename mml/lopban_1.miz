hence thesis by A23,XREAL_1:1;
end;
    ||.f+g.|| <= ||.f.|| +  ||.g.||
proof
reconsider f1=f, g1=g, h1=f+g as bounded LinearOperator of X,Y by Def10;
A40:
now let t be VECTOR of X such that
  A41: ||.t.|| <= 1;
 A42:  ||.h1.t.||= ||.f1.t+g1.t.|| by Th41;
 A43:  ||.f1.t+g1.t.|| <=||.f1.t.||+||.g1.t.|| by NORMSP_1:def 2;
 A44:   ||.f1.t.||<= ||.f.||*||.t.|| by Th38;
     ||.g1.t.||<= ||.g.||*||.t.|| by Th38;
 then A45:  ||.f1.t.||+||.g1.t.|| <= ||.f.||*||.t.|| +  ||.g.||*||.t.||
 by A44,XREAL_1:9;
     0 <= ||.f.|| by Th39;
 then A46:  ||.f.||*||.t.|| <= ||.f.||*1 by A41,XREAL_1:66;
     0 <= ||.g.|| by Th39;
 then ||.g.||*||.t.|| <= ||.g.||*1 by A41,XREAL_1:66;
    then ||.f.||*||.t.|| + ||.g.||*||.t.|| <= ||.f.||*1 + ||.g.||*1
 by A46,XREAL_1:9;
   then ||.f1.t.||+||.g1.t.|| <= ||.f.|| + ||.g.|| by A45,XREAL_1:2;
    hence ||.h1.t.|| <= ||.f.|| + ||.g.|| by A42,A43,XREAL_1:2;
end;
A47:
     now let r be Real such that
        A48:  r in PreNorms(h1);
           r in {||.h1.t.|| where t is VECTOR of X : ||.t.|| <= 1 }
    by A48;
        then consider t be VECTOR of X such that
          A49:   r=||.h1.t.|| & ||.t.|| <= 1;
          thus r <= ||.f.|| + ||.g.|| by A40,A49;
  end;
A50:
(for s be real number st s in PreNorms(h1) holds s <= ||.f.|| + ||.g.||)
implies sup PreNorms(h1) <= ||.f.|| + ||.g.|| by SEQ_4:62;
  BoundedLinearOperatorsNorm(X,Y).(f+g) = sup PreNorms(h1) by Th36;
