theorem Th38:
    for X, Y be RealNormSpace
    for f being Point of
       R_NormSpace_of_BoundedLinearOperators(X,Y)
    for g be bounded LinearOperator of X,Y st g=f holds
      for t be VECTOR of X holds ||.g.t.|| <= ||.f.|| * ||.t.||
   proof
       let X, Y be RealNormSpace;
       let f being Point of
          R_NormSpace_of_BoundedLinearOperators(X,Y);
      let g be bounded LinearOperator of X,Y such that
     A1:g=f;
A2: R_NormSpace_of_BoundedLinearOperators(X,Y)
     = NORMSTR (# BoundedLinearOperators(X,Y),
         Zero_(BoundedLinearOperators(X,Y),
         R_VectorSpace_of_LinearOperators(X,Y)),
          Add_(BoundedLinearOperators(X,Y),
          R_VectorSpace_of_LinearOperators(X,Y)),
           Mult_(BoundedLinearOperators(X,Y),
           R_VectorSpace_of_LinearOperators(X,Y)),
           BoundedLinearOperatorsNorm(X,Y) #) by Def15;
 A3: PreNorms(g) is non empty bounded_above by Th32;
      now let t be VECTOR of X;
         now per cases;
         case A4: t  = 0.X;
             then A5: g.t  =g.(0*0.X) by RLVECT_1:23
                   .=0*g.(0.X) by Def6
                   .=0.Y by RLVECT_1:23;
                    ||.t.|| = 0 by A4,NORMSP_1:def 2;
            hence ||.g.t.|| <= ||.f.||*||.t.|| by A5,NORMSP_1:def 2;
       end;
       case t <> 0.X;
          then A6:||.t.|| <> 0 by NORMSP_1:def 2;
          then A7: ||.t.|| > 0 by NORMSP_1:8;
          A8:abs( ||.t.||")
          =abs( 1*||.t.||")
          .=abs( 1/||.t.||) by XCMPLX_0:def 9
          .=1/abs( ||.t.||) by ABSVALUE:15
          .=1/||.t.|| by A7,ABSVALUE:def 1
          .=1*||.t.||" by XCMPLX_0:def 9
          .=||.t.||";
          reconsider t1= ( ||.t.||")*t as VECTOR of X;
          A9: ||.t1.|| =abs( ||.t.||")*||.t.|| by NORMSP_1:def 2
                      .=1 by A6,A8,XCMPLX_0:def 7;
           ||.g.t.||/||.t.|| = ||.g.t.||*||.t.||" by XCMPLX_0:def 9
                          .=||. ||.t.||"*g.t.|| by A8,NORMSP_1:def 2
                          .=||.g.t1.|| by Def6;
        then ||.g.t.||/||.t.|| in
              {||.g.s.|| where s is VECTOR of X : ||.s.|| <= 1 } by A9;
          then ||.g.t.||/||.t.|| in PreNorms(g) by Def13;
          then ||.g.t.||/||.t.||
              <= sup PreNorms(g) by A3,SEQ_4:def 4;
          then ||.g.t.||/||.t.||
              <= BoundedLinearOperatorsNorm(X,Y).g by Th36;
           then A10: ||.g.t.||/||.t.|| <= ||.f.|| by A1,A2,NORMSP_1:def 1;
              ||.g.t.||/||.t.||*||.t.||
             = ||.g.t.||*||.t.||"*||.t.|| by XCMPLX_0:def 9
            .=||.g.t.||*(||.t.||"*||.t.||)
            .=||.g.t.||*1 by A6,XCMPLX_0:def 7
            .=||.g.t.||;
           hence ||.g.t.|| <= ||.f.||*||.t.|| by A7,A10,AXIOMS:25;
        end;
        end;
     hence ||.g.t.|| <= ||.f.||*||.t.||;
    end;
    hence thesis;
