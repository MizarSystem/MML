  A43: ||.t.|| <= 1;
 A44:  ||.h1.t.||= ||.f1.t+g1.t.|| by Th41;
 A45:  ||.f1.t+g1.t.|| <=||.f1.t.||+||.g1.t.|| by NORMSP_1:def 2;
 A46:   ||.f1.t.||<= ||.f.||*||.t.|| by Th38;
     ||.g1.t.||<= ||.g.||*||.t.|| by Th38;
 then A47:  ||.f1.t.||+||.g1.t.|| <= ||.f.||*||.t.|| +  ||.g.||*||.t.||
 by A46,REAL_1:55;
     0 <= ||.f.|| by Th39;
 then A48:  ||.f.||*||.t.|| <= ||.f.||*1 by A43,AXIOMS:25;
     0 <= ||.g.|| by Th39;
 then ||.g.||*||.t.|| <= ||.g.||*1 by A43,AXIOMS:25;
    then ||.f.||*||.t.|| + ||.g.||*||.t.|| <= ||.f.||*1 + ||.g.||*1
 by A48,REAL_1:55;
   then ||.f1.t.||+||.g1.t.|| <= ||.f.|| + ||.g.|| by A47,AXIOMS:22;
    hence ||.h1.t.|| <= ||.f.|| + ||.g.|| by A44,A45,AXIOMS:22;
end;
A49:
     now let r be Real such that
        A50:  r in PreNorms(h1);
           r in {||.h1.t.|| where t is VECTOR of X : ||.t.|| <= 1 }
    by A50,Def13;
        then consider t be VECTOR of X such that
          A51:   r=||.h1.t.|| & ||.t.|| <= 1;
          thus r <= ||.f.|| + ||.g.|| by A42,A51;
  end;
A52:
(for s be real number st s in PreNorms(h1) holds s <= ||.f.|| + ||.g.||)
implies sup PreNorms(h1) <= ||.f.|| + ||.g.|| by SEQ_4:62;
  BoundedLinearOperatorsNorm(X,Y).(f+g) = sup PreNorms(h1) by Th36;
hence ||.f+g.|| <=||.f.|| + ||.g.|| by A1,A49,A52,NORMSP_1:def 1;
end;
hence thesis by A3,A5,A14;
end;

theorem Th44:
  for X,Y be RealNormSpace holds
  R_NormSpace_of_BoundedLinearOperators(X,Y) is RealNormSpace-like
      proof
       let X,Y be RealNormSpace;
      for x, y being Point of R_NormSpace_of_BoundedLinearOperators(X,Y)
       for a be Real holds
  ( ||.x.|| = 0 iff
  x = 0.R_NormSpace_of_BoundedLinearOperators(X,Y) ) &
  ||.a*x.|| = abs(a) * ||.x.|| &
  ||.x+y.|| <= ||.x.|| +  ||.y.|| by Th43;
   hence thesis by NORMSP_1:def 2;
  end;

theorem Th45:
  for X, Y be RealNormSpace holds
    R_NormSpace_of_BoundedLinearOperators(X,Y) is RealNormSpace
     proof
    let X, Y be RealNormSpace;
A1: R_NormSpace_of_BoundedLinearOperators(X,Y)
  = NORMSTR (# BoundedLinearOperators(X,Y),
         Zero_(BoundedLinearOperators(X,Y),
         R_VectorSpace_of_LinearOperators(X,Y)),
          Add_(BoundedLinearOperators(X,Y),
          R_VectorSpace_of_LinearOperators(X,Y)),
           Mult_(BoundedLinearOperators(X,Y),
           R_VectorSpace_of_LinearOperators(X,Y)),
           BoundedLinearOperatorsNorm(X,Y) #) by Def15;
      RLSStruct (# BoundedLinearOperators(X,Y),
            Zero_(BoundedLinearOperators(X,Y),
            R_VectorSpace_of_LinearOperators(X,Y)),
             Add_(BoundedLinearOperators(X,Y),
             R_VectorSpace_of_LinearOperators(X,Y)),
            Mult_(BoundedLinearOperators(X,Y),
            R_VectorSpace_of_LinearOperators(X,Y)) #) is RealLinearSpace;
      hence thesis by A1,Th44,RSSPACE3:4;
     end;

registration
   let X, Y be RealNormSpace;
   cluster R_NormSpace_of_BoundedLinearOperators(X,Y) ->
               RealNormSpace-like RealLinearSpace-like
               Abelian add-associative right_zeroed right_complementable;
   coherence by Th45;
end;

theorem Th46:
  for X, Y be RealNormSpace
  for f,g,h be Point of R_NormSpace_of_BoundedLinearOperators(X,Y) holds
      h = f-g iff for x be VECTOR of X holds h.x = f.x - g.x
proof
     let X, Y be RealNormSpace;
     let f,g,h be Point of R_NormSpace_of_BoundedLinearOperators(X,Y);
   R_NormSpace_of_BoundedLinearOperators(X,Y)
  = NORMSTR (# BoundedLinearOperators(X,Y),
         Zero_(BoundedLinearOperators(X,Y),
         R_VectorSpace_of_LinearOperators(X,Y)),
          Add_(BoundedLinearOperators(X,Y),
          R_VectorSpace_of_LinearOperators(X,Y)),
           Mult_(BoundedLinearOperators(X,Y),
           R_VectorSpace_of_LinearOperators(X,Y)),
           BoundedLinearOperatorsNorm(X,Y) #) by Def15;
      then reconsider f'=f,g'=g,h'=h as bounded LinearOperator of X,Y by Def10;
hereby assume h=f-g;
     then h+g=f-(g-g) by RLVECT_1:43;
     then h+g=f-0.R_NormSpace_of_BoundedLinearOperators(X,Y) by RLVECT_1:28;
     then A1: h+g=f by RLVECT_1:26;
       now let x be VECTOR of X;
         f'.x=h'.x +  g'.x by A1,Th41;
       then f'.x-g'.x=h'.x + (g'.x-g'.x) by RLVECT_1:42;
       then f'.x-g'.x=h'.x + 0.Y by RLVECT_1:28;
       hence f'.x-g'.x=h'.x by RLVECT_1:10;
     end;
     hence for x be VECTOR of X holds h.x = f.x - g.x;
end;
assume
     A2: for x be VECTOR of X holds h.x = f.x - g.x;
       now let x be VECTOR of X;
         h'.x = f'.x - g'.x by A2;
       then h'.x + g'.x= f'.x - (g'.x- g'.x) by RLVECT_1:43;
       then h'.x + g'.x= f'.x - 0.Y by RLVECT_1:28;
       hence h'.x + g'.x= f'.x by RLVECT_1:26;
     end;
     then f=h+g by Th41;
     then f-g=h+(g-g) by RLVECT_1:42;
     then f-g=h+0.R_NormSpace_of_BoundedLinearOperators(X,Y) by RLVECT_1:28;
     hence f-g=h by RLVECT_1:10;
end;

begin :: Real Banach Space of Bounded Linear Operators

definition let X be RealNormSpace;
  attr X is complete means :Def16:
    for seq be sequence of X holds
    seq is Cauchy_sequence_by_Norm implies seq is convergent;
end;

registration
  cluster complete RealNormSpace;
  existence
  proof
    take l1_Space;
    thus thesis by Def16,RSSPACE3:11;
  end;
end;

definition
  mode RealBanachSpace is complete RealNormSpace;
end;

Lm3:
for e be Real, seq be Real_Sequence st ( seq is convergent &
 ex k be Nat st (for i be Nat st k <= i holds seq.i <=e ) ) holds
  lim seq <=e
proof
   let e be Real;
   let seq be Real_Sequence such that
A1:seq is convergent and
A2:ex k be Nat st (for i be Nat st k <= i holds seq.i <=e );
   reconsider cseq = NAT --> e as Real_Sequence by FUNCOP_1:57;
A3:for n be Nat holds cseq.n=e by FUNCOP_1:13;
   reconsider e1=e as real number;
A5:cseq is convergent by SEQ_4:39;
   consider k be Nat such that
A6:for i be Nat st k <= i holds seq.i <=e by A2;
A7:(seq ^\k) is convergent by A1,SEQ_4:33;
A8:lim (seq ^\k)=lim seq by A1,SEQ_4:33;
A9:now let i be Nat;
A10: (seq ^\k).i = seq.(k+i) by SEQM_3:def 9;
       k <= k+i by NAT_1:29;
     then seq.(k+i) <=e by A6;
     hence (seq ^\k) .i <= cseq.i by A3,A10;
  end;
    lim cseq = cseq.0 by SEQ_4:41 .= e by A3;
  hence thesis by A5,A7,A8,A9,SEQ_2:32;
end;

theorem Th47:
    for X be RealNormSpace
      for seq be sequence of X
        st seq is convergent
       holds ||.seq.|| is convergent & lim ||.seq.|| = ||.lim seq.||
proof
 let X be RealNormSpace;
 let seq be sequence of X such that
 A1:  seq is convergent;
 A2: now let e1 be real number such that
   A3: e1 >0;
   reconsider e =e1 as Real by XREAL_0:def 1;
   consider k be Nat such that
   A4:      for n be Nat st n >= k holds ||.seq.n - lim seq.|| < e
              by A1,A3,NORMSP_1:def 11;
   A5:
   now let m be Nat such that
      A6:   m >= k;
          ||.seq.m.||= ||.seq.||.m by NORMSP_1:def 10;
      then A7:    abs( ||.seq.||.m - ||.lim seq.||  ) <= ||.seq.m - lim seq.||
            by NORMSP_1:13;
           ||.seq.m - lim seq.||  <e by A4,A6;
      hence abs( ||.seq.||.m - ||.lim seq.||  ) <e1
      by A7,AXIOMS:22;
   end;
   take k;
   thus for m be Nat st m >= k holds
             abs(||.seq.||.m - ||.lim seq.|| ) < e1 by A5;
 end;
 then ||.seq.|| is convergent by SEQ_2:def 6;
 hence thesis by A2,SEQ_2:def 7;
end;

theorem Th48:
    for X, Y be RealNormSpace st Y is complete
      for seq be sequence of R_NormSpace_of_BoundedLinearOperators(X,Y)
               st seq is Cauchy_sequence_by_Norm
 holds seq is convergent
proof
    let X, Y be RealNormSpace such that
 A1:   Y is complete;
    let vseq be sequence of R_NormSpace_of_BoundedLinearOperators(X,Y)
     such that
A2: vseq is Cauchy_sequence_by_Norm;
A3:  ||.vseq.|| is convergent
proof
   now let e1 be real number such that
   A4: e1 >0;
   reconsider e =e1 as Real by XREAL_0:def 1;
          consider k be Nat such that
   A5:      for n, m be Nat st ( n >= k & m >= k ) holds
                ||.(vseq.n) - (vseq.m).|| < e
              by A2,A4,RSSPACE3:10;
A6: now
      let m be Nat such that
      A7:   m >= k;
      A8:  abs( ||.vseq.m.||- ||.vseq.k.|| )
         <= ||.(vseq.m) - (vseq.k).|| by NORMSP_1:13;
      A9:   ||.vseq.k.||= ||.vseq.||.k by NORMSP_1:def 10;
      A10:   ||.vseq.m.||= ||.vseq.||.m by NORMSP_1:def 10;
           ||.(vseq.m) - (vseq.k).|| <e by A5,A7;
      hence abs( ||.vseq.||.m - ||.vseq.||.k  ) <e1
      by A8,A9,A10,AXIOMS:22;
   end;
   take k;
   thus for m be Nat st m >= k holds
             abs(||.vseq.||.m - ||.vseq.||.k ) < e1 by A6;
 end;
 hence ||.vseq.|| is convergent by SEQ_4:58;
end;
    defpred P[set, set] means ex xseq be sequence of Y st
    (for n be Nat holds xseq.n=modetrans((vseq.n),X,Y).$1)
    & xseq is convergent & $2= lim xseq;
A11: R_NormSpace_of_BoundedLinearOperators(X,Y)
    = NORMSTR (# BoundedLinearOperators(X,Y),
           Zero_(BoundedLinearOperators(X,Y),
           R_VectorSpace_of_LinearOperators(X,Y)),
            Add_(BoundedLinearOperators(X,Y),
            R_VectorSpace_of_LinearOperators(X,Y)),
             Mult_(BoundedLinearOperators(X,Y),
             R_VectorSpace_of_LinearOperators(X,Y)),
             BoundedLinearOperatorsNorm(X,Y) #) by Def15;
A12: for x be Element of X ex y be Element of Y st  P[x,y]
   proof
       let x be Element of X;
       deffunc F(Nat) = modetrans((vseq.$1),X,Y).x;
       consider xseq be sequence of Y such that
A13:  for n be Nat holds xseq.n = F(n) from FUNCT_2:sch 4;
A14: for m,k be Nat holds
             ||.xseq.m-xseq.k.|| <= ||.vseq.m - vseq.k.|| * ||.x.||
      proof
      let m,k be Nat;
           vseq.m is bounded LinearOperator of X,Y by A11,Def10;
then A15:         modetrans((vseq.m),X,Y)=vseq.m by Th35;
            vseq.k is bounded LinearOperator of X,Y by A11,Def10;
then A16:         modetrans((vseq.k),X,Y)=vseq.k by Th35;
         reconsider h1=vseq.m-vseq.k as bounded LinearOperator of X,Y by A11,
Def10;
           A17:  xseq.m =modetrans((vseq.m),X,Y).x by A13;
              xseq.k =modetrans((vseq.k),X,Y).x by A13;
           then xseq.m - xseq.k = h1.x by A15,A16,A17,Th46;
          hence thesis by Th38;
         end;
A18: xseq is convergent
     proof
          now
            let e be Real such that
A19:          e > 0;
               thus ex k be Nat st
                for n, m be Nat st ( n >= k & m >= k )
                holds ||.xseq.n -xseq.m.|| < e
                proof
                   now per cases;
                   case A20:x=0.X;
                   take k=0;
                   thus for n, m be Nat st ( n >= k & m >= k ) holds
                   ||.xseq.n -xseq.m.|| < e
                   proof
                     let n, m be Nat such that n >= k & m >= k;
                     A21: xseq.n=modetrans((vseq.n),X,Y).x by A13
                        .=modetrans((vseq.n),X,Y).(0*x) by A20,RLVECT_1:23
                        .=0*modetrans((vseq.n),X,Y).x by Def6
                        .=0.Y by RLVECT_1:23;
                       xseq.m=modetrans((vseq.m),X,Y).x by A13
                        .=modetrans((vseq.m),X,Y).(0*x) by A20,RLVECT_1:23
                        .=0*modetrans((vseq.m),X,Y).x by Def6
                        .=0.Y by RLVECT_1:23;
                     then ||.xseq.n -xseq.m.|| = ||.0.Y.|| by A21,RLVECT_1:26
                        .=0 by NORMSP_1:def 2;
                     hence ||.xseq.n -xseq.m.|| < e by A19;
                   end;
                   end;
                   case x <>0.X;
                   then A22:  ||.x.|| <> 0 by NORMSP_1:def 2;
                   then A23:  ||.x.|| > 0 by NORMSP_1:8;
                   then e/||.x.||>0 by A19,SEQ_2:6;
                   then consider k be Nat such that
A24:                for n, m be Nat st n >= k & m >= k holds
                     ||.(vseq.n) - (vseq.m).|| < e/||.x.||
                        by A2,RSSPACE3:10;
                   take k;
                   thus for n, m be Nat st ( n >= k & m >= k )
                     holds ||.xseq.n-xseq.m.|| < e
                   proof
                     let n,m be Nat such that
A25:                  n >=k & m >= k;
A26:                 ||.xseq.n-xseq.m.||  <=
                     ||.(vseq.n) - (vseq.m).|| * ||.x.|| by A14;
                       ||.(vseq.n) - (vseq.m).|| < e/||.x.|| by A24,A25;
then A27:                 ||.(vseq.n) - (vseq.m).|| * ||.x.||
                       < e/||.x.|| * ||.x.|| by A23,REAL_1:70;
                       e/||.x.|| * ||.x.|| = e*||.x.||"* ||.x.||
                            by XCMPLX_0:def 9
                         .= e*(||.x.||"* ||.x.||)
                         .= e*1 by A22,XCMPLX_0:def 7
                         .=e;
                     hence ||.xseq.n-xseq.m.|| < e by A26,A27,AXIOMS:22;
                   end;
                 end;
                 end;
                 hence thesis;
               end;
            end;
            then xseq is Cauchy_sequence_by_Norm by RSSPACE3:10;
         hence thesis by A1,Def16;
         end;
  take y = lim xseq;
  thus thesis by A13,A18;
  end;
   consider f be Function of the carrier of X,the carrier of Y such that
A28:for x be Element of X holds P[x,f.x] from FUNCT_2:sch 3(A12);
   reconsider tseq=f as Function of X,Y;
  tseq is LinearOperator of X,Y
proof
A29: now let x,y be VECTOR of X;
  consider xseq be sequence of Y such that
  A30: (for n be Nat holds xseq.n=modetrans((vseq.n),X,Y).x)
       & xseq is convergent
       & tseq.x = lim xseq by A28;
  consider yseq be sequence of Y such that
  A31: (for n be Nat holds yseq.n=modetrans((vseq.n),X,Y).y)
       & yseq is convergent
       & tseq.y = lim yseq by A28;
  consider zseq be sequence of Y such that
  A32: (for n be Nat holds zseq.n=modetrans((vseq.n),X,Y).(x+y))
       & zseq is convergent
       & tseq.(x+y) = lim zseq by A28;
    zseq=xseq+yseq
  proof
       now let n be Nat;
     thus zseq.n=modetrans((vseq.n),X,Y).(x+y) by A32
         .= modetrans((vseq.n),X,Y).x+modetrans((vseq.n),X,Y).y
          by Def5
         .= xseq.n + modetrans((vseq.n),X,Y).y by A30
         .= xseq.n +yseq.n by A31;
      end;
    hence thesis by NORMSP_1:def 5;
  end;
 hence tseq.(x+y)=tseq.x+tseq.y by A30,A31,A32,NORMSP_1:42;
end;
  now
       let x be VECTOR of X;
       let a be Real;
  consider xseq be sequence of Y such that
  A33: (for n be Nat holds xseq.n=modetrans((vseq.n),X,Y).x)
       & xseq is convergent
       & tseq.x = lim xseq by A28;
  consider zseq be sequence of Y such that
  A34: (for n be Nat holds zseq.n=modetrans((vseq.n),X,Y).(a*x))
       & zseq is convergent
       & tseq.(a*x) = lim zseq by A28;
    zseq=a*xseq
  proof
       now let n be Nat;
     thus zseq.n=modetrans((vseq.n),X,Y).(a*x) by A34
         .= a*modetrans((vseq.n),X,Y).x by Def6
         .= a*xseq.n by A33;
      end;
    hence thesis by NORMSP_1:def 8;
  end;
 hence tseq.(a*x)=a*tseq.x by A33,A34,NORMSP_1:45;
end;
hence thesis by A29,Def5,Def6;
end;
then reconsider tseq as LinearOperator of X,Y;
A35: tseq is bounded
proof
A36: now
       let x be VECTOR of X;
       consider xseq be sequence of Y such that
  A37: (for n be Nat holds  xseq.n=modetrans((vseq.n),X,Y).x)
       & xseq is convergent
       & tseq.x = lim xseq by A28;
A38: for m be Nat holds
             ||.xseq.m.|| <= ||.vseq.m.|| * ||.x.||
      proof
      let m be Nat;
      vseq.m is bounded LinearOperator of X,Y by A11,Def10;
then A39:         modetrans((vseq.m),X,Y)=vseq.m by Th35;
         reconsider h1=vseq.m as bounded LinearOperator of X,Y by A11,Def10;
            xseq.m =modetrans((vseq.m),X,Y).x by A37;
          hence thesis by A39,Th38;
         end;
  A40:     ||.xseq.|| is convergent by A37,Th24;
  A41:     ||.tseq.x.|| = lim ||.xseq.|| by A37,Th24;
  A42:    for n be Nat holds ||.xseq.||.n  <=( ||.x.||(#)||.vseq.||).n
            proof
            let n be Nat;
             A43: ||.xseq.||.n = ||.(xseq.n).|| by NORMSP_1:def 10;
             A44: ||.(xseq.n).|| <= ||.vseq.n.|| * ||.x.|| by A38;
               ||.vseq.n.|| = ||.vseq.||.n by NORMSP_1:def 10;
             hence thesis by A43,A44,SEQ_1:13;
            end;
   A45:  ||.x.||(#)||.vseq.|| is convergent by A3,SEQ_2:21;
      lim ( ||.x.||(#)||.vseq.|| ) = lim (||.vseq.|| )* ||.x.||
 by A3,SEQ_2:22;
   hence ||.tseq.x.|| <= lim (||.vseq.|| )* ||.x.|| by A40,A41,A42,A45,SEQ_2:32
;
    end;
   take K= lim (||.vseq.|| );
      K >=0
    proof
       now let n be Nat;
           ||.vseq.n.|| >=0 by NORMSP_1:8;
         hence ||.vseq.||.n >=0 by NORMSP_1:def 10;
      end;
     hence thesis by A3,SEQ_2:31;
    end;
    hence thesis by A36;
end;

A46:
   for e be Real st
    e > 0 ex k be Nat st for n be Nat st
     n >= k holds
        for x be VECTOR of X holds
            ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e* ||.x.||
   proof
     let e be Real such that
      A47:e > 0;
      consider k be Nat such that
      A48:  for n, m be Nat st ( n >= k & m >= k ) holds
             ||.(vseq.n) - (vseq.m).|| < e
                        by A2,A47,RSSPACE3:10;
      A49: now let n be Nat such that
         A50: n >= k;
             now let x be VECTOR of X;
              consider xseq be sequence of Y such that
              A51:
              (for n be Nat holds  xseq.n=modetrans((vseq.n),X,Y).x)
                & xseq is convergent
                & tseq.x = lim xseq by A28;
               A52:
                for m,k be Nat holds
                 ||.xseq.m-xseq.k.|| <= ||.vseq.m - vseq.k.|| * ||.x.||
                proof
                let m,k be Nat;
                  vseq.m is bounded LinearOperator of X,Y by A11,Def10;
                 then A53:           modetrans((vseq.m),X,Y)=vseq.m by Th35;
                  vseq.k is bounded LinearOperator of X,Y by A11,Def10;
                then A54:            modetrans((vseq.k),X,Y)=vseq.k by Th35;
                reconsider h1=vseq.m-vseq.k
              as bounded LinearOperator of X,Y by A11,Def10;
                A55:  xseq.m =modetrans((vseq.m),X,Y).x by A51;
                   xseq.k =modetrans((vseq.k),X,Y).x by A51;
                 then xseq.m - xseq.k =h1.x by A53,A54,A55,Th46;
                 hence thesis by Th38;
                 end;
           A56:  for m be Nat st m >=k holds
                    ||.xseq.n-xseq.m.|| <= e * ||.x.||
                    proof
                      let m be Nat such that
                      A57:  m >=k;
                      A58:  ||.xseq.n-xseq.m.||
                      <=  ||.vseq.n - vseq.m.|| * ||.x.|| by A52;
                      A59:  ||.vseq.n - vseq.m.|| <e by A48,A50,A57;
                         0 <= ||.x.|| by NORMSP_1:8;
                      then ||.vseq.n - vseq.m.|| * ||.x.|| <= e* ||.x.||
                  by A59,AXIOMS:25;
                      hence thesis by A58,AXIOMS:22;
                    end;
                    ||.xseq.n-tseq.x.|| <= e * ||.x.||
                  proof
                    deffunc F(Nat) = ||.xseq.$1 - xseq.n.||;
                    consider rseq be Real_Sequence such that
  A60:              for m be Nat holds rseq.m = F(m) from SEQ_1:sch 1;
  A61:            rseq is convergent & lim rseq = ||.tseq.x-xseq.n.||
                    proof
                        A62: xseq - xseq.n is convergent by A51,NORMSP_1:36;
                        A63: lim (xseq-xseq.n)= tseq.x - xseq.n
                    by A51,NORMSP_1:44;
                            rseq = ||.xseq - xseq.n.||
                         proof
                            now let x be set such that
                           A64:  x in NAT;
                           reconsider k=x as Nat by A64;
                           thus rseq.x = ||.xseq.k - xseq.n.|| by A60
                             .= ||.(xseq - xseq.n).k.|| by NORMSP_1:def 7
                             .= ||.(xseq - xseq.n).||.x by NORMSP_1:def 10;
                         end;
                         hence thesis by SEQ_1:8;
                         end;
                         hence thesis by A62,A63,Th47;
                     end;
              for m be Nat st m >= k holds rseq.m <= e * ||.x.||
                    proof
                      let m be Nat such that
                      A65:  m >=k;
                        rseq.m = ||.xseq.m-xseq.n.|| by A60
                                     .= ||.xseq.n-xseq.m.|| by NORMSP_1:11;
                      hence thesis by A56,A65;
                    end;
                   then lim rseq <= e * ||.x.|| by A61,Lm3;
                   hence thesis by A61,NORMSP_1:11;
                  end;
                 hence ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e* ||.x.||
                      by A51;
           end;
         hence for x be VECTOR of X holds
          ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e* ||.x.||;
     end;
     take k;
     thus thesis by A49;
   end;
  reconsider tseq as bounded LinearOperator of X,Y by A35;
  reconsider tv=tseq as Point of
   R_NormSpace_of_BoundedLinearOperators(X,Y) by A11,Def10;

A66:
   for e be Real st
    e > 0 ex k be Nat st for n be Nat st
     n >= k holds ||.vseq.n - tv.|| <= e
   proof
   let e be Real such that
      A67: e > 0;
      consider k be Nat such that
      A68: for n be Nat st n >= k holds
          for x be VECTOR of X holds
  ||.modetrans((vseq.n),X,Y).x - tseq.x.|| <= e* ||.x.|| by A46,A67;
        now let n be Nat such that
        A69: n >= k;
        set f1=modetrans((vseq.n),X,Y);
        set g1=tseq;
        reconsider h1=vseq.n-tv as bounded LinearOperator of X,Y by A11,Def10;
       A70: now let t be VECTOR of X such that
       A71: ||.t.|| <= 1;
           vseq.n is bounded LinearOperator of X,Y by A11,Def10;
         then modetrans((vseq.n),X,Y)=vseq.n by Th35;
       then A72:  ||.h1.t.||= ||.f1.t-g1.t.|| by Th46;
       A73:  ||.f1.t-g1.t.|| <=e* ||.t.|| by A68,A69;
         e*||.t.|| <= e*1 by A67,A71,AXIOMS:25;
       hence ||.h1.t.|| <=e by A72,A73,AXIOMS:22;
      end;
     A74:
       now let r be Real such that
        A75:  r in PreNorms(h1);
           r in {||.h1.t.|| where t is VECTOR of X : ||.t.|| <= 1 }
    by A75,Def13;
        then consider t be VECTOR of X such that
          A76: r=||.h1.t.|| & ||.t.|| <= 1;
          thus r <=e by A70,A76;
      end;
     A77:
     (for s be real number st s in PreNorms(h1) holds s <= e)
      implies sup PreNorms(h1) <=e by SEQ_4:62;
       BoundedLinearOperatorsNorm(X,Y).(vseq.n-tv)
      = sup PreNorms(h1) by Th36;
     hence ||.vseq.n-tv.|| <=e by A11,A74,A77,NORMSP_1:def 1;
      end;
     hence thesis;
end;
    for e be Real st
    e > 0 ex m be Nat st for n be Nat st
     n >= m holds ||.(vseq.n) - tv.|| < e
   proof
    let e be Real such that
      A78: e > 0;
      A79: e/2 > 0 by A78,SEQ_2:3;
      A80: e/2<e by A78,SEQ_2:4;
      consider m be Nat such that
          A81: for n be Nat st
          n >= m holds ||.(vseq.n) - tv.|| <= e/2 by A66,A79;
            now let n be Nat such that
        A82: n >= m;
              ||.(vseq.n) - tv.|| <= e/2 by A81,A82;
            hence ||.(vseq.n) - tv.|| < e by A80,AXIOMS:22;
          end;
    hence thesis;
   end;
   hence thesis by NORMSP_1:def 9;
end;

theorem Th49:
  for X be RealNormSpace
  for Y be RealBanachSpace holds
     R_NormSpace_of_BoundedLinearOperators(X,Y) is RealBanachSpace
proof
    let X be RealNormSpace;
    let Y be RealBanachSpace;
      for seq be sequence of R_NormSpace_of_BoundedLinearOperators(X,Y)
      st seq is Cauchy_sequence_by_Norm holds seq is convergent by Th48;
    hence thesis by Def16;
end;

registration
  let X be RealNormSpace;
  let Y be RealBanachSpace;
  cluster R_NormSpace_of_BoundedLinearOperators (X,Y) -> complete;
  coherence by Th49;
end;

