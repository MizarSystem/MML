          then
A272:     n = Sum do1 + Sum d'1 by A62,Th1;
          bo1 <> 0 by A1,PREPOWER:12;
          then Sum d'1 mod (b|^o1) = 0 by A271,PEPIN:6;
          then n div (b|^o1) = (Sum d'1 div (b|^o1)) + (Sum do1 div (b|^o1))
          by A272,NAT_D:19;
          then
A273:     (n div (b|^o1)) * b|^o1 = Sum d'1 by A262,A271,NAT_D:3;
          reconsider do=E|o as XFinSequence of NAT by AFINSQ_1:16;
A274:     len e = len E by A60;
          then
A275:     len do=o by A77,A80,AFINSQ_1:14;
          then
A276:     len do>=0 qua Nat+1 by A193,NAT_1:13;
A277:     dom E = len do + len d' by A60,A77,A264,A275;
          now
            let k be Nat;
            reconsider K=k as Element of NAT by ORDINAL1:def 13;
            assume
A278:       k in dom d';
            then k < len d' by NAT_1:45;
            then o+k < dom E by A275,A277,XREAL_1:10;
            then o+k in dom E by NAT_1:45;
            then E.(o+k) = e.(o+k)*b|^(o+k) by A61;
            then d'.K = e.(o+k)*b|^(o+k) by A264,A278
              .= e.(o+k)*(b|^o*b|^k) by NEWTON:13
              .= (e.(o+k)*(b|^k))*(b|^o);
            hence b|^o divides d'.k by NAT_D:def 3;
