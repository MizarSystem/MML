      REAL_1, NAT_1, ABSVALUE, SEQ_1, SEQ_2, SEQM_3, PROB_1, RELSET_1,
      SQUARE_1, PARTFUN1, RFUNCT_1, RCOMP_1, RFUNCT_2;
