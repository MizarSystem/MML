    end;
    then s is divergent_to+infty by Lm4,Th47,Th69;
    then f/*s is convergent & lim(f/*s)=g by A1,A2,A7,Def12;
    then consider n such that
A8: for m st n<=m holds abs((f/*s).m-g)<g1 by A3,SEQ_2:def 7;
    abs((f/*s).n-g)<g1 by A8;
