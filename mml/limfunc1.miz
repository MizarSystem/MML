    hence (f1(#)f2)/*s is convergent by A14,SEQ_4:35;
    lim((f1/*(s^\k))(#)(f2/*(s^\k)))=0 by A16,A13,SEQ_2:40;
    hence lim((f1(#)f2)/*s)=0 by A17,A14,SEQ_4:36;
