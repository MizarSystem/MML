      then consider r such that
A14:  for r1 st r<r1 & r1 in dom f holds abs(f.r1-g)<g1 by A9,
      A13;
      consider n such that
A15:  for m st n<=m holds r<s.m by A10,Def4;
      take n;
      let m;
A16:  s.m in rng s by VALUED_0:28;
