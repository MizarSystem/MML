        then f/*seq is divergent_to+infty by A7,A9,Def10;
        then (f/*seq)" is convergent & lim((f/*seq)")=0 by Th61;
        hence (f^)/*seq is convergent & lim((f^)/*seq)=0 by A10,RFUNCT_2:27;
      end;
