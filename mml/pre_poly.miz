theorem Th4:
  for X being set, R being Relation st field R c= X holds R is Relation of X
proof
  let X be set, R be Relation;
  assume
A1: field R c= X;
  R c= [:X,X:]
  proof
    let x,y be set;
    assume [x,y] in R;
    then x in field R & y in field R by RELAT_1:30;
    hence thesis by A1,ZFMISC_1:def 2;
  end;
  hence thesis;
end;

