A1: rng (F^G) = rng F \/ rng G by FINSEQ_1:44;
    now
      let y be set;
      assume y in rng (F^G);
      then y in rng F or y in rng G by A1,XBOOLE_0:def 3;
      hence y is Cardinal by Th10;
    end;
    hence thesis by Th10;
  end;
end;

