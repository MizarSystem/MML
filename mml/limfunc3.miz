 vocabularies NUMBERS, REAL_1, SUBSET_1, SEQ_1, PARTFUN1, XREAL_0, ORDINAL1,
      CARD_1, ARYTM_3, XXREAL_0, ARYTM_1, RELAT_1, TARSKI, VALUED_1, XBOOLE_0,
