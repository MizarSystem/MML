   by A1; take g1, g2;  not f.g1 in {0} & not f.g2 in {0} by A3,TARSKI:def 1
;
  then not g1 in f"{0} & not g2 in f"{0} by FUNCT_1:def 13;
  then g1 in dom f\f"{0} & g2 in dom f\f"{0} by A3,XBOOLE_0:def 4; hence
