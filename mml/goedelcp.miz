begin :: Henkin`s Theorem

 reserve b,b1,b2,c,d for set,
         X,Y for Subset of CQC-WFF,
         i,j,k,m,n for Nat,
         p,p1,q,r,s,s1 for Element of CQC-WFF,
         x,x1,x2,y,y1 for bound_QC-variable,
         A for non empty set,
         J for interpretation of A,
         v for Element of Valuations_in A,
         f1,f2 for FinSequence of CQC-WFF,
         CX,CY,CZ for Consistent Subset of CQC-WFF,
         JH for Henkin_interpretation of CX,
         a for Element of A;

definition let X;
  attr X is negation_faithful means
  :Def1: X |- p or X |- 'not' p;
end;

definition let X;
  attr X is with_examples means
  :Def2: for x,p holds ex y st X |- ('not' Ex(x,p)) 'or' (p.(x,y));
end;

theorem
  Th1: CX is negation_faithful implies (CX |- p iff not CX |- 'not' p)
                                               by Def1,HENMODEL:def 3;

theorem
  Th27: for f being FinSequence of CQC-WFF holds
           |- f^<*'not' p 'or' q*> & |- f^<*p*> implies |- f^<*q*>
  proof
    let f be FinSequence of CQC-WFF such that
A1: |- f^<*'not' p 'or' q*> & |- f^<*p*>;
    set f1 = f^<*'not' p*>^<*p*>;
A2: Ant(f1) = f^<*'not' p*> & Ant(f^<*p*>) = f & Suc(f^<*p*>) = p
                                                     by CALCUL_1:5; then
    Ant(f^<*p*>) is_Subsequence_of Ant(f1) & Suc(f^<*p*>) = Suc(f1)
                                      by CALCUL_1:5,CALCUL_1:8; then
A3: |- f1 by A1,CALCUL_1:36;
    set f2 = f^<*'not' p*>^<*'not' p*>;
A4: Ant(f2) = f^<*'not' p*> & Suc(f2) = 'not' p by CALCUL_1:5; then
A5: (Ant(f2)).(len f+1) = 'not' p by FINSEQ_1:59;
    len f+1 = len f + len <*'not' p *> by FINSEQ_1:56; then
    len f+1 = len Ant(f2) by A4,FINSEQ_1:35; then
    len f+1 in dom Ant(f2) by A4,CALCUL_1:10; then
    Suc(f2) is_tail_of Ant(f2) by A4,A5,CALCUL_1:def 3; then
A6: |- f2 by CALCUL_1:33;
    0+1 <= len f2 by CALCUL_1:10; then
A7: 0 < len f2;
    Ant(f1) = Ant(f2) & 'not' Suc(f1) = Suc(f2) by A4,CALCUL_1:5; then
    |- Ant(f1)^<*'not' Suc(f1)*> by A6,A7,CALCUL_1:3; then
A8: |- Ant(f1)^<*q*> by A3,CALCUL_1:44;
    set f3 = f^<*q*>^<*q*>;
A9: Ant(f3) =  f^<*q*> & Suc(f3) = q by CALCUL_1:5; then
A10: (Ant(f3)).(len f+1) = q by FINSEQ_1:59;
    len f+1 = len f + len <*q*> by FINSEQ_1:56; then
    len f+1 = len Ant(f3) by A9,FINSEQ_1:35; then
    len f+1 in dom Ant(f3) by A9,CALCUL_1:10; then
    Suc(f3) is_tail_of Ant(f3) by A9,A10,CALCUL_1:def 3; then
     |- f3 by CALCUL_1:33; then
     |- f^<*'not' p 'or' q*>^<*q*> by A2,A8,CALCUL_1:53; then
     |- f^<*'not' ('not' 'not' p '&' 'not' q)*>^<*q*> by QC_LANG2:def 3; then
A11: |- f^<*'not' q*>^<*'not' 'not' p '&' 'not' q*> by CALCUL_1:48;
     set f4 = f^<*'not' q*>^<*'not' 'not' p '&' 'not' q*>;
     Suc(f4) = 'not' 'not' p '&' 'not' q by CALCUL_1:5; then
A12: |- Ant(f4)^<*'not' 'not' p*> &
     |- Ant(f4)^<*'not' q*> by A11,CALCUL_1:41,CALCUL_1:40;
     set f5 = Ant(f4)^<*'not' 'not' p*>;
     set f6 = Ant(f4)^<*'not' q*>;
     Ant(f5) = Ant(f4) & Suc(f5) = 'not' 'not' p & Ant(f6) = Ant(f4) &
     Suc(f6) = 'not' q by CALCUL_1:5; then
     |- Ant(f4)^<*'not' 'not' p '&' 'not' q*> by A12,CALCUL_1:39; then
     |- f^<*'not' q*>^<*'not' 'not' p '&' 'not' q*> by CALCUL_1:5; then
     |- f^<*'not' ('not' 'not' p '&' 'not' q)*>^<*q*> by CALCUL_1:48; then
A14: |- f^<*'not' p 'or' q*>^<*q*> by QC_LANG2:def 3;
     1 <= len (f^<*'not' p 'or' q*>) by CALCUL_1:10; then
     |- Ant(f^<*'not' p 'or' q*>)^<*q*> by A1,A14,CALCUL_1:45;
     hence thesis by CALCUL_1:5;
  end;

theorem
  Th2: X is with_examples implies (X |- Ex(x,p) iff ex y st X |- p.(x,y))
   proof
     assume A1: X is with_examples;
     thus X |- Ex(x,p) implies ex y st X |- p.(x,y)
     proof
       assume X |- Ex(x,p); then
       consider f1 such that
A2:    rng f1 c= X & |- f1^<*Ex(x,p)*> by HENMODEL:def 2;
       consider y such that
A3:    X |- 'not' Ex(x,p) 'or' (p.(x,y)) by A1,Def2;
       consider f2 such that
A4:    rng f2 c= X & |- f2^<*'not' Ex(x,p) 'or' (p.(x,y))*>
          by A3,HENMODEL:def 2;
       take y;
       |- f1^f2^<*Ex(x,p)*> & |- f1^f2^<*'not' Ex(x,p) 'or' (p.(x,y))*>
                                      by A2,A4,HENMODEL:5,CALCUL_2:20; then
A5:    |- f1^f2^<*p.(x,y)*> by Th27;
       rng(f1^f2) = rng f1 \/ rng f2 by FINSEQ_1:44; then
       rng(f1^f2) c= X by A2,A4,XBOOLE_1:8;
       hence thesis by A5,HENMODEL:def 2;
     end;
     thus (ex y st X |- p.(x,y)) implies X |- Ex(x,p)
     proof
       given y such that A6: X |- p.(x,y);
       consider f1 such that
A7:    rng f1 c= X & |- f1^<*p.(x,y)*> by A6,HENMODEL:def 2;
       |- f1^<*Ex(x,p)*> by A7,CALCUL_1:58;
       hence thesis by A7,HENMODEL:def 2;
     end;
  end;

theorem
  Th3: (CX is negation_faithful & CX is with_examples implies
                  (JH,valH |= p iff CX |- p)) implies
         (CX is negation_faithful & CX is with_examples implies
        (JH,valH |= 'not' p iff CX |- 'not' p))
          by Def1,HENMODEL:def 3,VALUAT_1:28;

theorem
  Th4: |- f1^<*p*> & |- f1^<*q*> implies |- f1^<*p '&' q*>
   proof
     set g = f1^<*p*>;
     set g1 = f1^<*q*>;
     assume A1: |- g & |- g1;
A2:  Ant(g) = f1 & Suc(g) = p & Suc(g1) = q by CALCUL_1:5; then
     Ant(g) = Ant(g1) by CALCUL_1:5;
     hence thesis by A1,A2,CALCUL_1:39;
   end;

theorem
  Th5: X |- p & X |- q iff X |- p '&' q
  proof
    thus X |- p & X |- q implies X |- p '&' q
    proof
      assume A1: X |- p & X |- q; then
      consider f1 such that A2: rng f1 c= X & |- f1^<*p*> by HENMODEL:def 2;
      consider f2 such that A3: rng f2 c= X & |- f2^<*q*> by A1,HENMODEL:def 2;
      |- f1^f2^<*p*> & |- f1^f2^<*q*>
        by A2,A3,HENMODEL:5,CALCUL_2:20; then
A4:   |- f1^f2^<*p '&' q*> by Th4;
      rng(f1^f2) = rng f1 \/ rng f2 by FINSEQ_1:44; then
      rng(f1^f2) c= X by A2,A3,XBOOLE_1:8;
      hence thesis by A4,HENMODEL:def 2;
    end;
    thus X |- p '&' q implies X |- p & X |- q
    proof
      assume X |- p '&' q; then
      consider f1 such that A1: rng f1 c= X & |- f1^<*p '&' q*>
                                                       by HENMODEL:def 2;
      |- f1^<*p*> & |- f1^<*q*> by A1,CALCUL_2:23,22;
      hence thesis by A1,HENMODEL:def 2;
    end;
  end;

theorem
  Th6: ((CX is negation_faithful & CX is with_examples implies
                  (JH,valH |= p iff CX |- p)) &
         (CX is negation_faithful & CX is with_examples implies
                  (JH,valH |= q iff CX |- q))) implies
         (CX is negation_faithful & CX is with_examples
             implies (JH,valH |= p '&' q iff CX |- p '&' q))
               by Th5,VALUAT_1:29;

theorem
  Th7: for p st QuantNbr(p) <= 0 holds
       CX is negation_faithful & CX is with_examples implies
                                                     (JH,valH |= p iff CX |- p)
  proof
    defpred P[Element of CQC-WFF] means
            CX is negation_faithful & CX is with_examples implies
                                               (JH,valH |= $1 iff CX |- $1);
A1:  for r,s,x,k for l being CQC-variable_list of k
     for P being QC-pred_symbol of k holds
      P[VERUM] & P[P!l] &
      (P[r] implies P['not' r]) &
      (P[r] & P[s] implies P[r '&' s])
        by Def1,HENMODEL:def 3,VALUAT_1:28,Th5,VALUAT_1:29,HENMODEL:17,16;
A2:   for p st QuantNbr(p) = 0 holds P[p] from SUBSTUT2:sch 3(A1);
      now let p; assume QuantNbr(p) <= 0; then
        QuantNbr(p) = 0 by NAT_1:18;
        hence P[p] by A2;
      end;
      hence thesis;
  end;

theorem
  Th8: J,v |= Ex(x,p) iff ex a st J,v.(x|a) |= p
   proof
A1:  (J,v |= 'not' All(x,'not' p) iff not J,v |= All(x,'not' p))
                                                                by VALUAT_1:28;
     (ex a st not J,v.(x|a) |= 'not' p) iff ex a st J,v.(x|a) |= p
     proof
       thus (ex a st not J,v.(x|a) |= 'not' p) implies ex a st J,v.(x|a) |= p
       proof
         given a such that A4: not J,v.(x|a) |= 'not' p;
         take a;
         thus thesis by A4,VALUAT_1:28;
       end;
       thus (ex a st J,v.(x|a) |= p) implies ex a st not J,v.(x|a) |= 'not' p
       proof
         given a such that A4: J,v.(x|a) |= p;
         take a;
         thus thesis by A4,VALUAT_1:28;
       end;
     end;
     hence thesis by A1,SUBLEMMA:51,QC_LANG2:def 5;
   end;

theorem
  Th9: JH,valH |= Ex(x,p) iff ex y st JH,valH |= p.(x,y)
  proof
    thus JH,valH |= Ex(x,p) implies ex y st JH,valH |= p.(x,y)
    proof
      assume JH,valH |= Ex(x,p); then
      consider x_ being Element of HCar such that
A1:   JH,valH.(x|x_) |= p by Th8;
      rng valH = bound_QC-variables &
      dom valH = bound_QC-variables by HENMODEL:def 7,RELAT_1:71; then
      consider b such that A3: b in dom valH & valH.b = x_
        by FUNCT_1:def 5,HENMODEL:def 5;
      reconsider y = b as bound_QC-variable by A3;
      take y;
      thus thesis by A1,A3,CALCUL_1:24;
    end;
    thus (ex y st JH,valH |= p.(x,y)) implies JH,valH |= Ex(x,p)
    proof
      given y such that A4: JH,valH |= p.(x,y);
      consider x_ being Element of HCar such that
A5:   valH.y = x_ & JH,valH.(x|x_) |= p by A4,CALCUL_1:24;
      thus thesis by A5,Th8;
    end;
  end;

theorem
  Th10: J,v |= 'not' Ex(x,'not' p) iff J,v |= All(x,p)
  proof
A2: not J,v |= Ex(x,'not' p) iff for a holds not J,v.(x|a) |= 'not' p by Th8;
    (for a holds not J,v.(x|a) |= 'not' p) iff for a holds J,v.(x|a) |= p
    proof
      thus (for a holds not J,v.(x|a) |= 'not' p) implies
                                                     for a holds J,v.(x|a) |= p
      proof
        assume A3: for a holds not J,v.(x|a) |= 'not' p;
        let a;
        not J,v.(x|a) |= 'not' p by A3;
        hence thesis by VALUAT_1:28;
      end;
      thus (for a holds J,v.(x|a) |= p) implies
                                   for a holds not J,v.(x|a) |= 'not' p
      proof
        assume A4: for a holds J,v.(x|a) |= p;
        let a;
        J,v.(x|a) |= p by A4;
        hence thesis by VALUAT_1:28;
      end;
    end;
    hence thesis by VALUAT_1:28,A2,SUBLEMMA:51;
  end;

theorem
  Th11: X |- 'not' Ex(x,'not' p) iff X |- All(x,p)
  proof
    thus X |- 'not' Ex(x,'not' p) implies X |- All(x,p)
    proof
      assume X |- 'not' Ex(x,'not' p); then
      consider f1 such that
A1:   rng f1 c= X & |- f1^<*'not' Ex(x,'not' p)*> by HENMODEL:def 2;
      |- f1^<*All(x,p)*> by A1,CALCUL_1:68;
      hence thesis by A1,HENMODEL:def 2;
    end;
    thus X |- All(x,p) implies X |- 'not' Ex(x,'not' p)
    proof
      assume X |- All(x,p); then
      consider f1 such that
A2:   rng f1 c= X & |- f1^<*All(x,p)*> by HENMODEL:def 2;
      |- f1^<*'not' Ex(x,'not' p)*> by A2,CALCUL_1:68;
      hence thesis by A2,HENMODEL:def 2;
    end;
  end;

theorem
  QuantNbr(Ex(x,p)) = QuantNbr(p)+1
  proof
     QuantNbr(Ex(x,p)) = QuantNbr('not' All(x,'not' p)) by QC_LANG2:def 5
                      .= QuantNbr(All(x,'not' p)) by CQC_SIM1:16
                      .= QuantNbr('not' p) + 1 by CQC_SIM1:18;
     hence thesis by CQC_SIM1:16;
   end;

theorem
  Th13: QuantNbr(p) = QuantNbr(p.(x,y))
  proof
    QuantNbr(p) = QuantNbr(CQC_Sub([p,Sbst(x,y)])) by SUBSTUT2:25;
    hence thesis by SUBSTUT2:def 2;
  end;

 reserve L for PATH of q,p,
         F1,F2,F3 for QC-formula,
         a for set;

theorem
  Th14: for p st QuantNbr(p) = 1 holds
             (CX is negation_faithful & CX is with_examples
                                    implies (JH,valH |= p iff CX |- p))
  proof
    let p such that A1: QuantNbr(p) = 1 &
                           CX is negation_faithful & CX is with_examples;
     consider q such that A2: q is_subformula_of p & ex x,r st q = All(x,r)
                                                  by A1,SUBSTUT2:32;
     consider x,r such that A3: q = All(x,r) by A2;
A4:  QuantNbr(q) <= 1 & QuantNbr(q) = QuantNbr(r) + 1
                             by A1,A2,A3,SUBSTUT2:30,CQC_SIM1:18; then
     1 <= QuantNbr(q) by NAT_1:29; then
A5:  1 = QuantNbr(q) by A4,AXIOMS:21; then
A6:  1-1 = QuantNbr(r) + 1-1 by A4;
     consider L;
A8:  1 <= len L & L.1 = q & L.(len L) = p &
        for k st 1 <= k & k < len L
        ex G1,H1 being Element of QC-WFF st L.k = G1 & L.(k+1) = H1 &
        G1 is_immediate_constituent_of H1 by A2,SUBSTUT2:def 6;
      defpred P[Nat] means 1 <= $1 & $1 <= len L implies
                          ex p1 st p1 = L.$1 & QuantNbr(q) <= QuantNbr(p1) &
                                                  (CX |- p1 iff JH,valH |= p1);
A9:   P[0];
A10:  for k st P[k] holds P[k + 1]
      proof
        let k such that A11: P[k];
        assume A12: 1 <= k+1 & k+1 <= len L;
        set j = k+1;
A13:    now assume k = 0; then
A14:      L.j = q by A2,SUBSTUT2:def 6;
          take q;
          thus QuantNbr(q) <= QuantNbr(q);
A16:      now assume JH,valH |= Ex(x,'not' r); then
            consider y such that A17: JH,valH |= ('not' r).(x,y) by Th9;
            QuantNbr('not' r) = 0 by A6,CQC_SIM1:16; then
            QuantNbr(('not' r).(x,y)) = 0 by Th13; then
            CX |- ('not' r).(x,y) by A1,A17,Th7;
            hence CX |- Ex(x,'not' r) by A1,Th2;
          end;
          now assume CX |- Ex(x,'not' r); then
            consider y such that A18: CX |- ('not' r).(x,y) by A1,Th2;
            QuantNbr('not' r) = 0 by A6,CQC_SIM1:16; then
            QuantNbr(('not' r).(x,y)) = 0 by Th13; then
            JH,valH |= ('not' r).(x,y) by A1,A18,Th7;
            hence JH,valH |= Ex(x,'not' r) by Th9;
          end; then
          JH,valH |= 'not' Ex(x,'not' r) iff CX |- 'not' Ex(x,'not' r)
            by A1,Th1,A16,VALUAT_1:28;
          then JH,valH |= q iff CX |- q by A3,Th10,Th11;
          hence thesis by A14;
        end;
        now assume k <> 0; then
          0 < k & k < j by NAT_1:19,NAT_1:38; then
A19:      0+1 <= k & k < len L by A12,NAT_1:38; then
          consider G1,H1 being Element of QC-WFF such that
A20:      L.k = G1 & L.j = H1 & G1 is_immediate_constituent_of H1 by A8;
          consider p1 such that
A21:      p1 = L.k & QuantNbr(q) <= QuantNbr(p1) &
                                   (CX |- p1 iff JH,valH |= p1) by A11,A19;
          consider F3 such that
A22:                F3 = L.j & F3 is_subformula_of p by A2,A12,SUBSTUT2:27;
          reconsider s = H1 as Element of CQC-WFF by A2,A12,A20,SUBSTUT2:28;
          take s;
A24:      now assume s = 'not' p1; then
            QuantNbr(q) <= QuantNbr(s) &
            s = L.(k+1) & (CX |- s iff JH,valH |= s)
              by A1,A20,A21,Th3,CQC_SIM1:16;
            hence thesis;
          end;
A26:      1 <= QuantNbr(p1) by A5,A21;
A27:      QuantNbr(s) <= 1 by A1,A20,A22,SUBSTUT2:30;
A28:      now given F1 such that A29: s = p1 '&' F1;
            reconsider F1 as Element of CQC-WFF by A29,CQC_LANG:19;
            QuantNbr(s) = QuantNbr(p1) + QuantNbr(F1) by A29,CQC_SIM1:17; then
A31:        QuantNbr(p1) <= QuantNbr(s) by NAT_1:29; then
A32:        QuantNbr(q) <= QuantNbr(s) by A21,AXIOMS:22;
            QuantNbr(p1) <= 1 & 1 <= QuantNbr(s) by A5,A21,A27,A31,AXIOMS:22;
            then QuantNbr(p1) = 1 & QuantNbr(s) = 1 by A26,A27,AXIOMS:21; then
            1-1 = QuantNbr(F1)+1-1 by A29,CQC_SIM1:17; then
            CX |- F1 iff JH,valH |= F1 by A1,Th7; then
            QuantNbr(q) <= QuantNbr(s) &
            s = L.(k+1) & (CX |- s iff JH,valH |= s) by A1,A20,A21,A29,A32,Th6;
            hence thesis;
          end;
A33:      now given F1 such that A29: s = F1 '&' p1;
            reconsider F1 as Element of CQC-WFF by A29,CQC_LANG:19;
A34:        QuantNbr(s) = QuantNbr(p1) + QuantNbr(F1) by A29,CQC_SIM1:17; then
A35:        QuantNbr(p1) <= QuantNbr(s) by NAT_1:29; then
A36:        QuantNbr(q) <= QuantNbr(s) by A21,AXIOMS:22;
            QuantNbr(p1) <= 1 & 1 <= QuantNbr(s) by A26,A27,A35,AXIOMS:22; then
            QuantNbr(p1) = 1 & QuantNbr(s) = 1 by A5,A21,A27,AXIOMS:21; then
            1-1 = QuantNbr(F1)+1-1 by A34; then
            CX |- F1 iff JH,valH |= F1 by A1,Th7; then
            QuantNbr(q) <= QuantNbr(s) &
            s = L.(k+1) & (CX |- s iff JH,valH |= s) by A1,A20,A21,A29,A36,Th6;
            hence thesis;
          end;
          now given x such that A37: s = All(x,p1);
            1 < QuantNbr(p1) + 1 by A5,A21,NAT_1:38;
            hence contradiction by A27,A37,CQC_SIM1:18;
          end;
          hence thesis by A20,A21,A24,A28,A33,QC_LANG2:def 20;
        end;
        hence thesis by A13;
      end;
      for k holds P[k] from NAT_1:sch 1(A9,A10); then
      consider p1 such that
A38:  p1 = L.(len L) & QuantNbr(q) <= QuantNbr(p1) &
                                      (CX |- p1 iff JH,valH |= p1) by A8;
      thus thesis by A8,A38;
  end;

theorem
  Th15: for n st for p st QuantNbr(p) <= n holds
             (CX is negation_faithful & CX is with_examples
                                    implies (JH,valH |= p iff CX |- p)) holds
                 for p st QuantNbr(p) <= n+1 holds
                    (CX is negation_faithful & CX is with_examples
                                    implies (JH,valH |= p iff CX |- p))
  proof
    let n such that
A1:  for p st QuantNbr(p) <= n holds
             CX is negation_faithful & CX is with_examples
                                    implies (JH,valH |= p iff CX |- p);
    let p such that A2: QuantNbr(p) <= n+1 & CX is negation_faithful &
                                                           CX is with_examples;
A3: QuantNbr(p) <= n implies thesis by A1,A2;
    now assume A4: QuantNbr(p) = n+1; then
      1 <= QuantNbr(p) by NAT_1:29; then
      consider q such that A5: q is_subformula_of p & QuantNbr(q) = 1
                                              by SUBSTUT2:34;
      consider L;
A6:   1 <= len L & L.1 = q & L.(len L) = p &
        for k st 1 <= k & k < len L
        ex G1,H1 being Element of QC-WFF st L.k = G1 & L.(k+1) = H1 &
        G1 is_immediate_constituent_of H1 by A5,SUBSTUT2:def 6;
      defpred P[Nat] means 1 <= $1 & $1 <= len L implies
                          ex p1 st p1 = L.$1 & QuantNbr(q) <= QuantNbr(p1) &
                                              (CX |- p1 iff JH,valH |= p1);
A7:   P[0];
A8:   for k st P[k] holds P[k + 1]
      proof
        let k such that A9: P[k];
        assume A10: 1 <= k+1 & k+1 <= len L;
        set j = k+1;
A11:    now assume k = 0; then
A12:      L.j = q by A5,SUBSTUT2:def 6;
          take q;
          (JH,valH |= q iff CX |- q) by A2,A5,Th14;
          hence thesis by A12;
        end;
        now assume k <> 0; then
          0 < k & k < j by NAT_1:19,NAT_1:38; then
A13:      0+1 <= k & k < len L by A10,NAT_1:38; then
          consider G1,H1 being Element of QC-WFF such that
A14:      L.k = G1 & L.j = H1 & G1 is_immediate_constituent_of H1
            by A5,SUBSTUT2:def 6;
          consider p1 such that
A15:      QuantNbr(q) <= QuantNbr(p1) &
                      p1 = L.k & (CX |- p1 iff JH,valH |= p1) by A9,A13;
          consider F3 such that
A16:      F3 = L.j & F3 is_subformula_of p by A5,A10,SUBSTUT2:27;
          reconsider s = H1 as Element of CQC-WFF by A5,A10,A14,SUBSTUT2:28;
          take s;
A18:      now assume s = 'not' p1; then
            QuantNbr(q) <= QuantNbr(s) &
            s = L.(k+1) & (CX |- s iff JH,valH |= s)
              by A2,A14,A15,Th3,CQC_SIM1:16;
            hence thesis;
          end;
A21:      QuantNbr(s) <= n+1 by A4,A14,A16,SUBSTUT2:30;
A22:      now given F1 such that A23: s = p1 '&' F1;
            reconsider F1 as Element of CQC-WFF by A23,CQC_LANG:19;
A24:        QuantNbr(s) = QuantNbr(p1) + QuantNbr(F1) by A23,CQC_SIM1:17; then
            1+QuantNbr(F1) <= QuantNbr(s) by A5,A15,AXIOMS:24; then
            1+QuantNbr(F1) <= n+1 by A21,AXIOMS:22; then
            QuantNbr(F1)+1+(-1) <= n+1+(-1) by AXIOMS:24; then
            CX |- F1 iff JH,valH |= F1 by A1,A2; then
A25:        s = L.(k+1) & (CX |- s iff JH,valH |= s) by A2,A14,A15,A23,Th6;
            QuantNbr(p1) <= QuantNbr(s) by A24,NAT_1:29; then
            QuantNbr(q) <= QuantNbr(s) by A15,AXIOMS:22;
            hence thesis by A25;
          end;
A36:      now given F1 such that A26: s = F1 '&' p1;
            reconsider F1 as Element of CQC-WFF by A26,CQC_LANG:19;
A27:        QuantNbr(s) = QuantNbr(p1) + QuantNbr(F1) by A26,CQC_SIM1:17; then
            1+QuantNbr(F1) <= QuantNbr(s) by A5,A15,AXIOMS:24; then
            1+QuantNbr(F1) <= n+1 by A21,AXIOMS:22; then
            QuantNbr(F1)+1+(-1) <= n+1+(-1) by AXIOMS:24; then
            CX |- F1 iff JH,valH |= F1 by A1,A2; then
A28:        s = L.(k+1) & (CX |- s iff JH,valH |= s) by A2,A14,A15,A26,Th6;
            QuantNbr(p1) <= QuantNbr(s) by A27,NAT_1:29; then
            QuantNbr(q) <= QuantNbr(s) by A15,AXIOMS:22;
            hence thesis by A28;
          end;
          now given x such that A29: s = All(x,p1);
A30:        QuantNbr(s) = QuantNbr(p1) + 1 by A29,CQC_SIM1:18; then
            QuantNbr(p1) < n+1 by A21,NAT_1:38; then
            QuantNbr(p1) <= n by NAT_1:38; then
A31:        QuantNbr('not' p1) <= n by CQC_SIM1:16;
A32:        QuantNbr(q) <= QuantNbr(s) by A15,A30,NAT_1:38;
A33:        now assume JH,valH |= Ex(x,'not' p1); then
              consider y such that A34: JH,valH |= ('not' p1).(x,y) by Th9;
              QuantNbr(('not' p1).(x,y)) <= n by A31,Th13; then
              CX |- ('not' p1).(x,y) by A1,A2,A34;
              hence CX |- Ex(x,'not' p1) by A2,Th2;
            end;
            now assume CX |- Ex(x,'not' p1); then
              consider y such that A35: CX |- ('not' p1).(x,y) by A2,Th2;
              QuantNbr(('not' p1).(x,y)) <= n by A31,Th13; then
              JH,valH |= ('not' p1).(x,y) by A1,A2,A35;
              hence JH,valH |= Ex(x,'not' p1) by Th9;
            end; then
            JH,valH |= 'not' Ex(x,'not' p1) iff CX |- 'not' Ex(x,'not' p1)
                                  by A2,Th1,A33,VALUAT_1:28; then
            s = L.(k+1) & (JH,valH |= s iff CX |- s) by A14,A29,Th10,Th11;
            hence thesis by A32;
          end;
          hence thesis by A14,A15,A18,A22,A36,QC_LANG2:def 20;
        end;
        hence thesis by A11;
      end;
      for k holds P[k] from NAT_1:sch 1(A7,A8); then
      consider p1 such that
A37:  p1 = L.(len L) & QuantNbr(q) <= QuantNbr(p1) &
                                      (CX |- p1 iff JH,valH |= p1) by A6;
      thus thesis by A6,A37;
    end;
    hence thesis by A2,A3,NAT_1:26;
  end;

:: Ebb et al, Chapter V, Henkin's Theorem 1.10

theorem
  Th16: for p holds (CX is negation_faithful & CX is with_examples
                                    implies (JH,valH |= p iff CX |- p))
  proof
    defpred P[Element of CQC-WFF] means
          CX is negation_faithful & CX is with_examples
                                    implies (JH,valH |= $1 iff CX |- $1);
A1: for p st QuantNbr(p) <= 0 holds P[p] by Th7;
A2: for k st for p st QuantNbr(p) <= k holds P[p] holds
             for p st QuantNbr(p) <= k+1 holds P[p] by Th15;
    thus for p holds P[p] from SUBSTUT2:sch 2(A1,A2);
  end;

begin :: Satisfiability of Consistent Sets of Formulas with Finitely Many Free
      :: Variables

theorem
  Th17: QC-WFF is countable
   proof
     QC-WFF is QC-closed by QC_LANG1:def 10; then
A1:  QC-WFF is Subset of [:NAT,NAT:]* by QC_LANG1:def 9;
     [:NAT,NAT:] is countable by CARD_4:44,55; then
     [:NAT,NAT:]* is countable by CARD_4:65;
     hence thesis by A1,CARD_4:46;
   end;

definition
  func ExCl -> Subset of CQC-WFF means
  :Def3: a in it iff ex x,p st a = Ex(x,p);
  existence
  proof
    defpred P[set] means ex x,p st $1 = Ex(x,p);
    consider X being set such that
A1: for a holds a in X iff a in CQC-WFF & P[a] from XBOOLE_0:sch 1;
    for a st a in X holds a in CQC-WFF by A1; then
    reconsider X as Subset of CQC-WFF by TARSKI:def 3;
    take X;
    thus thesis by A1;
  end;
  uniqueness
  proof
    defpred P[set] means ex x,p st $1 = Ex(x,p);
    let A1,A2 be Subset of CQC-WFF such that
A3:  a in A1 iff P[a] and
A4:  a in A2 iff P[a];
     now let a;
      a in A1 iff P[a] by A3;
      hence a in A1 iff a in A2 by A4;
     end;
    hence A1 = A2 by TARSKI:2;
  end;
end;

theorem
  Th33: CQC-WFF is countable by Th17,CARD_4:46;

theorem
  Th18: ExCl is non empty & ExCl is countable
  proof
    consider x,p;
    set a = Ex(x,p);
    a in ExCl by Def3;
    hence ExCl is non empty;
    thus thesis by Th33,CARD_4:46;
  end;

Th19: for A being non empty set st A is countable holds
       ex f being Function st dom f = NAT & A = rng f
  proof
    let A be non empty set such that A1: A is countable;
    A c= A; then
    consider F being Function of NAT,A such that
A2: A = rng F by A1,SUPINF_2:def 14;
    dom F = NAT by FUNCT_2:def 1;
    hence thesis by A2;
  end;

definition let p be Element of QC-WFF such that A1: p is existential;
  func Ex-bound_in p -> bound_QC-variable means
  :Def9: ex q being Element of QC-WFF st p = Ex(it,q);
  existence by A1,QC_LANG2:def 13;
  uniqueness by QC_LANG2:19;
end;

definition let p be Element of CQC-WFF such that A1: p is existential;
  func Ex-the_scope_of p -> Element of CQC-WFF means
  :Def10: ex x st p = Ex(x,it);
  existence
   proof
     consider x,F1 such that A2: p = Ex(x,F1) by A1,QC_LANG2:def 13;
     p = 'not' All(x,'not' F1) by A2,QC_LANG2:def 5; then
     All(x,'not' F1) is Element of CQC-WFF by CQC_LANG:18; then
a2:  'not' F1 is Element of CQC-WFF by CQC_LANG:23;
     take F1;
     thus thesis by A2,a2,CQC_LANG:18;
   end;
   uniqueness by QC_LANG2:19;
end;

definition let F be Function of NAT,CQC-WFF,a be Nat;
  func bound_in(F,a) -> bound_QC-variable means
  :Def11: p = F.a implies it = Ex-bound_in p;
   existence
   proof
     reconsider p = F.a as Element of CQC-WFF;
     take x = Ex-bound_in p;
     thus thesis;
   end;
   uniqueness
   proof
     let x1,x2 such that
A1:    p = F.a implies x1 = Ex-bound_in p and
A2:    p = F.a implies x2 = Ex-bound_in p;
     reconsider q = F.a as Element of CQC-WFF;
     x1 = Ex-bound_in q & x2 = Ex-bound_in q by A1,A2;
     hence thesis;
   end;
end;

definition let F be Function of NAT,CQC-WFF,a be Nat;
  func the_scope_of(F,a) -> Element of CQC-WFF means
  :Def12: p = F.a implies it = Ex-the_scope_of p;
   existence
   proof
     reconsider p = F.a as Element of CQC-WFF;
     take q = Ex-the_scope_of p;
     thus thesis;
   end;
   uniqueness
   proof
     let q1,q2 be Element of CQC-WFF such that
A1:    p = F.a implies q1 = Ex-the_scope_of p and
A2:    p = F.a implies q2 = Ex-the_scope_of p;
     reconsider q = F.a as Element of CQC-WFF;
     q1 = Ex-the_scope_of q & q2 = Ex-the_scope_of q by A1,A2;
     hence thesis;
   end;
end;

definition let X;
  func still_not-bound_in X -> Element of bool bound_QC-variables equals
  :Def14: union {still_not-bound_in p : p in X};
  coherence
  proof
    set Y = union {still_not-bound_in p : p in X};
    now let a such that A1: a in Y;
      consider b such that
A2:           a in b & b in {still_not-bound_in p : p in X} by A1,TARSKI:def 4;
      consider p such that A3: b = still_not-bound_in p & p in X by A2;
      thus a in bound_QC-variables by A2,A3;
    end;
    hence thesis by TARSKI:def 3;
  end;
end;

theorem
  Th20: p in X implies X |- p
  proof
    assume A1: p in X;
    now let a such that A2: a in rng <*p*>;
      a in {p} by A2,FINSEQ_1:55;
      hence a in X by A1,TARSKI:def 1;
    end; then
A3: rng <*p*> c= X by TARSKI:def 3;
    |- <*>CQC-WFF^<*p*>^<*p*> by CALCUL_2:21; then
    |- <*p*>^<*p*> by FINSEQ_1:47;
    hence thesis by A3,HENMODEL:def 2;
  end;

theorem
  Th21: Ex-bound_in Ex(x,p) = x & Ex-the_scope_of Ex(x,p) = p
  proof
A1: Ex(x,p) is existential by QC_LANG2:def 13; then
    consider q being QC-formula such that
A2: Ex(x,p) = Ex(Ex-bound_in Ex(x,p),q) by Def9;
    consider y such that
A4: Ex(x,p) = Ex(y,Ex-the_scope_of Ex(x,p)) by A1,Def10;
    thus thesis by A2,QC_LANG2:19,A4;
  end;

theorem
  Th23: X |- VERUM
  proof
    set f = {};
    reconsider f as FinSequence of CQC-WFF by FINSEQ_1:29;
A1: rng f c= X by XBOOLE_1:2,RELAT_1:60;
    |- f^<*VERUM*> by HENMODEL:15;
    hence thesis by A1,HENMODEL:def 2;
  end;

theorem
  Th24: X |- 'not' VERUM iff X is Inconsistent
  proof
    thus X |- 'not' VERUM implies X is Inconsistent
    proof
    assume A1: X |- 'not' VERUM;
    X |- VERUM by Th23;
    hence thesis by A1,HENMODEL:def 3;
    end;
    thus X is Inconsistent implies X |- 'not' VERUM by HENMODEL:6;
  end;

 reserve C,D for Element of [:CQC-WFF,bool bound_QC-variables:];
 reserve K,K1,L for Subset of bound_QC-variables;

theorem
  Th26: for f,g being FinSequence of CQC-WFF st 0 < len f & |- f^<*p*> holds
                                                  |- Ant(f)^g^<*Suc(f)*>^<*p*>
  proof
    let f,g be FinSequence of CQC-WFF such that A1: 0 < len f & |- f^<*p*>;
    f is_Subsequence_of Ant(f)^g^<*Suc(f)*> by A1,CALCUL_1:13; then
    Ant(f^<*p*>) is_Subsequence_of Ant(f)^g^<*Suc(f)*> by CALCUL_1:5; then
A2: Ant(f^<*p*>) is_Subsequence_of Ant(Ant(f)^g^<*Suc(f)*>^<*p*>)
                                                          by CALCUL_1:5;
    Suc(f^<*p*>) = p by CALCUL_1:5; then
    Suc(f^<*p*>) = Suc(Ant(f)^g^<*Suc(f)*>^<*p*>) by CALCUL_1:5;
    hence thesis by A1,A2,CALCUL_1:36;
  end;

theorem
  Th27: still_not-bound_in {p} = still_not-bound_in p
  proof
A1: now let a such that A2: a in still_not-bound_in {p};
      a in union {still_not-bound_in q : q in {p}} by A2,Def14; then
      consider b such that
A3:    a in b & b in {still_not-bound_in q : q in {p}} by TARSKI:def 4;
      consider q such that A4: b = still_not-bound_in q & q in {p} by A3;
      thus a in still_not-bound_in p by A3,A4,TARSKI:def 1;
    end;
    now let a such that A4: a in still_not-bound_in p;
      set b = still_not-bound_in p;
      p in {p} by TARSKI:def 1; then
      b in {still_not-bound_in q : q in {p}}; then
      a in union {still_not-bound_in q : q in {p}} by A4,TARSKI:def 4;
      hence a in still_not-bound_in {p} by Def14;
    end;
    hence thesis by A1,TARSKI:2;
  end;

theorem Th28:
  still_not-bound_in (X \/ Y) = still_not-bound_in X \/ still_not-bound_in Y
  proof
    thus still_not-bound_in (X \/ Y) c=
                               still_not-bound_in X \/ still_not-bound_in Y
    proof
      set A = {still_not-bound_in p : p in X \/ Y};
      let b such that A1: b in still_not-bound_in (X \/ Y);
      b in union A by A1,Def14; then
      consider a such that
A2:        b in a & a in A by TARSKI:def 4;
      consider p such that A3: a = still_not-bound_in p & p in X \/ Y by A2;
A5:   now assume p in X; then
        a in {still_not-bound_in q : q in X} by A3; then
        b in union {still_not-bound_in q : q in X} by A2,TARSKI:def 4; then
A6:     b in still_not-bound_in X by Def14;
        still_not-bound_in X c= still_not-bound_in X \/ still_not-bound_in Y
                                                                 by XBOOLE_1:7;
        hence thesis by A6;
      end;
      now assume p in Y; then
        a in {still_not-bound_in q : q in Y} by A3; then
        b in union {still_not-bound_in q : q in Y} by A2,TARSKI:def 4; then
A7:     b in still_not-bound_in Y by Def14;
        still_not-bound_in Y c= still_not-bound_in X \/ still_not-bound_in Y
                                                                 by XBOOLE_1:7;
        hence thesis by A7;
      end;
      hence thesis by A3,XBOOLE_0:def 2,A5;
    end;
    thus still_not-bound_in X \/ still_not-bound_in Y c=
                                            still_not-bound_in (X \/ Y)
    proof
      let b such that A8: b in still_not-bound_in X \/ still_not-bound_in Y;
A10:  now assume b in still_not-bound_in X; then
        b in union {still_not-bound_in p : p in X}  by Def14; then
        consider a such that
A11:        b in a & a in {still_not-bound_in p : p in X} by TARSKI:def 4;
        consider p such that A12: a = still_not-bound_in p & p in X by A11;
        X c= X \/ Y by XBOOLE_1:7; then
        a in {still_not-bound_in q : q in X \/ Y} by A12; then
        b in union {still_not-bound_in q : q in X \/ Y} by A11,TARSKI:def 4;
        hence thesis by Def14;
      end;
      now assume b in still_not-bound_in Y; then
        b in union {still_not-bound_in p : p in Y}  by Def14; then
        consider a such that
A12:        b in a & a in {still_not-bound_in p : p in Y} by TARSKI:def 4;
        consider p such that A13: a = still_not-bound_in p & p in Y by A12;
        Y c= X \/ Y by XBOOLE_1:7; then
        a in {still_not-bound_in q : q in X \/ Y} by A13; then
        b in union {still_not-bound_in q : q in X \/ Y} by A12,TARSKI:def 4;
        hence thesis by Def14;
      end;
      hence thesis by A8,XBOOLE_0:def 2,A10;
    end;
  end;

theorem
  Th29: for A being Element of bool bound_QC-variables st A is finite holds
                                                      ex x st not x in A
  proof
    let A be Element of bool bound_QC-variables such that A1: A is finite;
    not (for b holds b in A iff b in bound_QC-variables)
      by TARSKI:2,A1,CALCUL_1:64; then
    consider b such that A4: not b in A & b in bound_QC-variables;
    reconsider x = b as bound_QC-variable by A4;
    take x;
    thus thesis by A4;
  end;

theorem
  Th30: X c= Y implies still_not-bound_in X c= still_not-bound_in Y
  proof
    set A = {still_not-bound_in p : p in X};
    assume A1: X c= Y;
    now let a such that A2: a in still_not-bound_in X;
      a in union A by A2,Def14; then
      consider b such that A3: a in b & b in A by TARSKI:def 4;
      consider p such that A4: b = still_not-bound_in p & p in X by A3;
      b in {still_not-bound_in q : q in Y} by A1,A4; then
      a in union {still_not-bound_in q : q in Y} by A3,TARSKI:def 4;
      hence a in still_not-bound_in Y by Def14;
    end;
    hence thesis by TARSKI:def 3;
  end;

theorem
  Th31: for f being FinSequence of CQC-WFF holds
                           still_not-bound_in rng f = still_not-bound_in f
  proof
    let f be FinSequence of CQC-WFF;
    set A = {still_not-bound_in p : p in rng f};
A1: now let a such that A2: a in still_not-bound_in rng f;
      a in union A by A2,Def14; then
      consider b such that A3: a in b & b in A by TARSKI:def 4;
      consider p such that A4: b = still_not-bound_in p & p in rng f by A3;
      consider c such that A5: c in dom f & f.c = p by A4,FUNCT_1:def 5;
      reconsider i = c as Nat by A5;
      thus a in still_not-bound_in f by A3,A4,A5,CALCUL_1:def 5;
    end;
    now let a such that A6: a in still_not-bound_in f;
      consider i,q such that A7: i in dom f & q = f.i &
                       a in still_not-bound_in q by A6,CALCUL_1:def 5;
      q in rng f by A7,FUNCT_1:def 5; then
      still_not-bound_in q in A; then
      a in union A by A7,TARSKI:def 4;
      hence a in still_not-bound_in rng f by Def14;
    end;
    hence thesis by A1,TARSKI:2;
  end;

:: Ebb et al, Chapter V, Lemma 2.1

theorem
  Th32: still_not-bound_in CX is finite implies
                                ex CY st CX c= CY & CY is with_examples
  proof
    assume A80: still_not-bound_in CX is finite;
    consider f being Function such that
A1: dom f = NAT & ExCl = rng f by Th18,Th19;
    reconsider f as Function of NAT,CQC-WFF by A1,FUNCT_2:4;
    defpred P[Nat,set,set] means
      ex K,L st K = $2`2 & L = K \/ still_not-bound_in {f.($1+1)} &
      $3 = [('not' (f.($1+1))) 'or'  (the_scope_of(f,($1+1)).(bound_in(f,$1+1),
                                               x.(min* {i : not x.i in L}))),
        K \/ still_not-bound_in
            ('not' (f.($1+1))) 'or' (the_scope_of(f,($1+1)).(bound_in(f,$1+1),
                                               x.(min* {j : not x.j in L})))];
A2: for n for C ex D st P[n,C,D]
    proof
      let n,C;
      set K = C`2;
      consider a,b such that
A3:  a in CQC-WFF & b in bool bound_QC-variables & C = [a,b] by ZFMISC_1:def 2;
      reconsider K as Subset of bound_QC-variables by A3,MCART_1:7;
      set L = K \/ still_not-bound_in {f.(n+1)};
      set D = [('not' (f.(n+1))) 'or' (the_scope_of(f,(n+1)).(bound_in(f,n+1),
                                              x.(min* {i : not x.i in L}))),
        K \/ still_not-bound_in
            ('not' (f.(n+1))) 'or' (the_scope_of(f,(n+1)).(bound_in(f,n+1),
                                               x.(min* {j : not x.j in L})))];
      take D;
      thus thesis;
    end;
    reconsider A = [('not' (f.0)) 'or' (the_scope_of(f,0).(bound_in(f,0),
    x.(min* {i : not x.i in still_not-bound_in (CX \/ {f.0})}))),
        still_not-bound_in (CX \/ {('not' (f.0)) 'or'
         (the_scope_of(f,0).(bound_in(f,0), x.(min* {j :
                       not x.j in still_not-bound_in (CX \/ {f.0})})))})]
                             as Element of [:CQC-WFF,bool bound_QC-variables:];
    consider h being Function of NAT,[:CQC-WFF,bool bound_QC-variables:]
      such that
A4:   h.0 = A & for n holds P[n,h.n,h.(n+1)] from RECDEF_1:sch 2(A2);
    set CY = CX \/ {(h.n)`1 : not contradiction};
    now let a such that A5: a in CY;
      now assume not a in CX; then
        a in {(h.n)`1 : not contradiction} by A5,XBOOLE_0:def 2; then
        consider n such that A7: a = (h.n)`1;
        consider c,d such that
A8:     c in CQC-WFF & d in bool bound_QC-variables & h.n = [c,d]
                                                          by ZFMISC_1:def 2;
        thus a in CQC-WFF by A7,A8,MCART_1:7;
      end;
      hence a in CQC-WFF;
    end; then
    reconsider CY as Subset of CQC-WFF by TARSKI:def 3;
    now let x,p;
      Ex(x,p) in ExCl by Def3; then
      consider a such that
A9:      a in dom f & f.a = Ex(x,p) by A1,FUNCT_1:def 5;
      reconsider a as Nat by A9;
      reconsider r' = f.a as Element of CQC-WFF;
      Ex-bound_in r' = x & Ex-the_scope_of r' = p by A9,Th21; then
A10:  bound_in(f,a) = x & the_scope_of(f,a) = p by Def11,Def12;
A11:  (h.a)`1 in {(h.n)`1 : not contradiction};
a12:  {(h.n)`1 : not contradiction} c= CY by XBOOLE_1:7;
A13:  now assume A14: a = 0;
        take y = x.(min* {i : not x.i in still_not-bound_in (CX \/ {r'})});
        (h.a)`1 = 'not' r' 'or' (the_scope_of(f,a).(bound_in(f,a),y))
                                                      by A4,A14,MCART_1:7;
        hence CY |- 'not' Ex(x,p) 'or' (p.(x,y)) by A9,A10,A11,a12,Th20;
      end;
      now assume a <> 0; then
        consider m such that A15: a = m+1 by NAT_1:22;
A16:    ex K,L st K = (h.m)`2 & L = K \/ still_not-bound_in {r'} &
        h.a = [('not' r') 'or'  (the_scope_of(f,a).(bound_in(f,a),
                                               x.(min* {i : not x.i in L}))),
        K \/ still_not-bound_in
            ('not' r') 'or' (the_scope_of(f,a).(bound_in(f,a),
                                      x.(min* {j : not x.j in L})))] by A4,A15;
        set K = (h.m)`2;
        set L = still_not-bound_in ({r'}) \/ K;
        take y = x.(min* {i : not x.i in L});
        (h.a)`1 = 'not' r' 'or' (the_scope_of(f,a).(bound_in(f,a),y))
                                                         by A16,MCART_1:7;
        hence CY |- ('not' Ex(x,p)) 'or' (p.(x,y)) by A9,A10,A11,a12,Th20;
      end;
      hence ex y st CY |- 'not' Ex(x,p) 'or' (p.(x,y)) by A13;
    end; then
A17: CY is with_examples by Def2;
     deffunc G(set) = CX \/ {(h.n)`1 : n in $1};
     consider F being Function such that A20: dom F = NAT &
                 for a st a in NAT holds F.a = G(a) from FUNCT_1:sch 3;
A21:  CY = union rng F
      proof
        thus CY c= union rng F
        proof
          let a; assume a in CY; then
A22:      a in CX or a in {(h.n)`1 : not contradiction} by XBOOLE_0:def 2;
A23:      now assume A24: a in CX;
A25:        F.0 = CX \/ {(h.n)`1 : n in 0} by A20;
            now let b such that A26: b in {(h.n)`1 : n in 0};
              consider n such that A27: b = (h.n)`1 & n in 0 by A26;
              thus contradiction by A27;
            end; then
            {(h.n)`1 : n in 0} = {} by XBOOLE_0:def 1; then
A28:        F.0 = CX by A25;
            F.0 in rng F by FUNCT_1:12,A20;
            hence thesis by A24,A28,TARSKI:def 4;
          end;
          now assume a in {(h.n)`1 : not contradiction}; then
            consider n such that A29: a = (h.n)`1;
            n < n+1 by NAT_1:38; then
            n in n+1 by EULER_1:1; then
A30:        a in {(h.m)`1 : m in n+1} by A29;
            F.(n+1) = CX \/ {(h.m)`1 : m in n+1} by A20; then
            {(h.m)`1 : m in n+1} c= F.(n+1) by XBOOLE_1:7; then
A31:        a in F.(n+1) by A30;
            F.(n+1) in rng F by A20,FUNCT_1:12;
            hence thesis by A31,TARSKI:def 4;
          end;
          hence thesis by A22,A23;
        end;
        thus union rng F c= CY
        proof
          let a; assume a in union rng F; then
          consider b such that A32: a in b & b in rng F by TARSKI:def 4;
          consider c such that A33: c in dom F & F.c = b by A32,FUNCT_1:def 5;
          reconsider n = c as Nat by A20,A33;
A34:      a in CX \/ {(h.m)`1 : m in n} by A20,A32,A33;
A35:      now assume A36: a in CX;
            CX c= CY by XBOOLE_1:7;
            hence thesis by A36;
          end;
          now assume a in {(h.m)`1 : m in n}; then
            consider m such that A38: a = (h.m)`1 & m in n;
A39:        a in {(h.i)`1 : not contradiction} by A38;
            {(h.i)`1 : not contradiction} c= CY by XBOOLE_1:7;
            hence a in CY by A39;
          end;
          hence thesis by A34,A35,XBOOLE_0:def 2;
        end;
      end;
A40:  for n,m st m in dom F & n in dom F & n < m holds F.n c= F.m
        proof
          let n,m such that A41: m in dom F & n in dom F & n < m;
A42:      F.n = CX \/ {(h.i)`1 : i in n} by A20;
A43:      F.m = CX \/ {(h.i)`1 : i in m} by A20;
          now let a such that A44: a in F.n;
A45:        now assume A46: a in CX;
              CX c= F.m by A43,XBOOLE_1:7;
              hence a in F.m by A46;
            end;
            now assume a in {(h.i)`1 : i in n}; then
              consider i such that A48: (h.i)`1 = a & i in n;
              i < n by A48,EULER_1:1; then
              i < m by A41,AXIOMS:22; then
              i in m by EULER_1:1; then
A49:          a in {(h.j)`1 : j in m} by A48;
              {(h.j)`1 : j in m} c= F.m by A43,XBOOLE_1:7;
              hence a in F.m by A49;
            end;
            hence a in F.m by A45,A42,A44,XBOOLE_0:def 2;
          end;
          hence thesis by TARSKI:def 3;
        end;
        rng F c= bool CQC-WFF
        proof
          let a such that A50: a in rng F;
          consider b such that A51: b in dom F & F.b = a by A50,FUNCT_1:def 5;
          reconsider b as Nat by A20,A51;
A52:      F.b = CX \/ {(h.i)`1 : i in b} by A20;
          now let c such that A53: c in {(h.i)`1 : i in b};
            consider i such that A54: (h.i)`1 = c & i in b by A53;
            thus c in CQC-WFF by A54,MCART_1:10;
          end; then
          {(h.i)`1 : i in b} c= CQC-WFF by TARSKI:def 3; then
          F.b c= CQC-WFF by A52,XBOOLE_1:8;
          hence a in bool CQC-WFF by A51;
        end; then
        reconsider F as Function of NAT,bool CQC-WFF by A20,FUNCT_2:4;
A53:    for n holds F.(n+1) = F.n \/ {(h.n)`1}
        proof
          let n;
          now let a; assume a in {(h.i)`1 : i in n+1}; then
            consider j such that A54: a = (h.j)`1 & j in n+1;
            j < n+1 by A54,EULER_1:1; then
            j+1 <= n+1 by NAT_1:38; then
A55:        j+1 <= n or j+1 = n+1 by NAT_1:26;
A56:        now assume j+1 = n+1; then
              j+1-1 = n+1-1; then
              j = n by XCMPLX_1:26; then
A57:          a in {(h.n)`1} by A54,TARSKI:def 1;
              {(h.n)`1} c= {(h.i)`1 : i in n} \/ {(h.n)`1} by XBOOLE_1:7;
              hence a in {(h.i)`1 : i in n} \/ {(h.n)`1} by A57;
            end;
            now assume j+1 <= n; then
              j < n by NAT_1:38; then
              j in n by EULER_1:1; then
A58:          a in {(h.k)`1 : k in n} by A54;
              {(h.k)`1 : k in n} c= {(h.i)`1 : i in n} \/ {(h.n)`1}
                                                                by XBOOLE_1:7;
              hence a in {(h.i)`1 : i in n} \/ {(h.n)`1} by A58;
            end;
            hence a in {(h.i)`1 : i in n} \/ {(h.n)`1} by A55,A56;
          end; then
A59:      {(h.k)`1 : k in n+1} c= {(h.i)`1 : i in n} \/ {(h.n)`1}
                                                               by TARSKI:def 3;
          now let a; assume a in {(h.i)`1 : i in n} \/ {(h.n)`1}; then
A60:        a in {(h.i)`1 : i in n} or a in {(h.n)`1} by XBOOLE_0:def 2;
A61:        now assume a in {(h.i)`1 : i in n}; then
              consider j such that A62: (h.j)`1 = a & j in n;
              n <= n+1 & j < n by A62,EULER_1:1,NAT_1:29; then
              j < n+1 by AXIOMS:22; then
              j in n+1 by EULER_1:1;
              hence a in {(h.i)`1 : i in n+1} by A62;
            end;
            now assume a in {(h.n)`1}; then
A63:          a = (h.n)`1 by TARSKI:def 1;
              n < n+1 by NAT_1:38; then
              n in n+1 by EULER_1:1;
              hence a in {(h.i)`1 : i in n+1} by A63;
            end;
            hence a in {(h.i)`1 : i in n+1} by A60,A61;
          end; then
          {(h.i)`1 : i in n} \/ {(h.n)`1} c= {(h.k)`1 : k in n+1}
                                                          by TARSKI:def 3; then
          {(h.i)`1 : i in n} \/ {(h.n)`1} = {(h.k)`1 : k in n+1}
                                                 by A59,XBOOLE_0:def 10; then
          F.(n+1) = CX \/ ({(h.k)`1 : k in n} \/ {(h.n)`1}) by A20; then
          F.(n+1) = CX \/ {(h.k)`1 : k in n} \/ {(h.n)`1} by XBOOLE_1:4;
          hence thesis by A20;
        end;
        defpred P[Nat] means (h.$1)`2 is finite &
                             (h.$1)`2 is Subset of bound_QC-variables;
A81:    for k holds (h.k)`2 is finite &
                    (h.k)`2 is Subset of bound_QC-variables
        proof
A82:      P[0]
          proof
A83:        (h.0)`2 = still_not-bound_in (CX \/ {('not' (f.0)) 'or'
               (the_scope_of(f,0).(bound_in(f,0), x.(min* {j :
                       not x.j in still_not-bound_in (CX \/ {f.0})})))})
                                                               by A4,MCART_1:7;
            reconsider s = ('not' (f.0)) 'or'
                    (the_scope_of(f,0).(bound_in(f,0), x.(min* {j :
                            not x.j in still_not-bound_in (CX \/ {f.0})})))
                                       as Element of CQC-WFF;
            still_not-bound_in s is finite by CQC_SIM1:20; then
            still_not-bound_in {s} is finite by Th27; then
            still_not-bound_in {s} \/ still_not-bound_in CX is finite
                                                       by A80,FINSET_1:14;
            hence thesis by A83,Th28;
          end;
A84:      for k st P[k] holds P[k+1]
          proof
            let k such that A85: P[k];
A86:        ex K,L st K = (h.k)`2 & L = K \/ still_not-bound_in {f.(k+1)} &
              h.(k+1) = [('not' (f.(k+1))) 'or'
                (the_scope_of(f,k+1).(bound_in(f,k+1),
                  x.(min* {i : not x.i in L}))), K \/ still_not-bound_in
            ('not' (f.(k+1))) 'or' (the_scope_of(f,k+1).(bound_in(f,k+1),
                                         x.(min* {j : not x.j in L})))] by A4;
            set K = (h.k)`2;
            reconsider K as Subset of bound_QC-variables by A85;
            set L = K \/ still_not-bound_in {f.(k+1)};
A87:        (h.(k+1))`2 = K \/ still_not-bound_in
            ('not' (f.(k+1))) 'or' (the_scope_of(f,(k+1)).(bound_in(f,k+1),
                              x.(min* {j : not x.j in L}))) by A86,MCART_1:7;
            set s = ('not' (f.(k+1))) 'or' (the_scope_of(f,(k+1)).
                               (bound_in(f,k+1),x.(min* {j : not x.j in L})));
            still_not-bound_in s is finite by CQC_SIM1:20;
            hence thesis by A87,A85,FINSET_1:14;
          end;
          for k holds P[k] from NAT_1:sch 1(A82,A84);
          hence thesis;
        end;
        defpred P[Nat] means still_not-bound_in (F.($1+1)) c= (h.$1)`2 &
                not x.(min* {i :
                   not x.i in still_not-bound_in {f.($1+1)} \/ (h.$1)`2})
                             in still_not-bound_in (F.($1+1) \/ {f.($1+1)});
A88:    for k holds still_not-bound_in (F.(k+1)) c= (h.k)`2 &
                not x.(min* {i :
                   not x.i in still_not-bound_in {f.(k+1)} \/ (h.k)`2})
                                   in still_not-bound_in (F.(k+1) \/ {f.(k+1)})
        proof
A89:      P[0]
          proof
            set r = ('not' (f.0)) 'or'
               (the_scope_of(f,0).(bound_in(f,0), x.(min* {k :
                       not x.k in still_not-bound_in (CX \/ {f.0})})));
            set A1 = {r};
            reconsider s = f.1 as Element of CQC-WFF;
A95:        (h.0)`2 = still_not-bound_in (CX \/ A1) by A4,MCART_1:7; then
            reconsider B = (h.0)`2 as Element of bool bound_QC-variables;
            reconsider C = still_not-bound_in {s} \/ B as Element of
                                                      bool bound_QC-variables;
            still_not-bound_in s is finite by CQC_SIM1:20; then
            still_not-bound_in {s} is finite &
            (h.0)`2 is finite by A81,Th27; then
            C is finite by FINSET_1:14; then
            consider x such that A90: not x in C by Th29;
            consider l being Nat such that A91: x.l = x by QC_LANG3:36;
            not x.l in C by A90,A91; then
            l in {i : not x.i in C}; then
            reconsider A  = {i : not x.i in C} as non empty set;
            now let a such that A92: a in A;
              consider i such that A93: a = i & not x.i in C by A92;
              thus a in NAT by A93;
            end; then
            reconsider A  = {i : not x.i in C} as non empty Subset of NAT
                                                               by TARSKI:def 3;
            set j = min* A;
            j in A by HENMODEL:def 1; then
            consider i such that A94: i = j & not x.i in C;
A96:        not x.j in (h.0)`2 &
            not x.j in still_not-bound_in {s} by A94,XBOOLE_0:def 2;
A98:        F.0 = CX \/ {(h.n)`1 : n in 0} by A20;
            now let b such that A100: b in {(h.n)`1 : n in 0};
              consider n such that A101: b = (h.n)`1 & n in 0 by A100;
              thus contradiction by A101;
            end; then
            {(h.n)`1 : n in 0} = {} by XBOOLE_0:def 1; then
A102:       F.0 = CX by A98;
            (h.0)`1 = r by A4,MCART_1:7; then
A103:       F.(0+1) = CX \/ {r} by A102,A53;
            reconsider D = F.1 as Subset of CQC-WFF;
A104:       still_not-bound_in D c= (h.0)`2 by A103,A95; then
            not x.j in still_not-bound_in D by A96; then
            not x.j in still_not-bound_in D \/ still_not-bound_in {s}
                                                        by A96,XBOOLE_0:def 2;
            hence thesis by A104,Th28;
          end;
A103:     for k st P[k] holds P[k+1]
          proof
            let k such that A104: P[k];
            reconsider s = f.(k+2) as Element of CQC-WFF;
A105:       ex K,L st K = (h.k)`2 & L = K \/ still_not-bound_in {f.(k+1)} &
                  h.(k+1) = [('not' (f.(k+1))) 'or' (the_scope_of(f,(k+1)).
                       (bound_in(f,k+1),x.(min* {i : not x.i in L}))),
                       K \/ still_not-bound_in
                ('not' (f.(k+1))) 'or'  (the_scope_of(f,k+1).(bound_in(f,k+1),
                                         x.(min* {j : not x.j in L})))] by A4;
            set K = (h.k)`2;
            reconsider K as Subset of bound_QC-variables by A81;
            set L = K \/ still_not-bound_in {f.(k+1)};
            set r = ('not' (f.(k+1))) 'or' (the_scope_of(f,(k+1)).
                       (bound_in(f,k+1),x.(min* {i : not x.i in L})));
A106:       (h.(k+1))`1 = r &
            (h.(k+1))`2 = K \/ still_not-bound_in r by A105,MCART_1:7; then
            reconsider B = (h.(k+1))`2 as Element of bool bound_QC-variables;
            reconsider C = still_not-bound_in {s} \/ B as Element of
                                                      bool bound_QC-variables;
            still_not-bound_in s is finite by CQC_SIM1:20; then
            still_not-bound_in {s} is finite &
            (h.(k+1))`2 is finite by A81,Th27; then
            C is finite by FINSET_1:14; then
            consider x such that A90: not x in C by Th29;
            consider l being Nat such that A91: x.l = x by QC_LANG3:36;
            l in {i : not x.i in still_not-bound_in {s} \/ B} by A90,A91; then
            reconsider A = {i : not x.i in still_not-bound_in {s} \/ B}
                                                              as non empty set;
            now let a such that A92: a in A;
              consider i such that A93: a = i & not x.i in C by A92;
              thus a in NAT by A93;
            end; then
            reconsider A = {i : not x.i in still_not-bound_in {s} \/ B}
                                    as non empty Subset of NAT by TARSKI:def 3;
            set j = min* A;
            j in A by HENMODEL:def 1; then
            consider i such that A94: i = j & not x.i in C;
A95:        not x.j in B & not x.j in still_not-bound_in {s}
                                                  by A94,XBOOLE_0:def 2;
            still_not-bound_in (F.(k+1)) \/ still_not-bound_in r c= B
                                                  by A106,A104,XBOOLE_1:9; then
            still_not-bound_in (F.(k+1)) \/ still_not-bound_in {r} c= B
                                                        by Th27; then
            still_not-bound_in (F.(k+1) \/ {r}) c= B by Th28; then
A96:        still_not-bound_in (F.(k+1+1)) c= B by A53,A106; then
            not x.j in still_not-bound_in (F.(k+1+1)) by A95; then
            not x.j in still_not-bound_in (F.(k+1+1)) \/ still_not-bound_in {s}
                                                by A95,XBOOLE_0:def 2;
            hence thesis by A96,Th28;
          end;
          for k holds P[k] from NAT_1:sch 1(A89,A103);
          hence thesis;
        end;
        defpred P[Nat] means F.$1 is Consistent;
A64:    P[0]
        proof
          now let a such that A65: a in {(h.i)`1 : i in 0};
            consider j such that A66: a = (h.j)`1 & j in 0 by A65;
            thus contradiction by A66;
          end; then
          {(h.i)`1 : i in 0} = {} by XBOOLE_0:def 1; then
          F.0 = CX \/ {} by A20;
          hence P[0];
        end;
A67:   for k st P[k] holds P[k+1]
       proof
         let k such that A68: P[k];
         consider c,d such that
A69:                 c in CQC-WFF & d in bool bound_QC-variables & h.k = [c,d]
                                                          by ZFMISC_1:def 2;
         reconsider r = (h.k)`1 as Element of CQC-WFF by A69,MCART_1:7;
         now assume F.(k+1) is Inconsistent; then
           F.(k+1) |- VERUM & F.(k+1) |- 'not' VERUM by HENMODEL:6; then
           F.k \/ {r} |- 'not' VERUM by A53; then
           consider f1 being FinSequence of CQC-WFF such that
A70:           rng f1 c= F.k & |- f1^<*r*>^<*'not' VERUM*> by HENMODEL:8;
A71:       |- f1^<*'not' (f.k)*>^<*'not' (f.k)*> by CALCUL_2:21;
A72:       now assume A73: k = 0; then
A74:         r = 'not' (f.0) 'or' (the_scope_of(f,0).(bound_in(f,0),
                x.(min* {i : not x.i in still_not-bound_in (CX \/ {f.0})})))
                                                        by A4,MCART_1:7;
             reconsider s = the_scope_of(f,0).(bound_in(f,0),
             x.(min* {i : not x.i in still_not-bound_in (CX \/ {f.0})}))
                                          as Element of CQC-WFF;
A75:         |- (f1^<*'not' (f.k)*>)^<*('not' (f.k)) 'or' s*>
                                                   by A71,CALCUL_1:51;
             0+1 <= len (f1^<*r*>) by CALCUL_1:10; then
             0 < len (f1^<*r*>); then
             |- Ant(f1^<*r*>)^<*'not' (f.k)*>^<*Suc(f1^<*r*>)*>^<*'not' VERUM*>
                                                           by A70,Th26; then
             |- f1^<*'not' (f.k)*>^<*Suc(f1^<*r*>)*>^<*'not' VERUM*>
                                                      by CALCUL_1:5; then
             |- (f1^<*'not' (f.k)*>)^<*r*>^<*'not' VERUM*> by CALCUL_1:5;then
A77:         |- f1^<*'not' (f.k)*>^<*'not' VERUM*>
               by CALCUL_2:24,A73,A74,A75;
             |- f1^<*s*>^<*s*> by CALCUL_2:21; then
A78:         |- f1^<*s*>^<*('not' (f.k)) 'or' s*> by CALCUL_1:52;
             0+1 <= len (f1^<*r*>) by CALCUL_1:10; then
             0 < len (f1^<*r*>); then
             |- Ant(f1^<*r*>)^<*s*>^<*Suc(f1^<*r*>)*>^<*'not' VERUM*>
                                                           by A70,Th26; then
             |- f1^<*s*>^<*Suc(f1^<*r*>)*>^<*'not' VERUM*> by CALCUL_1:5; then
             |- (f1^<*s*>)^<*r*>^<*'not' VERUM*> by CALCUL_1:5; then
A79:         |- f1^<*s*>^<*'not' VERUM*> by A73,A74,A78,CALCUL_2:24;
            set y = x.(min* {i : not x.i in still_not-bound_in (CX \/ {f.0})});
            reconsider r1 = f.0 as Element of CQC-WFF;
            set C = still_not-bound_in (CX \/ {r1});
            still_not-bound_in r1 is finite by CQC_SIM1:20; then
            still_not-bound_in {r1} is finite by Th27; then
            still_not-bound_in {r1} \/ still_not-bound_in CX is finite
                                                       by A80,FINSET_1:14; then
            C is finite by Th28; then
            consider x such that A80: not x in C by Th29;
            consider l being Nat such that A81: x.l = x by QC_LANG3:36;
            l in {i : not x.i in C} by A80,A81; then
            reconsider A  = {i : not x.i in C} as non empty set;
            now let a such that A82: a in A;
              consider i such that A83: a = i & not x.i in C by A82;
              thus a in NAT by A83;
            end; then
            reconsider A  = {i : not x.i in C} as non empty Subset of NAT
                                                       by TARSKI:def 3;
            set j = min* A;
            j in A by HENMODEL:def 1; then
            consider i such that A84: i = j & not x.i in C;
            not x.i in still_not-bound_in CX \/ still_not-bound_in {r1}
                                                                   by A84,Th28;
            then A85: not x.i in still_not-bound_in CX &
                      not x.i in still_not-bound_in {r1} by XBOOLE_0:def 2;
A86:        F.0 = CX \/ {(h.n)`1 : n in 0} by A20;
            now let b such that A100: b in {(h.n)`1 : n in 0};
              consider n such that A101: b = (h.n)`1 & n in 0 by A100;
