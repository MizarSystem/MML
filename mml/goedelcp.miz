            end; then
            {(h.n)`1 : n in 0} = {} by XBOOLE_0:def 1; then
            still_not-bound_in rng f1 c=
            still_not-bound_in CX by A86,A70,A73,Th30; then
            not x.i in still_not-bound_in rng f1 by A85; then
A88:        not x.j in still_not-bound_in f1 by A84,Th31;
            reconsider r2 = the_scope_of(f,0) as Element of CQC-WFF;
            reconsider y2 = bound_in(f,0) as bound_QC-variable;
            r1 in ExCl by A1,FUNCT_1:12; then
            consider y1,s1 such that A89: r1 = Ex(y1,s1) by Def3;
A90:        s1 = Ex-the_scope_of r1 & r2 = Ex-the_scope_of r1
                                                             by A89,Th21,Def12;
A91:        y1 = Ex-bound_in r1 & y2 = Ex-bound_in r1 by A89,Th21,Def11;
A92:        |- f1^<*s1.(y1,x.j)*>^<*'not' VERUM*> by A79,A90,A91;
            not x.j in still_not-bound_in r1 by A84,A85,Th27; then
            not x.j in still_not-bound_in <*r1*> by CALCUL_1:60; then
            not x.j in still_not-bound_in f1 \/ still_not-bound_in <*r1*>
                                                by A88,XBOOLE_0:def 2; then
A93:        not x.j in still_not-bound_in (f1^<*r1*>) by CALCUL_1:59;
            not x.j in still_not-bound_in 'not' VERUM by QC_LANG3:11,7;
            then not x.j in still_not-bound_in <*'not' VERUM*> by CALCUL_1:60;
            then not x.j in still_not-bound_in (f1^<*r1*>) \/
                    still_not-bound_in <*'not' VERUM*> by A93,XBOOLE_0:def 2;
            then not x.j in still_not-bound_in (f1^<*r1*>^<*'not' VERUM*>)
                                                       by CALCUL_1:59; then
A94:        |- f1^<*r1*>^<*'not' VERUM*> by A89,A92,CALCUL_1:61;
            |- f1^<*'not' VERUM*> by A73,A77,A94,CALCUL_2:26; then
            F.k |- 'not' VERUM by A70,HENMODEL:def 2;
            hence contradiction by A64,A73,Th24;
           end;
           now assume k <> 0; then
             consider k1 being Nat such that A95: k = k1+1 by NAT_1:22;
A96:         ex K,L st K = (h.k1)`2 & L = K \/ still_not-bound_in {f.(k1+1)} &
                  h.(k1+1) = [('not' (f.(k1+1))) 'or' (the_scope_of(f,(k1+1)).
                       (bound_in(f,k1+1),x.(min* {i : not x.i in L}))),
                       K \/ still_not-bound_in
              ('not' (f.(k1+1))) 'or' (the_scope_of(f,k1+1).(bound_in(f,k1+1),
                                         x.(min* {j : not x.j in L})))] by A4;
             set K = (h.k1)`2;
             set r1 = f.(k1+1);
             set L = K \/ still_not-bound_in {r1};
             set p1 = 'not' r1 'or' (the_scope_of(f,(k1+1)).
                       (bound_in(f,k1+1),x.(min* {i : not x.i in L})));
A97:         r = p1 &
                (h.(k1+1))`2 = K \/ still_not-bound_in p1 by A95,A96,MCART_1:7;
             reconsider s = (the_scope_of(f,(k1+1)).
                       (bound_in(f,k1+1),x.(min* {i : not x.i in L})))
               as Element of CQC-WFF;
A98:         |- (f1^<*'not' r1*>)^<*p1*> by A71,A95,CALCUL_1:51;
             0+1 <= len (f1^<*r*>) by CALCUL_1:10; then
             0 < len (f1^<*r*>); then
             |- Ant(f1^<*r*>)^<*'not' r1*>^<*Suc(f1^<*r*>)*>^<*'not' VERUM*>
                                                           by A70,Th26; then
             |- f1^<*'not' r1*>^<*Suc(f1^<*r*>)*>^<*'not' VERUM*>
                                                      by CALCUL_1:5; then
            |- (f1^<*'not' r1*>)^<*r*>^<*'not' VERUM*> by CALCUL_1:5; then
A100:       |- f1^<*'not' r1*>^<*'not' VERUM*> by CALCUL_2:24,A97,A98;
            |- f1^<*s*>^<*s*> by CALCUL_2:21; then
A101:       |- f1^<*s*>^<*p1*> by CALCUL_1:52;
            0+1 <= len (f1^<*r*>) by CALCUL_1:10; then
            0 < len (f1^<*r*>); then
            |- Ant(f1^<*r*>)^<*s*>^<*Suc(f1^<*r*>)*>^<*'not' VERUM*>
              by A70,Th26; then
            |- f1^<*s*>^<*Suc(f1^<*r*>)*>^<*'not' VERUM*> by CALCUL_1:5; then
            |- (f1^<*s*>)^<*p1*>^<*'not' VERUM*> by A97,CALCUL_1:5; then
A102:       |- f1^<*s*>^<*'not' VERUM*> by A101,CALCUL_2:24;
            set y = x.(min* {i : not x.i in L});
            set j = min* {i : not x.i in L};
            reconsider r2 = the_scope_of(f,k1+1) as Element of CQC-WFF;
            reconsider y2 = bound_in(f,k1+1) as bound_QC-variable;
            reconsider r1 as Element of CQC-WFF;
            r1 in ExCl by A1,FUNCT_1:12; then
            consider y1,s1 such that A103: r1 = Ex(y1,s1) by Def3;
A104:       s1 = Ex-the_scope_of r1 & r2 = Ex-the_scope_of r1
              by A103,Th21,Def12;
            y1 = Ex-bound_in r1 & y2 = Ex-bound_in r1 by A103,Th21,Def11; then
A106:       |- f1^<*s1.(y1,x.j)*>^<*'not' VERUM*> by A102,A104;
            reconsider Z = F.k as Subset of CQC-WFF;
            not y in still_not-bound_in (Z \/ {r1}) by A95,A88; then
            not y in still_not-bound_in Z \/ still_not-bound_in {r1}
                                                                by Th28; then
A107:       not y in still_not-bound_in Z & not y in still_not-bound_in {r1}
                                                      by XBOOLE_0:def 2;
            still_not-bound_in rng f1 c= still_not-bound_in Z by A70,Th30; then
            not y in still_not-bound_in rng f1 by A107; then
A108:       not y in still_not-bound_in f1 by Th31;
            not y in still_not-bound_in r1 by A107,Th27; then
            not y in still_not-bound_in <*r1*> by CALCUL_1:60; then
            not y in still_not-bound_in f1 \/
                     still_not-bound_in <*r1*> by A108,XBOOLE_0:def 2; then
A109:       not y in still_not-bound_in (f1^<*r1*>) by CALCUL_1:59;
            not x.j in still_not-bound_in 'not' VERUM by QC_LANG3:11,7;
            then not x.j in still_not-bound_in <*'not' VERUM*> by CALCUL_1:60;
            then not x.j in still_not-bound_in (f1^<*r1*>) \/
                    still_not-bound_in <*'not' VERUM*> by A109,XBOOLE_0:def 2;
            then not x.j in still_not-bound_in (f1^<*r1*>^<*'not' VERUM*>)
                                                       by CALCUL_1:59; then
            |- f1^<*r1*>^<*'not' VERUM*> by A103,A106,CALCUL_1:61; then
            |- f1^<*'not' VERUM*> by A100,CALCUL_2:26; then
            F.k |- 'not' VERUM by A70,HENMODEL:def 2;
            hence contradiction by A68,Th24;
         end;
         hence contradiction by A72;
       end;
       hence P[k+1];
     end;
     for n holds P[n] from NAT_1:sch 1(A64,A67); then
     for n,m st m in dom F & n in dom F & n < m holds F.n is Consistent &
       F.n c= F.m by A40; then
     reconsider CY as Consistent Subset of CQC-WFF by A21,HENMODEL:11;
     take CY;
     thus thesis by A17,XBOOLE_1:7;
   end;

theorem
  Th34: X |- p & X c= Y implies Y |- p
  proof
    assume A1: X |- p & X c= Y; then
    consider f being FinSequence of CQC-WFF such that
A2: rng f c= X & |- f^<*p*> by HENMODEL:def 2;
    rng f c= Y by A1,A2,XBOOLE_1:1;
    hence thesis by A2,HENMODEL:def 2;
 end;

 reserve C,D for Element of bool CQC-WFF;

:: Ebb et al, Chapter V, Lemma 2.2

theorem
  Th35: CX is with_examples implies ex CY st CX c= CY &
                CY is negation_faithful & CY is with_examples
  proof
    assume A35: CX is with_examples;
    consider f being Function such that
A1: dom f = NAT & CQC-WFF = rng f by Th19,Th33;
    reconsider f as Function of NAT,CQC-WFF by A1,FUNCT_2:4;
    defpred P[set,set,set] means
        ex X st X = $2 \/ {f.$1} & (X is Consistent implies $3 = X) &
                                (not X is Consistent implies $3 = $2);
A2: for n for C ex D st P[n,C,D]
    proof
      let n;
      reconsider p = f.n as Element of CQC-WFF;
      let C;
      set X = C \/ {p};
      reconsider X as Subset of CQC-WFF;
      not X is Consistent implies ex D st D = C &
        ex X st X = C \/ {p} &  (X is Consistent implies D = X) &
          (not X is Consistent implies D = C);
      hence thesis;
    end;
    consider h being Function of NAT,bool CQC-WFF such that
A6: h.0 = CX & for n holds P[n,h.n,h.(n+1)] from RECDEF_1:sch 2(A2);
    set CY = union rng h;
    now let a such that A7: a in CX;
      dom h = NAT by FUNCT_2:def 1; then 
      a in h.0 & h.0 in rng h by A6,A7,FUNCT_1:12;
      hence a in union rng h by TARSKI:def 4;
    end; then
A8: CX c= CY by TARSKI:def 3;
A13:  for n holds h.n c= h.(n+1)
      proof
        let n; let a such that A9: a in h.n;
        reconsider p = f.n as Element of CQC-WFF;
        set X = h.n \/ {p};
        reconsider X as Subset of CQC-WFF;
A10:    h.n c= X by XBOOLE_1:7;
        ex Y st Y = h.n \/ {f.n} & (Y is Consistent implies h.(n+1) = Y) &
           (not Y is Consistent implies h.(n+1) = h.n) by A6;
        hence a in h.(n+1) by A9,A10;
      end;
A14: for n,m st m in dom h & n in dom h & n < m holds h.n c= h.m
     proof
       let n,m such that A15: m in dom h & n in dom h & n < m;
       defpred P[Nat] means n <= $1 implies h.n c= h.$1;
A16:   P[0]
       proof
         assume n <= 0; then
         n = 0 by NAT_1:18;
         hence thesis;
       end;
A17:   for k st P[k] holds P[k+1]
       proof
         let k such that A18: P[k];
         assume A19: n <= k+1;
         now assume A21: n <= k;
           h.k c= h.(k+1) by A13;
           hence thesis by A21,A18,XBOOLE_1:1;
         end;
         hence thesis by A19,NAT_1:26;
       end;
       for k holds P[k] from NAT_1:sch 1(A16,A17);
       hence thesis by A15;
     end;
     defpred P[Nat] means h.$1 is Consistent;
A25: P[0] by A6;
A26: for k st P[k] holds P[k+1]
     proof
       let n such that A22: P[n];
       set p = f.n;
       set X = h.n \/ {p};
       reconsider X as Subset of CQC-WFF;
       ex Y st Y = h.n \/ {f.n} & (Y is Consistent implies h.(n+1) = Y) &
                    (not Y is Consistent implies h.(n+1) = h.n) by A6;
       hence thesis by A22;
     end;
     set CY = union rng h;
     for n holds P[n] from NAT_1:sch 1(A25,A26); then
     for n,m st m in dom h & n in dom h & n < m holds h.n is Consistent &
                     h.n c= h.m by A14; then
     reconsider CY as Consistent Subset of CQC-WFF by HENMODEL:11;
A25: CY is negation_faithful
     proof
       let p;
       consider a such that A26: a in dom f & f.a = p by A1,FUNCT_1:def 5;
       reconsider n = a as Nat by A26;
       set X =  h.n \/ {f.n};
       now assume not CY |- 'not' p; then
A27:     not CY \/ {p} is Inconsistent by HENMODEL:10;
         now assume h.n \/ {p} is Inconsistent; then
A28:       h.n \/ {p} |- 'not' VERUM by Th24;
           now let a such that A29: a in h.n;
             dom h = NAT by FUNCT_2:def 1; then
             h.n in rng h by FUNCT_1:12;
             hence a in CY by A29,TARSKI:def 4;
           end; then
           h.n c= CY by TARSKI:def 3; then
           h.n \/ {p} c= CY \/ {p} by XBOOLE_1:9; then
           CY \/ {p} |- 'not' VERUM by A28,Th34;
           hence contradiction by A27,Th24;
         end; then
A29:     h.n \/ {f.n} is Consistent by A26;
         ex Y st Y = h.n \/ {f.n} & (Y is Consistent implies h.(n+1) = Y) &
                    (not Y is Consistent implies h.(n+1) = h.n) by A6; then
A30:     h.(n+1) = h.n \/ {f.n} by A29;
         now let a such that A29: a in h.(n+1);
           dom h = NAT by FUNCT_2:def 1; then
           h.(n+1) in rng h by FUNCT_1:12;
           hence a in CY by A29,TARSKI:def 4;
         end; then
A32:     h.(n+1) c= CY by TARSKI:def 3;
         {p} c= h.(n+1) by A26,A30,XBOOLE_1:7; then
         {p} c= CY by A32,XBOOLE_1:1; then
         p in CY by ZFMISC_1:37;
         hence CY |- p by Th20;
       end;
       hence thesis;
     end;
A33: CY is with_examples
     proof
       let x,p;
       consider y such that
A34:   CX |- ('not' Ex(x,p)) 'or' (p.(x,y)) by A35,Def2;
       take y;
       thus thesis by A8,A34,Th34;
     end;
     take CY;
     thus thesis by A8,A25,A33;
   end;

 reserve JH1 for Henkin_interpretation of CZ,
         J for interpretation of A,
         v,w for Element of Valuations_in A;

theorem
  Th36: still_not-bound_in CX is finite implies ex CZ,JH1 st JH1,valH |= CX
  proof
    assume still_not-bound_in CX is finite; then
    consider CY such that A1: CX c= CY & CY is with_examples by Th32;
    consider CZ such that A2: CY c= CZ & CZ is negation_faithful &
    CZ is with_examples by A1,Th35;
    A3: CX c= CZ by A1,A2,XBOOLE_1:1;
    consider JH1;
A5: now let p such that A4: p in CX;
      CZ |- p by Th20,A3,A4;
      hence JH1,valH |= p by A2,Th16;
    end;
    take CZ,JH1;
    thus thesis by A5,CALCUL_1:def 11;
  end;
  
begin :: Goedel's Completeness Theorem,
      :: Ebb et al, Chapter V, Completeness Theorem 4.1 

theorem
  still_not-bound_in X is finite & X |= p implies X |- p
  proof
    assume A1: still_not-bound_in X is finite & X |= p;
    now assume not X |- p; then
      reconsider CX = X \/ {'not' p} as Consistent Subset of CQC-WFF
        by HENMODEL:9;
A10:  for A,J,v holds not J,v |= CX
      proof
        let A,J,v;
        now assume A4: J,v |= X \/ {'not' p};
          now let q such that A6: q in X;
            X c= X \/ {'not' p} by XBOOLE_1:7;
            hence J,v |= q by A4,CALCUL_1:def 11,A6;
          end; then
A7:       J,v |= X by CALCUL_1:def 11;
          now let q such that A6: q in {'not' p};
            {'not' p} c= X \/ {'not' p} by XBOOLE_1:7;
            hence J,v |= q by A4,CALCUL_1:def 11,A6;
          end; then
A8:       J,v |= {'not' p} by CALCUL_1:def 11;
          'not' p in {'not' p} by TARSKI:def 1; then
          J,v |= 'not' p by A8,CALCUL_1:def 11; then
          J,v |= X & not J,v |= p by A7,VALUAT_1:28;
          hence contradiction by A1,CALCUL_1:def 12;
        end; 
        hence not J,v |= CX;
      end;
      still_not-bound_in 'not' p is finite by CQC_SIM1:20; then
      still_not-bound_in {'not' p} is finite by Th27; then
      still_not-bound_in X \/
      still_not-bound_in {'not' p} is finite by A1,FINSET_1:14; then
      still_not-bound_in CX is finite by Th28; then
      consider CZ,JH1 such that A11: JH1,valH |= CX by Th36;
      thus contradiction by A10,A11;
    end;
    hence thesis;
  end;
