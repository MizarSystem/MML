    proof
      assume
A17:  j = l;
      M2*(i,j) = (SXLine(M1,l,a))*(j,i) by A11,A16,MATRIX_1:def 7
        .= a*(M1*(l,i)) by A15,A17,Def2
        .= a*(M*(i,l)) by A4,A13,A17,MATRIX_1:def 7;
      hence thesis;
    end;
    assume
A18: j <> l;
    M2*(i,j) = (SXLine(M1,l,a))*(j,i) by A11,A16,MATRIX_1:def 7
      .= M1*(j,i) by A15,A18,Def2
      .= M*(i,j) by A4,A13,MATRIX_1:def 7;
