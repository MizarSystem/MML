      0 <= mk+1+1 by NAT_1:18;
then A63: s.insloc (mk+1+1) = halt SCM+FSA by A48,A62;
A64: now let n be Nat;
       assume n = 0; hence
A65:   (Computation s).n = s by AMI_1:def 19; hence
A66:   CurInstr (Computation s).n = a:= intloc 0 by A1,A51,AMI_1:def 17;
