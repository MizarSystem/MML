    end;
A75: now
      let n;
      b.n in ].a1.n,x0.[ by A67;
      then b.n in {g1:a1.n<g1 & g1<x0} by RCOMP_1:def 2;
      then ex g1 st g1=b.n & a1.n<g1 & g1<x0;
