A100: z in F.i by A96,A99,XBOOLE_0:def 4;
A101: k in Seg len FG by A98,FINSEQ_1:def 3;
    then
A102: k <= la*lb by A12,FINSEQ_1:3;
A103: lb <> 0 by A12,A101;
