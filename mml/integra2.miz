            then D2.(j-1) <= D2.j by A3,A23,SEQ_4:154;
            then D2.(j-1) <= D1.i by A8,A10,XXREAL_0:2;
            then D2.(j-1) in {r:lower_bound A <= r & r <= D1.i} by A24;
