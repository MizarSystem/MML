A16:        D2.j in A;
            A = [.lower_bound A,upper_bound A.] by INTEGRA1:5;
            then D2.j in {r:lower_bound A<=r & r<=upper_bound A} by A16,
RCOMP_1:def 1;
            then ex x st x=D2.j & lower_bound A<=x & x<=upper_bound A;
