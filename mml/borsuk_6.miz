theorem Th28:
  { p where p is Point of [:I[01],I[01]:] : p`2 <= 1 - 2 * (p`1) }
     is closed non empty Subset of [:I[01],I[01]:]
  proof
     set GG = [:I[01],I[01]:], SS = [:R^1,R^1:];
     0 in the carrier of I[01] by JORDAN5A:2; then
     [0,0] in [:the carrier of I[01], the carrier of I[01]:]
       by ZFMISC_1:106; then
     reconsider x = [0,0] as Point of GG by BORSUK_1:def 5;
     x`1 = 0 & x`2 = 0 by MCART_1:7; then
A1:  x`2 <= 1 - 2 * (x`1);
     set P0 = { p where p is Point of GG : p`2 <= 1 - 2 * (p`1) };
A2:  x in P0 by A1;
     reconsider PA = { p where p is Point of SS : p`2 <= 1 - 2 * (p`1) }
       as closed Subset of SS by Th25;
A3:  GG is SubSpace of SS by BORSUK_3:25;
A4:  P0 = PA /\ [#] GG
     proof
       thus P0 c= PA /\ [#] GG
       proof
         let x be set;
         assume x in P0; then
         consider p being Point of GG such that
A5:      x = p & p`2 <= 1 - 2 * (p`1);
A6:      x in the carrier of GG by A5;
         the carrier of GG c= the carrier of SS by A3,BORSUK_1:35; then
         reconsider a = x as Point of SS by A6;
         a`2 <= 1 - 2 * (a`1) by A5; then
A7:      x in PA;
         x in [#] GG by A6,PRE_TOPC:def 3;
         hence thesis by A7,XBOOLE_0:def 3;
       end;
       let x be set;
       assume x in PA /\ [#] GG; then
A8:    x in PA & x in [#] GG by XBOOLE_0:def 3; then
       consider p being Point of SS such that
A9:    x = p & p`2 <= 1 - 2 * (p`1);
       thus thesis by A8,A9;
     end; then
     P0 c= [#] GG by XBOOLE_1:17; then
     reconsider P0 as Subset of GG by PRE_TOPC:def 3;
     P0 is closed by A3,A4,PRE_TOPC:43;
     hence thesis by A2;
  end;

theorem Th29:
