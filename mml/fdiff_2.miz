       r1 <= rx & r2 <= rx by SQUARE_1:46;
    then A7: r1 + 0 < rx + 1 & r2 + 0 < rx + 1 by REAL_1:67;
    then r1 in {g1: rn - 1 < g1 & g1 < rx + 1} by A6;
