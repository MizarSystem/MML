          hence a in f"D by A14,FUNCT_1:def 13;
        end;
        let a be set; assume
A15:      a in f"D;
then A16:    a in dom f & f.a in D by FUNCT_1:def 13;
        consider s, t being set such that
A17:       s in the carrier of N & t in the carrier of N & a = [s,t]
            by A2,A15,ZFMISC_1:def 2;
