P18:for e be Element of REAL st 0 < e
     ex N be Element of NAT st
      for n be Element of NAT st N<=n holds
       abs(upper_integral((H.n)||AB)
           -upper_integral(lim(H,AB)||AB )) <=e
    proof
     let e be Element of REAL;
     assume 0 < e; then
E1:   0 < e/(b-a) by a6,XREAL_1:141;
     e/(b-a) is Element of REAL by XREAL_0:def 1; then
     consider N be Element of NAT such that  
E2:   for n,k be Element of NAT st N<=n holds
       abs(upper_sum((H.n)||AB,T).k-upper_sum(H0,T).k) 
        <= (e/(b-a)) * (b-a) by E1,P16;
E3:  now let n,k be Element of NAT;
      assume N<=n; then
      abs(upper_sum((H.n)||AB,T).k-upper_sum(H0,T).k)
        <=(e/(b-a)) * (b-a) by E2; 
      hence abs(upper_sum((H.n)||AB,T).k-upper_sum(H0,T).k)
             <= e by a6,XCMPLX_1:88;
     end;
     take N;
     hereby let n be Element of NAT;
      assume AS3: N<=n;
  U1: now let k be Element of NAT;
       abs(upper_sum((H.n)||AB,T).k-upper_sum(H0,T).k) <=e by AS3,E3; then
       abs(upper_sum((H.n)||AB,T).k+(-upper_sum(H0,T)).k) <= e
         by SEQ_1:14; then
       abs((upper_sum((H.n)||AB,T)+-upper_sum(H0,T)).k) <= e
         by SEQ_1:11; then
       abs((upper_sum((H.n)||AB,T)-upper_sum(H0,T)).k) <= e by SEQ_1:15;
       hence abs(upper_sum((H.n)||AB,T)-upper_sum(H0,T)).k <= e by SEQ_1:16;
      end;
U2:   upper_sum((H.n)||AB,T) is convergent by P15; then
U4:   upper_sum((H.n)||AB,T)-upper_sum(H0,T) is convergent by P252,SEQ_2:25;
      lim(upper_sum((H.n)||AB,T)-upper_sum(H0,T))
       = lim upper_sum((H.n)||AB,T) -lim upper_sum(H0,T)
          by SEQ_2:26,U2,P252; then
      lim(upper_sum((H.n)||AB,T)-upper_sum(H0,T))
       = upper_integral((H.n)||AB) -lim upper_sum(H0,T) by P15; then
U5:   lim(upper_sum((H.n)||AB,T)-upper_sum(H0,T))
       = upper_integral((H.n)||AB) - upper_integral(lim(H,AB)||AB)
         by INTEGRA4:9,P251,P23,P20;
      abs(upper_sum((H.n)||AB,T)-upper_sum(H0,T)) is convergent
        by U4,SEQ_4:26; then
      lim abs(upper_sum((H.n)||AB,T)-upper_sum(H0,T)) <= e by PREPOWER:3,U1;
      hence abs(upper_integral((H.n)||AB) - upper_integral(lim(H,AB)||AB))
        <=e by U5,U4,SEQ_4:27;
     end;
    end;

LP18:
    for e be Element of REAL st 0 < e
     ex N be Element of NAT st
      for n be Element of NAT st N<=n holds
       abs(lower_integral((H.n)||AB)
           -lower_integral(lim(H,AB)||AB )) <=e
    proof
     let e be Element of REAL;
     assume AS1: 0 < e;
     consider N be Element of NAT such that
AS2:  for n,k be Element of NAT st N<=n holds
       abs(lower_sum((H.n)||AB,T).k-lower_sum(H0,T).k) <=e by AS1,LP17;
     take N;
     hereby let n be Element of NAT;
      set LHT = lower_sum((H.n)||AB,T);
      assume AS3: N<=n;
  U1: now let k be Element of NAT;
       abs(LHT.k - lower_sum(H0,T).k) <=e by AS3,AS2; then
       abs(LHT.k + (-lower_sum(H0,T)).k) <= e by SEQ_1:14; then
       abs((LHT + -lower_sum(H0,T)).k) <= e by SEQ_1:11; then
       abs((LHT - lower_sum(H0,T)).k) <= e by SEQ_1:15;
       hence abs(LHT - lower_sum(H0,T)).k <= e by SEQ_1:16;
      end;
U2:   LHT is convergent by P15; then
      lim(LHT - lower_sum(H0,T)) = lim LHT -lim lower_sum(H0,T)
        by SEQ_2:26,P252; then
      lim(LHT - lower_sum(H0,T))
        = lower_integral((H.n)||AB) -lim lower_sum(H0,T) by P15; then
U5:   lim(LHT - lower_sum(H0,T))
       = lower_integral((H.n)||AB) -lower_integral(lim(H,AB)||AB)
           by INTEGRA4:8,P251,P23,P20;
U4:   LHT - lower_sum(H0,T) is convergent by U2,P252,SEQ_2:25; then
      abs(LHT - lower_sum(H0,T)) is convergent by SEQ_4:26; then
      lim abs(LHT - lower_sum(H0,T)) <= e by PREPOWER:3,U1;
      hence
       abs(lower_integral((H.n)||AB) - lower_integral(lim(H,AB)||AB )) <=e
         by U5,U4,SEQ_4:27;
     end;
    end;
    set K1 = upper_integral(lim(H,AB)||AB);
    set L1 = lower_integral(lim(H,AB)||AB);
P251:
    now let e1 be Element of REAL;
     set e=e1/2;
     assume 0 < e1; then
AS01:0 < e1/2 & e1/2 < e1 by XREAL_1:217,218; then
 AS1:0 < e/2 by XREAL_1:217; then
     consider N1 be Element of NAT such that  
AS2:  for n be Element of NAT st N1<=n holds
       abs(upper_integral((H.n)||AB)
           -upper_integral(lim(H,AB)||AB )) <=e/2 by P18;
     consider N2 be Element of NAT such that  
AS3:  for n be Element of NAT st N2<=n holds
       abs(lower_integral((H.n)||AB)
           -lower_integral(lim(H,AB)||AB )) <=e/2 by LP18,AS1;
     reconsider n=max(N1,N2) as Element of NAT by XXREAL_0:16;
     set K = upper_integral((H.n)||AB);
     set L = lower_integral((H.n)||AB);
     N1<= n by XXREAL_0:25; then
U1:  abs(K - K1) <=e/2 by AS2;
     N2<=n by XXREAL_0:25; then
     abs(L - L1) <=e/2 by AS3; then
U5:  abs(K - L1) <= e/2 by P15;
     (K - K1) - (K - L1) = L1 - K1; then
U7:  abs(L1 - K1) <= abs(K - K1) + abs(K - L1) by COMPLEX1:143;
     abs(K - K1) + abs(K-L1) <= e/2+e/2 by U1,U5,XREAL_1:9; then
     abs(L1 - K1) <= e by U7,XXREAL_0:2;
     hence abs(L1 - K1) < e1 by AS01,XXREAL_0:2;
    end;
    now assume K1 <> L1; then
     0 < abs(L1 -K1) by COMPLEX1:148;
     hence contradiction by P251;
    end; then
    lim(H,AB)||AB is_integrable_on AB by INTEGRA1:def 17,P24;
    hence lim(H,AB) is_integrable_on AB by INTEGRA5:def 2;
P3: for p be real number st
     0<p ex n be Element of NAT st 
      for m be Element of NAT st n<=m holds
       abs(rseq.m-integral(lim(H,AB),a,b))<p
    proof       
     let p be real number;
     assume 0<p; then
AS01:0 < p/2 & p/2 < p by XREAL_1:217,218;
     set e=p/2;
     e is Element of REAL by XREAL_0:def 1; then
     consider N be Element of NAT such that  
U1:   for n be Element of NAT st N<=n holds
       abs(upper_integral((H.n)||AB)
        -upper_integral(lim(H,AB)||AB )) <=e by P18,AS01;
     take N;
     hereby let n be Element of NAT;
      assume N<=n; then
U2:   abs(upper_integral((H.n)||AB) - upper_integral(lim(H,AB)||AB)) <=e by U1;
      upper_integral((H.n)||AB) = integral((H.n)||AB); then
U3:   upper_integral((H.n)||AB) = rseq.n by P15;
      upper_integral(lim(H,AB)||AB) = integral(lim(H,AB),AB); then
      upper_integral(lim(H,AB)||AB) = integral(lim(H,AB),a,b)
        by a1,INTEGRA5:def 5;
      hence abs(rseq.n - integral(lim(H,AB),a,b)) < p by AS01,U3,U2,XXREAL_0:2;
     end;
    end;
    hence rseq is convergent by SEQ_2:def 6;
    hence lim rseq = integral(lim(H,AB),a,b) by P3,SEQ_2:def 7;
end;
