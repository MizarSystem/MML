      consider H1 be Function of AB,REAL such that 
P16A:  rng H1 = {e} & H1 is_bounded_on AB by INTEGRA4:5;
      len lower_volume(H1,T.k) = l0 by INTEGRA1:def 8; then
      reconsider R3=lower_volume(H1,T.k) as Element of l0-tuples_on REAL
        by FINSEQ_2:110;
      lower_sum(Hn,T).k-lower_sum(H0,T).k
       = lower_sum(Hn,T.k)-lower_sum(H0,T).k by INTEGRA2:def 5; then
      lower_sum(Hn,T).k-lower_sum(H0,T).k
       =lower_sum(Hn,T.k)-lower_sum(H0,T.k) by INTEGRA2:def 5; then
P16B: lower_sum(Hn,T).k-lower_sum(H0,T).k = Sum(R1-R2) by RVSUM_1:120;
      lower_sum(H0,T).k-lower_sum(Hn,T).k
       =lower_sum(H0,T.k)-lower_sum(Hn,T).k by INTEGRA2:def 5; then
      lower_sum(H0,T).k-lower_sum(Hn,T).k
       =lower_sum(H0,T.k)-lower_sum(Hn,T.k) by INTEGRA2:def 5; then
LP16B:lower_sum(H0,T).k-lower_sum(Hn,T).k
       =Sum(R2-R1) by RVSUM_1:120; 
P16C: for i be Element of NAT st i in Seg l0 holds 
       (R1-R2).i <= R3.i & (R2-R1).i <= R3.i 
      proof  
       let i be Element of NAT;
       assume T1: i in Seg l0;
       (R1-R2).i = R1.i-R2.i by RVSUM_1:48; then
       (R1-R2).i = inf rng(Hn|divset(T.k,i)) * vol divset(T.k,i)
                    - (lower_volume(H0,T.k)).i by INTEGRA1:def 8,T1; then
       (R1-R2).i = inf rng(Hn|divset(T.k,i)) * vol divset(T.k,i)
                    - inf rng(H0|divset(T.k,i)) * vol divset(T.k,i)
                     by INTEGRA1:def 8,T1; then
T2:    (R1-R2).i = (inf rng(Hn|divset(T.k,i)) - inf rng(H0|divset(T.k,i)))
                    * vol divset(T.k,i);
       (R2-R1).i = R2.i-R1.i by RVSUM_1:48; then
       (R2-R1).i = inf rng(H0|divset(T.k,i)) * vol divset(T.k,i)
                    - (lower_volume(Hn,T.k)).i by INTEGRA1:def 8,T1; then
       (R2-R1).i = inf rng(H0|divset(T.k,i)) * vol divset(T.k,i)
                    - inf rng(Hn|divset(T.k,i)) * vol divset(T.k,i)
                     by INTEGRA1:def 8,T1; then
LT2:   (R2-R1).i = (inf rng(H0|divset(T.k,i)) - inf rng(Hn|divset(T.k,i)))
                    * vol divset(T.k,i);
       i in dom Tk by T1,FINSEQ_1:def 3; then
B1:    divset(T.k,i) c= AB by INTEGRA1:10; then
D22:   H0|divset(T.k,i) is_bounded_below_on divset(T.k,i) by P21,INTEGRA2:6;
       reconsider g = H0|divset(T.k,i) as Function of divset(T.k,i),REAL
         by B1,FUNCT_2:38,P20;
       Hn is Function of AB,REAL by P15; then
       reconsider f = Hn|divset(T.k,i) as Function of divset(T.k,i),REAL
         by FUNCT_2:38,B1;
       Hn is_bounded_on AB by P15; then
       Hn is_bounded_below_on AB by RFUNCT_1:def 11; then
       Hn|divset(T.k,i) is_bounded_below_on divset(T.k,i)
          by INTEGRA2:6,B1; then
T33:   rng f is bounded_below & rng g is bounded_below by D22,INTEGRA1:13;
T341:  rng(H1|divset(T.k,i)) c= {e} by P16A,RELAT_1:99;
       consider x0 be set such that
T342:   x0 in divset(T.k,i) by XBOOLE_0:def 1;
       H1.x0 in {e} by B1,T342,FUNCT_2:6,P16A; then
       H1.x0 = e by TARSKI:def 1; then
T343:  (H1|divset(T.k,i)).x0 = e by T342,FUNCT_1:72;
       H1|divset(T.k,i) is Function of divset(T.k,i),REAL
         by B1,FUNCT_2:38; then
       e in rng(H1|divset(T.k,i)) by FUNCT_2:6,T342,T343; then
       {e} c= rng(H1|divset(T.k,i)) by ZFMISC_1:37; then
       rng(H1|divset(T.k,i)) = {e} by T341,XBOOLE_0:def 10; then
T36:   e=inf rng(H1|divset(T.k,i)) by SEQ_4:22;
T4:    0 <= vol divset(T.k,i) by INTEGRA1:11;
T37:   now let x be set;
        assume 
T361:    x in divset(T.k,i); then
        abs(f.x-g.x) = abs(Hn.x-g.x) by FUNCT_1:72; then
        abs(f.x-g.x) = abs(Hn.x-H0.x) by T361,FUNCT_1:72; then
        abs(f.x-g.x) = abs((H.n).x-H0.x) by T361,B1,FUNCT_1:72; then
        abs(f.x-g.x) = abs((H.n).x-(lim(H,AB)).x) by T361,B1,FUNCT_1:72;
        hence abs(f.x - g.x) <= e by T361,B1,P162,P163;
       end; then
       inf rng(Hn|divset(T.k,i)) - inf rng(H0|divset(T.k,i))
                <= inf rng(H1|divset(T.k,i)) by T33,T36,ThInf; then
       (R1-R2).i <= inf rng(H1|divset(T.k,i)) * vol divset(T.k,i)
         by T2,T4,XREAL_1:66;
       hence (R1-R2).i <= R3.i by INTEGRA1:def 8,T1;
       now let x be set;
        assume x in divset(T.k,i); then
        abs(f.x-g.x) <= inf (rng (H1|divset(T.k,i))) by T36,T37;
        hence abs(g.x-f.x) <= inf rng(H1|divset(T.k,i)) by COMPLEX1:146;
       end; then
       inf rng(H0|divset(T.k,i)) - inf rng(Hn|divset(T.k,i))
             <= inf rng(H1|divset(T.k,i)) by T33,ThInf; then
       (R2-R1).i <= inf rng(H1|divset(T.k,i)) * vol divset(T.k,i) 
         by LT2,XREAL_1:66,T4;
       hence thesis by INTEGRA1:def 8,T1;
      end; then
      for i be Element of NAT st i in Seg l0 holds (R1-R2).i <= R3.i; then
P16J: lower_sum(Hn,T).k-lower_sum(H0,T).k <=lower_sum(H1,T.k) 
        by P16B,RVSUM_1:112;
P16E: sup rng H1 = e by SEQ_4:22,P16A;
P16F: vol AB = b-a by a1,INTEGRA6:5;
      H1 is_bounded_above_on AB by P16A,RFUNCT_1:def 11; then
P16X: upper_sum(H1,T.k) <= e*(b-a) by P16E,P16F,INTEGRA1:29;
      lower_sum(H1,T.k) <= upper_sum(H1,T.k) by P16A,INTEGRA1:30; then
P16Y: lower_sum(H1,T.k) <= e*(b-a) by P16X,XXREAL_0:2; then
P163: lower_sum((H.n)||AB,T).k-lower_sum(H0,T).k <= e*(b-a) by P16J,XXREAL_0:2;
      for i be Element of NAT st i in Seg l0 holds (R2-R1).i <= R3.i
        by P16C; then
      lower_sum(H0,T).k-lower_sum(Hn,T).k <= Sum R3 by LP16B,RVSUM_1:112; then
      lower_sum(H0,T).k-lower_sum(Hn,T).k <= e*(b-a) by P16Y,XXREAL_0:2; then
      -e*(b-a) <= -(lower_sum(H0,T).k - lower_sum((H.n)||AB,T).k)
       by XREAL_1:26;
      hence abs(lower_sum((H.n)||AB,T).k-lower_sum(H0,T).k) 
                 <=e*(b-a) by ABSVALUE:12,P163;
     end;
    end;
a6: 0 < b-a by a1,XREAL_1:52;
LP17:
    for e be Element of REAL st 0 < e
     ex N be Element of NAT st
      for n,k be Element of NAT st N<=n holds
       abs(lower_sum((H.n)||AB,T).k-lower_sum(H0,T).k) <= e
    proof
     let e be Element of REAL;
     assume 0 < e; then
AS2: 0 < e/(b-a) by a6,XREAL_1:141;
     e/(b-a) is Element of REAL by XREAL_0:def 1; then
     consider N be Element of NAT such that
AS3:  for n,k be Element of NAT st N <= n holds
       abs(lower_sum((H.n)||AB,T).k-lower_sum(H0,T).k)
        <=(e/(b-a)) * (b-a) by AS2,LP16;
     take N;
     hereby let n,k be Element of NAT;
      assume N<=n; then
      abs(lower_sum((H.n)||AB,T).k-lower_sum(H0,T).k)
        <=(e/(b-a)) * (b-a) by AS3;
      hence abs(lower_sum((H.n)||AB,T).k-lower_sum(H0,T).k)
             <= e by a6,XCMPLX_1:88;
     end;
    end;
P252:
    lower_sum(H0,T) is convergent & lim lower_sum(H0,T) = lower_integral H0 &
    upper_sum(H0,T) is convergent & lim upper_sum(H0,T) = upper_integral H0
