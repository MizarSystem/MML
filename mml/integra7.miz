begin :: Preliminaries

 reserve a,b,r for real number;
 reserve A for non empty set;
 reserve X,x for set;
 reserve f,g,F,G for PartFunc of REAL,REAL;
 reserve n for Element of NAT;

theorem ThSup:
for f,g being Function of A,REAL st
 rng f is bounded_above & rng g is bounded_above &
 (for x be set st x in A holds abs(f.x-g.x)<=a) holds
  sup rng f - sup rng g <= a & sup rng g - sup rng f <= a
proof
   let f,g be Function of A,REAL;
   assume
A1: rng f is bounded_above & rng g is bounded_above &
    (for x be set st x in A holds abs(f.x-g.x)<=a);
A2:dom f = A & dom g = A by FUNCT_2:def 1; then
A3:rng f is non empty Subset of REAL &
   rng g is non empty Subset of REAL by RELAT_1:65;
   for b st b in rng f holds b <= sup rng g + a
   proof
    let b; 
    assume b in rng f; then 
    consider x being Element of A such that
A4:  x in dom f & b=f.x by PARTFUN1:26;
    abs(f.x-g.x)<=a by A1; then 
    f.x-g.x <= a by ABSVALUE:12; then
A5: b <= a + g.x by A4,XREAL_1:22;
    g.x in rng g by A2,FUNCT_1:12; then
    g.x <= sup rng g by A1,SEQ_4:def 4; then
    a + g.x <= a + sup rng g by XREAL_1:8;
    hence b <= sup rng g + a by A5,XXREAL_0:2;
   end; then
A6:sup rng f <= sup rng g + a by A3,SEQ_4:62;
   for b st b in rng g holds b <= sup rng f + a
   proof
    let b;
    assume b in rng g; then 
    consider x being Element of A such that
A7:  x in dom g & b=g.x by PARTFUN1:26;
    abs(f.x-g.x)<=a by A1; then
    abs(g.x-f.x)<=a by COMPLEX1:146; then 
    g.x-f.x <= a by ABSVALUE:12; then
A8: b <= a + f.x by A7,XREAL_1:22;
    f.x in rng f by A2,FUNCT_1:12; then
    f.x <= sup rng f by A1,SEQ_4:def 4; then
    a + f.x <= a + sup rng f by XREAL_1:8;
    hence b <= sup rng f + a by A8,XXREAL_0:2;
   end; then
   sup rng g  <= sup rng f + a by A3,SEQ_4:62;
   hence thesis by A6,XREAL_1:22;
end;

theorem ThInf:
for f,g being Function of A,REAL st
 rng f is bounded_below & rng g is bounded_below &
 (for x be set st x in A holds abs(f.x-g.x)<=a) holds
  inf rng f - inf rng g <= a & inf rng g - inf rng f <= a
proof
   let f,g be Function of A,REAL;
   assume
A1: rng f is bounded_below & rng g is bounded_below &
    (for x be set st x in A holds abs(f.x-g.x)<=a);
A2:dom f = A & dom g = A by FUNCT_2:def 1; then
A3:rng f is non empty Subset of REAL &
   rng g is non empty Subset of REAL by RELAT_1:65;
   for b st b in rng f holds inf rng g -a <=b
   proof
    let b;
    assume b in rng f; then 
    consider x being Element of A such that
A4:  x in dom f & b=f.x by PARTFUN1:26;
    abs(f.x-g.x)<=a by A1; then 
    abs(g.x-f.x)<=a by COMPLEX1:146; then 
    g.x-f.x <= a by ABSVALUE:12; then
A5: g.x - a <=b  by A4,XREAL_1:14;
    g.x in rng g by A2,FUNCT_1:12; then
    inf rng g <=g.x by A1,SEQ_4:def 5; then
    inf rng g - a <= g.x-a by XREAL_1:11;
    hence inf rng g - a <= b by A5,XXREAL_0:2;
   end; then
A6:inf rng g-a <= inf rng f by A3,SEQ_4:60;
   for b st b in rng g holds  inf rng f -a <= b
   proof
    let b;
    assume b in rng g; then
    consider x being Element of A such that
A7:  x in dom g & b=g.x by PARTFUN1:26;
    abs(f.x-g.x)<=a by A1; then 
    f.x-g.x <= a by ABSVALUE:12; then
A8: f.x - a <= b by A7,XREAL_1:14;
    f.x in rng f by A2,FUNCT_1:12; then
    inf rng f <= f.x by A1,SEQ_4:def 5; then
    inf rng f - a <= f.x -a by XREAL_1:11;
    hence inf rng f - a <= b by A8,XXREAL_0:2;
   end; then
   inf rng f -a <= inf rng g by A3,SEQ_4:60;
   hence thesis by A6,XREAL_1:14;
end;

theorem Lem1:
 f|X is_bounded_on X implies f is_bounded_on X
proof              
   assume f|X is_bounded_on X; then
   consider r be real number such that 
T1: for z be set st z in X /\ dom(f|X) holds abs((f|X).z) <= r by RFUNCT_1:90;
   now let z be set;
    assume 
T2:  z in X /\ dom f;
T3: dom(f|X) = dom f /\ X by FUNCT_1:68; then
    X /\ dom(f|X) = (X /\ X) /\ dom f by XBOOLE_1:16; then
    abs((f|X).z) <= r by T1,T2;
    hence abs(f.z) <= r by T2,T3,FUNCT_1:68;
   end; 
   hence f is_bounded_on X by RFUNCT_1:90;
end;

theorem Lem:
for x be Real st
 x in X & f|X is_differentiable_in x holds f is_differentiable_in x
proof
   let x be Real;
   assume A1: x in X & f|X is_differentiable_in x; then
   consider N be Neighbourhood of x such that
A2: N c= dom(f|X) &
    ex L be LINEAR, R be REST st
     for x1 be Real st x1 in N holds (f|X).x1 - (f|X).x = L.(x1-x) + R.(x1-x)
       by FDIFF_1:def 5;
   N c= dom f /\ X by A2,FUNCT_1:68; then
A3:N c= dom f by XBOOLE_1:18;
   consider L be LINEAR, R be REST such that
A4: for x1 be Real st x1 in N holds (f|X).x1 - (f|X).x = L.(x1-x) + R.(x1-x)
     by A2;
   now let x1 be Real;
    assume A5: x1 in N; then
    (f|X).x1 = f.x1 & (f|X).x = f.x by A1,A2,FUNCT_1:70,72;
    hence f.x1 - f.x = L.(x1-x) + R.(x1-x) by A4,A5;
   end;
   hence thesis by A3,FDIFF_1:def 5;
end;

theorem
f|X is_differentiable_on X implies f is_differentiable_on X
proof
   assume A1: f|X is_differentiable_on X; then
   X c= dom(f|X) & for x be Real st x in X holds (f|X)|X is_differentiable_in x
     by FDIFF_1:def 7; then
   X c= dom f /\ X by FUNCT_1:68; then
A2:X c= dom f by XBOOLE_1:18;
   now let x be Real;
    assume A3:x in X; then
    (f|X)|X is_differentiable_in x by A1,FDIFF_1:def 7;
    hence f|X is_differentiable_in x by A3,Lem;
   end;
   hence thesis by A2,FDIFF_1:def 7;
end;

theorem Lem03:
f is_differentiable_on X & g is_differentiable_on X implies
  f+g is_differentiable_on X & f-g is_differentiable_on X &
  f(#)g is_differentiable_on X
proof
   assume A2:f is_differentiable_on X & g is_differentiable_on X;
   reconsider Z = X as Subset of REAL by A2,FDIFF_1:15;
   reconsider Z as open Subset of REAL by A2,FDIFF_1:17;
   X c= dom f & (for x be Real st x in X holds f|X is_differentiable_in x) &
   X c= dom g & (for x be Real st x in X holds g|X is_differentiable_in x)
     by A2,FDIFF_1:def 7; then
   Z c= dom f /\ dom g by XBOOLE_1:19; then
   Z c= dom(f+g) & Z c= dom(f-g) & Z c= dom(f(#)g) by SEQ_1:def 3,def 4,def 5;
   hence thesis by A2,FDIFF_1:26,27,29;
end;

theorem Lem04:
f is_differentiable_on X implies r(#)f is_differentiable_on X 
proof
   assume A2:f is_differentiable_on X;
   reconsider Z = X as Subset of REAL by A2,FDIFF_1:15;
   reconsider Z as open Subset of REAL by A2,FDIFF_1:17;
   reconsider r1 = r as Real by XREAL_0:def 1;
   X c= dom f & (for x be Real st x in X holds f|X is_differentiable_in x)
     by A2,FDIFF_1:def 7; then
   Z c= dom(r(#)f) by SEQ_1:def 6; then
   (r1(#)f) is_differentiable_on X by A2,FDIFF_1:28;
   hence thesis;
end;

theorem Lem06:
(for x be set st x in X holds g.x <> 0) &
f is_differentiable_on X & g is_differentiable_on X 
 implies f/g is_differentiable_on X
proof
   assume that
A1: for x be set st x in X holds g.x <> 0 and
A2: f is_differentiable_on X & g is_differentiable_on X;
   reconsider Z = X as Subset of REAL by A2,FDIFF_1:15;
   reconsider Z as open Subset of REAL by A2,FDIFF_1:17;
   for x be Real st x in Z holds g.x <> 0 by A1;
   hence thesis by A2,FDIFF_2:21;
end;

theorem
(for x be set st x in X holds f.x <> 0) & f is_differentiable_on X 
 implies f^ is_differentiable_on X
proof
   assume that
A1: for x be set st x in X holds f.x <> 0 and
A2: f is_differentiable_on X;
   reconsider Z = X as Subset of REAL by A2,FDIFF_1:15;
   reconsider Z as open Subset of REAL by A2,FDIFF_1:17;
   for x be Real st x in Z holds f.x <> 0 by A1;
   hence thesis by A2,FDIFF_2:22;
end;

theorem Th1:
  a <= b & [' a,b '] c= X & F is_differentiable_on X &
    F`|X is_integrable_on [' a,b '] & F`|X is_bounded_on [' a,b ']
implies
  F.b = integral(F`|X,a,b) + F.a
proof
   assume that
A1: a <= b and
A2: [' a,b '] c= X & F is_differentiable_on X &
    F`|X is_integrable_on [' a,b '] & F`|X is_bounded_on [' a,b '];
B1:[' a,b '] = [.a,b.] by A1,INTEGRA5:def 4; then
   [.a,b.] = [.inf [.a,b.], sup [.a,b.].] by INTEGRA1:5; then
B5:a=inf [.a,b.] & b=sup [.a,b.] by B1,INTEGRA1:6;
   integral(F`|X,a,b) = integral(F`|X,[' a,b ']) by A1,INTEGRA5:def 5; then
   integral(F`|X,a,b) = F.(sup [' a,b ']) - F.(inf [' a,b ']) 
    by A2,INTEGRA5:13;
   hence thesis by B5,B1;
end;

begin :: The Definition of Indefinite Integral

definition let X be set, f be PartFunc of REAL,REAL;
  func IntegralFuncs(f,X) -> set means :Def00:
   x in it iff 
   ex F be PartFunc of REAL,REAL st 
    x = F & F is_differentiable_on X & F`|X = f|X;
existence
proof
   defpred P[set] means
    ex F be PartFunc of REAL,REAL st 
     $1 = F & F is_differentiable_on X & F`|X = f|X;
   consider Z being set such that
A1: x in Z iff x in PFuncs(REAL,REAL) & P[x] from XBOOLE_0:sch 1;
   take Z;
   let x;
   thus x in Z implies
         ex F being PartFunc of REAL,REAL st
          x = F & F is_differentiable_on X & F`|X = f|X by A1;
   given F being PartFunc of REAL,REAL such that
A2: x = F and
A3: F is_differentiable_on X and
A4: F`|X = f|X;
   F in PFuncs(REAL,REAL) by PARTFUN1:119;
   hence x in Z by A1,A2,A3,A4;
end;
uniqueness
proof
   let Z1,Z2 be set such that
A1: x in Z1 iff ex F be PartFunc of REAL,REAL st
                 x = F & F is_differentiable_on X & F`|X = f|X and
A2: x in Z2 iff ex F be PartFunc of REAL,REAL st
                 x = F & F is_differentiable_on X & F`|X = f|X;
   x in Z1 iff x in Z2
   proof
    x in Z1 iff ex F be PartFunc of REAL,REAL st
       x = F & F is_differentiable_on X & F`|X = f|X by A1;
    hence thesis by A2;
   end;
   hence thesis by TARSKI:2;
end;
end;

definition let X be set, F,f be PartFunc of REAL,REAL;
  pred F is_integral_of f,X means :Def0:
    F in IntegralFuncs(f,X);
end;

Lemma:
F is_integral_of f,X iff F is_differentiable_on X & F`|X = f|X
proof
   hereby assume F is_integral_of f,X; then
    F in IntegralFuncs(f,X) by Def0; then
    ex F' be PartFunc of REAL,REAL st
     F = F' & F' is_differentiable_on X & F'`|X = f|X by Def00;
    hence F is_differentiable_on X & F`|X = f|X;
   end;
   assume F is_differentiable_on X & F`|X = f|X; then
   F in IntegralFuncs(f,X) by Def00;
   hence thesis by Def0;
end;

theorem Th2:
  F is_integral_of f,X implies X c= dom F
proof              
   assume F is_integral_of f,X; then
   F in IntegralFuncs(f,X) by Def0; then
   ex G be PartFunc of REAL,REAL st
    F = G & G is_differentiable_on X & G`|X = f|X by Def00;
   hence X c= dom F by FDIFF_1:def 7;
end;

theorem
  F is_integral_of f,X & G is_integral_of g,X
    implies F+G is_integral_of f+g,X & F-G is_integral_of f-g,X
proof
   assume F is_integral_of f,X & G is_integral_of g,X; then
A2:F is_differentiable_on X & F`|X = f|X &
   G is_differentiable_on X & G`|X = g|X by Lemma; then
A4:F+G is_differentiable_on X & F-G is_differentiable_on X by Lem03;
   dom(f|X) = X & dom(g|X) = X by A2,FDIFF_1:def 8; then
   dom f /\ X = X & dom g /\ X = X by FUNCT_1:68; then
   X c= dom f & X c= dom g by XBOOLE_1:18; then
   X c= dom f /\ dom g by XBOOLE_1:19; then
P0:X c= dom(f+g) & X c= dom(f-g) by SEQ_1:def 3,def 4; then
   X = dom((f+g)|X) & X = dom((f-g)|X) by RELAT_1:91; then
P1:dom((F+G)`|X) = dom((f+g)|X) & dom((F-G)`|X) = dom((f-g)|X)
     by A4,FDIFF_1:def 8;
   now let x be Element of REAL;
    assume x in dom((F+G)`|X); then
D0: x in X by A4,FDIFF_1:def 8; then
D1: ((F+G)`|X).x = diff(F+G,x) by A4,FDIFF_1:def 8;
    F|X is_differentiable_in x & G|X is_differentiable_in x
      by D0,A2,FDIFF_1:def 7; then
    F is_differentiable_in x & G is_differentiable_in x by D0,Lem; then
D3: ((F+G)`|X).x = diff(F,x) + diff(G,x) by D1,FDIFF_1:21;
    (F`|X).x = diff(F,x) & (G`|X).x = diff(G,x) by A2,D0,FDIFF_1:def 8; then
    f.x = diff(F,x) & g.x = diff(G,x) by A2,D0,FUNCT_1:72; then
    ((F+G)`|X).x = (f+g).x by D3,D0,P0,SEQ_1:def 3;
    hence ((F+G)`|X).x = ((f+g)|X).x by D0,FUNCT_1:72;
   end; then
   (F+G)`|X = (f+g)|X by P1,PARTFUN1:34;
   hence F+G is_integral_of f+g,X by A4,Lemma;
   now let x be Element of REAL;
    assume x in dom((F-G)`|X); then
D0: x in X by A4,FDIFF_1:def 8; then
D1: ((F-G)`|X).x = diff(F-G,x) by A4,FDIFF_1:def 8;
    F|X is_differentiable_in x & G|X is_differentiable_in x
      by D0,A2,FDIFF_1:def 7; then
    F is_differentiable_in x & G is_differentiable_in x by D0,Lem; then
D3: ((F-G)`|X).x = diff(F,x) - diff(G,x) by D1,FDIFF_1:22;
    (F`|X).x = diff(F,x) & (G`|X).x = diff(G,x) by A2,D0,FDIFF_1:def 8; then
    f.x = diff(F,x) & g.x = diff(G,x) by A2,D0,FUNCT_1:72; then
    ((F-G)`|X).x = (f-g).x by D3,D0,P0,SEQ_1:def 4;
    hence ((F-G)`|X).x = ((f-g)|X).x by D0,FUNCT_1:72;
   end; then
   (F-G)`|X = (f-g)|X by P1,PARTFUN1:34;
   hence F-G is_integral_of f-g,X by A4,Lemma;
end;

theorem
  F is_integral_of f,X implies r(#)F is_integral_of r(#)f,X
proof
   assume F is_integral_of f,X; then
A2:F is_differentiable_on X & F`|X = f|X by Lemma; then
A4:r(#)F is_differentiable_on X by Lem04;
   dom(f|X) = X by A2,FDIFF_1:def 8; then
   dom f /\ X = X by FUNCT_1:68; then
   X c= dom f by XBOOLE_1:18; then
P0:X c= dom(r(#)f) by SEQ_1:def 6; then
   X = dom((r(#)f)|X) by RELAT_1:91; then
P1:dom((r(#)F)`|X) = dom((r(#)f)|X) by A4,FDIFF_1:def 8;
   now let x be Element of REAL;
    assume x in dom((r(#)F)`|X); then
D0: x in X by A4,FDIFF_1:def 8; then
D1: ((r(#)F)`|X).x = diff(r(#)F,x) by A4,FDIFF_1:def 8;
    reconsider r1 = r as Real by XREAL_0:def 1;
    F|X is_differentiable_in x by D0,A2,FDIFF_1:def 7; then
    F is_differentiable_in x by D0,Lem; then
D3: ((r1(#)F)`|X).x = r1*diff(F,x) by D1,FDIFF_1:23;
    (F`|X).x = diff(F,x) by A2,D0,FDIFF_1:def 8; then
    f.x = diff(F,x) by A2,D0,FUNCT_1:72; then
    ((r(#)F)`|X).x = (r(#)f).x by D3,D0,P0,SEQ_1:def 6;
    hence ((r(#)F)`|X).x = ((r(#)f)|X).x by D0,FUNCT_1:72;
   end; then
   (r(#)F)`|X = (r(#)f)|X by P1,PARTFUN1:34;
   hence r(#)F is_integral_of r(#)f,X by A4,Lemma;
end;

theorem
  F is_integral_of f,X & G is_integral_of g,X
    implies F(#)G is_integral_of f(#)G+F(#)g,X
proof
   assume A1:F is_integral_of f,X & G is_integral_of g,X; then
A2:F is_differentiable_on X & F`|X = f|X &
   G is_differentiable_on X & G`|X = g|X by Lemma; then
A4:F(#)G is_differentiable_on X by Lem03;
   dom(f|X) = X & dom(g|X) = X by A2,FDIFF_1:def 8; then
   dom f /\ X = X & dom g /\ X = X by FUNCT_1:68; then
   X c= dom f & X c= dom F & X c= dom g & X c= dom G by A1,Th2,XBOOLE_1:18;then
   X c= dom f /\ dom G & X c= dom F /\ dom g by XBOOLE_1:19; then
P2:X c= dom(f(#)G) & X c= dom(F(#)g) by SEQ_1:def 5; then
   X c= dom(f(#)G) /\ dom(F(#)g) by XBOOLE_1:19; then
P0:X c= dom(f(#)G+F(#)g) by SEQ_1:def 3; then
   X = dom((f(#)G+F(#)g)|X) by RELAT_1:91; then
P1:dom((F(#)G)`|X) = dom((f(#)G+F(#)g)|X) by A4,FDIFF_1:def 8;
   now let x be Element of REAL;
    assume x in dom((F(#)G)`|X); then
D0: x in X by A4,FDIFF_1:def 8; then
D1: ((F(#)G)`|X).x = diff(F(#)G,x) by A4,FDIFF_1:def 8;
    F|X is_differentiable_in x & G|X is_differentiable_in x
      by D0,A2,FDIFF_1:def 7; then
    F is_differentiable_in x & G is_differentiable_in x by D0,Lem; then
D3: ((F(#)G)`|X).x = (G.x)*diff(F,x) + (F.x)*diff(G,x) by D1,FDIFF_1:24;
    (F`|X).x = diff(F,x) & (G`|X).x = diff(G,x) by A2,D0,FDIFF_1:def 8; then
    f.x = diff(F,x) & g.x = diff(G,x) by A2,D0,FUNCT_1:72; then
    (F(#)g).x = (F.x)*diff(G,x) & (f(#)G).x = (G.x)*diff(F,x)
      by D0,P2,SEQ_1:def 5; then
    ((F(#)G)`|X).x = (F(#)g+f(#)G).x by D3,D0,P0,SEQ_1:def 3;
    hence ((F(#)G)`|X).x = ((F(#)g+f(#)G)|X).x by D0,FUNCT_1:72;
   end; then
   (F(#)G)`|X = (F(#)g+f(#)G)|X by P1,PARTFUN1:34;
   hence F(#)G is_integral_of f(#)G+F(#)g,X by A4,Lemma;
end;

theorem
  (for x be set st x in X holds G.x <> 0) &
    F is_integral_of f,X & G is_integral_of g,X implies
    F/G is_integral_of (f(#)G-F(#)g)/(G(#)G),X
proof
   assume that
A0: for x be set st x in X holds G.x <> 0 and
A1: F is_integral_of f,X & G is_integral_of g,X;
A2:F is_differentiable_on X & F`|X = f|X &
   G is_differentiable_on X & G`|X = g|X by A1,Lemma; then
A4:F/G is_differentiable_on X by A0,Lem06;
   dom(f|X) = X & dom(g|X) = X by A2,FDIFF_1:def 8; then
   dom f /\ X = X & dom g /\ X = X by FUNCT_1:68; then
A5:X c= dom f & X c= dom F & X c= dom g & X c= dom G
     by A1,Th2,XBOOLE_1:18; then
   X c= dom f /\ dom G & X c= dom F /\ dom g by XBOOLE_1:19; then
P2:X c= dom(f(#)G) & X c= dom(F(#)g) by SEQ_1:def 5; then
   X c= dom(f(#)G) /\ dom(F(#)g) by XBOOLE_1:19; then
P0:X c= dom(f(#)G-F(#)g) by SEQ_1:def 4; then
PQ:X = dom((f(#)G-F(#)g)|X) by RELAT_1:91;
   dom((G|X)(#)(G|X)) = dom(G|X) /\ dom(G|X) by SEQ_1:def 5; then
   dom((G|X)(#)(G|X)) = dom G /\ X by FUNCT_1:68; then
A7:dom((G|X)(#)(G|X)) = X by A5,XBOOLE_1:28;
   now assume (G|X)"{0} <> {}; then
    consider y be set such that
K1:  y in (G|X)"{0} by XBOOLE_0:def 1;
K2: y in dom(G|X) & (G|X).y in {0} by K1,FUNCT_1:def 13; then
    y in dom G /\ X by FUNCT_1:68; then
    y in X by XBOOLE_0:def 3; then
    G.y <> 0 & (G|X).y = G.y by A0,K2,FUNCT_1:70;
    hence contradiction by K2,TARSKI:def 1;
   end; then
   (dom(G|X) \ (G|X)"{0}) /\ (dom(G|X) \ (G|X)"{0}) = dom(G|X); then
   dom((G|X)(#)(G|X)) \ ((G|X)(#)(G|X))"{0} = dom(G|X) by RFUNCT_1:12; then
   dom((G|X)(#)(G|X)) \ ((G|X)(#)(G|X))"{0} = dom G /\ X by FUNCT_1:68; then
   dom((G|X)(#)(G|X)) \ ((G|X)(#)(G|X))"{0} = X by A5,XBOOLE_1:28; then
   dom((f(#)G-F(#)g)|X) /\ (dom((G|X)(#)(G|X)) \ ((G|X)(#)(G|X))"{0}) = X
     by PQ; then
   dom(((f(#)G-F(#)g)|X)/((G|X)(#)(G|X))) = X by RFUNCT_1:def 4; then
   dom(((f(#)G-F(#)g)|X)/((G(#)G)|X)) = X by RFUNCT_1:61; then
   dom(((f(#)G-F(#)g)/(G(#)G))|X) = X by RFUNCT_1:64; then
P1:dom((F/G)`|X) = dom(((f(#)G-F(#)g)/(G(#)G))|X) by A4,FDIFF_1:def 8;
   now let x be Element of REAL;
    assume x in dom((F/G)`|X); then
D0: x in X by A4,FDIFF_1:def 8; then
D1: ((F/G)`|X).x = diff(F/G,x) by A4,FDIFF_1:def 8;
L:  now let y be set;
     assume L1:y in dom(G|X); then
L2:  (G|X).y = G.y by FUNCT_1:70;
     y in dom G /\ X by L1,FUNCT_1:68; then
     y in X by XBOOLE_0:def 3; then
     G.y <> 0 by A0; then
     not (G|X).y in {0} by L2,TARSKI:def 1;
     hence not y in (G|X)"{0} by FUNCT_1:def 13;
    end;
D2: G.x <> 0 by A0,D0;
    X = dom G /\ X by A5,XBOOLE_1:28; then
    X = dom(G|X) by FUNCT_1:68; then
    x in dom(G|X) & not x in (G|X)"{0} by D0,L; then
    x in (dom(G|X)\(G|X)"{0}) /\ (dom(G|X)\(G|X)"{0}) by XBOOLE_0:def 4; then
LL: x in dom((G|X)(#)(G|X)) \ ((G|X)(#)(G|X))"{0} by RFUNCT_1:12;
    F|X is_differentiable_in x & G|X is_differentiable_in x
      by D0,A2,FDIFF_1:def 7; then
    F is_differentiable_in x & G is_differentiable_in x by D0,Lem; then
D3: ((F/G)`|X).x
     = (diff(F,x)*G.x - diff(G,x)*F.x)/((G.x)^2) by D1,D2,FDIFF_2:14;
    x in dom((f(#)G-F(#)g)) /\ (dom((G|X)(#)(G|X)) \ ((G|X)(#)(G|X))"{0})
      by LL,P0,D0,XBOOLE_0:def 3; then
Q1: x in dom((f(#)G-F(#)g)/((G|X)(#)(G|X))) by RFUNCT_1:def 4;
    (F`|X).x = diff(F,x) & (G`|X).x = diff(G,x) by A2,D0,FDIFF_1:def 8; then
    f.x = diff(F,x) & g.x = diff(G,x) by A2,D0,FUNCT_1:72; then
    (F(#)g).x = (F.x)*diff(G,x) & (f(#)G).x = (G.x)*diff(F,x)
      by D0,P2,SEQ_1:def 5; then
D4: (f(#)G-F(#)g).x = diff(F,x)*G.x - diff(G,x)*F.x by D0,P0,SEQ_1:def 4;
    G.x = (G|X).x by D0,FUNCT_1:72; then
    (G.x)^2 = (G|X).x * (G|X).x by SQUARE_1:def 3; then
    (G.x)^2 = ((G|X)(#)(G|X)).x by D0,A7,SEQ_1:def 5; then
    ((F/G)`|X).x = ((f(#)G-F(#)g)/((G|X)(#)(G|X))).x 
      by D3,D4,Q1,RFUNCT_1:def 4; then
    ((F/G)`|X).x = ((f(#)G-F(#)g)/(G(#)G)|X).x by RFUNCT_1:61;
    hence ((F/G)`|X).x = (((f(#)G-F(#)g)/(G(#)G))|X).x by RFUNCT_1:64;
   end; then
   (F/G)`|X = ((f(#)G-F(#)g)/(G(#)G))|X by P1,PARTFUN1:34;
   hence F/G is_integral_of (f(#)G-F(#)g)/(G(#)G),X by A4,Lemma;
end;

theorem
  a <= b & [' a,b '] c= dom f & f is_continuous_on [' a,b '] &
  ]. a,b .[ c= dom F &
  (for x be real number st x in ].a,b.[ holds F.x = integral(f,a,x) + F.a)
  implies
    F is_integral_of f,]. a,b .[
proof
    set C = [' a,b '];
    set O = ].a,b.[;
    assume that
A1:  a <= b and
A2:  [' a,b '] c= dom f and
A3:  f is_continuous_on [' a,b '] and
A4:  ]. a,b .[ c= dom F and
A5:  for x be real number st x in ].a,b.[ holds F.x = integral(f,a,x) +F.a;
    [' a,b '] = [.a,b.] by A1,INTEGRA5:def 4; then
A6: O c= [' a,b '] by RCOMP_1:15; then
A7: O c= dom f by A2,XBOOLE_1:1;
    deffunc G0(Element of REAL) = integral(f,a,$1);
    consider G1 be Function of REAL,REAL such that
A8:  for h be Real holds G1.h=G0(h) from FUNCT_2:sch 4;
    reconsider G=G1|O as PartFunc of REAL,REAL;
    dom G = dom G1 /\ O by FUNCT_1:68; then
A9: dom G = REAL /\ O by FUNCT_2:def 1; then
A10:O = dom G by XBOOLE_1:28;
A11:now let x be real number;
     reconsider z = x as Real by XREAL_0:def 1;
     assume x in ].a,b.[; then
     G.x = G1.x by A10,FUNCT_1:70; then
     G.x = integral(f,a,z) by A8;
     hence G.x = integral(f,a,x);
    end;
A12:for x be Real st x in O holds G is_differentiable_in x & diff(G,x)=f.x
    proof
     let x be Real;
     assume A13:x in ]. a,b .[;
     reconsider z = x as real number;
     f is_continuous_on O by A3,A6,FCONT_1:17; then
A14: f|O is_continuous_in z by FCONT_1:def 2,A13;
A15: f is_bounded_on [' a,b '] & f is_integrable_on [' a,b ']
       by A3,INTEGRA5:10,11;
     for r be real number st 0 < r
      ex s be real number st 
       0 < s & for x1 be real number st x1 in dom f & abs(x1-z) < s
        holds abs(f.x1 - f.z) < r
     proof
      let r be real number;
      assume 0 < r; then
      consider s be real number such that
A16:   0 < s & 
       for x1 be real number st x1 in dom(f|O) & abs(x1-z)<s 
        holds abs((f|O).x1 - (f|O).z) < r by A14,FCONT_1:3;
      consider ss1 be real number such that
A17:   0 < ss1 & ].z-ss1,z+ss1.[ c= O by RCOMP_1:40,A13;
      set s1=min(ss1,s);
      take s1;
      now let x1 be real number;
       assume R11:x1 in dom f & abs(x1-z)<s1;
       s1 <= s by XXREAL_0:17; then
A19:   abs(x1-z) < s by R11,XXREAL_0:2;
       s1 <= ss1 by XXREAL_0:17; then
       abs(x1-z) < ss1 by R11,XXREAL_0:2; then
A18:   x1 in ].z-ss1,z+ss1.[ by RCOMP_1:8;
       O = O /\ dom f by A7,XBOOLE_1:28; then
A20:   O = dom(f|O) by PARTFUN2:32; then
       abs(f.x1-f.z) = abs((f|O).x1 - f.z) by A17,A18,FUNCT_1:70; then
       abs(f.x1-f.z) = abs((f|O).x1 - (f|O).z) by A20,A13,FUNCT_1:70;
       hence abs(f.x1-f.z)<r by A17,A18,A19,A20,A16;
      end;
      hence thesis by A17,A16,XXREAL_0:15;
     end; then
     f is_continuous_in z by A13,A7,FCONT_1:3;
     hence thesis by A1,A2,A13,A10,A15,A11,INTEGRA6:28;
    end; then
    for x be Real st x in O holds G is_differentiable_in x; then
R7: G is_differentiable_on O by A10,FDIFF_1:16; 
    deffunc H0(Element of REAL) = F.a;
    consider H1 be Function of REAL,REAL such that
HPP1:for h be Real holds H1.h = H0(h) from FUNCT_2:sch 4;
    reconsider H=H1|O as PartFunc of REAL,REAL;
    dom H = dom H1 /\ O by FUNCT_1:68; then
E8: dom H = REAL /\ O by FUNCT_2:def 1; then
R8: dom H = O by XBOOLE_1:28;
    now let x be Element of REAL;
     assume O2:x in O /\ dom H;
     reconsider z=x as real number;
     H.x = H1.z by R8,O2,FUNCT_1:70; then
     H.x = F.a by HPP1;
     hence H/.x = F.a by O2,R8,FINSEQ_4:def 4;
    end; then
K1: H is_constant_on O by PARTFUN2:def 3; then
R10:H is_differentiable_on O & 
    for x be Real st x in O holds (H`|O).x = 0 by FDIFF_1:30,R8;
    dom(F|O) = dom F /\ O by PARTFUN2:32; then
R11a:dom(F|O) = O by A4,XBOOLE_1:28; then
X1: dom(F|O)= dom G /\ dom H by E8,A9,XBOOLE_1:28;
    now let x be set;
     assume
 X3:  x in dom(F|O); then
     reconsider z=x as real number by XREAL_0:def 1;
     (F|O).x = F.z  by X3,FUNCT_1:70; then
X4:  (F|O).x = integral(f,a,z) + F.a by A5,R11a,X3;
     reconsider z1=z as Real by XREAL_0:def 1;
     H.x = H1.z1 by X3,R8,R11a,FUNCT_1:70; then
     H.x = F.a by HPP1;
     hence (F|O).x = G.x + H.x by X4,R11a,X3,A11;
    end; then
R11:F|O = G+H by X1,SEQ_1:def 3; then
    F|O is_differentiable_on O &
    for x be Real st x in O holds ((F|O)`|O).x = diff(G,x) + diff(H,x) 
      by FDIFF_1:26,R11a,R10,R7; then
    for x be Real st x in O holds F|O is_differentiable_in x by FDIFF_1:16;then
P2: F is_differentiable_on O by A4,FDIFF_1:def 7; then
    dom(F`|O) = O by FDIFF_1:def 8; then
    dom(F`|O) = O /\ dom f by A7,XBOOLE_1:28; then
L1: dom(F`|O) = dom(f|O) by PARTFUN2:32;
    now let x be Real;
     assume x in dom (F`|O); then
L4:  x in O by P2,FDIFF_1:def 8; then
     x in dom f /\ O by A7,XBOOLE_0:def 3; then
L6:  x in dom (f|O) by PARTFUN2:32;
     (F`|O).x = ((F|O)`|O).x by P2,FDIFF_2:16; then
     (F`|O).x = diff(G,x) + diff(H,x) by L4,R11,FDIFF_1:26,R11a,R10,R7; then
L7:  (F`|O).x = f.x + diff(H,x) by A12,L4;
     (H`|O).x = 0 by L4,K1,FDIFF_1:30,R8; then
     diff(H,x) = 0 by L4,R10,FDIFF_1:def 8;
     hence (F`|O).x = (f|O).x by L6,L7,FUNCT_1:70;
    end; then
    F`|O = f|O by L1,PARTFUN1:34;
    hence F is_integral_of f,O by P2,Lemma;
end;

theorem
  for x,x0 be real number st
  f is_continuous_on [.a,b.] & x in ].a,b.[ & x0 in ].a,b.[ &
  F is_integral_of f,].a,b.[
    holds F.x = integral(f,x0,x) + F.x0
proof              
   let x,x0 be real number;
   set O = ].a,b.[;
   assume that
A3: f is_continuous_on [. a,b .] and
A4: x in ].a,b.[ & x0 in ].a,b.[ and
A5: F is_integral_of f,].a,b.[;
A6:F is_differentiable_on O & F`|O = f|O by A5,Lemma;
A7:O c= [.a,b.] by RCOMP_1:15;
B1:[.x0,x.] c=].a,b.[ & [.x,x0.] c=].a,b.[ by A4,RCOMP_1:17;
E1:now assume P1:x0 <= x; then
P2: [' x0,x '] c= ].a,b.[ by B1,INTEGRA5:def 4;
    (F`|].a,b.[)||[' x0,x '] = (f|].a,b.[)|[' x0,x '] by A5,Lemma; then
 R2:(F`|].a,b.[)||[' x0,x '] = f||[' x0,x '] by P2,FUNCT_1:82;
    [' x0,x '] c= [.a,b.] by A7,P2,XBOOLE_1:1; then
 R1:f is_continuous_on [' x0,x '] by A3,FCONT_1:17; then
    f is_integrable_on [' x0,x '] by INTEGRA5:17; then
    f||[' x0,x '] is_integrable_on [' x0,x '] by INTEGRA5:def 2; then
 P3:F`|].a,b.[ is_integrable_on [' x0,x '] by R2,INTEGRA5:def 2;
    f is_bounded_on [' x0,x '] by R1,INTEGRA5:10; then
    f||[' x0,x '] is_bounded_on [' x0,x '] by INTEGRA5:9; then
    F`|].a,b.[ is_bounded_on [' x0,x '] by R2,Lem1; then
    F.x = integral(f|].a,b.[,x0,x) +F.x0 by Th1,A6,P1,P2,P3; then
    F.x = integral(f|].a,b.[,[' x0,x ']) + F.x0 by INTEGRA5:def 5,P1; then
    F.x = integral(f,[' x0,x ']) + F.x0 by P2,FUNCT_1:82;
    hence F.x = integral(f,x0,x) +F.x0 by INTEGRA5:def 5,P1;
   end;
   now assume AP1: x < x0; then
 P2:[' x,x0 '] c= ].a,b.[ by B1,INTEGRA5:def 4;
    integral(f,x0,x) = -integral(f,[' x,x0 ']) by INTEGRA5:def 5,AP1; then
 PP:integral(f,x0,x) = -integral(f,x,x0) by INTEGRA5:def 5,AP1;
    (F`|]. a,b .[)|| [' x,x0 '] =(f|].a,b.[)|[' x,x0 '] by A5,Lemma; then
 R2:(F`|]. a,b .[)|| [' x,x0 '] = f|| [' x,x0 '] by P2,FUNCT_1:82;
    [' x,x0 '] c= [.a,b.] by A7,P2,XBOOLE_1:1; then
 R1:f is_continuous_on [' x,x0 '] by A3,FCONT_1:17; then
    f is_integrable_on [' x,x0 '] by INTEGRA5:17; then
    f||[' x,x0 '] is_integrable_on [' x,x0 '] by INTEGRA5:def 2; then
 P3:F`|].a,b.[ is_integrable_on [' x,x0 '] by R2,INTEGRA5:def 2;
    f is_bounded_on [' x,x0 '] by R1,INTEGRA5:10; then
    (F`|].a,b.[)|[' x,x0 '] is_bounded_on [' x,x0 '] by R2,INTEGRA5:9; then
    F`|].a,b.[ is_bounded_on [' x,x0 '] by Lem1; then
    F.x0 = integral(f|]. a,b .[,x,x0) +F.x by Th1,A6,AP1,P2,P3; then
    F.x0 = integral(f|]. a,b .[,[' x,x0 ']) +F.x by INTEGRA5:def 5,AP1; then
    F.x0 = integral(f,[' x,x0 ']) +F.x by P2,FUNCT_1:82; then
    F.x0 = integral(f,x,x0) +F.x by INTEGRA5:def 5,AP1;
    hence F.x = integral(f,x0,x) + F.x0 by PP;
   end;
   hence thesis by E1;
end;

theorem Th60:
  a <= b & [' a,b '] c= X & F is_integral_of f,X &
    f is_integrable_on [' a,b '] & f is_bounded_on [' a,b ']
   implies F.b = integral(f,a,b) + F.a
proof
   assume that
A3: a <= b and
A4: [' a,b '] c= X;
   assume
A1: F is_integral_of f,X; then
A2:F is_differentiable_on X & F`|X=f|X by Lemma;
   (F`|X)||[' a,b '] =(f|X )|[' a,b '] by A1,Lemma; then
R2:(F`|X)||[' a,b '] = f||[' a,b '] by A4,FUNCT_1:82;
   assume f is_integrable_on [' a,b ']; then
   f||[' a,b '] is_integrable_on [' a,b '] by INTEGRA5:def 2; then
P3:F`|X is_integrable_on [' a,b '] by R2,INTEGRA5:def 2;
   assume f is_bounded_on [' a,b ']; then
   (F`|X)|[' a,b '] is_bounded_on [' a,b '] by R2,INTEGRA5:9; then
   F`|X is_bounded_on [' a,b '] by Lem1; then
   F.b = integral(f|X,a,b) +F.a by Th1,A2,A3,A4,P3; then
   F.b = integral(f|X,[' a,b ']) +F.a by A3,INTEGRA5:def 5; then
   F.b =integral(f,[' a,b ']) +F.a by A4,FUNCT_1:82;
   hence F.b = integral(f,a,b) + F.a by A3,INTEGRA5:def 5;
end;

theorem Th6a:
  a <= b & [. a,b .] c= X & f is_continuous_on X implies
    f is_continuous_on [' a,b '] & f is_integrable_on [' a,b '] &
    f is_bounded_on [' a,b ']
proof              
   assume a <= b; then
A3:[. a,b .] = [' a,b '] by INTEGRA5:def 4;
   assume [. a,b .] c= X & f is_continuous_on X;
   hence f is_continuous_on [' a,b '] by A3,FCONT_1:17;
   hence thesis by INTEGRA5:17,10;
end;

theorem Th6:
  a <= b & [. a,b .] c= X & f is_continuous_on X & F is_integral_of f,X
    implies F.b = integral(f,a,b) + F.a
proof
   assume that
A3: a <= b and
A4: [. a,b .] c= X;
R0:[. a,b .] =[' a,b '] by A3,INTEGRA5:def 4;
   assume f is_continuous_on X; then
P1:f is_continuous_on [' a,b '] & f is_integrable_on [' a,b '] &
   f is_bounded_on [' a,b '] by A3,A4,Th6a;
   assume F is_integral_of f,X;
   hence F.b = integral(f,a,b) + F.a by P1,R0,Th60,A3,A4; 
end;

theorem Th100:
  b <= a & [' b,a '] c= X &
  f is_integrable_on [' b,a '] & g is_integrable_on [' b,a '] &
  f is_bounded_on [' b,a '] & g is_bounded_on [' b,a '] &
  X c= dom f & X c= dom g & F is_integral_of f,X & G is_integral_of g,X
  implies
    (F.a)*(G.a) - (F.b)*(G.b) = integral(f(#)G,b,a) + integral(F(#)g,b,a)
proof
    assume that
B1:  b <= a and
A3:  [' b,a '] c= X and
A4:  f is_integrable_on [' b,a '] & g is_integrable_on [' b,a '] and
A5:  f is_bounded_on [' b,a '] & g is_bounded_on [' b,a '];
    assume that
A1:  X c= dom f & X c= dom g and
A2:  F is_integral_of f,X & G is_integral_of g,X;
    set I = [' b,a '];
A6: X c= dom F & X c= dom G by A2,Th2; then
    X c= dom f /\ dom G & X c= dom F /\ dom g by A1,XBOOLE_1:19; then
    X c=dom (f(#)G) & X c=dom (F(#)g) by SEQ_1:def 5; then
A13:I c= dom(f(#)G) & I c= dom(F(#)g) by A3,XBOOLE_1:1;
A8: F is_differentiable_on X & F`|X=f|X &
    G is_differentiable_on X & G`|X=g|X by A2,Lemma; then
A9: X is open Subset of REAL by FDIFF_1:15,17; then
A21:F(#)G is_differentiable_on X by FDIFF_2:20,A8;
    F is_continuous_on X & G is_continuous_on X by A8,FDIFF_1:33; then
    F is_continuous_on I & G is_continuous_on I by A3,FCONT_1:17; then
A10:F is_integrable_on I & G is_integrable_on I &
    F is_bounded_on I & G is_bounded_on I by INTEGRA5:17,10;
    I c= dom f & I c= dom g & I c= dom F & I c= dom G
      by A1,A3,A6,XBOOLE_1:1; then
A11:f(#)G is_integrable_on I & F(#)g is_integrable_on I &
    f(#)G is_bounded_on I /\ I & F(#)g is_bounded_on I /\ I
      by A4,A5,A10,INTEGRA6:14,RFUNCT_1:101; then
A19:f(#)G + F(#)g is_integrable_on I &
    f(#)G + F(#)g is_bounded_on I /\ I by A13,INTEGRA6:11,RFUNCT_1:100;
A14:min(b,a) <= b & b <= max(b,a) by XXREAL_0:17,25;
A16:[. b,a .] = I by B1,INTEGRA5:def 4;
    min(b,a) = b by B1,XXREAL_0:def 8; then
    max(b,a) = a by XXREAL_0:36; then
A17:b in I & a in I by A14,A16,RCOMP_1:15; then
    a in X & b in X by A3; then
    a in dom F /\ dom G & b in dom F /\ dom G by A6,XBOOLE_0:def 3; then
A24:a in dom (F(#)G) & b in dom (F(#)G) by SEQ_1:def 5; then
A25:(F(#)G).b=(F.b)*(G.b) by SEQ_1:def 5;
    (F(#)G)`|X = (f|X)(#)G +F(#)(g|X) by A9,FDIFF_2:20,A8; then
    (F(#)G)`|X =(f(#)G)|X +F(#)(g|X) by RFUNCT_1:61; then
    (F(#)G)`|X =(f(#)G)|X +(F(#)g)|X by RFUNCT_1:61; then
    (F(#)G)`|X =(f(#)G+F(#)g)|X by RFUNCT_1:60; then
    F(#)G is_integral_of (f(#)G+F(#)g),X by A21,Lemma; then
    (F(#)G).a = integral(f(#)G+F(#)g,b,a) +(F(#)G).b 
      by Th60,B1,A3,A19; then
    (F(#)G).a-(F(#)G).b = integral(f(#)G,b,a) + integral(F(#)g,b,a) 
      by B1,A11,A13,A17,INTEGRA6:24;
    hence thesis by A24,A25,SEQ_1:def 5;
end;

theorem
  b <= a & [. b,a .] c= X & X c= dom f & X c= dom g &
  f is_continuous_on X & g is_continuous_on X &
  F is_integral_of f,X & G is_integral_of g,X
  implies
    (F.a)*(G.a) - (F.b)*(G.b) = integral(f(#)G,b,a)+ integral(F(#)g,b,a)
proof
   assume that
A7: b <= a and
A8: [. b,a .] c= X;
   assume that
A1: X c= dom f & X c= dom g and
A3: f is_continuous_on X & g is_continuous_on X and
A6: F is_integral_of f,X & G is_integral_of g,X;
A9:[. b,a .] =[' b,a '] by A7,INTEGRA5:def 4; then
   f is_continuous_on [' b,a '] & g is_continuous_on [' b,a '] 
     by FCONT_1:17,A3,A8; then
   f is_integrable_on [' b,a '] & f is_bounded_on [' b,a '] & 
   g is_integrable_on [' b,a '] & g is_bounded_on [' b,a '] 
     by INTEGRA5:10,17;
   hence thesis by Th100,A1,A6,A7,A8,A9;
end;

begin :: Examples of Indefinite Integral

theorem Th8:
  sin is_integral_of cos,REAL
proof
A1:dom(sin`|REAL) = REAL by FDIFF_1:def 8,SIN_COS:73;
   dom(cos|REAL) = (dom cos) /\ REAL by FUNCT_1:68; then
A2:dom(cos|REAL) = REAL /\ REAL by SIN_COS:def 22;
   now
    let x be set;
    assume A3: x in dom(sin`|REAL); then
    reconsider z=x as real number by XREAL_0:def 1;
    (sin`|REAL).x = diff(sin,z) by FDIFF_1:def 8,SIN_COS:73,A3; then 
    (sin`|REAL).x = cos.z by SIN_COS:69;
    hence (sin`|REAL).x = (cos|REAL).x by FUNCT_1:68,A3,A2;
   end; then
   sin`|REAL=cos|REAL by A1,A2,FUNCT_1:9;
   hence sin is_integral_of cos,REAL by Lemma,SIN_COS:73;
end;

theorem
  sin.b-sin.a = integral(cos,a,b)
proof
A1:cos is_continuous_on REAL by FDIFF_1:33,SIN_COS:72;
A2:min(a,b) <= a & a <= max(a,b) by XXREAL_0:17,25; then
A3:min(a,b) <= max(a,b) by XXREAL_0:2;
   [. min(a,b),max(a,b) .] c= REAL; then
A5:sin.max(a,b) = integral(cos,min(a,b),max(a,b)) + sin.min(a,b)
     by A1,A3,Th6,Th8;
A6:now assume A7:min(a,b) = a; then
    max(a,b) = b by XXREAL_0:36;
    hence sin.b - sin.a= integral(cos,a,b) by A5,A7; 
   end;
   now assume A8:min(a,b) = b; then
A9: max(a,b) = a by XXREAL_0:36;
    now assume b < a; then
     integral(cos,a,b) = -integral(cos,[' b,a ']) by INTEGRA5:def 5; then
     sin.a = -integral(cos,a,b) + sin.b by A5,A8,A9,A2,INTEGRA5:def 5;
     hence sin.b - sin.a= integral(cos,a,b);
    end;
    hence sin.b - sin.a= integral(cos,a,b) by A2,A8,A6,XXREAL_0:1;
   end;
   hence sin.b - sin.a= integral(cos,a,b) by A6,XXREAL_0:15;
end;

theorem Th10:
  (-1)(#)cos is_integral_of sin,REAL
proof  
   dom cos = REAL by SIN_COS:def 22; then
A2:dom((-1)(#)cos) =REAL by SEQ_1:def 6; then
A3:(-1)(#)cos is_differentiable_on REAL 
      by FDIFF_1:28,FCONT_3:4,SIN_COS:72; then
A4:dom (((-1)(#)cos)`|REAL) = REAL by FDIFF_1:def 8;
   dom(sin|REAL) = dom sin /\ REAL by FUNCT_1:68; then
A5:dom(sin|REAL) = REAL /\ REAL by SIN_COS:def 20;
   now let x be set;
    assume A6: x in dom(((-1)(#)cos)`|REAL); then
    reconsider z=x as real number by XREAL_0:def 1;
    (((-1)(#)cos)`|REAL).x=(-1)*diff(cos,z) 
      by SIN_COS:72,FCONT_3:4,A2,A6,FDIFF_1:28; then
    (((-1)(#)cos)`|REAL).x=(-1)*(-sin.z) by SIN_COS:68;
    hence (((-1)(#)cos)`|REAL).x=(sin|REAL).x by FUNCT_1:68,A6,A5;
   end; then
   ((-1)(#)cos)`|REAL=sin|REAL by A4,A5,FUNCT_1:9;
   hence (-1)(#)cos is_integral_of sin,REAL by Lemma,A3;
end;

theorem
  cos.a - cos.b = integral(sin,a,b)
proof  
P1:dom cos = REAL & REAL = dom sin by SIN_COS:def 20,def 22;
P2:sin is_continuous_on REAL by FDIFF_1:33,SIN_COS:73;
   min(a,b) in REAL & max(a,b) in REAL by XREAL_0:def 1; then
P7:min(a,b) in dom((-1)(#)cos) & max(a,b) in dom((-1)(#)cos) by P1,SEQ_1:def 6;
A2:min(a,b) <= a & a <= max(a,b) by XXREAL_0:17,25; then
A3:min(a,b) <= max(a,b) by XXREAL_0:2;
   [. min(a,b),max(a,b) .] c= REAL; then
   ((-1)(#)cos).max(a,b) 
    = integral(sin,min(a,b),max(a,b)) + ((-1)(#)cos).min(a,b)
      by Th6,P2,A3,Th10; then
   (-1)*cos.max(a,b) = integral(sin,min(a,b),max(a,b)) + ((-1)(#)cos).min(a,b)
      by SEQ_1:def 6,P7; then
   (-1)*cos.max(a,b) = integral(sin,min(a,b),max(a,b)) + (-1)*cos.min(a,b)
      by SEQ_1:def 6,P7; then
P: cos.min(a,b) - cos.max(a,b) = integral(sin,min(a,b),max(a,b));
Q: now assume A7:min(a,b) = a; then
    max(a,b) = b by XXREAL_0:36;
    hence cos.a - cos.b = integral(sin,a,b) by P,A7; 
   end;
   now assume A8:min(a,b) = b; then
A10:max(a,b) = a by XXREAL_0:36;
    b < a implies cos.a - cos.b= integral(sin,a,b)
    proof
     assume B:b < a; then
     integral(sin,a,b) = -integral(sin,[' b,a ']) by INTEGRA5:def 5; then
     integral(sin,a,b) = -integral(sin,b,a) by B,INTEGRA5:def 5;
     hence cos.a - cos.b= integral(sin,a,b) by A10,P,A8;
    end;
    hence cos.a - cos.b = integral(sin,a,b) by A2,A8,Q,XXREAL_0:1;
   end;
   hence cos.a - cos.b = integral(sin,a,b) by Q,XXREAL_0:15;
end;
 
theorem LM02:
  exp_R is_integral_of exp_R,REAL
proof
A1:dom(exp_R`|REAL) = REAL by FDIFF_1:def 8,TAYLOR_1:16;
A2:dom(exp_R|REAL) = REAL /\ REAL by TAYLOR_1:16,FUNCT_1:68;
   now
    let x be set;
    assume A3: x in dom(exp_R`|REAL); then
    reconsider z=x as real number by XREAL_0:def 1;
    reconsider z1=z as Real by XREAL_0:def 1;
    (exp_R`|REAL).x = diff(exp_R,z) by FDIFF_1:def 8,TAYLOR_1:16,A3; then 
    (exp_R`|REAL).x = exp_R.z1 by TAYLOR_1:16;
    hence (exp_R`|REAL).x = (exp_R|REAL).x by FUNCT_1:68,A2;
   end; then
   exp_R`|REAL=exp_R|REAL by A1,A2,FUNCT_1:9;
   hence exp_R is_integral_of exp_R,REAL by Lemma,TAYLOR_1:16;
end;

theorem
  exp_R.b-exp_R.a = integral(exp_R,a,b)
proof
A1:exp_R is_continuous_on REAL by FDIFF_1:33,TAYLOR_1:16;
A2:min(a,b) <= a & a <= max(a,b) by XXREAL_0:17,25; then
A3:min(a,b) <= max(a,b) by XXREAL_0:2;
   [. min(a,b),max(a,b) .] c= REAL; then
A5:exp_R.max(a,b) = integral(exp_R,min(a,b),max(a,b)) + exp_R.min(a,b)
     by A1,A3,Th6,LM02;
A6:min(a,b) = a implies exp_R.b - exp_R.a= integral(exp_R,a,b)
   proof
    assume A7:min(a,b) = a; then
    max(a,b) = b by XXREAL_0:36;
    hence exp_R.b - exp_R.a= integral(exp_R,a,b) by A5,A7; 
   end;
   min(a,b) = b implies exp_R.b - exp_R.a= integral(exp_R,a,b)
   proof
    assume A8:min(a,b) = b; then
A9: max(a,b) = a by XXREAL_0:36;
    b < a implies exp_R.b - exp_R.a= integral(exp_R,a,b)
    proof
     assume b < a; then
     integral(exp_R,a,b) = -integral(exp_R,[' b,a ']) by INTEGRA5:def 5; then
     exp_R.a = -integral(exp_R,a,b) + exp_R.b by A5,A8,A9,A2,INTEGRA5:def 5;
     hence exp_R.b - exp_R.a= integral(exp_R,a,b);
    end;
    hence exp_R.b - exp_R.a = integral(exp_R,a,b) by A2,A8,A6,XXREAL_0:1;
   end;
   hence exp_R.b - exp_R.a = integral(exp_R,a,b) by A6,XXREAL_0:15;
end;

theorem LM03:
  #Z (n+1) is_integral_of (n+1)(#)( #Z n),REAL
proof
P1:dom( #Z (n+1)) = REAL & dom( #Z n) = REAL by FUNCT_2:def 1;
   for x be Real st x in REAL holds 
    ( #Z (n+1))|REAL is_differentiable_in x
   proof
    let x be Real;
    assume x in REAL;
    #Z (n+1) is_differentiable_in x by TAYLOR_1:2;
    hence thesis by P1,RELAT_1:97;
   end; then
P2:#Z (n+1) is_differentiable_on REAL by P1,FDIFF_1:def 7;
P3:dom(( #Z (n+1))`|REAL) = REAL by P2,FDIFF_1:def 8;
   dom(((n+1)(#)( #Z n))|REAL) = (dom((n+1)(#)( #Z n))) /\ REAL
     by FUNCT_1:68; then
   dom(((n+1)(#)( #Z n))|REAL) = (dom( #Z n)) /\ REAL by SEQ_1:def 6; then
P4:dom(((n+1)(#)( #Z n))|REAL) = REAL /\ REAL by FUNCT_2:def 1;
   reconsider m = n+1 as natural number;
   now let x be set;
    assume P5: x in dom(( #Z (n+1))`|REAL); then
    reconsider z=x as real number by XREAL_0:def 1;
    (( #Z (n+1))`|REAL).x = diff(( #Z (n+1)),z) by P2,P5,FDIFF_1:def 8; then
    (( #Z (n+1))`|REAL).x = m * z #Z (m-1) by TAYLOR_1:2; then
P6: (( #Z (n+1))`|REAL).x = (n+1) * ( #Z n).x by TAYLOR_1:def 1;
    x in dom( #Z n) by P5,P1; then
    x in dom((n+1)(#)( #Z n)) by SEQ_1:def 6; then
    (( #Z (n+1))`|REAL).x = ((n+1)(#)( #Z n)).x by P6,SEQ_1:def 6;
    hence (( #Z (n+1))`|REAL).x = (((n+1)(#)( #Z n))|REAL).x 
      by FUNCT_1:68,P5,P4;
   end; then
   (( #Z (n+1))`|REAL) = (((n+1)(#)( #Z n))|REAL) by P3,P4,FUNCT_1:9;
   hence thesis by Lemma,P2;
end;

theorem
  ( #Z (n+1)).b - ( #Z (n+1)).a = integral((n+1)(#)( #Z n),a,b)
proof
P1:dom( #Z (n+1)) = REAL & dom( #Z n) = REAL by FUNCT_2:def 1; then
P2:REAL c= dom((n+1)(#)( #Z n)) by SEQ_1:def 6;
   for x be Real st x in REAL holds
    ( #Z n)|REAL is_differentiable_in x
   proof
    let x be Real;
    assume x in REAL;
    #Z n is_differentiable_in x by TAYLOR_1:2;
    hence thesis by P1,RELAT_1:97;
   end; then
   #Z n is_differentiable_on REAL by P1,FDIFF_1:def 7; then
   (n+1)(#)( #Z n) is_differentiable_on REAL &
   for x be Real st x in REAL holds
    (((n+1)(#)( #Z n))`|REAL).x = (n+1)*diff( #Z n,x)
      by P2,FCONT_3:4,FDIFF_1:28; then
A1:(n+1)(#)( #Z n) is_continuous_on REAL by FDIFF_1:33;
A2:min(a,b) <= a & a <= max(a,b) by XXREAL_0:17,25; then
A3:min(a,b) <= max(a,b) by XXREAL_0:2;
A4:[. min(a,b),max(a,b) .] c= REAL;
   #Z (n+1) is_integral_of (n+1)(#)( #Z n),REAL by LM03; then
A5:( #Z (n+1)).max(a,b)
    = integral((n+1)(#)( #Z n),min(a,b),max(a,b)) + ( #Z (n+1)).min(a,b)
     by A1,A3,A4,Th6;
A6:min(a,b) = a implies
    ( #Z (n+1)).b - ( #Z (n+1)).a= integral((n+1)(#)( #Z n),a,b)
   proof
    assume A7:min(a,b) = a; then
    max(a,b) = b by XXREAL_0:36;
    hence ( #Z (n+1)).b - ( #Z (n+1)).a
             = integral((n+1)(#)( #Z n),a,b) by A5,A7;
   end;
   min(a,b) = b implies 
    ( #Z (n+1)).b - ( #Z (n+1)).a= integral((n+1)(#)( #Z n),a,b)
   proof
    assume A8:min(a,b) = b; then
A9: max(a,b) = a by XXREAL_0:36;
    b < a implies
     ( #Z (n+1)).b - ( #Z (n+1)).a= integral((n+1)(#)( #Z n),a,b)
    proof
     assume b < a; then
     integral((n+1)(#)( #Z n),a,b) = -integral((n+1)(#)( #Z n),[' b,a '])
       by INTEGRA5:def 5; then
     ( #Z (n+1)).a = -integral((n+1)(#)( #Z n),a,b) + ( #Z (n+1)).b 
       by A5,A8,A9,A2,INTEGRA5:def 5;
     hence ( #Z (n+1)).b - ( #Z (n+1)).a= integral((n+1)(#)( #Z n),a,b);
    end;
    hence ( #Z (n+1)).b - ( #Z (n+1)).a= integral((n+1)(#)( #Z n),a,b) 
     by A2,A8,A6,XXREAL_0:1;
   end;
   hence thesis by A6,XXREAL_0:15;
end;

begin :: Uniform Convergent Functional Sequence

theorem
  for H be Functional_Sequence of REAL,REAL,
      rseq be Real_Sequence st
  a < b &
  (for n be Element of NAT holds
   H.n is_integrable_on [' a,b '] & H.n is_bounded_on [' a,b '] &
   rseq.n = integral(H.n,a,b) ) &
  H is_unif_conv_on [' a,b ']
    holds
  lim(H,[' a,b ']) is_bounded_on [' a,b '] &
  lim(H,[' a,b ']) is_integrable_on [' a,b '] &
  rseq is convergent &
  lim rseq = integral(lim(H,[' a,b ']),a,b)
proof
    let H be Functional_Sequence of REAL,REAL, rseq be Real_Sequence;
    set AB = [' a,b '];
    assume that
a1:  a < b and
a2:  (for n be Element of NAT holds
       H.n is_integrable_on AB & H.n is_bounded_on AB &
       rseq.n =integral(H.n,a,b) ) and
a3:  H is_unif_conv_on AB;
a4: AB common_on_dom H by SEQFUNC:44,a3;
    H is_point_conv_on AB by a3,SEQFUNC:23; then
a5: dom lim(H,AB) = AB by SEQFUNC:22;
    consider K be Element of NAT such that
P11: for n be Element of NAT,x be Element of REAL st n >= K & x in AB  
      holds abs((H.n).x-(lim(H,AB)).x) < 1 by a3,SEQFUNC:44;
    H.K is_bounded_on AB by a2; then
    consider r be real number such that 
P13: for c be set st c in AB /\ dom(H.K) holds 
      abs((H.K).c) <= r by RFUNCT_1:90;
    dom lim(H,AB) c= dom(H.K) by a4,a5,SEQFUNC:def 10; then
a6: AB /\ dom lim(H,AB) c= AB /\ dom(H.K) by XBOOLE_1:26;
    now let c be set;
     assume
P14:  c in AB /\ dom lim(H,AB); then
P142:abs((H.K).c) <= r by P13,a6;
     c in AB by P14,XBOOLE_0:def 3; then
     abs((H.K).c - (lim(H,AB)).c) < 1 by P11; then
P143:abs((H.K).c) + abs((H.K).c-(lim(H,AB)).c) <= r+1 by P142,XREAL_1:9;
     (lim(H,AB)).c = (H.K).c - ((H.K).c - (lim(H,AB)).c); then
     abs((lim(H,AB)).c) 
      <= abs((H.K).c) + abs((H.K).c-(lim(H,AB)).c) by COMPLEX1:143;
     hence abs((lim(H,AB)).c) <= r+1 by P143,XXREAL_0:2;
    end;
    hence 
P1: lim(H,AB) is_bounded_on AB by RFUNCT_1:90;
    consider T being DivSequence of AB such that
P251:delta T is convergent & lim delta T = 0 by INTEGRA4:11;
P15:for n be Element of NAT holds
     (H.n)||AB is Function of AB,REAL & 
     (H.n)||AB is_bounded_on AB &
     lower_sum((H.n)||AB,T) is convergent & 
     lim lower_sum((H.n)||AB,T) = lower_integral((H.n)||AB) &
     upper_sum((H.n)||AB,T) is convergent &
     lim upper_sum((H.n)||AB,T) = upper_integral((H.n)||AB) &
     lower_integral((H.n)||AB) = upper_integral((H.n)||AB) &
     rseq.n =integral((H.n)||AB)
    proof    
     let n be Element of NAT;
     AB c= dom (H.n) by a4,SEQFUNC:def 10; then
     (H.n)||AB is total by INTEGRA5:6;
     hence
P20:  (H.n)||AB is Function of AB,REAL by FUNCT_2:131; 
     H.n is_bounded_on AB by a2;
     hence (H.n)||AB is_bounded_on AB by INTEGRA5:9;
     hence lower_sum((H.n)||AB,T) is convergent &
      lim lower_sum((H.n)||AB,T) = lower_integral((H.n)||AB) &
      upper_sum((H.n)||AB,T) is convergent &
      lim upper_sum((H.n)||AB,T) = upper_integral((H.n)||AB)
        by P20,P251,INTEGRA4:8,9;
     (H.n) is_integrable_on AB by a2; then
     (H.n)||AB is_integrable_on AB by INTEGRA5:def 2;
     hence
      lower_integral((H.n)||AB) = upper_integral((H.n)||AB) by INTEGRA1:def 17;
     rseq.n = integral(H.n,a,b) by a2; then
     rseq.n = integral(H.n,AB) by a1,INTEGRA5:def 5;
     hence rseq.n = integral((H.n)||AB);
    end;
    set H0 = lim(H,AB)||AB;
    lim(H,AB) is_bounded_above_on AB &
    lim(H,AB) is_bounded_below_on AB by P1,RFUNCT_1:def 11; then
P21:H0 is_bounded_above_on AB & H0 is_bounded_below_on AB by INTEGRA5:7,8; then
P23:H0 is_bounded_on AB by RFUNCT_1:def 11;
    H0 is total by a5,INTEGRA5:6; then
P20:H0 is Function of AB,REAL by FUNCT_2:131; then
P24:H0 is_upper_integrable_on AB & H0 is_lower_integrable_on AB
     by P23,INTEGRA4:10;
P16:for e be Element of REAL st 0 < e
     ex N be Element of NAT st
      for n,k be Element of NAT st N<=n holds
       abs(upper_sum((H.n)||AB,T).k-upper_sum(H0,T).k) <= e*(b-a)
    proof
     let e be Element of REAL;
     assume 0 < e; then
     consider N be Element of NAT such that 
P162: for n be Element of NAT,x be Element of REAL st n>=N & x in AB  
       holds abs((H.n).x-(lim(H,AB)).x)<e by a3,SEQFUNC:44;
     take N;
     now let n,k be Element of NAT;
      assume P163:N<= n;
      set Hn = (H.n)||AB;
      reconsider Tk=T.k as DivisionPoint of AB by INTEGRA1:def 4;
      set l0 = len Tk;
      len upper_volume(Hn,T.k) = l0 by INTEGRA1:def 7; then
      reconsider R1=upper_volume(Hn,T.k) as Element of l0-tuples_on REAL
        by FINSEQ_2:110;
      len upper_volume(H0,T.k) = l0 by INTEGRA1:def 7; then
      reconsider R2=upper_volume(H0,T.k) as Element of l0-tuples_on REAL
        by FINSEQ_2:110;
      consider H1 be Function of AB,REAL such that
P16A:  rng H1 = {e} & H1 is_bounded_on AB by INTEGRA4:5;
      len upper_volume(H1,T.k) = l0 by INTEGRA1:def 7; then
      reconsider R3=upper_volume(H1,T.k) as Element of l0-tuples_on REAL
        by FINSEQ_2:110;
      upper_sum(Hn,T).k-upper_sum(H0,T).k 
       = upper_sum(Hn,T.k)-upper_sum(H0,T).k by INTEGRA2:def 4; then
      upper_sum(Hn,T).k-upper_sum(H0,T).k 
       =upper_sum(Hn,T.k)-upper_sum(H0,T.k) by INTEGRA2:def 4; then
P16B: upper_sum(Hn,T).k-upper_sum(H0,T).k = Sum(R1-R2) by RVSUM_1:120;
      upper_sum(H0,T).k-upper_sum(Hn,T).k
       = upper_sum(H0,T.k)-upper_sum(Hn,T).k by INTEGRA2:def 4; then
      upper_sum(H0,T).k-upper_sum(Hn,T).k
       = upper_sum(H0,T.k)-upper_sum(Hn,T.k) by INTEGRA2:def 4; then
LP16B:upper_sum(H0,T).k-upper_sum(Hn,T).k = Sum(R2-R1) by RVSUM_1:120;
P16C: for i be Element of NAT st i in Seg l0 holds
       (R1-R2).i <= R3.i & (R2-R1).i <= R3.i
      proof  
       let i be Element of NAT;
       assume T1: i in Seg l0;
       (R1-R2).i = R1.i-R2.i by RVSUM_1:48; then
       (R1-R2).i = sup rng(Hn|divset(T.k,i)) * vol divset(T.k,i)
                  - (upper_volume(H0,T.k)).i by INTEGRA1:def 7,T1; then
       (R1-R2).i = sup rng(Hn|divset(T.k,i)) * vol divset(T.k,i)
                  - sup rng(H0|divset(T.k,i)) * vol divset(T.k,i)
                     by INTEGRA1:def 7,T1; then
T2:    (R1-R2).i
        = (sup rng (Hn|divset(T.k,i)) - sup rng (H0|divset(T.k,i)))
           * vol divset(T.k,i);
       (R2-R1).i = R2.i-R1.i by RVSUM_1:48; then
       (R2-R1).i = sup rng(H0|divset(T.k,i)) * vol divset(T.k,i)
                  - (upper_volume(Hn,T.k)).i by INTEGRA1:def 7,T1; then
       (R2-R1).i = sup rng(H0|divset(T.k,i)) * vol divset(T.k,i)
                  - sup rng (Hn|divset(T.k,i)) * vol divset(T.k,i)
                     by INTEGRA1:def 7,T1; then
LT2:   (R2-R1).i = (sup rng(H0|divset(T.k,i)) - sup rng(Hn|divset(T.k,i)))
                  * vol divset(T.k,i);
T3:    sup rng(Hn|divset(T.k,i)) - sup rng(H0|divset(T.k,i))
        <= sup rng(H1|divset(T.k,i)) &
       sup rng(H0|divset(T.k,i)) - sup rng(Hn|divset(T.k,i))
        <= sup rng(H1|divset(T.k,i))  
       proof
        i in dom Tk by T1,FINSEQ_1:def 3; then
B1:     divset(T.k,i) c= AB by INTEGRA1:10; then
D21:    H0|divset(T.k,i) is_bounded_above_on divset(T.k,i) by P21,INTEGRA2:5;
        Hn is_bounded_on AB by P15; then
        Hn is_bounded_above_on AB by RFUNCT_1:def 11; then
E21:    Hn|divset(T.k,i) is_bounded_above_on divset(T.k,i) by INTEGRA2:5,B1;
        Hn is Function of AB, REAL by P15; then
        reconsider f = Hn|divset(T.k,i) as Function of divset(T.k,i),REAL
          by FUNCT_2:38,B1;
        reconsider g = H0|divset(T.k,i) as Function of divset(T.k,i),REAL
          by B1,FUNCT_2:38,P20;
T33:    rng f is bounded_above & rng g is bounded_above by D21,E21,INTEGRA1:15;
T341:   rng (H1|divset(T.k,i)) c= {e} by P16A,RELAT_1:99;
        consider x0 be set such that
T342:    x0 in divset(T.k,i) by XBOOLE_0:def 1;
        H1.x0 in {e} by B1,T342,FUNCT_2:6,P16A; then
        H1.x0 = e by TARSKI:def 1; then
T343:   (H1|divset(T.k,i)).x0 = e by T342,FUNCT_1:72;
        H1|divset(T.k,i) is Function of divset(T.k,i),REAL
          by B1,FUNCT_2:38; then
        e in rng(H1|divset(T.k,i)) by FUNCT_2:6,T342,T343; then
        {e} c= rng(H1|divset(T.k,i)) by ZFMISC_1:37; then
        rng(H1|divset(T.k,i)) = {e} by T341,XBOOLE_0:def 10; then
T36:    e=sup rng(H1|divset(T.k,i)) by SEQ_4:22;
T37:    for x be set st x in divset(T.k,i) holds abs(f.x-g.x) <=e
        proof
         let x be set;
         assume
T361:     x in divset(T.k,i); then
         abs(f.x-g.x) = abs(Hn.x-g.x) by FUNCT_1:72; then
         abs(f.x-g.x) = abs(Hn.x-H0.x) by T361,FUNCT_1:72; then
         abs(f.x-g.x) = abs((H.n).x-H0.x) by T361,B1,FUNCT_1:72; then
         abs(f.x-g.x) = abs((H.n).x-(lim(H,AB)).x) 
           by T361,B1,FUNCT_1:72;
         hence thesis by T361,B1,P162,P163;
        end;
        hence sup rng(Hn|divset(T.k,i)) - sup rng(H0|divset(T.k,i))
              <= sup rng(H1|divset(T.k,i)) by T33,T36,ThSup;
        for x be set st x in divset(T.k,i) holds
         abs(g.x-f.x) <= sup rng(H1|divset(T.k,i))
        proof
         let x be set;
         assume x in divset(T.k,i); then
         abs(f.x-g.x) <= sup rng(H1|divset(T.k,i)) by T36,T37;
         hence thesis by COMPLEX1:146;
        end;
        hence sup rng(H0|divset(T.k,i)) - sup rng(Hn|divset(T.k,i))
              <= sup rng(H1|divset(T.k,i)) by T33,ThSup;
       end;
T4:    0 <= vol divset(T.k,i) by INTEGRA1:11; then
       (R1-R2).i <= sup rng(H1|divset(T.k,i)) * vol divset(T.k,i)
         by T2,XREAL_1:66,T3;
       hence (R1-R2).i <= R3.i by INTEGRA1:def 7,T1;
       (R2-R1).i <= sup rng(H1|divset(T.k,i)) * vol divset(T.k,i)
         by LT2,XREAL_1:66,T3,T4;
       hence thesis by INTEGRA1:def 7,T1;
      end;
      for i be Element of NAT st
       i in Seg l0 holds (R1-R2).i <= R3.i by P16C; then
P16J: upper_sum(Hn,T).k-upper_sum(H0,T).k <= Sum R3 by P16B,RVSUM_1:112;
P16E: sup rng H1 = e by SEQ_4:22,P16A;
P16F: vol AB = b-a by a1,INTEGRA6:5;
      H1 is_bounded_above_on AB by P16A,RFUNCT_1:def 11; then
P16Y: upper_sum(H1,T.k) <= e*(b-a) by P16E,P16F,INTEGRA1:29; then
P163: upper_sum((H.n)||AB,T).k-upper_sum(H0,T).k <= e*(b-a)
       by P16J,XXREAL_0:2;
      for i be Element of NAT st
       i in Seg l0 holds (R2-R1).i <= R3.i by P16C; then
      upper_sum(H0,T).k-upper_sum(Hn,T).k <= Sum R3 by LP16B,RVSUM_1:112; then
      upper_sum(H0,T).k-upper_sum(Hn,T).k <= e*(b-a) by P16Y,XXREAL_0:2; then
      -e*(b-a) <= -(upper_sum(H0,T).k - upper_sum((H.n)||AB,T).k)
         by XREAL_1:26;
      hence abs(upper_sum((H.n)||AB,T).k-upper_sum(H0,T).k)
                 <=e*(b-a) by ABSVALUE:12,P163;
     end;
     hence thesis;
    end;
LP16:for e be Element of REAL st 0 < e
      ex N be Element of NAT st 
       for n,k be Element of NAT st N<=n holds
        abs(lower_sum((H.n)||AB,T).k-lower_sum(H0,T).k) <= e*(b-a) 
    proof
     let e be Element of REAL;
     assume 0 < e; then
     consider N be Element of NAT such that
P162: for n be Element of  NAT,x be Element of REAL st
       n>=N & x in AB holds
       abs((H.n).x-(lim(H,AB)).x)<e by SEQFUNC:44,a3;
     take N;
     hereby let n,k be Element of NAT;
      assume P163: N<= n;
      set Hn=(H.n)||AB;
      reconsider Tk=T.k as DivisionPoint of AB by INTEGRA1:def 4;
      set l0 = len Tk;
      len lower_volume(Hn,T.k) = l0 by INTEGRA1:def 8; then
      reconsider R1=lower_volume(Hn,T.k) as Element of l0-tuples_on REAL
        by FINSEQ_2:110;
      len lower_volume(H0,T.k) = l0 by INTEGRA1:def 8; then
      reconsider R2=lower_volume(H0,T.k) as Element of l0-tuples_on REAL
