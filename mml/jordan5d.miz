theorem Th37:
  1 <= i & i <= len GoB h & 1 <= j & j <= width GoB h implies ex q
  st q`1 = (GoB h)*(i,j)`1 & q in L~h
proof
  assume that
A1: 1 <= i and
A2: i <= len GoB h and
A3: 1 <= j and
A4: j <= width GoB h;
  consider k such that
A5: k in dom h and
  [i,j] in Indices GoB h and
A6: (h/.k)`1 = (GoB h)* (i,j)`1 by A1,A2,A3,A4,Th11;
  take q = h/.k;
  thus q`1 = (GoB h)*(i,j)`1 by A6;
  4 < len h by GOBOARD7:36;
  hence thesis by A5,GOBOARD1:16,XXREAL_0:2;
end;

