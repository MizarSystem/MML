theorem
  n_w_n h <> n_w_s h
proof
  set i1 = n_w_n h, i2 = n_w_s h;
A1: i1 <= i1 + 1 by NAT_1:11;
A2: 1 <= i1 by Def15;
  i1+1 <= len h by Def15;
  then i1 <= len h by A1,XXREAL_0:2;
  then i1 in dom h by A2,FINSEQ_3:27;
  then
A3: h.i1 = h/.i1 by PARTFUN1:def 8;
A4: i2 <= i2 + 1 by NAT_1:11;
A5: h.i2 = S-min L~h by Def13;
A6: 1 <= i2 by Def13;
  i2+1 <= len h by Def13;
  then i2 <= len h by A4,XXREAL_0:2;
  then i2 in dom h by A6,FINSEQ_3:27;
  then
A7: h.i2 = h/.i2 by PARTFUN1:def 8;
A8: h.i1 = N-min L~h by Def15;
  thus i1 <> i2
  proof
    assume i1 = i2;
    then
A9: N-bound(L~h) = (h/.i2)`2 by A8,A3,EUCLID:56
      .= S-bound(L~h) by A5,A7,EUCLID:56;
A10: 1 <= len h by GOBOARD7:36,XXREAL_0:2;
    then
A11: (h/.1)`2 >= S-bound (L~h) by Th13;
    consider ii be Element of NAT such that
A12: ii in dom h and
A13: (h/.ii)`2 <> (h/.1)`2 by GOBOARD7:33;
A14: ii <= len h by A12,FINSEQ_3:27;
A15: 1 <= ii by A12,FINSEQ_3:27;
    then
A16: (h/.ii)`2 <= N-bound (L~h) by A14,Th13;
A17: (h/.ii)`2 >= S-bound L~h by A15,A14,Th13;
    (h/.1)`2 <= N-bound (L~h) by A10,Th13;
    then (h/.1)`2 = N-bound (L~h) by A9,A11,XXREAL_0:1;
    hence thesis by A9,A13,A16,A17,XXREAL_0:1;
  end;
end;

