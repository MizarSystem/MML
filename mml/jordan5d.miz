    end;
    case
      LSeg(h,i) is horizontal;
      then LSeg(h/.i,h/.(i+1)) is horizontal by A3,A4,TOPREAL1:def 5;
      then
A15:  (h/.i)`2=(h/.(i+1))`2 by SPPOL_1:36;
