begin :: Preliminaries

reserve p, q for Point of TOP-REAL 2,
  r for Real,
  h for non constant standard special_circular_sequence,
  g for FinSequence of TOP-REAL 2,
  f for non empty FinSequence of TOP-REAL 2,
  I, i1, i, j, k for Element of NAT;

canceled 2;

theorem Th3:
  for n be Element of NAT, h be FinSequence of TOP-REAL n st len h
  >= 2 holds h/.len h in LSeg(h,len h-'1)
proof
  let n be Element of NAT;
  let h be FinSequence of TOP-REAL n;
  set i = len h;
  assume
A1: len h >= 2;
  then
A2: 2-1 <= i-1 by XREAL_1:11;
  i-'1+1 = len h by A1,XREAL_1:237,XXREAL_0:2;
  hence thesis by A2,TOPREAL1:27;
end;

theorem Th4:
  3 <= i implies i mod (i-'1) = 1
proof
  assume
A1: 3 <= i;
  then
A2: i-'1 = i-1 by XREAL_1:235,XXREAL_0:2;
  3-'1 = 2+1-'1 .= 2 by NAT_D:34;
  then 2 <= i-'1 by A1,NAT_D:42;
  then 1 < i-1 by A2,XXREAL_0:2;
  then 1+i < (i-1)+i by XREAL_1:10;
  then 1+i-1 < (i-1)+i-1 by XREAL_1:16;
  then
A3: i < (i-1) + (i-1);
  i-'1 <= i by NAT_D:35;
  hence i mod (i-'1) = i - (i-1) by A2,A3,JORDAN4:7
    .= 1;
end;

theorem Th5:
  p in rng h implies ex i be Element of NAT st 1 <= i & i+1 <= len h & h.i = p
proof
A1: 4 < len h by GOBOARD7:36;
A2: 1 < len h by GOBOARD7:36,XXREAL_0:2;
  assume p in rng h;
  then consider x be set such that
A3: x in dom h and
A4: p = h.x by FUNCT_1:def 5;
  reconsider i = x as Element of NAT by A3;
A5: 1 <= i by A3,FINSEQ_3:27;
  set j = S_Drop (i,h);
A6: i <= len h by A3,FINSEQ_3:27;
  1 <= j & j+1 <= len h & h.j = p
  proof
A7: i <= len h -' 1 + 1 by A5,A6,XREAL_1:237,XXREAL_0:2;
    per cases by A7,NAT_1:8;
    suppose
A8:   i <= len h-'1;
      then j = i by A5,JORDAN4:34;
      then j + 1 <= len h -' 1 + 1 by A8,XREAL_1:9;
      hence thesis by A4,A5,A2,A8,JORDAN4:34,XREAL_1:237;
    end;
    suppose
A9:   i = len h-'1+1;
      then i = len h by A1,XREAL_1:237,XXREAL_0:2;
      then i mod (len h -'1) = 1 by A1,Th4,XXREAL_0:2;
      then
A10:  j = 1 by JORDAN4:def 1;
A11:  1 <= len h by GOBOARD7:36,XXREAL_0:2;
      then
A12:  len h in dom h by FINSEQ_3:27;
      1 in dom h by A11,FINSEQ_3:27;
      then h.1 = h/.1 by PARTFUN1:def 8
        .= h/.len h by FINSEQ_6:def 1
        .= h.len h by A12,PARTFUN1:def 8;
      hence thesis by A4,A1,A9,A10,XREAL_1:237,XXREAL_0:2;
    end;
  end;
  hence thesis;
end;

theorem Th6:
  for g being FinSequence of REAL st r in rng g holds Incr g.1 <= r
  & r <= Incr g.len Incr g
proof
  let g be FinSequence of REAL;
  assume r in rng g;
  then r in rng Incr g by SEQ_4:def 25;
  then consider x being set such that
A1: x in dom Incr g and
A2: Incr g.x=r by FUNCT_1:def 5;
  reconsider j=x as Element of NAT by A1;
A3: x in Seg len Incr g by A1,FINSEQ_1:def 3;
  then
A4: j <= len Incr g by FINSEQ_1:3;
A5: 1 <= j by A3,FINSEQ_1:3;
  then
A6: 1 <= len Incr g by A4,XXREAL_0:2;
  then 1 in dom Incr g by FINSEQ_3:27;
  hence Incr g.1 <= r by A1,A2,A5,SEQ_4:154;
  len Incr g in dom Incr g by A6,FINSEQ_3:27;
  hence thesis by A1,A2,A4,SEQ_4:154;
end;

theorem Th7:
  1 <= i & i <= len h & 1 <= I & I <= width GoB h implies (GoB h)*(
  1,I)`1 <= (h/.i)`1 & (h/.i)`1 <= (GoB h)*(len GoB h,I)`1
proof
  assume that
A1: 1<=i and
A2: i<=len h and
A3: 1 <= I and
A4: I <= width GoB h;
A5: I<=width GoB(Incr(X_axis(h)),Incr(Y_axis(h))) by A4,GOBOARD2:def 3;
  i<=len (X_axis h) by A2,GOBOARD1:def 3;
  then
A6: i in dom (X_axis h) by A1,FINSEQ_3:27;
  then (X_axis(h)).i=(h/.i)`1 by GOBOARD1:def 3;
  then
A7: (h/.i)`1 in rng X_axis h by A6,FUNCT_1:def 5;
A8: GoB h=GoB(Incr(X_axis h),Incr(Y_axis(h))) by GOBOARD2:def 3;
  then 1<=len GoB(Incr(X_axis h),Incr(Y_axis h)) by GOBOARD7:34;
  then
A9: [1,I] in Indices GoB(Incr(X_axis h),Incr(Y_axis h)) by A3,A5,MATRIX_1:37;
A10: 1<=len GoB h by GOBOARD7:34;
  len GoB h <= len GoB(Incr(X_axis h),Incr(Y_axis h)) by GOBOARD2:def 3;
  then
A11: [len GoB h,I] in Indices GoB(Incr(X_axis h),Incr(Y_axis h)) by A3,A5,A10,
MATRIX_1:37;
  (GoB h)*(len GoB h,I)=(GoB(Incr(X_axis h),Incr(Y_axis h)))* (len GoB h,
  I) by GOBOARD2:def 3
    .=|[Incr(X_axis h).len GoB h,Incr(Y_axis(h)).I]| by A11,GOBOARD2:def 1;
  then
A12: (GoB h)*(len GoB h,I)`1 = Incr(X_axis h).len GoB h by EUCLID:56;
  (GoB h)*(1,I)=(GoB(Incr(X_axis(h)),Incr(Y_axis(h))))*(1,I) by GOBOARD2:def 3
    .=|[Incr(X_axis(h)).1,Incr(Y_axis(h)).I]| by A9,GOBOARD2:def 1;
  then
A13: (GoB h)*(1,I)`1=Incr(X_axis(h)).1 by EUCLID:56;
  len GoB h = len Incr (X_axis h) by A8,GOBOARD2:def 1;
  hence thesis by A12,A13,A7,Th6;
end;

theorem Th8:
  1 <= i & i <= len h & 1 <= I & I <= len GoB h implies (GoB h)*(I,
  1)`2 <= (h/.i)`2 & (h/.i)`2 <= (GoB h)*(I,width GoB h)`2
proof
  assume that
A1: 1<=i and
A2: i<=len h and
A3: 1 <= I and
A4: I <= len GoB h;
A5: GoB h=GoB(Incr(X_axis h),Incr(Y_axis h)) by GOBOARD2:def 3;
  then
A6: 1<=width GoB(Incr(X_axis h),Incr(Y_axis h)) by GOBOARD7:35;
  i <= len Y_axis h by A2,GOBOARD1:def 4;
  then
A7: i in dom Y_axis h by A1,FINSEQ_3:27;
  then (Y_axis h).i = (h/.i)`2 by GOBOARD1:def 4;
  then
A8: (h/.i)`2 in rng Y_axis h by A7,FUNCT_1:def 5;
  1<=width GoB h by GOBOARD7:35;
  then
A9: [I,width GoB h] in Indices GoB(Incr(X_axis h),Incr(Y_axis h)) by A3,A4,A5,
MATRIX_1:37;
  (GoB h)*(I,width GoB h)=(GoB(Incr(X_axis h),Incr(Y_axis h)))* (I,width
  GoB h) by GOBOARD2:def 3
    .=|[Incr(X_axis(h)).I,Incr(Y_axis(h)).width GoB h]| by A9,GOBOARD2:def 1;
  then
A10: (GoB h)*(I,width GoB h)`2 = Incr(Y_axis h).width GoB h by EUCLID:56;
  I<=len GoB(Incr(X_axis h),Incr(Y_axis h)) by A4,GOBOARD2:def 3;
  then
A11: [I,1] in Indices GoB(Incr(X_axis h),Incr(Y_axis h)) by A3,A6,MATRIX_1:37;
  (GoB h)*(I,1)=(GoB(Incr(X_axis h),Incr(Y_axis h)))*(I,1) by GOBOARD2:def 3
    .= |[Incr(X_axis(h)).I,Incr(Y_axis(h)).1]| by A11,GOBOARD2:def 1;
  then
A12: (GoB h)*(I,1)`2 = Incr(Y_axis h).1 by EUCLID:56;
  width GoB h = len Incr Y_axis h by A5,GOBOARD2:def 1;
  hence thesis by A10,A12,A8,Th6;
end;

theorem Th9:
  1 <= i & i <= len GoB f implies ex k,j st k in dom f & [i,j] in
  Indices GoB f & f/.k = (GoB f)*(i,j)
proof
  assume that
A1: 1 <= i and
A2: i <= len GoB f;
A3: i in dom GoB f by A1,A2,FINSEQ_3:27;
A4: GoB f = GoB(Incr X_axis f,Incr Y_axis f) by GOBOARD2:def 3;
  then len Incr X_axis f = len GoB f by GOBOARD2:def 1;
  then i in dom Incr X_axis f by A1,A2,FINSEQ_3:27;
  then (Incr X_axis f).i in rng Incr X_axis f by FUNCT_1:def 5;
  then (Incr X_axis f).i in rng X_axis f by SEQ_4:def 25;
  then consider k being Nat such that
A5: k in dom X_axis f and
A6: (X_axis f).k = (Incr X_axis f).i by FINSEQ_2:11;
A7: len X_axis f = len f by GOBOARD1:def 3
    .= len Y_axis f by GOBOARD1:def 4;
  then dom X_axis f = dom Y_axis f by FINSEQ_3:31;
  then (Y_axis f).k in rng Y_axis f by A5,FUNCT_1:def 5;
  then (Y_axis f).k in rng Incr Y_axis f by SEQ_4:def 25;
  then consider j being Nat such that
A8: j in dom Incr Y_axis f and
A9: (Y_axis f).k = (Incr Y_axis f).j by FINSEQ_2:11;
  reconsider k,j as Element of NAT by ORDINAL1:def 13;
A10: (X_axis f).k = (f/.k)`1 by A5,GOBOARD1:def 3;
  take k,j;
  len X_axis f = len f by GOBOARD1:def 3;
  hence k in dom f by A5,FINSEQ_3:31;
  j in Seg len Incr Y_axis f by A8,FINSEQ_1:def 3;
  then j in Seg width GoB(Incr X_axis f,Incr Y_axis f) by GOBOARD2:def 1;
  then [i,j] in [:dom GoB f, Seg width GoB f:] by A4,A3,ZFMISC_1:106;
  hence
A11: [i,j] in Indices GoB f by MATRIX_1:def 5;
  dom X_axis f = dom Y_axis f by A7,FINSEQ_3:31;
  then (Y_axis f).k = (f/.k)`2 by A5,GOBOARD1:def 4;
  hence f/.k = |[Incr(X_axis f).i,Incr(Y_axis f).j]| by A6,A9,A10,EUCLID:57
    .= (GoB f)*(i,j) by A4,A11,GOBOARD2:def 1;
end;

theorem Th10:
  1 <= j & j <= width GoB f implies ex k,i st k in dom f & [i,j]
  in Indices GoB f & f/.k = (GoB f)*(i,j)
proof
  assume that
A1: 1 <= j and
A2: j <= width GoB f;
A3: j in Seg width GoB f by A1,A2,FINSEQ_1:3;
A4: GoB f = GoB(Incr X_axis f,Incr Y_axis f) by GOBOARD2:def 3;
  then len Incr Y_axis f = width GoB f by GOBOARD2:def 1;
  then j in dom Incr Y_axis f by A1,A2,FINSEQ_3:27;
  then (Incr Y_axis f).j in rng Incr Y_axis f by FUNCT_1:def 5;
  then (Incr Y_axis f).j in rng Y_axis f by SEQ_4:def 25;
  then consider k being Nat such that
A5: k in dom Y_axis f and
A6: (Y_axis f).k = (Incr Y_axis f).j by FINSEQ_2:11;
A7: len X_axis f = len f by GOBOARD1:def 3
    .= len Y_axis f by GOBOARD1:def 4;
  then k in dom X_axis f by A5,FINSEQ_3:31;
  then (X_axis f).k in rng X_axis f by FUNCT_1:def 5;
  then (X_axis f).k in rng Incr X_axis f by SEQ_4:def 25;
  then consider i being Nat such that
A8: i in dom Incr X_axis f and
A9: (X_axis f).k = (Incr X_axis f).i by FINSEQ_2:11;
  reconsider k,i as Element of NAT by ORDINAL1:def 13;
  k in dom X_axis f by A5,A7,FINSEQ_3:31;
  then
A10: (X_axis f).k = (f/.k)`1 by GOBOARD1:def 3;
  take k,i;
  len Y_axis f = len f by GOBOARD1:def 4;
  hence k in dom f by A5,FINSEQ_3:31;
  len GoB(Incr X_axis f,Incr Y_axis f) = len Incr X_axis f by GOBOARD2:def 1;
  then i in dom GoB(Incr X_axis f,Incr Y_axis f) by A8,FINSEQ_3:31;
  then [i,j] in [:dom GoB f, Seg width GoB f:] by A4,A3,ZFMISC_1:106;
  hence
A11: [i,j] in Indices GoB f by MATRIX_1:def 5;
  (Y_axis f).k = (f/.k)`2 by A5,GOBOARD1:def 4;
  hence f/.k = |[Incr(X_axis f).i,Incr(Y_axis f).j]| by A6,A9,A10,EUCLID:57
    .= (GoB f)*(i,j) by A4,A11,GOBOARD2:def 1;
end;

theorem Th11:
  1 <= i & i <= len GoB f & 1 <= j & j <= width GoB f implies ex k
  st k in dom f & [i,j] in Indices GoB f & (f/.k)`1 = (GoB f)*(i,j)`1
proof
  assume that
A1: 1 <= i and
A2: i <= len GoB f and
A3: 1 <= j and
A4: j <= width GoB f;
A5: GoB f = GoB (Incr X_axis f,Incr Y_axis f) by GOBOARD2:def 3;
  then len Incr Y_axis f = width GoB f by GOBOARD2:def 1;
  then j in dom Incr Y_axis f by A3,A4,FINSEQ_3:27;
  then j in Seg len Incr Y_axis f by FINSEQ_1:def 3;
  then
A6: j in Seg width GoB(Incr X_axis f,Incr Y_axis f) by GOBOARD2:def 1;
  len Incr X_axis f = len GoB f by A5,GOBOARD2:def 1;
  then i in dom Incr X_axis f by A1,A2,FINSEQ_3:27;
  then (Incr X_axis f).i in rng Incr X_axis f by FUNCT_1:def 5;
  then (Incr X_axis f).i in rng X_axis f by SEQ_4:def 25;
  then consider k being Nat such that
A7: k in dom X_axis f and
A8: (X_axis f).k = (Incr X_axis f).i by FINSEQ_2:11;
  reconsider k as Element of NAT by ORDINAL1:def 13;
  take k;
  len X_axis f = len f by GOBOARD1:def 3;
  hence k in dom f by A7,FINSEQ_3:31;
  i in dom GoB f by A1,A2,FINSEQ_3:27;
  then [i,j] in [:dom GoB f, Seg width GoB f:] by A5,A6,ZFMISC_1:106;
  hence [i,j] in Indices GoB f by MATRIX_1:def 5;
  then
A9: (GoB f)*(i,j) = |[Incr(X_axis f).i,Incr(Y_axis f).j]| by A5,GOBOARD2:def 1;
  thus (f/.k)`1 = Incr(X_axis f).i by A7,A8,GOBOARD1:def 3
    .= (GoB f)*(i,j)`1 by A9,EUCLID:56;
end;

theorem Th12:
  1 <= i & i <= len GoB f & 1 <= j & j <= width GoB f implies ex k
  st k in dom f & [i,j] in Indices GoB f & (f/.k)`2 = (GoB f)*(i,j)`2
proof
  assume that
A1: 1 <= i and
A2: i <= len GoB f and
A3: 1 <= j and
A4: j <= width GoB f;
A5: GoB f = GoB (Incr X_axis f,Incr Y_axis f) by GOBOARD2:def 3;
  then len Incr Y_axis f = width GoB f by GOBOARD2:def 1;
  then j in dom Incr Y_axis f by A3,A4,FINSEQ_3:27;
  then j in Seg len Incr Y_axis f by FINSEQ_1:def 3;
  then
A6: j in Seg width GoB(Incr X_axis f,Incr Y_axis f) by GOBOARD2:def 1;
  len Incr Y_axis f = width GoB f by A5,GOBOARD2:def 1;
  then j in dom Incr Y_axis f by A3,A4,FINSEQ_3:27;
  then (Incr Y_axis f).j in rng Incr Y_axis f by FUNCT_1:def 5;
  then (Incr Y_axis f).j in rng Y_axis f by SEQ_4:def 25;
  then consider k being Nat such that
A7: k in dom Y_axis f and
A8: (Y_axis f).k = (Incr Y_axis f).j by FINSEQ_2:11;
  reconsider k as Element of NAT by ORDINAL1:def 13;
  take k;
  len Y_axis f = len f by GOBOARD1:def 4;
  hence k in dom f by A7,FINSEQ_3:31;
  i in dom GoB f by A1,A2,FINSEQ_3:27;
  then [i,j] in [:dom GoB f, Seg width GoB f:] by A5,A6,ZFMISC_1:106;
  hence [i,j] in Indices GoB f by MATRIX_1:def 5;
  then
A9: (GoB f)*(i,j) = |[Incr(X_axis f).i,Incr(Y_axis f).j]| by A5,GOBOARD2:def 1;
  thus (f/.k)`2 = Incr(Y_axis f).j by A7,A8,GOBOARD1:def 4
    .= (GoB f)*(i,j)`2 by A9,EUCLID:56;
end;

begin :: Extrema of projections

theorem Th13:
  1 <= i & i <= len h implies S-bound L~h <= (h/.i)`2 & (h/.i)`2 <= N-bound L~h
proof
A1: len h > 4 by GOBOARD7:36;
  assume that
A2: 1<=i and
A3: i<=len h;
  i in dom h by A2,A3,FINSEQ_3:27;
  then h/.i in L~h by A1,GOBOARD1:16,XXREAL_0:2;
  hence thesis by PSCOMP_1:71;
end;

theorem Th14:
  1 <= i & i <= len h implies W-bound L~h <= (h/.i)`1 & (h/.i)`1 <= E-bound L~h
proof
A1: len h > 4 by GOBOARD7:36;
  assume that
A2: 1<=i and
A3: i<=len h;
  i in dom h by A2,A3,FINSEQ_3:27;
  then h/.i in L~h by A1,GOBOARD1:16,XXREAL_0:2;
  hence thesis by PSCOMP_1:71;
end;

theorem Th15:
  for X being Subset of REAL st X = { q`2 : q`1 = W-bound L~h & q
in L~h } holds X = (proj2 | W-most L~h).:the carrier of (TOP-REAL 2)|(W-most L~
  h)
proof
  set T = (TOP-REAL 2)|(W-most L~h);
  set F = proj2 | W-most L~h;
  let X be Subset of REAL such that
A1: X = { q`2 : q`1 = W-bound L~h & q in L~h };
  thus X c= (proj2 | W-most L~h).:the carrier of T
  proof
    let x be set;
A2: dom F = the carrier of T by FUNCT_2:def 1
      .= [#] ((TOP-REAL 2)|(W-most L~h))
      .= W-most L~h by PRE_TOPC:def 10;
    assume x in X;
    then consider q1 being Point of TOP-REAL 2 such that
A3: q1`2 = x and
A4: q1`1 = W-bound L~h and
A5: q1 in L~h by A1;
A6: x = F.q1 by A3,A4,A5,PSCOMP_1:70,SPRECT_2:16;
A7: q1 in W-most L~h by A4,A5,SPRECT_2:16;
    then q1 in the carrier of T by A2,FUNCT_2:def 1;
    hence thesis by A2,A7,A6,FUNCT_1:def 12;
  end;
  thus (proj2 | W-most L~h).:the carrier of T c= X
  proof
    let x be set;
A8: W-most L~h c= L~h by XBOOLE_1:17;
    assume x in (proj2 | W-most L~h).:the carrier of T;
    then consider x1 be set such that
    x1 in dom F and
A9: x1 in the carrier of T and
A10: x = F.x1 by FUNCT_1:def 12;
    x1 in [#] ((TOP-REAL 2)|(W-most L~h)) by A9;
    then
