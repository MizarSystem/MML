Lm10: for i1, i2 be Element of NAT st 1 <= i1 & i1+1<=len h & 1<=i2 & i2+1<=
len h & h.i1=h.i2 holds i1 = i2
proof
  let i1, i2 be Element of NAT;
  assume that
A1: 1 <= i1 and
A2: i1+1 <= len h;
A3: i1 < len h by A2,NAT_1:13;
  assume that
A4: 1 <= i2 and
A5: i2+1 <= len h and
A6: h.i1 = h.i2;
A7: i2 < len h by A5,NAT_1:13;
  then
A8: i2 in dom h by A4,FINSEQ_3:27;
  assume
A9: i1 <> i2;
A10: now
    per cases by A9,XXREAL_0:1;
    suppose
      i1 < i2;
      hence h/.i1 <> h/.i2 by A1,A7,GOBOARD7:38;
    end;
    suppose
      i2 < i1;
      hence h/.i1 <> h/.i2 by A4,A3,GOBOARD7:38;
    end;
  end;
  i1 in dom h by A1,A3,FINSEQ_3:27;
  then h/.i1 = h.i2 by A6,PARTFUN1:def 8
    .= h/.i2 by A8,PARTFUN1:def 8;
  hence thesis by A10;
end;

