theorem Th33:
  p in L~h & 1 <= I & I <= width GoB h implies (GoB h)*(1,I)`1 <= p`1
proof
  assume that
A1: p in L~h and
A2: 1 <= I and
A3: I <= width GoB h;
  consider i such that
A4: 1<=i and
A5: i+1<=len h and
A6: p in LSeg(h/.i,h/.(i+1)) by A1,SPPOL_2:14;
  i<=i+1 by NAT_1:11;
  then i<=len h by A5,XXREAL_0:2;
  then
A7: (GoB h)*(1,I)`1<=(h/.i)`1 by A2,A3,A4,Th7;
  1<=i+1 by NAT_1:11;
  then
A8: (GoB h)*(1,I)`1<=(h/.(i+1))`1 by A2,A3,A5,Th7;
  now
    per cases;
    case
      (h/.i)`1<=(h/.(i+1))`1;
      then (h/.i)`1<=p`1 by A6,TOPREAL1:9;
      hence thesis by A7,XXREAL_0:2;
    end;
    case
      (h/.i)`1>(h/.(i+1))`1;
      then (h/.(i+1))`1<=p`1 by A6,TOPREAL1:9;
      hence thesis by A8,XXREAL_0:2;
    end;
  end;
  hence thesis;
end;

