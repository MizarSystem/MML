  then reconsider X as non empty Subset of REAL by A2;
  upper_bound X = A
  proof
A3: for p be real number st p in X holds p <= A
    proof
      let p be real number;
      assume p in X;
      then
A4:   ex s be Point of TOP-REAL 2 st p = s`1 & s in L~h;
      1 <= width GoB h by GOBOARD7:35;
