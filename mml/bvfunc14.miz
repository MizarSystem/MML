  proof
    let t be set;
    assume t in rng h;
    then consider x1 being set such that
A9: x1 in dom h and
A10: t = h.x1 by FUNCT_1:def 5;
    now
      per cases by A1,A9,ENUMSET1:def 7;
      case
        x1=A;
        hence thesis by A10,ENUMSET1:def 7;
      end;
      case
        x1=B;
        hence thesis by A10,ENUMSET1:def 7;
      end;
      case
        x1=C;
        hence thesis by A10,ENUMSET1:def 7;
      end;
      case
        x1=D;
        hence thesis by A10,ENUMSET1:def 7;
      end;
      case
        x1=E;
        hence thesis by A10,ENUMSET1:def 7;
      end;
      case
        x1=F;
        hence thesis by A10,ENUMSET1:def 7;
      end;
      case
        x1=J;
        hence thesis by A10,ENUMSET1:def 7;
      end;
      case
        x1=M;
        hence thesis by A10,ENUMSET1:def 7;
      end;
      case
        x1=N;
        hence thesis by A10,ENUMSET1:def 7;
      end;
    end;
    hence thesis;
  end;
A11: F in dom h by A1,ENUMSET1:def 7;
A12: E in dom h by A1,ENUMSET1:def 7;
A13: A in dom h by A1,ENUMSET1:def 7;
  {h.A,h.B,h.C,h.D,h.E,h.F,h.J,h.M,h.N} c= rng h
  proof
    let t be set;
    assume
A14: t in {h.A,h.B,h.C,h.D,h.E,h.F,h.J,h.M,h.N};
    now
      per cases by A14,ENUMSET1:def 7;
      case
        t=h.A;
        hence thesis by A13,FUNCT_1:def 5;
      end;
      case
        t=h.B;
        hence thesis by A2,FUNCT_1:def 5;
      end;
      case
        t=h.C;
        hence thesis by A7,FUNCT_1:def 5;
      end;
      case
        t=h.D;
        hence thesis by A6,FUNCT_1:def 5;
      end;
      case
        t=h.E;
        hence thesis by A12,FUNCT_1:def 5;
      end;
      case
        t=h.F;
        hence thesis by A11,FUNCT_1:def 5;
      end;
      case
        t=h.J;
        hence thesis by A4,FUNCT_1:def 5;
      end;
      case
        t=h.M;
        hence thesis by A3,FUNCT_1:def 5;
      end;
      case
        t=h.N;
        hence thesis by A5,FUNCT_1:def 5;
      end;
    end;
    hence thesis;
  end;
  hence thesis by A8,XBOOLE_0:def 10;
