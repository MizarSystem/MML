theorem Th18:
  for h being Function, A9,B9,C9,D9 being set st A<>B & A<>C & A<>
  D & B<>C & B<>D & C<>D & h = (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (A
  .--> A9) holds h.B = B9 & h.C = C9 & h.D = D9
proof
  let h be Function;
  let A9,B9,C9,D9 be set;
  assume that
A1: A<>B and
A2: A<>C and
A3: A<>D and
A4: B<>C and
A5: B<>D and
A6: C<>D and
A7: h = (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (A .--> A9);
A8: dom (A .--> A9) = {A} by FUNCOP_1:19;
  then not D in dom (A .--> A9) by A3,TARSKI:def 1;
  then
A9: h.D=((B .--> B9) +* (C .--> C9) +* (D .--> D9)).D by A7,
  FUNCT_4:12;
  not C in dom (A .--> A9) by A2,A8,TARSKI:def 1;
  then
A10: h.C=((B .--> B9) +* (C .--> C9) +* (D .--> D9)).C by A7,FUNCT_4:12;
A11: dom (D .--> D9) = {D} by FUNCOP_1:19;
  then not C in dom (D .--> D9) by A6,TARSKI:def 1;
  then
A12: h.C=((B .--> B9) +* (C .--> C9)).C by A10,FUNCT_4:12;
  not B in dom (A .--> A9) by A1,A8,TARSKI:def 1;
  then
A13: h.B=((B .--> B9) +* (C .--> C9) +* (D .--> D9)).B by A7,FUNCT_4:12;
  not B in dom (D .--> D9) by A5,A11,TARSKI:def 1;
  then
A14: h.B=((B .--> B9) +* (C .--> C9)).B by A13,FUNCT_4:12;
A15: dom (C .--> C9) = {C} by FUNCOP_1:19;
  then not B in dom (C .--> C9) by A4,TARSKI:def 1;
  then h.B=(B .--> B9).B by A14,FUNCT_4:12;
  hence h.B = B9 by FUNCOP_1:87;
  C in dom (C .--> C9) by A15,TARSKI:def 1;
  then h.C=(C .--> C9).C by A12,FUNCT_4:14;
  hence h.C = C9 by FUNCOP_1:87;
  D in dom (D .--> D9) by A11,TARSKI:def 1;
  then h.D=(D .--> D9).D by A9,FUNCT_4:14;
  hence thesis by FUNCOP_1:87;
end;

theorem Th19:
  for A,B,C,D being set,h being Function, A9,B9,C9,D9 being set st
h = (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (A .--> A9) holds dom h = {A,B
  ,C,D}
proof
  let A,B,C,D be set;
  let h be Function;
  let A9,B9,C9,D9 be set;
  assume
A1: h = (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (A .--> A9);
  dom ((B .--> B9) +* (C .--> C9)) = dom (B .--> B9) \/ dom (C .--> C9) by
  FUNCT_4:def 1;
  then
A2: dom ((B .--> B9) +* (C .--> C9) +* (D .--> D9)) = dom (B .--> B9) \/ dom
  (C .--> C9) \/ dom (D .--> D9) by FUNCT_4:def 1;
  dom (B .--> B9) = {B} by FUNCOP_1:19;
  then
  dom ((B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (A .--> A9)) = {B} \/
  dom (C .--> C9) \/ dom (D .--> D9) \/ dom (A .--> A9) by A2,FUNCT_4:def 1
    .= {B} \/ {C} \/ dom (D .--> D9) \/ dom (A .--> A9) by FUNCOP_1:19
    .= {B} \/ {C} \/ {D} \/ dom (A .--> A9) by FUNCOP_1:19
    .= {A} \/ (({B} \/ {C}) \/ {D}) by FUNCOP_1:19
    .= {A} \/ ({B,C} \/ {D}) by ENUMSET1:41
    .= {A} \/ {B,C,D} by ENUMSET1:43
    .= {A,B,C,D} by ENUMSET1:44;
  hence thesis by A1;
end;

theorem Th20:
  for h being Function,A9,B9,C9,D9 being set st G={A,B,C,D} & h =
(B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (A .--> A9) holds rng h = {h.A,h.B
  ,h.C,h.D}
proof
  let h be Function;
  let A9,B9,C9,D9 be set;
  assume that
A1: G={A,B,C,D} and
A2: h = (B .--> B9)+*(C .--> C9)+*(D .--> D9)+*(A .--> A9);
A3: dom h = G by A1,A2,Th19;
  then
A4: B in dom h by A1,ENUMSET1:def 2;
A5: rng h c= {h.A,h.B,h.C,h.D}
  proof
    let t be set;
    assume t in rng h;
    then consider x1 being set such that
A6: x1 in dom h and
A7: t = h.x1 by FUNCT_1:def 5;
    now
      per cases by A1,A3,A6,ENUMSET1:def 2;
      case
        x1=A;
        hence thesis by A7,ENUMSET1:def 2;
      end;
      case
        x1=B;
        hence thesis by A7,ENUMSET1:def 2;
      end;
      case
        x1=C;
        hence thesis by A7,ENUMSET1:def 2;
      end;
      case
        x1=D;
        hence thesis by A7,ENUMSET1:def 2;
      end;
    end;
    hence thesis;
  end;
A8: D in dom h by A1,A3,ENUMSET1:def 2;
A9: C in dom h by A1,A3,ENUMSET1:def 2;
A10: A in dom h by A1,A3,ENUMSET1:def 2;
  {h.A,h.B,h.C,h.D} c= rng h
  proof
    let t be set;
    assume
A11: t in {h.A,h.B,h.C,h.D};
    per cases by A11,ENUMSET1:def 2;
    suppose
      t=h.A;
      hence thesis by A10,FUNCT_1:def 5;
    end;
    suppose
      t=h.B;
      hence thesis by A4,FUNCT_1:def 5;
    end;
    suppose
      t=h.C;
      hence thesis by A9,FUNCT_1:def 5;
    end;
    suppose
      t=h.D;
      hence thesis by A8,FUNCT_1:def 5;
    end;
  end;
  hence thesis by A5,XBOOLE_0:def 10;
end;

theorem
  for z,u being Element of Y, h being Function st G is independent & G={
  A,B,C,D} & A<>B & A<>C & A<>D & B<>C & B<>D & C<>D holds EqClass(u,B '/\' C
  '/\' D) meets EqClass(z,A)
proof
  let z,u be Element of Y;
  let h be Function;
  assume that
A1: G is independent and
A2: G={A,B,C,D} and
A3: A<>B & A<>C & A<>D & B<>C & B<>D & C<>D;
  set h = (B .--> EqClass(u,B)) +* (C .--> EqClass(u,C)) +* (D .--> EqClass(u,
  D)) +* (A .--> EqClass(z,A));
A4: h.B = EqClass(u,B) by A3,Th18;
A5: h.D = EqClass(u,D) by A3,Th18;
A6: h.C = EqClass(u,C) by A3,Th18;
A7: rng h = {h.A,h.B,h.C,h.D} by A2,Th20;
  rng h c= bool Y
  proof
    let t be set;
    assume
A8: t in rng h;
    per cases by A7,A8,ENUMSET1:def 2;
    suppose
      t=h.A;
      then t=EqClass(z,A) by FUNCT_7:96;
      hence thesis;
    end;
    suppose
      t=h.B;
      hence thesis by A4;
    end;
    suppose
      t=h.C;
      hence thesis by A6;
    end;
    suppose
      t=h.D;
      hence thesis by A5;
    end;
  end;
  then reconsider FF=rng h as Subset-Family of Y;
A9: dom h = G by A2,Th19;
  for d being set st d in G holds h.d in d
  proof
    let d be set;
    assume
A10: d in G;
    per cases by A2,A10,ENUMSET1:def 2;
    suppose
A11:  d=A;
      h.A=EqClass(z,A) by FUNCT_7:96;
      hence thesis by A11;
    end;
    suppose
A12:  d=B;
      h.B=EqClass(u,B) by A3,Th18;
      hence thesis by A12;
    end;
    suppose
A13:  d=C;
      h.C=EqClass(u,C) by A3,Th18;
      hence thesis by A13;
    end;
    suppose
A14:  d=D;
      h.D=EqClass(u,D) by A3,Th18;
      hence thesis by A14;
    end;
  end;
  then Intersect FF <>{} by A1,A9,BVFUNC_2:def 5;
  then consider m being set such that
A15: m in Intersect FF by XBOOLE_0:def 1;
  A in dom h by A2,A9,ENUMSET1:def 2;
  then
A16: h.A in rng h by FUNCT_1:def 5;
  then
A17: m in meet FF by A15,SETFAM_1:def 10;
  D in dom h by A2,A9,ENUMSET1:def 2;
  then h.D in rng h by FUNCT_1:def 5;
  then
A18: m in h.D by A17,SETFAM_1:def 1;
  C in dom h by A2,A9,ENUMSET1:def 2;
  then h.C in rng h by FUNCT_1:def 5;
  then
A19: m in h.C by A17,SETFAM_1:def 1;
  B in dom h by A2,A9,ENUMSET1:def 2;
  then h.B in rng h by FUNCT_1:def 5;
  then m in h.B by A17,SETFAM_1:def 1;
  then m in EqClass(u,B) /\ EqClass(u,C) by A4,A6,A19,XBOOLE_0:def 4;
  then
A20: m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) by A5,A18,
  XBOOLE_0:def 4;
  h.A = EqClass(z,A) & m in h.A by A16,A17,FUNCT_7:96,SETFAM_1:def 1;
  then m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) /\ EqClass(z,A) by
  A20,XBOOLE_0:def 4;
  then
A21: EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) meets EqClass(z,A) by
  XBOOLE_0:4;
  EqClass(u,B '/\' C '/\' D) = EqClass(u,B '/\' C) /\ EqClass(u,D) by Th1;
  hence thesis by A21,Th1;
end;

theorem
  for z,u being Element of Y st G is independent & G={A,B,C,D} & A<>B &
  A<>C & A<>D & B<>C & B<>D & C<>D & EqClass(z,C '/\' D)=EqClass(u,C '/\' D)
  holds EqClass(u,CompF(A,G)) meets EqClass(z,CompF(B,G))
proof
  let z,u be Element of Y;
  assume that
A1: G is independent and
A2: G={A,B,C,D} and
A3: A<>B and
A4: A<>C & A<>D and
A5: B<>C & B<>D and
A6: C<>D and
A7: EqClass(z,C '/\' D)=EqClass(u,C '/\' D);
  set h = (B .--> EqClass(u,B)) +* (C .--> EqClass(u,C)) +* (D .--> EqClass(u,
  D)) +* (A .--> EqClass(z,A));
  set H=EqClass(z,CompF(B,G));
A8: A '/\' (C '/\' D) = A '/\' C '/\' D by PARTIT1:16;
A9: rng h = {h.A,h.B,h.C,h.D} by A2,Th20;
  rng h c= bool Y
  proof
    let t be set;
    assume
A10: t in rng h;
    per cases by A9,A10,ENUMSET1:def 2;
    suppose
      t=h.A;
      then t=EqClass(z,A) by FUNCT_7:96;
      hence thesis;
    end;
    suppose
      t=h.B;
      then t=EqClass(u,B) by A3,A4,A5,A6,Th18;
      hence thesis;
    end;
    suppose
      t=h.C;
      then t=EqClass(u,C) by A3,A4,A5,A6,Th18;
      hence thesis;
    end;
    suppose
      t=h.D;
      then t=EqClass(u,D) by A3,A4,A5,A6,Th18;
      hence thesis;
    end;
  end;
  then reconsider FF=rng h as Subset-Family of Y;
  set I=EqClass(z,A), GG=EqClass(u,B '/\' C '/\' D);
A11: GG = EqClass(u,B '/\' C) /\ EqClass(u,D) by Th1;
A12: for d being set st d in G holds h.d in d
  proof
    let d be set;
    assume
A13: d in G;
    per cases by A2,A13,ENUMSET1:def 2;
    suppose
A14:  d=A;
      h.A=EqClass(z,A) by FUNCT_7:96;
      hence thesis by A14;
    end;
    suppose
A15:  d=B;
      h.B=EqClass(u,B) by A3,A4,A5,A6,Th18;
      hence thesis by A15;
    end;
    suppose
A16:  d=C;
      h.C=EqClass(u,C) by A3,A4,A5,A6,Th18;
      hence thesis by A16;
    end;
    suppose
A17:  d=D;
      h.D=EqClass(u,D) by A3,A4,A5,A6,Th18;
      hence thesis by A17;
    end;
  end;
  dom h=G by A2,Th19;
  then (Intersect FF)<>{} by A1,A12,BVFUNC_2:def 5;
  then consider m being set such that
A18: m in Intersect FF by XBOOLE_0:def 1;
A19: dom h = G by A2,Th19;
  then A in dom h by A2,ENUMSET1:def 2;
  then
A20: h.A in rng h by FUNCT_1:def 5;
  then
A21: m in meet FF by A18,SETFAM_1:def 10;
  then
A22: h.A = EqClass(z,A) & m in h.A by A20,FUNCT_7:96,SETFAM_1:def 1;
  D in dom h by A2,A19,ENUMSET1:def 2;
  then h.D in rng h by FUNCT_1:def 5;
  then
A23: m in h.D by A21,SETFAM_1:def 1;
  C in dom h by A2,A19,ENUMSET1:def 2;
  then h.C in rng h by FUNCT_1:def 5;
  then
A24: m in h.C by A21,SETFAM_1:def 1;
  B in dom h by A2,A19,ENUMSET1:def 2;
  then h.B in rng h by FUNCT_1:def 5;
  then
A25: m in h.B by A21,SETFAM_1:def 1;
  h.B = EqClass(u,B) & h.C = EqClass(u,C) by A3,A4,A5,A6,Th18;
  then
A26: m in EqClass(u,B) /\ EqClass(u,C) by A25,A24,XBOOLE_0:def 4;
  h.D = EqClass(u,D) by A3,A4,A5,A6,Th18;
  then m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) by A23,A26,
  XBOOLE_0:def 4;
  then m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) /\ EqClass(z,A) by
  A22,XBOOLE_0:def 4;
  then GG /\ I <> {} by A11,Th1;
  then consider p being set such that
A27: p in GG /\ I by XBOOLE_0:def 1;
  reconsider p as Element of Y by A27;
  set K=EqClass(p,C '/\' D);
A28: p in GG by A27,XBOOLE_0:def 4;
  set L=EqClass(z,C '/\' D);
A29: z in I by EQREL_1:def 8;
  GG = EqClass(u,B '/\' (C '/\' D)) by PARTIT1:16;
  then
A30: GG c= EqClass(u,C '/\' D) by BVFUNC11:3;
  p in EqClass(p,C '/\' D) by EQREL_1:def 8;
  then K meets L by A7,A30,A28,XBOOLE_0:3;
  then K=L by EQREL_1:50;
  then z in K by EQREL_1:def 8;
  then
A31: z in I /\ K by A29,XBOOLE_0:def 4;
A32: p in K & p in I by A27,EQREL_1:def 8,XBOOLE_0:def 4;
  then p in I /\ K by XBOOLE_0:def 4;
  then I /\ K in INTERSECTION(A,C '/\' D) & not I /\ K in {{}} by
  SETFAM_1:def 5,TARSKI:def 1;
  then
A33: I /\ K in INTERSECTION(A,C '/\' D) \ {{}} by XBOOLE_0:def 5;
  CompF(B,G) = A '/\' C '/\' D by A2,A3,A5,Th8;
  then I /\ K in CompF(B,G) by A33,A8,PARTIT1:def 4;
  then
A34: I /\ K = H or I /\ K misses H by EQREL_1:def 6;
  z in H by EQREL_1:def 8;
  then p in H by A32,A31,A34,XBOOLE_0:3,def 4;
  then p in GG /\ H by A28,XBOOLE_0:def 4;
  then GG meets H by XBOOLE_0:4;
  hence thesis by A2,A3,A4,Th7;
end;

theorem
  for z,u being Element of Y st G is independent & G={A,B,C} & A<>B & B
  <>C & C<>A & EqClass(z,C)=EqClass(u,C) holds EqClass(u,CompF(A,G)) meets
  EqClass(z,CompF(B,G))
proof
  let z,u be Element of Y;
  assume that
A1: G is independent and
A2: G={A,B,C} and
A3: A<>B and
A4: B<>C and
A5: C<>A and
A6: EqClass(z,C)=EqClass(u,C);
  set h = (B .--> EqClass(u,B)) +* (C .--> EqClass(u,C)) +* (A .--> EqClass(z,
  A));
A7: dom (A .--> EqClass(z,A)) = {A} by FUNCOP_1:19;
  then A in dom (A .--> EqClass(z,A)) by TARSKI:def 1;
  then h.A = (A .--> EqClass(z,A)).A by FUNCT_4:14;
  then
A8: h.A = EqClass(z,A) by FUNCOP_1:87;
  set H=EqClass(z,CompF(B,G)), I=EqClass(z,A), GG=EqClass(u,B '/\' C);
A9: GG /\ I = EqClass(u,B) /\ EqClass(u,C) /\ EqClass(z,A) by Th1;
  dom ((B .--> EqClass(u,B)) +* (C .--> EqClass(u,C))) = dom (B .-->
  EqClass(u,B)) \/ dom (C .--> EqClass(u,C)) by FUNCT_4:def 1;
  then
A10: dom ((B .--> EqClass(u,B)) +* (C .--> EqClass(u,C)) +* (A .--> EqClass(
z,A))) = dom (B .--> EqClass(u,B)) \/ dom (C .--> EqClass(u,C)) \/ dom (A .-->
  EqClass(z,A)) by FUNCT_4:def 1;
A11: dom (C .--> EqClass(u,C)) = {C} by FUNCOP_1:19;
  then
A12: C in dom (C .--> EqClass(u,C)) by TARSKI:def 1;
  not B in dom (A .--> EqClass(z,A)) by A3,A7,TARSKI:def 1;
  then
A13: h.B=((B .--> EqClass(u,B)) +* (C .--> EqClass(u,C))).B by
  FUNCT_4:
  12;
  not B in dom (C .--> EqClass(u,C)) by A4,A11,TARSKI:def 1;
  then h.B=(B .--> EqClass(u,B)).B by A13,FUNCT_4:12;
  then
A14: h.B = EqClass(u,B) by FUNCOP_1:87;
  not C in dom (A .--> EqClass(z,A)) by A5,A7,TARSKI:def 1;
  then h.C=((B .--> EqClass(u,B)) +* (C .--> EqClass(u,C))).C by
  FUNCT_4:
  12;
  then h.C=(C .--> EqClass(u,C)).C by A12,FUNCT_4:14;
  then
A15: h.C = EqClass(u,C) by FUNCOP_1:87;
  dom (B .--> EqClass(u,B)) = {B} by FUNCOP_1:19;
  then
A16: dom ((B .--> EqClass(u,B)) +* (C .--> EqClass(u,C)) +* (A .--> EqClass(
  z,A))) = {A} \/ {B} \/ {C} by A10,A11,A7,XBOOLE_1:4
    .= {A,B} \/ {C} by ENUMSET1:41
    .= {A,B,C} by ENUMSET1:43;
A17: rng h c= {h.A,h.B,h.C}
  proof
    let t be set;
    assume t in rng h;
    then consider x1 being set such that
A18: x1 in dom h and
A19: t = h.x1 by FUNCT_1:def 5;
    now
      per cases by A16,A18,ENUMSET1:def 1;
      case
        x1=A;
        hence thesis by A19,ENUMSET1:def 1;
      end;
      case
        x1=B;
        hence thesis by A19,ENUMSET1:def 1;
      end;
      case
        x1=C;
        hence thesis by A19,ENUMSET1:def 1;
      end;
    end;
    hence thesis;
  end;
  rng h c= bool Y
  proof
    let t be set;
    assume
A20: t in rng h;
    now
      per cases by A17,A20,ENUMSET1:def 1;
      case
        t=h.A;
        hence thesis by A8;
      end;
      case
        t=h.B;
        hence thesis by A14;
      end;
      case
        t=h.C;
        hence thesis by A15;
      end;
    end;
    hence thesis;
  end;
  then reconsider FF=rng h as Subset-Family of Y;
A21: z in H by EQREL_1:def 8;
  for d being set st d in G holds h.d in d
  proof
    let d be set;
    assume
A22: d in G;
    now
      per cases by A2,A22,ENUMSET1:def 1;
      case
        d=A;
        hence thesis by A8;
      end;
      case
        d=B;
        hence thesis by A14;
      end;
      case
        d=C;
        hence thesis by A15;
      end;
    end;
    hence thesis;
  end;
  then (Intersect FF)<>{} by A1,A2,A16,BVFUNC_2:def 5;
  then consider m being set such that
A23: m in Intersect FF by XBOOLE_0:def 1;
  A in dom h by A16,ENUMSET1:def 1;
  then
A24: h.A in rng h by FUNCT_1:def 5;
  then
A25: Intersect FF = meet (rng h) by SETFAM_1:def 10;
  C in dom h by A16,ENUMSET1:def 1;
  then h.C in rng h by FUNCT_1:def 5;
  then
A26: m in h.C by A25,A23,SETFAM_1:def 1;
  B in dom h by A16,ENUMSET1:def 1;
  then h.B in rng h by FUNCT_1:def 5;
  then m in h.B by A25,A23,SETFAM_1:def 1;
  then
A27: m in EqClass(u,B) /\ EqClass(u,C) by A14,A15,A26,XBOOLE_0:def 4;
  m in h.A by A24,A25,A23,SETFAM_1:def 1;
  then GG /\ I <> {} by A8,A9,A27,XBOOLE_0:def 4;
  then consider p being set such that
A28: p in GG /\ I by XBOOLE_0:def 1;
  reconsider p as Element of Y by A28;
  set K=EqClass(p,C);
A29: I /\ K in INTERSECTION(A,C) by SETFAM_1:def 5;
  set L=EqClass(z,C);
A30: p in EqClass(p,C) by EQREL_1:def 8;
A31: p in GG by A28,XBOOLE_0:def 4;
  p in K & p in I by A28,EQREL_1:def 8,XBOOLE_0:def 4;
  then
A32: p in I /\ K by XBOOLE_0:def 4;
  then not I /\ K in {{}} by TARSKI:def 1;
  then I /\ K in INTERSECTION(A,C) \ {{}} by A29,XBOOLE_0:def 5;
  then
A33: I /\ K in A '/\' C by PARTIT1:def 4;
  GG c= L by A6,BVFUNC11:3;
  then K meets L by A31,A30,XBOOLE_0:3;
  then K=L by EQREL_1:50;
  then
A34: z in K by EQREL_1:def 8;
  z in I by EQREL_1:def 8;
  then
A35: z in I /\ K by A34,XBOOLE_0:def 4;
  CompF(B,G) = A '/\' C by A2,A3,A4,Th5;
  then
A36: I /\ K = H or I /\ K misses H by A33,EQREL_1:def 6;
  GG=EqClass(u,CompF(A,G)) by A2,A3,A5,Th4;
  hence thesis by A32,A31,A35,A21,A36,XBOOLE_0:3;
end;

theorem Th24:
  G={A,B,C,D,E} & A<>B & A<>C & A<>D & A<>E implies CompF(A,G) = B
  '/\' C '/\' D '/\' E
proof
  assume that
A1: G={A,B,C,D,E} and
A2: A<>B and
A3: A<>C and
A4: A<>D and
A5: A<>E;
  per cases;
  suppose
A6: B = C;
    then G = {A,B,B,D} \/ {E} by A1,ENUMSET1:50
      .= {B,B,A,D} \/ {E} by ENUMSET1:110
      .= {B,B,A,D,E} by ENUMSET1:50
      .= {B,A,D,E} by ENUMSET1:72
      .={A,B,D,E} by ENUMSET1:108;
    hence CompF(A,G) = B '/\' D '/\' E by A2,A4,A5,Th7
      .= B '/\' C '/\' D '/\' E by A6,PARTIT1:15;
  end;
  suppose
A7: B = D;
    then G = {A,B,C,B} \/ {E} by A1,ENUMSET1:50
      .= {B,B,A,C} \/ {E} by ENUMSET1:112
      .= {B,B,A,C,E} by ENUMSET1:50
      .= {B,A,C,E} by ENUMSET1:72
      .={A,B,C,E} by ENUMSET1:108;
    hence CompF(A,G) = B '/\' C '/\' E by A2,A3,A5,Th7
      .= B '/\' D '/\' C '/\' E by A7,PARTIT1:15
      .= B '/\' C '/\' D '/\' E by PARTIT1:16;
  end;
  suppose
A8: B = E;
    then G = {A} \/ {B,C,D,B} by A1,ENUMSET1:47
      .= {A} \/ {B,B,C,D} by ENUMSET1:105
      .= {A} \/ {B,C,D} by ENUMSET1:71
      .={A,B,C,D} by ENUMSET1:44;
    hence CompF(A,G) = B '/\' C '/\' D by A2,A3,A4,Th7
      .= B '/\' E '/\' C '/\' D by A8,PARTIT1:15
      .= B '/\' E '/\' (C '/\' D) by PARTIT1:16
      .= B '/\' (C '/\' D) '/\' E by PARTIT1:16
      .= B '/\' C '/\' D '/\' E by PARTIT1:16;
  end;
  suppose
A9: C = D;
    then G = {A,B,C,C} \/ {E} by A1,ENUMSET1:50
      .= {C,C,A,B} \/ {E} by ENUMSET1:118
      .= {C,A,B} \/ {E} by ENUMSET1:71
      .= {C,A,B,E} by ENUMSET1:46
      .={A,B,C,E} by ENUMSET1:110;
    hence CompF(A,G) = B '/\' C '/\' E by A2,A3,A5,Th7
      .= B '/\' (C '/\' D) '/\' E by A9,PARTIT1:15
      .= B '/\' C '/\' D '/\' E by PARTIT1:16;
  end;
  suppose
A10: C = E;
    then G = {A} \/ {B,C,D,C} by A1,ENUMSET1:47
      .= {A} \/ {C,C,B,D} by ENUMSET1:117
      .= {A} \/ {C,B,D} by ENUMSET1:71
      .= {A,C,B,D} by ENUMSET1:44
      .={A,B,C,D} by ENUMSET1:104;
    hence CompF(A,G) = B '/\' C '/\' D by A2,A3,A4,Th7
      .= B '/\' (C '/\' E) '/\' D by A10,PARTIT1:15
      .= B '/\' (C '/\' E '/\' D) by PARTIT1:16
      .= B '/\' (C '/\' D '/\' E) by PARTIT1:16
      .= B '/\' (C '/\' D) '/\' E by PARTIT1:16
      .= B '/\' C '/\' D '/\' E by PARTIT1:16;
  end;
  suppose
A11: D = E;
    then G = {A} \/ {B,C,D,D} by A1,ENUMSET1:47
      .= {A} \/ {D,D,B,C} by ENUMSET1:118
      .= {A} \/ {D,B,C} by ENUMSET1:71
      .= {A,D,B,C} by ENUMSET1:44
      .={A,B,C,D} by ENUMSET1:105;
    hence CompF(A,G) = B '/\' C '/\' D by A2,A3,A4,Th7
      .= B '/\' C '/\' (D '/\' E) by A11,PARTIT1:15
      .= B '/\' (C '/\' (D '/\' E)) by PARTIT1:16
      .= B '/\' (C '/\' D '/\' E) by PARTIT1:16
      .= B '/\' (C '/\' D) '/\' E by PARTIT1:16
      .= B '/\' C '/\' D '/\' E by PARTIT1:16;
  end;
  suppose
A12: B<>C & B<>D & B<>E & C<>D & C<>E & D<>E;
A13: ( not D in {A})& not E in {A} by A4,A5,TARSKI:def 1;
A14: not B in {A} by A2,TARSKI:def 1;
    G \ {A}={A} \/ {B,C,D,E} \ {A} by A1,ENUMSET1:47;
    then
A15: G \ {A} = ({A} \ {A}) \/ ({B,C,D,E} \ {A}) by XBOOLE_1:42;
A16: not C in {A} by A3,TARSKI:def 1;
    A in {A} by TARSKI:def 1;
    then
A17: {A} \ {A}={} by ZFMISC_1:68;
    {B,C,D,E} \ {A} = ({B} \/ {C,D,E}) \ {A} by ENUMSET1:44
      .= ({B} \ {A}) \/ ({C,D,E} \ {A}) by XBOOLE_1:42
      .= {B} \/ ({C,D,E} \ {A}) by A14,ZFMISC_1:67
      .= {B} \/ (({C} \/ {D,E}) \ {A}) by ENUMSET1:42
      .= {B} \/ (({C} \ {A}) \/ ({D,E} \ {A})) by XBOOLE_1:42
      .= {B} \/ (({C} \ {A}) \/ {D,E}) by A13,ZFMISC_1:72
      .= {B} \/ ({C} \/ {D,E}) by A16,ZFMISC_1:67
      .= {B} \/ {C,D,E} by ENUMSET1:42;
    then
A18: G \ {A} = ({A} \ {A}) \/ {B,C,D,E} by A15,ENUMSET1:44;
A19: B '/\' C '/\' D '/\' E c= '/\' (G \ {A})
    proof
      let x be set;
      assume
A20:  x in B '/\' C '/\' D '/\' E;
      then
A21:  x<>{} by EQREL_1:def 6;
      x in INTERSECTION(B '/\' C '/\' D,E) \ {{}} by A20,PARTIT1:def 4;
      then consider bcd,e being set such that
A22:  bcd in B '/\' C '/\' D and
A23:  e in E and
A24:  x = bcd /\ e by SETFAM_1:def 5;
      bcd in INTERSECTION(B '/\' C,D) \ {{}} by A22,PARTIT1:def 4;
      then consider bc,d being set such that
A25:  bc in B '/\' C and
A26:  d in D and
A27:  bcd = bc /\ d by SETFAM_1:def 5;
      bc in INTERSECTION(B,C) \ {{}} by A25,PARTIT1:def 4;
      then consider b,c being set such that
A28:  b in B and
A29:  c in C and
A30:  bc = b /\ c by SETFAM_1:def 5;
      set h = (B .--> b) +* (C .--> c) +* (D .--> d) +* (E .--> e);
A31:  dom (C .--> c) = {C} by FUNCOP_1:19;
      then
A32:  C in dom (C .--> c) by TARSKI:def 1;
A33:  dom (D .--> d) = {D} by FUNCOP_1:19;
      then
A34:  D in dom (D .--> d) by TARSKI:def 1;
A35:  not C in dom (D .--> d) by A12,A33,TARSKI:def 1;
A36:  dom (E .--> e) = {E} by FUNCOP_1:19;
      then E in dom (E .--> e) by TARSKI:def 1;
      then
A37:  h.E = (E .--> e).E by FUNCT_4:14;
      then
A38:  h.E = e by FUNCOP_1:87;
      not C in dom (E .--> e) by A12,A36,TARSKI:def 1;
      then h.C=((B .--> b) +* (C .--> c) +* (D .--> d)).C by FUNCT_4:12;
      then h.C=((B .--> b) +* (C .--> c)).C by A35,FUNCT_4:12;
      then
A39:  h.C=(C .--> c).C by A32,FUNCT_4:14;
      then
A40:  h.C = c by FUNCOP_1:87;
      not D in dom (E .--> e) by A12,A36,TARSKI:def 1;
      then h.D=((B .--> b) +* (C .--> c) +* (D .--> d)).D by FUNCT_4:12;
      then
A41:  h.D = (D .--> d).D by A34,FUNCT_4:14;
      then
A42:  h.D = d by FUNCOP_1:87;
A43:  not B in dom (C .--> c) by A12,A31,TARSKI:def 1;
A44:  not B in dom (D .--> d) by A12,A33,TARSKI:def 1;
      not B in dom (E .--> e) by A12,A36,TARSKI:def 1;
      then h.B=((B .--> b) +* (C .--> c) +* (D .--> d)).B by FUNCT_4:12;
      then h.B=((B .--> b) +* (C .--> c)).B by A44,FUNCT_4:12;
      then
A45:  h.B=(B .--> b).B by A43,FUNCT_4:12;
      then
A46:  h.B = b by FUNCOP_1:87;
A47:  for p being set st p in (G \ {A}) holds h.p in p
      proof
        let p be set;
        assume
A48:    p in (G \ {A});
        now
          per cases by A15,A17,A48,ENUMSET1:def 2;
          case
            p=D;
            hence thesis by A26,A41,FUNCOP_1:87;
          end;
          case
            p=B;
            hence thesis by A28,A45,FUNCOP_1:87;
          end;
          case
            p=C;
            hence thesis by A29,A39,FUNCOP_1:87;
          end;
          case
            p=E;
            hence thesis by A23,A37,FUNCOP_1:87;
          end;
        end;
        hence thesis;
      end;
      dom ((B .--> b) +* (C .--> c)) = dom (B .--> b) \/ dom (C .--> c)
      by FUNCT_4:def 1;
      then dom ((B .--> b) +* (C .--> c) +* (D .--> d)) = dom (B .--> b) \/
      dom (C .--> c) \/ dom (D .--> d) by FUNCT_4:def 1;
      then
A49:  dom ((B .--> b) +* (C .--> c) +* (D .--> d) +* (E .--> e)) = dom (B
      .--> b) \/ dom (C .--> c) \/ dom (D .--> d) \/ dom (E .--> e) by FUNCT_4:
      def 1;
      dom (B .--> b) = {B} by FUNCOP_1:19;
      then
A50:  dom ((B .--> b) +* (C .--> c) +* (D .--> d) +* (E .--> e)) = {B} \/
      {C} \/ {D} \/ {E} by A49,A31,A33,FUNCOP_1:19
        .= {B,C} \/ {D} \/ {E} by ENUMSET1:41
        .= {B,C,D} \/ {E} by ENUMSET1:43
        .= {B,C,D,E} by ENUMSET1:46;
      then
A51:  D in dom h by ENUMSET1:def 2;
A52:  rng h c= {h.D,h.B,h.C,h.E}
      proof
        let t be set;
        assume t in rng h;
        then consider x1 being set such that
A53:    x1 in dom h and
A54:    t = h.x1 by FUNCT_1:def 5;
        now
          per cases by A50,A53,ENUMSET1:def 2;
          case
            x1=D;
            hence thesis by A54,ENUMSET1:def 2;
          end;
          case
            x1=B;
            hence thesis by A54,ENUMSET1:def 2;
          end;
          case
            x1=C;
            hence thesis by A54,ENUMSET1:def 2;
          end;
          case
            x1=E;
            hence thesis by A54,ENUMSET1:def 2;
          end;
        end;
        hence thesis;
      end;
      rng h c= bool Y
      proof
        let t be set;
        assume
A55:    t in rng h;
        now
          per cases by A52,A55,ENUMSET1:def 2;
          case
            t=h.D;
            hence thesis by A26,A42;
          end;
          case
            t=h.B;
            hence thesis by A28,A46;
          end;
          case
            t=h.C;
            hence thesis by A29,A40;
          end;
          case
            t=h.E;
            hence thesis by A23,A38;
          end;
        end;
        hence thesis;
      end;
      then reconsider F=rng h as Subset-Family of Y;
A56:  C in dom h by A50,ENUMSET1:def 2;
A57:  E in dom h by A50,ENUMSET1:def 2;
A58:  B in dom h by A50,ENUMSET1:def 2;
A59:  {h.D,h.B,h.C,h.E} c= rng h
      proof
        let t be set;
        assume
A60:    t in {h.D,h.B,h.C,h.E};
        now
          per cases by A60,ENUMSET1:def 2;
          case
            t=h.D;
            hence thesis by A51,FUNCT_1:def 5;
          end;
          case
            t=h.B;
            hence thesis by A58,FUNCT_1:def 5;
          end;
          case
            t=h.C;
            hence thesis by A56,FUNCT_1:def 5;
          end;
          case
            t=h.E;
            hence thesis by A57,FUNCT_1:def 5;
          end;
        end;
        hence thesis;
      end;
      then
A61:  {h.D,h.B,h.C,h.E} = rng h by A52,XBOOLE_0:def 10;
      reconsider h as Function;
A62:  x c= Intersect F
      proof
        let u be set;
A63:    h.D in {h.D,h.B,h.C,h.E} by ENUMSET1:def 2;
        assume
A64:    u in x;
        for y be set holds y in F implies u in y
        proof
          let y be set;
          assume
A65:      y in F;
          now
            per cases by A52,A65,ENUMSET1:def 2;
            case
A66:          y=h.D;
              u in d /\ ((b /\ c) /\ e) by A24,A27,A30,A64,XBOOLE_1:16;
              hence thesis by A42,A66,XBOOLE_0:def 4;
            end;
            case
A67:          y=h.B;
              u in (c /\ (d /\ b)) /\ e by A24,A27,A30,A64,XBOOLE_1:16;
              then u in c /\ ((d /\ b) /\ e) by XBOOLE_1:16;
              then u in c /\ ((d /\ e) /\ b) by XBOOLE_1:16;
              then u in (c /\ (d /\ e)) /\ b by XBOOLE_1:16;
              hence thesis by A46,A67,XBOOLE_0:def 4;
            end;
            case
A68:          y=h.C;
              u in (c /\ (b /\ d)) /\ e by A24,A27,A30,A64,XBOOLE_1:16;
              then u in c /\ (b /\ d /\ e) by XBOOLE_1:16;
              hence thesis by A40,A68,XBOOLE_0:def 4;
            end;
            case
              y=h.E;
              hence thesis by A24,A38,A64,XBOOLE_0:def 4;
            end;
          end;
          hence thesis;
        end;
        then u in meet F by A59,A63,SETFAM_1:def 1;
        hence thesis by A59,A63,SETFAM_1:def 10;
      end;
A69:  rng h <> {} by A51,FUNCT_1:12;
      Intersect F c= x
      proof
        let t be set;
        assume t in Intersect F;
        then
A70:    t in meet (rng h) by A69,SETFAM_1:def 10;
        h.D in rng h by A61,ENUMSET1:def 2;
        then
A71:    t in h.D by A70,SETFAM_1:def 1;
        h.C in rng h by A61,ENUMSET1:def 2;
        then
A72:    t in h.C by A70,SETFAM_1:def 1;
        h.B in rng h by A61,ENUMSET1:def 2;
        then t in h.B by A70,SETFAM_1:def 1;
        then t in b /\ c by A46,A40,A72,XBOOLE_0:def 4;
        then
A73:    t in (b /\ c) /\ d by A42,A71,XBOOLE_0:def 4;
        h.E in rng h by A61,ENUMSET1:def 2;
        then t in h.E by A70,SETFAM_1:def 1;
        hence thesis by A24,A27,A30,A38,A73,XBOOLE_0:def 4;
      end;
      then x = Intersect F by A62,XBOOLE_0:def 10;
      hence thesis by A18,A17,A50,A47,A21,BVFUNC_2:def 1;
    end;
    '/\' (G \ {A}) c= B '/\' C '/\' D '/\' E
    proof
      let x be set;
      assume x in '/\' (G \ {A});
      then consider h being Function, F being Subset-Family of Y such that
A74:  dom h=(G \ {A}) and
A75:  rng h = F and
A76:  for d being set st d in (G \ {A}) holds h.d in d and
A77:  x=Intersect F and
A78:  x<>{} by BVFUNC_2:def 1;
      D in dom h by A18,A17,A74,ENUMSET1:def 2;
      then
A79:  h.D in rng h by FUNCT_1:def 5;
      set mbc=h.B /\ h.C;
A80:  not x in {{}} by A78,TARSKI:def 1;
      E in (G \ {A}) by A18,A17,ENUMSET1:def 2;
      then
A81:  h.E in E by A76;
      D in (G \ {A}) by A18,A17,ENUMSET1:def 2;
      then
A82:  h.D in D by A76;
      C in (G \ {A}) by A18,A17,ENUMSET1:def 2;
      then
A83:  h.C in C by A76;
      E in dom h by A18,A17,A74,ENUMSET1:def 2;
      then
A84:  h.E in rng h by FUNCT_1:def 5;
      set mbcd=(h.B /\ h.C) /\ h.D;
      B in dom h by A18,A17,A74,ENUMSET1:def 2;
      then
A85:  h.B in rng h by FUNCT_1:def 5;
      C in dom h by A18,A17,A74,ENUMSET1:def 2;
      then
A86:  h.C in rng h by FUNCT_1:def 5;
A87:  x c= h.B /\ h.C /\ h.D /\ h.E
      proof
        let m be set;
        assume m in x;
        then
A88:    m in meet (rng h) by A75,A77,A85,SETFAM_1:def 10;
        then m in h.B & m in h.C by A85,A86,SETFAM_1:def 1;
        then
A89:    m in h.B /\ h.C by XBOOLE_0:def 4;
        m in h.D by A79,A88,SETFAM_1:def 1;
        then
A90:    m in h.B /\ h.C /\ h.D by A89,XBOOLE_0:def 4;
        m in h.E by A84,A88,SETFAM_1:def 1;
        hence thesis by A90,XBOOLE_0:def 4;
      end;
      then mbcd<>{} by A78;
      then
A91:  not mbcd in {{}} by TARSKI:def 1;
      mbc<>{} by A78,A87;
      then
A92:  not mbc in {{}} by TARSKI:def 1;
      B in (G \ {A}) by A18,A17,ENUMSET1:def 2;
      then h.B in B by A76;
      then mbc in INTERSECTION(B,C) by A83,SETFAM_1:def 5;
      then mbc in INTERSECTION(B,C) \ {{}} by A92,XBOOLE_0:def 5;
      then mbc in B '/\' C by PARTIT1:def 4;
      then mbcd in INTERSECTION(B '/\' C,D) by A82,SETFAM_1:def 5;
      then mbcd in INTERSECTION(B '/\' C,D) \ {{}} by A91,XBOOLE_0:def
      5;
      then
A93:  mbcd in B '/\' C '/\' D by PARTIT1:def 4;
      h.B /\ h.C /\ h.D /\ h.E c= x
      proof
        let m be set;
        assume
A94:    m in h.B /\ h.C /\ h.D /\ h.E;
        then
A95:    m in h.B /\ h.C /\ h.D by XBOOLE_0:def 4;
        then
A96:    m in h.B /\ h.C by XBOOLE_0:def 4;
A97:    rng h c= {h.B,h.C,h.D,h.E}
        proof
          let u be set;
          assume u in rng h;
          then consider x1 being set such that
A98:      x1 in dom h and
A99:      u = h.x1 by FUNCT_1:def 5;
          now
            per cases by A15,A17,A74,A98,ENUMSET1:def 2;
            case
              x1=B;
              hence thesis by A99,ENUMSET1:def 2;
            end;
            case
              x1=C;
              hence thesis by A99,ENUMSET1:def 2;
            end;
            case
              x1=D;
              hence thesis by A99,ENUMSET1:def 2;
            end;
            case
              x1=E;
              hence thesis by A99,ENUMSET1:def 2;
            end;
          end;
          hence thesis;
        end;
        for y being set holds y in rng h implies m in y
        proof
          let y be set;
          assume
A100:     y in rng h;
          now
            per cases by A97,A100,ENUMSET1:def 2;
            case
              y=h.B;
              hence thesis by A96,XBOOLE_0:def 4;
            end;
            case
              y=h.C;
              hence thesis by A96,XBOOLE_0:def 4;
            end;
            case
              y=h.D;
              hence thesis by A95,XBOOLE_0:def 4;
            end;
            case
              y=h.E;
              hence thesis by A94,XBOOLE_0:def 4;
            end;
          end;
          hence thesis;
        end;
        then m in meet (rng h) by A85,SETFAM_1:def 1;
        hence thesis by A75,A77,A85,SETFAM_1:def 10;
      end;
      then ((h.B /\ h.C) /\ h.D) /\ h.E = x by A87,XBOOLE_0:def 10;
      then x in INTERSECTION(B '/\' C '/\' D,E) by A81,A93, SETFAM_1:
      def 5;
      then x in INTERSECTION(B '/\' C '/\' D,E) \ {{}} by A80, XBOOLE_0
      :def 5;
      hence thesis by PARTIT1:def 4;
    end;
    then '/\' (G \ {A}) = B '/\' C '/\' D '/\' E by A19,XBOOLE_0:def 10;
    hence thesis by BVFUNC_2:def 7;
  end;
end;

theorem Th25:
  G={A,B,C,D,E} & A<>B & B<>C & B<>D & B<>E implies CompF(B,G) = A
  '/\' C '/\' D '/\' E
proof
  assume that
A1: G={A,B,C,D,E} and
A2: A<>B & B<>C & B<>D & B<>E;
  {A,B,C,D,E}={A,B} \/ {C,D,E} by ENUMSET1:48;
  then G={B,A,C,D,E} by A1,ENUMSET1:48;
  hence thesis by A2,Th24;
end;

theorem Th26:
  G={A,B,C,D,E} & A<>C & B<>C & C<>D & C<>E implies CompF(C,G) = A
  '/\' B '/\' D '/\' E
proof
  assume that
A1: G={A,B,C,D,E} and
A2: A<>C & B<>C & C<>D & C<>E;
  {A,B,C,D,E}={A,B,C} \/ {D,E} by ENUMSET1:49;
  then {A,B,C,D,E}={A} \/ {B,C} \/ {D,E} by ENUMSET1:42;
  then {A,B,C,D,E}={A,C,B} \/ {D,E} by ENUMSET1:42;
  then {A,B,C,D,E}={A,C} \/ {B} \/ {D,E} by ENUMSET1:43;
  then {A,B,C,D,E}={C,A,B} \/ {D,E} by ENUMSET1:43;
  then G={C,A,B,D,E} by A1,ENUMSET1:49;
  hence thesis by A2,Th24;
end;

theorem Th27:
  G={A,B,C,D,E} & A<>D & B<>D & C<>D & D<>E implies CompF(D,G) = A
  '/\' B '/\' C '/\' E
proof
  assume that
A1: G={A,B,C,D,E} and
A2: A<>D & B<>D & C<>D & D<>E;
  {A,B,C,D,E}={A,B} \/ {C,D,E} by ENUMSET1:48;
  then {A,B,C,D,E}={A,B} \/ ({C,D} \/ {E}) by ENUMSET1:43;
  then {A,B,C,D,E}={A,B} \/ {D,C,E} by ENUMSET1:43;
  then G={A,B,D,C,E} by A1,ENUMSET1:48;
  hence thesis by A2,Th26;
end;

theorem
  G={A,B,C,D,E} & A<>E & B<>E & C<>E & D<>E implies CompF(E,G) = A '/\'
  B '/\' C '/\' D
proof
  assume that
A1: G={A,B,C,D,E} and
A2: A<>E & B<>E & C<>E & D<>E;
  {A,B,C,D,E}={A,B,C} \/ {D,E} by ENUMSET1:49;
  then G={A,B,C,E,D} by A1,ENUMSET1:49;
  hence thesis by A2,Th27;
end;

theorem Th29:
  for A,B,C,D,E being set, h being Function, A9,B9,C9,D9,E9 being
set st A<>B & A<>C & A<>D & A<>E & B<>C & B<>D & B<>E & C<>D & C<>E & D<>E & h
= (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9) +* (A .--> A9) holds
  h.A = A9 & h.B = B9 & h.C = C9 & h.D = D9 & h.E = E9
proof
  let A,B,C,D,E be set;
  let h be Function;
  let A9,B9,C9,D9,E9 be set;
  assume that
A1: A<>B and
A2: A<>C and
A3: A<>D and
A4: A<>E and
A5: B<>C and
A6: B<>D and
A7: B<>E and
A8: C<>D and
A9: C<>E and
A10: D<>E and
A11: h = (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9) +* (A
  .--> A9);
A12: dom (A .--> A9) = {A} by FUNCOP_1:19;
  then A in dom (A .--> A9) by TARSKI:def 1;
  then
A13: h.A = (A .--> A9).A by A11,FUNCT_4:14;
  not C in dom (A .--> A9) by A2,A12,TARSKI:def 1;
  then
A14: h.C=((B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9)).C by A11
  ,FUNCT_4:12;
A15: dom (D .--> D9) = {D} by FUNCOP_1:19;
  then not B in dom (D .--> D9) by A6,TARSKI:def 1;
  then
A16: ((B .--> B9) +* (C .--> C9) +* (D .--> D9)).B= ((B .--> B9) +* (C .-->
  C9)).B by FUNCT_4:12;
  not E in dom (A .--> A9) by A4,A12,TARSKI:def 1;
  then
A17: h.E=((B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9)).E by A11
  ,FUNCT_4:12;
A18: dom (E .--> E9) = {E} by FUNCOP_1:19;
  then E in dom (E .--> E9) by TARSKI:def 1;
  then
A19: h.E=(E .--> E9).E by A17,FUNCT_4:14;
  not C in dom (D .--> D9) by A8,A15,TARSKI:def 1;
  then
A20: ((B .--> B9) +* (C .--> C9) +* (D .--> D9)).C= ((B .--> B9) +* (C .-->
  C9)).C by FUNCT_4:12;
  not C in dom (E .--> E9) by A9,A18,TARSKI:def 1;
  then
A21: h.C=((B .--> B9) +* (C .--> C9) +* (D .--> D9)).C by A14, FUNCT_4:
  12;
A22: dom (C .--> C9) = {C} by FUNCOP_1:19;
  then C in dom (C .--> C9) by TARSKI:def 1;
  then
A23: h.C=(C .--> C9).C by A21,A20,FUNCT_4:14;
  not D in dom (A .--> A9) by A3,A12,TARSKI:def 1;
  then
A24: h.D=((B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9)).D by A11
  ,FUNCT_4:12;
  not D in dom (E .--> E9) by A10,A18,TARSKI:def 1;
  then
A25: h.D=((B .--> B9) +* (C .--> C9) +* (D .--> D9)).D by A24, FUNCT_4:
  12;
  D in dom (D .--> D9) by A15,TARSKI:def 1;
  then
A26: h.D=(D .--> D9).D by A25,FUNCT_4:14;
  not B in dom (A .--> A9) by A1,A12,TARSKI:def 1;
  then
A27: h.B=((B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9)).B by A11
  ,FUNCT_4:12;
  not B in dom (E .--> E9) by A7,A18,TARSKI:def 1;
  then
A28: h.B=((B .--> B9) +* (C .--> C9) +* (D .--> D9)).B by A27, FUNCT_4:
  12;
  not B in dom (C .--> C9) by A5,A22,TARSKI:def 1;
  then h.B=(B .--> B9).B by A28,A16,FUNCT_4:12;
  hence thesis by A13,A23,A26,A19,FUNCOP_1:87;
end;

theorem Th30:
  for A,B,C,D,E being set, h being Function, A9,B9,C9,D9,E9 being
set st h = (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9) +* (A .-->
  A9) holds dom h = {A,B,C,D,E}
proof
  let A,B,C,D,E be set;
  let h be Function;
  let A9,B9,C9,D9,E9 be set;
  assume
A1: h = (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9) +* (A
  .--> A9);
A2: dom (D .--> D9) = {D} & dom (E .--> E9) = {E} by FUNCOP_1:19;
  dom ((B .--> B9) +* (C .--> C9)) = dom (B .--> B9) \/ dom (C .--> C9) by
  FUNCT_4:def 1;
  then
  dom ((B .--> B9) +* (C .--> C9) +* (D .--> D9)) = dom (B .--> B9) \/ dom
  (C .--> C9) \/ dom (D .--> D9) by FUNCT_4:def 1;
  then
  dom ((B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9)) = dom (B
  .--> B9) \/ dom (C .--> C9) \/ dom (D .--> D9) \/ dom (E .--> E9) by FUNCT_4
  :def 1;
  then
A3: dom ((B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9) +* (A .-->
A9)) = dom (B .--> B9) \/ dom (C .--> C9) \/ dom (D .--> D9) \/ dom (E .--> E9)
  \/ dom (A .--> A9) by FUNCT_4:def 1;
  dom (B .--> B9) = {B} & dom (C .--> C9) = {C} by FUNCOP_1:19;
  then dom ((B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9) +* (A
  .--> A9)) = {A} \/ (({B} \/ {C}) \/ {D} \/ {E}) by A3,A2,FUNCOP_1:19
    .= {A} \/ ({B,C} \/ {D} \/ {E}) by ENUMSET1:41
    .= {A} \/ ({B,C,D} \/ {E}) by ENUMSET1:43
    .= {A} \/ {B,C,D,E} by ENUMSET1:46
    .= {A,B,C,D,E} by ENUMSET1:47;
  hence thesis by A1;
end;

theorem Th31:
  for A,B,C,D,E being set, h being Function, A9,B9,C9,D9,E9 being
set st h = (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9) +* (A .-->
  A9) holds rng h = {h.A,h.B,h.C,h.D,h.E}
proof
  let A,B,C,D,E be set;
  let h be Function;
  let A9,B9,C9,D9,E9 be set;
  assume
  h = (B .--> B9) +* (C .--> C9) +* (D .--> D9) +* (E .--> E9) +* (A .--> A9 );
  then
A1: dom h = {A,B,C,D,E} by Th30;
  then
A2: B in dom h by ENUMSET1:def 3;
A3: D in dom h by A1,ENUMSET1:def 3;
A4: C in dom h by A1,ENUMSET1:def 3;
A5: rng h c= {h.A,h.B,h.C,h.D,h.E}
  proof
    let t be set;
    assume t in rng h;
    then consider x1 being set such that
A6: x1 in dom h and
A7: t = h.x1 by FUNCT_1:def 5;
    now
      per cases by A1,A6,ENUMSET1:def 3;
      case
        x1=A;
        hence thesis by A7,ENUMSET1:def 3;
      end;
      case
        x1=B;
        hence thesis by A7,ENUMSET1:def 3;
      end;
      case
        x1=C;
        hence thesis by A7,ENUMSET1:def 3;
      end;
      case
        x1=D;
        hence thesis by A7,ENUMSET1:def 3;
      end;
      case
        x1=E;
        hence thesis by A7,ENUMSET1:def 3;
      end;
    end;
    hence thesis;
  end;
A8: E in dom h by A1,ENUMSET1:def 3;
A9: A in dom h by A1,ENUMSET1:def 3;
  {h.A,h.B,h.C,h.D,h.E} c= rng h
  proof
    let t be set;
    assume
A10: t in {h.A,h.B,h.C,h.D,h.E};
    now
      per cases by A10,ENUMSET1:def 3;
      case
        t=h.A;
        hence thesis by A9,FUNCT_1:def 5;
      end;
      case
        t=h.B;
        hence thesis by A2,FUNCT_1:def 5;
      end;
      case
        t=h.C;
        hence thesis by A4,FUNCT_1:def 5;
      end;
      case
        t=h.D;
        hence thesis by A3,FUNCT_1:def 5;
      end;
      case
        t=h.E;
        hence thesis by A8,FUNCT_1:def 5;
      end;
    end;
    hence thesis;
  end;
  hence thesis by A5,XBOOLE_0:def 10;
end;

theorem
  for G being Subset of PARTITIONS(Y), A,B,C,D,E being a_partition of Y,
z,u being Element of Y, h being Function st G is independent & G={A,B,C,D,E} &
  A<>B & A<>C & A<>D & A<>E & B<>C & B<>D & B<>E & C<>D & C<>E & D<>E holds
  EqClass(u,B '/\' C '/\' D '/\' E) meets EqClass(z,A)
proof
  let G be Subset of PARTITIONS(Y);
  let A,B,C,D,E be a_partition of Y;
  let z,u be Element of Y;
  let h be Function;
  assume that
A1: G is independent and
A2: G={A,B,C,D,E} and
A3: A<>B & A<>C & A<>D & A<>E & B<>C & B<>D & B<>E & C<>D & C<>E & D<>E;
  set h = (B .--> EqClass(u,B)) +* (C .--> EqClass(u,C)) +* (D .--> EqClass(u,
  D)) +* (E .--> EqClass(u,E)) +* (A .--> EqClass(z,A));
A4: h.B = EqClass(u,B) by A3,Th29;
A5: h.D = EqClass(u,D) by A3,Th29;
A6: h.C = EqClass(u,C) by A3,Th29;
A7: h.E = EqClass(u,E) by A3,Th29;
A8: rng h = {h.A,h.B,h.C,h.D,h.E} by Th31;
  rng h c= bool Y
  proof
    let t be set;
    assume
A9: t in rng h;
    now
      per cases by A8,A9,ENUMSET1:def 3;
      case
        t=h.A;
        then t=EqClass(z,A) by A3,Th29;
        hence thesis;
      end;
      case
        t=h.B;
        hence thesis by A4;
      end;
      case
        t=h.C;
        hence thesis by A6;
      end;
      case
        t=h.D;
        hence thesis by A5;
      end;
      case
        t=h.E;
        hence thesis by A7;
      end;
    end;
    hence thesis;
  end;
  then reconsider FF=rng h as Subset-Family of Y;
A10: dom h = G by A2,Th30;
  for d being set st d in G holds h.d in d
  proof
    let d be set;
    assume
A11: d in G;
    now
      per cases by A2,A11,ENUMSET1:def 3;
      case
A12:    d=A;
        h.A=EqClass(z,A) by A3,Th29;
        hence thesis by A12;
      end;
      case
A13:    d=B;
        h.B=EqClass(u,B) by A3,Th29;
        hence thesis by A13;
      end;
      case
A14:    d=C;
        h.C=EqClass(u,C) by A3,Th29;
        hence thesis by A14;
      end;
      case
A15:    d=D;
        h.D=EqClass(u,D) by A3,Th29;
        hence thesis by A15;
      end;
      case
A16:    d=E;
        h.E=EqClass(u,E) by A3,Th29;
        hence thesis by A16;
      end;
    end;
    hence thesis;
  end;
  then (Intersect FF)<>{} by A1,A10,BVFUNC_2:def 5;
  then consider m being set such that
A17: m in Intersect FF by XBOOLE_0:def 1;
  A in dom h by A2,A10,ENUMSET1:def 3;
  then
A18: h.A in rng h by FUNCT_1:def 5;
  then
A19: m in meet FF by A17,SETFAM_1:def 10;
  then
A20: m in h.A by A18,SETFAM_1:def 1;
  D in dom h by A2,A10,ENUMSET1:def 3;
  then h.D in rng h by FUNCT_1:def 5;
  then
A21: m in h.D by A19,SETFAM_1:def 1;
  C in dom h by A2,A10,ENUMSET1:def 3;
  then h.C in rng h by FUNCT_1:def 5;
  then
A22: m in h.C by A19,SETFAM_1:def 1;
  B in dom h by A2,A10,ENUMSET1:def 3;
  then h.B in rng h by FUNCT_1:def 5;
  then m in h.B by A19,SETFAM_1:def 1;
  then m in EqClass(u,B) /\ EqClass(u,C) by A4,A6,A22,XBOOLE_0:def 4;
  then
A23: m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) by A5,A21,
  XBOOLE_0:def 4;
  E in dom h by A2,A10,ENUMSET1:def 3;
  then h.E in rng h by FUNCT_1:def 5;
  then m in h.E by A19,SETFAM_1:def 1;
  then
A24: m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) /\ EqClass(u,E) by
  A7,A23,XBOOLE_0:def 4;
  set GG=EqClass(u,B '/\' C '/\' D '/\' E);
  GG = EqClass(u,B '/\' C '/\' D) /\ EqClass(u,E) by Th1;
  then
A25: GG = EqClass(u,B '/\' C) /\ EqClass(u,D) /\ EqClass(u,E) by Th1;
  h.A = EqClass(z,A) by A3,Th29;
  then m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) /\ EqClass(u,E) /\
  EqClass(z,A) by A20,A24,XBOOLE_0:def 4;
  then EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) /\ EqClass(u,E) meets
  EqClass(z,A) by XBOOLE_0:4;
  hence thesis by A25,Th1;
end;

theorem
  for G being Subset of PARTITIONS(Y), A,B,C,D,E being a_partition of Y,
z,u being Element of Y st G is independent & G={A,B,C,D,E} & A<>B & A<>C & A<>D
& A<>E & B<>C & B<>D & B<>E & C<>D & C<>E & D<>E & EqClass(z,C '/\' D '/\' E)=
EqClass(u,C '/\' D '/\' E) holds EqClass(u,CompF(A,G)) meets EqClass(z,CompF(B,
  G))
proof
  let G be Subset of PARTITIONS(Y);
  let A,B,C,D,E be a_partition of Y;
  let z,u be Element of Y;
  assume that
A1: G is independent and
A2: G={A,B,C,D,E} and
A3: A<>B and
A4: A<>C & A<>D & A<>E and
A5: B<>C & B<>D & B<>E and
A6: C<>D & C<>E & D<>E and
A7: EqClass(z,C '/\' D '/\' E)=EqClass(u,C '/\' D '/\' E);
  set h = (B .--> EqClass(u,B)) +* (C .--> EqClass(u,C)) +* (D .--> EqClass(u,
  D)) +* (E .--> EqClass(u,E)) +* (A .--> EqClass(z,A));
A8: h.B = EqClass(u,B) by A3,A4,A5,A6,Th29;
A9: h.E = EqClass(u,E) by A3,A4,A5,A6,Th29;
A10: h.D = EqClass(u,D) by A3,A4,A5,A6,Th29;
A11: h.C = EqClass(u,C) by A3,A4,A5,A6,Th29;
A12: rng h = {h.A,h.B,h.C,h.D,h.E} by Th31;
  rng h c= bool Y
  proof
    let t be set;
    assume
A13: t in rng h;
    now
      per cases by A12,A13,ENUMSET1:def 3;
      case
        t=h.A;
        then t=EqClass(z,A) by A3,A4,A5,A6,Th29;
        hence thesis;
      end;
      case
        t=h.B;
        hence thesis by A8;
      end;
      case
        t=h.C;
        hence thesis by A11;
      end;
      case
        t=h.D;
        hence thesis by A10;
      end;
      case
        t=h.E;
        hence thesis by A9;
      end;
    end;
    hence thesis;
  end;
  then reconsider FF=rng h as Subset-Family of Y;
A14: dom h = G by A2,Th30;
  for d being set st d in G holds h.d in d
  proof
    let d be set;
    assume
A15: d in G;
    now
      per cases by A2,A15,ENUMSET1:def 3;
      case
A16:    d=A;
        h.A=EqClass(z,A) by A3,A4,A5,A6,Th29;
        hence thesis by A16;
      end;
      case
A17:    d=B;
        h.B=EqClass(u,B) by A3,A4,A5,A6,Th29;
        hence thesis by A17;
      end;
      case
A18:    d=C;
        h.C=EqClass(u,C) by A3,A4,A5,A6,Th29;
        hence thesis by A18;
      end;
      case
A19:    d=D;
        h.D=EqClass(u,D) by A3,A4,A5,A6,Th29;
        hence thesis by A19;
      end;
      case
A20:    d=E;
        h.E=EqClass(u,E) by A3,A4,A5,A6,Th29;
        hence thesis by A20;
      end;
    end;
    hence thesis;
  end;
  then (Intersect FF)<>{} by A1,A14,BVFUNC_2:def 5;
  then consider m being set such that
A21: m in Intersect FF by XBOOLE_0:def 1;
  A in dom h by A2,A14,ENUMSET1:def 3;
  then
A22: h.A in rng h by FUNCT_1:def 5;
  then
A23: m in meet FF by A21,SETFAM_1:def 10;
  then
A24: m in h.A by A22,SETFAM_1:def 1;
  D in dom h by A2,A14,ENUMSET1:def 3;
  then h.D in rng h by FUNCT_1:def 5;
  then
A25: m in h.D by A23,SETFAM_1:def 1;
  C in dom h by A2,A14,ENUMSET1:def 3;
  then h.C in rng h by FUNCT_1:def 5;
  then
A26: m in h.C by A23,SETFAM_1:def 1;
  B in dom h by A2,A14,ENUMSET1:def 3;
  then h.B in rng h by FUNCT_1:def 5;
  then m in h.B by A23,SETFAM_1:def 1;
  then m in EqClass(u,B) /\ EqClass(u,C) by A8,A11,A26,XBOOLE_0:def 4;
  then
A27: m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) by A10,A25,
  XBOOLE_0:def 4;
  E in dom h by A2,A14,ENUMSET1:def 3;
  then h.E in rng h by FUNCT_1:def 5;
  then m in h.E by A23,SETFAM_1:def 1;
  then
A28: m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) /\ EqClass(u,E) by
  A9,A27,XBOOLE_0:def 4;
  set GG=EqClass(u,(((B '/\' C) '/\' D) '/\' E));
  set I=EqClass(z,A);
  GG = EqClass(u,B '/\' C '/\' D) /\ EqClass(u,E) by Th1;
  then
A29: GG = EqClass(u,B '/\' C) /\ EqClass(u,D) /\ EqClass(u,E) by Th1;
  h.A = EqClass(z,A) by A3,A4,A5,A6,Th29;
  then m in EqClass(u,B) /\ EqClass(u,C) /\ EqClass(u,D) /\ EqClass(u,E) /\
  EqClass(z,A) by A24,A28,XBOOLE_0:def 4;
  then GG /\ I <> {} by A29,Th1;
  then consider p being set such that
A30: p in GG /\ I by XBOOLE_0:def 1;
  reconsider p as Element of Y by A30;
  set K=EqClass(p,C '/\' D '/\' E);
A31: p in GG by A30,XBOOLE_0:def 4;
A32: z in I by EQREL_1:def 8;
  set L=EqClass(z,C '/\' D '/\' E);
A33: p in EqClass(p,C '/\' D '/\' E) by EQREL_1:def 8;
  GG = EqClass(u,((B '/\' (C '/\' D)) '/\' E)) by PARTIT1:16;
  then GG = EqClass(u,B '/\' (C '/\' D '/\' E)) by PARTIT1:16;
  then GG c= L by A7,BVFUNC11:3;
  then K meets L by A31,A33,XBOOLE_0:3;
  then K=L by EQREL_1:50;
  then z in K by EQREL_1:def 8;
  then
A34: z in I /\ K by A32,XBOOLE_0:def 4;
  set H=EqClass(z,CompF(B,G));
  A '/\' (C '/\' D '/\' E) = A '/\' (C '/\' D) '/\' E by PARTIT1:16;
  then
A35: A '/\' (C '/\' D '/\' E) = A '/\' C '/\' D '/\' E by PARTIT1:16;
A36: p in K & p in I by A30,EQREL_1:def 8,XBOOLE_0:def 4;
  then p in I /\ K by XBOOLE_0:def 4;
  then I /\ K in INTERSECTION(A,C '/\' D '/\' E) & not I /\ K in {{}} by
  SETFAM_1:def 5,TARSKI:def 1;
  then
A37: I /\ K in INTERSECTION(A,C '/\' D '/\' E) \ {{}} by
  XBOOLE_0:def 5;
  CompF(B,G) = A '/\' C '/\' D '/\' E by A2,A3,A5,Th25;
  then I /\ K in CompF(B,G) by A37,A35,PARTIT1:def 4;
  then
A38: I /\ K = H or I /\ K misses H by EQREL_1:def 6;
  z in H by EQREL_1:def 8;
  then p in H by A36,A34,A38,XBOOLE_0:3,def 4;
  then p in GG /\ H by A31,XBOOLE_0:def 4;
  then GG meets H by XBOOLE_0:4;
  hence thesis by A2,A3,A4,Th24;
end;
 
