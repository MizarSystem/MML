    end;
    J in dom h by A78,ENUMSET1:def 4;
    then
A87: h.J in rng h by FUNCT_1:def 5;
    F in dom h by A78,ENUMSET1:def 4;
    then
A88: h.F in rng h by FUNCT_1:def 5;
    B in dom h by A78,ENUMSET1:def 4;
    then
A89: h.B in rng h by FUNCT_1:def 5;
    {h.B,h.C,h.D,h.E,h.F,h.J} c= rng h
    proof
      let t be set;
      assume t in {h.B,h.C,h.D,h.E,h.F,h.J};
      hence thesis by A79,A89,A82,A81,A88,A87,ENUMSET1:def 4;
    end;
    then
A90: rng h = {h.B,h.C,h.D,h.E,h.F,h.J} by A84,XBOOLE_0:def 10;
A91: h.J = j by A6,Th40;
A92: h.F = f by A6,Th40;
A93: h.E = e by A6,Th40;
A94: h.D = d by A6,Th40;
    rng h c= bool Y
    proof
