    end;
A104: Intersect FF = meet (rng h) by A79,SETFAM_1:def 10;
    Intersect FF c= x
    proof
      let t be set;
      assume
A105: t in Intersect FF;
      h.C in rng h by A90,ENUMSET1:def 4;
      then
A106: t in c by A83,A104,A105,SETFAM_1:def 1;
      h.B in rng h by A90,ENUMSET1:def 4;
