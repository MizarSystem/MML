    then reconsider FF=rng h as Subset-Family of Y;
A95: dom ((B .--> b) +* (C .--> c) +* (D .--> d) +* (E .--> e) +* (F .-->
    f) +* (J .--> j)) = {J,B,C,D,E,F} by Th41
      .= {J} \/ {B,C,D,E,F} by ENUMSET1:51
      .= {B,C,D,E,F,J} by ENUMSET1:55;
    reconsider h as Function;
A96: x c= Intersect FF
    proof
      let u be set;
      assume
A97:  u in x;
      for y be set holds y in FF implies u in y
      proof
        let y be set;
        assume
A98:    y in FF;
        now
          per cases by A84,A98,ENUMSET1:def 4;
          case
A99:        y=h.D;
            u in (d /\ ((b /\ c) /\ e)) /\ f /\ j by A65,A68,A71,A74,A76,
            A97,XBOOLE_1:16;
