  F) '/\' J by PARTIT1:16
    .= A '/\' (C '/\' D '/\' E) '/\' F '/\' J by PARTIT1:16
    .= A '/\' (C '/\' D) '/\' E '/\' F '/\' J by PARTIT1:16
    .= A '/\' C '/\' D '/\' E '/\' F '/\' J by PARTIT1:16;
A10: h.B = EqClass(u,B) by A3,Th52;
A11: h.F = EqClass(u,F) by A3,Th52;
A12: h.E = EqClass(u,E) by A3,Th52;
A13: h.J = EqClass(u,J) by A3,Th52;
A14: h.D = EqClass(u,D) by A3,Th52;
A15: h.C = EqClass(u,C) by A3,Th52;
A16: rng h = {h.A,h.B,h.C,h.D,h.E,h.F,h.J} by Th54;
  rng h c= bool Y
  proof
    let t be set;
    assume t in rng h;
    then t=h.A or t=h.B or t=h.C or t=h.D or t=h.E or t=h.F or t=h.J by A16,
    ENUMSET1:def 5;
    hence thesis by A5,A10,A15,A14,A12,A11,A13;
