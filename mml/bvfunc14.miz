theorem Th7:
  G={A,B,C,D} & A<>B & A<>C & A<>D implies CompF(A,G) = B '/\' C '/\' D
proof
  assume that
A1: G={A,B,C,D} and
A2: A<>B and
A3: A<>C and
A4: A<>D;
  per cases;
  suppose
A5: B = C;
    then G = {B,B,A,D} by A1,ENUMSET1:116
      .= {B,A,D} by ENUMSET1:71
      .= {A,B,D} by ENUMSET1:99;
    hence CompF(A,G)= B '/\' D by A2,A4,Th4
      .= B '/\' C '/\' D by A5,PARTIT1:15;
  end;
  suppose
A6: B = D;
    then G = {B,B,A,C} by A1,ENUMSET1:112
      .= {B,A,C} by ENUMSET1:71
      .= {A,B,C} by ENUMSET1:99;
    hence CompF(A,G) = B '/\' C by A2,A3,Th4
      .= B '/\' D '/\' C by A6,PARTIT1:15
      .= B '/\' C '/\' D by PARTIT1:16;
  end;
  suppose
A7: C = D;
    then G = {C,C,A,B} by A1,ENUMSET1:118
      .= {C,A,B} by ENUMSET1:71
      .= {A,B,C} by ENUMSET1:100;
    hence CompF(A,G) = B '/\' C by A2,A3,Th4
      .= B '/\' (C '/\' D) by A7,PARTIT1:15
      .= B '/\' C '/\' D by PARTIT1:16;
  end;
  suppose
A8: B<>C & B<>D & C<>D;
    G \ {A}={A} \/ {B,C,D} \ {A} by A1,ENUMSET1:44;
    then
A9: G \ {A} = ({A} \ {A}) \/ ({B,C,D} \ {A}) by XBOOLE_1:42;
A10: not B in {A} by A2,TARSKI:def 1;
A11: not C in {A} by A3,TARSKI:def 1;
A12: not D in {A} by A4,TARSKI:def 1;
    {B,C,D} \ {A} = ({B} \/ {C,D}) \ {A} by ENUMSET1:42
      .= ({B} \ {A}) \/ ({C,D} \ {A}) by XBOOLE_1:42
      .= ({B} \ {A}) \/ ({C,D}) by A11,A12,ZFMISC_1:72
      .= {B} \/ {C,D} by A10,ZFMISC_1:67
      .= {B,C,D} by ENUMSET1:42;
    then
A13: G \ {A} = {} \/ {B,C,D} by A9,XBOOLE_1:37
      .= {B,C,D};
A14: B '/\' C '/\' D c= '/\' (G \ {A})
    proof
      let x be set;
      assume
A15:  x in B '/\' C '/\' D;
      then x in INTERSECTION(B '/\' C,D) \ {{}} by PARTIT1:def 4;
      then consider a,d being set such that
A16:  a in B '/\' C & d in D & x = a /\ d by SETFAM_1:def 5;
      a in INTERSECTION(B,C) \ {{}} by A16,PARTIT1:def 4;
      then consider b,c being set such that
A17:  b in B & c in C & a = b /\ c by SETFAM_1:def 5;
      set h = (B .--> b) +* (C .--> c) +* (D .--> d);
A18:  dom h = {B,C,D} by Lm1;
A19:  h.D = d by FUNCT_7:96;
A20:  h.B = b by A8,Lm3;
A21:  h.C = c by A8,Lm2;
A22:  for p being set st p in (G \ {A}) holds h.p in p
      proof
        let p be set;
        assume
A23:    p in (G \ {A});
        now per cases by A13,A23,ENUMSET1:def 1;
          case p=D;
            hence thesis by A16,FUNCT_7:96;
          end;
          case p=B;
            hence thesis by A8,A17,Lm3;
          end;
          case p=C;
            hence thesis by A8,A17,Lm2;
          end;
        end;
        hence thesis;
      end;
A24:  D in dom h by A18,ENUMSET1:def 1;
A25:  rng h = {h.B,h.C,h.D} by Lm4
        .= {h.D,h.B,h.C} by ENUMSET1:100;
      rng h c= bool Y
      proof
        let t be set;
        assume
A26:    t in rng h;
        now per cases by A25,A26,ENUMSET1:def 1;
          case t=h.D;
            hence thesis by A16,A19;
          end;
          case t=h.B;
            hence thesis by A17,A20;
          end;
          case t=h.C;
            hence thesis by A17,A21;
          end;
        end;
        hence thesis;
      end;
      then reconsider F=rng h as Subset-Family of Y;
A27:  rng h <> {} by A24,FUNCT_1:12;
A28:  x c= Intersect F
      proof
        let u be set;
        assume
A29:    u in x;
        for y be set holds y in F implies u in y
        proof
          let y be set;
          assume
A30:      y in F;
          now per cases by A25,A30,ENUMSET1:def 1;
            case y=h.D;
              hence thesis by A16,A19,A29,XBOOLE_0:def 3;
            end;
            case
A31:          y=h.B;
              u in b /\ (c /\ d) by A16,A17,A29,XBOOLE_1:16;
              hence thesis by A20,A31,XBOOLE_0:def 3;
            end;
            case
A32:          y=h.C;
              u in c /\ (b /\ d) by A16,A17,A29,XBOOLE_1:16;
              hence thesis by A21,A32,XBOOLE_0:def 3;
            end;
          end;
          hence thesis;
        end;
        then u in meet F by A25,SETFAM_1:def 1;
        hence thesis by A25,SETFAM_1:def 10;
      end;
      Intersect F c= x
      proof
        let t be set;
        assume t in Intersect F;
        then
A33:    t in meet (rng h) by A27,SETFAM_1:def 10;
        h.B in rng h & h.C in rng h & h.D in rng h by A25,ENUMSET1:def 1;
        then
A34:    t in h.B & t in h.C & t in h.D by A33,SETFAM_1:def 1;
        then t in b /\ c by A20,A21,XBOOLE_0:def 3;
        hence thesis by A16,A17,A19,A34,XBOOLE_0:def 3;
      end;
      then
A35:  x = Intersect F by A28,XBOOLE_0:def 10;
      x<>{} by A15,EQREL_1:def 6;
      hence thesis by A13,A18,A22,A35,BVFUNC_2:def 1;
    end;
    '/\' (G \ {A}) c= B '/\' C '/\' D
    proof
      let x be set;
      assume x in '/\' (G \ {A});
      then consider h being Function, F being Subset-Family of Y such that
A36:  dom h=(G \ {A}) & rng h = F &
      (for d being set st d in (G \ {A}) holds h.d in d) &
      x=Intersect F & x<>{} by BVFUNC_2:def 1;
      B in (G \ {A}) & C in (G \ {A}) & D in (G \ {A}) by A13,ENUMSET1:def 1;
      then
A37:  h.B in B & h.C in C & h.D in D by A36;
      B in dom h & C in dom h & D in dom h by A13,A36,ENUMSET1:def 1;
      then
A38:  h.B in rng h & h.C in rng h & h.D in rng h by FUNCT_1:def 5;
A39:  not (x in {{}}) by A36,TARSKI:def 1;
A40:  h.B /\ h.C /\ h.D c= x
      proof
        let m be set;
        assume
A41:    m in h.B /\ h.C /\ h.D;
        then
A42:    m in h.B /\ h.C & m in h.D by XBOOLE_0:def 3;
A43:    rng h c= {h.B,h.C,h.D}
        proof
          let u be set;
          assume u in rng h;
          then consider x1 being set such that
A44:      x1 in dom h & u = h.x1 by FUNCT_1:def 5;
          now per cases by A13,A36,A44,ENUMSET1:def 1;
            case x1=B;
              hence thesis by A44,ENUMSET1:def 1;
            end;
            case x1=C;
              hence thesis by A44,ENUMSET1:def 1;
            end;
            case x1=D;
              hence thesis by A44,ENUMSET1:def 1;
            end;
          end;
          hence thesis;
        end;
        for y being set holds y in rng h implies m in y
        proof
          let y be set;
          assume
A45:      y in rng h;
          now per cases by A43,A45,ENUMSET1:def 1;
            case y=h.B;
              hence thesis by A42,XBOOLE_0:def 3;
            end;
            case y=h.C;
              hence thesis by A42,XBOOLE_0:def 3;
            end;
            case y=h.D;
              hence thesis by A41,XBOOLE_0:def 3;
            end;
          end;
          hence thesis;
        end;
        then m in meet (rng h) by A38,SETFAM_1:def 1;
        hence thesis by A36,A38,SETFAM_1:def 10;
      end;
A46:  x c= h.B /\ h.C /\ h.D
      proof
        let m be set;
        assume m in x;
        then m in meet (rng h) by A36,A38,SETFAM_1:def 10;
        then
A47:    m in h.B & m in h.C & m in h.D by A38,SETFAM_1:def 1;
        then m in h.B /\ h.C by XBOOLE_0:def 3;
        hence thesis by A47,XBOOLE_0:def 3;
      end;
      then
A48:  (h.B /\ h.C) /\ h.D = x by A40,XBOOLE_0:def 10;
      set m=h.B /\ h.C;
      m<>{} by A36,A46;
      then
A49:  not (m in {{}}) by TARSKI:def 1;
      m in INTERSECTION(B,C) by A37,SETFAM_1:def 5;
      then m in INTERSECTION(B,C) \ {{}} by A49,XBOOLE_0:def 4;
      then m in B '/\' C by PARTIT1:def 4;
      then x in INTERSECTION(B '/\' C,D) by A37,A48,SETFAM_1:def 5;
      then x in INTERSECTION(B '/\' C,D) \ {{}} by A39,XBOOLE_0:def 4;
      hence thesis by PARTIT1:def 4;
    end;
    then '/\' (G \ {A}) = B '/\' C '/\' D by A14,XBOOLE_0:def 10;
    hence thesis by BVFUNC_2:def 7;
  end;
end;

theorem Th8:
  G={A,B,C,D} & A<>B & B<>C & B<>D implies CompF(B,G) = A '/\' C '/\' D
proof
  {A,B,C,D} = {B,A,C,D} by ENUMSET1:108;
  hence thesis by Th7;
end;

