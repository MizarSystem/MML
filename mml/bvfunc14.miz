      end;
      then x = Intersect F by A34,XBOOLE_0:def 10;
      hence thesis by A31,A37,A27,BVFUNC_2:def 1;
    end;
    CompF(A,G)='/\' (G \ {A}) by BVFUNC_2:def 7;
    hence thesis by A25,A8,XBOOLE_0:def 10;
