   ((e1 - e2)*(e3 - e4))/(([*2,0*]*<i>*<i>)*[*2,0*]) by XCMPLX_1:4
 .=((e1 + e2)*(e3 + e4))/([*2,0*]*[*2,0*]) -
   ((e1 - e2)*(e3 - e4))/([*2,0*]*(-1r)*[*2,0*]) by COMPLEX1:37,XCMPLX_1:4
 .=((e1 + e2)*(e3 + e4))/([*2,0*]*[*2,0*]) -
