 .=((e1 + e2)*(e3 + e4))/([*2,0*]*[*2,0*]) -
   - ((e1 - e2)*(e3 - e4))/([*2,0*]*[*2,0*]) by XCMPLX_1:189
 .=((e1 + e2)*(e3 + e4))/([*2,0*]*[*2,0*]) +
   ((e1 - e2)*(e3 - e4))/([*2,0*]*[*2,0*]) by XCMPLX_1:151
 .=((e1+e2)*(e3+e4) + (e1-e2)*(e3-e4))/([*2,0*]*[*2,0*]) by XCMPLX_1:63
 .=((e1+e2)*e3+(e1+e2)*e4 + (e1-e2)*(e3-e4))
   /([*2,0*]*[*2,0*]) by XCMPLX_1:8
 .=(e1*e3+e2*e3 + (e1+e2)*e4 + (e1-e2)*(e3-e4))
