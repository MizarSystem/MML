 .=( e1*e3+e1*e3 -(-e2*e4+e1*e4- e1*e4-e2*e4))/([*2,0*]*[*2,0*]) by XCMPLX_0:
def 8
 .=( e1*e3+e1*e3 -(-e2*e4-e2*e4))/([*2,0*]*[*2,0*]) by XCMPLX_1:26
 .=( e1*e3+e1*e3 -(-e2*e4)+e2*e4)/([*2,0*]*[*2,0*]) by XCMPLX_1:37
