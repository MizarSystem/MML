A4: z in X and
A5: x >= z and
A6: y >= z by A1,A2,A3;
    x"/\"y >= z by A5,A6,YELLOW_0:23;
    hence thesis by A4,Def20;
  end;
  assume
A7: for x,y being Element of L st x in X & y in X holds x"/\"y in X;
  let x,y be Element of L;
