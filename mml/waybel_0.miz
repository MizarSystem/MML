    thus I c= uparrow x
    proof
      let y be set;
      assume
A3:   y in I;
      then reconsider y as Element of L;
      x <= y by A2,A3,LATTICE3:def 8;
      hence thesis by Th18;
    end;
    let z be set;
    assume
A4: z in uparrow x;
    then reconsider z as Element of L;
