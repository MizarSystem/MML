       then [:X, T:] = [:X, Sigma InclPoset the topology of Y:] &
       g is continuous by Th10,YELLOW12:36;
      hence *graph f is open Subset of [:X, Y:] by A1;
     end;
    then 4_10_4[Y] by Lm4;
