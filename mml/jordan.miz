    end;
    consider f being Function of the carrier of X, the carrier of X such that
A7: for x being set st x in the carrier of X holds P[x,f.x]
    from FUNCT_2:sch 1(A3);
    reconsider f as Function of X, X;
    take f;
    let x be Point of X;
    thus thesis by A7;
  end;
  uniqueness
  proof
    let f, g be Function of X, X such that
A8: for x being Point of X
    ex y being Point of TOP-REAL n st x = y & f.x = HC(y,p,o,r) and
A9: for x being Point of X
    ex y being Point of TOP-REAL n st x = y & g.x = HC(y,p,o,r);
    now
      let x be set such that
A10:  x in the carrier of X;
A11:  ex y being Point of TOP-REAL n st x = y & f.x = HC(y,p,o,r) by A8,A10;
      ex y being Point of TOP-REAL n st x = y & g.x = HC(y,p,o,r) by A9,A10;
      hence f.x = g.x by A11;
    end;
    hence thesis by FUNCT_2:18;
  end;
end;

theorem Th66:
  for o, p being Point of TOP-REAL 2,
  r being positive (real number) st p in Ball(o,r) holds
  RotateCircle(o,r,p) is continuous
proof
  let o, p be Point of TOP-REAL 2;
  let r be positive (real number) such that
A1: p in Ball(o,r);
  set D = Tdisk(o,r);
  set cB = cl_Ball(o,r);
  set Bp = Sphere(o,r);
  set OK = [:{p},Bp:];
  set D1 = T2|{p};
  set D2 = T2|Bp;
  set S1 = Tcircle(o,r);
A2: D2 = S1 by TOPREALB:def 6;
A3: Ball(o,r) misses Sphere(o,r) by TOPREAL9:19;
A4: p in {p} by TARSKI:def 1;
A5: Bp c= cB by TOPREAL9:17;
A6: Ball(o,r) c= cB by TOPREAL9:16;
A7: the carrier of D = cB by BROUWER:3;
A8: the carrier of D1 = {p} by PRE_TOPC:29;
A9: the carrier of D2 = Bp by PRE_TOPC:29;
  set TD = [:T2,T2:] | OK;
  set gg = RotateCircle(o,r,p);
  set xo = diffX2_1(o);
  set yo = diffX2_2(o);
  set dx = diffX1_X2_1;
  set dy = diffX1_X2_2;
  set fx2 = Proj2_1;
  set fy2 = Proj2_2;
  reconsider rr = r^2 as Real by XREAL_0:def 1;
  set f1 = (the carrier of [:T2,T2:]) --> rr;
  reconsider f1 as continuous RealMap of [:T2,T2:] by Lm6;
  set Zf1 = f1 | OK;
  set Zfx2 = fx2 | OK;
  set Zfy2 = fy2 | OK;
  set Zdx = dx | OK;
  set Zdy = dy | OK;
  set Zxo = xo | OK;
  set Zyo = yo | OK;
  set xx = Zxo(#)Zdx;
  set yy = Zyo(#)Zdy;
  set m = Zdx(#)Zdx + Zdy(#)Zdy;
A10: the carrier of TD = OK by PRE_TOPC:29;
  A11: for
 y being Point of D1, z being Point of D2 holds Zdx. [y,z] = dx. [y,z]
  proof
    let y be Point of D1;
    let z be Point of D2;
    [y,z] in OK by A8,A9,ZFMISC_1:def 2;
    hence thesis by FUNCT_1:72;
  end;
  A12: for
 y being Point of D1, z being Point of D2 holds Zdy. [y,z] = dy. [y,z]
  proof
    let y be Point of D1;
    let z be Point of D2;
    [y,z] in OK by A8,A9,ZFMISC_1:def 2;
    hence thesis by FUNCT_1:72;
  end;
  A13: for
 y being Point of D1, z being Point of D2 holds Zfx2. [y,z] = fx2. [y,z
  ]
  proof
    let y be Point of D1;
    let z be Point of D2;
    [y,z] in OK by A8,A9,ZFMISC_1:def 2;
    hence thesis by FUNCT_1:72;
  end;
  A14: for
 y being Point of D1, z being Point of D2 holds Zfy2. [y,z] = fy2. [y,z
  ]
  proof
    let y be Point of D1;
    let z be Point of D2;
    [y,z] in OK by A8,A9,ZFMISC_1:def 2;
    hence thesis by FUNCT_1:72;
  end;
  A15: for
 y being Point of D1, z being Point of D2 holds Zf1. [y,z] = f1. [y,z]
  proof
    let y be Point of D1;
    let z be Point of D2;
    [y,z] in OK by A8,A9,ZFMISC_1:def 2;
    hence thesis by FUNCT_1:72;
  end;
  A16: for
 y being Point of D1, z being Point of D2 holds Zxo. [y,z] = xo. [y,z]
  proof
    let y be Point of D1;
    let z be Point of D2;
    [y,z] in OK by A8,A9,ZFMISC_1:def 2;
    hence thesis by FUNCT_1:72;
  end;
  A17: for
 y being Point of D1, z being Point of D2 holds Zyo. [y,z] = yo. [y,z]
  proof
    let y be Point of D1;
    let z be Point of D2;
    [y,z] in OK by A8,A9,ZFMISC_1:def 2;
    hence thesis by FUNCT_1:72;
  end;
  now
    let b be real number;
    assume b in rng m;
    then consider a being set such that
A18: a in dom m and
A19: m.a = b by FUNCT_1:def 5;
    consider y, z being set such that
A20: y in {p} and
A21: z in Bp and
A22: a = [y,z] by A10,A18,ZFMISC_1:def 2;
A23: y = p by A20,TARSKI:def 1;
    reconsider y, z as Point of T2 by A20,A21;
A24: y <> z by A1,A3,A21,A23,XBOOLE_0:3;
A25: [y,z] = [[y,z]`1,[y,z]`2] by Lm5,MCART_1:23;
    then
A26: y = [y,z]`1 by ZFMISC_1:33;
A27: z = [y,z]`2 by A25,ZFMISC_1:33;
A28: dx. [y,z] = [y,z]`1`1 - [y,z]`2`1 by Def3;
A29: dy. [y,z] = [y,z]`1`2 - [y,z]`2`2 by Def4;
    set r1 = y`1-z`1;
    set r2 = y`2-z`2;
A30: Zdx. [y,z] = dx. [y,z] by A8,A9,A11,A20,A21;
A31: Zdy. [y,z] = dy. [y,z] by A8,A9,A12,A20,A21;
A32: m. [y,z] = (Zdx(#)Zdx). [y,z] + (Zdy(#)Zdy). [y,z] by A18,A22,VALUED_1:1
      .= Zdx. [y,z] * Zdx. [y,z] + (Zdy(#)Zdy). [y,z] by VALUED_1:5
      .= r1^2+r2^2 by A26,A27,A28,A29,A30,A31,VALUED_1:5;
    now
      assume
A33:  r1^2+r2^2 = 0;
      then
A34:  r1 = 0 by COMPLEX1:2;
      r2 = 0 by A33,COMPLEX1:2;
      hence contradiction by A24,A34,TOPREAL3:11;
    end;
    hence 0 < b by A19,A22,A32;
  end;
  then reconsider m as positive-yielding continuous RealMap of TD
  by PARTFUN3:def 1;
  set p1 = (xx+yy)(#)(xx+yy);
  set p2 = Zxo(#)Zxo + Zyo(#)Zyo - Zf1;
A35: dom p2 = the carrier of TD by FUNCT_2:def 1;
  now
    let b be real number;
    assume b in rng p2;
    then consider a being set such that
A36: a in dom p2 and
A37: p2.a = b by FUNCT_1:def 5;
    consider y, z being set such that
A38: y in {p} and
A39: z in Bp and
A40: a = [y,z] by A10,A36,ZFMISC_1:def 2;
    reconsider y, z as Point of T2 by A38,A39;
    set r3 = z`1-o`1, r4 = z`2-o`2;
A41: Zf1. [y,z] = f1. [y,z] by A8,A9,A15,A38,A39;
A42: Zxo. [y,z] = xo. [y,z] by A8,A9,A16,A38,A39;
A43: Zyo. [y,z] = yo. [y,z] by A8,A9,A17,A38,A39;
    [y,z] = [[y,z]`1,[y,z]`2] by Lm5,MCART_1:23;
    then
A44: z = [y,z]`2 by ZFMISC_1:33;
A45: xo. [y,z] = [y,z]`2`1 - o`1 by Def1;
A46: yo. [y,z] = [y,z]`2`2 - o`2 by Def2;
A47: p2. [y,z] = (Zxo(#)Zxo + Zyo(#)Zyo). [y,z] - Zf1. [y,z]
    by A36,A40,VALUED_1:13
      .= (Zxo(#)Zxo + Zyo(#)Zyo). [y,z] - r^2 by A41,FUNCOP_1:13
      .= (Zxo(#)Zxo). [y,z] + (Zyo(#)Zyo). [y,z] - r^2 by A36,A40,VALUED_1:1
      .= Zxo. [y,z] * Zxo. [y,z] + (Zyo(#)Zyo). [y,z] - r^2 by VALUED_1:5
      .= r3^2+r4^2-r^2 by A42,A43,A44,A45,A46,VALUED_1:5;
    |. z-o .| <= r by A5,A39,TOPREAL9:8;
    then
A48: |. z-o .|^2 <= r^2 by SQUARE_1:77;
    |. z-o .|^2 = ((z-o)`1)^2+((z-o)`2)^2 by JGRAPH_1:46
      .= r3^2+((z-o)`2)^2 by TOPREAL3:8
      .= r3^2+r4^2 by TOPREAL3:8;
    then r3^2+r4^2-r^2 <= r^2-r^2 by A48,XREAL_1:11;
    hence 0 >= b by A37,A40,A47;
  end;
  then reconsider p2 as nonpositive-yielding continuous RealMap of TD
  by PARTFUN3:def 3;
  set pp = p1 - m(#)p2;
  set k = (-(xx+yy) + sqrt(pp)) / m;
  set x3 = Zfx2 + k(#)Zdx;
  set y3 = Zfy2 + k(#)Zdy;
  reconsider X3 = x3, Y3 = y3 as Function of TD,R^1 by TOPMETR:24;
  set F = <:X3,Y3:>;
  set R = R2Homeomorphism;
A49: for x being Point of D2 holds gg.x = (R*F). [p,x]
  proof
    let x be Point of D2;
    consider y being Point of T2 such that
A50: x = y and
A51: gg.x = HC(y,p,o,r) by A1,A2,Def8;
A52: x <> p by A1,A3,A9,XBOOLE_0:3;
A53: [p,y] in OK by A4,A9,A50,ZFMISC_1:def 2;
    set r1 = p`1-y`1, r2 = p`2-y`2, r3 = y`1-o`1, r4 = y`2-o`2;
    set l = (-(r3*r1+r4*r2)+sqrt((r3*r1+r4*r2)^2-(r1^2+r2^2)*(r3^2+r4^2-r^2)))
    / (r1^2+r2^2);
A54: [p,y] = [[p,y]`1,[p,y]`2] by Lm5,MCART_1:23;
    then
A55: p = [p,y]`1 by ZFMISC_1:33;
A56: y = [p,y]`2 by A54,ZFMISC_1:33;
A57: fx2. [p,y] = [p,y]`2`1 by Def5;
A58: fy2. [p,y] = [p,y]`2`2 by Def6;
A59: dx. [p,y] = [p,y]`1`1 - [p,y]`2`1 by Def3;
A60: dy. [p,y] = [p,y]`1`2 - [p,y]`2`2 by Def4;
A61: xo. [p,y] = [p,y]`2`1 - o`1 by Def1;
A62: yo. [p,y] = [p,y]`2`2 - o`2 by Def2;
A63: dom X3 = the carrier of TD by FUNCT_2:def 1;
A64: dom Y3 = the carrier of TD by FUNCT_2:def 1;
A65: dom pp = the carrier of TD by FUNCT_2:def 1;
A66: p is Point of D1 by A8,TARSKI:def 1;
    then
A67: Zdx. [p,y] = dx. [p,y] by A11,A50;
A68: Zdy. [p,y] = dy. [p,y] by A12,A50,A66;
A69: Zf1. [p,y] = f1. [p,y] by A15,A50,A66;
A70: Zxo. [p,y] = xo. [p,y] by A16,A50,A66;
A71: Zyo. [p,y] = yo. [p,y] by A17,A50,A66;
A72: m. [p,y] = (Zdx(#)Zdx). [p,y] + (Zdy(#)Zdy). [p,y] by A10,A53,VALUED_1:1
      .= Zdx. [p,y] * Zdx. [p,y] + (Zdy(#)Zdy). [p,y] by VALUED_1:5
      .= r1^2+r2^2 by A55,A56,A59,A60,A67,A68,VALUED_1:5;
A73: xx. [p,y] = Zxo. [p,y] * Zdx. [p,y] by VALUED_1:5;
A74: yy. [p,y] = Zyo. [p,y] * Zdy. [p,y] by VALUED_1:5;
A75: (xx+yy). [p,y] = xx. [p,y] + yy. [p,y] by A10,A53,VALUED_1:1;
    then
A76: p1. [p,y] = (r3*r1+r4*r2)^2
    by A55,A56,A59,A60,A61,A62,A67,A68,A70,A71,A73,A74,VALUED_1:5;
A77: p2. [p,y] = (Zxo(#)Zxo + Zyo(#)Zyo). [p,y] - Zf1. [p,y]
    by A10,A35,A53,VALUED_1:13
      .= (Zxo(#)Zxo + Zyo(#)Zyo). [p,y] - r^2 by A69,FUNCOP_1:13
      .= (Zxo(#)Zxo). [p,y] + (Zyo(#)Zyo). [p,y] - r^2 by A10,A53,VALUED_1:1
      .= Zxo. [p,y] * Zxo. [p,y] + (Zyo(#)Zyo). [p,y] - r^2 by VALUED_1:5
      .= r3^2+r4^2-r^2 by A56,A61,A62,A70,A71,VALUED_1:5;
    dom sqrt pp = the carrier of TD by FUNCT_2:def 1;
    then
A78: sqrt(pp). [p,y] = sqrt(pp. [p,y]) by A10,A53,PARTFUN3:def 5
      .= sqrt(p1. [p,y] - (m(#)p2). [p,y]) by A10,A53,A65,VALUED_1:13
      .= sqrt((r3*r1+r4*r2)^2-(r1^2+r2^2)*(r3^2+r4^2-r^2))
    by A72,A76,A77,VALUED_1:5;
    dom k = the carrier of TD by FUNCT_2:def 1;
    then
A79: k. [p,y] = (-(xx+yy) + sqrt(pp)). [p,y] * (m. [p,y])" by A10,A53,
RFUNCT_1:def 4
      .= (-(xx+yy) + sqrt(pp)). [p,y] / m. [p,y] by XCMPLX_0:def 9
      .= ((-(xx+yy)). [p,y] + sqrt(pp). [p,y]) / (r1^2+r2^2)
    by A10,A53,A72,VALUED_1:1
      .= l by A55,A56,A59,A60,A61,A62,A67,A68,A70,A71,A73,A74,A75,A78,
VALUED_1:8;
A80: X3. [p,y] = Zfx2. [p,y] + (k(#)Zdx). [p,y] by A10,A53,VALUED_1:1
      .= y`1 + (k(#)Zdx). [p,y] by A13,A50,A56,A57,A66
      .= y`1+l*r1 by A55,A56,A59,A67,A79,VALUED_1:5;
A81: Y3. [p,y] = Zfy2. [p,y] + (k(#)Zdy). [p,y] by A10,A53,VALUED_1:1
      .= y`2 + (k(#)Zdy). [p,y] by A14,A50,A56,A58,A66
      .= y`2+l*r2 by A55,A56,A60,A68,A79,VALUED_1:5;
    y in the carrier of D2 by A50;
    hence gg.x = |[ y`1+l*r1, y`2+l*r2 ]| by A1,A5,A6,A7,A9,A50,A51,A52,
BROUWER:8
      .= R. [X3. [p,y], Y3. [p,y]] by A80,A81,TOPREALA:def 2
      .= R.(F. [p,y]) by A10,A53,A63,A64,FUNCT_3:69
      .= (R*F). [p,x] by A10,A50,A53,FUNCT_2:21;
