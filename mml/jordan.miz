A84: V1 is open and
A85: V1 /\ [#]S1 = V by A82,TOPS_2:32;
A86: gg.pp = (R*F). [pp,p] by A46;
    R" is being_homeomorphism by TOPREALA:56,TOPS_2:70;
    then
A87: R" .:V1 is open by A84,TOPGRP_1:25;
A88: dom F = the carrier of [:T2,T2:] | OK by FUNCT_2:def 1;
A89: dom R = the carrier of [:R^1,R^1:] by FUNCT_2:def 1;
    then
A90: rng F c= dom R;
    then
A91: dom (R*F) = dom F by RELAT_1:46;
A92: rng R = [#]T2 by TOPREALA:56,TOPS_2:def 5;
A93: R"*(R*F) = R"*R*F by RELAT_1:55
      .= id dom R*F by A92,TOPREALA:56,TOPS_2:65;
    dom id dom R = dom R by RELAT_1:71;
    then
A94: dom (id dom R*F) = dom F by A90,RELAT_1:46;
    for x being set st x in dom F holds (id dom R*F).x = F.x
    proof
      let x be set such that
A95:  x in dom F;
A96:  F.x in rng F by A95,FUNCT_1:def 5;
      thus (id dom R*F).x = id dom R.(F.x) by A95,FUNCT_1:23
        .= F.x by A89,A96,FUNCT_1:35;
    end;
    then
A97: id dom R*F = F by A94,FUNCT_1:9;
    (R*F). [p1,fp] in V1 by A81,A85,A86,XBOOLE_0:def 4;
    then R" .((R*F). [p1,fp]) in R" .:V1 by FUNCT_2:43;
    then (R"*(R*F)). [p1,fp] in R" .:V1 by A6,A83,A88,A91,FUNCT_1:23;
