    take XX;
    pp in X1 by A105,A107,ZFMISC_1:106;
    hence pp in XX by XBOOLE_0:def 4;
    thus XX is open;
    let b be set;
    assume b in gg.:XX;
    then consider a being Point of D1 such that
A109: a in XX and
A110: b = gg.a by FUNCT_2:116;
    reconsider a1 = a, fa = fp as Point of T2 by PRE_TOPC:55;
A111: a in X1 by A109,XBOOLE_0:def 4;
A112: [a,p] in OK by A2,A4,ZFMISC_1:def 2;
    fa in Y1 by A105,A107,ZFMISC_1:106;
    then [a,fa] in Z by A107,A111,ZFMISC_1:def 2;
