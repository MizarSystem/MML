theorem Th93:
  P is compact &
  |[-1,0]|,|[1,0]| realize-max-dist-in P & A is_inside_component_of P implies
  A c= closed_inside_of_rectangle(-1,1,-3,3)
proof
  assume that
A1: P is compact and
A2: |[-1,0]|,|[1,0]| realize-max-dist-in P and
A3: A is_inside_component_of P;
  let x be set;
  assume that
A4: x in A and
A5: not x in R;
  P c= R by A2,Th71;
  then
A6: R` c= P` by SUBSET_1:31;
  reconsider x as Point of T2 by A4;
A7: not (rl <= x`1 & x`1 <= rp & rd <= x`2 & x`2 <= rg) by A5;
  per cases;
  suppose
A8: 0 <= x`1;
    set E = east_halfline(x);
    E c= R`
    proof
      let e be set;
      assume
A9:   e in E;
      then reconsider e as Point of T2;
A10:  e`1 >= x`1 by A9,TOPREAL1:def 13;
      now
        assume e in R;
        then ex p st e = p & rl <= p`1 & p`1 <= rp & rd <= p`2 & p`2 <= rg;
        hence contradiction by A7,A8,A9,A10,TOPREAL1:def 13,XXREAL_0:2;
      end;
      hence thesis by SUBSET_1:50;
    end;
    then E c= P` by A6,XBOOLE_1:1;
    then E misses P by SUBSET_1:43;
    then
A11: E c= UBD P by A1,JORDAN2C:135;
    x in E by TOPREAL1:45;
    then A meets UBD P by A4,A11,XBOOLE_0:3;
    hence thesis by A3,Th14;
  end;
  suppose
A12: x`1 < 0;
    set E = west_halfline(x);
    E c= R`
    proof
      let e be set;
      assume
A13:  e in E;
      then reconsider e as Point of T2;
A14:  e`1 <= x`1 by A13,TOPREAL1:def 15;
      now
        assume e in R;
        then ex p st e = p & rl <= p`1 & p`1 <= rp & rd <= p`2 & p`2 <= rg;
        hence contradiction by A7,A12,A13,A14,TOPREAL1:def 15,XXREAL_0:2;
      end;
      hence thesis by SUBSET_1:50;
    end;
    then E c= P` by A6,XBOOLE_1:1;
    then E misses P by SUBSET_1:43;
    then
A15: E c= UBD P by A1,JORDAN2C:134;
    x in E by TOPREAL1:45;
    then A meets UBD P by A4,A15,XBOOLE_0:3;
    hence thesis by A3,Th14;
  end;
end;

Lm89: p in R implies R c= Ball(p,10)
proof
  assume p in R;
  then consider p1 such that
A1: p1 = p and
A2: rl <= p1`1 and
A3: p1`1 <= rp and
A4: rd <= p1`2 and
A5: p1`2 <= rg;
  let x be set;
  assume
A6: x in R;
  then reconsider x as Point of T2;
  consider p2 such that
A7: p2 = x and
A8: rl <= p2`1 and
A9: p2`1 <= rp and
A10: rd <= p2`2 and
A11: p2`2 <= rg by A6;
A12: ex s, t being Point of Euclid 2 st
  s = p1 & t = p2 & dist(p1,p2) = dist(s,t) by TOPREAL6:def 1;
  dist(p1,p2) <= (rp-rl) + (rg-rd) by A2,A3,A4,A5,A8,A9,A10,A11,TOPREAL6:104;
  then dist(p1,p2) < 10 by XXREAL_0:2;
  then |.x-p.| < 10 by A1,A7,A12,SPPOL_1:62;
  hence thesis by TOPREAL9:7;
end;

theorem
  |[-1,0]|,|[1,0]| realize-max-dist-in C implies LSeg(|[0,3]|,|[0,-3]|) meets C
proof
  assume
A1: a,b realize-max-dist-in C;
  set Jc = Upper_Arc C;
  consider Pf being Path of c,d, f being Function
  of I[01], T2|LSeg(c,d) such that
A2: rng f = LSeg(c,d) and
A3: Pf = f by Th43;
A4: a = W-min C by A1,Th79;
  b = E-max C by A1,Th80;
  then Jc is_an_arc_of a,b by A4,JORDAN6:def 8;
  then consider Pg being Path of a,b, g being Function
  of I[01], T2|Jc such that
A5: rng g = Jc and
A6: Pg = g by Th42;
A7: Jc c= C by JORDAN6:76;
A8: C c= R by A1,Th71;
A9: a in C by A1,JORDAN24:def 1;
A10: b in C by A1,JORDAN24:def 1;
A11: the carrier of TR = R by PRE_TOPC:29;
  reconsider AR = a, BR = b, CR = c, DR = d
  as Point of TR by A8,A9,A10,Lm62,Lm63,Lm67,PRE_TOPC:29;
  rng Pg c= the carrier of TR by A5,A6,A7,A8,A11,XBOOLE_1:1;
  then reconsider h = Pg as Path of AR,BR by Th30;
  LSeg(c,d) c= R by Lm62,Lm63,Lm67,JORDAN1:def 1;
  then reconsider v = Pf as Path of CR,DR by A2,A3,A11,Th30;
  consider s, t being Point of I[01] such that
A12: h.s = v.t by Lm16,Lm17,Lm21,Lm23,JGRAPH_8:6;
A13: dom h = the carrier of I[01] by FUNCT_2:def 1;
  dom v = the carrier of I[01] by FUNCT_2:def 1;
  then
A14: v.t in rng Pf by FUNCT_1:def 5;
  h.s in rng Pg by A13,FUNCT_1:def 5;
  hence thesis by A2,A3,A5,A6,A7,A12,A14,XBOOLE_0:3;
end;

Lm90: |[-1,0]|,|[1,0]| realize-max-dist-in C implies
ex Jc, Jd being compact with_the_max_arc Subset of T2 st
Jc is_an_arc_of |[-1,0]|,|[1,0]| & Jd is_an_arc_of |[-1,0]|,|[1,0]| &
C = Jc \/ Jd & Jc /\ Jd = {|[-1,0]|,|[1,0]|} & UMP C in Jc & LMP C in Jd &
W-bound C = W-bound Jc & E-bound C = E-bound Jc
proof
  assume
A1: a,b realize-max-dist-in C;
  set U = Upper_Arc C;
  set L = Lower_Arc C;
A2: U \/ L = C by JORDAN6:def 9;
A3: UMP C in C by JORDAN21:43;
  LMP C in C by JORDAN21:44;
  then
A4: LMP C in U or LMP C in L by A2,XBOOLE_0:def 3;
A5: W-min C = a by A1,Th79;
A6: E-max C = b by A1,Th80;
  per cases by A2,A3,XBOOLE_0:def 3;
  suppose
A7: UMP C in U;
    take U, L;
    thus thesis by A4,A5,A6,A7,JORDAN21:26,27,63,JORDAN6:65;
  end;
  suppose
A8: UMP C in L;
    take L, U;
    thus thesis by A4,A5,A6,A8,JORDAN21:28,29,62,JORDAN6:65;
  end;
end;

theorem Th95:
  |[-1,0]|,|[1,0]| realize-max-dist-in C implies
  for Jc, Jd being compact with_the_max_arc Subset of TOP-REAL 2 st
  Jc is_an_arc_of |[-1,0]|,|[1,0]| & Jd is_an_arc_of |[-1,0]|,|[1,0]| &
  C = Jc \/ Jd & Jc /\ Jd = {|[-1,0]|,|[1,0]|} & UMP C in Jc & LMP C in Jd &
  W-bound C = W-bound Jc & E-bound C = E-bound Jc
  for Ux being Subset of TOP-REAL 2 st Ux = Component_of Down ((1/2) *
  ((UMP (LSeg(LMP Jc,|[0,-3]|) /\ Jd)) + LMP Jc), C`)
  holds Ux is_inside_component_of C & for V being Subset of TOP-REAL 2 st
  V is_inside_component_of C holds V = Ux
proof
  set m = UMP C;
  set j = LMP C;
  assume
A1: a,b realize-max-dist-in C;
  let Jc, Jd being compact with_the_max_arc Subset of T2 such that
A2: Jc is_an_arc_of a,b and
A3: Jd is_an_arc_of a,b and
A4: C = Jc \/ Jd and
A5: Jc /\ Jd = {a,b} and
A6: UMP C in Jc and
A7: LMP C in Jd and
A8: W-bound C = W-bound Jc and
A9: E-bound C = E-bound Jc;
  set l = LMP Jc;
  set LJ = LSeg(l,d) /\ Jd;
  set k = UMP LJ;
  set x = (1/2)*(k+l);
  set w = (W-bound C + E-bound C) / 2;
  let Ux be Subset of TOP-REAL 2 such that
A10: Ux = Component_of Down (x,C`);
A11: C c= R by A1,Th71;
A12: W-bound C = rl by A1,Th75;
A13: E-bound C = rp by A1,Th76;
A14: a in C by A1,JORDAN24:def 1;
A15: b in C by A1,JORDAN24:def 1;
A16: m in C by JORDAN21:43;
A17: l in Jc by JORDAN21:44;
A18: Jd c= C by A4,XBOOLE_1:7;
A19: Jc c= C by A4,XBOOLE_1:7;
  then
A20: l in C by A17;
A21: m`2 < c`2 by A1,Lm21,Th83,JORDAN21:43;
A22: l`1 = 0 by A8,A9,A12,A13,EUCLID:56;
A23: c`1 = w by A1,Lm87;
A24: m`1 = w by EUCLID:56;
A25: m <> a by A12,A13,Lm16,EUCLID:56;
A26: m <> b by A12,A13,Lm17,EUCLID:56;
A27: l <> a by A8,A9,A12,A13,Lm16,EUCLID:56;
A28: l <> b by A8,A9,A12,A13,Lm17,EUCLID:56;
  then consider Pml being Path of m,l such that
A29: rng Pml c= Jc and
A30: rng Pml misses {a,b} by A2,A6,A17,A25,A26,A27,Th44;
  set ml = rng Pml;
A31: ml c= C by A19,A29,XBOOLE_1:1;
A32: j in C by A7,A18;
A33: LSeg(l,d) is vertical by A22,Lm22,SPPOL_1:37;
A34: d`2 <= j`2 by A1,A7,A18,Lm23,Th84;
A35: j`1 = 0 by A12,A13,EUCLID:56;
  l in Vertical_Line w by A12,A13,A22,JORDAN6:34;
  then
A36: l in C /\ Vertical_Line w by A17,A19,XBOOLE_0:def 4;
  then j`2 <= l`2 by JORDAN21:42;
  then j in LSeg(l,d) by A22,A34,A35,Lm22,GOBOARD7:8;
  then
A37: LJ is non empty by A7,XBOOLE_0:def 4;
A38: LJ is compact;
A39: LJ is vertical by A33,Th4;
  then
A40: k in LJ by A37,JORDAN21:43;
  then
A41: k in LSeg(l,d) by XBOOLE_0:def 4;
A42: k in Jd by A40,XBOOLE_0:def 4;
  then
A43: k in C by A18;
A44: d in LSeg(l,d) by RLTOPSP1:69;
  then
A45: k`1 = 0 by A33,A41,Lm22,SPPOL_1:def 3;
  then
A46: k <> a by EUCLID:56;
A47: k <> b by A45,EUCLID:56;
A48: j <> a by A35,EUCLID:56;
  j <> b by A35,EUCLID:56;
  then consider Pkj being Path of k,j such that
A49: rng Pkj c= Jd and
A50: rng Pkj misses {a,b} by A3,A7,A42,A46,A47,A48,Th44;
  set kj = rng Pkj;
A51: kj c= C by A18,A49,XBOOLE_1:1;
A52: x in LSeg(k,l) by RLTOPSP1:70;
A53: Component_of Down(x,C`) is a_component by CONNSP_1:43;
A54: the carrier of T2|C` = C` by PRE_TOPC:29;
A55: LSeg(l,k) is vertical by A22,A45,SPPOL_1:37;
A56: k in LSeg(l,k) by RLTOPSP1:69;
A57: l = |[l`1,l`2]| by EUCLID:57;
A58: k = |[k`1,k`2]| by EUCLID:57;
A59: d = |[d`1,d`2]| by EUCLID:57;
  d`2 <= l`2 by A1,A17,A19,Lm23,Th84;
  then
A60: k`2 <= l`2 by A22,A41,A57,A59,Lm22,JGRAPH_6:9;
A61: a <> k by A45,EUCLID:56;
  b <> k by A45,EUCLID:56;
  then not k in {a,b} by A61,TARSKI:def 2;
  then
A62: k <> l by A5,A17,A42,XBOOLE_0:def 4;
  then k`2 <> l`2 by A22,A45,TOPREAL3:11;
  then
A63: k`2 < l`2 by A60,XXREAL_0:1;
  k in Vertical_Line w by A12,A13,A45,JORDAN6:34;
  then k in C /\ Vertical_Line w by A18,A42,XBOOLE_0:def 4;
  then j`2 <= k`2 by JORDAN21:42;
  then d`2 <= k`2 by A1,A7,A18,Lm23,Th84,XXREAL_0:2;
  then
A64: LSeg(l,k) c= LSeg(l,d) by A33,A45,A55,A60,Lm22,GOBOARD7:65;
A65: LSeg(l,k) \ {l,k} c= C`
  proof
    let q be set;
    assume that
A66: q in LSeg(l,k) \ {l,k} and
A67: not q in C`;
A68: q in LSeg(l,k) by A66,XBOOLE_0:def 5;
    reconsider q as Point of T2 by A66;
A69: q in C by A67,SUBSET_1:50;
A70: q`1 = w by A12,A13,A45,A55,A56,A68,SPPOL_1:def 3;
    then
A71: q in Vertical_Line w by JORDAN6:34;
    per cases by A4,A69,XBOOLE_0:def 3;
    suppose q in Jc;
      then q in Jc /\ Vertical_Line w by A71,XBOOLE_0:def 4;
      then
A72:  l`2 <= q`2 by A8,A9,JORDAN21:42;
      q`2 <= l`2 by A22,A45,A57,A58,A60,A68,JGRAPH_6:9;
      then l`2 = q`2 by A72,XXREAL_0:1;
      then l = q by A12,A13,A22,A70,TOPREAL3:11;
      then q in {l,k} by TARSKI:def 2;
      hence contradiction by A66,XBOOLE_0:def 5;
    end;
    suppose q in Jd;
      then
A73:  q in LJ by A64,A68,XBOOLE_0:def 4;
A74:  q`1 = d`1 by A33,A44,A64,A68,SPPOL_1:def 3;
A75:  W-bound LSeg(l,d) <= W-bound LJ by A73,PSCOMP_1:132,XBOOLE_1:17;
A76:  E-bound LJ <= E-bound LSeg(l,d) by A73,PSCOMP_1:130,XBOOLE_1:17;
A77:  W-bound LJ = E-bound LJ by A37,A39,SPRECT_1:17;
A78:  W-bound LSeg(l,d) = d`1 by A22,Lm22,SPRECT_1:62;
      then W-bound LSeg(l,d) = W-bound LJ by A22,A75,A76,A77,Lm22,SPRECT_1:65;
      then q in Vertical_Line ((W-bound LJ + E-bound LJ) / 2)
      by A74,A77,A78,JORDAN6:34;
      then q in LJ /\ Vertical_Line ((W-bound LJ + E-bound LJ) / 2)
      by A73,XBOOLE_0:def 4;
      then
A79:  q`2 <= k`2 by JORDAN21:41;
      k`2 <= q`2 by A22,A45,A57,A58,A60,A68,JGRAPH_6:9;
      then k`2 = q`2 by A79,XXREAL_0:1;
      then k = q by A12,A13,A45,A70,TOPREAL3:11;
      then q in {l,k} by TARSKI:def 2;
      hence contradiction by A66,XBOOLE_0:def 5;
    end;
  end;
  then reconsider X = LSeg(l,k) \ {l,k} as Subset of T2|C` by PRE_TOPC:29;
  now
    assume x in {l,k};
    then x = l or x = k by TARSKI:def 2;
    hence contradiction by A62,Th1;
  end;
  then
A80: x in LSeg(l,k) \ {l,k} by A52,XBOOLE_0:def 5;
  then Component_of(x,C`) = Component_of Down(x,C`) by A65,CONNSP_3:27;
  then
A81: x in Component_of Down(x,C`) by A65,A80,CONNSP_3:26;
  then
A82: X meets Ux by A10,A80,XBOOLE_0:3;
  LSeg(l,k) \ {l,k} is connected by JORDAN1:52;
  then X is connected by CONNSP_1:24;
  then
A83: X c= Component_of Down(x,C`) by A10,A53,A82,CONNSP_1:38;
A84: LSeg(l,k) c= R by A11,A20,A43,JORDAN1:def 1;
A85: the carrier of TR = R by PRE_TOPC:29;
  reconsider AR = a, BR = b, CR = c, DR = d
  as Point of TR by A11,A14,A15,Lm62,Lm63,Lm67,PRE_TOPC:29;
  consider Pcm being Path of c,m, fcm being Function of I[01], T2|LSeg(c,m)
  such that
A86: rng fcm = LSeg(c,m) and
A87: Pcm = fcm by Th43;
A88: LSeg(c,m) c= R by A11,A16,Lm62,Lm67,JORDAN1:def 1;
A89: ml c= R by A11,A31,XBOOLE_1:1;
  thus Ux is_inside_component_of C
  proof
    thus
A90: Ux is_a_component_of C` by A10,A53,CONNSP_1:def 6;
    assume not Ux is Bounded;
    then not Ux c= Ball(x,10) by JORDAN2C:16;
    then consider u being set such that
A91: u in Ux and
A92: not u in Ball(x,10) by TARSKI:def 3;
A93: R c= Ball(x,10) by A52,A84,Lm89;
    reconsider u as Point of T2 by A91;
A94: Ux is open by A90,SPRECT_3:18;
    Component_of Down(x,C`) is connected by A53,CONNSP_1:def 5;
    then
A95: Ux is connected by A10,CONNSP_1:24;
    x in Ball(x,10) by Th16;
    then consider P1 being Subset of T2 such that
A96: P1 is_S-P_arc_joining x,u and
A97: P1 c= Ux by A10,A81,A91,A92,A94,A95,TOPREAL4:30;
A98: P1 is_an_arc_of x,u by A96,TOPREAL4:3;
    reconsider P2 = P1 as Subset of T2|C` by A10,A97,XBOOLE_1:1;
A99: P2 c= Component_of Down(x,C`) by A10,A97;
A100: P2 misses C by A54,SUBSET_1:43;
    then
A101: P2 misses Jc by A4,XBOOLE_1:7,63;
A102: P2 misses Jd by A4,A100,XBOOLE_1:7,63;
A103: x`1 = 1/2*((k+l)`1) by TOPREAL3:9
      .= 1/2*(k`1+l`1) by TOPREAL3:7
      .= 0 by A22,A45;
    then
A104: LSeg(d,x) is vertical by Lm22,SPPOL_1:37;
A105: x = |[x`1,x`2]| by EUCLID:57;
A106: x`2 < l`2 by A63,Th3;
A107: k`2 < x`2 by A63,Th2;
    then
A108: d`2 <= x`2 by A1,A18,A42,Lm23,Th84,XXREAL_0:2;
    d`1 = d`1;
    then
A109: LSeg(d,x) c= LSeg(d,l) by A33,A104,A106,A108,GOBOARD7:65;
A110: LSeg(d,x) misses Jc
    proof
      assume not thesis;
      then consider q being set such that
A111: q in LSeg(d,x) and
A112: q in Jc by XBOOLE_0:3;
      reconsider q as Point of T2 by A111;
      q`2 <= x`2 by A59,A103,A105,A108,A111,Lm22,JGRAPH_6:9;
      then
A113: q`2 < l`2 by A106,XXREAL_0:2;
      q`1 = 0 by A33,A44,A109,A111,Lm22,SPPOL_1:def 3;
      then q in Vertical_Line w by A12,A13,JORDAN6:34;
      then q in Jc /\ Vertical_Line w by A112,XBOOLE_0:def 4;
      hence contradiction by A8,A9,A113,JORDAN21:42;
    end;
    set n = First_Point(P1,x,u,dR);
A114: not u in R by A92,A93;
A115: Fr R = dR by Th52;
    u in P1 by A98,TOPREAL1:4;
    then
A116: P1 \ R <> {}T2 by A114,XBOOLE_0:def 5;
    x in P1 by A98,TOPREAL1:4;
    then P1 meets R by A52,A84,XBOOLE_0:3;
    then
A117: P1 meets dR by A98,A115,A116,CONNSP_1:23,JORDAN6:11;
    P1 is closed by A96,JORDAN6:12,TOPREAL4:3;
    then
A118: n in P1 /\ dR by A98,A117,JORDAN5C:def 1;
    then
A119: n in dR by XBOOLE_0:def 4;
A120: n in P1 by A118,XBOOLE_0:def 4;
    set alpha = Segment(P1,x,u,x,n);
A121: rd < k`2 by A1,A18,A42,Th84;
    l`2 <= m`2 by A36,JORDAN21:41;
    then x`2 < m`2 by A106,XXREAL_0:2;
    then not x in dR by A21,A103,A105,A107,A121,Lm86;
    then
A122: alpha is_an_arc_of x,n by A96,A119,A120,JORDAN16:39,TOPREAL4:3;
A123: alpha misses Jc by A101,JORDAN16:5,XBOOLE_1:63;
A124: alpha misses Jd by A102,JORDAN16:5,XBOOLE_1:63;
    consider Pdx being Path of d,x,
    fdx being Function of I[01], T2|LSeg(d,x) such that
A125: rng fdx = LSeg(d,x) and
A126: Pdx = fdx by Th43;
    consider PJc being Path of a,b, fJc being Function
    of I[01], T2|Jc such that
A127: rng fJc = Jc and
A128: PJc = fJc by A2,Th42;
    consider PJd being Path of a,b, fJd being Function
    of I[01], T2|Jd such that
A129: rng fJd = Jd and
A130: PJd = fJd by A3,Th42;
    consider Palpha being Path of x,n,
    falpha being Function of I[01], T2|alpha such that
A131: rng falpha = alpha and
A132: Palpha = falpha by A122,Th42;
    n in R by A119,Lm67;
    then
A133: ex p st p = n & rl <= p`1 & p`1 <= rp & rd <= p`2 & p`2 <= rg;
    rng PJc c= the carrier of TR by A11,A19,A85,A127,A128,XBOOLE_1:1;
    then reconsider h = PJc as Path of AR,BR by Th30;
    rng PJd c= the carrier of TR by A11,A18,A85,A129,A130,XBOOLE_1:1;
    then reconsider H = PJd as Path of AR,BR by Th30;
A134: LSeg(d,x) c= R by A52,A84,Lm63,Lm67,JORDAN1:def 1;
A135: alpha c= R by A52,A84,A96,A114,Th57,TOPREAL4:3;
A136: ld in LSeg(ld,lg) by RLTOPSP1:69;
A137: pd in LSeg(pd,pg) by RLTOPSP1:69;
    LSeg(lg,c) misses C by A1,Lm78;
    then
A138: LSeg(lg,c) misses Jc by A4,XBOOLE_1:7,63;
A139: LSeg(lg,c) c= R by Lm67,Lm70,XBOOLE_1:1;
A140: LSeg(pg,c) c= R by Lm67,Lm71,XBOOLE_1:1;
    LSeg(pg,c) misses C by A1,Lm79;
    then
A141: LSeg(pg,c) misses Jc by A4,XBOOLE_1:7,63;
    consider Plx being Path of l,x, flx being Function of I[01], T2|LSeg(l,x)
    such that
A142: rng flx = LSeg(l,x) and
A143: Plx = flx by Th43;
    set PCX = Pcm + Pml + Plx;
A144: rng PCX = rng Pcm \/ rng Pml \/ rng Plx by Th40;
A145: ml misses Jd
    proof
      assume ml meets Jd;
      then consider q being set such that
A146: q in ml and
A147: q in Jd by XBOOLE_0:3;
      q in {a,b} by A5,A29,A146,A147,XBOOLE_0:def 4;
      hence contradiction by A30,A146,XBOOLE_0:3;
    end;
A148: LSeg(c,m) /\ C = {m} by A1,Th91;
A149: LSeg(c,m) misses Jd
    proof
      assume LSeg(c,m) meets Jd;
      then consider q being set such that
A150: q in LSeg(c,m) and
A151: q in Jd by XBOOLE_0:3;
      q in {m} by A18,A148,A150,A151,XBOOLE_0:def 4;
      then q = m by TARSKI:def 1;
      then m in {a,b} by A5,A6,A151,XBOOLE_0:def 4;
      hence contradiction by A25,A26,TARSKI:def 2;
    end;
    LSeg(l,x) is vertical by A22,A103,SPPOL_1:37;
    then
A152: LSeg(l,x) c= LSeg(l,k) by A45,A55,A103,A106,A107,GOBOARD7:65;
    l in LSeg(l,x) by RLTOPSP1:69;
    then {l} c= LSeg(l,x) by ZFMISC_1:37;
    then
A153: LSeg(l,x) = LSeg(l,x) \ {l} \/ {l} by XBOOLE_1:45;
    LSeg(l,x) \ {l} c= LSeg(l,k) \ {l,k}
    proof
      let q be set;
      assume
A154: q in LSeg(l,x) \ {l};
      then
A155: q in LSeg(l,x) by ZFMISC_1:64;
A156: q <> l by A154,ZFMISC_1:64;
      q <> k by A22,A57,A103,A105,A106,A107,A155,JGRAPH_6:9;
      then not q in {l,k} by A156,TARSKI:def 2;
      hence thesis by A152,A155,XBOOLE_0:def 5;
    end;
    then LSeg(l,x) \ {l} c= C` by A65,XBOOLE_1:1;
    then LSeg(l,x) \ {l} misses C by SUBSET_1:43;
    then
A157: LSeg(l,x) \ {l} misses Jd by A4,XBOOLE_1:7,63;
    {l} misses Jd
    proof
      assume {l} meets Jd;
      then l in Jd by ZFMISC_1:56;
      then l in {a,b} by A5,A17,XBOOLE_0:def 4;
      hence thesis by A27,A28,TARSKI:def 2;
    end;
    then LSeg(l,x) misses Jd by A153,A157,XBOOLE_1:70;
    then
A158: rng PCX misses Jd by A86,A87,A142,A143,A144,A145,A149,XBOOLE_1:114;
