    then a in P` by XBOOLE_0:def 4;
    then not a in P by XBOOLE_0:def 5;
    hence thesis by A4,A9,A11,A14,A29,ZFMISC_1:64;
  end;
  f|F1 is Function of F1,A by A13,A16,FUNCT_2:38;
  then reconsider f1 = f|F1 as Function
  of T1,T2B1 by A16,A23,A25,A26,FUNCT_2:9;
  reconsider g1 = id F3 as Function of T3,T2B1 by A24,A25,A28,FUNCT_2:9;
A30: F1 = [#]T1 by PRE_TOPC:29;
A31: F3 = [#]T3 by PRE_TOPC:29;
A32: [#]T1 \/ [#]T3 = [#]D
  proof
    thus [#]T1 \/ [#]T3 c= [#]D by A30,A31,XBOOLE_1:8;
    let p be set;
    assume
A33: p in [#]D;
    per cases;
    suppose p in P;
      then p in F1 by A19,A33,XBOOLE_0:def 4;
