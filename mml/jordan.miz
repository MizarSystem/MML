A105: [p1,fp] in Z and
A106: Z in SF by A103,TARSKI:def 4;
    consider X1, Y1 being Subset of T2 such that
A107: Z = [:X1,Y1:] and
A108: X1 is open and Y1 is open by A104,A106;
    set ZZ = Z /\ [#]TD;
