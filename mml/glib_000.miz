registration
  cluster trivial non-Dmulti -> finite GraphSeq;
  coherence
  proof
    let GS be GraphSeq;
    assume GS is trivial & GS is non-Dmulti;
    then reconsider GS9=GS as trivial non-Dmulti GraphSeq;
    for x being Nat holds GS9.x is finite;
    hence thesis by Def60;
