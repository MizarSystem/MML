registration
  cluster trivial loopless -> finite GraphSeq;
  coherence
  proof
    let GS be GraphSeq;
    assume GS is trivial & GS is loopless;
    then reconsider GS9=GS as trivial loopless GraphSeq;
    for x being Nat holds GS9.x is finite;
    hence thesis by Def60;
  end;
