    m <= k implies ex g st g = (PR.m)`1 & Ant(g) |= Suc(g) by A8,A9,A11,A10,
XXREAL_0:2;
    hence thesis by A12,A14,NAT_1:8;
  end;
A67: P[0];
  for k holds P[k] from NAT_1:sch 1(A67,A7);
  then consider g such that
A68: g = (PR.(len PR))`1 and
A69: Ant(g) |= Suc(g) by A6;
  let A,J,v;
