A28:    k in dom PR1 and
A29:    n = len PR + k;
A30:    1 <= k by A28,FINSEQ_3:25;
A31:    k <= len PR1 by A28,FINSEQ_3:25;
        then n <= len PR1 + len PR by A29,XREAL_1:6;
        then
A32:    n <= len (PR^PR1) by FINSEQ_1:22;
