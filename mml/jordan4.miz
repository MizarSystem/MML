GOBOARD5:def 4;
                hence LSeg(f,(i1-'1)+j1) /\ LSeg(f,j2)={} by XBOOLE_0:def 7;
              end;
              case
                (i1-'1)+j1 < len f-'1;
                then (i1-'1)+j1+1 < len f-'1+1 by XREAL_1:8;
                then LSeg(f,(i1-'1)+j1) misses LSeg(f,j2) by A28,A99,
GOBOARD5:def 4;
                hence LSeg(f,(i1-'1)+j1) /\ LSeg(f,j2)={} by XBOOLE_0:def 7;
              end;
            end;
            hence contradiction by A67,A68,A80,A81,A79,A91,XBOOLE_0:def 4;
