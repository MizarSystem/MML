A35: f/.1=f/.len f by FINSEQ_6:def 1;
A36: 1+1<=len f by A9,NAT_1:13;
A37: 1<=len f1 by A20,NAT_1:11;
A38: now
    per cases by A14,XXREAL_0:1;
    case
A39:  i2>1;
      union { LSeg((f/^(i1-'1)),i) : 1 <= i & i+1 <= len (f/^(i1-'1)) }
