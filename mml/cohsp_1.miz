      let x be set; assume x in f.a /\ f.b;
then A16:     x in f.a & x in f.b by XBOOLE_0:def 3;
A17:     a c= a \/ b & b c= a \/ b by XBOOLE_1:7;
       then f.a c= f.(a \/ b) by A13,A15,Def12;
