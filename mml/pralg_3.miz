then A41:  dom (const(o,product Cs)) = Ji by Th4
                               .= e by A39,FUNCT_1:35;
 now
    let i1 be set; assume
A42:  i1 in e;
      then reconsider ii = i1 as Element of Ji by A39,FUNCT_1:35;
