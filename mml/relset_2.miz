theorem :: (23)
  R.:^X = R.:^( R~.:^(R.:^X) ) & R~.:^Y = (R~).:^(R.:^((R~).:^Y))
proof
  thus R.:^X = R.:^( R~.:^(R.:^X) )
  proof
    thus R.:^X c= R.:^( R~.:^(R.:^X) ) by TH28;
    X c= (R~).:^(R.:^X) by TH28;
    hence R.:^( R~.:^(R.:^X) ) c= R.:^X by TH11;
  end;
  thus R~.:^Y c= (R~).:^(R.:^((R~).:^Y)) by TH28;
  Y c= R.:^((R~).:^Y) by TH28;
  hence (R~).:^(R.:^((R~).:^Y)) c= R~.:^Y by TH11;
end;

