    end;
  end;
  suppose (.:R).:{_{X2}_} <> {};
    then
A5: y in meet ((.:R).:{_{X2}_}) by A2,SETFAM_1:def 10;
    per cases;
    suppose (.:R).:{_{X1}_} = {};
      then Intersect((.:R).:{_{X1}_}) = B by SETFAM_1:def 10;
      hence thesis by A2;
    end;
    suppose
A6:   (.:R).:{_{X1}_} <> {};
      (.:R).:{_{X1}_} c= (.:R).:{_{X2}_} by A1,RELAT_1:156;
      then meet ((.:R).:{_{X2}_}) c= meet ((.:R).:{_{X1}_}) by A6,
