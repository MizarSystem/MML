A28: rng h c= dom R1;
    let n be Nat;
    R is total by FDIFF_1:def 3;
    then dom R = REAL by PARTFUN1:def 4;
    then
A29: rng h c= dom R;
A30: n in NAT by ORDINAL1:def 13;
    then ex x st x in N & x in N1 & h.n = x-x0 by A22;
    then r*(h.n) + R.(h.n) = diff(SVF1(1,f,z),x2)*(h.n) + R1.(h.n) by A25;
    then
A31: (r*(h.n))/(h.n) + (R.(h.n))/(h.n) = (diff(SVF1(1,f,z),x2)*(h.n) + R1.
    (h.n))/(h.n) by XCMPLX_1:63;
A32: (R.(h.n))/(h.n) = (R.(h.n))*(h.n)" by XCMPLX_0:def 9
      .= (R.(h.n))*(h".n) by VALUED_1:10
      .= ((R/*h).n)*(h".n) by A30,A29,FUNCT_2:185
      .= ((h")(#)(R/*h)).n by A30,SEQ_1:12;
    h is non-zero by FDIFF_1:def 1;
    then
A33: h.n <> 0 by A30,SEQ_1:7;
A34: (R1.(h.n))/(h.n) = (R1.(h.n))*(h.n)" by XCMPLX_0:def 9
      .= (R1.(h.n))*(h".n) by VALUED_1:10
      .= ((R1/*h).n)*(h".n) by A30,A28,FUNCT_2:185
      .= ((h")(#)(R1/*h)).n by A30,SEQ_1:12;
A35: (diff(SVF1(1,f,z),x2)*(h.n))/(h.n) = diff(SVF1(1,f,z),x2)*((h.n)/(h.n
    )) by XCMPLX_1:75
      .= diff(SVF1(1,f,z),x2)*1 by A33,XCMPLX_1:60
      .= diff(SVF1(1,f,z),x2);
    (r*(h.n))/(h.n) = r*((h.n)/(h.n)) by XCMPLX_1:75
      .= r*1 by A33,XCMPLX_1:60
      .= r;
    then
    r + (R.(h.n))/(h.n) = diff(SVF1(1,f,z),x2) + (R1.(h.n))/(h.n) by A31,
    A35,XCMPLX_1:63;
    then r = diff(SVF1(1,f,z),x2) + (((h")(#)(R1/*h)).n - ((h")(#)(R/*h)).n)
    by A32,A34;
    hence r - diff(SVF1(1,f,z),x2) = (((h")(#)(R1/*h))-((h")(#)(R/*h))).n by
    A30,RFUNCT_2:6;
  end;
  then ((h")(#)(R1/*h))-((h")(#)(R/*h)) is constant & (((h")(#)(R1/*h))-((h")
  (#)(R /*h))).1 = r-diff(SVF1(1,f,z),x2) by VALUED_0:def 18;
  then
A36: lim (((h")(#)(R1/*h))-((h")(#)(R/*h))) = r-diff(SVF1(1,f,z),x2) by
  SEQ_4:40;
A37: (h")(#)(R1/*h) is convergent & lim ((h")(#)(R1/*h)) = 0 by FDIFF_1:def
  3;
  (h")(#)(R/*h) is convergent & lim ((h")(#)(R/*h)) = 0 by FDIFF_1:def 3;
  then r-diff(SVF1(1,f,z),x2) = 0-0 by A36,A37,SEQ_2:26;
  hence thesis by A1,A10,FINSEQ_1:98;
end;

