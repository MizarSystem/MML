(k > n implies seq1.k=0.X) and
A2: for k holds
(k <= n implies seq2.k = (z ExpSeq).k * (Partial_Sums(w ExpSeq).n
                          -Partial_Sums(w ExpSeq).(n-'k)) )&
(k > n implies seq2.k=0.X);
   now let k;
     per cases;
     suppose A3:k <= n;
     hence seq1.k =(z ExpSeq).k * (Partial_Sums(w ExpSeq).n
              -Partial_Sums(w ExpSeq).(n-'k)) by A1
           .= seq2.k by A2,A3;
     end;
     suppose A4:k > n;
     hence seq1.k = 0.X by A1
           .= seq2.k by A2,A4;
     end;
end;
hence seq1=seq2 by FUNCT_2:113;
end;
end;

theorem Th14:
 z ExpSeq.(n+1) = (1/(n+1) * z) * (z ExpSeq.n)  &
 z ExpSeq.0=1_ X  &
||.(z ExpSeq).n.|| <= (||.z.|| ExpSeq ).n
proof
 A1:
  now
  let n be Nat;
 thus (z ExpSeq).(n+1)=1/((n+1) !)*z #N (n+1) by Def2
               .=1/((n+1)!)*(z GeoSeq).(n+1)  by LOPBAN_3:def 14
               .=1/((n+1)!)*((z GeoSeq).n * z) by LOPBAN_3:def 13
               .=1/ ((n+1)!  )* (z #N n * z) by LOPBAN_3:def 14
               .=1/(n!  * (n+1))*(z #N n * z) by NEWTON:21
               .=((1*1)/((n!)  * (n+1)))*(z* z #N n ) by Lm1
               .=((1/(n!))  * (1/(n+1))  )*(z* z #N n )
             by XCMPLX_1:77
               .=( (1/(n+1))*z)*((1/(n!)) * z #N n  ) by LOPBAN_3:43
               .=((1 /(n+1)) *z)*(z ExpSeq.n) by Def2;
 end;
 A2:(z ExpSeq).0=1/(0!) * z #N 0 by Def2
            .=1/(0! )*(z GeoSeq).0 by LOPBAN_3:def 14
            .=1/1 *1_ X by LOPBAN_3:def 13,NEWTON:18
            .=1_ X by RLVECT_1:def 9;
  defpred X[Nat] means
     ||. ((z ExpSeq)).$1 .|| <=( ||.z.|| ExpSeq ).$1;
     ( ||.z.|| ExpSeq ).0 =  ((||.z.||) |^ 0) / (0!) by SIN_COS:def 9
            .= 1/(0!) by NEWTON:9
            .= 1/((Prod_real_n).0) by SIN_COS:def 7
            .= 1/1 by SIN_COS:def 5
            .= 1;
 then A3: X[0] by A2,LOPBAN_3:43;
 A4:
 for n be Nat st X[n] holds X[n+1]
   proof
     let n such that
   A5:  X[n];
    0 <= n+1 by NAT_1:18;
   then A6:abs(n+1)  =n+1 by ABSVALUE:def 1;
   A7:     ||. ((z ExpSeq)).(n+1) .||
      =||. (1/(n+1) *z)*  (z ExpSeq.n ) .|| by A1;
   A8:
     ||. (1/(n+1) *z)*  (z ExpSeq.n ) .||
      <=   ||. (1/(n+1) *z) .|| * ||. (z ExpSeq.n ) .||
            by LOPBAN_3:43;
   A9:
      ||. (1/(n+1) *z) .||
      =abs(1/(n+1)) *  ||.z.|| by NORMSP_1:def 2
     .= 1/(n+1) *  ||.z.|| by A6,ABSVALUE:15;
     0<=   ||. (1/(n+1) *z) .|| by NORMSP_1:8;
then A10:||. (1/(n+1) *z) .|| * ||. (z ExpSeq.n ) .||
       <=
         ||. (1/(n+1) *z) .|| * (||.z.|| ExpSeq ).n
       by A5,AXIOMS:25;
      1/(n+1) *  ||.z.||  *  (||.z.|| ExpSeq ).n
    =  1/(n+1) * ( (||.z.|| ExpSeq ).n *  ||.z.|| )
    .= 1/(n+1) * (  ((||.z.||)|^n)/(n!) *||.z.|| )
        by SIN_COS:def 9
    .=1/(n+1) * (  ((||.z.||)|^n)*||.z.|| /(n!) )
  by XCMPLX_1:75
     .=1/(n+1) * (  (||.z.||)|^(n+1) /(n!) ) by NEWTON:11
    .= ((||.z.||)|^ (n+1)) /((n)!*(n+1))
  by XCMPLX_1:104
   .= ((||.z.||)|^ (n+1)) /((n+1)!) by SIN_COS:2
    .=( ||.z.|| ExpSeq ).(n+1) by SIN_COS:def 9;
  hence X[n+1] by A7,A8,A9,A10,AXIOMS:22;
 end;
    for n holds X[n] from NAT_1:sch 1(A3,A4);
 hence thesis by A1,A2;
 end;

theorem Th15:
0 < k implies (Sift(seq)).k=seq.(k -' 1)
proof
 assume
  A1:  0 < k;
  then A2:  0+1 <= k by INT_1:20;
  consider m such that
  A3:   m+1=k by A1,NAT_1:22;
  A4: m=k-1 by A3;
  thus (Sift(seq)).k=seq.m by A3,Def5
             .=seq.(k -' 1) by A2,A4,SCMFSA_7:3;
end;

theorem Th16:
Partial_Sums(seq).k=Partial_Sums(Sift(seq)).k+seq.k
proof
  defpred X[Nat] means Partial_Sums(seq).$1=Partial_Sums(Sift(seq)).$1+seq.$1;
  Partial_Sums(seq).0=seq.0 by LOPBAN_3:def 1
                           .= 0.X + seq.0 by RLVECT_1:10
                   .=(Sift(seq)).0 + seq.0 by Def5
                   .=Partial_Sums(Sift(seq)).0+seq.0 by LOPBAN_3:def 1;
   then A1: X[0];
A2: for k st X[k] holds X[k+1]
  proof let k such that
 A3:   Partial_Sums(seq).k=Partial_Sums(Sift(seq)).k+seq.k;
 thus Partial_Sums(seq).(k+1) = (Partial_Sums(Sift(seq)).k+seq.k) + seq.(k+1)
 by A3,LOPBAN_3:def 1
      .=(Partial_Sums(Sift(seq)).k+(Sift(seq)).(k+1)) + seq.(k+1) by Def5
      .=Partial_Sums(Sift(seq)).(k+1)+seq.(k+1) by LOPBAN_3:def 1;
 end;
    for k holds X[k] from NAT_1:sch 1(A1,A2);
  hence thesis;
 end;

theorem Th17:
for z,w st z,w are_commutative holds
(z+w) #N n = Partial_Sums(Expan(n,z,w)).n
proof
let z,w such that
  A1: z,w are_commutative;

  A2:(z+w) #N 0 =((z+w) GeoSeq).0 by LOPBAN_3:def 14
            .=1_ X by LOPBAN_3:def 13;
  A3:0-'0=0-0 by BINARITH:def 3
              .=0;
  defpred X[Nat] means (z+w) #N $1 = Partial_Sums(Expan($1,z,w)).$1;
      Partial_Sums(Expan(0,z,w)).0 = Expan(0,z,w).0 by LOPBAN_3:def 1
                .= (Coef(0)).0 * (z #N 0) * (w #N 0) by A3,Def6
                .= 1/(1 * 1) * z #N 0 * w #N 0 by A3,Def3,NEWTON:18
                .= z #N 0 * w #N 0 by RLVECT_1:def 9
                .= (z GeoSeq).0 * w #N 0 by LOPBAN_3:def 14
                .= (z GeoSeq).0 * (w GeoSeq).0 by LOPBAN_3:def 14
                .= 1_ X * (w GeoSeq).0 by LOPBAN_3:def 13
                .= 1_ X  * 1_ X by LOPBAN_3:def 13
                .= 1_ X by LOPBAN_3:43;
  then A4: X[0] by A2;
  A5: for n st X[n] holds X[n+1]
   proof let n such that
   A6: (z+w) #N n = Partial_Sums(Expan(n,z,w)).n;
   A7: (z+w) #N (n+1) =((z+w) GeoSeq ).(n+1) by LOPBAN_3:def 14
                .=((z+w) GeoSeq ).n * (z+w) by LOPBAN_3:def 13
                .=(Partial_Sums(Expan(n,z,w)).n) * (z+w) by A6,LOPBAN_3:def 14

           .=(Partial_Sums(Expan(n,z,w))*(z+w)) .n by LOPBAN_3:def 10
             .=(Partial_Sums(Expan(n,z,w)*(z+w))).n by Th9;
        Expan(n,z,w)*(z+w) =( Expan(n,z,w)*z)+(Expan(n,z,w)*w)
     proof
         now let k be Nat;
      thus ( Expan(n,z,w)*(z+w) ).k= Expan(n,z,w).k *(z+w)
                  by LOPBAN_3:def 10
               .= Expan(n,z,w).k * z + Expan(n,z,w).k *w by LOPBAN_3:43
               .=( Expan(n,z,w)*z).k+ Expan(n,z,w).k *w by LOPBAN_3:def 10
               .=(Expan(n,z,w)*z).k+(Expan(n,z,w)*w).k by LOPBAN_3:def 10
               .=( (Expan(n,z,w)*z)+(Expan(n,z,w)*w)).k by NORMSP_1:def 5;
      end;
     hence thesis by FUNCT_2:113;
     end;
     then A8:(z+w) #N (n+1) =( Partial_Sums( (Expan(n,z,w)*z))
        +Partial_Sums(( (Expan(n,z,w)*w)))).n by A7,LOPBAN_3:20
                    .= Partial_Sums((Expan(n,z,w)*z)).n
                      +Partial_Sums((Expan(n,z,w)*w)).n by NORMSP_1:def 5;
     A9:Partial_Sums(( Expan(n,z,w)*z)).n
         =Partial_Sums((Expan(n,z,w)*z)).(n+1)
         proof
          A10: Partial_Sums((Expan(n,z,w)*z)).(n+1)
          =Partial_Sums((Expan(n,z,w)*z)).n+(Expan(n,z,w)*z).(n+1)
 by LOPBAN_3:def 1
          .=Partial_Sums((Expan(n,z,w)*z)).n
           + Expan(n,z,w).(n+1) *z by LOPBAN_3:def 10;
              n < n+1 & n+1 is Nat by REAL_1:69;
           hence Partial_Sums(( Expan(n,z,w)*z)).(n+1)
            =Partial_Sums((Expan(n,z,w)*z)).n
            +0.X *z by A10,Def6
            .=Partial_Sums((Expan(n,z,w)*z)).n
             +0.X by LOPBAN_3:43
            .=Partial_Sums((Expan(n,z,w)*z)).n by LOPBAN_3:43;
         end;

     A11:Partial_Sums(( Expan(n,z,w)*w )).n
         =Partial_Sums(( Expan(n,z,w)*w )).(n+1)
         proof
          A12: Partial_Sums((Expan(n,z,w)*w)).(n+1)
          =Partial_Sums((Expan(n,z,w)*w)).n+(Expan(n,z,w)*w).(n+1)
 by LOPBAN_3:def 1
          .=Partial_Sums((Expan(n,z,w)*w)).n
           + Expan(n,z,w).(n+1) *w by LOPBAN_3:def 10;
              n < n+1 & n+1 is Nat by REAL_1:69;
           hence Partial_Sums(( Expan(n,z,w)*w)).(n+1)
            =Partial_Sums((Expan(n,z,w)*w)).n
            +0.X *w by A12,Def6
            .=Partial_Sums((Expan(n,z,w)*w)).n
             +0.X by LOPBAN_3:43
            .=Partial_Sums((Expan(n,z,w)*w)).n by LOPBAN_3:43;
         end;

     A13:Partial_Sums(( Expan(n,z,w)*z)).(n+1)
         =Partial_Sums(Sift(( Expan(n,z,w)*z))).(n+1)
        proof
        A14:Partial_Sums(( Expan(n,z,w)*z)).(n+1)
          =Partial_Sums(Sift(( Expan(n,z,w)*z))).(n+1)
          +( Expan(n,z,w)*z).(n+1) by Th16;
              0 +n < n+1 by REAL_1:69;
then A15:        Expan(n,z,w).(n+1)=0.X by Def6;
           thus Partial_Sums(( Expan(n,z,w)*z)).(n+1)
             =Partial_Sums(Sift(( Expan(n,z,w)*z))).(n+1)
              + Expan(n,z,w).(n+1)*z by A14,LOPBAN_3:def 10
            .=Partial_Sums(Sift(( Expan(n,z,w)*z))).(n+1)
             +0.X by A15,LOPBAN_3:43
        .= Partial_Sums(Sift((Expan(n,z,w)*z))).(n+1)
          by LOPBAN_3:43;
         end;
    A16:( Expan(n,z,w)*w)+Sift(( Expan(n,z,w)*z))
         =Expan(n+1,z,w)
     proof
        now let k be Nat;
      A17: now assume
       A18:  n+1 < k;
      A19:  n < k & 1 <= k & 1 <k & 0 <k & n <k-1 & n < k-'1
             proof
                 0 <= n by NAT_1:18;
then A20:       0+1 <= n+1 by AXIOMS:24;
A21:            n+1-1 < k -1 by A18,REAL_1:54;
then A22:          n+0 < k-1+1 by REAL_1:67;
                 1 <= k by A18,A20,AXIOMS:22;
            hence thesis by A18,A20,A21,A22,AXIOMS:22,SCMFSA_7:3;
       end;
           (( Expan(n,z,w)*w)+Sift(( Expan(n,z,w)*z))).k
     =(Expan(n,z,w)*w).k + (Sift((Expan(n,z,w)*z))).k by NORMSP_1:def 5
     .=(Expan(n,z,w).k*w) + (Sift(( Expan(n,z,w)*z))).k by LOPBAN_3:def 10
       .=(Expan(n,z,w).k*w) +  ((Expan(n,z,w)*z)).(k -' 1) by A19,Th15
       .=(Expan(n,z,w).k*w) + ((Expan(n,z,w).(k -' 1)*z))
               by LOPBAN_3:def 10
       .=( 0.X*w )+ ((Expan(n,z,w).(k -' 1))*z) by A19,Def6
       .= 0.X+  ((Expan(n,z,w).(k -' 1))*z) by LOPBAN_3:43
       .=0.X + ( 0.X*z) by A19,Def6
       .=0.X + 0.X by LOPBAN_3:43
       .=0.X by LOPBAN_3:43;
        hence (( Expan(n,z,w)*w)+Sift((Expan(n,z,w)*z))).k
         =Expan(n+1,z,w).k by A18,Def6;
      end;
         now assume
       A23: k <= (n+1);
       A24: now assume
            A25: k=n+1;
      thus (( Expan(n,z,w)*w)
      +Sift((Expan(n,z,w)*z))).k=Expan(n+1,z,w).k
              proof
              A26: n < n+1 by REAL_1:69;
               A27: n-'n= n-n by SCMFSA_7:3
                           .= 0;
              A28: n+1-'(n+1)= n+1-(n+1) by SCMFSA_7:3
                               .= 0;
              A29: n! <>0 & (n+1)! <>0 by NEWTON:23;
              A30: (Coef(n)).(n) = n! /((n!  ) * (0!  )) by A27,Def3
                              .= 1 by A29,NEWTON:18,XCMPLX_1:60;

              A31: (Coef(n+1)).(n+1) = (n+1)!  /(((n+1)!  ) * (0!  ))
              by A28,Def3
                              .= 1 by A29,NEWTON:18,XCMPLX_1:60;
              thus (( Expan(n,z,w)*w)+Sift((Expan(n,z,w)*z))).k =
       (Expan(n,z,w)*w).(n+1)+(Sift((Expan(n,z,w)*z ))).(n+1)
 by A25,NORMSP_1:def 5
             .=( Expan(n,z,w).(n+1) *w) + (Sift((Expan(n,z,w)*z))).(n+1)
 by LOPBAN_3:def 10
             .=( 0.X*w  ) + (Sift(( Expan(n,z,w)*z))).(n+1) by A26,Def6
             .=(  0.X ) + (Sift(( Expan(n,z,w)*z))).(n+1) by LOPBAN_3:43
             .= (Sift((Expan(n,z,w)*z ))).(n+1) by LOPBAN_3:43
             .= (( Expan(n,z,w)*z)).(n) by Def5
             .=  (Expan(n,z,w)).n *z by LOPBAN_3:def 10
             .=((Coef(n)).n * (z #N n) * (w #N (n-' n))) *z by Def6
             .=(Coef(n)).n * (z #N n) * ((w #N (n-' n)) *z) by LOPBAN_3:43
             .=(Coef(n)).n * (z #N n) * ( z*(w #N (n-' n))) by A1,Lm2
             .=(Coef(n)).n * (z #N n) *  z*(w #N (n-' n)) by LOPBAN_3:43
           .=(Coef(n)).n *( (z #N n) *  z)*(w #N (n-' n)) by LOPBAN_3:43
    .= ((Coef(n+1)).(n+1) * (z #N (n+1)) * (w #N (n-' n)))
     by A30,A31,Lm1
             .= Expan(n+1,z,w).k by A25,A27,A28,Def6;
              end;
          end;
          now assume
           A32: k < n+1;
            A33: now assume
             A34: k=0;
            thus (( Expan(n,z,w)*w)+Sift(( Expan(n,z,w)*z))).k
              =Expan(n+1,z,w).k
              proof
              A35: 0 <= n by NAT_1:18;
              A36: 0 <= n+1 by NAT_1:18;
   A37:         n-'0 =n-0 by A35,SCMFSA_7:3;
   A38:         n+1-'0 =n+1-0 by A36,SCMFSA_7:3;
              A39: n!  <>0 & (n+1)!  <>0 by NEWTON:23;
               A40: (Coef(n)).0 = n!  /((0!  ) * (n!  )) by A35,A37,Def3
                              .= 1 by A39,NEWTON:18,XCMPLX_1:60;
               A41: (Coef(n+1)).0 = (n+1)!  /((0!  ) * ((n+1)!  ))
              by A36,A38,Def3
                         .= 1 by A39,NEWTON:18,XCMPLX_1:60;
              thus
                (( Expan(n,z,w)*w)+Sift(( Expan(n,z,w)*z))).k =
              ( Expan(n,z,w)*w).0+(Sift((Expan(n,z,w)*z))).0
 by A34,NORMSP_1:def 5
             .=(Expan(n,z,w).0 *w) + (Sift(( Expan(n,z,w)*z))).0
 by LOPBAN_3:def 10
             .=( Expan(n,z,w).0 *w ) + 0.X by Def5
             .=( Expan(n,z,w).0 *w) by LOPBAN_3:43
             .= ((Coef(n)).0 * (z #N 0)   * (w #N (n-' 0)))*w
           by A35,Def6
             .= (Coef(n)).0 *(z #N 0)   *((w #N (n-' 0))*w)
               by LOPBAN_3:43
            .= (Coef(n+1)).0  *(z #N 0)* (w #N ((n+1)-'0))
           by A37,A38,A40,A41,Lm1
             .= Expan(n+1,z,w).k by A34,A36,Def6;
              end;
            end;
              now assume A42: k<>0;
              then A43: 0<k by NAT_1:19;
              then A44: 0+1 <= k by INT_1:20;
              A45:k+1-1 <= n+1-1 by A32,INT_1:20;
                 k < k+1 by REAL_1:69;
              then k-1 <= k+1-1 by REAL_1:49;
              then k-1 <= n by A45,AXIOMS:22;
              then A46: k-'1 <= n by A44,SCMFSA_7:3;
              thus (( Expan(n,z,w)*w)+Sift((Expan(n,z,w)*z))).k
              =Expan(n+1,z,w).k
              proof
              A47:
              (( Expan(n,z,w)*w )+Sift(( Expan(n,z,w)*z))).k
              =( Expan(n,z,w)*w).k+(Sift((Expan(n,z,w)*z))).k
 by NORMSP_1:def 5
            .=(Expan(n,z,w).k)*w + (Sift(( Expan(n,z,w)*z))).k
 by LOPBAN_3:def 10
             .=(Expan(n,z,w).k)*w + (Expan(n,z,w)*z).( k-'1) by A43,Th15
             .=(Expan(n,z,w).k)*w  + ( Expan(n,z,w).(k-'1))*z
 by LOPBAN_3:def 10
             .=( (Coef(n)).k * (z #N k) * (w #N (n-' k)) ) *w
               + ( Expan(n,z,w).(k-'1))*z by A45,Def6
             .=(((Coef(n)).k * (z #N k) * (w #N (n-' k)))) *w
               + ( ((Coef(n)).(k -' 1) * (z #N (k-'1))
                    * (w #N (n-' (k -' 1)))))*z by A46,Def6
             .=((Coef(n)).k * ((z #N k)) * ((w #N (n-' k))*w))
               + ( ((Coef(n)).(k -' 1)) * (z #N (k -' 1))
                    * (w #N (n-' (k-'1)))) *z
             by LOPBAN_3:43
             .=((Coef(n)).k * ((z #N k)) * ((w #N ((n-' k)+1))))
               + ( ((Coef(n)).(k -' 1)) * (z #N (k -' 1))
                    * (w #N (n-' (k-'1)))) *z
             by Lm1
             .=((Coef(n)).k * ((z #N k)) * ((w #N ((n-' k)+1))))
               +  ((Coef(n)).(k -' 1)) * (z #N (k -' 1))
                    *((w #N (n-' (k-'1))) *z)
             by LOPBAN_3:43
             .=((Coef(n)).k * ((z #N k)) * ((w #N ((n-' k)+1))))
               +  ((Coef(n)).(k -' 1)) * (z #N (k -' 1))
                    *(z*(w #N (n-' (k-'1))))
             by A1,Lm2
             .=((Coef(n)).k * ((z #N k)) * ((w #N ((n-' k)+1))))
                  +  ((Coef(n)).(k -' 1)) * (z #N (k -' 1))
                       *z*(w #N (n-' (k-'1)))
                by LOPBAN_3:43
            .=((Coef(n)).k * ((z #N k)) * ((w #N ((n-' k)+1))))
           +  ((Coef(n)).(k -' 1)) *((z #N (k -' 1))*z) *(w #N (n-' (k-'1)))
                by LOPBAN_3:43
            .=((Coef(n)).k * ((z #N k)) * ((w #N ((n-' k)+1))))
              +  ((Coef(n)).(k -' 1)) *(z #N ((k -' 1)+1)) *(w #N (n-' (k-'1)))
            by Lm1;
              A48:
             w #N ((n-' k)+1) = w #N (n+1-'k)
              proof
               (n-'k)+1=n-k+1 by A45,SCMFSA_7:3
                      .=n+1-k
                      .=(n+1-'k) by A32,SCMFSA_7:3;
              hence w #N ((n-' k)+1) = w #N (n+1-'k);
              end;
              A49: w #N (n-' (k-'1))
               =w #N  (n+1-'k)
              proof
                (n-'(k-'1))=n-(k-'1) by A46,SCMFSA_7:3
                             .=n-(k-1) by A44,SCMFSA_7:3
                             .=n+1-k
                             .=(n+1-'k) by A32,SCMFSA_7:3;
              hence w #N (n-' (k-'1))=w #N (n+1-'k);
              end;
               z #N ((k -' 1)+1)= z #N k
              proof
               (k-'1)+1 =k-1+1 by A44,SCMFSA_7:3
                             .=k;
              hence z #N ((k -' 1)+1)= z #N k;
              end;
               then A50:
              ((Expan(n,z,w)*w)+Sift((Expan(n,z,w)*z))).k
             =(Coef(n)).k * ((z #N k) * (w #N (n+1-'k)))
              +  (Coef(n)).((k -' 1)) *(z #N k) *(w #N (n+1-'k))
               by A47,A48,A49,LOPBAN_3:43
            .=(Coef(n)).k * ((z #N k) * (w #N (n+1-'k)))
              +  (Coef(n)).((k -' 1)) *((z #N k) *(w #N (n+1-'k)))
                              by LOPBAN_3:43
              .= ((Coef(n)).k +(Coef(n)).(k -' 1) )
                 * ((z #N k) * (w #N ((n+1) -' k) ))
               by LOPBAN_3:43;
              A51: (Coef(n)).k +(Coef(n)).(k -' 1)
              =n!  /( (k!  ) * ((n -' k)!  ) )
              +(Coef(n)).(k-'1) by A45,Def3
              .=n!  /( (k!  ) * ((n -' k)!  ) )
               +n!  /(((k-'1)!  ) * (((n-'(k-'1)))!  )) by A46,Def3;
              A52: ( (k!  ) * ((n -' k)!  ) ) * (n+1 - k)
               =(k!  ) * (((n -' k)!  ) * (n+1 - k));
              A53: (n-'(k-'1)) =n-(k-'1) by A46,SCMFSA_7:3
                      .=n-(k-1) by A44,SCMFSA_7:3
                      .=n+1-k
                      .=n+1-'k by A32,SCMFSA_7:3;
              A54: (((k-'1)! ) * (((n-'(k-'1)))! )) * k
               =k * ((k-'1)! ) * (((n-'(k-'1)))! )
                .=(k! ) * ((n+1-'k)! ) by A43,A53,Th13;
              (k! ) <> 0 & (n-'k)! <>0 & (k-'1)! <>0
                      & ((n-'(k-'1)))! <>0 & ((n+1-'k))! <>0 &
                      (n+1-k) <>0 & k <>0
                        by A32,A42,NEWTON:23,SQUARE_1:11;
                 then A55:
                 n! /( (k! ) * ((n-'k)!) )
                  =(n! * (n+1-k)) /(( (k! ) * ((n-'k)! ) ) * (n+1-k))
                by XCMPLX_1:92
                 .=(n! * (n+1-k))/((k! ) * ((n+1-'k)! ))
 by A45,A52,Th13;
                    n! /(((k-'1)! ) * (((n-'(k-'1)))! ))
                 =(n! * k)/((k! ) * ((n+1-'k)! ))
               by A42,A54,XCMPLX_1:92;
                   then (Coef(n)).k +(Coef(n)).(k-'1)
                   =((n! * (n+1-k))+(n! * k))
                    / ((k! ) * ((n+1-'k)! )) by A51,A55,XCMPLX_1:63
                  .=( n! * (n+1-k+k)) / ((k! ) * ((n+1-'k)! ))
                  .=((n+1)! ) / ((k! ) * ((n+1-'k)! )) by NEWTON:21
                  .=(Coef(n+1)).k by A32,Def3;
               hence ((Expan(n,z,w)*w)+Sift((Expan(n,z,w)*z))).k
                  =(Coef(n+1)).k * (z #N k) * (w #N (n+1-'k) )
 by A50,LOPBAN_3:43
              .=Expan(n+1,z,w).k by A32,Def6;
              end;
            end;
            hence ((Expan(n,z,w)*w)+Sift((Expan(n,z,w)*z))).k
              =Expan(n+1,z,w).k by A33;
          end;
            hence ((Expan(n,z,w)*w)+Sift((Expan(n,z,w)*z))).k
          =Expan(n+1,z,w).k by A23,A24,REAL_1:def 5;
        end;
     hence ((Expan(n,z,w)*w)+Sift((Expan(n,z,w)*z))).k
          =Expan(n+1,z,w).k by A17;
    end;
    hence thesis by FUNCT_2:113;
   end;
 thus (z+w) #N (n+1) =(Partial_Sums(( Expan(n,z,w)*w)) +
                      Partial_Sums(Sift(( Expan(n,z,w)*z)))).(n+1)
       by A8,A9,A11,A13,NORMSP_1:def 5
   .=Partial_Sums(Expan(n+1,z,w)).(n+1) by A16,LOPBAN_3:20;
     end;
      for n holds X[n] from NAT_1:sch 1(A4,A5);
  hence thesis;
end;

theorem Th18:
  Expan_e(n,z,w)=(1/(n!)) * Expan(n,z,w)
 proof
         now
       let k be Nat;
       A1: now assume A2:n <k;
          hence Expan_e(n,z,w).k=0.X by Def7
                          .=(1/(n!)) * 0.X by LOPBAN_3:43
                          .=(1/(n!)) * Expan(n,z,w).k by A2,Def6
                          .=((1/(n!)) * Expan(n,z,w)).k by NORMSP_1:def 8;
          end;
            now assume A3:k <= n;
          A4:n!  <> 0 by NEWTON:23;
          thus
          A5:
          Expan_e(n,z,w).k
           = (Coef_e(n)).k * (z #N k) * (w #N (n -' k)) by A3,Def7
          .= 1/( (k!) * ( (n -' k)!) ) * (z #N k) * (w #N (n -' k))
        by A3,Def4;
           1/((k! ) * ((n-'k)! ))
          =(((n! ) * 1 )/(n! )) /((k! ) * ((n-'k)!)) by A4,XCMPLX_1:60
         .= (1 /(n! )) * (n! ) /((k! ) * ((n-'k)!))
      by XCMPLX_1:75;
          hence Expan_e(n,z,w).k
          = (1/(n!  )) * (n!  ) /((k!  ) * ((n -' k)!  ))
                 * ((z #N k) * (w #N (n -' k))) by A5,LOPBAN_3:43
          .= (1/(n!  )) * ( (n!  ) /((k!  ) * ((n -' k)!  )) )
                 * ((z #N k) * (w #N (n -' k))) by XCMPLX_1:75
          .= (1/(n!  )) * (( (n!  ) /((k!  ) * ((n -' k)!  )) )
                 * ((z #N k) * (w #N (n -' k)))) by LOPBAN_3:43
          .= (1/(n!  )) * ((n!  ) /((k!  ) * ((n -' k)!  ))
                 * (z #N k) * (w #N (n -' k))) by LOPBAN_3:43
          .= (1/(n!  )) * ((Coef(n)).k * ((z #N k))
            * (w #N (n -' k))) by A3,Def3
          .= (1/(n!)) * Expan(n,z,w).k by A3,Def6
          .= ( (1/(n!)) * Expan(n,z,w) ).k by NORMSP_1:def 8;
         end;
        hence Expan_e(n,z,w).k = ( (1/(n!  )) * Expan(n,z,w) ).k by A1;
       end;
     hence Expan_e(n,z,w)=((1/(n!  )) * Expan(n,z,w)) by FUNCT_2:113;
 end;

theorem Th19:
for z,w st z,w are_commutative holds
1/ (n! ) *((z+w) #N n)  = Partial_Sums(Expan_e(n,z,w)).n
proof
let z,w such that
A1: z,w are_commutative;
 thus 1/(n!)*((z+w) #N n)= 1/(n! )*(Partial_Sums(Expan(n,z,w)).n) by A1,Th17
       .= ((1/(n!  )) * (Partial_Sums(Expan(n,z,w)))).n by NORMSP_1:def 8
       .= Partial_Sums( (1/(n!  )) * Expan(n,z,w)).n by LOPBAN_3:24
       .= Partial_Sums(Expan_e(n,z,w)).n by Th18;
end;

theorem Th20:
0.X  ExpSeq is norm_summable & Sum(0.X  ExpSeq)=1_ X
proof
  defpred X[set] means Partial_Sums( ||. 0.X ExpSeq.||).$1=1;
   Partial_Sums(||.(0.X  ExpSeq).||).0
        =||. (0.X  ExpSeq) .||.0 by SERIES_1:def 1
       .=||. (0.X  ExpSeq).0 .|| by NORMSP_1:def 10
       .=||. 1_ X .|| by Th14
       .=1 by LOPBAN_3:43;
  then A1: X[0];
 A2: for n st X[n] holds X[n+1]
  proof let n such that
    A3: Partial_Sums(||.0.X ExpSeq .||).n=1;
    thus Partial_Sums(||.0.X ExpSeq .||).(n+1)
         =1 + ||.0.X ExpSeq .||.(n+1) by A3,SERIES_1:def 1
        .=1 + ||.(0.X ExpSeq ).(n+1).|| by NORMSP_1:def 10
        .=1 + ||.(1/(n+1)*0.X)*(0.X ExpSeq .n)  .|| by Th14
        .=1 + ||.(0.X)*(0.X ExpSeq .n)  .|| by LOPBAN_3:43
        .=1 + ||.0.X.|| by LOPBAN_3:43
       .=1 + 0 by LOPBAN_3:43
        .=1;
    end;
    A4:  0.X ExpSeq  is norm_summable
       proof
            for n holds X[n] from NAT_1:sch 1(A1,A2);
         then Partial_Sums(||.0.X ExpSeq .||) is constant by SEQM_3:def 6;
         then Partial_Sums(||.0.X ExpSeq .||) is convergent by SEQ_4:39;
         hence ||.0.X ExpSeq .|| is summable by SERIES_1:def 2;
       end;
  defpred X[set] means Partial_Sums(0.X ExpSeq ).$1=1_ X;
   Partial_Sums(0.X ExpSeq ).0
        =(0.X ExpSeq ).0 by LOPBAN_3:def 1
       .=1_ X by Th14;
  then A5: X[0];
 A6: for n st X[n] holds X[n+1]
   proof let n such that
    A7: Partial_Sums(0.X ExpSeq ).n=1_ X;
    thus Partial_Sums(0.X ExpSeq ).(n+1)
        =1_ X + (0.X ExpSeq ).(n+1) by A7,LOPBAN_3:def 1
        .=1_ X + (1/(n+1) *0.X)*(0.X ExpSeq .n) by Th14
        .=1_ X + (0.X)*(0.X ExpSeq .n) by LOPBAN_3:43
        .=1_ X + 0.X by LOPBAN_3:43
        .=1_ X by LOPBAN_3:43;
    end;
      for n holds X[n] from NAT_1:sch 1(A5,A6);
     then lim(Partial_Sums(0.X ExpSeq ))=1_ X by Th2;
hence thesis by A4,LOPBAN_3:def 3;
end;

registration
   let X;
   let z be Element of X;
  cluster z ExpSeq -> norm_summable;
   coherence
    proof
    A1: ex m be Nat st for n be Nat st  m<=n
       holds ||.(z ExpSeq).n.|| <= (||.z.|| ExpSeq ).n
    proof
    take 0;
      thus
        for n be Nat st  0<=n
       holds ||.(z ExpSeq).n.|| <= (||.z.|| ExpSeq ).n by Th14;
    end;
     ||.z.|| ExpSeq is summable by SIN_COS:49;
    hence z ExpSeq is norm_summable by A1,LOPBAN_3:32;
  end;
end;

theorem Th21:
(z ExpSeq).0 = 1_ X  & Expan(0,z,w).0 = 1_ X
proof
thus (z ExpSeq).0
    = 1/(0!)*(z #N 0) by Def2
   .= 1/(0!  )*(z GeoSeq).0 by LOPBAN_3:def 14
   .= 1/1 *1_ X
 by LOPBAN_3:def 13,NEWTON:18
   .= 1_ X by LOPBAN_3:43;
  A1: 0-'0=0 by GOBOARD9:1;
  hence Expan(0,z,w).0 = (Coef(0)).0 * (z #N 0) * (w #N 0) by Def6
                     .= 1/(1 * 1) * z #N 0 * w #N 0 by A1,Def3,NEWTON:18
                     .= z #N 0 * w #N 0 by LOPBAN_3:43
                     .= (z GeoSeq).0 * w #N 0 by LOPBAN_3:def 14
                     .= (z GeoSeq).0 * (w GeoSeq).0 by LOPBAN_3:def 14
                     .= 1_ X  * (w GeoSeq).0 by LOPBAN_3:def 13
                     .= 1_ X * 1_ X by LOPBAN_3:def 13
                     .= 1_ X by LOPBAN_3:43;
end;

theorem Th22:
l <= k implies (Alfa(k+1,z,w)).l = (Alfa(k,z,w)).l + Expan_e(k+1,z,w).l
proof
assume
A1:l <= k;
        k < k+1 by REAL_1:69;
  then A2:l <= k+1 by A1,AXIOMS:22;
  then (k+1-'l)=k+1-l by SCMFSA_7:3;
  then A3: (k+1-'l)=k-l+1
               .=(k-'l)+1 by A1,SCMFSA_7:3;
  then A4: (Alfa(k+1,z,w)).l
          =((z ExpSeq).l )* (Partial_Sums(w ExpSeq).((k -' l)+1)) by A2,Def8
         .=(z ExpSeq).l * (Partial_Sums(w ExpSeq).((k -' l))
              +(w ExpSeq).(((k+1)-'l))) by A3,LOPBAN_3:def 1
         .=((z ExpSeq).l * (Partial_Sums(w ExpSeq).((k -' l)))
          +((z ExpSeq).l * (w ExpSeq).(( (k+1) -' l)))) by LOPBAN_3:43
         .=(Alfa(k,z,w)).l+((z ExpSeq).l * (w ExpSeq).(( (k+1) -' l)))
         by A1,Def8;
       (z ExpSeq).l * (w ExpSeq).(((k+1)-' l))
    =1/(l!  )*(z #N l) * (w ExpSeq).(((k+1)-' l)) by Def2
   .=1/(l! )*(z #N l) * (1/(((k+1)-'l ) !)*(w #N (((k+1)-'l)))) by Def2
   .=(1/(l! )* (1/(((k+1)-'l ) !))*((z #N l) *(w #N (((k+1)-'l)))))
 by LOPBAN_3:43
   .=(1/ ((l! )* (((k+1)-'l ) !))*((z #N l) *(w #N (((k+1)-'l)))))
 by XCMPLX_1:103
   .= ((Coef_e(k+1)).l) * ((z #N l) * (w #N (((k+1)-'l)))) by A2,Def4
   .= ((Coef_e(k+1)).l) * (z #N l) * (w #N (((k+1)-'l))) by LOPBAN_3:43
   .=Expan_e(k+1,z,w).l by A2,Def7;
   hence (Alfa(k+1,z,w)).l = (Alfa(k,z,w)).l + Expan_e(k+1,z,w).l by A4;
 end;

theorem Th23:
Partial_Sums((Alfa(k+1,z,w))).k
   = (Partial_Sums(( Alfa(k,z,w)))).k
     + (Partial_Sums(( Expan_e(k+1,z,w) ))).k
proof
          now let l be Nat; assume
             l <= k;
         hence (Alfa(k+1,z,w)).l = (Alfa(k,z,w)).l + Expan_e(k+1,z,w).l
 by Th22
              .= ( (Alfa(k,z,w)) + Expan_e(k+1,z,w)).l by NORMSP_1:def 5;
       end;
    hence Partial_Sums((Alfa(k+1,z,w))).k
         =Partial_Sums(((Alfa(k,z,w)) + Expan_e(k+1,z,w))).k by Th11
         .=(Partial_Sums(( Alfa(k,z,w)))
           +Partial_Sums(( Expan_e(k+1,z,w )))).k by LOPBAN_3:20
         .= (Partial_Sums(( Alfa(k,z,w)))).k
         + (Partial_Sums(( Expan_e(k+1,z,w) ))).k by NORMSP_1:def 5;
end;

theorem Th24:
(z ExpSeq).k=(Expan_e(k,z,w)).k
proof
 A1:   0 = k -k;
 then A2:   k-'k=0 by SCMFSA_7:3;
 A3:   (k-'k)! =1 by A1,NEWTON:18,SCMFSA_7:3;
thus (Expan_e(k,z,w)).(k)=((Coef_e(k)).k) * (z #N k) * (w #N 0) by A2,Def7
                        .=( (Coef_e(k)).k) * (z #N k) * 1_ X by LOPBAN_3:44
                        .=( (Coef_e(k)).k) * (z #N k) by LOPBAN_3:43
                        .=(1/((k!) * 1)) * (z #N k) by A3,Def4
                        .=(z ExpSeq).k by Def2;
end;

theorem Th25:
for z,w st z,w are_commutative holds
Partial_Sums((z+w) ExpSeq).n = Partial_Sums(Alfa(n,z,w)).n
proof
let z,w such that
A1: z ,w are_commutative;
    A2:Partial_Sums((z+w) ExpSeq).0
        =((z+w) ExpSeq).0 by LOPBAN_3:def 1
        .=1_ X by Th21;
      defpred X[Nat] means
       Partial_Sums((z+w) ExpSeq).$1=Partial_Sums(Alfa($1,z,w)).$1;
    A3: 0-'0=0 by GOBOARD9:1;
         Partial_Sums(Alfa(0,z,w)).0
       = Alfa(0,z,w).0 by LOPBAN_3:def 1
      .= (z ExpSeq).0 * Partial_Sums(w ExpSeq).0 by A3,Def8
      .= (z ExpSeq).0 * (w ExpSeq).0 by LOPBAN_3:def 1
      .=1_ X * (w ExpSeq).0 by Th21
      .=1_ X * 1_ X by Th21
      .= 1_ X by LOPBAN_3:43;
 then A4: X[0] by A2;
 A5: for k st X[k] holds X[k+1]
  proof let k such that
   A6:Partial_Sums((z+w) ExpSeq).k=Partial_Sums(Alfa(k,z,w)).k;
   thus
          Partial_Sums((z+w) ExpSeq).(k+1)=Partial_Sums(Alfa(k+1,z,w)).(k+1)
       proof
   A7: Partial_Sums((Alfa(k+1,z,w))).(k+1)
   =Partial_Sums((Alfa(k+1,z,w))).k+(Alfa(k+1,z,w)).(k+1) by LOPBAN_3:def 1
  .=((Partial_Sums(( Alfa(k,z,w) ))).k
   + (Partial_Sums(( Expan_e(k+1,z,w) ))).k )
   + (Alfa(k+1,z,w)).(k+1) by Th23
  .= Partial_Sums((z+w) ExpSeq).k
   + ((Partial_Sums(( Expan_e(k+1,z,w) ))).k
   + (Alfa(k+1,z,w)).(k+1)) by A6,LOPBAN_3:43;
     k+1-'(k+1)=0 by GOBOARD9:1;
   then (Alfa(k+1,z,w)).(k+1)
    =(z ExpSeq).(k+1) * Partial_Sums(w ExpSeq).0 by Def8
   .=(z ExpSeq).(k+1) * ((w ExpSeq).0) by LOPBAN_3:def 1
   .=(z ExpSeq).(k+1) * 1_ X by Th21
   .=(z ExpSeq).(k+1) by LOPBAN_3:43
   .=(Expan_e(k+1,z,w)).(k+1) by Th24;
   then (Partial_Sums(( Expan_e(k+1,z,w) ))).k
   + (Alfa(k+1,z,w)).(k+1)
  =(Partial_Sums(( Expan_e(k+1,z,w) ))).(k+1) by LOPBAN_3:def 1
  .=1/((k+1)!)*  (z+w) #N (k+1) by A1,Th19;
  hence Partial_Sums((Alfa(k+1,z,w))).(k+1)
   =Partial_Sums((z+w) ExpSeq).k
    +(z+w) ExpSeq.(k+1) by A7,Def2
   .=Partial_Sums((z+w) ExpSeq).(k+1) by LOPBAN_3:def 1;
   end;
  end;
     for n holds X[n] from NAT_1:sch 1(A4,A5);
  hence thesis;
end;

theorem Th26:
for z,w st z,w are_commutative holds
Partial_Sums(z ExpSeq).k * Partial_Sums(w ExpSeq).k
  -Partial_Sums((z+w) ExpSeq).k
       = Partial_Sums(Conj(k,z,w)).k
proof
let z,w such that
A1:z,w are_commutative;
A2: (Partial_Sums(z ExpSeq).k) * (Partial_Sums(w ExpSeq).k)
       -Partial_Sums((z+w) ExpSeq).k
   =Partial_Sums(z ExpSeq).k * Partial_Sums(w ExpSeq).k
       -Partial_Sums(Alfa(k,z,w)).k by A1,Th25
   .=( Partial_Sums(z ExpSeq)*  (Partial_Sums(w ExpSeq).k) ).k
   -Partial_Sums(Alfa(k,z,w)).k by LOPBAN_3:def 10
 .=(Partial_Sums((z ExpSeq)*(Partial_Sums(w ExpSeq).k))) .k
       -Partial_Sums(Alfa(k,z,w)).k by Th9
  .=(Partial_Sums( (z ExpSeq)*(Partial_Sums(w ExpSeq).k) )
       -Partial_Sums(Alfa(k,z,w))).k by NORMSP_1:def 6
  .=Partial_Sums((( (z ExpSeq)*(Partial_Sums(w ExpSeq).k)))
       -(Alfa(k,z,w))).k by LOPBAN_3:21;
     for l be Nat st l <= k holds
     ( (z ExpSeq)*(Partial_Sums(w ExpSeq).k)
       - (Alfa(k,z,w))).l
      =Conj(k,z,w).l
      proof
      let l be Nat such that
            A3:l <= k;
            thus ( (z ExpSeq)*( Partial_Sums(w ExpSeq).k)
           - (Alfa(k,z,w)) ).l
            =  ((z ExpSeq)*(Partial_Sums(w ExpSeq).k) ).l
                -(Alfa(k,z,w)).l
          by NORMSP_1:def 6
           .= ( (z ExpSeq).l )*( Partial_Sums(w ExpSeq).k)
            -Alfa(k,z,w).l by LOPBAN_3:def 10
           .=((z ExpSeq).l) * ( Partial_Sums(w ExpSeq).k)
            -((z ExpSeq).l) * ( Partial_Sums(w ExpSeq).(k-'l))
 by A3,Def8
           .=((z ExpSeq).l) *
            ( Partial_Sums(w ExpSeq).k
             -Partial_Sums(w ExpSeq).(k-'l) ) by LOPBAN_3:43
           .=Conj(k,z,w).l by A3,Def9;
      end;
     hence
        (Partial_Sums(z ExpSeq).k) * (Partial_Sums(w ExpSeq).k)
       -Partial_Sums((z+w) ExpSeq).k
     =Partial_Sums((Conj(k,z,w))).k by A2,Th11;
end;

theorem Th27:
0 <= (||. z .|| ExpSeq).n
 proof
     defpred P[Nat] means 0 <= ||.z.|| |^$1;
     A1:  (n!) >0 by NEWTON:23;
     ||.z.|| |^0 =1 by NEWTON:9;
   then A2: P[0];
   A3: for k st P[k] holds P[k+1]
     proof
        let k such that
        A4: P[k];
        A5:  ||.z.|| |^ (k+1)
              =  ||.z.|| |^ k *  ||.z.|| by NEWTON:11;
          0 <= ||.z.|| by NORMSP_1:8;
        hence
            0<= ||.z.|| |^ (k+1) by A4,A5,SQUARE_1:19;
     end;
    for k holds P[k] from NAT_1:sch 1(A2,A3);
  then A6:   0 <= ||.z.|| |^n;
  A7:   ||.z.|| |^n/(n!) =  ||.z.|| |^n *((n!))"
                      by XCMPLX_0:def 9;
   0 < ((n!))" by A1,REAL_1:72;
  then 0<= ||.z.|| |^n *((n!))" by A6,SQUARE_1:19;
   hence thesis by A7,SIN_COS:def 9;
 end;

theorem Th28:
||. Partial_Sums((z ExpSeq)).k .|| <= Partial_Sums(||.z.|| ExpSeq).k &
 Partial_Sums((||.z.|| ExpSeq)).k <= Sum(||.z.|| ExpSeq) &
  ||. Partial_Sums(( z ExpSeq)).k .|| <= Sum(||.z.|| ExpSeq)
proof
  defpred X[Nat] means ||. Partial_Sums(( z ExpSeq)).$1 .||
       <= Partial_Sums((||.z.|| ExpSeq)).$1;
A1: ||. Partial_Sums(( z ExpSeq)).0 .||
    = ||. ((z ExpSeq)).0 .|| by LOPBAN_3:def 1
   .= ||. 1/ (0! )*(z #N 0).|| by Def2
   .= ||. 1/(0 !)*((z GeoSeq)).0 .|| by LOPBAN_3:def 14
   .= ||. 1 /1 *1_ X.|| by LOPBAN_3:def 13,NEWTON:18
   .= ||. 1_ X.|| by LOPBAN_3:43

 .= 1 by LOPBAN_3:43;
   Partial_Sums((||.z.|| ExpSeq)).0
    = ((||.z.|| ExpSeq)).0 by SERIES_1:def 1
   .= ((||.z.|| ) |^ 0 ) / (0!) by SIN_COS:def 9
   .= 1 by NEWTON:9,18;
 then A2: X[0] by A1;
 A3: for k st X[k] holds X[k+1]
   proof let k such that
     A4: ||. Partial_Sums(( z ExpSeq)).k .||
       <= Partial_Sums((||.z.|| ExpSeq)).k;
     A5: ||. Partial_Sums(( z ExpSeq)).(k+1) .||
      =||. Partial_Sums(( z ExpSeq)).k
         + ((z ExpSeq)).(k+1) .|| by LOPBAN_3:def 1;
    A6: ||. Partial_Sums(( z ExpSeq)).k + ((z ExpSeq)).(k+1) .||
       <= ||. Partial_Sums(( z ExpSeq)).k.|| + ||. ((z ExpSeq)).(k+1) .||
 by LOPBAN_3:43;
       ||. ((z ExpSeq)).(k+1) .|| <=(||.z.|| ExpSeq).(k+1) by Th14;
    then ||. Partial_Sums(( z ExpSeq)).k.|| + ||. ((z ExpSeq)).(k+1) .||
    <= ||. Partial_Sums(( z ExpSeq)).k.|| +  (||.z.|| ExpSeq).(k+1)
  by REAL_1:55;
     then A7: ||. Partial_Sums(( z ExpSeq)).(k+1) .||
     <= ||. Partial_Sums(( z ExpSeq)).k.|| +
     (||.z.|| ExpSeq).(k+1) by A5,A6,AXIOMS:22;
    A8: ||. Partial_Sums(( z ExpSeq)).k.|| + (||.z.|| ExpSeq).(k+1)
        <= Partial_Sums((||.z.|| ExpSeq)).k + (||.z.|| ExpSeq).(k+1)
 by A4,AXIOMS:24;
        Partial_Sums((||.z.|| ExpSeq)).k + (||.z.|| ExpSeq).(k+1)
        =Partial_Sums((||.z.|| ExpSeq)).(k+1) by SERIES_1:def 1;
    hence ||. Partial_Sums(( z ExpSeq)).(k+1) .||
      <= Partial_Sums((||.z.|| ExpSeq)).(k+1) by A7,A8,AXIOMS:22;
    end;
  A9: for k holds X[k] from NAT_1:sch 1(A2,A3);
   hence ||. Partial_Sums(( z ExpSeq)).k .||
      <= Partial_Sums((||.z.|| ExpSeq)).k;
 A10: for  n holds  0 <= (||.z.|| ExpSeq).n by Th27;
 then A11:Partial_Sums((||.z.|| ExpSeq ))
   is non-decreasing by SERIES_1:19;
 ||.z.|| ExpSeq is summable by SIN_COS:49;
then A12:Partial_Sums(||.z.|| ExpSeq) is bounded_above
by A10,SERIES_1:20;
 then Partial_Sums((||.z.|| ExpSeq)).k
  <= lim(Partial_Sums((||.z.|| ExpSeq )))
by A11,SEQ_4:54;
  hence Partial_Sums((||.z.|| ExpSeq)).k
  <= Sum(||.z.|| ExpSeq) by SERIES_1:def 3;
 A13:
 now let k;
          lim(Partial_Sums((||.z.|| ExpSeq)))=Sum(||.z.|| ExpSeq)
       by SERIES_1:def 3;
         hence Partial_Sums((||.z.|| ExpSeq)).k <= Sum(||.z.|| ExpSeq)
      by A11,A12,SEQ_4:54;
   end;

 A14: ||. Partial_Sums(( z ExpSeq)).k .||
      <= Partial_Sums((||.z.|| ExpSeq)).k by A9;
        Partial_Sums((||.z.|| ExpSeq)).k <= Sum(||.z.|| ExpSeq) by A13;
 hence
  ||. Partial_Sums(( z ExpSeq)).k .|| <= Sum(||.z.|| ExpSeq)
 by A14,AXIOMS:22;
