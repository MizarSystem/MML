A3:   x in dom -z;
      thus (-z).x = -z.x by VALUED_1:8
        .= compcomplex.(z.x) by BINOP_2:def 1
        .= g.x by A2,A1,A3,FUNCT_1:22;
    end;
    hence thesis by A1,FUNCT_1:9,VALUED_1:8;
  end;
end;

notation let z;
  let c be complex number;
  synonym c*z for c(#)z;
end;

definition let z;
  let c be complex number;
  redefine func c*z -> FinSequence of COMPLEX equals
  (c multcomplex)*z;
  coherence
  proof
    thus rng (c(#)z) c= COMPLEX;
  end;
  compatibility
  proof
    set g = (c multcomplex)*z;
    dom (c multcomplex) = COMPLEX by FUNCT_2:def 1;
    then rng z c= dom (c multcomplex);
    then
A1: dom (c(#)z) = dom z & dom g = dom z by RELAT_1:46,VALUED_1:def 5;
    now
      let x be set;
      assume
A2:   x in dom (c(#)z);
A3:   c is Element of COMPLEX & z.x is Element of COMPLEX by XCMPLX_0:def 2;
      thus (c(#)z).x = c*z.x by VALUED_1:6
        .= (c multcomplex).(z.x) by A3,Lm1
        .= g.x by A1,A2,FUNCT_1:22;
    end;
    hence thesis by A1,FUNCT_1:9;
  end;
end;

definition let z;
  redefine func abs z -> FinSequence of REAL equals
  abscomplex*z;
  coherence
  proof
    thus rng abs z c= REAL;
  end;
  compatibility
  proof
    set g = abscomplex*z;
    dom abscomplex = COMPLEX by FUNCT_2:def 1;
    then rng z c= dom abscomplex;
    then
A1: dom abs z = dom z & dom g = dom z by RELAT_1:46,VALUED_1:def 11;
    now
      let x be set;
      assume
A2:   x in dom abs z;
A3:   z.x is Element of COMPLEX by XCMPLX_0:def 2;
      thus (abs z).x = abs(z.x) by VALUED_1:18
        .= abscomplex.(z.x) by A3,Def6
        .= g.x by A1,A2,FUNCT_1:22;
    end;
    hence thesis by A1,FUNCT_1:9;
  end;
end;

definition let n;
  func COMPLEX n -> FinSequence-DOMAIN of COMPLEX equals
  n-tuples_on COMPLEX;
  correctness;
end;

registration let n;
  cluster COMPLEX n -> non empty;
  coherence;
end;

reserve x,z,z1,z2,z3 for Element of COMPLEX n,
  A,B for Subset of COMPLEX n;

Lm2: dom z = Seg n
proof
  len z = n by FINSEQ_1:def 18;
  hence thesis by FINSEQ_1:def 3;
end;

theorem Th21:
  k in Seg n implies z.k in COMPLEX
proof
  len z = n by FINSEQ_1:def 18;
  then Seg n = dom z by FINSEQ_1:def 3;
  hence thesis by FINSEQ_2:13;
end;

definition let n,z1,z2;
  redefine func z1 + z2 -> Element of COMPLEX n;
  coherence by FINSEQ_2:140;
end;

theorem Th24:
  k in Seg n & c1 = z1.k & c2 = z2.k implies (z1 + z2).k = c1 + c2
proof
  assume that
A1: k in Seg n and
A2: c1 = z1.k & c2 = z2.k;
  k in dom(z1+z2) by A1,Lm2;
  hence (z1 + z2).k = addcomplex.(c1,c2) by A2,FUNCOP_1:28
    .= c1 + c2 by BINOP_2:def 3;
end;

definition let n;
  func 0c n -> FinSequence of COMPLEX equals
  n |-> 0c;
  correctness;
end;

definition let n;
  redefine func 0c n -> Element of COMPLEX n;
  coherence;
end;

theorem
  z + 0c n = z & z = 0c n + z by BINOP_2:1,FINSEQOP:57;

definition let n,z;
  redefine func -z -> Element of COMPLEX n;
  coherence by FINSEQ_2:133;
end;

theorem Th29:
  k in Seg n & c = z.k implies (-z).k = -c
proof
  assume that
A1: k in Seg n and
A2: c = z.k;
  k in dom(-z) by A1,Lm2;
  hence (-z).k = compcomplex.c by A2,FUNCT_1:22
    .= -c by BINOP_2:def 1;
end;

Lm3: -0c n = 0c n
proof
  thus -0c n = n|->(compcomplex.0c) by FINSEQOP:17
    .= n|->-(0c qua complex number) by BINOP_2:def 1
    .= 0c n;
end;

theorem
  z + -z = 0c n & -z + z = 0c n by Th7,Th8,BINOP_2:1,FINSEQOP:77;

theorem
  z1 + z2 = 0c n implies z1 = -z2 & z2 = -z1 by Th7,Th8,BINOP_2:1,
  FINSEQOP:78;

theorem
  -(-z) = z;

theorem
  -z1 = -z2 implies z1 = z2
proof
  assume -z1 = -z2;
  hence z1 = -(-z2)  .= z2;
end;

Lm4: z1 + z = z2 + z implies z1 = z2
proof
  assume z1 + z = z2 + z;
  then z1 + (z + -z)= (z2 + z) + -z by FINSEQOP:29;
  then
A1: z1 + (z + -z)= z2 + (z + -z) by FINSEQOP:29;
  z + -z = 0c n by Th7,Th8,BINOP_2:1,FINSEQOP:77;
  then z1 = z2 + 0c n by A1,BINOP_2:1,FINSEQOP:57;
  hence thesis by BINOP_2:1,FINSEQOP:57;
end;

theorem
  z1 + z = z2 + z or z1 + z = z + z2 implies z1 = z2 by Lm4;

theorem Th35:
  -(z1 + z2) = -z1 + -z2
proof
  (z1 + z2) + (-z1 + -z2) = z2 + z1 + -z1 + -z2 by FINSEQOP:29
    .= z2 + (z1 + -z1) + -z2 by FINSEQOP:29
    .= z2 + 0c n + -z2 by Th7,Th8,BINOP_2:1,FINSEQOP:77
    .= z2 + -z2 by BINOP_2:1,FINSEQOP:57
    .= 0c n by Th7,Th8,BINOP_2:1,FINSEQOP:77;
  hence thesis by Th7,Th8,BINOP_2:1,FINSEQOP:78;
end;

definition let n,z1,z2;
  redefine func z1 - z2 -> Element of COMPLEX n;
  coherence by FINSEQ_2:140;
end;

theorem
  k in Seg n implies (z1 - z2).k = z1.k - z2.k
proof
  assume that
A1: k in Seg n;
  set c1 = z1.k, c2 = z2.k;
  k in dom(z1 - z2) by A1,Lm2;
  hence (z1 - z2).k = diffcomplex.(c1,c2) by FUNCOP_1:28
    .= c1 - c2 by BINOP_2:def 4;
end;

theorem
  z - 0c n = z
proof
  thus z - 0c n = z + 0c n by Lm3
    .= z by BINOP_2:1,FINSEQOP:57;
end;

theorem
  0c n - z = -z
proof
  thus 0c n - z = 0c n + -z .= -z by BINOP_2:1,FINSEQOP:57;
end;

theorem
  z1 - -z2 = z1 + z2;

theorem Th41:
  -(z1 - z2) = z2 - z1
proof
  thus -(z1 - z2) = -z1 + -(-z2) by Th35
    .= z2 - z1;
end;

theorem Th42:
  -(z1 - z2) = -z1 + z2
proof
  thus -(z1 - z2) = -z1 + -(-z2) by Th35
    .= -z1 + z2;
end;

theorem Th43:
  z - z = 0c n
proof
  thus z - z = z + -z .= 0c n by Th7,Th8,BINOP_2:1,FINSEQOP:77;
end;

theorem Th44:
  z1 - z2 = 0c n implies z1 = z2
proof
  assume z1 - z2 = 0c n;
  then z1 + - z2 = 0c n;
  then z1 = -(-z2) by Th7,Th8,BINOP_2:1,FINSEQOP:78;
  hence thesis;
end;

theorem Th45:
  z1 - z2 - z3 = z1 - (z2 + z3)
proof
  thus z1 - z2 - z3 = z1 + -z2 + -z3 .= z1 + (- z2 + -z3) by FINSEQOP:29
    .= z1 - (z2 + z3) by Th35;
end;

theorem Th46:
  z1 + (z2 - z3) = z1 + z2 - z3
proof
  thus z1 + (z2 - z3) = z1 + (z2 + -z3) .= z1 + z2 + -z3 by FINSEQOP:29
    .= z1 + z2 - z3;
end;

theorem
  z1 - (z2 - z3) = z1 - z2 + z3
proof
  thus z1 - (z2 - z3) = z1 + (-z2 + z3) by Th42
    .= z1 + -z2 + z3 by FINSEQOP:29
    .= z1 - z2 + z3;
end;

theorem
  z1 - z2 + z3 = z1 + z3 - z2 by Th46;

theorem Th49:
  z1 = z1 + z - z
proof
  thus z1 = z1 + 0c n by BINOP_2:1,FINSEQOP:57
    .= z1 + (z - z) by Th43
    .= z1 + z - z by Th46;
end;

theorem Th50:
  z1 + (z2 - z1) = z2
proof
  thus z1 + (z2 - z1) = z2 + -z1 + z1 .= z2 + (-z1 + z1) by FINSEQOP:29
    .= z2 + 0c n by Th7,Th8,BINOP_2:1,FINSEQOP:77
    .= z2 by BINOP_2:1,FINSEQOP:57;
end;

theorem Th51:
  z1 = z1 - z + z
proof
  thus z1 = z1 + 0c n by BINOP_2:1,FINSEQOP:57
    .= z1 + (-z + z) by Th7,Th8,BINOP_2:1,FINSEQOP:77
    .= z1 + -z + z by FINSEQOP:29
    .= z1 - z + z;
end;

definition let n,z,c;
  redefine func c*z -> Element of COMPLEX n;
  coherence by FINSEQ_2:133;
end;

theorem Th52:
  k in Seg n & c9 = z.k implies (c*z).k = c*c9
proof
  assume that
A1: k in Seg n and
A2: c9 = z.k;
  k in dom(c*z) by A1,Lm2;
  hence (c*z).k = (c multcomplex).c9 by A2,FUNCT_1:22
    .= c*c9 by Lm1;
end;

theorem
  c1*(c2*z) = (c1*c2)*z
proof
  thus (c1*c2)*z = multcomplex[;](multcomplex.(c1,c2),id COMPLEX)*z by BINOP_2
  :def 5
    .= multcomplex[;](c1,multcomplex[;](c2,id COMPLEX))*z by FUNCOP_1:77
    .= (multcomplex[;](c1,id COMPLEX)*multcomplex[;](c2,id COMPLEX))*z by
  FUNCOP_1:69
    .= c1*(c2*z) by RELAT_1:55;
end;

theorem
  (c1 + c2)*z = c1*z + c2*z
proof
  set c1M = multcomplex[;](c1,id COMPLEX), c2M = multcomplex[;](c2,id COMPLEX);
  thus (c1 + c2)*z = multcomplex[;](addcomplex.(c1,c2),id COMPLEX)*z by
  BINOP_2:def 3
    .= addcomplex.:(c1M,c2M)*z by Th15,FINSEQOP:36
    .= c1*z + c2*z by FUNCOP_1:31;
end;

theorem
  c*(z1+z2) = c*z1 + c*z2 by Th15,FINSEQOP:52,55;

theorem
  1r*z = z
proof
A1: rng z c= COMPLEX;
  thus 1r*z = (id COMPLEX)*z by BINOP_2:6,FINSEQOP:45
    .= z by A1,RELAT_1:79;
end;

theorem
  0c*z = 0c n
proof
A1: rng z c= COMPLEX;
  thus 0c*z = multcomplex[;](0c,(id COMPLEX)*z) by FUNCOP_1:44
    .= multcomplex[;](0c,z) by A1,RELAT_1:79
    .= 0c n by Th7,Th15,BINOP_2:1,FINSEQOP:80;
end;

theorem
  (-1r)*z = -z;

definition let n,z;
  redefine func abs z -> Element of n-tuples_on REAL;
  correctness by FINSEQ_2:133;
end;

theorem Th59:
  k in Seg n & c = z.k implies (abs z).k = |.c.|
proof
  assume that
A1: k in Seg n and
A2: c = z.k;
  len abs z = n by FINSEQ_1:def 18;
  then k in dom abs z by A1,FINSEQ_1:def 3;
  hence (abs z).k = abscomplex.c by A2,FUNCT_1:22
    .= |.c.| by Def6;
end;

theorem Th60:
  abs 0c n = n |-> 0
proof
  thus abs 0c n = n |-> abscomplex.0c by FINSEQOP:17
    .= n |-> 0 by Def6,COMPLEX1:130;
end;

theorem Th61:
  abs -z = abs z
proof
  now
    let j be Nat;
    assume
A1: j in Seg n;
    then reconsider c = z.j, c9 = (-z).j as Element of COMPLEX by Th21;
    thus (abs -z).j = |.c9.| by A1,Th59
      .= |.-c.| by A1,Th29
      .= |.c.| by COMPLEX1:138
      .= (abs z).j by A1,Th59;
  end;
  hence thesis by FINSEQ_2:139;
end;

theorem Th62:
  abs(c*z) = |.c.|*(abs z)
proof
  now
    let j be Nat;
    reconsider w = j as Element of NAT by ORDINAL1:def 13;
    assume
A1: j in Seg n;
    then reconsider c9 = z.j, cc = (c*z).j as Element of COMPLEX by Th21;
    reconsider ac = (abs z).w as Real;
    thus (abs(c*z)).j = |.cc.| by A1,Th59
      .= |.c*c9.| by A1,Th52
      .= |.c.|*|.c9.| by COMPLEX1:151
      .= |.c.|*ac by A1,Th59
      .= (|.c.|*(abs z)).j by RVSUM_1:67;
  end;
  hence thesis by FINSEQ_2:139;
end;

definition
  let z be FinSequence of COMPLEX;
  func |.z.| -> Real equals
  sqrt Sum sqr abs z;
  correctness;
end;

theorem Th63:
  |.0c n.| = 0
proof
  thus |.0c n .| = sqrt Sum sqr (n |-> (0 qua Real)) by Th60
    .= sqrt Sum (n |-> (0 qua Real)^2) by RVSUM_1:82
    .= sqrt (n*(0 qua Nat)) by RVSUM_1:110
    .= 0 by SQUARE_1:82;
end;

theorem Th64:
  |.z.| = 0 implies z = 0c n
proof
  assume
A1: |.z.| = 0;
  now
    let j be Nat;
    assume
A2: j in Seg n;
    then reconsider c = z.j as Element of COMPLEX by Th21;
    0 <= Sum sqr abs z by RVSUM_1:116;
    then (abs z).j = (n|->0).j by A1,RVSUM_1:121,SQUARE_1:92;
    then |.c.| = (n|-> 0).j by A2,Th59;
    then c = 0c by A2,COMPLEX1:131,FUNCOP_1:13;
    hence z.j = (n|->0c).j by A2,FUNCOP_1:13;
  end;
  hence thesis by FINSEQ_2:139;
end;

theorem Th65:
  0 <= |.z.|
proof
  0 <= Sum sqr abs z by RVSUM_1:116;
  hence thesis by SQUARE_1:def 4;
end;

theorem
  |.-z.| = |.z.| by Th61;

theorem
  |.c*z.| = |.c.|*|.z.|
proof
A1: 0 <= |.c.|^2 & 0 <= Sum sqr abs z by RVSUM_1:116,XREAL_1:65;
  thus |.c*z.| = sqrt Sum sqr (|.c.|*abs z) by Th62
    .= sqrt Sum (|.c.|^2 * sqr abs z) by RVSUM_1:84
    .= sqrt (|.c.|^2 * Sum sqr abs z) by RVSUM_1:117
    .= sqrt |.c.|^2 * sqrt Sum sqr abs z by A1,SQUARE_1:97
    .= |.c.|*|.z.| by COMPLEX1:132,SQUARE_1:89;
end;

theorem Th68:
  |.z1 + z2.| <= |.z1.| + |.z2.|
proof
A1: 0 <= Sum sqr abs (z1 + z2) by RVSUM_1:116;
A2: 0 <= Sum sqr abs z1 by RVSUM_1:116;
  then
A3: 0 <= sqrt Sum sqr abs z1 by SQUARE_1:def 4;
A4: for k be Nat holds k in Seg n implies 0 <= (mlt(abs z1,abs z2)).k
  proof
    let k be Nat;
    set r = (mlt(abs z1,abs z2)).k;
    assume
A5: k in Seg n;
    then reconsider c1 = z1.k, c2 = z2.k as Element of COMPLEX by Th21;
    (abs z1).k = |.c1.| & (abs z2).k = |.c2.| by A5,Th59;
    then
A6: r = |.c1.|*|.c2.| by RVSUM_1:87;
    0 <= |.c1.| & 0 <= |.c2.| by COMPLEX1:132;
    hence thesis by A6;
  end;
  0 <= (Sum mlt(abs z1,abs z2))^2 by XREAL_1:65;
  then
A7: sqrt(Sum mlt(abs z1,abs z2))^2 <= sqrt((Sum sqr abs z1)*(Sum sqr abs z2
  )) by RVSUM_1:122,SQUARE_1:94;
  len mlt(abs z1,abs z2) = n by FINSEQ_1:def 18;
  then dom mlt(abs z1,abs z2) = Seg n by FINSEQ_1:def 3;
  then Sum mlt(abs z1,abs z2) <= sqrt((Sum sqr abs z1)*(Sum sqr abs z2)) by
  A4,A7,RVSUM_1:114,SQUARE_1:89;
  then 2*Sum mlt(abs z1,abs z2) <= 2*sqrt((Sum sqr abs z1)*(Sum sqr abs z2))
  by XREAL_1:66;
  then
  Sum sqr abs z1+(2*Sum mlt(abs z1,abs z2)) <= Sum sqr abs z1+2*sqrt((Sum
  sqr abs z1)*(Sum sqr abs z2)) by XREAL_1:9;
  then
A8: Sum sqr abs z1+(2*Sum mlt(abs z1,abs z2)) + Sum sqr abs z2 <= Sum sqr
  abs z1+2*sqrt((Sum sqr abs z1)*(Sum sqr abs z2)) + Sum sqr abs z2 by
  XREAL_1:9;
A9: for k be Nat holds k in Seg n implies (sqr abs (z1 + z2)).k <= (sqr (abs
  z1 + abs z2)).k
  proof
    let k be Nat;
    set r2 = (sqr (abs z1 + abs z2)).k;
    len(abs z1 + abs z2) = n by FINSEQ_1:def 18;
    then
A10: dom (abs z1 + abs z2) = Seg n by FINSEQ_1:def 3;
    assume
A11: k in Seg n;
    then reconsider c12 = (z1 + z2).k as Element of COMPLEX by Th21;
    reconsider abs912 = (abs (z1 + z2)).k as Real;
    0 <= |.c12.| by COMPLEX1:132;
    then
A12: 0 <= abs912 by A11,Th59;
    reconsider abs1 = (abs z1).k, abs2 = (abs z2).k as Real;
    reconsider c1 = z1.k, c2 = z2.k as Element of COMPLEX by A11,Th21;
    reconsider abs12 = (abs z1 + abs z2).k as Real;
    |.c1 + c2.| <= |.c1.| + |.c2.| by COMPLEX1:142;
    then |.c12.| <= |.c1.| + |.c2.| by A11,Th24;
    then |.c12.| <= |.c1.| + abs2 by A11,Th59;
    then |.c12.| <= abs1 + abs2 by A11,Th59;
    then |.c12.| <= abs12 by A11,A10,VALUED_1:def 1;
    then abs912 <= abs12 by A11,Th59;
    then (abs912)^2 <= (abs12)^2 by A12,SQUARE_1:77;
    then (abs912)^2 <= r2 by VALUED_1:11;
    hence thesis by VALUED_1:11;
  end;
A13: 0 <= Sum sqr abs z2 by RVSUM_1:116;
  then
A14: 0 <= sqrt Sum sqr abs z2 by SQUARE_1:def 4;
A15: Sum sqr abs z1 = (sqrt Sum sqr abs z1)^2 by A2,SQUARE_1:def 4;
A16: (sqrt Sum sqr abs z2)^2 = Sum sqr abs z2 by A13,SQUARE_1:def 4;
  Sum sqr (abs z1 + abs z2) = Sum (sqr abs z1 + 2*mlt(abs z1,abs z2) + sqr
  abs z2) by RVSUM_1:98
    .= Sum(sqr abs z1 + 2*mlt(abs z1,abs z2)) + Sum sqr abs z2 by RVSUM_1:
  119
    .= Sum sqr abs z1 + Sum(2*mlt(abs z1,abs z2)) + Sum sqr abs z2 by
  RVSUM_1:119
    .= Sum sqr abs z1 + (2*Sum mlt(abs z1,abs z2))+Sum sqr abs z2 by RVSUM_1
  :117;
  then Sum sqr abs (z1 + z2) <= Sum sqr abs z1 + (2*Sum mlt(abs z1,abs z2))+
  Sum sqr abs z2 by A9,RVSUM_1:112;
  then Sum sqr abs (z1 + z2) <= Sum sqr abs z1+2*sqrt((Sum sqr abs z1)*(Sum
  sqr abs z2)) + Sum sqr abs z2 by A8,XXREAL_0:2;
  then Sum sqr abs (z1 + z2) <= Sum sqr abs z1+2*((sqrt Sum sqr abs z1)*(sqrt
  Sum sqr abs z2)) + Sum sqr abs z2 by A2,A13,SQUARE_1:97;
  then sqrt Sum sqr abs (z1 + z2) <= sqrt(((sqrt Sum sqr abs z1) + (sqrt Sum
  sqr abs z2))^2) by A15,A16,A1,SQUARE_1:94;
  hence thesis by A3,A14,SQUARE_1:89;
end;

theorem Th69:
  |.z1 - z2.| <= |.z1.| + |.z2.|
proof
  |.z1 - z2.| <= |.z1.| + |.-z2.| by Th68;
  hence thesis by Th61;
end;

theorem
  |.z1.| - |.z2.| <= |.z1 + z2.|
proof
  z1 = z1 + z2 - z2 by Th49;
  then |.z1.| <= |.z1 + z2.| + |.z2.| by Th69;
  hence thesis by XREAL_1:22;
end;

theorem
  |.z1.| - |.z2.| <= |.z1 - z2.|
proof
  z1 = z1 - z2 + z2 by Th51;
  then |.z1.| <= |.z1 - z2.| + |.z2.| by Th68;
  hence thesis by XREAL_1:22;
end;

theorem Th72:
  |.z1 - z2.| = 0 iff z1 = z2
proof
  thus |.z1 - z2.| = 0 implies z1 = z2
  proof
    assume |.z1 - z2.| = 0;
    then z1 - z2 = 0c n by Th64;
    hence thesis by Th44;
  end;
  assume z1 = z2;
  then z1 - z2 = 0c n by Th43;
  hence thesis by Th63;
end;

theorem
  z1 <> z2 implies 0 < |.z1 - z2.|
proof
  assume z1 <> z2;
  then 0 <> |.z1 - z2.| by Th72;
  hence thesis by Th65;
end;

theorem Th74:
  |.z1 - z2.| = |.z2 - z1.|
proof
  thus |.z1 - z2.| = |.-(z2 - z1).| by Th41
    .= |.z2 - z1.| by Th61;
end;

theorem Th75:
  |.z1 - z2.| <= |.z1 - z.| + |.z - z2.|
proof
  |.z1 - z2.| = |.z1 - z + z - z2.| by Th51
    .= |.(z1 - z) + (z - z2).| by Th46;
  hence thesis by Th68;
end;

definition
  let n;
  let A be Subset of COMPLEX n;
  attr A is open means
  :Def15:
  for x st x in A ex r st 0 < r & for z st |.z.| < r holds x + z in A;
end;

definition let n;
  let A be Subset of COMPLEX n;
  attr A is closed means
  for x st for r st r > 0 ex z st |.z.| < r & x + z in A holds x in A;
end;

theorem
  for A being Subset of COMPLEX n st A = {} holds A is open
proof
  let A be Subset of COMPLEX n;
  assume
A1: A = {};
  let x;
  thus thesis by A1;
end;

theorem
  for A being Subset of COMPLEX n st A = COMPLEX n holds A is open
proof
  let A be Subset of COMPLEX n;
  assume
A1: A = COMPLEX n;
  let x such that
  x in A;
  take j=1;
  thus 0 < j;
  let z such that
  |.z.| < j;
  thus thesis by A1;
end;

theorem
  for AA being Subset-Family of COMPLEX n st for A being Subset of
  COMPLEX n st A in AA holds A is open for A being Subset of COMPLEX n st A =
  union AA holds A is open
proof
  let AA be Subset-Family of COMPLEX n such that
A1: for A being Subset of COMPLEX n st A in AA holds A is open;
  let A be Subset of COMPLEX n such that
A2: A = union AA;
  let x;
  assume x in A;
  then consider B being set such that
A3: x in B and
A4: B in AA by A2,TARSKI:def 4;
  reconsider B as Subset of COMPLEX n by A4;
  B is open by A1,A4;
  then consider r such that
A5: 0 < r and
A6: for z st |.z.| < r holds x + z in B by A3,Def15;
  take r;
  thus 0 < r by A5;
  let z;
  assume |.z.| < r;
  then x + z in B by A6;
  hence thesis by A2,A4,TARSKI:def 4;
end;

theorem
  for A,B being Subset of COMPLEX n st A is open & B is open for C
  being Subset of COMPLEX n st C = A /\ B holds C is open
proof
  let A,B be Subset of COMPLEX n such that
A1: A is open and
A2: B is open;
  let C be Subset of COMPLEX n such that
A3: C = A /\ B;
  let x;
  assume
A4: x in C;
  then x in A by A3,XBOOLE_0:def 4;
  then consider r1 such that
A5: 0 < r1 and
A6: for z st |.z.| < r1 holds x + z in A by A1,Def15;
  x in B by A3,A4,XBOOLE_0:def 4;
  then consider r2 such that
A7: 0 < r2 and
A8: for z st |.z.| < r2 holds x + z in B by A2,Def15;
  take min(r1,r2);
  thus 0 < min(r1,r2) by A5,A7,XXREAL_0:15;
  let z;
  assume
A9: |.z.| < min(r1,r2);
  min(r1,r2) <= r2 by XXREAL_0:17;
  then |.z.| < r2 by A9,XXREAL_0:2;
  then
A10: x + z in B by A8;
  min(r1,r2) <= r1 by XXREAL_0:17;
  then |.z.| < r1 by A9,XXREAL_0:2;
  then x + z in A by A6;
  hence thesis by A3,A10,XBOOLE_0:def 4;
end;

definition
  let n,x,r;
  func Ball(x,r) -> Subset of COMPLEX n equals
  { z : |.z - x.| < r };
  coherence
  proof
    defpred P[FinSequence of COMPLEX] means |.$1 - x.| < r;
    { z : P[z] } c= COMPLEX n from FRAENKEL:sch 10;
    hence thesis;
  end;
end;

theorem Th80:
  z in Ball(x,r) iff |.x - z.| < r
proof
A1: z in { z2 : |.z2 - x.| < r } iff ex z1 st z = z1 & |.z1 - x.| < r;
  |.z - x.| = |.x - z.| by Th74;
  hence thesis by A1;
end;

theorem
  0 < r implies x in Ball(x,r)
proof
  assume
A1: 0 < r;
  |.x - x.| = 0 by Th72;
  hence thesis by A1;
end;

theorem
  Ball(z1,r1) is open
proof
  let x;
  assume x in Ball(z1,r1); then
A1: |.z1 - x.| < r1 by Th80;
  take r = r1 - |.z1 - x.|;
  thus 0 < r by A1,XREAL_1:52;
  let z;
  assume |.z.| < r; then
A2: |.z.| + |.z1 - x.| < r + |.z1 - x.| by XREAL_1:8;
  z1 - x - z = z1 - (x + z) by Th45;
  then |.z1 - (x + z).| <= |.z.| + |.z1 - x.| by Th69;
  then |.z1 - (x + z).| < r + |.z1 - x.| by A2,XXREAL_0:2;
  hence thesis by Th80;
end;

definition let n,x,A;
  func dist(x,A) -> Real means
  :Def18:
  for X being Subset of REAL st X = {|.x
  - z.| : z in A} holds it = lower_bound X;
  existence
  proof
    deffunc f(Element of COMPLEX n) = |.x - $1.|;
    defpred P[set] means $1 in A;
    reconsider X = {f(z) : P[z]} as Subset of REAL from DOMAIN_1:sch 8;
    take lower_bound X;
    thus thesis;
  end;
  uniqueness
  proof
    deffunc f(Element of COMPLEX n) = |.x - $1.|;
    defpred P[set] means $1 in A;
    reconsider X = {f(z) : P[z]} as Subset of REAL from DOMAIN_1:sch 8;
    let r1,r2 be Real such that
A1: for X being Subset of REAL st X = {|.x - z.| : z in A} holds r1 =
    lower_bound X and
A2: for X being Subset of REAL st X = {|.x - z.| : z in A} holds r2 =
    lower_bound X;
    r1 = lower_bound X by A1;
    hence thesis by A2;
  end;
end;

definition let n,A,r;
  func Ball(A,r) -> Subset of COMPLEX n equals
  { z : dist(z,A) < r };
  coherence
  proof
    defpred P[Element of COMPLEX n] means dist($1,A) < r;
    { z : P[z] } c= COMPLEX n from FRAENKEL:sch 10;
    hence thesis;
  end;
end;

theorem Th84:
  for X being Subset of REAL, r st X <> {} & for r9 st r9 in X
  holds r <= r9 holds lower_bound X >= r
proof
  let X be Subset of REAL, r such that
A1: X <> {} and
A2: for r9 st r9 in X holds r <= r9;
  for r9 be ext-real number st r9 in X holds r <= r9 by A2;
  then r is LowerBound of X by XXREAL_2:def 2;
  then
A3: X is bounded_below by XXREAL_2:def 9;
  now
    let r9 be real number;
    assume r9 > 0;
    then consider r1 be real number such that
A4: r1 in X and
A5: r1 < lower_bound X + r9 by A1,A3,Def5;
    r <= r1 by A2,A4;
    hence lower_bound X + r9 >= r by A5,XXREAL_0:2;
  end;
  hence thesis by XREAL_1:43;
end;

theorem Th85:
  A <> {} implies dist(x,A) >= 0
proof
  defpred P[set] means $1 in A;
  deffunc f(Element of COMPLEX n) = |.x - $1.|;
  reconsider X = {f(z) : P[z]} as Subset of REAL from DOMAIN_1:sch 8;
  assume A <> {};
  then consider z1 such that
A1: z1 in A by SUBSET_1:10;
A2: |.x - z1.| in X by A1;
A3: now
    let r9;
    assume r9 in X;
    then ex z st r9 = |.x - z.| & z in A;
    hence r9>= 0 by Th65;
  end;
  dist(x,A) = lower_bound X by Def18;
  hence thesis by A2,A3,Th84;
end;

theorem Th86:
  A <> {} implies dist(x + z,A) <= dist(x,A) + |.z.|
proof
  defpred P[set] means $1 in A;
  deffunc g(Element of COMPLEX n) = |.x + z - $1.|;
  reconsider Y = {g(z1) : P[z1]} as Subset of REAL from DOMAIN_1:sch 8;
  deffunc f(Element of COMPLEX n) = |.x - $1.|;
A1: Y is bounded_below
  proof
    take 0;
    let r be ext-real number;
    assume r in Y;
    then ex z1 st r = |.x + z - z1 .| & z1 in A;
    hence thesis by Th65;
  end;
  reconsider X = {f(z1) : P[z1]} as Subset of REAL from DOMAIN_1:sch 8;
  assume A <> {};
  then consider z2 such that
A2: z2 in A by SUBSET_1:10;
A3: dist(x+z,A) = lower_bound Y by Def18;
A4: now let r9;
    assume r9 in X;
    then consider z3 such that
A5: r9 = |.x - z3.| and
A6: z3 in A;
    |.x + z - z3.| = |.x - z3 + z.| by Th46; then
A7: |.x + z - z3.| <= r9 + |.z.| by A5,Th68;
    |.x + z - z3.| in Y by A6;
    then |.x + z - z3.| >= dist(x + z,A) by A3,A1,Def5;
    then r9+ |.z.| >= dist(x + z,A) by A7,XXREAL_0:2;
    hence r9 >= dist(x + z,A) - |.z.| by XREAL_1:22;
  end;
A8: |.x - z2.| in X by A2;
  dist(x,A) = lower_bound X by Def18;
  then dist(x + z,A) - |.z.| <= dist(x,A) by A8,A4,Th84;
  hence thesis by XREAL_1:22;
end;

theorem Th87:
  x in A implies dist(x,A) = 0
proof
  defpred P[set] means $1 in A;
  deffunc f(Element of COMPLEX n) = |.x - $1.|;
  reconsider X = {f(z): P[z]} as Subset of REAL from DOMAIN_1:sch 8;
  assume
A1: x in A;
  then
A2: |.x - x.| in X;
A3: now
    reconsider r = |.x - x.| as real number;
    let r1 be real number such that
A4: 0<r1;
    take r;
    thus r in X by A1;
    thus r<(0 qua Nat)+r1 by A4,Th72;
  end;
A5: now
    let r be real number;
    assume r in X;
    then ex z st r = |.x - z .| & z in A;
    hence 0<=r by Th65;
  end;
A6: X is bounded_below
  proof
    take 0;
    let x be ext-real number;
    thus thesis by A5;
  end;
  thus dist(x,A) = lower_bound X by Def18
    .= 0 by A2,A5,A6,A3,Def5;
end;

theorem
  not x in A & A <> {} & A is closed implies dist(x,A) > 0
proof
  assume that
A1: not x in A and
A2: A <> {} and
A3: for x st for r st r > 0 ex z st |.z.| < r & x + z in A holds x in A and
A4: dist(x,A) <= 0;
A5: dist(x,A) = 0 by A2,A4,Th85;
  now
    deffunc f(Element of COMPLEX n) = |.x - $1.|;
    defpred P[set] means $1 in A;
    reconsider X = {f(z) : P[z]} as Subset of REAL from DOMAIN_1:sch 8;
    let r such that
A6: r > 0;
    consider z such that
A7: z in A by A2,SUBSET_1:10;
A8: X is bounded_below
    proof
      take 0;
      let r be ext-real number;
      assume r in X;
      then ex z st r = |.x - z .| & z in A;
      hence thesis by Th65;
    end;
A9: |.x - z.| in X by A7;
    dist(x,A) = lower_bound X & (0 qua Nat)+r = r by Def18;
    then consider r9 be real number such that
A10: r9 in X and
A11: r9< r by A5,A6,A9,A8,Def5;
    consider z1 such that
A12: r9 = |.x - z1.| & z1 in A by A10;
    take z = z1 - x;
    thus |.z.| < r & x + z in A by A11,A12,Th50,Th74;
  end;
  hence contradiction by A1,A3;
end;

theorem
  A <> {} implies |.z1 - x.| + dist(x,A) >= dist(z1,A)
proof
  x + (z1 - x) = z1 by Th50;
  hence thesis by Th86;
end;

theorem Th90:
  z in Ball(A,r) iff dist(z,A) < r
proof
  z in { z2 : dist(z2,A) < r } iff ex z1 st z = z1 & dist(z1,A) < r;
  hence thesis;
end;

theorem Th91:
  0 < r & x in A implies x in Ball(A,r)
proof
  assume that
A1: 0 < r and
A2: x in A;
  dist(x,A) = 0 by A2,Th87;
  hence thesis by A1;
end;

theorem
  0 < r implies A c= Ball(A,r)
proof
  assume
A1: r > 0;
  let x be set;
  assume x in A;
  hence thesis by A1,Th91;
end;

theorem
  A <> {} implies Ball(A,r1) is open
proof
  assume
A1: A <> {};
  let x;
  assume x in Ball(A,r1); then
A2: dist(x,A) < r1 by Th90;
  take r = r1 - dist(x,A);
  thus 0 < r by A2,XREAL_1:52;
  let z;
  assume |.z.| < r; then
A3: |.z.| + dist(x,A) < r + dist(x,A) by XREAL_1:8;
  dist(x + z,A) <= |.z.| + dist(x,A) by A1,Th86;
  then dist(x + z,A) < r + dist(x,A) by A3,XXREAL_0:2;
  hence thesis;
end;

definition
  let n,A,B;
  func dist(A,B) -> Real means
  :Def20:
  for X being Subset of REAL st X = {|.x
  - z.| : x in A & z in B} holds it = lower_bound X;
  existence
  proof
    deffunc f(Element of COMPLEX n,Element of COMPLEX n) = |.$1 - $2.|;
    defpred P[set,set] means $1 in A & $2 in B;
    reconsider X = {f(x,z) : P[x,z]} as Subset of REAL from DOMAIN_1:sch 9;
    take lower_bound X;
    thus thesis;
  end;
  uniqueness
  proof
    deffunc f(Element of COMPLEX n,Element of COMPLEX n) = |.$1 - $2.|;
    defpred P[set,set] means $1 in A & $2 in B;
    reconsider X = {f(x,z): P[x,z]} as Subset of REAL from DOMAIN_1:sch 9;
    let r1,r2 be Real such that
A1: for X being Subset of REAL st X = {|.x - z.| : x in A & z in B}
    holds r1 = lower_bound X and
A2: for X being Subset of REAL st X = {|.x - z.| : x in A & z in B}
    holds r2 = lower_bound X;
    r1 = lower_bound X by A1;
    hence thesis by A2;
  end;
end;

definition
  canceled;
end;

theorem
  for X,Y being Subset of REAL holds X <> {} & Y <> {} implies X ++ Y <> {}
proof
  let X,Y be Subset of REAL;
  assume X <> {};
  then consider x being Real such that
A1: x in X by SUBSET_1:10;
  assume Y <> {};
  then consider y being Real such that
A2: y in Y by SUBSET_1:10;
  x + y in X ++ Y by A1,A2,MEMBER_1:46;
  hence thesis;
end;

theorem Th95:
  for X,Y being Subset of REAL holds X is bounded_below &
    Y is bounded_below implies X++Y is bounded_below
proof
  let X,Y be Subset of REAL;
  assume X is bounded_below;
  then consider r1 be real number such that
B1: r1 is LowerBound of X by XXREAL_2:def 9;
A1: for r be real number st r in X holds r1<=r by B1,XXREAL_2:def 2;
  assume Y is bounded_below;
  then consider r2 be real number such that
B2: r2 is LowerBound of Y by XXREAL_2:def 9;
A2: for r be real number st r in Y holds r2<=r by B2,XXREAL_2:def 2;
  take r1 + r2;
  let r be ext-real number;
  assume r in X++Y; then
  r in {r22+r12 where r22 is Element of COMPLEX,
    r12 is Element of COMPLEX : r22 in X & r12 in Y} by MEMBER_1:def 6;
  then consider r22,r12 being Element of COMPLEX such that
A3: r = r22 + r12 and
A4: r22 in X and
A5: r12 in Y;
    reconsider r9 = r22, r99 = r12 as real number by A4,A5;
A6: r2 <= r99 by A2,A5;
  r1 <= r9 by A1,A4;
  hence thesis by A3,A6,XREAL_1:9;
end;

theorem Th96:
  for X,Y being Subset of REAL st
  X <> {} & X is bounded_below & Y <> {} & Y is bounded_below holds
  lower_bound (X ++ Y) = lower_bound X + lower_bound Y
proof
  let X,Y be Subset of REAL such that
A1: X <> {} and
A2: X is bounded_below and
A3: Y <> {} and
A4: Y is bounded_below;
A5: now
    let r9 be real number;
    assume 0<r9;
    then
A6: r9/2 > 0 by XREAL_1:217;
    then consider r1 be real number such that
A7: r1 in X and
A8: r1<lower_bound X+r9/2 by A1,A2,Def5;
    consider r2 be real number such that
A9: r2 in Y and
A10: r2<lower_bound Y+r9/2 by A3,A4,A6,Def5;
     reconsider r = r1 + r2 as real number;
    take r;
    thus r in X++Y by A7,A9,MEMBER_1:46;
    lower_bound X + r9/2 + (lower_bound Y + r9/2) = lower_bound X +
    lower_bound Y + r9;
    hence r<lower_bound X + lower_bound Y+r9 by A8,A10,XREAL_1:10;
  end;
A11: now
    let r be real number;
    assume r in X++Y; then
    r in {r22+r12 where r22 is Element of COMPLEX,
      r12 is Element of COMPLEX : r22 in X & r12 in Y} by MEMBER_1:def 6;
    then consider r11,r22 being Element of COMPLEX such that
A12: r = r11 + r22 and
A13: r11 in X & r22 in Y;
    reconsider r1 = r11, r2 = r22 as real number by A13;
    r1 >= lower_bound X & r2 >= lower_bound Y by A2,A4,A13,Def5;
    hence lower_bound X + lower_bound Y<=r by A12,XREAL_1:9;
  end;
  X ++ Y <> {} & X ++ Y is bounded_below by A1,A2,A3,A4,Th95;
  hence thesis by A11,A5,Def5;
end;

theorem Th97:
  for X,Y being Subset of REAL st Y is bounded_below & X <> {} &
  for r st r in X ex r1 st r1 in Y & r1 <= r holds
    lower_bound X >= lower_bound Y
proof
  let X,Y be Subset of REAL such that
A1: Y is bounded_below and
A2: X <> {} and
A3: for r st r in X ex r1 st r1 in Y & r1 <= r;
  now
    let r1;
    assume r1 in X;
    then consider r2 such that
A4: r2 in Y and
A5: r2 <= r1 by A3;
    lower_bound Y <= r2 by A1,A4,Def5;
    hence r1 >= lower_bound Y by A5,XXREAL_0:2;
  end;
  hence thesis by A2,Th84;
end;

theorem Th98:
  A <> {} & B <> {} implies dist(A,B) >= 0
proof
  defpred P[set,set] means $1 in A & $2 in B;
  deffunc f(Element of COMPLEX n,Element of COMPLEX n) = |.$1 - $2.|;
  reconsider Z = {f(z1,z): P[z1,z]} as Subset of REAL from DOMAIN_1:sch 9;
  assume that
A1: A <> {} and
A2: B <> {};
  consider z1 such that
A3: z1 in A by A1,SUBSET_1:10;
A4: now
    let r9;
    assume r9 in Z;
    then ex z1,z st r9 = |.z1 - z.| & z1 in A & z in B;
    hence r9>= 0 by Th65;
  end;
  consider z2 such that
A5: z2 in B by A2,SUBSET_1:10;
A6: dist(A,B) = lower_bound Z by Def20;
  |.z1 - z2.| in Z by A3,A5;
  hence thesis by A6,A4,Th84;
end;

theorem
  dist(A,B) = dist(B,A)
proof
  defpred R[set,set] means $1 in B & $2 in A;
  deffunc f(Element of COMPLEX n,Element of COMPLEX n) = |.$1 - $2.|;
  reconsider Y = {f(z,z1) : R[z,z1]} as Subset of REAL from DOMAIN_1:sch 9;
  defpred P[set,set] means $1 in A & $2 in B;
  reconsider X = {f(z1,z) : P[z1,z]} as Subset of REAL from DOMAIN_1:sch 9;
A1: now
    let r;
A2: now
      given z,z1 such that
A3:   r =|.z - z1.| & z in B & z1 in A;
      take z1,z;
      thus r =|.z1 - z.| & z1 in A & z in B by A3,Th74;
    end;
    now
      given z1,z such that
A4:   r =|.z1 - z.| & z1 in A & z in B;
      take z,z1;
      thus r =|.z - z1.| & z in B & z1 in A by A4,Th74;
    end;
    hence r in X iff r in Y by A2;
  end;
  dist(A,B) = lower_bound X & dist(B,A) = lower_bound Y by Def20;
  hence thesis by A1,SUBSET_1:8;
end;

theorem Th100:
  A <> {} & B <> {} implies dist(x,A) + dist(x,B) >= dist(A,B)
proof
  defpred Y[set] means $1 in B;
  deffunc g(Element of COMPLEX n) = |.x - $1.|;
  reconsider Y = {g(z) : Y[z]} as Subset of REAL from DOMAIN_1:sch 8;
  defpred P[set,set] means $1 in A & $2 in B;
  defpred X[set] means $1 in A;
  deffunc f(Element of COMPLEX n,Element of COMPLEX n) = |.$1 - $2.|;
  reconsider X = {g(z) : X[z]} as Subset of REAL from DOMAIN_1:sch 8;
  assume that
A1: A <> {} and
A2: B <> {};
  consider z2 such that
A3: z2 in B by A2,SUBSET_1:10;
A4: Y <> {} & Y is bounded_below
  proof
    |.x - z2.| in Y by A3;
    hence Y <> {};
    take 0;
    let r be ext-real number;
    assume r in Y;
    then ex z1 st r = |.x - z1 .| & z1 in B;
    hence thesis by Th65;
  end;
A5: lower_bound X = dist(x,A) & lower_bound Y = dist(x,B) by Def18;
A6: |.x - z2 .| in Y by A3;
  reconsider Z = {f(z1,z) : P[z1,z]} as Subset of REAL from DOMAIN_1:sch 9;
  consider z1 such that
A7: z1 in A by A1,SUBSET_1:10;
A8: now
    let r;
    assume r in X ++ Y; then
    r in {r2+r1 where r2 is Element of COMPLEX,
      r1 is Element of COMPLEX : r2 in X & r1 in Y} by MEMBER_1:def 6;
    then consider r2,r1 being Element of COMPLEX such that
A9: r = r2 + r1 and
A10: r2 in X and
A11: r1 in Y;
    consider z2 such that
A12: r1 = |.x - z2.| & z2 in B by A11;
    consider z1 such that
A13: r2 = |.x - z1.| and
A14: z1 in A by A10;
    take r3 = |.z1 - z2.|;
    r2 = |.z1 - x.| by A13,Th74;
    hence r3 in Z & r3 <= r by A9,A14,A12,Th75;
  end;
A15: Z is bounded_below
  proof
    take 0;
    let r be ext-real number;
    assume r in Z;
    then ex z1,z st r = |.z1 - z .| & z1 in A & z in B;
    hence thesis by Th65;
  end;
A16: X <> {} & X is bounded_below
  proof
    |.x - z1.| in X by A7;
    hence X <> {};
    take 0;
    let r be ext-real number;
    assume r in X;
    then ex z st r = |.x - z .| & z in A;
    hence thesis by Th65;
  end;
A17: lower_bound Z = dist(A,B) by Def20;
A:  X++Y c= REAL by MEMBERED:3;
  |.x - z1.| in X by A7;
  then |.x - z1.| + |.x - z2 .| in X ++ Y by A6,MEMBER_1:46;
  then lower_bound (X ++ Y) >= lower_bound Z by A15,A8,Th97,A;
  hence thesis by A16,A4,A5,A17,Th96;
end;

theorem
  A meets B implies dist(A,B) = 0
proof
  assume A meets B;
  then consider z being set such that
A1: z in A and
A2: z in B by XBOOLE_0:3;
  reconsider z as Element of COMPLEX n by A1;
  dist(z,A) = 0 & dist(z,B) = 0 by A1,A2,Th87;
  then (0 qua Nat) + (0 qua Nat) >= dist(A,B) by A1,A2,Th100;
  hence thesis by A1,A2,Th98;
end;

definition
  let n;
  func ComplexOpenSets(n) -> Subset-Family of COMPLEX n equals
  {A where A is
  Subset of COMPLEX n: A is open};
  coherence
  proof
    set S = {A where A is Subset of COMPLEX n: A is open};
    S c= bool COMPLEX n
    proof
      let x be set;
      assume x in S;
      then ex A being Subset of COMPLEX n st x = A & A is open;
      hence thesis;
    end;
    hence thesis;
  end;
end;

theorem
  for A being Subset of COMPLEX n holds A in ComplexOpenSets n iff A is open
proof
  let B be Subset of COMPLEX n;
  B in {A where A is Subset of COMPLEX n: A is open} iff ex C being Subset
  of COMPLEX n st B = C & C is open;
  hence thesis;
end;

theorem
  for A being Subset of COMPLEX n holds A is closed iff A` is open
proof
  let A be Subset of COMPLEX n;
  thus A is closed implies A` is open
  proof
    assume
A1: for x st for r st r > 0 ex z st |.z.| < r & x + z in A holds x in A;
    let x;
    assume x in A`;
    then not x in A by XBOOLE_0:def 5;
    then consider r such that
A2: r > 0 and
A3: for z st |.z.| < r holds not x + z in A by A1;
    take r;
    thus 0 < r by A2;
    let z;
    assume |.z.| < r;
    hence thesis by A3,SUBSET_1:50;
  end;
  assume
A4: for x st x in A` ex r st 0 < r & for z st |.z.| < r holds x + z in A`;
  let x such that
A5: for r st r > 0 ex z st |.z.| < r & x + z in A;
  now
    let r;
    assume r > 0;
    then consider z such that
A6: |.z.| < r & x + z in A by A5;
    take z;
    thus |.z.| < r & not x + z in A` by A6,XBOOLE_0:def 5;
  end;
  then not x in A` by A4;
  hence thesis by SUBSET_1:50;
end;

begin :: moved from GOBOARD2, 2010.03.01, A.T.

reserve
  v,v1,v2 for FinSequence of REAL,
  n,m,k for Element of NAT,
  x for set;

defpred P[Nat] means for R being finite Subset of REAL st R <> {} & card R =
$1 holds R is bounded_above & upper_bound(R) in R & R is bounded_below &
lower_bound(R) in R;

Lm1: P[0];

Lm2: for n st P[n] holds P[n+1]
proof
  let n such that
A1: P[n];
  let R be finite Subset of REAL such that
A2: R <> {} and
A3: card R = n+1;
  now
    per cases;
    suppose
A4:   n=0;
      thus R is bounded_above;
      consider x such that
A5:   R={x} by A3,A4,CARD_2:60;
      x in R by A5,TARSKI:def 1;
      then reconsider r=x as Real;
      upper_bound R = r by A5,Th22;
      hence upper_bound R in R by A5,TARSKI:def 1;
      thus R is bounded_below;
      lower_bound R = r by A5,Th22;
      hence lower_bound R in R by A5,TARSKI:def 1;
    end;
    suppose
A6:   n<>0;
      consider x being Element of R;
      reconsider x as Real by A2,TARSKI:def 3;
      reconsider X = R \ {x} as finite Subset of REAL;
      set u = upper_bound X, m = max(x,u), l = lower_bound X, mi = min(x,l);
      {x} c= R by A2,ZFMISC_1:37;
      then card (R\{x}) = card R - card {x} by CARD_2:63;
      then
A7:   card X = n+1-1 by A3,CARD_1:50
        .=n;
      then
A8:   upper_bound X in X by A1,A6,CARD_1:47;
A9:   now
        reconsider s = m as real number;
        let r be real number such that
A10:    0<r;
        take s;
        now
          per cases by XXREAL_0:16;
          suppose
            m=x;
            hence s in R by A2;
            thus m-r<s by A10,XREAL_1:46;
          end;
          suppose
            m=u;
            hence s in R by A8,XBOOLE_0:def 5;
            thus m-r<s by A10,XREAL_1:46;
          end;
        end;
        hence s in R & m-r<s;
      end;
A11:  lower_bound X in X by A1,A6,A7,CARD_1:47;
A12:  now
        reconsider s = mi as real number;
        let r be real number such that
A13:    0<r;
        take s;
        now
          per cases by XXREAL_0:15;
          suppose
            mi=x;
            hence s in R by A2;
            thus s<mi+r by A13,XREAL_1:31;
          end;
          suppose
            mi=l;
            hence s in R by A11,XBOOLE_0:def 5;
            thus s<mi+r by A13,XREAL_1:31;
          end;
        end;
        hence s in R & s<mi+r;
      end;
      thus R is bounded_above;
A14:  u<=m by XXREAL_0:25;
A15:  now
        let s be real number such that
A16:    s in R;
        now
          per cases;
          suppose
            s=x;
            hence s<=m by XXREAL_0:25;
          end;
          suppose
            s<>x;
            then not s in {x} by TARSKI:def 1;
            then s in X by A16,XBOOLE_0:def 5;
            then s<=u by Def4;
            hence s<=m by A14,XXREAL_0:2;
          end;
        end;
        hence s<=m;
      end;
      now
        per cases by XXREAL_0:16;
        suppose
          m=x;
          hence m in R by A2;
        end;
        suppose
          m=u;
          hence m in R by A8,XBOOLE_0:def 5;
        end;
      end;
      hence upper_bound R in R by A15,A9,Def4;
      thus R is bounded_below;
A17:  mi<=l by XXREAL_0:17;
A18:  now
        let s be real number such that
A19:    s in R;
        now
          per cases;
          suppose
            s=x;
            hence mi<=s by XXREAL_0:17;
          end;
          suppose
            s<>x;
            then not s in {x} by TARSKI:def 1;
            then s in X by A19,XBOOLE_0:def 5;
            then l<=s by Def5;
            hence mi<=s by A17,XXREAL_0:2;
          end;
        end;
        hence mi<=s;
      end;
      now
        per cases by XXREAL_0:15;
        suppose
          mi=x;
          hence mi in R by A2;
        end;
        suppose
          mi=l;
          hence mi in R by A11,XBOOLE_0:def 5;
        end;
      end;
      hence lower_bound R in R by A18,A12,Def5;
    end;
  end;
  hence thesis;
end;

Lm3: for n holds P[n] from NAT_1:sch 1(Lm1,Lm2);

theorem Th1:
  for R being finite Subset of REAL holds R <> {} implies R is
bounded_above & upper_bound(R) in R & R is bounded_below & lower_bound(R) in R
proof
  let R be finite Subset of REAL;
  assume
A1: R <> {};
  P[card R] by Lm3;
  hence thesis by A1;
end;

theorem
  for n being Nat for f being FinSequence holds 1 <= n & n+1 <= len
  f iff n in dom f & n+1 in dom f
proof
  let n be Nat;
  let f be FinSequence;
  thus 1<=n & n+1 <= len f implies n in dom f & n+1 in dom f
  proof
    assume that
A1: 1<=n and
A2: n+1 <= len f;
    n<=n+1 by NAT_1:11;
    then 1<=n+1 & n<=len f by A2,NAT_1:11,XXREAL_0:2;
    hence thesis by A1,A2,FINSEQ_3:27;
  end;
  thus thesis by FINSEQ_3:27;
end;

theorem
  for n being Nat for f being FinSequence holds 1 <= n & n+2 <= len f
  iff n in dom f & n+1 in dom f & n+2 in dom f
proof
  let n be Nat;
  let f be FinSequence;
  thus 1<=n & n+2 <= len f implies n in dom f & n+1 in dom f & n+2 in dom f
  proof
    assume that
A1: 1<=n and
A2: n+2 <= len f;
    n+1<=n+1+1 by NAT_1:11;
    then
A3: 1<=n+1 & n+1<=len f by A2,NAT_1:11,XXREAL_0:2;
    n<=n+2 by NAT_1:11;
    then 1<=n+2 & n<=len f by A1,A2,XXREAL_0:2;
    hence thesis by A1,A2,A3,FINSEQ_3:27;
  end;
  assume that
A4: n in dom f and
  n+1 in dom f and
A5: n+2 in dom f;
  thus thesis by A4,A5,FINSEQ_3:27;
end;

theorem
  for D being non empty set, f1,f2 being FinSequence of D, n st 1
  <= n & n <= len f2 holds (f1^f2)/.(n + len f1) = f2/.n
proof
  let D be non empty set, f1,f2 be FinSequence of D, n such that
A1: 1 <= n and
A2: n <= len f2;
A3: len f1 < n + len f1 by A1,NAT_1:19;
  len(f1^f2) = len f1 + len f2 by FINSEQ_1:35;
  then
A4: n + len f1 <= len(f1^f2) by A2,XREAL_1:8;
  n + len f1 >= n by NAT_1:11;
  then n + len f1 >= 1 by A1,XXREAL_0:2;
  hence (f1^f2)/.(n + len f1) = (f1^f2).(n + len f1) by A4,FINSEQ_4:24
    .= f2.(n + len f1 - len f1) by A3,A4,FINSEQ_1:37
    .= f2/.n by A1,A2,FINSEQ_4:24;
end;

theorem
  v is increasing implies for n,m st n in dom v & m in dom v & n<=m
  holds v.n <= v.m
proof
  assume
A1: v is increasing;
  let n,m such that
A2: n in dom v & m in dom v and
A3: n<=m;
  now
    per cases;
    suppose
      n=m;
      hence thesis;
    end;
    suppose
      n<>m;
      then n<m by A3,XXREAL_0:1;
      hence thesis by A1,A2,SEQM_3:def 1;
    end;
  end;
  hence thesis;
end;

theorem Th19:
  v is increasing implies for n,m st n in dom v & m in dom v & n<>
  m holds v.n<>v.m
proof
  assume
A1: v is increasing;
  let n,m;
  assume that
A2: n in dom v & m in dom v and
A3: n<>m;
  now
    per cases by A3,XXREAL_0:1;
    suppose
      n<m;
      hence thesis by A1,A2,SEQM_3:def 1;
    end;
    suppose
      n>m;
      hence thesis by A1,A2,SEQM_3:def 1;
    end;
  end;
  hence thesis;
end;

theorem Th20:
  v is increasing & v1 = v|Seg n implies v1 is increasing
proof
  assume that
A1: v is increasing and
A2: v1 = v|Seg n;
  now
    per cases;
    suppose
      n<=len v;
      then Seg n c= Seg len v by FINSEQ_1:7;
      then
A3:   Seg n c= dom v by FINSEQ_1:def 3;
      then Seg n = dom v /\ Seg n by XBOOLE_1:28;
      then
A4:   dom v1 = Seg n by A2,RELAT_1:90;
      now
        let m,k such that
A5:     m in dom v1 & k in dom v1 and
A6:     m<k;
        set r=v1.m, s=v1.k;
        r=v.m & s=v.k by A2,A5,FUNCT_1:70;
        hence r<s by A1,A3,A4,A5,A6,SEQM_3:def 1;
      end;
      hence thesis by SEQM_3:def 1;
    end;
    suppose
      len v<=n;
      hence thesis by A1,A2,FINSEQ_2:23;
    end;
  end;
  hence thesis;
end;

defpred P1[Element of NAT] means for v st card rng v = $1 holds ex v1 st rng
v1 = rng v & len v1 = card rng v & v1 is increasing;

Lm4: P1[0]
proof
  let v;
  assume card rng v = 0;
  then
A1: rng v = {};
  take v1 = v;
  thus rng v1 = rng v;
  thus len v1 = card rng v by A1,RELAT_1:64;
  for n,m holds n in dom v1 & m in dom v1 & n<m implies v1.n < v1.m by A1,
RELAT_1:60,64;
  hence thesis by SEQM_3:def 1;
end;

Lm5: for n st P1[n] holds P1[n+1]
proof
  let n such that
A1: P1[n];
  let v such that
A2: card rng v = n+1;
  now
    reconsider R = rng v as finite Subset of REAL;
    set u = upper_bound(R), X = R \ {u};
    set f = X|v;
    Seq f=f*Sgm(dom f) & rng Sgm(dom f)=dom f by FINSEQ_1:71;
    then
A3: rng Seq f = rng f by RELAT_1:47
      .= X by RELAT_1:120,XBOOLE_1:36;
    then reconsider f1 = Seq f as FinSequence of REAL by FINSEQ_1:def 4;
    reconsider X as finite set;
    upper_bound(R) in R by A2,Th1,CARD_1:47;
    then
A4: {u} c= R by ZFMISC_1:37;
    then
A5: card X = card R - card {u} by CARD_2:63;
A6: card {u}=1 by CARD_1:50;
    then consider v2 such that
A7: rng v2 = rng f1 and
A8: len v2 = card rng f1 and
A9: v2 is increasing by A1,A2,A3,A5;
    take v1 = v2 ^ <* u *>;
    thus rng v1 = X \/ rng <*u*> by A3,A7,FINSEQ_1:44
      .= (rng v \ {u}) \/ {u} by FINSEQ_1:56
      .= rng v \/ {u} by XBOOLE_1:39
      .= rng v by A4,XBOOLE_1:12;
    thus
A10: len v1 = n+ len <*u*> by A2,A3,A5,A6,A8,FINSEQ_1:35
      .= card rng v by A2,FINSEQ_1:56;
    now
      let k,m;
      assume that
A11:  k in dom v1 and
A12:  m in dom v1 and
A13:  k<m;
A14:  1<=m by A12,FINSEQ_3:27;
A15:  m<=len v1 by A12,FINSEQ_3:27;
      set r=v1.k, s=v1.m;
A16:  1<=k by A11,FINSEQ_3:27;
A17:  k<=len v1 by A11,FINSEQ_3:27;
      now
        per cases;
        suppose
A18:      m=len v1;
          k<len v1 by A13,A15,XXREAL_0:2;
          then k<=len v2 by A2,A3,A5,A6,A8,A10,NAT_1:13;
          then
A19:      k in dom v2 by A16,FINSEQ_3:27;
          then r=v2.k by FINSEQ_1:def 7;
          then
A20:      r in rng v2 by A19,FUNCT_1:def 5;
          then r in R by A3,A7,XBOOLE_0:def 5;
          then
A21:      r<=u by Def4;
          not r in {u} by A3,A7,A20,XBOOLE_0:def 5;
          then
A22:      r<>u by TARSKI:def 1;
          s=u by A2,A3,A5,A6,A8,A10,A18,FINSEQ_1:59;
          hence r<s by A21,A22,XXREAL_0:1;
        end;
        suppose
          m <>len v1;
          then m<len v1 by A15,XXREAL_0:1;
          then m<=len v2 by A2,A3,A5,A6,A8,A10,NAT_1:13;
          then
A23:      m in dom v2 by A14,FINSEQ_3:27;
          then
A24:      s=v2.m by FINSEQ_1:def 7;
          k<len v1 by A13,A17,A15,XXREAL_0:1;
          then k<=len v2 by A2,A3,A5,A6,A8,A10,NAT_1:13;
          then
A25:      k in dom v2 by A16,FINSEQ_3:27;
          then r=v2.k by FINSEQ_1:def 7;
          hence r<s by A9,A13,A25,A23,A24,SEQM_3:def 1;
        end;
      end;
      hence r<s;
    end;
    hence v1 is increasing by SEQM_3:def 1;
  end;
  hence thesis;
end;

Lm6: for n holds P1[n] from NAT_1:sch 1(Lm4,Lm5);

theorem
  for v holds ex v1 st rng v1 = rng v & len v1 = card rng v & v1 is
  increasing by Lm6;

defpred P3[Element of NAT] means for v1,v2 st len v1=$1 & len v2=$1 & rng v1=
rng v2 & v1 is increasing & v2 is increasing holds v1=v2;

Lm7: P3[0]
proof
  let v1,v2;
  assume that
A1: len v1=0 and
A2: len v2=0 and
  rng v1=rng v2 and
  v1 is increasing and
  v2 is increasing;
  v1={} by A1;
  hence thesis by A2;
end;

Lm8: for n st P3[n] holds P3[n+1]
proof
  let n such that
A1: P3[n];
  let v1,v2 such that
A2: len v1=n+1 and
A3: len v2=n+1 and
A4: rng v1=rng v2 and
A5: v1 is increasing and
A6: v2 is increasing;
  reconsider X = rng v1 as Subset of REAL;
  set u = upper_bound(X);
  X <> {} by A2,CARD_1:47,RELAT_1:64;
  then
A8: upper_bound(X) in X by Th1;
  then consider m being Nat such that
A9: m in dom v2 and
A10: v2.m=u by A4,FINSEQ_2:11;
  reconsider m as Element of NAT by ORDINAL1:def 13;
A11: m<=len v2 by A9,FINSEQ_3:27;
A12: n<=n+1 by NAT_1:11;
  then Seg n c= Seg(n+1) by FINSEQ_1:7;
  then
A13: Seg n = Seg(n+1) /\ Seg n by XBOOLE_1:28;
  dom v2=Seg len v2 by FINSEQ_1:def 3;
  then
A14: dom (v2|Seg n) =Seg n by A3,A13,RELAT_1:90;
  dom v1 = Seg len v1 by FINSEQ_1:def 3;
  then
A15: dom (v1|Seg n) = Seg n by A2,A13,RELAT_1:90;
  then reconsider f1 = v1|Seg n, f2 = v2|Seg n as FinSequence by A14,
FINSEQ_1:def 2;
  rng f2 c= rng v2 by RELAT_1:99; then
A16: rng f2 c= REAL by XBOOLE_1:1;
  rng f1 c= rng v1 by RELAT_1:99;
  then rng f1 c= REAL by XBOOLE_1:1;
  then reconsider f1, f2 as FinSequence of REAL by A16,FINSEQ_1:def 4;
  consider k being Nat such that
A17: k in dom v1 and
A18: v1.k=u by A8,FINSEQ_2:11;
  reconsider k as Element of NAT by ORDINAL1:def 13;
A19: k<=len v1 by A17,FINSEQ_3:27;
A20: 1<=k by A17,FINSEQ_3:27;
A21: now
    assume k<>len v1;
    then k<len v1 by A19,XXREAL_0:1;
    then
A22: k+1<=len v1 by NAT_1:13;
    reconsider s=v1.(k+1) as Real;
A23: k<k+1 by NAT_1:13;
    1<=k+1 by A20,NAT_1:13;
    then
A24: k+1 in dom v1 by A22,FINSEQ_3:27;
    then v1.(k+1) in rng v1 by FUNCT_1:def 5;
    then s<=u by Def4;
    hence contradiction by A5,A17,A18,A24,A23,SEQM_3:def 1;
  end;
A25: 1<=m by A9,FINSEQ_3:27;
A26: now
    assume m<>len v2;
    then m<len v2 by A11,XXREAL_0:1;
    then
A27: m+1<=len v2 by NAT_1:13;
    reconsider s=v2.(m+1) as Real;
A28: m<m+1 by NAT_1:13;
    1<=m+1 by A25,NAT_1:13;
    then
A29: m+1 in dom v2 by A27,FINSEQ_3:27;
    then v2.(m+1) in rng v2 by FUNCT_1:def 5;
    then s<=u by A4,Def4;
    hence contradiction by A6,A9,A10,A29,A28,SEQM_3:def 1;
  end;
A30: len f1 = n by A15,FINSEQ_1:def 3;
  then
A31: dom f1 c= dom v1 by A2,A12,FINSEQ_3:32;
A32: rng f1 = rng v1 \ {u}
  proof
    thus rng f1 c= rng v1 \ {u}
    proof
      let x;
      assume x in rng f1;
      then consider i being Nat such that
A33:  i in dom f1 and
A34:  f1.i=x by FINSEQ_2:11;
A35:  x=v1.i by A33,A34,FUNCT_1:70;
A36:  v1.i in rng v1 by A31,A33,FUNCT_1:def 5;
      i<=n by A15,A33,FINSEQ_1:3;
      then i<>n+1 by NAT_1:13;
      then x<>u by A2,A5,A17,A18,A21,A31,A33,A35,Th19;
      then not x in {u} by TARSKI:def 1;
      hence thesis by A35,A36,XBOOLE_0:def 5;
    end;
    let x;
    assume
A37: x in rng v1 \ {u};
    then x in rng v1 by XBOOLE_0:def 5;
    then consider i being Nat such that
A38: i in dom v1 and
A39: v1.i=x by FINSEQ_2:11;
A40: i<=len v1 by A38,FINSEQ_3:27;
    not x in {u} by A37,XBOOLE_0:def 5;
    then i <> len v1 by A18,A21,A39,TARSKI:def 1;
    then i<len v1 by A40,XXREAL_0:1;
    then
A41: i<=len f1 by A2,A30,NAT_1:13;
    1<=i by A38,FINSEQ_3:27;
    then
A42: i in dom f1 by A41,FINSEQ_3:27;
    then f1.i in rng f1 by FUNCT_1:def 5;
    hence thesis by A39,A42,FUNCT_1:70;
  end;
A43: dom v1 = Seg len v1 by FINSEQ_1:def 3;
A44: now
    let i be Nat;
    assume
A45: i in dom v1;
    then
A46: 1<=i by A43,FINSEQ_1:3;
A47: i<=len f1+1 by A2,A30,A43,A45,FINSEQ_1:3;
    now
      per cases;
      suppose
        i=len f1+1;
        hence (f1^<*u*>).i=v1.i by A2,A18,A21,A30,FINSEQ_1:59;
      end;
      suppose
        i<>len f1+1;
        then i < len f1+1 by A47,XXREAL_0:1;
        then i<=len f1 by NAT_1:13;
        then
A48:    i in dom f1 by A15,A30,A46,FINSEQ_1:3;
        hence (f1^<*u*>).i=f1.i by FINSEQ_1:def 7
          .= v1.i by A48,FUNCT_1:70;
      end;
    end;
    hence (f1^<*u*>).i=v1.i;
  end;
  len (f1^<*u*>) = n + len <*u*> by A30,FINSEQ_1:35
    .= len v1 by A2,FINSEQ_1:56;
  then
A49: v1=f1^<*u*> by A44,FINSEQ_2:10;
A50: len f2 = n by A14,FINSEQ_1:def 3;
  then
A51: len (f2^<*u*>) = n + len <*u*> by FINSEQ_1:35
    .= len v2 by A3,FINSEQ_1:56;
A52: dom f2 c= dom v2 by A3,A12,A50,FINSEQ_3:32;
A53: rng f2 = rng v2 \ {u}
  proof
    thus rng f2 c= rng v2 \ {u}
    proof
      let x;
      assume x in rng f2;
      then consider i being Nat such that
A54:  i in dom f2 and
A55:  f2.i=x by FINSEQ_2:11;
A56:  x=v2.i by A54,A55,FUNCT_1:70;
A57:  v2.i in rng v2 by A52,A54,FUNCT_1:def 5;
      i<=n by A14,A54,FINSEQ_1:3;
      then i<>n+1 by NAT_1:13;
      then x<>u by A3,A6,A9,A10,A26,A52,A54,A56,Th19;
      then not x in {u} by TARSKI:def 1;
      hence thesis by A56,A57,XBOOLE_0:def 5;
    end;
    let x;
    assume
A58: x in rng v2 \ {u};
    then x in rng v2 by XBOOLE_0:def 5;
    then consider i being Nat such that
A59: i in dom v2 and
A60: v2.i=x by FINSEQ_2:11;
A61: i<=len v2 by A59,FINSEQ_3:27;
    not x in {u} by A58,XBOOLE_0:def 5;
    then i <> len v2 by A10,A26,A60,TARSKI:def 1;
    then i<len v2 by A61,XXREAL_0:1;
    then
A62: i<=len f2 by A3,A50,NAT_1:13;
    1<=i by A59,FINSEQ_3:27;
    then
A63: i in dom f2 by A62,FINSEQ_3:27;
    then f2.i in rng f2 by FUNCT_1:def 5;
    hence thesis by A60,A63,FUNCT_1:70;
  end;
A64: dom v2 = Seg len v2 by FINSEQ_1:def 3;
A65: now
    let i be Nat;
    assume
A66: i in dom v2;
    then
A67: 1<=i by A64,FINSEQ_1:3;
A68: i<=len f2+1 by A3,A50,A64,A66,FINSEQ_1:3;
    now
      per cases;
      suppose
        i=len f2+1;
        hence (f2^<*u*>).i=v2.i by A3,A10,A26,A50,FINSEQ_1:59;
      end;
      suppose
        i<>len f2+1;
        then i < len f2+1 by A68,XXREAL_0:1;
        then i<=len f2 by NAT_1:13;
        then
A69:    i in dom f2 by A14,A50,A67,FINSEQ_1:3;
        hence (f2^<*u*>).i=f2.i by FINSEQ_1:def 7
          .= v2.i by A69,FUNCT_1:70;
      end;
    end;
    hence (f2^<*u*>).i=v2.i;
  end;
  f1 is increasing & f2 is increasing by A5,A6,Th20;
  then f1=f2 by A1,A4,A30,A50,A32,A53;
  hence thesis by A49,A51,A65,FINSEQ_2:10;
end;

Lm9: for n holds P3[n] from NAT_1:sch 1(Lm7,Lm8);

theorem
  for v1,v2 st len v1 = len v2 & rng v1 = rng v2 & v1 is increasing & v2
  is increasing holds v1 = v2 by Lm9;

definition
  let v;
  func Incr(v) ->increasing FinSequence of REAL means
  :Def25:
  rng it = rng v & len it = card rng v;
  existence
  proof
    consider v1 such that
A1: rng v1 = rng v & len v1 = card rng v and
A2: v1 is increasing by Lm6;
    reconsider v1 as increasing FinSequence of REAL by A2;
    take v1;
    thus thesis by A1;
  end;
  uniqueness by Lm9;
end;

registration
  let v be non empty FinSequence of REAL;
  cluster Incr v -> non empty;
  coherence
proof
A2: len Incr(v)=card rng v by Def25;
  assume Incr(v) is empty;
  then rng v = {} by A2;
  hence contradiction;
end;
end;
