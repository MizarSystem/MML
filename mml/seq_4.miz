  proof
    let b be BinOp of COMPLEX;
    now
      let c1,c2;
      thus diffcomplex.(c1,c2) = c1 - c2 by BINOP_2:def 4
        .= c1 + - c2
        .= addcomplex.(c1,- c2) by BINOP_2:def 3
        .= addcomplex.(c1,compcomplex.c2) by BINOP_2:def 1
