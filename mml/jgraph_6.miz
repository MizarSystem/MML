          then f.p3 <> W-min(P) by A38,EUCLID:56;
          hence LE f.p2,f.p3,P by A17,A39,JORDAN6:def 10;
        end;
      end;
      hence LE f.p2,f.p3,P;
    end;
    case
A40:  p2 in LSeg(|[1,1]|,|[1,-1]|);
      then
A41:  p2`1=1 & -1<=p2`2 & p2`2<=1 by Th9;
A42:  (p2`2)^2 >=0 by XREAL_1:65;
A43:  sqrt(1+(p2`2)^2)>0 by A42,SQUARE_1:93;
      p2<>0.REAL 2 by A41,EUCLID:56,58;
      then
A44:  f.p2= |[p2`1/sqrt(1+(p2`2/p2`1)^2),p2`2/sqrt(1+(p2`2/p2`1)^2) ]|
