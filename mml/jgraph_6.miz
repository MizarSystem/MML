A27: x in Vertical_Line((W-bound(P)+E-bound(P))/2) by A9,JORDAN6:def 6;
A28: (|[b,c]|)`1=b by EUCLID:56;
A29: (|[b,c]|)`2=c by EUCLID:56;
A30: (|[a,c]|)`1=a by EUCLID:56;
