          then
A62:      (f.p3)`2<0 by A56,A58,REAL_2:128;
          then f.p3 in {p9 where p9 is Point of TOP-REAL 2:p9 in P & p9`2<=0}
          by A9;
          then
A63:      f.p3 in Lower_Arc(P) by A3,JGRAPH_5:38;
          W-min(P)=|[-1,0]| by A3,JGRAPH_5:32;
          then
A64:      f.p3 <> W-min(P) by A62,EUCLID:56;
          now per cases;
            case (f.p2)`2>=0;

then f.p2 in {p9 where p9 is Point of TOP-REAL 2:p9 in P & p9`2>=0} by A8;
              then f.p2 in Upper_Arc(P) by A3,JGRAPH_5:37;
              hence LE f.p2,f.p3,P by A63,A64,JORDAN6:def 10;
            end;
            case
A65:          (f.p2)`2<0;
