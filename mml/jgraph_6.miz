    end;
    case
A66:  p2 in LSeg(|[1,-1]|,|[-1,-1]|)& p2<>|[-1,-1]|;
      then
A67:  p2`2=-1 & -1<=p2`1 & p2`1<=1 by Th11;
A68:  (p2`1)^2 >=0 by XREAL_1:65;
A69:  sqrt(1+(p2`1)^2)>0 by A68,SQUARE_1:93;
A70:  -p2`2>=p2`1 by A67;
      p2<>0.REAL 2 by A67,EUCLID:56,58;
      then
A71:  f.p2= |[p2`1/sqrt(1+(p2`1/p2`2)^2),p2`2/sqrt(1+(p2`1/p2`2)^2) ]|
