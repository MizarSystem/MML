      then
A39:  Sq_Circ.px=|[px`1/sqrt(1+(px`2/px`1)^2),px`2/sqrt(1+(px`2/px`1 )^2) ]|
      by A35,JGRAPH_2:11,JGRAPH_3:def 1;
      px`2<=px`1 & --px`1>=-px`2 or px`2>=px`1 & px`2<=-px`1
      by A27,A28,A38,XREAL_1:26,66;
      then
A40:  px`2<=px`1 & px`1>=-px`2 or px`2>=px`1 & -px`2>=--px`1 by XREAL_1:26;
A41:  px`1/sqrt(1+(px`2/px`1)^2)=q`1 by A27,A28,A30,XCMPLX_1:90;
A42:  px`2/sqrt(1+(px`2/px`1)^2)=q`2 by A27,A28,A30,XCMPLX_1:90;
A43:  dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
      not px`1=0 by A27,A28,A35,A38,XREAL_1:66;
      then
A44:  (px`1)^2>0 by SQUARE_1:74;
A45:  (px`2)^2>=0 by XREAL_1:65;
      (px`1)^2/(sqrt(1+(px`2/px`1)^2))^2+(px`2/sqrt(1+(px`2/px`1)^2)) ^2 <=1
      by A30,A31,A32,A33,A34,XCMPLX_1:77;
      then (px`1)^2/(sqrt(1+(px`2/px`1)^2))^2+(px`2)^2/(sqrt(1+(px`2/px`1)^2))
      ^2<=1 by XCMPLX_1:77;
      then (px`1)^2/(1+(px`2/px`1)^2)+(px`2)^2/(sqrt(1+(px`2/px`1)^2))^2<=1
      by A29,SQUARE_1:def 4;
      then (px`1)^2/(1+(px`2/px`1)^2)+(px`2)^2/(1+(px`2/px`1)^2)<=1
      by A29,SQUARE_1:def 4;
      then ((px`1)^2/(1+(px`2/px`1)^2)
      +(px`2)^2/(1+(px`2/px`1)^2))*(1+(px`2/px`1)^2)<=1 *(1+(px`2/px`1)^2)
      by A29,XREAL_1:66;
      then (px`1)^2/(1+(px`2/px`1)^2)*(1+(px`2/px`1)^2)
      +(px`2)^2/(1+(px`2/px`1)^2)*(1+(px`2/px`1)^2)<=1 *(1+(px`2/px`1)^2);
      then (px`1)^2+(px`2)^2/(1+(px`2/px`1)^2)*(1+(px`2/px`1)^2)
      <=1 *(1+(px`2/px`1)^2) by A29,XCMPLX_1:88;
      then
A46:  (px`1)^2+(px`2)^2<=1 *(1+(px`2/px`1)^2) by A29,XCMPLX_1:88;
      1 *(1+(px`2/px`1)^2) =1+(px`2)^2/(px`1)^2 by XCMPLX_1:77;
      then (px`1)^2+(px`2)^2-1<=1+(px`2)^2/(px`1)^2-1 by A46,XREAL_1:11;
      then ((px`1)^2+(px`2)^2-1)*(px`1)^2<=((px`2)^2/(px`1)^2)*(px`1)^2
      by A44,XREAL_1:66;
      then ((px`1)^2+((px`2)^2-1))*(px`1)^2<=(px`2)^2 by A44,XCMPLX_1:88;
      then
A47:  (px`1)^2*(px`1)^2+(px`1)^2*((px`2)^2-1)-(px`2)^2<=0 by REAL_2:106;
      (px`1)^2*(px`1)^2+((px`1)^2*(px`2)^2-(px`1)^2*1)-(px`2)^2
      = ((px`1)^2-1)*((px`1)^2+(px`2)^2);
      then (px`1)^2-1<=0 & (px`1)^2-1>=0 or
      (px`1)^2-1<=0 & (px`1)^2+(px`2)^2>=0 or
      (px`1)^2+(px`2)^2<=0 & (px`1)^2-1>=0 or
      (px`1)^2+(px`2)^2<=0 & (px`1)^2+(px`2)^2>=0 by A47,REAL_2:122;
      then (px`1)^2-1+1<=0+1 & ((px`1)^2+(px`2)^2)>=0 by A44,A45,XREAL_1:9;
      then
A48:  px`1<=1 & px`1>= -1 by SQUARE_1:59,117;
      then px`2<=1 & 1>= -px`2 or px`2>= -1 & -px`2>=px`1 by A40,XXREAL_0:2;
      then px`2<=1 & -1<= --px`2 or px`2>= -1 & -px`2>= -1
      by A48,XREAL_1:26,XXREAL_0:2;
      then px`2<=1 & -1<=px`2 or px`2>= -1 & --px`2<= --1 by XREAL_1:26;
      then px in Kb by A1,A48;
      hence ex x being set st x in dom Sq_Circ & x in Kb & y=Sq_Circ.x
      by A21,A39,A41,A42,A43,EUCLID:57;
    end;
    case
A49:  q<>0.REAL 2 & not(q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
      set px=|[q`1*sqrt(1+(q`1/q`2)^2),q`2*sqrt(1+(q`1/q`2)^2)]|;
A50:  q<>0.REAL 2 & (q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 & q`1<=-q`2)
      by A49,JGRAPH_2:23;
A51:  px`2 = q`2*sqrt(1+(q`1/q`2)^2) &
      px`1 = q`1*sqrt(1+(q`1/q`2)^2) by EUCLID:56;
      1+(q`1/q`2)^2>0 by XREAL_1:36,65;
      then
A52:  sqrt(1+(q`1/q`2)^2)>0 by SQUARE_1:93;
A53:  1+(px`1/px`2)^2>0 by XREAL_1:36,65;
A54:  px`1/px`2=q`1/q`2 by A51,A52,XCMPLX_1:92;
A55:  q`2=q`2*sqrt(1+(q`1/q`2)^2)/(sqrt(1+(q`1/q`2)^2))by A52,XCMPLX_1:90
        .=px`2/(sqrt(1+(q`1/q`2)^2)) by EUCLID:56;
A56:  q`1=q`1*sqrt(1+(q`1/q`2)^2)/(sqrt(1+(q`1/q`2)^2))by A52,XCMPLX_1:90
        .=px`1/(sqrt(1+(q`1/q`2)^2)) by EUCLID:56;
A57:  |.q.|^2=q`2^2+q`1^2 by JGRAPH_3:10;
A58:  |.q.|^2<=1 by A21,SQUARE_1:59,77;
A59:  now
        assume px`2=0 & px`1=0;
        then
A60:    q`2*sqrt(1+(q`1/q`2)^2)=0 & q`1*sqrt(1+(q`1/q`2)^2)=0 by EUCLID:56;
        then q`1=0 by A52,XCMPLX_1:6;
        hence contradiction by A49,A52,A60,XCMPLX_1:6;
      end;
      q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 &
      q`1*sqrt(1+(q`1/q`2)^2) <= (-q`2)*sqrt(1+(q`1/q`2)^2)
      by A50,A52,XREAL_1:66;
      then
A61:  q`1<=q`2 & (-q`2)*sqrt(1+(q`1/q`2)^2) <= q`1*sqrt(1+(q`1/q`2) ^2)
      or px`1>=px`2 & px`1<=-px`2 by A51,A52,XREAL_1:66;
      then px`1<=px`2 & -px`2<=px`1 or px`1>=px`2 & px`1<=-px`2
      by A51,A52,XREAL_1:66;
      then
A62:  Sq_Circ.px=|[px`1/sqrt(1+(px`1/px`2)^2),px`2/sqrt(1+(px`1/px`2 )^2) ]|
      by A59,JGRAPH_2:11,JGRAPH_3:14;
      px`1<=px`2 & --px`2>=-px`1 or px`1>=px`2 & px`1<=-px`2
      by A51,A52,A61,XREAL_1:26,66;
      then
A63:  px`1<=px`2 & px`2>=-px`1 or px`1>=px`2 & -px`1>=--px`2 by XREAL_1:26;
A64:  px`2/sqrt(1+(px`1/px`2)^2)=q`2 by A51,A52,A54,XCMPLX_1:90;
A65:  px`1/sqrt(1+(px`1/px`2)^2)=q`1 by A51,A52,A54,XCMPLX_1:90;
A66:  dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
      not px`2=0 by A51,A52,A59,A61,XREAL_1:66;
      then
A67:  (px`2)^2>0 by SQUARE_1:74;
A68:  (px`1)^2>=0 by XREAL_1:65;
      (px`2)^2/(sqrt(1+(px`1/px`2)^2))^2+(px`1/sqrt(1+(px`1/px`2)^2)) ^2 <= 1
      by A54,A55,A56,A57,A58,XCMPLX_1:77;
      then (px`2)^2/(sqrt(1+(px`1/px`2)^2))^2+(px`1)^2/(sqrt(1+(px`1/px`2)^2))
      ^2<=1 by XCMPLX_1:77;
      then (px`2)^2/(1+(px`1/px`2)^2)+(px`1)^2/(sqrt(1+(px`1/px`2)^2))^2<=1
      by A53,SQUARE_1:def 4;
      then (px`2)^2/(1+(px`1/px`2)^2)+(px`1)^2/(1+(px`1/px`2)^2)<=1
      by A53,SQUARE_1:def 4;
      then ((px`2)^2/(1+(px`1/px`2)^2)
      +(px`1)^2/(1+(px`1/px`2)^2))*(1+(px`1/px`2)^2)<=1 *(1+(px`1/px`2)^2)
      by A53,XREAL_1:66;
      then (px`2)^2/(1+(px`1/px`2)^2)*(1+(px`1/px`2)^2)
      +(px`1)^2/(1+(px`1/px`2)^2)*(1+(px`1/px`2)^2)<=1 *(1+(px`1/px`2)^2);
      then (px`2)^2+(px`1)^2/(1+(px`1/px`2)^2)*(1+(px`1/px`2)^2)
      <=1 *(1+(px`1/px`2)^2) by A53,XCMPLX_1:88;
      then
A69:  (px`2)^2+(px`1)^2<=1 *(1+(px`1/px`2)^2) by A53,XCMPLX_1:88;
      1 *(1+(px`1/px`2)^2) =1+(px`1)^2/(px`2)^2 by XCMPLX_1:77;
      then (px`2)^2+(px`1)^2-1<=1+(px`1)^2/(px`2)^2-1 by A69,XREAL_1:11;
      then ((px`2)^2+(px`1)^2-1)*(px`2)^2<=((px`1)^2/(px`2)^2)*(px`2)^2
      by A67,XREAL_1:66;
      then ((px`2)^2+((px`1)^2-1))*(px`2)^2<=(px`1)^2 by A67,XCMPLX_1:88;
      then
A70:  (px`2)^2*(px`2)^2+(px`2)^2*((px`1)^2-1)-(px`1)^2<=0 by REAL_2:106;
      (px`2)^2*(px`2)^2+((px`2)^2*(px`1)^2-(px`2)^2*1)-(px`1)^2
      = ((px`2)^2-1)*((px`2)^2+(px`1)^2);
      then ((px`2)^2-1<=0 or ((px`2)^2+(px`1)^2)<=0)
      &((px`2)^2-1>=0 or (px`2)^2+(px`1)^2>=0) by A70,REAL_2:122;
      then (px`2)^2-1+1<=0+1 & ((px`2)^2+(px`1)^2)>=0 by A67,A68,XREAL_1:9;
      then
A71:  px`2<=1 & px`2>= -1 by SQUARE_1:59,117;
      then px`1<=1 & 1>= -px`1 or px`1>= -1 & -px`1>=px`2 by A63,XXREAL_0:2;
      then px`1<=1 & -1<= --px`1 or px`1>= -1 & -px`1>= -1
      by A71,XREAL_1:26,XXREAL_0:2;
      then px`1<=1 & -1<=px`1 or px`1>= -1 & --px`1<= --1 by XREAL_1:26;
      then px in Kb by A1,A71;
      hence ex x being set st x in dom Sq_Circ & x in Kb & y=Sq_Circ.x
      by A21,A62,A64,A65,A66,EUCLID:57;
    end;
  end;
  hence thesis by FUNCT_1:def 12;
end;

theorem Th37:
  for Kb,Cb being Subset of TOP-REAL 2 st
  Kb={p: not(-1 <p`1 & p`1< 1 & -1 <p`2 & p`2< 1)}&
  Cb={p2 where p2 is Point of TOP-REAL 2: |.p2.|>=1} holds Sq_Circ.:Kb=Cb
proof
  let Kb,Cb be Subset of TOP-REAL 2;
  assume
A1: Kb={p: not(-1 <p`1 & p`1< 1 & -1 <p`2 & p`2< 1)}&
  Cb={p2 where p2 is Point of TOP-REAL 2: |.p2.|>=1};
  thus Sq_Circ.:Kb c= Cb
  proof
    let y be set;
    assume y in Sq_Circ.:Kb;
    then consider x being set such that
A2: x in dom Sq_Circ & x in Kb & y=Sq_Circ.x by FUNCT_1:def 12;
    consider q being Point of TOP-REAL 2 such that
A3: q=x &( not(-1 <q`1 & q`1< 1 & -1 <q`2 & q`2< 1) ) by A1,A2;
    now per cases;
      case q=0.REAL 2;
        then q`1=0 & q`2=0 by EUCLID:56,58;
        hence contradiction by A3;
      end;
      case
A4:     q<>0.REAL 2 & (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
        then
A5:     Sq_Circ.q=|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2 )]|
        by JGRAPH_3:def 1;
A6:     not(-1 <q`2 & q`2< 1) implies -1>=q`1 or q`1>=1
        proof
          assume
A7:       not(-1 <q`2 & q`2< 1);
          now per cases by A7;
            case
A8:           -1>=q`2;
              then -q`1<= -1 or q`2>=q`1 & q`2<= -q`1 by A4,XXREAL_0:2;
              hence -1>=q`1 or q`1>=1 by A8,XREAL_1:26,XXREAL_0:2;
            end;
            case q`2>=1;
              then 1<=q`1 or 1<= -q`1 by A4,XXREAL_0:2;
              then 1<=q`1 or --q`1<= -1 by XREAL_1:26;
              hence -1>=q`1 or q`1>=1;
            end;
          end;
          hence -1>=q`1 or q`1>=1;
        end;
A9:     (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`1
        = q`1/sqrt(1+(q`2/q`1)^2) &
        (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`2
        = q`2/sqrt(1+(q`2/q`1)^2) by EUCLID:56;
A10:    1+(q`2/q`1)^2>0 by XREAL_1:36,65;
A11:    now
          assume
A12:      q`1=0;
          then q`2=0 by A4;
          hence contradiction by A4,A12,EUCLID:57,58;
        end;
        then
A13:    (q`1)^2>0 by SQUARE_1:74;
        (q`1)^2>=1^2 by A3,A6,SQUARE_1:118;
        then
A14:    sqrt((q`1)^2)>=1 by SQUARE_1:83,94;
        |.(|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|).|^2
        =((q`1)/sqrt(1+(q`2/(q`1))^2))^2+(q`2/sqrt(1+(q`2/(q`1))^2))^2
        by A9,JGRAPH_3:10
          .=(q`1)^2/(sqrt(1+(q`2/(q`1))^2))^2+(q`2/sqrt(1+(q`2/(q`1))^2))^2
        by XCMPLX_1:77
          .=(q`1)^2/(sqrt(1+(q`2/(q`1))^2))^2
        +(q`2)^2/(sqrt(1+(q`2/(q`1))^2))^2 by XCMPLX_1:77
          .=(q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
        by A10,SQUARE_1:def 4
          .=(q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(1+(q`2/q`1)^2)
        by A10,SQUARE_1:def 4
          .=((q`1)^2+(q`2)^2)/(1+(q`2/q`1)^2) by XCMPLX_1:63
          .=((q`1)^2+(q`2)^2)/(1+(q`2)^2/(q`1)^2) by XCMPLX_1:77
          .=((q`1)^2+(q`2)^2)/((q`1)^2/(q`1)^2+(q`2)^2/(q`1)^2)
        by A13,XCMPLX_1:60
          .=((q`1)^2+(q`2)^2)/(((q`1)^2+(q`2)^2)/(q`1)^2)
        by XCMPLX_1:63
          .=(q`1)^2*(((q`1)^2+(q`2)^2)/((q`1)^2+(q`2)^2))
        by XCMPLX_1:82
          .=(q`1)^2*1 by A11,COMPLEX1:2,XCMPLX_1:60
          .=(q`1)^2;
        then |.(|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|).|>=1
        by A14,SQUARE_1:89;
        hence ex p2 being Point of TOP-REAL 2 st p2=y & |.p2.|>=1 by A2,A3,A5;
      end;
      case
A15:    q<>0.REAL 2 & not(q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
        then
A16:    Sq_Circ.q=|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2 )]|
        by JGRAPH_3:def 1;
A17:    (|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|)`1
        = q`1/sqrt(1+(q`1/q`2)^2) &
        (|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|)`2
        = q`2/sqrt(1+(q`1/q`2)^2) by EUCLID:56;
A18:    1+(q`1/q`2)^2>0 by XREAL_1:36,65;
A19:    q`2 <> 0 by A15;
        then
A20:    (q`2)^2>0 by SQUARE_1:74;
        not(-1 <q`1 & q`1< 1) implies -1>=q`2 or q`2>=1
        proof
          assume
A21:      not(-1 <q`1 & q`1< 1);
          now per cases by A21;
            case
A22:          -1>=q`1;

then q`2<= -1 or q`1<q`2 & -q`2<= --q`1 by A15,XREAL_1:26,XXREAL_0:2;
              then -q`2<= -1 or -1>=q`2 by A22,XXREAL_0:2;
              hence -1>=q`2 or q`2>=1 by XREAL_1:26;
            end;
            case
A23:          q`1>=1;
              --q`1<= -q`2 & q`2<=q`1 or q`2>=q`1 & q`2>= -q`1
              by A15,XREAL_1:26;
              then 1<= -q`2 or q`2>=q`1 & q`2>= -q`1 by A23,XXREAL_0:2;
              then -1>= --q`2 or 1<=q`2 by A23,XREAL_1:26,XXREAL_0:2;
              hence -1>=q`2 or q`2>=1;
            end;
          end;
          hence -1>=q`2 or q`2>=1;
        end;
        then (q`2)^2>=1^2 by A3,SQUARE_1:118;
        then
A24:    sqrt((q`2)^2)>=1 by SQUARE_1:83,94;
        |.(|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|).|^2
        =((q`1)/sqrt(1+(q`1/(q`2))^2))^2+(q`2/sqrt(1+(q`1/(q`2))^2))^2
        by A17,JGRAPH_3:10
          .=(q`1)^2/(sqrt(1+(q`1/(q`2))^2))^2+(q`2/sqrt(1+(q`1/(q`2))^2))^2
        by XCMPLX_1:77
          .=(q`1)^2/(sqrt(1+(q`1/(q`2))^2))^2
        +(q`2)^2/(sqrt(1+(q`1/(q`2))^2))^2 by XCMPLX_1:77
          .=(q`1)^2/(1+(q`1/q`2)^2)+(q`2)^2/(sqrt(1+(q`1/q`2)^2))^2
        by A18,SQUARE_1:def 4
          .=(q`1)^2/(1+(q`1/q`2)^2)+(q`2)^2/(1+(q`1/q`2)^2)
        by A18,SQUARE_1:def 4
          .=((q`1)^2+(q`2)^2)/(1+(q`1/q`2)^2) by XCMPLX_1:63
          .=((q`1)^2+(q`2)^2)/(1+(q`1)^2/(q`2)^2) by XCMPLX_1:77
          .=((q`1)^2+(q`2)^2)/((q`1)^2/(q`2)^2+(q`2)^2/(q`2)^2)
        by A20,XCMPLX_1:60
          .=((q`1)^2+(q`2)^2)/(((q`1)^2+(q`2)^2)/(q`2)^2) by XCMPLX_1:63
          .=(q`2)^2*(((q`1)^2+(q`2)^2)/((q`1)^2+(q`2)^2)) by XCMPLX_1:82
          .=(q`2)^2*1 by A19,COMPLEX1:2,XCMPLX_1:60
          .=(q`2)^2;
        then |.(|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|).|>=1
        by A24,SQUARE_1:89;
        hence ex p2 being Point of TOP-REAL 2 st p2=y & |.p2.|>=1
        by A2,A3,A16;
      end;
    end;
    hence y in Cb by A1;
  end;
  let y be set;
  assume y in Cb;
  then consider p2 being Point of TOP-REAL 2 such that
A25: p2=y & |.p2.|>=1 by A1;
  set q=p2;
  now per cases;
    case q=0.REAL 2;
      hence contradiction by A25,TOPRNS_1:24;
    end;
    case
A26:  q<>0.REAL 2 & (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
      set px=|[q`1*sqrt(1+(q`2/q`1)^2),q`2*sqrt(1+(q`2/q`1)^2)]|;
A27:  px`1 = q`1*sqrt(1+(q`2/q`1)^2) &
      px`2 = q`2*sqrt(1+(q`2/q`1)^2) by EUCLID:56;
      1+(q`2/q`1)^2>0 by XREAL_1:36,65;
      then
A28:  sqrt(1+(q`2/q`1)^2)>0 by SQUARE_1:93;
A29:  1+(px`2/px`1)^2>0 by XREAL_1:36,65;
A30:  px`2/px`1=q`2/q`1 by A27,A28,XCMPLX_1:92;
A31:  q`1=q`1*sqrt(1+(q`2/q`1)^2)/(sqrt(1+(q`2/q`1)^2))by A28,XCMPLX_1:90
        .=px`1/(sqrt(1+(q`2/q`1)^2)) by EUCLID:56;
