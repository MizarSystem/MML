theorem Th41:
  for a,b,c,d being real number st a <= b & c <= d holds
  LSeg(|[a,c]|,|[a,d]|) /\ LSeg(|[a,c]|,|[b,c]|) = {|[a,c]|}
proof
  let a,b,c,d be real number;
  assume
A1: a <= b & c <= d;
  for ax being set
  holds ax in LSeg(|[a,c]|,|[a,d]|) /\ LSeg(|[a,c]|,|[b,c]|) iff ax = |[a,c]|
  proof
    let ax be set;
    thus ax in LSeg(|[a,c]|,|[a,d]|) /\ LSeg(|[a,c]|,|[b,c]|)
    implies ax = |[a,c]|
    proof
      assume ax in LSeg(|[a,c]|,|[a,d]|) /\ LSeg(|[a,c]|,|[b,c]|);
      then
A2:   ax in LSeg(|[a,c]|,|[a,d]|)& ax in LSeg(|[a,c]|,|[b,c]|)
      by XBOOLE_0:def 3;
      then ax in { p2 : p2`1 <= b & p2`1 >= a & p2`2 = c } by A1,Th39;
      then
A3:   ex p2 st p2 = ax & p2`1 <= b & p2`1 >= a & p2`2 = c;
      ax in { p2 : p2`1 = a & p2`2 <= d & p2`2 >= c } by A1,A2,Th39;
      then ex p st p = ax & p`1 = a & p`2 <= d & p`2 >= c;
