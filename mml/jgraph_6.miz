    end;
    assume ax = |[a,c]|;
    then ax in LSeg(|[a,c]|,|[a,d]|) & ax in LSeg(|[a,c]|,|[b,c]|)
    by TOPREAL1:6;
    hence ax in LSeg(|[a,c]|,|[a,d]|) /\ LSeg(|[a,c]|,|[b,c]|)
    by XBOOLE_0:def 3;
  end;
  hence thesis by TARSKI:def 1;
end;

