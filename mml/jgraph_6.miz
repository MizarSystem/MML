    end;
   assume ax = |[b,c]|;
   then ax in LSeg(|[a,c]|,|[b,c]|) & ax in LSeg(|[b,c]|,|[b,d]|)
                     by TOPREAL1:6;
   hence ax in LSeg(|[a,c]|,|[b,c]|) /\ LSeg(|[b,c]|,|[b,d]|)
                   by XBOOLE_0:def 3;
