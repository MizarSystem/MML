A257:               p=x & (p`1=-1 & -1 <=p`2 & p`2<=1 or
                    p`2=1 & -1<=p`1 & p`1<=1 or
                    p`1=1 & -1 <=p`2 & p`2<=1 or p`2=-1 & -1<=p`1 & p`1<=1)
                    by A256;
                    thus x in {q: q`1=-1 & -1 <=q`2 & q`2<=1 or
                    q`1=1 & -1 <=q`2 & q`2<=1 or -1=q`2 & -1<=q`1 & q`1<=1 or
                    1=q`2 & -1<=q`1 & q`1<=1} by A257;
                  end; thus
                  {p: p`1=-1 & -1 <=p`2 & p`2<=1 or
                  p`1=1 & -1 <=p`2 & p`2<=1 or -1=p`2 & -1<=p`1 & p`1<=1 or
                  1=p`2 & -1<=p`1 & p`1<=1} c= K
                  proof
                    let x be set;
                    assume x in {p: p`1=-1 & -1 <=p`2 & p`2<=1 or
                    p`1=1 & -1 <=p`2 & p`2<=1 or -1=p`2 & -1<=p`1 & p`1<=1 or
                    1=p`2 & -1<=p`1 & p`1<=1};
                    then consider p such that
A258:               p=x & (p`1=-1 & -1 <=p`2 & p`2<=1 or
                    p`1=1 & -1 <=p`2 & p`2<=1 or -1=p`2 & -1<=p`1 & p`1<=1 or
                    1=p`2 & -1<=p`1 & p`1<=1);
                    thus x in K by A256,A258;
                  end;
                end;
                thus f.p1,f.p2,f.p3,f.p4 are_in_this_order_on P
                by A190,A191,A192,JORDAN17:def 1;
              end;
            end;
            hence f.p1,f.p2,f.p3,f.p4 are_in_this_order_on P;
          end;
        end;
        hence f.p1,f.p2,f.p3,f.p4 are_in_this_order_on P;
      end;
      case
A259:   p1 in LSeg(|[-1,1]|,|[1,1]|);
A260:   |[-1,1]| in LSeg(|[-1,1]|,|[1,1]|) by TOPREAL1:6;
A261:   |[-1,1]| in LSeg(|[-1,-1]|,|[-1,1]|) by TOPREAL1:6;
A262:   (|[-1,1]|)`1=-1 & (|[-1,1]|)`2=1 by EUCLID:56;
        p1`2=1 & -1 <=p1`1 & p1`1<=1 by A259,Th11;
        then
A263:   LE |[-1,1]|,p1,K by A259,A260,A262,Th70;
        then
A264:   LE f.p1,f.p2,P by A1,A5,A261,A262,Th76;
A265:   LE |[-1,1]|,p2,K by A5,A263,Th60,JORDAN6:73;
        then
A266:   LE f.p2,f.p3,P by A1,A5,A261,A262,Th76;
        LE |[-1,1]|,p3,K by A5,A265,Th60,JORDAN6:73;
        then LE f.p3,f.p4,P by A1,A5,A261,A262,Th76;
        hence f.p1,f.p2,f.p3,f.p4 are_in_this_order_on P
        by A264,A266,JORDAN17:def 1;
      end;
      case
A267:   p1 in LSeg(|[1,1]|,|[1,-1]|);
A268:   |[-1,1]| in LSeg(|[-1,1]|,|[1,1]|) by TOPREAL1:6;
A269:   |[-1,1]| in LSeg(|[-1,-1]|,|[-1,1]|) by TOPREAL1:6;
A270:   (|[-1,1]|)`1=-1 & (|[-1,1]|)`2=1 by EUCLID:56;
A271:   |[1,1]| in LSeg(|[1,1]|,|[1,-1]|) by TOPREAL1:6;
A272:   |[1,1]| in LSeg(|[-1,1]|,|[1,1]|) by TOPREAL1:6;
A273:   (|[1,1]|)`1=1 & (|[1,1]|)`2=1 by EUCLID:56;
        then
A274:   LE |[-1,1]|,|[1,1]|,K by A268,A270,A272,Th70;
        p1`1=1 & -1 <=p1`2 & p1`2<=1 by A267,Th9;
        then LE |[1,1]|,p1,K by A267,A271,A273,Th71;
        then
A275:   LE |[-1,1]|,p1,K by A274,Th60,JORDAN6:73;
        then
A276:   LE f.p1,f.p2,P by A1,A5,A269,A270,Th76;
A277:   LE |[-1,1]|,p2,K by A5,A275,Th60,JORDAN6:73;
        then
A278:   LE f.p2,f.p3,P by A1,A5,A269,A270,Th76;
        LE |[-1,1]|,p3,K by A5,A277,Th60,JORDAN6:73;
        then LE f.p3,f.p4,P by A1,A5,A269,A270,Th76;
        hence f.p1,f.p2,f.p3,f.p4 are_in_this_order_on P
        by A276,A278,JORDAN17:def 1;
      end;
      case
A279:   p1 in LSeg(|[1,-1]|,|[-1,-1]|) & p1 <> W-min(K);
A280:   |[-1,1]| in LSeg(|[-1,1]|,|[1,1]|) by TOPREAL1:6;
A281:   |[-1,1]| in LSeg(|[-1,-1]|,|[-1,1]|) by TOPREAL1:6;
A282:   (|[-1,1]|)`1=-1 & (|[-1,1]|)`2=1 by EUCLID:56;
A283:   |[1,1]| in LSeg(|[-1,1]|,|[1,1]|) by TOPREAL1:6;
        (|[1,1]|)`1=1 & (|[1,1]|)`2=1 by EUCLID:56;
        then
A284:   LE |[-1,1]|,|[1,1]|,K by A280,A282,A283,Th70;
A285:   |[1,-1]| in LSeg(|[1,1]|,|[1,-1]|) by TOPREAL1:6;
A286:   |[1,-1]| in LSeg(|[1,-1]|,|[-1,-1]|) by TOPREAL1:6;
A287:   (|[1,-1]|)`1=1 & (|[1,-1]|)`2= -1 by EUCLID:56;
        LE |[1,1]|,|[1,-1]|,K by A283,A285,Th70;
        then
A288:   LE |[-1,1]|,|[1,-1]|,K by A284,Th60,JORDAN6:73;
        W-min(K)=|[-1,-1]| by Th56;
        then
A289:   (W-min(K))`1=-1 & (W-min(K))`2=-1 by EUCLID:56;
        p1`2=-1 & -1 <=p1`1 & p1`1<=1 by A279,Th11;
        then LE |[1,-1]|,p1,K by A279,A286,A287,A289,Th72;
        then
A290:   LE |[-1,1]|,p1,K by A288,Th60,JORDAN6:73;
        then
A291:   LE f.p1,f.p2,P by A1,A5,A281,A282,Th76;
A292:   LE |[-1,1]|,p2,K by A5,A290,Th60,JORDAN6:73;
        then
A293:   LE f.p2,f.p3,P by A1,A5,A281,A282,Th76;
        LE |[-1,1]|,p3,K by A5,A292,Th60,JORDAN6:73;
        then LE f.p3,f.p4,P by A1,A5,A281,A282,Th76;
        hence f.p1,f.p2,f.p3,f.p4 are_in_this_order_on P
        by A291,A293,JORDAN17:def 1;
      end;
    end;
    hence f.p1,f.p2,f.p3,f.p4 are_in_this_order_on P;
  end;
end;

theorem Th83:
  for p1,p2 being Point of TOP-REAL 2,
  P being non empty compact Subset of TOP-REAL 2 st
  P is_simple_closed_curve & p1 in P & p2 in P & not LE p1,p2,P
  holds LE p2,p1,P
proof
  let p1,p2 be Point of TOP-REAL 2,
  P be non empty compact Subset of TOP-REAL 2;
  assume
A1: P is_simple_closed_curve & p1 in P & p2 in P & not LE p1,p2,P;
  then
A2: P=Upper_Arc(P) \/ Lower_Arc(P) by JORDAN6:def 9;
A3: not p1=W-min(P) by A1,JORDAN7:3;
  per cases by A1,A2,XBOOLE_0:def 2;
  suppose
A4: p1 in Upper_Arc(P) & p2 in Upper_Arc(P);
A5: Upper_Arc(P) is_an_arc_of W-min(P),E-max(P) by A1,JORDAN6:def 8;
    set q1=W-min(P),q2=E-max(P);
    set Q= Upper_Arc(P);
    now per cases;
      case
A6:     p1<>p2;
        now per cases by A4,A5,A6,JORDAN5C:14;
          case LE p1,p2,Q,q1,q2 & not LE p2,p1,Q,q1,q2;
            hence contradiction by A1,A4,JORDAN6:def 10;
          end;
          case LE p2,p1,Q,q1,q2 & not LE p1,p2,Q,q1,q2;
            hence LE p2,p1,P by A4,JORDAN6:def 10;
          end;
        end;
        hence LE p2,p1,P;
      end;
      case p1=p2;
        hence LE p2,p1,P by A1,JORDAN6:71;
      end;
    end;
    hence LE p2,p1,P;
  end;
  suppose
A7: p1 in Upper_Arc(P) & p2 in Lower_Arc(P);
    now per cases;
      case p2=W-min(P);
        hence LE p2,p1,P by A1,JORDAN7:3;
      end;
      case p2<>W-min(P);
        hence contradiction by A1,A7,JORDAN6:def 10;
      end;
    end;
    hence LE p2,p1,P;
  end;
  suppose p1 in Lower_Arc(P) & p2 in Upper_Arc(P);
    hence LE p2,p1,P by A3,JORDAN6:def 10;
  end;
  suppose
A8: p1 in Lower_Arc(P) & p2 in Lower_Arc(P);
A9: Lower_Arc(P) is_an_arc_of E-max(P),W-min(P) by A1,JORDAN6:65;
    set q2=W-min(P),q1=E-max(P);
    set Q= Lower_Arc(P);
    now per cases;
      case
A10:    p1<>p2;
        now per cases by A8,A9,A10,JORDAN5C:14;
          case
A11:        LE p1,p2,Q,q1,q2 & not LE p2,p1,Q,q1,q2;
            now per cases;
              case p2=W-min(P);
                hence LE p2,p1,P by A1,JORDAN7:3;
              end;
              case p2<>W-min(P);
                hence contradiction by A1,A8,A11,JORDAN6:def 10;
              end;
            end;
            hence LE p2,p1,P;
          end;
          case LE p2,p1,Q,q1,q2 & not LE p1,p2,Q,q1,q2;
            hence LE p2,p1,P by A3,A8,JORDAN6:def 10;
          end;
        end;
        hence LE p2,p1,P;
      end;
      case p1=p2;
        hence LE p2,p1,P by A1,JORDAN6:71;
      end;
    end;
    hence LE p2,p1,P;
  end;
end;

theorem
  for p1,p2,p3 being Point of TOP-REAL 2,
  P being non empty compact Subset of TOP-REAL 2 st
  P is_simple_closed_curve & p1 in P & p2 in P & p3 in P holds
  LE p1,p2,P & LE p2,p3,P or LE p1,p3,P & LE p3,p2,P or
  LE p2,p1,P & LE p1,p3,P or LE p2,p3,P & LE p3,p1,P or
  LE p3,p1,P & LE p1,p2,P or LE p3,p2,P & LE p2,p1,P by Th83;

theorem
  for p1,p2,p3 being Point of TOP-REAL 2,
  P being non empty compact Subset of TOP-REAL 2 st
  P is_simple_closed_curve & p1 in P & p2 in P & p3 in P & LE p2,p3,P holds
  LE p1,p2,P or LE p2,p1,P & LE p1,p3,P or LE p3,p1,P by Th83;

theorem
  for p1,p2,p3,p4 being Point of TOP-REAL 2,
  P being non empty compact Subset of TOP-REAL 2 st
  P is_simple_closed_curve & p1 in P & p2 in P & p3 in P & p4 in P &
  LE p2,p3,P & LE p3,p4,P holds
  LE p1,p2,P or LE p2,p1,P & LE p1,p3,P or LE p3,p1,P & LE p1,p4,P or
  LE p4,p1,P by Th83;

theorem Th87:
  for p1,p2,p3,p4 being Point of TOP-REAL 2,
  P being non empty compact Subset of TOP-REAL 2,
  f being Function of TOP-REAL 2,TOP-REAL 2 st P= circle(0,0,1) & f=Sq_Circ &
  LE f.p1,f.p2,P & LE f.p2,f.p3,P & LE f.p3,f.p4,P
  holds p1,p2,p3,p4 are_in_this_order_on rectangle(-1,1,-1,1)
proof
  let p1,p2,p3,p4 be Point of TOP-REAL 2,
  P be non empty compact Subset of TOP-REAL 2,
  f be Function of TOP-REAL 2,TOP-REAL 2;
  set K = rectangle(-1,1,-1,1);
  assume
A1: P= circle(0,0,1) & f=Sq_Circ &
  LE f.p1,f.p2,P & LE f.p2,f.p3,P & LE f.p3,f.p4,P;
A2: K is_simple_closed_curve by Th60;
A3: P={p: |.p.|=1} by A1,Th33;
  then
A4: LE f.p1,f.p3,P by A1,JGRAPH_3:36,JORDAN6:73;
A5: LE f.p2,f.p4,P by A1,A3,JGRAPH_3:36,JORDAN6:73;
A6: f.:K=P by A1,A3,Lm8,Th45,JGRAPH_3:33;
A7: dom f = the carrier of TOP-REAL 2 by FUNCT_2:def 1;
A8: P={p: |.p.|=1} by A1,Th33;
  then
A9: P is_simple_closed_curve by JGRAPH_3:36;
  then f.p1 in P by A1,JORDAN7:5;
  then consider x1 being set such that
A10: x1 in dom f & x1 in K & f.p1=f.x1 by A6,FUNCT_1:def 12;
A11: p1 in K by A1,A7,A10,FUNCT_1:def 8;
  f.p2 in P by A1,A9,JORDAN7:5;
  then consider x2 being set such that
A12: x2 in dom f & x2 in K & f.p2=f.x2 by A6,FUNCT_1:def 12;
A13: p2 in K by A1,A7,A12,FUNCT_1:def 8;
  f.p3 in P by A1,A9,JORDAN7:5;
  then consider x3 being set such that
A14: x3 in dom f & x3 in K & f.p3=f.x3 by A6,FUNCT_1:def 12;
A15: p3 in K by A1,A7,A14,FUNCT_1:def 8;
  f.p4 in P by A1,A9,JORDAN7:5;
  then consider x4 being set such that
A16: x4 in dom f & x4 in K & f.p4=f.x4 by A6,FUNCT_1:def 12;
A17: p4 in K by A1,A7,A16,FUNCT_1:def 8;
  now
    assume
A18: not p1,p2,p3,p4 are_in_this_order_on K;
A19: now
      assume
A20:  p1,p2,p4,p3 are_in_this_order_on K;
      now per cases by A20,JORDAN17:def 1;
        case
A21:      LE p1,p2,K & LE p2,p4,K & LE p4,p3,K;
          then f.p1,f.p2,f.p4,f.p3 are_in_this_order_on P by A1,Th82;
          then LE f.p1,f.p2,P & LE f.p2,f.p4,P & LE f.p4,f.p3,P or
          LE f.p2,f.p4,P & LE f.p4,f.p3,P & LE f.p3,f.p1,P or
          LE f.p4,f.p3,P & LE f.p3,f.p1,P & LE f.p1,f.p2,P or
          LE f.p3,f.p1,P & LE f.p1,f.p2,P & LE f.p2,f.p4,P by JORDAN17:def 1;
          then f.p3=f.p4 or f.p3=f.p1 by A1,A4,A8,JGRAPH_3:36,JORDAN6:72;
          then
A22:      p3=p4 or p3=p1 by A1,A7,FUNCT_1:def 8;
          LE p1,p4,K by A21,Th60,JORDAN6:73;
          then p3=p1 & p1=p4 by A18,A20,A21,A22,Th60,JORDAN6:72;
          hence contradiction by A18,A20;
        end;
        case
A23:      LE p2,p4,K & LE p4,p3,K & LE p3,p1,K;
          then f.p2,f.p4,f.p3,f.p1 are_in_this_order_on P by A1,Th82;
          then LE f.p2,f.p4,P & LE f.p4,f.p3,P & LE f.p3,f.p1,P or
          LE f.p4,f.p3,P & LE f.p3,f.p1,P & LE f.p1,f.p2,P or
          LE f.p3,f.p1,P & LE f.p1,f.p2,P & LE f.p2,f.p4,P or
          LE f.p1,f.p2,P & LE f.p2,f.p4,P & LE f.p4,f.p3,P by JORDAN17:def 1;
          then f.p3=f.p4 or LE f.p3,f.p2,P by A1,A9,JORDAN6:72,73;
          then f.p3=f.p4 or f.p3=f.p2 by A1,A8,JGRAPH_3:36,JORDAN6:72;
          then p3=p4 or p3=p2 by A1,A7,FUNCT_1:def 8;
          then p3=p2 & p4=p2 by A18,A20,A23,Th60,JORDAN6:72;
          hence contradiction by A18,A20;
        end;
        case
A24:      LE p4,p3,K & LE p3,p1,K & LE p1,p2,K;
          then f.p4,f.p3,f.p1,f.p2 are_in_this_order_on P by A1,Th82;
          then LE f.p4,f.p3,P & LE f.p3,f.p1,P & LE f.p1,f.p2,P or
          LE f.p3,f.p1,P & LE f.p1,f.p2,P & LE f.p2,f.p4,P or
          LE f.p1,f.p2,P & LE f.p2,f.p4,P & LE f.p4,f.p3,P or
          LE f.p2,f.p4,P & LE f.p4,f.p3,P & LE f.p3,f.p1,P by JORDAN17:def 1;
          then f.p3=f.p4 or LE f.p3,f.p2,P by A1,A9,JORDAN6:72,73;
          then f.p3=f.p4 or f.p3=f.p2 by A1,A8,JGRAPH_3:36,JORDAN6:72;
          then p3=p4 or p3=p2 by A1,A7,FUNCT_1:def 8;
          then p3=p2 & p3=p1 by A18,A20,A24,Th60,JORDAN6:72;
          hence contradiction by A2,A18,A20,JORDAN17:12;
        end;
        case
A25:      LE p3,p1,K & LE p1,p2,K & LE p2,p4,K;
          then f.p3,f.p1,f.p2,f.p4 are_in_this_order_on P by A1,Th82;
          then LE f.p4,f.p3,P & LE f.p3,f.p1,P & LE f.p1,f.p2,P or
          LE f.p3,f.p1,P & LE f.p1,f.p2,P & LE f.p2,f.p4,P or
          LE f.p1,f.p2,P & LE f.p2,f.p4,P & LE f.p4,f.p3,P or
          LE f.p2,f.p4,P & LE f.p4,f.p3,P & LE f.p3,f.p1,P by JORDAN17:def 1;
          then f.p3=f.p4 or LE f.p3,f.p2,P by A1,A9,JORDAN6:72,73;
          then f.p3=f.p4 or f.p3=f.p2 by A1,A8,JGRAPH_3:36,JORDAN6:72;
          then p3=p4 or p3=p2 by A1,A7,FUNCT_1:def 8;
          then p3=p2 & p3=p1 by A18,A20,A25,Th60,JORDAN6:72;
          hence contradiction by A2,A18,A20,JORDAN17:12;
        end;
      end;
      hence contradiction;
    end;
A26: now
      assume
A27:  p1,p3,p4,p2 are_in_this_order_on K;
      now per cases by A27,JORDAN17:def 1;
        case LE p1,p3,K & LE p3,p4,K & LE p4,p2,K;
          then f.p1,f.p3,f.p4,f.p2 are_in_this_order_on P by A1,Th82;
          then LE f.p1,f.p3,P & LE f.p3,f.p4,P & LE f.p4,f.p2,P or
          LE f.p3,f.p4,P & LE f.p4,f.p2,P & LE f.p2,f.p1,P or
          LE f.p4,f.p2,P & LE f.p2,f.p1,P & LE f.p1,f.p3,P or
          LE f.p2,f.p1,P & LE f.p1,f.p3,P & LE f.p3,f.p4,P by JORDAN17:def 1;
          then f.p4=f.p2 or f.p2=f.p1 by A1,A5,A8,JGRAPH_3:36,JORDAN6:72;
          then
A28:      p4=p2 or p2=p1 by A1,A7,FUNCT_1:def 8;
          then f.p3=f.p2 or f.p4=f.p1 by A1,A2,A9,A18,A27,JORDAN17:12
          ,JORDAN6:72;
          then p3=p2 or p4=p1 by A1,A7,FUNCT_1:def 8;
          hence contradiction by A2,A18,A27,A28,JORDAN17:12;
        end;
        case LE p3,p4,K & LE p4,p2,K & LE p2,p1,K;
          then f.p3,f.p4,f.p2,f.p1 are_in_this_order_on P by A1,Th82;
          then
A29:      LE f.p1,f.p3,P & LE f.p3,f.p4,P & LE f.p4,f.p2,P or
          LE f.p3,f.p4,P & LE f.p4,f.p2,P & LE f.p2,f.p1,P or
          LE f.p4,f.p2,P & LE f.p2,f.p1,P & LE f.p1,f.p3,P or
          LE f.p2,f.p1,P & LE f.p1,f.p3,P & LE f.p3,f.p4,P by JORDAN17:def 1;
          then f.p4=f.p2 or f.p2=f.p1 by A1,A5,A8,JGRAPH_3:36,JORDAN6:72;
          then
A30:      p4=p2 or p2=p1 by A1,A7,FUNCT_1:def 8;
          f.p2=f.p1 or LE f.p3,f.p2,P by A1,A9,A29,JORDAN6:72,73;
          then f.p2=f.p1 or f.p3=f.p2 by A1,A8,JGRAPH_3:36,JORDAN6:72;
          then p2=p1 or p3=p2 by A1,A7,FUNCT_1:def 8;
          hence contradiction by A2,A18,A27,A30,JORDAN17:12;
        end;
        case LE p4,p2,K & LE p2,p1,K & LE p1,p3,K;
          then f.p4,f.p2,f.p1,f.p3 are_in_this_order_on P by A1,Th82;
