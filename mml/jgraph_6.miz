theorem Th33:
  for r being real number holds inside_of_circle(0,0,r)={p : |.p.|<r} &
  (r>0 implies circle(0,0,r)={p2 : |.p2.|=r}) &
  outside_of_circle(0,0,r)={p3 : |.p3.|>r} &
  closed_inside_of_circle(0,0,r)={q : |.q.|<=r} &
  closed_outside_of_circle(0,0,r)={q2 : |.q2.|>=r}
proof
  let r be real number;
  defpred P[Point of TOP-REAL 2] means |.$1- |[0,0]| .|<r;
  defpred Q[Point of TOP-REAL 2] means |.$1.|<r;
  deffunc F(set)=$1;
A1: for p holds P[p] iff Q[p] by EUCLID:58,JORDAN2C:13;
  inside_of_circle(0,0,r) = {F(p) where p is Point of TOP-REAL 2: P[p]}
    .= {F(p2) where p2 is Point of TOP-REAL 2: Q[p2]} from FRAENKEL:sch 3(A1);
  hence inside_of_circle(0,0,r)={p : |.p.|<r};
  defpred P[Point of TOP-REAL 2] means |.$1- |[0,0]| .|=r;
  defpred Q[Point of TOP-REAL 2] means |.$1.|=r;
A2: for p holds P[p] iff Q[p] by EUCLID:58,JORDAN2C:13;
  hereby
    assume r>0;
    circle(0,0,r)= {F(p): P[p]}

.= {F(p2) where p2 is Point of TOP-REAL 2: Q[p2]} from FRAENKEL:sch 3(A2);
    hence circle(0,0,r)={p2 : |.p2.|=r};
  end;
  defpred P[Point of TOP-REAL 2] means |.$1- |[0,0]| .|>r;
  defpred Q[Point of TOP-REAL 2] means |.$1.|>r;
A3: for p holds P[p] iff Q[p] by EUCLID:58,JORDAN2C:13;
  outside_of_circle(0,0,r)= {F(p): P[p]}
    .= {F(p2) where p2 is Point of TOP-REAL 2: Q[p2]} from FRAENKEL:sch 3(A3);
  hence outside_of_circle(0,0,r)={p3 : |.p3.|>r};
  defpred P[Point of TOP-REAL 2] means |.$1- |[0,0]| .|<=r;
  defpred Q[Point of TOP-REAL 2] means |.$1.|<=r;
A4: for p holds P[p] iff Q[p] by EUCLID:58,JORDAN2C:13;
  closed_inside_of_circle(0,0,r)= {F(p): P[p]}
    .= {F(p2) where p2 is Point of TOP-REAL 2: Q[p2]} from FRAENKEL:sch 3(A4);
  hence closed_inside_of_circle(0,0,r)={p : |.p.|<=r};
  defpred P[Point of TOP-REAL 2] means |.$1- |[0,0]| .|>=r;
  defpred Q[Point of TOP-REAL 2] means |.$1.|>=r;
A5: for p holds P[p] iff Q[p] by EUCLID:58,JORDAN2C:13;
  closed_outside_of_circle(0,0,r)= {F(p): P[p]}
    .= {F(p2) where p2 is Point of TOP-REAL 2: Q[p2]} from FRAENKEL:sch 3(A5);
  hence closed_outside_of_circle(0,0,r)={p3 : |.p3.|>=r};
end;

theorem Th34:
  for Kb,Cb being Subset of TOP-REAL 2 st
  Kb={p: -1 <p`1 & p`1< 1 & -1 <p`2 & p`2< 1}&
  Cb={p2 where p2 is Point of TOP-REAL 2: |.p2.|<1} holds Sq_Circ.:Kb=Cb
proof
  let Kb,Cb be Subset of TOP-REAL 2;
  assume
A1: Kb={p: -1 <p`1 & p`1< 1 & -1 <p`2 & p`2< 1}&
  Cb={p2 where p2 is Point of TOP-REAL 2: |.p2.|<1};
  thus Sq_Circ.:Kb c= Cb
  proof
    let y be set;
    assume y in Sq_Circ.:Kb;
    then consider x being set such that
A2: x in dom Sq_Circ & x in Kb & y=Sq_Circ.x by FUNCT_1:def 12;
    consider q being Point of TOP-REAL 2 such that
A3: q=x & -1 <q`1 & q`1< 1 & -1 <q`2 & q`2< 1 by A1,A2;
    now per cases;
      case
A4:     q=0.REAL 2;
        then
A5:     Sq_Circ.q=q by JGRAPH_3:def 1;
        |.q.|=0 by A4,TOPRNS_1:24;
        hence ex p2 being Point of TOP-REAL 2 st p2=y & |.p2.|<1 by A2,A3,A5;
      end;
      case
A6:     q<>0.REAL 2 & (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
        then
A7:     Sq_Circ.q=|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2 )]|
        by JGRAPH_3:def 1;
A8:     (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`1
        = q`1/sqrt(1+(q`2/q`1)^2) &
        (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`2
        = q`2/sqrt(1+(q`2/q`1)^2) by EUCLID:56;
A9:     1+(q`2/q`1)^2>0 by XREAL_1:36,65;
A10:    now
          assume
A11:      q`1=0;
          then q`2=0 by A6;
          hence contradiction by A6,A11,EUCLID:57,58;
        end;
        then
A12:    (q`1)^2>0 by SQUARE_1:74;
        (q`1)^2<1^2 by A3,SQUARE_1:120;
        then
A13:    sqrt((q`1)^2)<1 by A12,SQUARE_1:83,95;
        |.(|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|).|^2
        =((q`1)/sqrt(1+(q`2/(q`1))^2))^2+(q`2/sqrt(1+(q`2/(q`1))^2))^2
        by A8,JGRAPH_3:10
          .=(q`1)^2/(sqrt(1+(q`2/(q`1))^2))^2+(q`2/sqrt(1+(q`2/(q`1))^2))^2
        by XCMPLX_1:77
          .=(q`1)^2/(sqrt(1+(q`2/(q`1))^2))^2
        +(q`2)^2/(sqrt(1+(q`2/(q`1))^2))^2 by XCMPLX_1:77
          .=(q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
        by A9,SQUARE_1:def 4
          .=(q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(1+(q`2/q`1)^2)
        by A9,SQUARE_1:def 4
          .=((q`1)^2+(q`2)^2)/(1+(q`2/q`1)^2) by XCMPLX_1:63
          .=((q`1)^2+(q`2)^2)/(1+(q`2)^2/(q`1)^2) by XCMPLX_1:77
          .=((q`1)^2+(q`2)^2)/((q`1)^2/(q`1)^2+(q`2)^2/(q`1)^2)
        by A12,XCMPLX_1:60
          .=((q`1)^2+(q`2)^2)/(((q`1)^2+(q`2)^2)/(q`1)^2)
        by XCMPLX_1:63
          .=(q`1)^2*(((q`1)^2+(q`2)^2)/((q`1)^2+(q`2)^2))
        by XCMPLX_1:82
          .=(q`1)^2*1 by A10,COMPLEX1:2,XCMPLX_1:60
          .=(q`1)^2;
        then |.(|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|).|<1
        by A13,SQUARE_1:89;
        hence ex p2 being Point of TOP-REAL 2 st p2=y & |.p2.|<1 by A2,A3,A7;
      end;
      case
A14:    q<>0.REAL 2 & not(q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
        then
A15:    Sq_Circ.q=|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2 )]|
        by JGRAPH_3:def 1;
A16:    (|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|)`1
        = q`1/sqrt(1+(q`1/q`2)^2) &
        (|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|)`2
        = q`2/sqrt(1+(q`1/q`2)^2) by EUCLID:56;
A17:    1+(q`1/q`2)^2>0 by XREAL_1:36,65;
A18:    q`2 <> 0 by A14;
        then
A19:    (q`2)^2>0 by SQUARE_1:74;
        (q`2)^2<1^2 by A3,SQUARE_1:120;
        then
A20:    sqrt((q`2)^2)<1 by A19,SQUARE_1:83,95;
        |.(|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|).|^2
        =((q`1)/sqrt(1+(q`1/(q`2))^2))^2+(q`2/sqrt(1+(q`1/(q`2))^2))^2
        by A16,JGRAPH_3:10
          .=(q`1)^2/(sqrt(1+(q`1/(q`2))^2))^2+(q`2/sqrt(1+(q`1/(q`2))^2))^2
        by XCMPLX_1:77
          .=(q`1)^2/(sqrt(1+(q`1/(q`2))^2))^2
        +(q`2)^2/(sqrt(1+(q`1/(q`2))^2))^2 by XCMPLX_1:77
          .=(q`1)^2/(1+(q`1/q`2)^2)+(q`2)^2/(sqrt(1+(q`1/q`2)^2))^2
        by A17,SQUARE_1:def 4
          .=(q`1)^2/(1+(q`1/q`2)^2)+(q`2)^2/(1+(q`1/q`2)^2)
        by A17,SQUARE_1:def 4
          .=((q`1)^2+(q`2)^2)/(1+(q`1/q`2)^2) by XCMPLX_1:63
          .=((q`1)^2+(q`2)^2)/(1+(q`1)^2/(q`2)^2) by XCMPLX_1:77
          .=((q`1)^2+(q`2)^2)/((q`1)^2/(q`2)^2+(q`2)^2/(q`2)^2)
        by A19,XCMPLX_1:60
          .=((q`1)^2+(q`2)^2)/(((q`1)^2+(q`2)^2)/(q`2)^2)
        by XCMPLX_1:63
          .=(q`2)^2*(((q`1)^2+(q`2)^2)/((q`1)^2+(q`2)^2))
        by XCMPLX_1:82
          .=(q`2)^2*1 by A18,COMPLEX1:2,XCMPLX_1:60
          .=(q`2)^2;
        then |.(|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|).|<1
        by A20,SQUARE_1:89;
        hence ex p2 being Point of TOP-REAL 2 st p2=y & |.p2.|<1 by A2,A3,A15;
      end;
    end;
    hence y in Cb by A1;
  end;
  let y be set;
  assume y in Cb;
  then consider p2 being Point of TOP-REAL 2 such that
A21: p2=y & |.p2.|<1 by A1;
  set q=p2;
  now per cases;
    case
A22:  q=0.REAL 2;
      then q`1=0 & q`2=0 by EUCLID:56,58;
      then
A23:  y in Kb by A1,A21;
A24:  Sq_Circ".y=y by A21,A22,JGRAPH_3:38;
A25:  dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
      y=Sq_Circ.y by A21,A24,FUNCT_1:57,JGRAPH_3:54;
      hence ex x being set st x in dom Sq_Circ & x in Kb & y=Sq_Circ.x
      by A23,A25;
    end;
    case
A26:  q<>0.REAL 2 & (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
      set px=|[q`1*sqrt(1+(q`2/q`1)^2),q`2*sqrt(1+(q`2/q`1)^2)]|;
A27:  px`1 = q`1*sqrt(1+(q`2/q`1)^2) &
      px`2 = q`2*sqrt(1+(q`2/q`1)^2) by EUCLID:56;
      1+(q`2/q`1)^2>0 by XREAL_1:36,65;
      then
A28:  sqrt(1+(q`2/q`1)^2)>0 by SQUARE_1:93;
A29:  1+(px`2/px`1)^2>0 by XREAL_1:36,65;
A30:  px`2/px`1=q`2/q`1 by A27,A28,XCMPLX_1:92;
A31:  q`1=q`1*sqrt(1+(q`2/q`1)^2)/(sqrt(1+(q`2/q`1)^2))by A28,XCMPLX_1:90
        .=px`1/(sqrt(1+(q`2/q`1)^2)) by EUCLID:56;
A32:  q`2=q`2*sqrt(1+(q`2/q`1)^2)/(sqrt(1+(q`2/q`1)^2))by A28,XCMPLX_1:90
        .=px`2/(sqrt(1+(q`2/q`1)^2)) by EUCLID:56;
A33:  |.q.|^2=q`1^2+q`2^2 by JGRAPH_3:10;
A34:  |.q.|^2<1 by A21,SQUARE_1:59,78;
A35:  now
        assume px`1=0 & px`2=0;
        then
A36:    q`1*sqrt(1+(q`2/q`1)^2)=0 & q`2*sqrt(1+(q`2/q`1)^2)=0 by EUCLID:56;
        then
A37:    q`1=0 by A28,XCMPLX_1:6;
        q`2=0 by A28,A36,XCMPLX_1:6;
        hence contradiction by A26,A37,EUCLID:57,58;
      end;
      q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 &
      q`2*sqrt(1+(q`2/q`1)^2) <= (-q`1)*sqrt(1+(q`2/q`1)^2)
      by A26,A28,XREAL_1:66;
      then
A38:  q`2<=q`1 & (-q`1)*sqrt(1+(q`2/q`1)^2) <= q`2*sqrt(1+(q`2/q`1) ^2)
      or px`2>=px`1 & px`2<=-px`1 by A27,A28,XREAL_1:66;
      then px`2<=px`1 & -px`1<=px`2 or px`2>=px`1 & px`2<=-px`1
      by A27,A28,XREAL_1:66;
      then
A39:  Sq_Circ.px=|[px`1/sqrt(1+(px`2/px`1)^2),px`2/sqrt(1+(px`2/px`1 )^2) ]|
      by A35,JGRAPH_2:11,JGRAPH_3:def 1;
      px`2<=px`1 & --px`1>=-px`2 or px`2>=px`1 & px`2<=-px`1
      by A27,A28,A38,XREAL_1:26,66;
      then
A40:  px`2<=px`1 & px`1>=-px`2 or px`2>=px`1 & -px`2>=--px`1 by XREAL_1:26;
A41:  px`1/sqrt(1+(px`2/px`1)^2)=q`1 by A27,A28,A30,XCMPLX_1:90;
A42:  px`2/sqrt(1+(px`2/px`1)^2)=q`2 by A27,A28,A30,XCMPLX_1:90;
A43:  dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
      not px`1=0 by A27,A28,A35,A38,XREAL_1:66;
      then
A44:  (px`1)^2>0 by SQUARE_1:74;
A45:  (px`2)^2>=0 by XREAL_1:65;
      (px`1)^2/(sqrt(1+(px`2/px`1)^2))^2+(px`2/sqrt(1+(px`2/px`1)^2)) ^2 < 1
      by A30,A31,A32,A33,A34,XCMPLX_1:77;
      then (px`1)^2/(sqrt(1+(px`2/px`1)^2))^2+(px`2)^2/(sqrt(1+(px`2/px`1)^2))
      ^2<1 by XCMPLX_1:77;
      then (px`1)^2/(1+(px`2/px`1)^2)+(px`2)^2/(sqrt(1+(px`2/px`1)^2))^2<1
      by A29,SQUARE_1:def 4;
      then (px`1)^2/(1+(px`2/px`1)^2)+(px`2)^2/(1+(px`2/px`1)^2)<1
      by A29,SQUARE_1:def 4;
      then ((px`1)^2/(1+(px`2/px`1)^2)
      +(px`2)^2/(1+(px`2/px`1)^2))*(1+(px`2/px`1)^2)<1 *(1+(px`2/px`1)^2)
      by A29,XREAL_1:70;
      then (px`1)^2/(1+(px`2/px`1)^2)*(1+(px`2/px`1)^2)
      +(px`2)^2/(1+(px`2/px`1)^2)*(1+(px`2/px`1)^2)<1 *(1+(px`2/px`1)^2);
      then (px`1)^2+(px`2)^2/(1+(px`2/px`1)^2)*(1+(px`2/px`1)^2)<1 *(1+(px`2/
      px `1)^2) by A29,XCMPLX_1:88;
      then
A46:  (px`1)^2+(px`2)^2<1 *(1+(px`2/px`1)^2) by A29,XCMPLX_1:88;
