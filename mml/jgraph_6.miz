          LE f.p2,f.p1,P & LE f.p1,f.p3,P & LE f.p3,f.p4,P by JORDAN17:def 1;
          then f.p4=f.p2 or f.p2=f.p1 by A1,A5,A8,JGRAPH_3:36,JORDAN6:72;
          then
A32:      p4=p2 or p2=p1 by A1,A7,FUNCT_1:def 8;
          f.p2=f.p1 or LE f.p3,f.p2,P by A1,A9,A31,JORDAN6:72,73;
          then f.p2=f.p1 or f.p3=f.p2 by A1,A8,JGRAPH_3:36,JORDAN6:72;
          then p2=p1 or p3=p2 by A1,A7,FUNCT_1:def 8;
          hence contradiction by A2,A18,A27,A32,JORDAN17:12;
        end;
        case
A33:      LE p2,p1,K & LE p1,p3,K & LE p3,p4,K;
          then f.p2,f.p1,f.p3,f.p4 are_in_this_order_on P by A1,Th82;
          then LE f.p1,f.p3,P & LE f.p3,f.p4,P & LE f.p4,f.p2,P or
          LE f.p3,f.p4,P & LE f.p4,f.p2,P & LE f.p2,f.p1,P or
