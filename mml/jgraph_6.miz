theorem Th67:
  for a,b,c,d being real number,p1,p2 being Point of TOP-REAL 2 st a<b & c <d
  & p1 in LSeg(|[b,c]|,|[b,d]|) & p2 in LSeg(|[b,c]|,|[b,d]|)
  holds LE p1,p2,rectangle(a,b,c,d) iff p1`2>=p2`2
proof
  let a,b,c,d be real number,p1,p2 be Point of TOP-REAL 2;
  set K = rectangle(a,b,c,d);
  assume
A1: a<b & c <d & p1 in LSeg(|[b,c]|,|[b,d]|) & p2 in LSeg(|[b,c]|,|[b,d]|);
  then
A2: K is_simple_closed_curve by Th60;
A3: p1`1=b & c <=p1`2 & p1`2 <= d by A1,Th9;
A4: p2`1=b & c <=p2`2 & p2`2 <= d by A1,Th9;
A5: W-min(K)= |[a,c]| by A1,Th56;
A6: E-max(K)= |[b,d]| by A1,Th56;
A7: Lower_Arc(K)=LSeg(|[b,d]|,|[b,c]|) \/ LSeg(|[b,c]|,|[a,c]|) by A1,Th62;
  then
A8: LSeg(|[b,d]|,|[b,c]|) c= Lower_Arc(K) by XBOOLE_1:7;
A9: Upper_Arc(K) /\ Lower_Arc(K)={W-min(K),E-max(K)} by A2,JORDAN6:def 9;
A10: now
    assume p1 in Upper_Arc(K);
    then
A11: p1 in Upper_Arc(K) /\ Lower_Arc(K) by A1,A8,XBOOLE_0:def 3;
    now
      assume p1=W-min(K);
      then p1`1=a by A5,EUCLID:56;
      hence contradiction by A1,TOPREAL3:17;
    end;
    hence p1=E-max(K) by A9,A11,TARSKI:def 2;
  end;
  thus LE p1,p2,K implies p1`2>=p2`2
  proof
    assume LE p1,p2,K;
    then
A12: p1 in Upper_Arc(K) & p2 in Lower_Arc(K)& not p2=W-min(K) or
    p1 in Upper_Arc(K) & p2 in Upper_Arc(K) &
    LE p1,p2,Upper_Arc(K),W-min(K),E-max(K) or
    p1 in Lower_Arc(K) & p2 in Lower_Arc(K)& not p2=W-min(K) &
    LE p1,p2,Lower_Arc(K),E-max(K),W-min(K) by JORDAN6:def 10;
    now per cases;
      case p1=E-max(K);
        hence p1`2>=p2`2 by A4,A6,EUCLID:56;
      end;
      case
A13:    p1<>E-max(K);

consider f being Function of I[01],(TOP-REAL 2)|(Lower_Arc(K)) such that
A14:    f is_homeomorphism
        & f.0=E-max(K) & f.1=W-min(K) & rng f=Lower_Arc(K) &

(for r being Real st r in [.0,1/2.] holds f.r=(1-2*r)*|[b,d]|+(2*r)*|[b,c]|)&
        (for r being Real st r in [.1/2,1.] holds
        f.r=(1-(2*r-1))*|[b,c]|+(2*r-1)*|[a,c]|)&
        (for p being Point of TOP-REAL 2 st p in LSeg(|[b,d]|,|[b,c]|)
        holds 0<=((p`2)-d)/(c-d)/2 & ((p`2)-d)/(c-d)/2<=1
        & f.(((p`2)-d)/(c-d)/2)=p)&
        (for p being Point of TOP-REAL 2 st p in LSeg(|[b,c]|,|[a,c]|)
        holds 0<=((p`1)-b)/(a-b)/2+1/2 & ((p`1)-b)/(a-b)/2+1/2<=1
        & f.(((p`1)-b)/(a-b)/2+1/2)=p) by A1,Th64;
        reconsider s1=((p1`2)-d)/(c-d)/2,s2=((p2`2)-d)/(c-d)/2 as Real;
A15:    f.s1=p1 by A1,A14;
A16:    f.s2=p2 by A1,A14;
        d-c>0 by A1,XREAL_1:52;
        then
A17:    -(d-c)< -0 by XREAL_1:26;
A18:    0<=s1 & s1<=1 by A1,A14;
        0<=s2 & s2<=1 by A1,A14;
        then s1<=s2 by A10,A12,A13,A14,A15,A16,A18,JORDAN5C:def 3;
        then ((p1`2)-d)/(c-d)/2*2<=((p2`2)-d)/(c-d)/2*2 by XREAL_1:66;
        then ((p1`2)-d)/(c-d)*(c-d)>= ((p2`2)-d)/(c-d)*(c-d) by A17,XREAL_1:67;
        then ((p1`2)-d)>= ((p2`2)-d)/(c-d)*(c-d) by A17,XCMPLX_1:88;
        then ((p1`2)-d)>= ((p2`2)-d) by A17,XCMPLX_1:88;
        then ((p1`2)-d)+d >= ((p2`2)-d)+d by XREAL_1:9;
        hence p1`2>=p2`2;
      end;
    end;
    hence thesis;
  end;
  thus p1`2>=p2`2 implies LE p1,p2,K
  proof
    assume
A19: p1`2>=p2`2;
    now per cases;
      case p2=W-min (K);
        then p2=|[a,c]| by A1,Th56;
        hence contradiction by A1,A4,EUCLID:56;
      end;
      case
A20:    p2<>W-min(K);
        for g being Function of I[01], (TOP-REAL 2)|Lower_Arc(K),
        s1, s2 being Real st g is_homeomorphism
        & g.0 = E-max(K) & g.1 = W-min(K) & g.s1 = p1 & 0 <= s1 & s1 <= 1
        & g.s2 = p2 & 0 <= s2 & s2 <= 1 holds s1 <= s2
        proof
          let g be Function of I[01], (TOP-REAL 2)|Lower_Arc(K),
          s1, s2 be Real;
          assume
A21:      g is_homeomorphism & g.0 = E-max(K) & g.1 = W-min(K)
          & g.s1 = p1 & 0 <= s1 & s1 <= 1 & g.s2 = p2 & 0 <= s2 & s2 <= 1;
A22:      dom g=the carrier of I[01] by FUNCT_2:def 1;
A23:      g is one-to-one by A21,TOPS_2:def 5;
A24:      the carrier of ((TOP-REAL 2)|Lower_Arc(K))
          =Lower_Arc(K) by PRE_TOPC:29;
          then reconsider g1=g as Function of I[01],TOP-REAL 2 by FUNCT_2:9;
          g is continuous by A21,TOPS_2:def 5;
          then
A25:      g1 is continuous by PRE_TOPC:56;
          reconsider h1=proj1 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
          reconsider h2=proj2 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
A26:      (TOP-REAL 2)|([#](TOP-REAL 2))=TOP-REAL 2 by TSEP_1:3;
          then
A27:      (for p being Point of (TOP-REAL 2)|([#](TOP-REAL 2)) holds
          h1.p=proj1.p) implies h1 is continuous by JGRAPH_2:39;
          (for p being Point of (TOP-REAL 2)|([#](TOP-REAL 2)) holds
          h2.p=proj2.p) implies h2 is continuous by A26,JGRAPH_2:40;
          then consider h being Function of TOP-REAL 2,R^1 such that
A28:      (for p being Point of TOP-REAL 2, r1,r2 being real number st
          h1.p=r1 & h2.p=r2 holds h.p=r1+r2) & h is continuous
          by A27,JGRAPH_2:29;
          reconsider k=h*g1 as Function of I[01],R^1;
A29:      k is continuous Function of I[01],R^1 by A25,A28;
A30:      E-max K=|[b,d]| by A1,Th56;
          now
            assume
A31:        s1>s2;
A32:        dom g=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
            0 in [.0,1.] by XXREAL_1:1;
            then
A33:        k.0=h.(E-max(K)) by A21,A32,FUNCT_1:23
              .=h1.(E-max(K))+h2.(E-max(K)) by A28
              .=(E-max(K))`1+proj2.(E-max(K)) by PSCOMP_1:def 28
              .=(E-max(K))`1+(E-max(K))`2 by PSCOMP_1:def 29
              .=b+(E-max(K))`2 by A30,EUCLID:56
              .=b+d by A30,EUCLID:56;
            s1 in [.0,1.] by A21,XXREAL_1:1;
            then
A34:        k.s1=h.p1 by A21,A32,FUNCT_1:23
              .=h1.p1+h2.p1 by A28
              .=p1`1+proj2.p1 by PSCOMP_1:def 28
              .=b+p1`2 by A3,PSCOMP_1:def 29;
A35:        s2 in [.0,1.] by A21,XXREAL_1:1;
            then k.s2=h.p2 by A21,A32,FUNCT_1:23
              .=proj1.p2 +proj2.p2 by A28
              .=p2`1+proj2.p2 by PSCOMP_1:def 28
              .=b+p2`2 by A4,PSCOMP_1:def 29;
            then
A36:        k.0>=k.s1 & k.s1>=k.s2 by A3,A19,A33,A34,XREAL_1:9;
A37:        0 in [.0,1.] by XXREAL_1:1;
            then
A38:        [.0,s2.] c= [.0,1.] by A35,XXREAL_2:def 12;
            reconsider B=[.0,s2.] as Subset of I[01] by A35,A37,BORSUK_1:83
            ,XXREAL_2:def 12;
A39:        B is connected by A21,A35,A37,BORSUK_1:83,BORSUK_4:49;
A40:        0 in B by A21,XXREAL_1:1;
A41:        s2 in B by A21,XXREAL_1:1;
A42:        k.0 is Real by XREAL_0:def 1;
A43:        k.s2 is Real by XREAL_0:def 1;
            k.s1 is Real by XREAL_0:def 1;
            then consider xc being Point of I[01] such that
A44:        xc in B & k.xc =k.s1 by A29,A36,A39,A40,A41,A42,A43,TOPREAL5:11;
            xc in [.0,1.] by BORSUK_1:83;
            then reconsider rxc=xc as Real;
A45:        k is one-to-one
            proof
              for x1,x2 being set st x1 in dom k & x2 in dom k &
              k.x1=k.x2 holds x1=x2
              proof
                let x1,x2 be set;
                assume
A46:            x1 in dom k & x2 in dom k & k.x1=k.x2;
                then reconsider r1=x1 as Point of I[01];
                reconsider r2=x2 as Point of I[01] by A46;
A47:            k.x1=h.(g1.x1) by A46,FUNCT_1:22
                  .=h1.(g1.r1)+h2.(g1.r1) by A28
                  .=(g1.r1)`1+proj2.(g1.r1) by PSCOMP_1:def 28
                  .=(g1.r1)`1+(g1.r1)`2 by PSCOMP_1:def 29;
A48:            k.x2=h.(g1.x2) by A46,FUNCT_1:22
                  .=h1.(g1.r2)+h2.(g1.r2) by A28
                  .=(g1.r2)`1+proj2.(g1.r2) by PSCOMP_1:def 28
                  .=(g1.r2)`1+(g1.r2)`2 by PSCOMP_1:def 29;
A49:            g.r1 in Lower_Arc(K) by A24;
A50:            g.r2 in Lower_Arc(K) by A24;
                reconsider gr1=g.r1 as Point of TOP-REAL 2 by A49;
                reconsider gr2=g.r2 as Point of TOP-REAL 2 by A50;
                now per cases by A7,A24,XBOOLE_0:def 2;
                  case
A51:                g.r1 in LSeg(|[b,c]|,|[b,d]|) &
                    g.r2 in LSeg(|[b,c]|,|[b,d]|);
                    then
A52:                (gr1)`1=b & c <=(gr1)`2 & (gr1)`2 <=d by A1,Th9;
                    (gr2)`1=b & c <=(gr2)`2 & (gr2)`2 <=d by A1,A51,Th9;
                    then |[(gr1)`1,(gr1)`2]|=g.r2 by A46,A47,A48,A52,EUCLID:57;
                    then g.r1=g.r2 by EUCLID:57;
                    hence x1=x2 by A22,A23,FUNCT_1:def 8;
                  end;
                  case
A53:                g.r1 in LSeg(|[b,c]|,|[b,d]|) &
                    g.r2 in LSeg(|[b,c]|,|[a,c]|);
                    then
A54:                (gr1)`1=b & c <=(gr1)`2 & (gr1)`2 <=d by A1,Th9;
A55:                (gr2)`2=c & a <=(gr2)`1 & (gr2)`1 <=b by A1,A53,Th11;
A56:                b+(gr1)`2=(gr2)`1 +c by A1,A46,A47,A48,A53,A54,Th11;
A57:                now
                      assume b<>gr2`1;
                      then b>gr2`1 by A55,XXREAL_0:1;
                      hence contradiction by A54,A56,XREAL_1:10;
                    end;
                    now
                      assume gr1`2<>c;
                      then c <gr1`2 by A54,XXREAL_0:1;
                      hence contradiction by A46,A47,A48,A54,A55,XREAL_1:10;
                    end;
                    then |[(gr1)`1,(gr1)`2]|=g.r2 by A54,A55,A57,EUCLID:57;
                    then g.r1=g.r2 by EUCLID:57;
                    hence x1=x2 by A22,A23,FUNCT_1:def 8;
                  end;
                  case
A58:                g.r1 in LSeg(|[b,c]|,|[a,c]|) &
                    g.r2 in LSeg(|[b,c]|,|[b,d]|);
                    then
A59:                (gr2)`1=b & c <=(gr2)`2 & (gr2)`2 <=d by A1,Th9;
A60:                (gr1)`2=c & a <=(gr1)`1 & (gr1)`1 <=b by A1,A58,Th11;
A61:                b+(gr2)`2=(gr1)`1 +c by A1,A46,A47,A48,A58,A59,Th11;
A62:                now
                      assume b<>gr1`1;
                      then b> gr1`1 by A60,XXREAL_0:1;
                      hence contradiction by A59,A61,XREAL_1:10;
                    end;
                    now
                      assume gr2`2<> c;
                      then c <gr2`2 by A59,XXREAL_0:1;
                      hence contradiction by A46,A47,A48,A59,A60,XREAL_1:10;
                    end;
                    then |[(gr2)`1,(gr2)`2]|=g.r1 by A59,A60,A62,EUCLID:57;
                    then g.r1=g.r2 by EUCLID:57;
                    hence x1=x2 by A22,A23,FUNCT_1:def 8;
                  end;
                  case
A63:                g.r1 in LSeg(|[b,c]|,|[a,c]|) &
                    g.r2 in LSeg(|[b,c]|,|[a,c]|);
                    then
A64:                (gr1)`2=c & a <=(gr1)`1 & (gr1)`1 <=b by A1,Th11;
                    (gr2)`2=c & a <=(gr2)`1 & (gr2)`1 <=b by A1,A63,Th11;
                    then |[(gr1)`1,(gr1)`2]|=g.r2 by A46,A47,A48,A64,EUCLID:57;
                    then g.r1=g.r2 by EUCLID:57;
                    hence x1=x2 by A22,A23,FUNCT_1:def 8;
                  end;
                end;
                hence x1=x2;
              end;
              hence thesis by FUNCT_1:def 8;
            end;
A65:        dom k=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
            then s1 in dom k by A21,XXREAL_1:1;
            then rxc=s1 by A38,A44,A45,A65,FUNCT_1:def 8;
            hence contradiction by A31,A44,XXREAL_1:1;
          end;
          hence s1 <= s2;
        end;
        then p1 in Lower_Arc(K) & p2 in Lower_Arc(K) & not p2=W-min(K) &
        LE p1,p2,Lower_Arc(K),E-max(K),W-min(K) by A1,A8,A20,JORDAN5C:def 3;
        hence LE p1,p2,K by JORDAN6:def 10;
      end;
    end;
    hence thesis;
  end;
end;

theorem Th68:
  for a,b,c,d being real number,p1,p2 being Point of TOP-REAL 2 st a<b & c <d
  & p1 in LSeg(|[a,c]|,|[b,c]|) & p2 in LSeg(|[a,c]|,|[b,c]|)
  holds LE p1,p2,rectangle(a,b,c,d) &
  p1<>W-min rectangle(a,b,c,d) iff p1`1>=p2`1 & p2<>W-min rectangle(a,b,c,d)
proof
  let a,b,c,d be real number,p1,p2 be Point of TOP-REAL 2;
  set K = rectangle(a,b,c,d);
  assume
A1: a<b & c <d & p1 in LSeg(|[a,c]|,|[b,c]|) & p2 in LSeg(|[a,c]|,|[b,c]|);
  then
A2: K is_simple_closed_curve by Th60;
A3: p1`2=c & a <=p1`1 & p1`1 <= b by A1,Th11;
A4: p2`2=c & a <=p2`1 & p2`1 <= b by A1,Th11;
A5: W-min(K)= |[a,c]| by A1,Th56;
A6: E-max(K)= |[b,d]| by A1,Th56;
A7: Lower_Arc(K)=LSeg(|[b,d]|,|[b,c]|) \/ LSeg(|[b,c]|,|[a,c]|) by A1,Th62;
  then
A8: LSeg(|[b,c]|,|[a,c]|) c= Lower_Arc(K) by XBOOLE_1:7;
  then
A9: p1 in Lower_Arc(K) by A1;
A10: Lower_Arc(K) c= K by A2,JORDAN6:76;
A11: Upper_Arc(K) /\ Lower_Arc(K)={W-min(K),E-max(K)} by A2,JORDAN6:def 9;
A12: now
    assume p1 in Upper_Arc(K);
    then p1 in Upper_Arc(K) /\ Lower_Arc(K) by A1,A8,XBOOLE_0:def 3;
    then p1=W-min(K) or p1=E-max(K) by A11,TARSKI:def 2;
    hence p1=W-min(K) by A1,A3,A6,EUCLID:56;
  end;
  thus LE p1,p2,K & p1<>W-min(K) implies p1`1>=p2`1 & p2<>W-min(K)
  proof
    assume
A13: LE p1,p2,K & p1<>W-min(K);
    then
A14: p1 in Upper_Arc(K) & p2 in Lower_Arc(K)& not p2=W-min(K) or
    p1 in Upper_Arc(K) & p2 in Upper_Arc(K) &
    LE p1,p2,Upper_Arc(K),W-min(K),E-max(K) or
    p1 in Lower_Arc(K) & p2 in Lower_Arc(K)& not p2=W-min(K) &
    LE p1,p2,Lower_Arc(K),E-max(K),W-min(K) by JORDAN6:def 10;
    consider f being Function of I[01],(TOP-REAL 2)|(Lower_Arc(K)) such that
A15: f is_homeomorphism & f.0=E-max(K) & f.1=W-min(K) & rng f=Lower_Arc(K) &

(for r being Real st r in [.0,1/2.] holds f.r=(1-2*r)*|[b,d]|+(2*r)*|[b,c]|)&
    (for r being Real st r in [.1/2,1.] holds
    f.r=(1-(2*r-1))*|[b,c]|+(2*r-1)*|[a,c]|)&
    (for p being Point of TOP-REAL 2 st p in LSeg(|[b,d]|,|[b,c]|)
    holds 0<=((p`2)-d)/(c-d)/2 & ((p`2)-d)/(c-d)/2<=1
    & f.(((p`2)-d)/(c-d)/2)=p)&
    (for p being Point of TOP-REAL 2 st p in LSeg(|[b,c]|,|[a,c]|)
    holds 0<=((p`1)-b)/(a-b)/2+1/2 & ((p`1)-b)/(a-b)/2+1/2<=1
    & f.(((p`1)-b)/(a-b)/2+1/2)=p) by A1,Th64;
    reconsider s1=((p1`1)-b)/(a-b)/2+1/2,s2=((p2`1)-b)/(a-b)/2+1/2 as Real;
A16: f.s1=p1 by A1,A15;
A17: f.s2=p2 by A1,A15;
    b-a>0 by A1,XREAL_1:52;
    then
A18: -(b-a)< -0 by XREAL_1:26;
A19: 0<=s1 & s1<=1 by A1,A15;
    0<=s2 & s2<=1 by A1,A15;
    then s1<=s2 by A12,A13,A14,A15,A16,A17,A19,JORDAN5C:def 3;
    then ((p1`1)-b)/(a-b)/2<= ((p2`1)-b)/(a-b)/2 by XREAL_1:8;
    then ((p1`1)-b)/(a-b)/2*2<=((p2`1)-b)/(a-b)/2*2 by XREAL_1:66;
    then ((p1`1)-b)/(a-b)*(a-b)>= ((p2`1)-b)/(a-b)*(a-b) by A18,XREAL_1:67;
    then ((p1`1)-b)>= ((p2`1)-b)/(a-b)*(a-b) by A18,XCMPLX_1:88;
    then ((p1`1)-b)>= ((p2`1)-b) by A18,XCMPLX_1:88;
    then ((p1`1)-b)+b >= ((p2`1)-b)+b by XREAL_1:9;
    hence p1`1>=p2`1;
    now
      assume
A20:  p2=W-min(K);
      then LE p2,p1,K by A2,A9,A10,JORDAN7:3;
      hence contradiction by A1,A13,A20,Th60,JORDAN6:72;
    end;
    hence thesis;
  end;
  thus p1`1>=p2`1 & p2<>W-min(K) implies LE p1,p2,K & p1<>W-min(K)
  proof
    assume
A21: p1`1>=p2`1 & p2<>W-min(K);
A22: for g being Function of I[01], (TOP-REAL 2)|Lower_Arc(K),
    s1, s2 being Real st g is_homeomorphism & g.0 = E-max(K) & g.1 = W-min(K)
    & g.s1 = p1 & 0 <= s1 & s1 <= 1 & g.s2 = p2 & 0 <= s2 & s2 <= 1
    holds s1 <= s2
    proof
      let g be Function of I[01], (TOP-REAL 2)|Lower_Arc(K), s1, s2 be Real;
      assume
A23:  g is_homeomorphism & g.0 = E-max(K) & g.1 = W-min(K)
      & g.s1 = p1 & 0 <= s1 & s1 <= 1 & g.s2 = p2 & 0 <= s2 & s2 <= 1;
A24:  dom g=the carrier of I[01] by FUNCT_2:def 1;
A25:  g is one-to-one by A23,TOPS_2:def 5;
A26:  the carrier of ((TOP-REAL 2)|Lower_Arc(K)) =Lower_Arc(K) by PRE_TOPC:29;
      then reconsider g1=g as Function of I[01],TOP-REAL 2 by FUNCT_2:9;
      g is continuous by A23,TOPS_2:def 5;
      then
A27:  g1 is continuous by PRE_TOPC:56;
      reconsider h1=proj1 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
      reconsider h2=proj2 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
A28:  (TOP-REAL 2)|([#](TOP-REAL 2))=TOP-REAL 2 by TSEP_1:3;
      then
A29:  (for p being Point of (TOP-REAL 2)|([#](TOP-REAL 2)) holds
      h1.p=proj1.p) implies h1 is continuous by JGRAPH_2:39;
      (for p being Point of (TOP-REAL 2)|([#](TOP-REAL 2)) holds
      h2.p=proj2.p) implies h2 is continuous by A28,JGRAPH_2:40;
      then consider h being Function of TOP-REAL 2,R^1 such that
A30:  (for p being Point of TOP-REAL 2, r1,r2 being real number st
      h1.p=r1 & h2.p=r2 holds h.p=r1+r2) & h is continuous by A29,JGRAPH_2:29;
      reconsider k=h*g1 as Function of I[01],R^1;
A31:  k is continuous Function of I[01],R^1 by A27,A30;
A32:  E-max K=|[b,d]| by A1,Th56;
      now
        assume
A33:    s1>s2;
A34:    dom g=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
        0 in [.0,1.] by XXREAL_1:1;
        then
A35:    k.0=h.(E-max(K)) by A23,A34,FUNCT_1:23
          .=h1.(E-max(K))+h2.(E-max(K)) by A30
          .=(E-max(K))`1+proj2.(E-max(K)) by PSCOMP_1:def 28
          .=(E-max(K))`1+(E-max(K))`2 by PSCOMP_1:def 29
          .=(E-max(K))`1+d by A32,EUCLID:56
          .=b+d by A32,EUCLID:56;
        s1 in [.0,1.] by A23,XXREAL_1:1;
        then
A36:    k.s1=h.p1 by A23,A34,FUNCT_1:23
          .=proj1.p1 +proj2.p1 by A30
          .=p1`1+proj2.p1 by PSCOMP_1:def 28
          .=p1`1 +c by A3,PSCOMP_1:def 29;
A37:    s2 in [.0,1.] by A23,XXREAL_1:1;
        then k.s2=h.p2 by A23,A34,FUNCT_1:23
          .=h1.p2+h2.p2 by A30
          .=p2`1+proj2.p2 by PSCOMP_1:def 28
          .=p2`1 +c by A4,PSCOMP_1:def 29;
        then
A38:    k.0>=k.s1 & k.s1>=k.s2 by A1,A3,A21,A35,A36,XREAL_1:9;
A39:    0 in [.0,1.] by XXREAL_1:1;
        then
A40:    [.0,s2.] c= [.0,1.] by A37,XXREAL_2:def 12;
        reconsider B=[.0,s2.] as Subset of I[01] by A37,A39,BORSUK_1:83
        ,XXREAL_2:def 12;
A41:    B is connected by A23,A37,A39,BORSUK_1:83,BORSUK_4:49;
A42:    0 in B by A23,XXREAL_1:1;
A43:    s2 in B by A23,XXREAL_1:1;
A44:    k.0 is Real by XREAL_0:def 1;
A45:    k.s2 is Real by XREAL_0:def 1;
        k.s1 is Real by XREAL_0:def 1;
        then consider xc being Point of I[01] such that
A46:    xc in B & k.xc =k.s1 by A31,A38,A41,A42,A43,A44,A45,TOPREAL5:11;
        xc in [.0,1.] by BORSUK_1:83;
        then reconsider rxc=xc as Real;
A47:    k is one-to-one
        proof
          for x1,x2 being set st x1 in dom k & x2 in dom k &
          k.x1=k.x2 holds x1=x2
          proof
            let x1,x2 be set;
            assume
A48:        x1 in dom k & x2 in dom k & k.x1=k.x2;
            then reconsider r1=x1 as Point of I[01];
            reconsider r2=x2 as Point of I[01] by A48;
A49:        k.x1=h.(g1.x1) by A48,FUNCT_1:22
              .=h1.(g1.r1)+h2.(g1.r1) by A30
              .=(g1.r1)`1+proj2.(g1.r1) by PSCOMP_1:def 28
              .=(g1.r1)`1+(g1.r1)`2 by PSCOMP_1:def 29;
A50:        k.x2=h.(g1.x2) by A48,FUNCT_1:22
              .=h1.(g1.r2)+h2.(g1.r2) by A30
              .=(g1.r2)`1+proj2.(g1.r2) by PSCOMP_1:def 28
              .=(g1.r2)`1+(g1.r2)`2 by PSCOMP_1:def 29;
A51:        g.r1 in Lower_Arc(K) by A26;
A52:        g.r2 in Lower_Arc(K) by A26;
            reconsider gr1=g.r1 as Point of TOP-REAL 2 by A51;
            reconsider gr2=g.r2 as Point of TOP-REAL 2 by A52;
            now per cases by A7,A26,XBOOLE_0:def 2;
              case
A53:            g.r1 in LSeg(|[b,d]|,|[b,c]|) & g.r2 in LSeg(|[b,d]|,|[b,c]|);
                then
A54:            (gr1)`1=b & c <=(gr1)`2 & (gr1)`2 <=d by A1,Th9;
                (gr2)`1=b & c <=(gr2)`2 & (gr2)`2 <=d by A1,A53,Th9;
                then |[(gr1)`1,(gr1)`2]|=g.r2 by A48,A49,A50,A54,EUCLID:57;
                then g.r1=g.r2 by EUCLID:57;
                hence x1=x2 by A24,A25,FUNCT_1:def 8;
              end;
              case
A55:            g.r1 in LSeg(|[b,d]|,|[b,c]|) & g.r2 in LSeg(|[b,c]|,|[a,c]|);
                then
A56:            (gr1)`1=b & c <=(gr1)`2 & (gr1)`2 <=d by A1,Th9;
A57:            (gr2)`2=c & a <=(gr2)`1 & (gr2)`1 <=b by A1,A55,Th11;
A58:            b+(gr1)`2=(gr2)`1 +c by A1,A48,A49,A50,A55,A56,Th11;
A59:            now
                  assume b<>gr2`1;
                  then b>gr2`1 by A57,XXREAL_0:1;
                  hence contradiction by A56,A58,XREAL_1:10;
                end;
                now
                  assume gr1`2<>c;
                  then c <gr1`2 by A56,XXREAL_0:1;
                  hence contradiction by A48,A49,A50,A56,A57,XREAL_1:10;
                end;
                then |[(gr1)`1,(gr1)`2]|=g.r2 by A56,A57,A59,EUCLID:57;
                then g.r1=g.r2 by EUCLID:57;
                hence x1=x2 by A24,A25,FUNCT_1:def 8;
              end;
              case
A60:            g.r1 in LSeg(|[b,c]|,|[a,c]|) & g.r2 in LSeg(|[b,d]|,|[b,c]|);
                then
A61:            (gr2)`1=b & c <=(gr2)`2 & (gr2)`2 <=d by A1,Th9;
A62:            (gr1)`2=c & a <=(gr1)`1 & (gr1)`1 <=b by A1,A60,Th11;
A63:            b+(gr2)`2=(gr1)`1 +c by A1,A48,A49,A50,A60,A61,Th11;
A64:            now
                  assume b<>gr1`1;
                  then b>gr1`1 by A62,XXREAL_0:1;
                  hence contradiction by A61,A63,XREAL_1:10;
                end;
                now
                  assume gr2`2<>c;
                  then c <gr2`2 by A61,XXREAL_0:1;
                  hence contradiction by A48,A49,A50,A61,A62,XREAL_1:10;
                end;
                then |[(gr2)`1,(gr2)`2]|=g.r1 by A61,A62,A64,EUCLID:57;
                then g.r1=g.r2 by EUCLID:57;
                hence x1=x2 by A24,A25,FUNCT_1:def 8;
              end;
              case
A65:            g.r1 in LSeg(|[b,c]|,|[a,c]|) & g.r2 in LSeg(|[b,c]|,|[a,c]|);
                then
A66:            (gr1)`2=c & a <=(gr1)`1 & (gr1)`1 <=b by A1,Th11;
                (gr2)`2=c & a <=(gr2)`1 & (gr2)`1 <=b by A1,A65,Th11;
                then |[(gr1)`1,(gr1)`2]|=g.r2 by A48,A49,A50,A66,EUCLID:57;
                then g.r1=g.r2 by EUCLID:57;
                hence x1=x2 by A24,A25,FUNCT_1:def 8;
              end;
            end;
            hence x1=x2;
          end;
          hence thesis by FUNCT_1:def 8;
        end;
A67:    dom k=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
        then s1 in dom k by A23,XXREAL_1:1;
        then rxc=s1 by A40,A46,A47,A67,FUNCT_1:def 8;
        hence contradiction by A33,A46,XXREAL_1:1;
      end;
      hence s1 <= s2;
    end;
A68: now
      assume
A69:  p1=W-min(K);
      then p1`1=a & p1`2=c by A5,EUCLID:56;
      then p1`1=p2`1 by A4,A21,XXREAL_0:1;
      then |[(p1)`1,(p1)`2]|=p2 by A3,A4,EUCLID:57;
      hence contradiction by A21,A69,EUCLID:57;
    end;
    p1 in Lower_Arc(K) & p2 in Lower_Arc(K) & not p2=W-min(K) &
    LE p1,p2,Lower_Arc(K),E-max(K),W-min(K) by A1,A8,A21,A22,JORDAN5C:def 3;
    hence LE p1,p2,K by JORDAN6:def 10;
    thus thesis by A68;
  end;
end;

theorem Th69:
  for a,b,c,d being real number,p1,p2 being Point of TOP-REAL 2 st a<b & c <d
  & p1 in LSeg(|[a,c]|,|[a,d]|)

holds LE p1,p2,rectangle(a,b,c,d) iff p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
  or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
  or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min rectangle(a,b,c,d)
proof
  let a,b,c,d be real number,p1,p2 be Point of TOP-REAL 2;
  set K = rectangle(a,b,c,d);
  assume
A1: a<b & c <d & p1 in LSeg(|[a,c]|,|[a,d]|);
  then
A2: K is_simple_closed_curve by Th60;
  Upper_Arc(K)=LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|) by A1,Th61;
  then
A3: LSeg(|[a,c]|,|[a,d]|) c= Upper_Arc(K) by XBOOLE_1:7;
A4: p1`1=a & c <=p1`2 & p1`2 <= d by A1,Th9;
  thus LE p1,p2,K implies p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
  or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
  or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K)
  proof
    assume
A5: LE p1,p2,K;
    then
A6: p1 in K & p2 in K by A2,JORDAN7:5;
    K= LSeg(|[ a,c ]|, |[ a,d ]|) \/ LSeg(|[ a,d ]|, |[ b,d ]|)
    \/ (LSeg(|[ a,c ]|, |[ b,c ]|) \/ LSeg(|[ b,c ]|, |[ b,d ]|))
    by SPPOL_2:def 3
      .=LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|)
    \/ LSeg(|[b,d]|,|[b,c]|) \/ LSeg(|[b,c]|,|[a,c]|) by XBOOLE_1:4;
    then p2 in LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|)
    \/ LSeg(|[b,d]|,|[b,c]|) or
    p2 in LSeg(|[b,c]|,|[a,c]|) by A6,XBOOLE_0:def 2;
    then
A7: p2 in LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|) or
    p2 in LSeg(|[b,d]|,|[b,c]|) or
    p2 in LSeg(|[b,c]|,|[a,c]|) by XBOOLE_0:def 2;
    now per cases by A7,XBOOLE_0:def 2;
      case p2 in LSeg(|[a,c]|,|[a,d]|);
        hence p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
        or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A1,A5,Th65;
      end;
      case p2 in LSeg(|[a,d]|,|[b,d]|);
        hence p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
        or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
      end;
      case p2 in LSeg(|[b,d]|,|[b,c]|);
        hence p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
        or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
      end;
      case
A8:     p2 in LSeg(|[b,c]|,|[a,c]|);
        now per cases;
          case p2=W-min(K);
            then LE p2,p1,K by A2,A6,JORDAN7:3;
            hence p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
            or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
            or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K)
            by A1,A5,Th60,JORDAN6:72;
          end;
          case p2<>W-min(K);
            hence p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
            or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
            or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A8;
          end;
        end;
        hence p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
        or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
      end;
    end;
    hence p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
    or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
    or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
  end;
A9: W-min(K)= |[a,c]| by A1,Th56;
  thus p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
  or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
  or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) implies LE p1,p2,K
  proof
    assume
A10: p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2
    or p2 in LSeg(|[a,d]|,|[b,d]|) or p2 in LSeg(|[b,d]|,|[b,c]|)
    or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
    now per cases by A10;
      case p2 in LSeg(|[a,c]|,|[a,d]|) & p1`2<=p2`2;
        hence LE p1,p2,K by A1,Th65;
      end;
      case
A11:    p2 in LSeg(|[a,d]|,|[b,d]|);
        then
A12:    p2`2=d & a <=p2`1 & p2`1 <= b by A1,Th11;
A13:    Upper_Arc(K)=LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|)
        by A1,Th61;
        then
A14:    p2 in Upper_Arc(K) by A11,XBOOLE_0:def 2;
A15:    p1 in Upper_Arc(K) by A1,A13,XBOOLE_0:def 2;
        LE p1,p2,Upper_Arc(K),W-min(K),E-max(K)
        proof
          for g being Function of I[01], (TOP-REAL 2)|Upper_Arc(K),
          s1, s2 being Real st g is_homeomorphism
          & g.0 = W-min(K) & g.1 = E-max(K) & g.s1 = p1 & 0 <= s1 & s1 <= 1
          & g.s2 = p2 & 0 <= s2 & s2 <= 1 holds s1 <= s2
          proof
            let g be Function of I[01], (TOP-REAL 2)|Upper_Arc(K),
            s1, s2 be Real;
            assume
A16:        g is_homeomorphism & g.0 = W-min(K) & g.1 = E-max(K)
            & g.s1 = p1 & 0 <= s1 & s1 <= 1 & g.s2 = p2 & 0 <= s2 & s2 <= 1;
A17:        dom g=the carrier of I[01] by FUNCT_2:def 1;
A18:        g is one-to-one by A16,TOPS_2:def 5;
A19:        the carrier of ((TOP-REAL 2)|Upper_Arc(K))
            =Upper_Arc(K) by PRE_TOPC:29;
            then reconsider g1=g as Function of I[01],TOP-REAL 2 by FUNCT_2:9;
            g is continuous by A16,TOPS_2:def 5;
            then
A20:        g1 is continuous by PRE_TOPC:56;
            reconsider h1=proj1 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
            reconsider h2=proj2 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
A21:        (TOP-REAL 2)|([#](TOP-REAL 2))=TOP-REAL 2 by TSEP_1:3;
            then
A22:        (for p being Point of (TOP-REAL 2)|([#](TOP-REAL 2 )) holds
            h1.p=proj1.p) implies h1 is continuous by JGRAPH_2:39;
            (for p being Point of (TOP-REAL 2)|([#](TOP-REAL 2)) holds
            h2.p=proj2.p) implies h2 is continuous by A21,JGRAPH_2:40;
            then consider h being Function of TOP-REAL 2,R^1 such that
A23:        (for p being Point of TOP-REAL 2, r1,r2 being real number st
            h1.p=r1 & h2.p=r2 holds h.p=r1+r2) & h is continuous
            by A22,JGRAPH_2:29;
            reconsider k=h*g1 as Function of I[01],R^1;
A24:        k is continuous Function of I[01],R^1 by A20,A23;
A25:        W-min K=|[a,c]| by A1,Th56;
            now
              assume
A26:          s1>s2;
A27:          dom g=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
              0 in [.0,1.] by XXREAL_1:1;
              then
A28:          k.0=h.(W-min(K)) by A16,A27,FUNCT_1:23
                .=h1.(W-min(K))+h2.(W-min(K)) by A23
                .=(W-min(K))`1+proj2.(W-min(K)) by PSCOMP_1:def 28
                .=(W-min(K))`1+(W-min(K))`2 by PSCOMP_1:def 29
                .=(W-min(K))`1+c by A25,EUCLID:56
                .=a+c by A25,EUCLID:56;
              s1 in [.0,1.] by A16,XXREAL_1:1;
              then
A29:          k.s1=h.p1 by A16,A27,FUNCT_1:23
                .=proj1.p1 +proj2.p1 by A23
                .=p1`1+proj2.p1 by PSCOMP_1:def 28
                .=a +p1`2 by A4,PSCOMP_1:def 29;
A30:          s2 in [.0,1.] by A16,XXREAL_1:1;
              then k.s2=h.p2 by A16,A27,FUNCT_1:23
                .=proj1.p2 +proj2.p2 by A23
                .=p2`1+proj2.p2 by PSCOMP_1:def 28
                .=p2`1 +d by A12,PSCOMP_1:def 29;
              then
A31:          k.0<=k.s1 & k.s1<=k.s2 by A4,A12,A28,A29,XREAL_1:9;
A32:          0 in [.0,1.] by XXREAL_1:1;
              then
A33:          [.0,s2.] c= [.0,1.] by A30,XXREAL_2:def 12;
              reconsider B=[.0,s2.] as Subset of I[01] by A30,A32,BORSUK_1:83
              ,XXREAL_2:def 12;
A34:          B is connected by A16,A30,A32,BORSUK_1:83,BORSUK_4:49;
A35:          0 in B by A16,XXREAL_1:1;
A36:          s2 in B by A16,XXREAL_1:1;
A37:          k.0 is Real by XREAL_0:def 1;
A38:          k.s2 is Real by XREAL_0:def 1;
              k.s1 is Real by XREAL_0:def 1;
              then consider xc being Point of I[01] such that
A39:          xc in B & k.xc =k.s1 by A24,A31,A34,A35,A36,A37,A38,TOPREAL5:11;
              xc in [.0,1.] by BORSUK_1:83;
              then reconsider rxc=xc as Real;
A40:          k is one-to-one
              proof
                for x1,x2 being set st x1 in dom k & x2 in dom k &
                k.x1=k.x2 holds x1=x2
                proof
                  let x1,x2 be set;
                  assume
A41:              x1 in dom k & x2 in dom k & k.x1=k.x2;
                  then reconsider r1=x1 as Point of I[01];
                  reconsider r2=x2 as Point of I[01] by A41;
A42:              k.x1=h.(g1.x1) by A41,FUNCT_1:22
                    .=h1.(g1.r1)+h2.(g1.r1) by A23
                    .=(g1.r1)`1+proj2.(g1.r1) by PSCOMP_1:def 28
                    .=(g1.r1)`1+(g1.r1)`2 by PSCOMP_1:def 29;
A43:              k.x2=h.(g1.x2) by A41,FUNCT_1:22
                    .=h1.(g1.r2)+h2.(g1.r2) by A23
                    .=(g1.r2)`1+proj2.(g1.r2) by PSCOMP_1:def 28
                    .=(g1.r2)`1+(g1.r2)`2 by PSCOMP_1:def 29;
A44:              g.r1 in Upper_Arc(K) by A19;
A45:              g.r2 in Upper_Arc(K) by A19;
                  reconsider gr1=g.r1 as Point of TOP-REAL 2 by A44;
                  reconsider gr2=g.r2 as Point of TOP-REAL 2 by A45;
                  now per cases by A13,A19,XBOOLE_0:def 2;
                    case
A46:                  g.r1 in LSeg(|[a,c]|,|[a,d]|) &
                      g.r2 in LSeg(|[a,c]|,|[a,d]|);
                      then
A47:                  (gr1)`1=a & c <=(gr1)`2 & (gr1 )`2 <=d by A1,Th9;
                      (gr2)`1=a & c <=(gr2)`2 & (gr2)`2 <=d by A1,A46,Th9;

then |[(gr1)`1,(gr1)`2]|=g.r2 by A41,A42,A43,A47,EUCLID:57;
                      then g.r1=g.r2 by EUCLID:57;
                      hence x1=x2 by A17,A18,FUNCT_1:def 8;
                    end;
                    case
A48:                  g.r1 in LSeg(|[a,c]|,|[a,d]|) &
                      g.r2 in LSeg(|[a,d]|,|[b,d]|);
                      then
A49:                  (gr1)`1=a & c <=(gr1)`2 & (gr1 )`2 <=d by A1,Th9;
A50:                  (gr2)`2=d & a <=(gr2)`1 & (gr2)`1 <=b by A1,A48,Th11;
A51:                  a+(gr1)`2=(gr2)`1 +d by A1,A41,A42,A43,A48,A49,Th11;
A52:                  now
                        assume a<>gr2`1;
                        then a<gr2`1 by A50,XXREAL_0:1;
                        hence contradiction by A49,A51,XREAL_1:10;
                      end;
                      now
                        assume gr1`2<>d;
                        then d>gr1`2 by A49,XXREAL_0:1;
                        hence contradiction by A41,A42,A43,A49,A50,XREAL_1:10;
                      end;
                      then |[(gr1)`1,(gr1)`2]|=g.r2 by A49,A50,A52,EUCLID:57;
                      then g.r1=g.r2 by EUCLID:57;
                      hence x1=x2 by A17,A18,FUNCT_1:def 8;
                    end;
                    case
A53:                  g.r1 in LSeg(|[a,d]|,|[b,d]|) &
                      g.r2 in LSeg(|[a,c]|,|[a,d]|);
                      then
A54:                  (gr2)`1=a & c <=(gr2)`2 & (gr2 )`2 <=d by A1,Th9;
A55:                  (gr1)`2=d & a <=(gr1)`1 & (gr1)`1 <=b by A1,A53,Th11;
A56:                  a+(gr2)`2=(gr1)`1 +d by A1,A41,A42,A43,A53,A54,Th11;
A57:                  now
                        assume a<>gr1`1;
                        then a<gr1`1 by A55,XXREAL_0:1;
                        hence contradiction by A54,A56,XREAL_1:10;
                      end;
                      now
                        assume gr2`2<>d;
                        then d>gr2`2 by A54,XXREAL_0:1;
                        hence contradiction by A41,A42,A43,A54,A55,XREAL_1:10;
                      end;
                      then |[(gr2)`1,(gr2)`2]|=g.r1 by A54,A55,A57,EUCLID:57;
                      then g.r1=g.r2 by EUCLID:57;
                      hence x1=x2 by A17,A18,FUNCT_1:def 8;
                    end;
                    case
A58:                  g.r1 in LSeg(|[a,d]|,|[b,d]|) &
                      g.r2 in LSeg(|[a,d]|,|[b,d]|);
                      then
A59:                  (gr1)`2=d & a <=(gr1)`1 & (gr1 )`1 <=b by A1,Th11;
                      (gr2)`2=d & a <=(gr2)`1 & (gr2)`1 <=b by A1,A58,Th11;

then |[(gr1)`1,(gr1)`2]|=g.r2 by A41,A42,A43,A59,EUCLID:57;
                      then g.r1=g.r2 by EUCLID:57;
                      hence x1=x2 by A17,A18,FUNCT_1:def 8;
                    end;
                  end;
                  hence x1=x2;
                end;
                hence thesis by FUNCT_1:def 8;
              end;
A60:          dom k=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
              then s1 in dom k by A16,XXREAL_1:1;
              then rxc=s1 by A33,A39,A40,A60,FUNCT_1:def 8;
              hence contradiction by A26,A39,XXREAL_1:1;
            end;
            hence s1 <= s2;
          end;
          hence thesis by A14,A15,JORDAN5C:def 3;
        end;
        hence LE p1,p2,K by A14,A15,JORDAN6:def 10;
      end;
      case
A61:    p2 in LSeg(|[b,d]|,|[b,c]|);
A62:    p2`1 =b by A61,TOPREAL3:17;
        Lower_Arc(K)=LSeg(|[b,d]|,|[b,c]|) \/ LSeg(|[b,c]|,|[a,c]|) by A1,Th62;
        then
A63:    LSeg(|[b,d]|,|[b,c]|) c= Lower_Arc(K) by XBOOLE_1:7;
        p2 <> W-min(K) by A1,A9,A62,EUCLID:56;
        hence LE p1,p2,K by A1,A3,A61,A63,JORDAN6:def 10;
      end;
      case
A64:    p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
        Lower_Arc(K)=LSeg(|[b,d]|,|[b,c]|) \/ LSeg(|[b,c]|,|[a,c]|) by A1,Th62;
        then LSeg(|[b,c]|,|[a,c]|) c= Lower_Arc(K) by XBOOLE_1:7;
        hence LE p1,p2,K by A1,A3,A64,JORDAN6:def 10;
      end;
    end;
    hence LE p1,p2,K;
  end;
end;

theorem Th70:
  for a,b,c,d being real number,p1,p2 being Point of TOP-REAL 2 st a<b & c <d
  & p1 in LSeg(|[a,d]|,|[b,d]|)

holds LE p1,p2,rectangle(a,b,c,d) iff p2 in LSeg(|[a,d]|,|[b,d]|) & p1`1<=p2`1
  or p2 in LSeg(|[b,d]|,|[b,c]|)
  or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min rectangle(a,b,c,d)
proof
  let a,b,c,d be real number,p1,p2 be Point of TOP-REAL 2;
  set K = rectangle(a,b,c,d);
  assume
A1: a<b & c <d & p1 in LSeg(|[a,d]|,|[b,d]|);
  then
A2: K is_simple_closed_curve by Th60;
  Upper_Arc(K)=LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|) by A1,Th61;
  then
A3: LSeg(|[a,d]|,|[b,d]|) c= Upper_Arc(K) by XBOOLE_1:7;
A4: p1`2=d & a <=p1`1 & p1`1 <= b by A1,Th11;
  thus LE p1,p2,K implies p2 in LSeg(|[a,d]|,|[b,d]|) & p1`1<=p2`1
  or p2 in LSeg(|[b,d]|,|[b,c]|) or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K)
  proof
    assume
A5: LE p1,p2,K;
    then
A6: p1 in K & p2 in K by A2,JORDAN7:5;
    K= LSeg(|[ a,c ]|, |[ a,d ]|) \/ LSeg(|[ a,d ]|, |[ b,d ]|)
    \/ (LSeg(|[ a,c ]|, |[ b,c ]|) \/ LSeg(|[ b,c ]|, |[ b,d ]|))
    by SPPOL_2:def 3
      .=LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|)
    \/ LSeg(|[b,d]|,|[b,c]|) \/ LSeg(|[b,c]|,|[a,c]|) by XBOOLE_1:4;
    then p2 in LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|)
    \/ LSeg(|[b,d]|,|[b,c]|) or
    p2 in LSeg(|[b,c]|,|[a,c]|) by A6,XBOOLE_0:def 2;
    then
A7: p2 in LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|) or
    p2 in LSeg(|[b,d]|,|[b,c]|) or
    p2 in LSeg(|[b,c]|,|[a,c]|) by XBOOLE_0:def 2;
    now per cases by A7,XBOOLE_0:def 2;
      case p2 in LSeg(|[a,c]|,|[a,d]|);
        then LE p2,p1,K by A1,Th69;
        hence p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1
        or p2 in LSeg(|[b,d]|,|[b,c]|)
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A1,A5,Th60,JORDAN6:72;
      end;
      case p2 in LSeg(|[a,d]|,|[b,d]|);
        hence p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1
        or p2 in LSeg(|[b,d]|,|[b,c]|)
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A1,A5,Th66;
      end;
      case p2 in LSeg(|[b,d]|,|[b,c]|);
        hence p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1
        or p2 in LSeg(|[b,d]|,|[b,c]|)
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
      end;
      case
A8:     p2 in LSeg(|[b,c]|,|[a,c]|);
        now per cases;
          case p2=W-min(K);
            then LE p2,p1,K by A2,A6,JORDAN7:3;
            hence p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1
            or p2 in LSeg(|[b,d]|,|[b,c]|)

or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A1,A5,Th60,JORDAN6:72;
          end;
          case p2<>W-min(K);
            hence p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1
            or p2 in LSeg(|[b,d]|,|[b,c]|)
            or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A8;
          end;
        end;
        hence p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1
        or p2 in LSeg(|[b,d]|,|[b,c]|)
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
      end;
    end;
    hence p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1
    or p2 in LSeg(|[b,d]|,|[b,c]|)
    or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
  end;
A9: W-min(K)= |[a,c]| by A1,Th56; thus
  p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1 or p2 in LSeg(|[b,d]|,|[b,c]|)
  or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) implies LE p1,p2,K
  proof
    assume
A10: p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1 or p2 in LSeg(|[b,d]|,|[b,c]|)
    or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
    now per cases by A10;
      case
A11:    p2 in LSeg(|[a,d]|,|[b,d]|)& p1`1<=p2`1;
        then
A12:    p2`2=d & a <=p2`1 & p2`1 <= b by A1,Th11;
A13:    Upper_Arc(K)=LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|)
        by A1,Th61;
        then
A14:    p2 in Upper_Arc(K) by A11,XBOOLE_0:def 2;
A15:    p1 in Upper_Arc(K) by A1,A13,XBOOLE_0:def 2;
        LE p1,p2,Upper_Arc(K),W-min(K),E-max(K)
        proof
          for g being Function of I[01], (TOP-REAL 2)|Upper_Arc(K),
          s1, s2 being Real st g is_homeomorphism
          & g.0 = W-min(K) & g.1 = E-max(K) & g.s1 = p1 & 0 <= s1 & s1 <= 1
          & g.s2 = p2 & 0 <= s2 & s2 <= 1 holds s1 <= s2
          proof
            let g be Function of I[01], (TOP-REAL 2)|Upper_Arc(K),
            s1, s2 be Real;
            assume
A16:        g is_homeomorphism & g.0 = W-min(K) & g.1 = E-max(K)
            & g.s1 = p1 & 0 <= s1 & s1 <= 1 & g.s2 = p2 & 0 <= s2 & s2 <= 1;
A17:        dom g=the carrier of I[01] by FUNCT_2:def 1;
A18:        g is one-to-one by A16,TOPS_2:def 5;
A19:        the carrier of ((TOP-REAL 2)|Upper_Arc(K))
            =Upper_Arc(K) by PRE_TOPC:29;
            then reconsider g1=g as Function of I[01],TOP-REAL 2 by FUNCT_2:9;
            g is continuous by A16,TOPS_2:def 5;
            then
A20:        g1 is continuous by PRE_TOPC:56;
            reconsider h1=proj1 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
            reconsider h2=proj2 as Function of TOP-REAL 2,R^1 by TOPMETR:24;
A21:        (TOP-REAL 2)|([#](TOP-REAL 2))=TOP-REAL 2 by TSEP_1:3;
            then
A22:        (for p being Point of (TOP-REAL 2)|([#](TOP-REAL 2 )) holds
            h1.p=proj1.p) implies h1 is continuous by JGRAPH_2:39;
            (for p being Point of (TOP-REAL 2)|([#](TOP-REAL 2)) holds
            h2.p=proj2.p) implies h2 is continuous by A21,JGRAPH_2:40;
            then consider h being Function of TOP-REAL 2,R^1 such that
A23:        (for p being Point of TOP-REAL 2, r1,r2 being real number st
            h1.p=r1 & h2.p=r2 holds h.p=r1+r2) & h is continuous
            by A22,JGRAPH_2:29;
            reconsider k=h*g1 as Function of I[01],R^1;
A24:        k is continuous Function of I[01],R^1 by A20,A23;
A25:        W-min K=|[a,c]| by A1,Th56;
            now
              assume
A26:          s1>s2;
A27:          dom g=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
              0 in [.0,1.] by XXREAL_1:1;
              then
A28:          k.0=h.(W-min(K)) by A16,A27,FUNCT_1:23
                .=h1.(W-min(K))+h2.(W-min(K)) by A23
                .=(W-min(K))`1+proj2.(W-min(K)) by PSCOMP_1:def 28
                .=(W-min(K))`1+(W-min(K))`2 by PSCOMP_1:def 29
                .=(W-min(K))`1+c by A25,EUCLID:56
                .=a+c by A25,EUCLID:56;
              s1 in [.0,1.] by A16,XXREAL_1:1;
              then
A29:          k.s1=h.p1 by A16,A27,FUNCT_1:23
                .=proj1.p1 +proj2.p1 by A23
                .=p1`1+proj2.p1 by PSCOMP_1:def 28
                .=p1`1+d by A4,PSCOMP_1:def 29;
A30:          s2 in [.0,1.] by A16,XXREAL_1:1;
              then k.s2=h.p2 by A16,A27,FUNCT_1:23
                .=proj1.p2 +proj2.p2 by A23
                .=p2`1+proj2.p2 by PSCOMP_1:def 28
                .=p2`1 +d by A12,PSCOMP_1:def 29;
              then
A31:          k.0<=k.s1 & k.s1<=k.s2 by A1,A4,A11,A28,A29,XREAL_1:9;
A32:          0 in [.0,1.] by XXREAL_1:1;
              then
A33:          [.0,s2.] c= [.0,1.] by A30,XXREAL_2:def 12;
              reconsider B=[.0,s2.] as Subset of I[01] by A30,A32,BORSUK_1:83
              ,XXREAL_2:def 12;
A34:          B is connected by A16,A30,A32,BORSUK_1:83,BORSUK_4:49;
A35:          0 in B by A16,XXREAL_1:1;
A36:          s2 in B by A16,XXREAL_1:1;
A37:          k.0 is Real by XREAL_0:def 1;
A38:          k.s2 is Real by XREAL_0:def 1;
              k.s1 is Real by XREAL_0:def 1;
              then consider xc being Point of I[01] such that
A39:          xc in B & k.xc =k.s1 by A24,A31,A34,A35,A36,A37,A38,TOPREAL5:11;
              xc in [.0,1.] by BORSUK_1:83;
              then reconsider rxc=xc as Real;
A40:          k is one-to-one
              proof
                for x1,x2 being set st x1 in dom k & x2 in dom k &
                k.x1=k.x2 holds x1=x2
                proof
                  let x1,x2 be set;
                  assume
A41:              x1 in dom k & x2 in dom k & k.x1=k.x2;
                  then reconsider r1=x1 as Point of I[01];
                  reconsider r2=x2 as Point of I[01] by A41;
A42:              k.x1=h.(g1.x1) by A41,FUNCT_1:22
                    .=h1.(g1.r1)+h2.(g1.r1) by A23
                    .=(g1.r1)`1+proj2.(g1.r1) by PSCOMP_1:def 28
                    .=(g1.r1)`1+(g1.r1)`2 by PSCOMP_1:def 29;
A43:              k.x2=h.(g1.x2) by A41,FUNCT_1:22
                    .=h1.(g1.r2)+h2.(g1.r2) by A23
                    .=(g1.r2)`1+proj2.(g1.r2) by PSCOMP_1:def 28
                    .=(g1.r2)`1+(g1.r2)`2 by PSCOMP_1:def 29;
A44:              g.r1 in Upper_Arc(K) by A19;
A45:              g.r2 in Upper_Arc(K) by A19;
                  reconsider gr1=g.r1 as Point of TOP-REAL 2 by A44;
                  reconsider gr2=g.r2 as Point of TOP-REAL 2 by A45;
                  now per cases by A13,A19,XBOOLE_0:def 2;
                    case
A46:                  g.r1 in LSeg(|[a,c]|,|[a,d]|) &
                      g.r2 in LSeg(|[a,c]|,|[a,d]|);
                      then
A47:                  (gr1)`1=a & c <=(gr1)`2 & (gr1 )`2 <=d by A1,Th9;
                      (gr2)`1=a & c <=(gr2)`2 & (gr2)`2 <=d by A1,A46,Th9;

then |[(gr1)`1,(gr1)`2]|=g.r2 by A41,A42,A43,A47,EUCLID:57;
                      then g.r1=g.r2 by EUCLID:57;
                      hence x1=x2 by A17,A18,FUNCT_1:def 8;
                    end;
                    case
A48:                  g.r1 in LSeg(|[a,c]|,|[a,d]|) &
                      g.r2 in LSeg(|[a,d]|,|[b,d]|);
                      then
A49:                  (gr1)`1=a & c <=(gr1)`2 & (gr1 )`2 <=d by A1,Th9;
A50:                  (gr2)`2=d & a <=(gr2)`1 & (gr2)`1 <=b by A1,A48,Th11;
A51:                  a+(gr1)`2=(gr2)`1 +d by A1,A41,A42,A43,A48,A49,Th11;
A52:                  now
                        assume a<>gr2`1;
                        then a<gr2`1 by A50,XXREAL_0:1;
                        hence contradiction by A49,A51,XREAL_1:10;
                      end;
                      now
                        assume gr1`2<>d;
                        then d>gr1`2 by A49,XXREAL_0:1;
                        hence contradiction by A41,A42,A43,A49,A50,XREAL_1:10;
                      end;
                      then |[(gr1)`1,(gr1)`2]|=g.r2 by A49,A50,A52,EUCLID:57;
                      then g.r1=g.r2 by EUCLID:57;
                      hence x1=x2 by A17,A18,FUNCT_1:def 8;
                    end;
                    case
A53:                  g.r1 in LSeg(|[a,d]|,|[b,d]|) &
                      g.r2 in LSeg(|[a,c]|,|[a,d]|);
                      then
A54:                  (gr2)`1=a & c <=(gr2)`2 & (gr2 )`2 <=d by A1,Th9;
A55:                  (gr1)`2=d & a <=(gr1)`1 & (gr1)`1 <=b by A1,A53,Th11;
A56:                  a+(gr2)`2=(gr1)`1 +d by A1,A41,A42,A43,A53,A54,Th11;
A57:                  now
                        assume a<>gr1`1;
                        then a<gr1`1 by A55,XXREAL_0:1;
                        hence contradiction by A54,A56,XREAL_1:10;
                      end;
                      now
                        assume gr2`2<>d;
                        then d>gr2`2 by A54,XXREAL_0:1;
                        hence contradiction by A41,A42,A43,A54,A55,XREAL_1:10;
                      end;
                      then |[(gr2)`1,(gr2)`2]|=g.r1 by A54,A55,A57,EUCLID:57;
                      then g.r1=g.r2 by EUCLID:57;
                      hence x1=x2 by A17,A18,FUNCT_1:def 8;
                    end;
                    case
A58:                  g.r1 in LSeg(|[a,d]|,|[b,d]|) &
                      g.r2 in LSeg(|[a,d]|,|[b,d]|);
                      then
A59:                  (gr1)`2=d & a <=(gr1)`1 & (gr1 )`1 <=b by A1,Th11;
                      (gr2)`2=d & a <=(gr2)`1 & (gr2)`1 <=b by A1,A58,Th11;

then |[(gr1)`1,(gr1)`2]|=g.r2 by A41,A42,A43,A59,EUCLID:57;
                      then g.r1=g.r2 by EUCLID:57;
                      hence x1=x2 by A17,A18,FUNCT_1:def 8;
                    end;
                  end;
                  hence x1=x2;
                end;
                hence thesis by FUNCT_1:def 8;
              end;
A60:          dom k=[.0,1.] by BORSUK_1:83,FUNCT_2:def 1;
              then s1 in dom k by A16,XXREAL_1:1;
              then rxc=s1 by A33,A39,A40,A60,FUNCT_1:def 8;
              hence contradiction by A26,A39,XXREAL_1:1;
            end;
            hence s1 <= s2;
          end;
          hence thesis by A14,A15,JORDAN5C:def 3;
        end;
        hence LE p1,p2,K by A14,A15,JORDAN6:def 10;
      end;
      case
A61:    p2 in LSeg(|[b,d]|,|[b,c]|);
A62:    p2`1 =b by A61,TOPREAL3:17;
        Lower_Arc(K)=LSeg(|[b,d]|,|[b,c]|) \/ LSeg(|[b,c]|,|[a,c]|) by A1,Th62;
        then
A63:    LSeg(|[b,d]|,|[b,c]|) c= Lower_Arc(K) by XBOOLE_1:7;
        p2 <> W-min(K) by A1,A9,A62,EUCLID:56;
        hence LE p1,p2,K by A1,A3,A61,A63,JORDAN6:def 10;
      end;
      case
A64:    p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
        Lower_Arc(K)=LSeg(|[b,d]|,|[b,c]|) \/ LSeg(|[b,c]|,|[a,c]|) by A1,Th62;
        then LSeg(|[b,c]|,|[a,c]|) c= Lower_Arc(K) by XBOOLE_1:7;
        hence LE p1,p2,K by A1,A3,A64,JORDAN6:def 10;
      end;
    end;
    hence LE p1,p2,K;
  end;
end;

theorem Th71:
  for a,b,c,d being real number,p1,p2 being Point of TOP-REAL 2
  st a<b & c <d & p1 in LSeg(|[b,d]|,|[b,c]|)

holds LE p1,p2,rectangle(a,b,c,d) iff p2 in LSeg(|[b,d]|,|[b,c]|) & p1`2>=p2`2
  or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min rectangle(a,b,c,d)
proof
  let a,b,c,d be real number,p1,p2 be Point of TOP-REAL 2;
  set K = rectangle(a,b,c,d);
  assume
A1: a<b & c <d & p1 in LSeg(|[b,d]|,|[b,c]|);
  then
A2: K is_simple_closed_curve by Th60;
A3: p1`1=b & c <=p1`2 & p1`2 <= d by A1,Th9;
  thus LE p1,p2,K implies p2 in LSeg(|[b,d]|,|[b,c]|) & p1`2>=p2`2
  or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K)
  proof
    assume
A4: LE p1,p2,K;
    then
A5: p1 in K & p2 in K by A2,JORDAN7:5;
    K= LSeg(|[ a,c ]|, |[ a,d ]|) \/ LSeg(|[ a,d ]|, |[ b,d ]|)
    \/ (LSeg(|[ a,c ]|, |[ b,c ]|) \/ LSeg(|[ b,c ]|, |[ b,d ]|))
    by SPPOL_2:def 3
      .=LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|)
    \/ LSeg(|[b,d]|,|[b,c]|) \/ LSeg(|[b,c]|,|[a,c]|) by XBOOLE_1:4;
    then p2 in LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|)
    \/ LSeg(|[b,d]|,|[b,c]|) or
    p2 in LSeg(|[b,c]|,|[a,c]|) by A5,XBOOLE_0:def 2;
    then
A6: p2 in LSeg(|[a,c]|,|[a,d]|) \/ LSeg(|[a,d]|,|[b,d]|) or
    p2 in LSeg(|[b,d]|,|[b,c]|) or
    p2 in LSeg(|[b,c]|,|[a,c]|) by XBOOLE_0:def 2;
    now per cases by A6,XBOOLE_0:def 2;
      case p2 in LSeg(|[a,c]|,|[a,d]|);
        then LE p2,p1,K by A1,Th69;
        hence p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A1,A4,Th60,JORDAN6:72;
      end;
      case p2 in LSeg(|[a,d]|,|[b,d]|);
        then LE p2,p1,K by A1,Th70;
        hence p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A1,A4,Th60,JORDAN6:72;
      end;
      case p2 in LSeg(|[b,d]|,|[b,c]|);
        hence p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A1,A4,Th67;
      end;
      case
A7:     p2 in LSeg(|[b,c]|,|[a,c]|);
        now per cases;
          case p2=W-min(K);
            then LE p2,p1,K by A2,A5,JORDAN7:3;
            hence p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2

or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A1,A4,Th60,JORDAN6:72;
          end;
          case p2<>W-min(K);
            hence p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2
            or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) by A7;
          end;
        end;
        hence p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2
        or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
      end;
    end;
    hence p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2
    or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
  end; thus
  p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2
  or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K) implies LE p1,p2,K
  proof
    assume
A8: p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2
    or p2 in LSeg(|[b,c]|,|[a,c]|) & p2<>W-min(K);
    now per cases by A8;
      case
A9:     p2 in LSeg(|[b,d]|,|[b,c]|)& p1`2>=p2`2;
        then
A10:    p2`1=b & c <=p2`2 & p2`2 <= d by A1,Th9;
        W-min K=|[a,c]| by A1,Th56;
        then
A11:    p2 <> W-min(K) by A1,A10,EUCLID:56;
