              end;
              case
A52:            g.r1 in LSeg(|[a,c]|,|[a,d]|) & g.r2 in LSeg(|[a,d]|,|[b,d]|);
                then
A53:            (gr1)`1=a & c <=(gr1)`2 & (gr1)`2 <=d by A1,Th9;
A54:            (gr2)`2=d & a <=(gr2)`1 & (gr2)`1 <=b by A1,A52,Th11;
A55:            a+(gr1)`2=(gr2)`1 +d by A1,A45,A46,A47,A52,A53,Th11;
A56:            now
                  assume a<>gr2`1;
                  then a<gr2`1 by A54,XXREAL_0:1;
                  hence contradiction by A53,A55,XREAL_1:10;
                end;
                now
                  assume gr1`2<>d;
                  then d>gr1`2 by A53,XXREAL_0:1;
