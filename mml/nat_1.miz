      REAL_1, XXREAL_0;
 constructors NUMBERS, XCMPLX_0, XXREAL_0, XREAL_0, CARD_1, WELLORD2;
 registrations SUBSET_1, ORDINAL1, NUMBERS, XXREAL_0, XREAL_0, REAL_1,
      XBOOLE_0, CARD_1, FINSET_1;
 requirements REAL, NUMERALS, SUBSET, BOOLE, ARITHM;
 definitions ORDINAL1, TARSKI, XBOOLE_0, REAL_1, CARD_1;
 theorems AXIOMS, REAL_1, ORDINAL1, XCMPLX_1, XREAL_1, XXREAL_0, TARSKI,
      ORDINAL2, XBOOLE_1, XBOOLE_0, CARD_1, ZFMISC_1;
