begin

definition
  mode Nat is natural number;
end;

 reserve x for Real,
         p,k,l,m,n,s,h,i,j,k1,t,t1 for Nat,
         X for Subset of REAL;

:: The results of axioms of natural numbers

theorem                       :: axiom of induction
 Th1: for X st 0 in X & for x st x in X holds x + 1 in X
  for n holds n in X
   proof let A be Subset of REAL such that
A1:  0 in A;
     assume x in A implies x + 1 in A;
     then for x being real number st x in A holds x + 1 in A;
then A2:  NAT c= A by A1,AXIOMS:29;
    let n;
     n in NAT by ORDINAL1:def 13;
    hence n in A by A2;
   end;

:: Addition and multiplication

:: The natural numbers are real numbers therefore some theorems of real
:: numbers are translated for natural numbers.

registration let n,k be Nat;
  cluster n + k -> natural;
  coherence
  proof
     defpred P[Real] means ex k st $1 = k & n + k is natural;
     consider X such that
A1:  x in X iff P[x] from REAL_1:sch 1;
     n + 0 = n;
     then A2:    0 in X by A1;
      now let x; assume x in X;
       then consider k such that
A3:    x = k & n + k is natural by A1;
A4:    (n + k) + 1 = n + (k + 1);
       reconsider k as Element of NAT by ORDINAL1:def 13;
       n + k is Element of NAT by A3,ORDINAL1:def 13;
       then k + 1 is Element of NAT & (n + k) + 1 is Element of NAT
       by AXIOMS:28;
       hence x + 1 in X by A1,A3,A4;
     end;
     then k in X by A2,Th1;
     then ex m st k = m & n + m is natural by A1;
     hence thesis;
  end;
end;

definition let n be Nat, k be Element of NAT;
  redefine func n + k -> Element of NAT;
  coherence by ORDINAL1:def 13;
 end;

definition let n be Element of NAT, k be Nat;
  redefine func n + k -> Element of NAT;
  coherence by ORDINAL1:def 13;
 end;

:: Now we can form and prove the scheme of induction.

scheme Ind { P[Nat] } :
 for k being Element of NAT holds P[k]
   provided
A1:   P[0] and
A2:   for k being Element of NAT st P[k] holds P[k + 1]
proof let k;
  defpred P1[Real] means ex k st P[k] & k = $1;
    consider X such that
A3:   x in X iff P1[x] from REAL_1:sch 1;
A4:   0 in X by A1,A3;
       for x st x in X holds x + 1 in X
      proof let x; assume x in X;
       then consider k such that
A5:      P[k] & k = x by A3;
         reconsider k as Element of NAT by ORDINAL1:def 13;
          P[k + 1] by A2,A5;
       hence thesis by A3,A5;
      end;
     then k in X by A4,Th1;
     then ex n st P[n] & n = k by A3;
    hence thesis;
   end;

scheme NatInd { P[Nat] } :
 for k being Nat holds P[k]
   provided
A1:   P[0] and
A2:   for k be Nat st P[k] holds P[k + 1]
proof
A3: P[0] by A1;
A4:   for k be Element of NAT st P[k] holds P[k + 1] by A2;
A5:   for k be Element of NAT holds P[k] from Ind(A3,A4);
      let k be Nat;
       k is Element of NAT by ORDINAL1:def 13;
      hence thesis by A5;
   end;

:: Like addition, the result of multiplication of two natural numbers is
:: a natural number.

registration let n,k be Nat;
  cluster n * k -> natural;
  coherence
  proof
    defpred P[Nat] means n*$1 is natural;
A1: P[0];
A2: for m be Nat holds P[m] implies P[m+1]
    proof
      let m be Nat;
      assume P[m];
      then reconsider k = n * m as Nat;
      k + n is Nat;
      hence thesis;
    end;
    for m be Nat holds P[m] from NatInd(A1,A2);
    hence thesis;
  end;
end;

definition let n be Nat, k be Element of NAT;
  redefine func n * k -> Element of NAT;
  coherence by ORDINAL1:def 13;
end;

definition let n be Element of NAT, k be Nat;
  redefine func n * k -> Element of NAT;
  coherence by ORDINAL1:def 13;
end;

                ::::::::::::::::::::
                :: Order relation ::
                ::::::::::::::::::::

:: Some theorems of not great relation "<=" in real numbers are translated
:: to natural number easy and it is necessary to have them here.

theorem Th2:
  0 <= i
  proof
     defpred P[Nat] means 0 <= $1;
A1:  P[0];
A2:  now let n; assume P[n];
      then 0 + 1 <= n + 1 by XREAL_1:8;
      hence P[n + 1];
     end;
    for k holds P[k] from NatInd(A1,A2);
   hence thesis;
  end;

theorem
   0 <> i implies 0 < i by Th2;

theorem
   i <= j implies i * h <= j * h
  proof assume
A1:  i <= j;
    0 <= h by Th2;
   hence thesis by A1,XREAL_1:66;
  end;

theorem
   0 < i + 1
  proof assume
A1: 0 >= i + 1;
     0 <= i by Th2;
    then 0 + 1 <= 0 by A1,XREAL_1:9;
    hence contradiction;
  end;

theorem Th6:
  i = 0 or ex k st i = k + 1
  proof
    defpred P[Nat] means $1 = 0 or ex k st $1 = k + 1;
A1: P[0];
A2: P[h] implies P[h + 1];
     for i holds P[i] from NatInd(A1,A2);
    hence thesis;
  end;

theorem Th7:
  i + j = 0 implies i = 0 & j = 0
  proof assume
A1:  i + j = 0;
    0 <= i & 0 <= j by Th2;
   then 0 + i <= j + i & 0 + j <= i + j & 0 + i = i & 0 + j = j by XREAL_1:9;
   hence thesis by A1,Th2;
  end;

registration
  cluster non zero Nat;
  existence
  proof
    take 1;
    thus thesis;
  end;
end;

registration let m be Nat, n be non zero Nat;
  cluster m + n -> non zero;
  coherence by Th7;
  cluster n + m -> non zero;
  coherence;
end;

scheme DefbyInd { N()->Nat, F(Nat,Nat)->Nat, P[Nat,Nat] } :
  (for k being Nat ex n being Nat st P[k,n]) &
    for k,n,m being Nat st P[k,n] & P[k,m] holds n = m
     provided
A1:    for k,n being Nat holds P[k,n] iff
       k = 0 & n = N() or
       ex m,l being Nat st k = m + 1 & P[m,l] & n = F(k,l)
  proof
    reconsider N = N() as Element of NAT by ORDINAL1:def 13;
      defpred P[Nat] means ex n being Nat st P[$1,n];
       P[0,N] by A1;
then A2:   P[0];
A3: for k being Nat holds P[k] implies P[k+1]
      proof let k be Nat;
        given n being Nat such that
A4:    P[k,n];
       reconsider F = F(k+1,n) as Element of NAT by ORDINAL1:def 13;
       take F;
       thus P[k+1,F] by A1,A4;
      end;
    thus for k being Nat holds P[k] from NatInd(A2,A3);
    defpred P[Nat] means
    for n,m being Nat st P[$1,n] & P[$1,m] holds n = m;
A5:   P[0]
      proof let n,m be Nat such that
A6:     P[0,n] & P[0,m];
        (not ex m,l being Nat st 0 = m + 1 & P[m,l] & n = F(0,l))&
        not ex n,l being Nat st 0 = n + 1 & P[n,l] & m = F(0,l);
        then n = N() & m = N() by A1,A6;
       hence n = m;
      end;
A7:   for k being Nat st P[k] holds P[k+1]
      proof let k be Nat such that
A8:      for n,m being Nat st P[k,n] & P[k,m] holds n = m;
       let n,m be Nat such that
A9:      P[k+1,n] & P[k+1,m];
       consider l,u being Nat such that
A10:   k + 1 = l + 1 & P[l,u] & n = F(k + 1,u) by A1,A9;
       ex v,w being Nat  st k + 1 = v + 1 & P[v,w] & m = F(k + 1,w)
         by A1,A9;
       hence n = m by A8,A10;
      end;
    thus for k being Nat holds P[k] from NatInd(A5,A7);
   end;

theorem Th8:
  i <= j + 1 implies i <= j or i = j + 1
  proof
     defpred P[Nat] means
     for j holds $1 <= j + 1 implies $1 <= j or $1 = j+1;
A1:  P[0] by Th2;
A2:  for i st P[i] holds P[i+1]
     proof let i such that
A3:     for j holds i <= j + 1 implies i <= j or i = j + 1;
        let j;
        assume that
A4:     i + 1 <= j + 1;
A5:     now assume
A6:       j = 0;
then A7:       i <= 0 by A4,XREAL_1:8;
             0 <= i by Th2;
        hence thesis by A6,A7,XXREAL_0:1;
       end;
         now given k such that
A8:       j = k + 1;
          i <= k + 1 by A4,A8,XREAL_1:8;
         then i <= k or i = k + 1 by A3;
        hence thesis by A8,XREAL_1:8;
       end;
     hence thesis by A5,Th6;
    end;
     for i holds P[i] from NatInd(A1,A2);
    hence thesis;
  end;

theorem
   i <= j & j <= i + 1 implies i = j or j = i + 1
  proof assume
A1:  i <= j & j <= i + 1;
     then j <= i or j = i + 1 by Th8;
     hence thesis by A1,XXREAL_0:1;
  end;

theorem Th10:
  i <= j implies ex k st j = i + k
  proof
     defpred P[Nat] means
     i <= $1 implies ex k st $1 = i + k;
A1: P[0]
     proof assume
A2:   i <= 0;
      take 0;
      thus thesis by A2,Th2;
     end;
A3:  for j st P[j] holds P[j+1]
     proof let j such that
A4:   i <= j implies ex k st j = i + k;
      assume
A5:     i <= j + 1;
A6:     now assume i <= j;
        then consider k such that
A7:     j = i + k by A4;
         i + k + 1 = i + (k + 1);
        hence thesis by A7;
       end;
       now assume i = j + 1; then j + 1 = i + 0;
        hence thesis;
       end;
      hence thesis by A5,A6,Th8;
     end;
    for j holds P[j] from NatInd(A1,A3);
   hence thesis;
  end;

theorem Th11:
  i <= i + j
  proof
A1:  0 + i = i;
    0 <= j by Th2;
   hence thesis by A1,XREAL_1:9;
  end;

scheme CompInd { P[Nat] } :
  for k being Nat holds P[k]
   provided
A1:   for k being Nat st
      for n being Nat st n < k holds P[n] holds P[k]
  proof
      defpred P1[Nat] means
      for n being Nat st n < $1 holds P[n];
A2:   P1[0] by Th2;
A3:   for k be Nat holds P1[k] implies P1[k+1]
      proof
        let k be Nat;
        assume
A4:   for n being Nat st n < k holds P[n];
      let n be Nat; assume n < k + 1;
      then n <= k by Th8;
      then n < k or n = k & n <= k by REAL_1:def 5;
      hence thesis by A1,A4;
    end;
    let k be Nat;
    for k being Nat holds P1[k] from NatInd(A2,A3);
    then for n being Nat st n < k holds P[n];
    hence thesis by A1;
   end;

:: Principle of minimum

scheme Min { P[Nat] } :
 ex k being Nat st P[k] &
 for n being Nat st P[n] holds k <= n
  provided
A1:  ex k being Nat st P[k]
     proof assume A2: not thesis;
      defpred P1[Nat] means not P[$1];
A3:   for k being Nat st
      for n being Nat st n < k holds P1[n] holds P1[k]
      proof let k be Nat;
A4:    not (ex n being Nat st P[n] & not k <= n) implies not P[k]
       by A2;
       assume for n being Nat st n < k holds not P[n];
       hence thesis by A4;
      end;
      for k be Nat holds P1[k] from CompInd(A3);
    hence contradiction by A1;
   end;

:: Principle of maximum

scheme Max { P[Nat], N()->Nat } :
  ex k being Nat st P[k] &
  for n being Nat st P[n] holds n <= k
   provided
A1:   for k being Nat st P[k] holds k <= N() and
A2:   ex k being Nat st P[k]
  proof
      defpred P1[Nat] means
      for n being Nat st P[n] holds n <= $1;
       A3:   ex k being Nat st P1[k] by A1;
    consider k being Nat such that
A4:   P1[k] and
A9:   for m being Nat st P1[m] holds k <= m from Min(A3);
    take k;
    thus P[k]
      proof assume
A5:      not P[k];
       consider n being Nat such that
A6:      P[n] by A2;
         n <= k & n <> k by A4,A5,A6;
       then k <> 0 by Th2;
       then consider m such that
A7:      k = m + 1 by Th6;
        for n being Nat st P[n] holds n <= m by A7,Th8,A4,A5;
        then A8:    k <= m by A9;
        -m + m + 1 = -m + (m + 1) & -m + m + 0 = -m + (m + 0);
       hence contradiction by A7,A8,XREAL_1:8;
      end;
    thus thesis by A4;
   end;

theorem Th12:
  i <= j implies i <= j + h
  proof assume
A1:  i <= j;
        0 <= h by Th2;
    then i + 0 <= i + h & i + h <= j + h & i + 0 = i by A1,XREAL_1:9;
   hence i <= j + h by XXREAL_0:2;
  end;

theorem Th13:
  i < j + 1 iff i <= j
  proof
   thus i < j + 1 implies i <= j by Th8;
   assume
A1:  i <= j;
   hence i <= j + 1 by Th12;
   assume A2: i = j + 1;
A3:  j + (- j) = 0;
    j + 1 + (- j) = 1;
   hence contradiction by A1,A2,A3,XREAL_1:8;
  end;

theorem Th14:
  j < 1 implies j = 0
 proof
  assume j < 1;
    then j < 0 + 1;
    then A1: j <= 0 by Th13;
   assume j <> 0;
  hence thesis by A1,Th2;
 end;

theorem
   i * j = 1 implies i = 1
  proof assume
A1:  i * j = 1;
    then i <> 0;
   then consider m such that
A2:  i = m + 1 by Th6;
     j <> 0 by A1;
   then consider l such that
A3:  j = l + 1 by Th6;
     m * l + m + l + 1 = 0 + 1 by A1,A2,A3;
    then m * l + m = 0 & l = 0 by Th7;
   hence thesis by A2;
  end;

theorem Th16:
  k <> 0 implies n < n + k
 proof
  assume k <> 0;
   then A1: n <> n + k;
     n <= n + k & n <= k + n by Th12;
  hence n < n + k by A1,REAL_1:def 5;
 end;

scheme Regr { P[Nat] } :
  P[0]
  provided
A1:  ex k being Nat st P[k] and
A2:  for k being Nat st k <> 0 & P[k]
     ex n being Nat st n < k & P[n]
proof
   consider k being Nat such that
A4:  P[k] & for n being Nat st P[n] holds k <= n from Min(A1);
     now assume k <> 0;
       then ex n being Nat st n < k & P[n] by A2,A4;
       hence contradiction by A4;
    end;
   hence P[0] by A4;
  end;

:: Exact division and rest of division

theorem
   0 < m implies for n ex k,t st n = (m*k)+t & t < m
   proof
    assume
A1:   0 < m;
     defpred P[Nat] means ex k,t st $1 = (m*k)+t & t < m;
         0 = m*0+0;
then A2:   P[0] by A1;
A3:   for n st P[n] holds P[n+1]
      proof let n;
       given k1,t1 such that
A4:     n = (m*k1)+t1 & t1 < m;
A5:        t1+1 <= m by A4,Th13;
A6:     t1+1 < m implies ex k,t st n+1 = (m*k)+t & t < m
         proof assume
A7:        t1+1 < m;
          take k = k1 , t = t1+1;
          thus n+1 = m*k+t by A4;
          thus t < m by A7;
         end;
            t1+1 = m implies ex k,t st n+1 = (m*k)+t & t < m
         proof assume
A8:        t1+1 = m;
          take k = k1+1 , t = 0;
          thus n+1 = m*k+t by A4,A8;
          thus t < m by A1;
         end;
       hence ex k,t st n+1 = (m*k)+t & t < m by A5,A6,REAL_1:def 5;
      end;
    thus for n holds P[n] from NatInd(A2,A3);
   end;

theorem
   n = m*k+t & t < m & n = m*k1+t1 & t1 < m implies k = k1 & t = t1
   proof
    assume
A1:   n = m*k+t & t < m & n = m*k1+t1 & t1 < m;
A2:   now assume k <= k1;
      then consider u being Nat such that
A3:     k1 = k + u by Th10;
           m * (k + u) + t1 = m * k + (m * u + t1);
then A4:     m * u + t1 = t by A1,A3,XCMPLX_1:2;
           now given w being Nat such that
A5:       u = w + 1;
           m * u + t1 = m + (m * w + t1) by A5;
        hence contradiction by A1,A4,Th11;
       end;
then A6:     u = 0 by Th6;
      hence k = k1 by A3;
      thus t = t1 by A1,A3,A6,XCMPLX_1:2;
     end;
         now assume k1 <= k;
      then consider u being Nat such that
A7:     k = k1 + u by Th10;
           m * (k1 + u) + t = m * k1 + (m * u + t);
then A8:     m * u + t = t1 by A1,A7,XCMPLX_1:2;
           now given w being Nat such that
A9:       u = w + 1;
           m * u + t = m + (m * w + t) by A9;
        hence contradiction by A1,A8,Th11;
       end;
then A10:     u = 0 by Th6;
      hence k = k1 by A7;
       thus t = t1 by A1,A7,A10,XCMPLX_1:2;
     end;
    hence thesis by A2;
   end;

registration
  cluster -> ordinal Nat;
  coherence;
end;

registration
  cluster non empty ordinal Subset of REAL;
  existence proof take NAT; thus thesis; end;
end;

theorem
   k < k + n iff 1 <= n
 proof
  thus k < k + n implies 1 <= n
   proof assume A1: k < k + n;
     assume not 1 <= n;
      then n = 0 by Th14;
    hence thesis by A1;
   end;
   assume 1 <= n;
   hence thesis by Th16;
 end;

theorem
   k < n implies n - 1 is Element of NAT
 proof
   assume k < n;
   then k + 1 <= n by Th13;
   then consider j being Nat such that A1: n = k + 1 + j by Th10;
    n - 1 = k + j by A1;
   hence thesis by ORDINAL1:def 13;
 end;

theorem
   k <= n implies n - k is Element of NAT
 proof
   assume
A1: k <= n;
   per cases by A1,REAL_1:def 5;
   suppose k < n;
   then k + 1 <= n by Th13;
   then consider j being Nat such that A2: n = k + 1 + j by Th10;
   reconsider j as Element of NAT by ORDINAL1:def 13;
    n - k = 1 + j by A2;
   hence thesis;
   end;
   suppose k = n;
   then n - k = 0;
   hence thesis;
   end;
 end;

