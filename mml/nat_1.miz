                :: Order relation ::
                ::::::::::::::::::::

:: Some theorems of not great relation "<=" in real numbers are translated
:: to natural number easy and it is necessary to have them here.

canceled 15;

theorem Th18:
  0 <= i
  proof
     defpred P[Nat] means 0 <= $1;
A1:  P[0];
A2:  now let n; assume P[n];
      then 0 + 1 <= n + 1 by XREAL_1:8;
      hence P[n + 1];
     end;
   for k holds P[k] from NatInd(A1,A2);
   hence thesis;
  end;

theorem
  0 <> i implies 0 < i by Th18;

theorem Th20:
  i <= j implies i * h <= j * h
  proof assume
A1:  i <= j;
   0 <= h by Th18;
   hence thesis by A1,XREAL_1:66;
  end;

theorem
  0 < i + 1
  proof assume
A1: 0 >= i + 1;
    0 <= i by Th18;
    then 0 + 1 <= 0 by A1,XREAL_1:9;
    hence contradiction;
  end;

theorem Th22:
  i = 0 or ex k st i = k + 1
  proof
    defpred P[Nat] means $1 = 0 or ex k st $1 = k + 1;
A1: P[0];
A2: P[h] implies P[h + 1];
    for i holds P[i] from NatInd(A1,A2);
    hence thesis;
  end;

theorem Th23:
  i + j = 0 implies i = 0 & j = 0
  proof assume
A1:  i + j = 0;
   0 <= i & 0 <= j by Th18;
   then 0 + i <= j + i & 0 + j <= i + j & 0 + i = i & 0 + j = j by XREAL_1:9;
   hence thesis by A1,Th18;
  end;

registration
  cluster non zero Nat;
  existence
  proof
    take 1;
    thus thesis;
  end;
end;

registration let m be Nat, n be non zero Nat;
  cluster m + n -> non zero;
  coherence by Th23;
  cluster n + m -> non zero;
  coherence;
end;

scheme DefbyInd { N()->Nat, F(Nat,Nat)->Nat, P[Nat,Nat] } :
  (for k being Element of NAT ex n being Element of NAT st P[k,n]) &
    for k,n,m being Element of NAT st P[k,n] & P[k,m] holds n = m
     provided
A1:    for k,n being Element of NAT holds P[k,n] iff
       k = 0 & n = N() or
       ex m,l being Element of NAT st k = m + 1 & P[m,l] & n = F(k,l)
  proof
    reconsider N = N() as Element of NAT by ORDINAL1:def 13;
      defpred P[Nat] means ex n being Element of NAT st P[$1,n];
      P[0,N] by A1;
then A2:   P[0];
A3: for k being Element of NAT holds P[k] implies P[k+1]
      proof let k be Element of NAT;
        given n being Element of NAT such that
A4:    P[k,n];
       reconsider F = F(k+1,n) as Element of NAT by ORDINAL1:def 13;
       take F;
       thus P[k+1,F] by A1,A4;
      end;
    thus for k being Element of NAT holds P[k] from Ind(A2,A3);
    defpred P[Nat] means
    for n,m being Element of NAT st P[$1,n] & P[$1,m] holds n = m;
A5:   P[0]
      proof let n,m be Element of NAT such that
A6:     P[0,n] & P[0,m];
        (not ex m,l being Element of NAT st 0 = m + 1 & P[m,l] & n = F(0,l) ) &
        not ex n,l being Element of NAT st 0 = n + 1 & P[n,l] & m = F(0,l);
        then n = N() & m = N() by A1,A6;
       hence n = m;
      end;
A7:   for k being Element of NAT st P[k] holds P[k+1]
      proof let k be Element of NAT such that
A8:      for n,m being Element of NAT st P[k,n] & P[k,m] holds n = m;
       let n,m be Element of NAT such that
A9:      P[k+1,n] & P[k+1,m];
       consider l,u being Element of NAT such that
A10:      k + 1 = l + 1 & P[l,u] & n = F(k + 1,u) by A1,A9;
        ex v,w being Element of NAT  st k + 1 = v + 1 & P[v,w] & m = F(k + 1,w)
         by A1,A9;
       hence n = m by A8,A10;
      end;
    thus for k being Element of NAT holds P[k] from Ind(A5,A7);
   end;

canceled 2;

theorem Th26:
  i <= j + 1 implies i <= j or i = j + 1
  proof
     defpred P[Nat] means
     for j holds $1 <= j + 1 implies $1 <= j or $1 = j+1;
A1:  P[0] by Th18;
A2:  for i st P[i] holds P[i+1]
     proof let i such that
A3:     for j holds i <= j + 1 implies i <= j or i = j + 1;
        let j;
        assume that
A4:     i + 1 <= j + 1;
A5:     now assume
A6:       j = 0;
then A7:       i <= 0 by A4,XREAL_1:8;
            0 <= i by Th18;
        hence thesis by A6,A7,XXREAL_0:1;
       end;
        now given k such that
A8:       j = k + 1;
         i <= k + 1 by A4,A8,XREAL_1:8;
         then i <= k or i = k + 1 by A3;
        hence thesis by A8,XREAL_1:8;
       end;
     hence thesis by A5,Th22;
    end;
    for i holds P[i] from NatInd(A1,A2);
    hence thesis;
  end;

theorem
  i <= j & j <= i + 1 implies i = j or j = i + 1
  proof assume
A1:  i <= j & j <= i + 1;
     then j <= i or j = i + 1 by Th26;
     hence thesis by A1,XXREAL_0:1;
  end;

theorem Th28:
  i <= j implies ex k st j = i + k
  proof
     defpred P[Nat] means
     i <= $1 implies ex k st $1 = i + k;
A1: P[0]
     proof assume
A2:   i <= 0;
      take 0;
      thus thesis by A2,Th18;
     end;
A3:  for j st P[j] holds P[j+1]
     proof let j such that
A4:   i <= j implies ex k st j = i + k;
      assume
A5:     i <= j + 1;
A6:     now assume i <= j;
        then consider k such that
A7:     j = i + k by A4;
        i + k + 1 = i + (k + 1);
        hence thesis by A7;
       end;
      now assume i = j + 1; then j + 1 = i + 0;
        hence thesis;
       end;
      hence thesis by A5,A6,Th26;
     end;
   for j holds P[j] from NatInd(A1,A3);
   hence thesis;
  end;

theorem Th29:
  i <= i + j
  proof
A1:  0 + i = i;
   0 <= j by Th18;
   hence thesis by A1,XREAL_1:9;
  end;

scheme CompInd { P[Nat] } :
  for k being Element of NAT holds P[k]
   provided
A1:   for k being Element of NAT st
      for n being Element of NAT st n < k holds P[n] holds P[k]
  proof
      defpred P1[Nat] means
      for n being Element of NAT st n < $1 holds P[n];
A2:   P1[0] by Th18;
A3:   for k be Element of NAT holds P1[k] implies P1[k+1]
      proof
        let k be Element of NAT;
        assume
A4:   for n being Element of NAT st n < k holds P[n];
      let n be Element of NAT; assume n < k + 1;
      then n <= k by Th26;
      then n < k or n = k & n <= k by REAL_1:def 5;
      hence thesis by A1,A4;
    end;
    let k be Element of NAT;
    for k being Element of NAT holds P1[k] from Ind(A2,A3);
    then for n being Element of NAT st n < k holds P[n];
    hence thesis by A1;
   end;

:: Principle of minimum

scheme Min { P[Nat] } :
 ex k being Element of NAT st P[k] &
 for n being Element of NAT st P[n] holds k <= n
  provided
A1:  ex k being Element of NAT st P[k]
proof assume A2: not thesis;
      defpred P1[Nat] means not P[$1];
A3:   for k being Element of NAT st
      for n being Element of NAT st n < k holds P1[n] holds P1[k]
      proof let k be Element of NAT;
A4:    not (ex n being Element of NAT st P[n] & not k <= n) implies not P[k]
       by A2;
       assume for n being Element of NAT st n < k holds not P[n];
       hence thesis by A4;
      end;
      for k be Element of NAT holds P1[k] from CompInd(A3);
    hence contradiction by A1;
   end;

:: Principle of maximum

scheme Max { P[Nat], N()->Nat } :
  ex k being Element of NAT st P[k] &
  for n being Element of NAT st P[n] holds n <= k
   provided
A1:   for k being Element of NAT st P[k] holds k <= N() and
A2:   ex k being Element of NAT st P[k]
  proof
      defpred P1[Nat] means
      for n being Element of NAT st P[n] holds n <= $1;
      N() in NAT by ORDINAL1:def 13;
      then
A3:   ex k being Element of NAT st P1[k] by A1;
    consider k being Element of NAT such that
A4:   P1[k] &
      for m being Element of NAT st P1[m] holds k <= m from Min(A3);
    take k;
    thus P[k]
      proof assume
A5:      not P[k];
       consider n being Element of NAT such that
A6:      P[n] by A2;
        n <= k & n <> k by A4,A5,A6;
       then k <> 0 by Th18;
       then consider m such that
A7:      k = m + 1 by Th22;
         reconsider m as Element of NAT by ORDINAL1:def 13;
        now let n be Element of NAT; assume P[n];
         then n <= k & n <> k by A4,A5;
         hence n <= m by A7,Th26;
        end;
        then A8:    k <= m by A4;
       (- m) + m + 1 = (- m) + (m + 1) & (- m) + m + 0 = (- m) + (m + 0);
       hence contradiction by A7,A8,XREAL_1:8;
      end;
    thus thesis by A4;
   end;

canceled 7;

theorem Th37:
  i <= j implies i <= j + h
  proof assume
A1:  i <= j;
       0 <= h by Th18;
    then i + 0 <= i + h & i + h <= j + h & i + 0 = i by A1,XREAL_1:9;
   hence i <= j + h by XXREAL_0:2;
  end;

theorem Th38:
  i < j + 1 iff i <= j
  proof
   thus i < j + 1 implies i <= j by Th26;
   assume
A1:  i <= j;
   hence i <= j + 1 by Th37;
   assume A2: i = j + 1;
A3:  j + (- j) = 0;
   j + 1 + (- j) = 1;
   hence contradiction by A1,A2,A3,XREAL_1:8;
  end;

theorem Th39:
  j < 1 implies j = 0
 proof
  assume j < 1;
    then j < 0 + 1;
    then A1: j <= 0 by Th38;
   assume j <> 0;
  hence thesis by A1,Th18;
 end;

theorem Th40:
  i * j = 1 implies i = 1 & j = 1
  proof assume
A1:  i * j = 1;
    then i <> 0;
   then consider m such that
A2:  i = m + 1 by Th22;
    j <> 0 by A1;
   then consider l such that
A3:  j = l + 1 by Th22;
    m * l + m + l + 1 = 0 + 1 by A1,A2,A3;
    then m * l + m = 0 & l = 0 by Th23;
   hence thesis by A2,A3;
  end;

theorem Th41:
  k <> 0 implies n < n + k
 proof
  assume A1: k <> 0;
   A2: n <> n + k by A1;
    n <= n + k & n <= k + n by Th37;
  hence n < n + k by A2,REAL_1:def 5;
 end;

scheme Regr { P[Nat] } :
  P[0]
  provided
A1:  ex k being Element of NAT st P[k] and
A2:  for k being Element of NAT st k <> 0 & P[k]
     ex n being Element of NAT st n < k & P[n]
proof
A3: ex k being Element of NAT st P[k] by A1;
   consider k being Element of NAT such that
A4:  P[k] & for n being Element of NAT st P[n] holds k <= n from Min(A3);
       now assume k <> 0;
      then ex n being Element of NAT st n < k & P[n] by A2,A4;
     hence contradiction by A4;
    end;
   hence P[0] by A4;
  end;

:: Exact division and rest of division

theorem Th42:
  0 < m implies for n ex k,t st n = (m*k)+t & t < m
   proof
    assume
A1:   0 < m;
     defpred P[Nat] means ex k,t st $1 = (m*k)+t & t < m;
        0 = m*0+0;
then A2:   P[0] by A1;
A3:   for n st P[n] holds P[n+1]
      proof let n;
       given k1,t1 such that
A4:     n = (m*k1)+t1 & t1 < m;
A5:        t1+1 <= m by A4,Th38;
A6:     t1+1 < m implies ex k,t st n+1 = (m*k)+t & t < m
         proof assume
A7:        t1+1 < m;
          take k = k1 , t = t1+1;
          thus n+1 = m*k+t by A4;
          thus t < m by A7;
         end;
           t1+1 = m implies ex k,t st n+1 = (m*k)+t & t < m
         proof assume
A8:        t1+1 = m;
          take k = k1+1 , t = 0;
          thus n+1 = m*k+t by A4,A8;
          thus t < m by A1;
         end;
       hence ex k,t st n+1 = (m*k)+t & t < m by A5,A6,REAL_1:def 5;
      end;
    thus for n holds P[n] from NatInd(A2,A3);
   end;

theorem Th43:
  n = m*k+t & t < m & n = m*k1+t1 & t1 < m implies k = k1 & t = t1
   proof
    assume
A1:   n = m*k+t & t < m & n = m*k1+t1 & t1 < m;
A2:   now assume k <= k1;
      then consider u being Nat such that
A3:     k1 = k + u by Th28;
          m * (k + u) + t1 = m * k + (m * u + t1);
then A4:     m * u + t1 = t by A1,A3,XCMPLX_1:2;
          now given w being Nat such that
A5:       u = w + 1;
          m * u + t1 = m + (m * w + t1) by A5;
        hence contradiction by A1,A4,Th29;
       end;
then A6:     u = 0 by Th22;
      hence k = k1 by A3;
      thus t = t1 by A1,A3,A6,XCMPLX_1:2;
     end;
        now assume k1 <= k;
      then consider u being Nat such that
A7:     k = k1 + u by Th28;
          m * (k1 + u) + t = m * k1 + (m * u + t);
then A8:     m * u + t = t1 by A1,A7,XCMPLX_1:2;
          now given w being Nat such that
A9:       u = w + 1;
          m * u + t = m + (m * w + t) by A9;
        hence contradiction by A1,A8,Th29;
       end;
then A10:     u = 0 by Th22;
      hence k = k1 by A7;
       thus t = t1 by A1,A7,A10,XCMPLX_1:2;
     end;
    hence thesis by A2;
   end;

definition let k,l be Nat;
  func k div l -> Nat means       :: the exact division
:Def1:  ( ex t being Nat st k = l * it + t & t < l ) or it = 0 & l = 0;
   existence
    proof
      per cases;
      suppose l <> 0;
        then 0 < l by Th18;
       hence thesis by Th42;
     end;
      suppose l = 0;
        hence thesis;
      end;
    end;
   uniqueness
    proof let t1,t2 be Nat such that
A1:   (ex t being Nat st k = l * t1 + t & t < l) or t1 = 0 & l = 0 and
A2:   (ex t being Nat st k = l * t2 + t & t < l) or t2 = 0 & l = 0;
      now let t; assume
A3:     t < 0;
        then ex m st 0 = t + m by Th28;
       hence contradiction by A3,Th23;
      end;
     hence thesis by A1,A2,Th43;
    end;

  func k mod l -> Nat means         :: the rest of division
:Def2: ( ex t being Nat st k = l * t + it & it < l ) or it = 0 & l = 0;
   existence
    proof
       now assume
A4:    l <> 0;
       0 <= l by Th18;
       then ex n,t st k = l * n + t & t < l by A4,Th42;
       hence thesis;
      end;
     hence thesis;
    end;
   uniqueness
    proof let t1,t2 be Nat such that
A5:  (ex t being Nat st k = l * t + t1 & t1 < l) or t1 = 0 & l = 0 and
A6:  (ex t being Nat st k = l * t + t2 & t2 < l) or t2 = 0 & l = 0;
      now let t; assume
A7:     t < 0;
        then ex m st 0 = t + m by Th28;
       hence contradiction by A7,Th23;
      end;
     hence thesis by A5,A6,Th43;
    end;
 end;

definition let k,l be Nat;
  redefine func k div l -> Element of NAT;
  coherence by ORDINAL1:def 13;
  redefine func k mod l -> Element of NAT;
  coherence by ORDINAL1:def 13;
end;

canceled 2;

theorem Th46:
  0 < i implies j mod i < i
   proof assume 0 < i;
     then ex t being Nat st j = i * t + (j mod i) & j mod i < i by Def2;
    hence j mod i < i;
   end;

theorem Th47:
  0 < i implies j = i * (j div i) + (j mod i)
   proof assume 0 < i;
     then ex k being Nat st j = i * k + (j mod i) & j mod i < i by Def2;
    hence j = i * (j div i) + (j mod i) by Def1;
   end;

:: The divisibility relation

definition let k,l be Nat;
  pred k divides l means
:Def3:  ex t being Nat st l = k * t;
  reflexivity
   proof let i;
    i = i * 1;
    hence thesis;
   end;
end;

canceled;

theorem Th49:
  j divides i iff i = j * (i div j)
   proof
     thus j divides i implies i = j * (i div j)
     proof assume j divides i;
       then consider k being Nat such that
A1:    i = j * k by Def3;
A2:    i = j * k + 0 by A1;
       now assume
A3:      j <> 0;
A4:      0 <= j by Th18;
then A5: i = j * (i div j) + (i mod j) by A3,Th47;
         i mod j < j by A3,A4,Th46;
         hence i = j * (i div j) by A2,A3,A4,A5,Th43;
       end;
       hence i = j * (i div j) by A1;
     end;
     assume i = j * (i div j);
     hence j divides i by Def3;
   end;

