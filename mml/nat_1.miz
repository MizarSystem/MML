:: from ALGSEQ_1

theorem Th70:
  m<n+1 implies m<n or m=n
 proof
  assume m<n+1;
  then m<=n by Th38;
  hence thesis by REAL_1:def 5;
 end;

theorem
  k<2 implies k=0 or k=1
 proof
  assume k<2;
  then k<1+1;
  then k<1 or k=1 by Th70;
  hence thesis by Th39;
 end;

registration
  cluster non zero Nat;
  existence
  proof
    take 1;
    thus thesis;
  end;
end;

registration
  cluster -> non negative Element of NAT;
  coherence by Th18;
end;

registration
  cluster -> non negative (natural number);
  coherence by Th18;
end;

:: from AMI_5, 2005.11.16, A.T.

theorem
   for m,k being Nat st k <> 0 holds m * k div k = m
  proof
   let m,k be Nat;
   assume k <> 0;
   then m*k = k * m + 0 & 0 < k or m = 0 & k = 0;
   hence m*k div k = m by Def1;
  end;

:: from GR_CY_1, 2005.11.16, A.T.

theorem Th1:
 for m,n,k being natural number holds m mod n = (n*k + m) mod n
 proof
   let m,n,k be natural number;
  per cases;
  suppose A1: n > 0;
       m mod n = t implies (n*k + m) mod n = t
   proof
    assume m mod n = t;
    then consider q such that A2: m = n * q + t & t < n by A1,Def2;
        ex g st n*k + m = n*g + t & t < n
      proof
        reconsider g = k+q as Nat by ORDINAL2:def 21;
        take g;
       thus thesis by A2;
      end;
    hence thesis by Def2;
   end;
 hence thesis;
 end;
 suppose n = 0;
 hence thesis;
end;
end;

theorem Th2:
 for p,s,n being natural number holds (p+s) mod n = ((p mod n)+s) mod n
 proof
   let p,s,n be natural number;
  per cases;
  suppose n > 0;
  then (p + s) mod n = (n*(p div n) + (p mod n) + s) mod n by Th47
                .=(n*(p div n) + ((p mod n) + s)) mod n
                .=((p mod n) + s) mod n by Th1;
   hence thesis;
   end;
   suppose
A1:  n = 0;
   hence (p+s) mod n = 0 by Def2
     .= ((p mod n)+s) mod n by A1,Def2;
 end;
 end;

theorem
   for p,s,n being natural number holds  (p+s) mod n = (p + ( s mod n)) mod n
   by Th2;

theorem Th4:
 for n,k being natural number holds k < n implies k mod n = k
 proof
   let n,k be natural number;
   assume A1:k<n;
A2: k is Nat by ORDINAL2:def 21;
    ex p st k=n*p + k & k<n
   proof
      k=n*0 + k;
     hence thesis by A1;
   end;
  hence thesis by A2,Def2;
 end;

theorem Th5:
 for n being natural number holds n mod n = 0
 proof
   let n be natural number;
  per cases;
  suppose A1:n>0;
      ex p st n=n*p +0 & 0<n
   proof
        n=n*1+0;
    hence thesis by A1;
   end;
  hence thesis by Def2;
  end;
  suppose n = 0;
  hence thesis by Def2;
 end;
 end;

theorem Th6:
 for n being natural number holds 0 = 0 mod n
  proof
    let n be natural number;
        n = 0 or n > 0;
    hence thesis by Th4,Def2;
  end;

