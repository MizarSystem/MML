theorem Th22:
  m < n+1 implies m < n or m = n
 proof
  assume m<n+1;
  then m<=n by Th13;
  hence thesis by REAL_1:def 5;
 end;

theorem
   k < 2 implies k = 0 or k = 1
 proof
  assume k<2;
  then k<1+1;
  hence thesis by Th14,Th22;
 end;

registration
  cluster non zero Element of NAT;
  existence
  proof
    take 1;
    thus thesis;
  end;
