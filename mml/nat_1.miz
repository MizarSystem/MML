theorem Th82:
  n <= 1 implies n = 0 or n = 1
 proof
  assume A1: n <= 1;
  assume A2: not (n = 0 or n = 1);
  then n < 0+1 by A1,REAL_1:def 5;
  then n <= 0 by Th38;
  hence contradiction by A2;
 end;

theorem Th83:
  n <= 2 implies n = 0 or n = 1 or n = 2
 proof
  assume A1: n <= 2;
  assume A2: not (n = 0 or n = 1 or n = 2);
  then n < 1+1 by A1,REAL_1:def 5;
  then n <= 1 by Th38;
  hence thesis by A2,Th82;
 end;

