:: from BINOM, 2006.05.28, A.T.

scheme Ind1{M() -> Nat, P[Nat]} :
  for i being Nat st M() <= i holds P[i]
provided
  A1:P[M()] and
  A2:for j being Nat st M() <= j holds P[j] implies P[j+1]
proof
let i;
assume A3: M() <= i;
defpred Q[Nat] means P[M() + $1];
A4: Q[0] by A1;
    reconsider M = M() as Element of NAT by ORDINAL1:def 13;
A5: now let m be Element of NAT;
    assume  Q[m];
     then P[M+m+1] by A2,Th11;
    hence Q[m+1];
    end;
A7: for m being Element of NAT holds Q[m] from Ind(A4,A5);
    consider m being Nat such that
A8: i = M() + m by A3,Th10;
     m in NAT by ORDINAL1:def 13;
   hence thesis by A7,A8;
end;

:: from INT_2, 2006.05.30, AG

scheme CompInd1 { a() -> Nat, P[Nat] } :
  for k being Nat st k >= a() holds P[k]
  provided
  A1: for k being Nat st k>=a() &
  (for n being Nat st n>=a() & n<k holds P[n]) holds P[k]
proof
     defpred P1[Nat] means
      for n being Nat st (n>=a() & n<$1) holds P[n];
A2: P1[a()];
A3: for k being Nat st k>=a() holds P1[k] implies P1[k+1]
     proof
      let k be Nat;
      assume k>=a();
      assume A4: for n being Nat st n>=a() & n<k holds P[n];
      let n be Nat;
      assume A5: n>=a() & n<k+1;
      then n<=k by Th13;
      then n<k or n = k by REAL_1:def 5;
      hence thesis by A1,A4,A5;
     end;
    let k be Nat;
A6: for k being Nat st k>=a() holds P1[k] from Ind1(A2,A3);
    assume A7: k>=a();
    then for n being Nat st (n>=a() & n<k) holds P[n] by A6;
    hence P[k] by A1,A7;
   end;

