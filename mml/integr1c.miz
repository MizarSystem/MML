      proof
        let x0 be set such that
A14:    x0 in dom u0;
A15:    x0 in dom <:x,y:> &
        <:x,y:>.x0 in dom ((Re(f+g))*(R2-to-C)) by A14,A8,FUNCT_1:21;
        set R2 = <:x,y:>.x0;
A16:    R2 in dom (R2-to-C) &
        (R2-to-C).R2 in dom (Re(f+g)) by A15,FUNCT_1:21;then
        (R2-to-C).R2 in dom (Re f + Re g) by MESFUN6C:5;then
        (R2-to-C).R2 in (dom Re f /\ dom Re g) by VALUED_1:def 1;
