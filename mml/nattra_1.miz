A38:  F2 = F29 by XTUPLE_0:1;
A39:  t1 = t19 by A35,XTUPLE_0:1;
A40:  [F,F1] = [F9,F19] by A34,XTUPLE_0:1;
      then F = F9 by XTUPLE_0:1;
      hence m.[[[F1,F2],t1],[[F,F1],t]] = [[F,F2],t1`*`t] by A36,A40,A37,A39
,A38,XTUPLE_0:1;
