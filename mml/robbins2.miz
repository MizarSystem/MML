Lm1: for L being satisfying_DN_1 (non empty ComplLLattStr) holds L is
join-commutative
proof
  let L;
  for x, y holds x + y = y + x by Th27;
  hence thesis by LATTICES:def 4;
end;

registration
  cluster satisfying_DN_1 -> join-commutative (non empty ComplLLattStr);
  coherence by Lm1;
end;

