  let L be satisfying_DN_1 (non empty ComplLLattStr);
  let x, y, z be Element of L;
  set X = ((x + y)` + x)`, Y = (x + y)`;
  (X + Y)` = x by Th7;
  hence thesis by Th15;
end;

