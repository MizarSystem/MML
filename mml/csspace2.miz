       end;
A57:   Partial_Sums(|.seq_t-seq_id(vseq.n).|
          (#)|.seq_t-seq_id(vseq.n).| ) is bounded_above
       proof
         now let i be Nat;
           0 <= i by NAT_1:18; then
           Partial_Sums(|.seq_t-seq_id(vseq.n).|
              (#)|.seq_t-seq_id(vseq.n).| ).i <=e*e by A49;
