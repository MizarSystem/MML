     end;
     for n be Nat st n >= k holds
        ( |.seq_t-seq_id(vseq.n).|(#)|.seq_t-seq_id(vseq.n).| is summable &
          |.Sum((seq_t-seq_id(vseq.n))(#)(seq_t-seq_id(vseq.n))*').| < e1*e1 )
     proof
       let n be Nat such that
A48:   n >= k;
A49:   for i be Nat st 0 <= i holds
       Partial_Sums(|.seq_t-seq_id(vseq.n).|
                       (#)|.seq_t-seq_id(vseq.n).| ).i <=e*e
       proof
         let i be Nat such that 0 <=i;
         deffunc F(Nat) = Partial_Sums(|.(seq_id(vseq.$1 - vseq.n)).|
