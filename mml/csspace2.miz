         .=|.((seq_t).i).| * |.((seq_t).i).| by COMSEQ_1:def 14;
A68:   (2(#)a + 2(#)b).i = (2(#)a).i + (2(#)b).i by SEQ_1:11
                      .=2*(a.i)+ (2(#)b).i by SEQ_1:13
                      .=2*(a.i)+ 2*(b.i) by SEQ_1:13;
