A59:   Sum(|.seq_t-seq_id(vseq.n).|(#)|.seq_t-seq_id(vseq.n).|)
         = lim Partial_Sums( |.seq_t-seq_id(vseq.n).|
                       (#)|.seq_t-seq_id(vseq.n).| ) by SERIES_1:def 3;
       lim Partial_Sums(|.seq_t-seq_id(vseq.n).|(#)|.seq_t-seq_id(vseq.n).|)
         <=e*e by A49,A58,RSSPACE2:6; then
P2:    Sum(|.seq_t-seq_id(vseq.n).|(#)|.seq_t-seq_id(vseq.n).|) < e1*e1
         by A26,A59,AXIOMS:22;
       |.seq_t-seq_id(vseq.n).| = |.(seq_t-seq_id(vseq.n))*'.| by LM; then
P1:    Sum(|.(seq_t-seq_id(vseq.n))(#)(seq_t-seq_id(vseq.n))*'.|) < e1*e1
         by P2,COMSEQ_1:49;

       |.seq_t-seq_id(vseq.n).|(#)|.(seq_t-seq_id(vseq.n))*'.| is summable
