   end;
   hence thesis by CLVECT_2:9;
end; then
Lm6: Complex_l2_Space is complete by CLVECT_2:def 12;

registration
  cluster Complex_l2_Space -> Hilbert; :: complete;
  coherence by Lm6,CLVECT_2:def 13;
end;

begin  :: Some corollaries of complex sequences

theorem
for z1, z2 be Complex st
 (Re z1)*(Im z2) = (Re z2)*(Im z1) & (Re z1)*(Re z2)+(Im z1)*(Im z2) >= 0 holds
  |.z1 + z2.| = |.z1.| + |.z2.| by Cor2a;

theorem
for x,y be Complex holds 2*|.x*y.| <= |.x.|^2 + |.y.|^2 by Lm2a;

theorem
for x,y be Complex holds
  |.x+y.|*|.x+y.| <= 2*|.x.|*|.x.| + 2*|.y.|*|.y.| &
  |.x.|*|.x.| <= 2*|.x-y.|*|.x-y.| + 2*|.y.|*|.y.| by Lm2;

theorem
for seq be Complex_Sequence holds seq = (seq*')*' by Cor1;

theorem
for seq be Complex_Sequence holds Partial_Sums (seq*') = (Partial_Sums seq)*'
 by Cor2;

theorem
for seq be Complex_Sequence, n be Nat st
 (for i be Nat holds (Re seq).i >= 0 & (Im seq).i = 0) holds
   (|.Partial_Sums seq.|).n = (Partial_Sums |.seq.|).n by Cor2b;

theorem
for seq be Complex_Sequence st seq is summable holds
 Sum (seq*') = (Sum seq)*' by Cor3;

theorem
for seq be Complex_Sequence st seq is absolutely_summable holds
  |. Sum seq .| <= Sum |.seq.| by Cor4;

theorem
for seq be Complex_Sequence st seq is summable &
 (for n be Nat holds (Re seq).n >= 0 & (Im seq).n = 0) holds
  |. Sum seq .| = Sum |.seq.| by Cor5;

theorem
for seq be Complex_Sequence, n be Nat holds
 (Re (seq(#)seq*')).n >= 0 & (Im (seq(#)seq*')).n = 0 by Cor6;

theorem
for seq be Complex_Sequence st
 ( seq is absolutely_summable & Sum |.seq.|=0 ) holds
    for n be Nat holds seq.n =0c
proof
   let seq be Complex_Sequence such that
A2:seq is absolutely_summable and
A3:Sum |.seq.|=0;

A5:Partial_Sums |.seq.| is bounded_above by A2,COMSEQ_3:62;
A6:for n be Nat holds (Partial_Sums |.seq.|).n <= Sum |.seq.|
   proof
     let n be Nat;
     (Partial_Sums(|.seq.|)).n <= lim Partial_Sums |.seq.| by A5,SEQ_4:54;
     hence (Partial_Sums(|.seq.|)).n <=Sum |.seq.| by SERIES_1:def 3;
   end;
B1:now assume
     ex n be Nat st (|.seq.|).n <> 0; then
     consider n1 be Nat such that
A7:  (|.seq.|).n1 <> 0;
A8:  for n be Nat holds 0 <= Partial_Sums(|.seq.|).n
     proof
       let n be Nat;
A9:    n=n+0;
A10:   Partial_Sums(|.seq.|).0 = (|.seq.|).0 by SERIES_1:def 1;
       (|.seq.|).0 = |.(seq.0).| by COMSEQ_1:def 14; then
       0 <=(|.seq.|).0 by COMPLEX1:132;
       hence 0 <=Partial_Sums(|.seq.|).n by A9,A10,SEQM_3:11;
     end;
     Partial_Sums(|.seq.|).n1 >0
     proof
       now per cases;
         case
         n1=0; then
         Partial_Sums(|.seq.|).(n1) <> 0 by A7,SERIES_1:def 1;
         hence Partial_Sums(|.seq.|).(n1) > 0 by A8;
         end;
         case
A12:     n1<>0;
         0 <= n1 by NAT_1:18; then
A13:     0 + 1 <= n1 by A12,INT_1:20;
         set nn=n1-1;
A14:     nn is Nat by A13,INT_1:18;
A15:     nn+1 =n1;
         (|.seq.|).n1 = |.(seq.n1).| by COMSEQ_1:def 14; then
A16:     0 <= (|.seq.|).n1 by COMPLEX1:132;
         0 <= Partial_Sums(|.seq.|).nn by A8,A14; then
         (|.seq.|).(n1)+0 <= (|.seq.|).(n1)+Partial_Sums(|.seq.|).nn
           by REAL_1:55;
         hence Partial_Sums(|.seq.|).n1 > 0 by A7,A14,A15,A16,SERIES_1:def 1;
         end;
       end;
       hence Partial_Sums(|.seq.|).n1 > 0;
     end;
     hence contradiction by A3,A6;
   end;
   for n be Nat holds seq.n =0c
   proof
     let n be Nat;
     0 = (|.seq.|).n by B1
      .= |.(seq.n).| by COMSEQ_1:def 14;
     hence thesis by COMPLEX1:131;
   end;
   hence thesis;
end;

theorem
for seq being Complex_Sequence holds
 |.seq.| = |.seq*'.| by LM;

theorem
for c be Complex, seq be Complex_Sequence st seq is convergent
for rseq be Real_Sequence st
  (for m be Nat holds rseq .m = |.seq.m-c.|*|.seq.m-c.|)
holds (rseq is convergent & lim rseq = |.lim seq-c.|*|.lim seq-c.| )
 by Lm4;

theorem
for c be Complex, seq1 be Real_Sequence, seq be Complex_Sequence st
 seq is convergent & seq1 is convergent
  for rseq be Real_Sequence st
  (for i be Nat holds rseq.i = |.seq.i-c.|*|.seq.i-c.|+seq1.i) holds
   (rseq is convergent & lim rseq = |.lim seq-c.|*|.lim seq-c.|+lim seq1 )
    by Lm5;

