             .= ( |.seq_t-seq_id(vseq.n).|(#)|.seq_t-seq_id(vseq.n).| ).(i+1)
                + Partial_Sums(|.seq_t-seq_id(vseq.n).|
                      (#)|.seq_t-seq_id(vseq.n).|).i by CSSPACE:3
