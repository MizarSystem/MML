       end;
A43:   for i be Nat st P[i] holds P[i+1]
       proof
          now
           let i be Nat such that
A44:       for rseq be Real_Sequence st
            ( for m be Nat holds
               rseq.m=Partial_Sums(|.seq_id(vseq.m-vseq.n).|
                       (#)|.seq_id(vseq.m-vseq.n).|).i ) holds
