     end;

     hence d is summable by A61,A66,SERIES_1:24;
   end;
   tseq in the_set_of_ComplexSequences by CSSPACE:def 1; then
   reconsider tv=tseq as Point of Complex_l2_Space
     by A60,CSSPACE:def 11,def 20;

   for e be Real st
    e > 0 ex m be Nat st
     for n be Nat st n >= m holds ||.(vseq.n) - tv.|| < e
   proof
   let e be Real such that
A71: e > 0;

     consider m be Nat such that
A72: for n be Nat st n >= m holds
      ( |.seq_t-seq_id(vseq.n).|(#)|.seq_t-seq_id(vseq.n).| is summable &
        |.Sum((seq_t-(seq_id(vseq.n)))(#)(seq_t-(seq_id(vseq.n)))*').| < e*e )
