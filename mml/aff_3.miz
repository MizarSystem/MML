      end;
      assume
A75:  b9,x // C;
      C // P by A14,A15,AFF_1:58;
      then b9,x // P by A75,AFF_1:57;
      then x in P by A3,A11,AFF_1:37;
      then x=b or D=P by A3,A10,A29,A40,A72,AFF_1:30;
      then b=p or M=A9 by A7,A12,A24,A39,A42,A50,A41,A35,A58,A71,AFF_1:30
      ,59;
      then b in K by A46,A50,A49,A38,A74,AFF_1:59;
      hence contradiction by A16,A32,A46,A51,AFF_1:33;
    end;
    then consider y such that
A76: y in C and
A77: LIN b9,x,y by A4,AFF_1:73;
A78: LIN b9,y,x by A77,AFF_1:15;
A79: not LIN b9,a9,y
    proof
A80:  now
        assume x=p;
        then p=b or D=A9 by A29,A40,A42,A50,A41,A72,AFF_1:30;
        then LIN b,b9,a9 or c in A9 by A26,A48,A47,A25,AFF_1:26,33;
