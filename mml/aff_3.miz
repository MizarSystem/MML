environ

 vocabularies DIRAF, SUBSET_1, AFF_1, ANALOAF, INCSP_1, AFF_2, AFF_3;
 notations STRUCT_0, ANALOAF, DIRAF, AFF_1, AFF_2;
 constructors AFF_1, AFF_2;
 definitions AFF_2;
 theorems AFF_1, AFF_2;

begin

reserve AP for AffinPlane;
reserve a,a',b,b',c,c',d,x,y,o,p,q for Element of AP;
reserve A,C,D',M,N,P for Subset of AP;

definition
  let AP;
  attr AP is satisfying_DES1 means
  :Def1:
  for A,P,C,o,a,a',b,b',c,c',p,q st A
is being_line & P is being_line & C is being_line & P<>A & P<>C & A<>C & o in A
& a in A & a' in A & o in P & b in P & b' in P & o in C & c in C & c' in C & o
<>a & o<>b & o<>c & p<>q & not LIN b,a,c & not LIN b',a',c' & a<>a' & LIN b,a,p
  & LIN b',a',p & LIN b,c,q & LIN b',c',q & a,c // a',c' holds a,c // p,q;
end;

definition
  let AP;
  attr AP is satisfying_DES1_1 means
  :Def2:
  for A,P,C,o,a,a',b,b',c,c',p,q st
A is being_line & P is being_line & C is being_line & P<>A & P<>C & A<>C & o in
A & a in A & a' in A & o in P & b in P & b' in P & o in C & c in C & c' in C &
o<>a & o<>b & o<>c & p<>q & c <>q & not LIN b,a,c & not LIN b',a',c' & LIN b,a,
  p & LIN b',a',p & LIN b,c,q & LIN b',c',q & a,c // p,q holds a,c // a',c';
end;

definition
  let AP;
  attr AP is satisfying_DES1_2 means
  :Def3:
  for A,P,C,o,a,a',b,b',c,c',p,q st
A is being_line & P is being_line & C is being_line & P<>A & P<>C & A<>C & o in
A & a in A & a' in A & o in P & b in P & b' in P & c in C & c' in C & o<>a & o
<>b & o<>c & p<>q & not LIN b,a,c & not LIN b',a',c' & c <>c' & LIN b,a,p & LIN
  b',a',p & LIN b,c,q & LIN b',c',q & a,c // a',c' & a,c // p,q holds o in C;
end;

definition
  let AP;
  attr AP is satisfying_DES1_3 means
  :Def4:
  for A,P,C,o,a,a',b,b',c,c',p,q st
A is being_line & P is being_line & C is being_line & P<>A & P<>C & A<>C & o in
A & a in A & a' in A & b in P & b' in P & o in C & c in C & c' in C & o<>a & o
<>b & o<>c & p<>q & not LIN b,a,c & not LIN b',a',c' & b<>b' & a<>a' & LIN b,a,
  p & LIN b',a',p & LIN b,c,q & LIN b',c',q & a,c // a',c' & a,c // p,q holds o
  in P;
end;

definition
  let AP;
  attr AP is satisfying_DES2 means
  :Def5:
  for A,P,C,a,a',b,b',c,c',p,q st A is
being_line & P is being_line & C is being_line & A<>P & A<>C & P<>C & a in A &
a' in A & b in P & b' in P & c in C & c' in C & A // P & A // C & not LIN b,a,c
  & not LIN b',a',c' & p<>q & a<>a' & LIN b,a,p & LIN b',a',p & LIN b,c,q & LIN
  b',c',q & a,c // a',c' holds a,c // p,q;
end;

definition
  let AP;
  attr AP is satisfying_DES2_1 means
  :Def6:
  for A,P,C,a,a',b,b',c,c',p,q st A
is being_line & P is being_line & C is being_line & A<>P & A<>C & P<>C & a in A
& a' in A & b in P & b' in P & c in C & c' in C & A // P & A // C & not LIN b,a
,c & not LIN b',a',c' & p<>q & LIN b,a,p & LIN b',a',p & LIN b,c,q & LIN b',c',
  q & a,c // p,q holds a,c // a',c';
end;

definition
  let AP;
  attr AP is satisfying_DES2_2 means
  :Def7:
  for A,P,C,a,a',b,b',c,c',p,q st A
is being_line & P is being_line & C is being_line & A<>P & A<>C & P<>C & a in A
& a' in A & b in P & b' in P & c in C & c' in C & A // C & not LIN b,a,c & not
LIN b',a',c' & p<>q & a<>a' & LIN b,a,p & LIN b',a',p & LIN b,c,q & LIN b',c',q
  & a,c // a',c' & a,c // p,q holds A // P;
end;

definition
  let AP;
  attr AP is satisfying_DES2_3 means
  :Def8:
  for A,P,C,a,a',b,b',c,c',p,q st A
is being_line & P is being_line & C is being_line & A<>P & A<>C & P<>C & a in A
& a' in A & b in P & b' in P & c in C & c' in C & A // P & not LIN b,a,c & not
LIN b',a',c' & p<>q & c <>c' & LIN b,a,p & LIN b',a',p & LIN b,c,q & LIN b',c',
  q & a,c // a',c' & a,c // p,q holds A // C;
end;

canceled 8;

theorem
  AP is satisfying_DES1 implies AP is satisfying_DES1_1
proof
  assume
A1: AP is satisfying_DES1;
  let A,P,C,o,a,a',b,b',c,c',p,q;
  assume that
A2: A is being_line and
A3: P is being_line and
A4: C is being_line and
A5: P<>A and
A6: P<>C and
A7: A<>C and
A8: o in A and
A9: a in A and
A10: a' in A and
A11: o in P and
A12: b in P & b' in P and
A13: o in C & c in C and
A14: c' in C and
A15: o<>a and
A16: o<>b and
A17: o<>c and
A18: p<>q and
A19: c <>q and
A20: not LIN b,a,c and
A21: not LIN b',a',c' and
A22: LIN b,a,p and
A23: LIN b',a',p and
A24: LIN b,c,q and
A25: LIN b',c',q and
A26: a,c // p,q;
A27: LIN o,a,a' & LIN b',p,a' by A2,A8,A9,A10,A23,AFF_1:15,33;
  set K=Line(b,a);
A28: a in K by AFF_1:26;
  then
A29: K<>P by A2,A3,A5,A8,A9,A11,A15,AFF_1:30;
A30: not LIN o,a,c
  proof
    assume
    LIN o,a,c;
    then c in A by A2,A8,A9,A15,AFF_1:39;
    hence contradiction by A2,A4,A7,A8,A13,A17,AFF_1:30;
  end;
A31: p in K by A22,AFF_1:def 2;
A32: LIN o,c,c' & LIN b',q,c' by A4,A13,A14,A25,AFF_1:15,33;
A33: b<>c & a <> p by A19,A20,A24,A26,AFF_1:16,23;
A34: a'<>c' & b<>a by A20,A21,AFF_1:16;
  b<>a by A20,AFF_1:16;
  then
A35: K is being_line by AFF_1:def 3;
  set M=Line(b,c);
A36: c in M by AFF_1:26;
  then
A37: M<>P by A3,A4,A6,A11,A13,A17,AFF_1:30;
  b<>c by A20,AFF_1:16;
  then
A38: M is being_line by AFF_1:def 3;
A39: b in M & q in M by A24,AFF_1:26,def 2;
  q<>b
  proof
    assume
A40: q=b;
    ( not LIN b,c,a)& c,a // q,p by A20,A26,AFF_1:13,15;
    hence contradiction by A18,A22,A40,AFF_1:69;
  end;
  then
A41: q<>b' by A3,A12,A38,A39,A37,AFF_1:30;
A42: b in K by AFF_1:26;
  p<>b by A18,A20,A24,A26,AFF_1:69;
  then
A43: p<>b' by A3,A12,A35,A42,A31,A29,AFF_1:30;
A44: not LIN b',p,q
  proof
    set N=Line(p,q);
A45: N is being_line by A18,AFF_1:def 3;
    assume
    LIN b',p,q;
    then LIN p,q,b' by AFF_1:15;
    then
A46: b' in N by AFF_1:def 2;
    q in N & LIN q,b',c' by A25,AFF_1:15,26;
    then
A47: c' in N by A41,A45,A46,AFF_1:39;
    p in N & LIN p,b',a' by A23,AFF_1:15,26;
    then a' in N by A43,A45,A46,AFF_1:39;
    hence contradiction by A21,A45,A46,A47,AFF_1:33;
  end;
  K<>M by A20,A35,A42,A28,A36,AFF_1:33;
  hence
  thesis by A1,A3,A11,A12,A16,A26,A35,A38,A42,A28,A36,A31,A39,A37,A29,A34
  ,A30,A44,A33,A27,A32,Def1;
end;

theorem
  AP is satisfying_DES1_1 implies AP is satisfying_DES1
proof
  assume
A1: AP is satisfying_DES1_1;
  let A,P,C,o,a,a',b,b',c,c',p,q;
  assume that
A2: A is being_line and
A3: P is being_line and
A4: C is being_line and
A5: P<>A and
A6: P<>C and
A7: A<>C and
A8: o in A and
A9: a in A and
A10: a' in A and
A11: o in P and
A12: b in P and
A13: b' in P and
A14: o in C and
A15: c in C and
A16: c' in C and
A17: o<>a and
A18: o<>b and
A19: o<>c and
A20: p<>q and
A21: not LIN b,a,c and
A22: not LIN b',a',c' and
A23: a<>a' and
A24: LIN b,a,p and
A25: LIN b',a',p and
A26: LIN b,c,q and
A27: LIN b',c',q and
A28: a,c // a',c';
A29: a'<>b' by A22,AFF_1:16;
  set M=Line(b,c);
A30: c in M by AFF_1:26;
  then
A31: M<>P by A3,A4,A6,A11,A14,A15,A19,AFF_1:30;
A32: M<>P by A3,A4,A6,A11,A14,A15,A19,A30,AFF_1:30;
A33: b in M by AFF_1:26;
  set K=Line(b,a);
A34: a in K by AFF_1:26;
  then
A35: K<>P by A2,A3,A5,A8,A9,A11,A17,AFF_1:30;
A36: p in K by A24,AFF_1:def 2;
A37: a'<>c' & b<>a by A21,A22,AFF_1:16;
A38: b<>c by A21,AFF_1:16;
A39: q in M by A26,AFF_1:def 2;
A40: b'<>c' by A22,AFF_1:16;
A41: b in K by AFF_1:26;
A42: not LIN o,a,c
  proof
    assume
    LIN o,a,c;
    then c in A by A2,A8,A9,A17,AFF_1:39;
    hence contradiction by A2,A4,A7,A8,A14,A15,A19,AFF_1:30;
  end;
A43: c <>c'
  proof
    assume
    c =c';
    then
A44: c,a // c,a' by A28,AFF_1:13;
    LIN o,a,a' & not LIN o,c,a by A2,A8,A9,A10,A42,AFF_1:15,33;
    hence contradiction by A23,A44,AFF_1:23;
  end;
  b<>c by A21,AFF_1:16;
  then
A45: M is being_line by AFF_1:def 3;
  b<>a by A21,AFF_1:16;
  then
A46: K is being_line by AFF_1:def 3;
A47: K<>P by A2,A3,A5,A8,A9,A11,A17,A34,AFF_1:30;
A48: now
    set C'=Line(b',c');
    set A'=Line(b',a');
A49: c' in C' by AFF_1:26;
A50: A' is being_line & b' in A' by A29,AFF_1:26,def 3;
A51: a' in A' by AFF_1:26;
    then
A52: A'<>C' by A22,A50,A49,AFF_1:33;
A53: q in C' by A27,AFF_1:def 2;
A54: p in A' by A25,AFF_1:def 2;
A55: C' is being_line & b' in C' by A40,AFF_1:26,def 3;
    assume
A56: LIN b',p,q;
    then
A57: LIN b',q,p by AFF_1:15;
A58: now
A59:  C'<>M
      proof
        assume
        C'=M;
        then LIN c,c',b by A45,A33,A30,A49,AFF_1:33;
        then b in C by A4,A15,A16,A43,AFF_1:39;
        hence contradiction by A3,A4,A6,A11,A12,A14,A18,AFF_1:30;
      end;
      assume
      b'<>q;
      then
A60:  p in C' by A57,A55,A53,AFF_1:39;
      then LIN b,a,b' by A24,A50,A54,A55,A52,AFF_1:30;
      then b' in K by AFF_1:def 2;
      then
A61:  b=b' by A3,A12,A13,A46,A41,A47,AFF_1:30;
      p=b' by A50,A54,A55,A52,A60,AFF_1:30;
      then p=q by A45,A33,A39,A55,A53,A61,A59,AFF_1:30;
      hence thesis by AFF_1:12;
    end;
    now
A62:  A'<>K
      proof
        assume
        A'=K;
        then LIN a,a',b by A46,A41,A34,A51,AFF_1:33;
        then b in A by A2,A9,A10,A23,AFF_1:39;
        hence contradiction by A2,A3,A5,A8,A11,A12,A18,AFF_1:30;
      end;
      assume
      b'<>p;
      then
A63:  q in A' by A56,A50,A54,AFF_1:39;
      then LIN b,c,b' by A26,A50,A55,A53,A52,AFF_1:30;
      then b' in M by AFF_1:def 2;
      then
A64:  b=b' by A3,A12,A13,A45,A33,A32,AFF_1:30;
      q=b' by A50,A55,A53,A52,A63,AFF_1:30;
      then p=q by A46,A41,A36,A50,A54,A64,A62,AFF_1:30;
      hence thesis by AFF_1:12;
    end;
    hence thesis by A20,A58;
  end;
A65: K<>M by A21,A46,A41,A34,A30,AFF_1:33;
  now
A66: LIN o,c,c' & LIN b',q,c' by A4,A14,A15,A16,A27,AFF_1:15,33;
    assume
A67: not LIN b',p,q;
    LIN o,a,a' & LIN b',p,a' by A2,A8,A9,A10,A25,AFF_1:15,33;
    hence
    thesis by A1,A3,A11,A12,A13,A18,A28,A46,A45,A41,A34,A33,A30,A36,A39,
    A31,A35,A37,A38,A65,A42,A43,A67,A66,Def2;
  end;
  hence thesis by A48;
end;

theorem
  AP is Desarguesian implies AP is satisfying_DES1
proof
  assume
A1: AP is Desarguesian;
  let A,P,C,o,a,a',b,b',c,c',p,q;
  assume that
A2: A is being_line and
A3: P is being_line and
A4: C is being_line and
A5: P<>A and
A6: P<>C and
A7: A<>C and
A8: o in A and
A9: a in A and
A10: a' in A and
A11: o in P and
A12: b in P and
A13: b' in P and
A14: o in C and
A15: c in C and
A16: c' in C and
A17: o<>a and
A18: o<>b and
A19: o<>c and
  p<>q and
A20: not LIN b,a,c and
A21: not LIN b',a',c' and
A22: a<>a' and
A23: LIN b,a,p and
A24: LIN b',a',p and
A25: LIN b,c,q and
A26: LIN b',c',q and
A27: a,c // a',c';
  set D=Line(b,c);
  b<>c by A20,AFF_1:16;
  then D is being_line by AFF_1:def 3;
  then consider D' such that
A28: c' in D' and
A29: D // D' by AFF_1:63;
A30: D' is being_line by A29,AFF_1:50;
  set M=Line(b',c');
A31: q in M by A26,AFF_1:def 2;
A32: b in D by AFF_1:26;
A33: c in D by AFF_1:26;
  not D' // P
  proof
    assume
    D' // P;
    then D // P by A29,AFF_1:58;
    then c in P by A12,A32,A33,AFF_1:59;
    hence contradiction by A3,A4,A6,A11,A14,A15,A19,AFF_1:30;
  end;
  then consider d such that
A34: d in D' and
A35: d in P by A3,A30,AFF_1:72;
A36: q in D by A25,AFF_1:def 2;
  then
A37: d,c' // b,q by A32,A28,A29,A34,AFF_1:53;
A38: a<>b & b,a // b,p by A20,A23,AFF_1:16,def 1;
A39: c,a // c',a' by A27,AFF_1:13;
  c,b // c',d by A32,A33,A28,A29,A34,AFF_1:53;
  then b,a // d,a' by A1,A2,A3,A4,A6,A7,A8,A9,A10,A11,A12,A14,A15,A16,A17,A18
  ,A19,A35,A39,AFF_2:def 4;
  then
A40: d,a' // b,p by A38,AFF_1:14;
  set K=Line(b',a');
A41: b' in K & p in K by A24,AFF_1:26,def 2;
A42: a'<>b' by A21,AFF_1:16;
  then
A43: K is being_line by AFF_1:def 3;
A44: b' in M by AFF_1:26;
A45: c' in M by AFF_1:26;
A46: b'<>c' by A21,AFF_1:16;
  then
A47: M is being_line by AFF_1:def 3;
A48: not LIN o,a,c
  proof
    assume
    LIN o,a,c;
    then c in A by A2,A8,A9,A17,AFF_1:39;
    hence contradiction by A2,A4,A7,A8,A14,A15,A19,AFF_1:30;
  end;
A49: c <>c'
  proof
    assume
    c =c';
    then
A50: c,a // c,a' by A27,AFF_1:13;
    LIN o,a,a' & not LIN o,c,a by A2,A8,A9,A10,A48,AFF_1:15,33;
    hence contradiction by A22,A50,AFF_1:23;
  end;
A51: d<>b'
  proof
    assume
    d=b';
    then M=D' by A46,A47,A44,A45,A28,A30,A34,AFF_1:30;
    then D=M by A31,A36,A29,AFF_1:59;
    then LIN c,c',b by A47,A45,A32,A33,AFF_1:33;
    then b in C by A4,A15,A16,A49,AFF_1:39;
    hence contradiction by A3,A4,A6,A11,A12,A14,A18,AFF_1:30;
  end;
A52: a'<>c' by A21,AFF_1:16;
A53: o<>a'
  proof
    assume
A54: o=a';
    LIN o,c,c' by A4,A14,A15,A16,AFF_1:33;
    hence contradiction by A27,A52,A48,A54,AFF_1:69;
  end;
  o<>c'
  proof
A55: not LIN o,c,a by A48,AFF_1:15;
    assume
A56: o=c';
    LIN o,a,a' & c,a // c',a' by A2,A8,A9,A10,A27,AFF_1:13,33;
    hence contradiction by A53,A56,A55,AFF_1:69;
  end;
  then
A57: M<>P by A3,A4,A6,A11,A14,A16,A45,AFF_1:30;
A58: a' in K by AFF_1:26;
  then K<>P by A2,A3,A5,A8,A10,A11,A53,AFF_1:30;
  then a',c' // p,q by A1,A3,A12,A13,A42,A46,A43,A47,A58,A41,A44,A45,A31,
  A57,A35,A51,A40,A37,AFF_2:def 4;
  hence thesis by A27,A52,AFF_1:14;
end;

theorem
  AP is Desarguesian implies AP is satisfying_DES1_2
proof
  assume
A1: AP is Desarguesian;
  then
A2: AP is satisfying_DES_1 by AFF_2:16;
  let A,P,C,o,a,a',b,b',c,c',p,q;
  assume that
A3: A is being_line and
A4: P is being_line and
A5: C is being_line and
A6: P<>A and
A7: P<>C and
  A<>C and
A8: o in A and
A9: a in A & a' in A and
A10: o in P and
A11: b in P and
A12: b' in P and
A13: c in C and
A14: c' in C and
A15: o<>a and
A16: o<>b and
  o<>c and
A17: p<>q and
A18: not LIN b,a,c and
A19: not LIN b',a',c' and
A20: c <>c' and
A21: LIN b,a,p and
A22: LIN b',a',p and
A23: LIN b,c,q and
A24: LIN b',c',q and
A25: a,c // a',c' and
A26: a,c // p,q;
A27: b<>p by A17,A18,A23,A26,AFF_1:69;
  set K=Line(b',a');
A28: p in K by A22,AFF_1:def 2;
  a'<>b' by A19,AFF_1:16;
  then
A29: K is being_line by AFF_1:def 3;
A30: b<>q
  proof
    assume
A31: b=q;
    ( not LIN b,c,a)& c,a // q,p by A18,A26,AFF_1:13,15;
    hence contradiction by A17,A21,A31,AFF_1:69;
  end;
  set M=Line(b',c');
A32: q in M by A24,AFF_1:def 2;
A33: c' in M by AFF_1:26;
A34: a<>c by A18,AFF_1:16;
A35: b'<>p
  proof
    assume
A36: b'=p;
    a',c' // p,q by A25,A26,A34,AFF_1:14;
    hence contradiction by A17,A19,A24,A36,AFF_1:69;
  end;
A37: b'<>q
  proof
    a',c' // p,q by A25,A26,A34,AFF_1:14;
    then
A38: c',a' // q,p by AFF_1:13;
    assume
A39: b'=q;
    not LIN b',c',a' by A19,AFF_1:15;
    hence contradiction by A17,A22,A39,A38,AFF_1:69;
  end;
A40: b<>c by A18,AFF_1:16;
A41: a<>b by A18,AFF_1:16;
A42: b'<>a' & b'<>c' by A19,AFF_1:16;
A43: a' in K by AFF_1:26;
A44: b' in M by AFF_1:26;
A45: b'<>c' by A19,AFF_1:16;
  then
A46: M is being_line by AFF_1:def 3;
A47: b' in K by AFF_1:26;
  then
A48: K<>M by A19,A29,A43,A33,AFF_1:33;
  now
A49: now
      p,q // a',c' by A25,A26,A34,AFF_1:14;
      then
A50:  c',a' // q,p by AFF_1:13;
A51:  b,a // b,p by A21,AFF_1:def 1;
      set D=Line(b,c);
A52:  b in D by AFF_1:26;
      D is being_line by A40,AFF_1:def 3;
      then consider D' such that
A53:  c' in D' and
A54:  D // D' by AFF_1:63;
A55:  D' is being_line by A54,AFF_1:50;
A56:  q in D by A23,AFF_1:def 2;
      assume
A57:  M<>P;
      not D' // P
      proof
        assume
        D' // P;
        then D // P by A54,AFF_1:58;
        then q in P by A11,A52,A56,AFF_1:59;
        hence contradiction by A4,A12,A46,A44,A32,A37,A57,AFF_1:30;
      end;
      then consider d such that
A58:  d in D' and
A59:  d in P by A4,A55,AFF_1:72;
A60:  c in D by AFF_1:26;
A61:  d<>b'
      proof
        assume
        d=b';
        then M=D' by A45,A46,A44,A33,A53,A55,A58,AFF_1:30;
        then
A62:    D=M by A32,A56,A54,AFF_1:59;
        then LIN c,c',b by A46,A33,A52,A60,AFF_1:33;
        then
A63:    b in C by A5,A13,A14,A20,AFF_1:39;
        set N=Line(a,c);
        set T=Line(b,a);
A64:    b in T by AFF_1:26;
A65:    c in N by AFF_1:26;
A66:    a in T by AFF_1:26;
A67:    N is being_line by A34,AFF_1:def 3;
A68:    a in N by AFF_1:26;
A69:    a<>a'
        proof
          assume
          a=a';
          then LIN a,c,c' by A25,AFF_1:def 1;
          then c' in N by AFF_1:def 2;
          then N=C by A5,A13,A14,A20,A67,A65,AFF_1:30;
          hence contradiction by A13,A18,A63,A67,A68,AFF_1:33;
        end;
A70:    T is being_line & p in T by A21,A41,AFF_1:def 2,def 3;
A71:    b<>b'
        proof
A72:      K<>T
          proof
            assume
            K=T;
            then T=A by A3,A9,A29,A43,A66,A69,AFF_1:30;
            hence contradiction by A3,A4,A6,A8,A10,A11,A16,A64,AFF_1:30;
          end;
          assume
          b=b';
          hence contradiction by A29,A47,A28,A35,A64,A70,A72,AFF_1:30;
        end;
        LIN c,c',b' by A46,A44,A33,A60,A62,AFF_1:33;
        then b' in C by A5,A13,A14,A20,AFF_1:39;
        hence contradiction by A4,A5,A7,A11,A12,A63,A71,AFF_1:30;
      end;
      c',d // q,b by A52,A56,A53,A54,A58,AFF_1:53;
      then d,a' // b,p by A1,A4,A11,A12,A42,A29,A46,A47,A43,A28,A44,A33,A32,
      A48,A57,A59,A61,A50,AFF_2:def 4;
      then
A73:  b,a // d,a' by A27,A51,AFF_1:14;
      b,c // d,c' by A52,A60,A53,A54,A58,AFF_1:53;
      hence
      thesis by A2,A3,A4,A5,A6,A8,A9,A10,A11,A13,A14,A15,A16,A18,A20,A25
      ,A59,A73,AFF_2:def 5;
    end;
    now
      assume
A74:  M=P;
      LIN b,q,c by A23,AFF_1:15;
      then c in P by A11,A46,A32,A30,A74,AFF_1:39;
      then P=Line(c,c') by A20,A46,A33,A74,AFF_1:71;
      hence thesis by A5,A10,A13,A14,A20,AFF_1:71;
    end;
    hence thesis by A49;
  end;
  hence thesis;
end;

theorem
  AP is satisfying_DES1_2 implies AP is satisfying_DES1_3
proof
  assume
A1: AP is satisfying_DES1_2;
  let A,P,C,o,a,a',b,b',c,c',p,q;
  assume that
A2: A is being_line and
A3: P is being_line and
A4: C is being_line and
A5: P<>A and
A6: P<>C and
A7: A<>C and
A8: o in A and
A9: a in A and
A10: a' in A and
A11: b in P and
A12: b' in P and
A13: o in C and
A14: c in C and
A15: c' in C and
A16: o<>a and
A17: o<>b and
A18: o<>c and
A19: p<>q and
A20: not LIN b,a,c and
A21: not LIN b',a',c' and
A22: b<>b' and
A23: a<>a' and
A24: LIN b,a,p and
A25: LIN b',a',p and
A26: LIN b,c,q and
A27: LIN b',c',q and
A28: a,c // a',c' and
A29: a,c // p,q;
  set D=Line(b,c), T=Line(b,a), N=Line(p,q), M=Line(a',c'), K=Line(b',a');
  assume
A30: not thesis;
A31: not LIN o,c,a
  proof
    assume
    LIN o,c,a;
    then a in C by A4,A13,A14,A18,AFF_1:39;
    hence contradiction by A2,A4,A7,A8,A9,A13,A16,AFF_1:30;
  end;
A32: c <>c'
  proof
    assume
    c =c';
    then
A33: c,a // c,a' by A28,AFF_1:13;
    LIN o,a,a' by A2,A8,A9,A10,AFF_1:33;
    hence contradiction by A23,A31,A33,AFF_1:23;
  end;
  a<>c by A20,AFF_1:16;
  then
A34: a',c' // p,q by A28,A29,AFF_1:14;
A35: p<>b & p<>b' & q<>b & q<>b'
  proof
A36: now
      assume
A37:  b'=q;
      ( not LIN b',c',a')& c',a' // q,p by A21,A34,AFF_1:13,15;
      hence contradiction by A19,A25,A37,AFF_1:69;
    end;
A38: now
      assume
A39:  b=q;
      ( not LIN b,c,a)& c,a // q,p by A20,A29,AFF_1:13,15;
      hence contradiction by A19,A24,A39,AFF_1:69;
    end;
    assume
    not thesis;
    hence contradiction by A20,A21,A26,A27,A29,A34,A38,A36,AFF_1:69;
  end;
A40: b<>c by A20,AFF_1:16;
  then
A41: D is being_line & c in D by AFF_1:38;
A42: b in D by A40,AFF_1:38;
  then
A43: q in D by A26,A40,A41,AFF_1:39;
A44: now
    assume
    not C // P;
    then consider x such that
A45: x in C and
A46: x in P by A3,A4,AFF_1:72;
A47: x<>c
    proof
A48:  LIN q,b',c' & LIN q,b',b' by A27,AFF_1:15,16;
      assume
A49:  x=c;
      then LIN b,c,b' & LIN b,c,c by A3,A11,A12,A46,AFF_1:33;
      then LIN q,b',c by A26,A40,AFF_1:17;
      then
A50:  b' in C by A4,A14,A15,A32,A35,A48,AFF_1:17,39;
      then LIN c,c',q by A3,A4,A6,A12,A14,A27,A46,A49,AFF_1:30;
      then
A51:  q in C by A4,A14,A15,A32,AFF_1:39;
      c =b' by A3,A4,A6,A12,A14,A46,A49,A50,AFF_1:30;
      then C=D by A4,A14,A35,A41,A43,A51,AFF_1:30;
      hence contradiction by A3,A4,A6,A11,A12,A22,A42,A50,AFF_1:30;
    end;
A52: x<>b
    proof
A53:  LIN q,c',b' by A27,AFF_1:15;
      assume
A54:  x=b;
      then q in C by A4,A14,A26,A40,A45,AFF_1:39;
      then q=c' or b' in C by A4,A15,A53,AFF_1:39;
      then c',a' // c',p by A3,A4,A6,A11,A12,A22,A34,A45,A54,AFF_1:13
      ,30;
      then LIN c',a',p by AFF_1:def 1;
      then
A55:  LIN p,a',c' by AFF_1:15;
      LIN p,a',b' & LIN p,a',a' by A25,AFF_1:15,16;
      then p=a' by A21,A55,AFF_1:17;
      then LIN a,a',b by A24,AFF_1:15;
      then b in A by A2,A9,A10,A23,AFF_1:39;
      hence contradiction by A2,A4,A7,A8,A13,A17,A45,A54,AFF_1:30;
    end;
A56: c,a // q,p & c,a // c',a' by A28,A29,AFF_1:13;
    ( not LIN b,c,a)& not LIN b',c',a' by A20,A21,AFF_1:15;
    then x in A by A1,A2,A3,A4,A6,A9,A10,A11,A12,A14,A15,A19,A23,A24,A25,A26
    ,A27,A45,A46,A47,A52,A56,Def3;
    hence contradiction by A2,A4,A7,A8,A13,A30,A45,A46,AFF_1:30;
  end;
A57: a<>b by A20,AFF_1:16;
A58: a'<>b' by A21,AFF_1:16;
  then
A59: a' in K by AFF_1:38;
A60: K is being_line & b' in K by A58,AFF_1:38;
  then
A61: p in K by A25,A58,A59,AFF_1:39;
A62: now
    assume
    not P // A;
    then consider x such that
A63: x in P and
A64: x in A by A2,A3,AFF_1:72;
A65: x<>b
    proof
      assume
A66:  x=b;
      then p in A by A2,A9,A24,A57,A64,AFF_1:39;
      then a',c' // a',q or b' in A by A2,A10,A34,A60,A59,A61,AFF_1:30;
      then LIN a',c',q by A2,A3,A5,A11,A12,A22,A64,A66,AFF_1:30,def 1;
      then
A67:  LIN q,c',a' by AFF_1:15;
      LIN q,c',b' & LIN q,c',c' by A27,AFF_1:15,16;
      then q=c' by A21,A67,AFF_1:17;
      then LIN c,c',b by A26,AFF_1:15;
      then b in C by A4,A14,A15,A32,AFF_1:39;
      hence contradiction by A2,A4,A7,A8,A13,A17,A64,A66,AFF_1:30;
    end;
    x<>a
    proof
      assume
      x=a;
      then p in P & K<>P by A2,A3,A5,A9,A10,A11,A23,A24,A57,A59,A63,
      AFF_1:
      30,39;
      hence contradiction by A3,A12,A35,A60,A61,AFF_1:30;
    end;
    then x in C by A1,A2,A3,A4,A5,A9,A10,A11,A12,A14,A15,A19,A20,A21,A24,A25
    ,A26,A27,A28,A29,A32,A63,A64,A65,Def3;
    hence contradiction by A2,A4,A7,A8,A13,A30,A63,A64,AFF_1:30;
  end;
  not P // A or not C // P
  proof
    assume
    not thesis;
    then C // A by AFF_1:58;
    hence contradiction by A7,A8,A13,AFF_1:59;
  end;
  hence contradiction by A62,A44;
end;

theorem
  AP is satisfying_DES1_2 implies AP is Desarguesian
proof
  assume
A1: AP is satisfying_DES1_2;
  let A,P,C,o,a,b,c,a',b',c';
  assume that
A2: o in A and
A3: o in P and
A4: o in C and
A5: o<>a and
A6: o<>b and
A7: o<>c and
A8: a in A and
A9: a' in A and
A10: b in P and
A11: b' in P and
A12: c in C and
A13: c' in C and
A14: A is being_line and
A15: P is being_line and
A16: C is being_line and
A17: A<>P and
A18: A<>C and
A19: a,b // a',b' and
A20: a,c // a',c';
  now
A21: not LIN o,b,a & not LIN o,a,c
    proof
A22:  now
        assume
        LIN o,a,c;
        then c in A by A2,A5,A8,A14,AFF_1:39;
        hence contradiction by A2,A4,A7,A12,A14,A16,A18,AFF_1:30;
      end;
A23:  now
        assume
        LIN o,b,a;
        then a in P by A3,A6,A10,A15,AFF_1:39;
        hence contradiction by A2,A3,A5,A8,A14,A15,A17,AFF_1:30;
      end;
      assume
      not thesis;
      hence thesis by A23,A22;
    end;
A24: b=b' implies thesis
    proof
A25:  LIN o,c,c' by A4,A12,A13,A16,AFF_1:33;
A26:  LIN o,a,a' by A2,A8,A9,A14,AFF_1:33;
      assume
A27:  b=b';
      then b,a // b,a' by A19,AFF_1:13;
      then a,c // a,c' by A20,A21,A26,AFF_1:23;
      then c =c' by A21,A25,AFF_1:23;
      hence thesis by A27,AFF_1:11;
    end;
A28: a'=o implies thesis
    proof
      assume
A29:  a'=o;
      LIN o,b,b' & not LIN o,a,b by A3,A10,A11,A15,A21,AFF_1:15,33;
      then
A30:  o=b' by A19,A29,AFF_1:69;
      LIN o,c,c' by A4,A12,A13,A16,AFF_1:33;
      then o=c' by A20,A21,A29,AFF_1:69;
      hence thesis by A30,AFF_1:12;
    end;
A31: c'=o implies thesis
    proof
A32:  c,a // c',a' by A20,AFF_1:13;
      assume
A33:  c'=o;
      LIN o,a,a' & not LIN o,c,a by A2,A8,A9,A14,A21,AFF_1:15,33;
      hence thesis by A28,A33,A32,AFF_1:69;
    end;
    set K=Line(a,c);
A34: a in K by AFF_1:26;
A35: a<>c by A2,A4,A5,A8,A12,A14,A16,A18,AFF_1:30;
    then
A36: K is being_line by AFF_1:def 3;
A37: c in K by AFF_1:26;
A38: a<>b by A2,A3,A5,A8,A10,A14,A15,A17,AFF_1:30;
A39: LIN a,b,c implies thesis
    proof
      consider N such that
A40:  a' in N and
A41:  K // N by A36,AFF_1:63;
A42:  N is being_line by A41,AFF_1:50;
      a',c' // K by A20,A35,AFF_1:43,46;
      then a',c' // N by A41,AFF_1:57;
      then
A43:  c' in N by A40,A42,AFF_1:37;
      assume
      LIN a,b,c;
      then LIN a,c,b by AFF_1:15;
      then
A44:  b in K by AFF_1:def 2;
      then K=Line(a,b) by A38,A36,A34,AFF_1:71;
      then a',b' // K by A19,A38,AFF_1:43,46;
      then a',b' // N by A41,AFF_1:57;
      then b' in N by A40,A42,AFF_1:37;
      hence thesis by A37,A44,A41,A43,AFF_1:53;
    end;
    assume
A45: P<>C;
A46: now
      set T=Line(b',a');
      set D=Line(b,a);
      set N=Line(a',c');
      assume that
A47:  o<>a' and
A48:  o<>b' and
A49:  o<>c' and
A50:  b<>b' and
A51:  not LIN a,b,c;
A52:  c' in N by AFF_1:26;
      assume
      not b,c // b',c';
      then consider q such that
A53:  LIN b,c,q and
A54:  LIN b',c',q by AFF_1:74;
      consider M such that
A55:  q in M and
A56:  K // M by A36,AFF_1:63;
A57:  M is being_line by A56,AFF_1:50;
      not a,b // M
      proof
        assume
        a,b // M;
        then a,b // K by A56,AFF_1:57;
        then b in K by A36,A34,AFF_1:37;
        hence contradiction by A36,A34,A37,A51,AFF_1:33;
      end;
      then consider p such that
A58:  p in M and
A59:  LIN a,b,p by A57,AFF_1:73;
A60:  a' in N by AFF_1:26;
A61:  p<>q
      proof
A62:    LIN p,b,a & LIN p,b,b by A59,AFF_1:15,16;
        assume
A63:    p=q;
        then LIN p,b,c by A53,AFF_1:15;
        then p=b by A51,A62,AFF_1:17;
        then LIN b,b',c' by A54,A63,AFF_1:15;
        then c' in P by A10,A11,A15,A50,AFF_1:39;
        hence contradiction by A3,A4,A13,A15,A16,A45,A49,AFF_1:30;
      end;
A64:  c,a // q,p by A34,A37,A55,A56,A58,AFF_1:53;
A65:  LIN b,a,p by A59,AFF_1:15;
A66:  b'<>c' by A3,A4,A11,A13,A15,A16,A45,A48,AFF_1:30;
A67:  a'<>c' by A2,A4,A9,A13,A14,A16,A18,A47,AFF_1:30;
      then
A68:  N is being_line by AFF_1:def 3;
A69:  K // N by A20,A35,A67,AFF_1:51;
      then
A70:  N // M by A56,AFF_1:58;
A71:  a'<>b' by A2,A3,A9,A11,A14,A15,A17,A47,AFF_1:30;
A72:  not LIN a',b',c'
      proof
        assume
        LIN a',b',c';
        then LIN a',c',b' by AFF_1:15;
        then b' in N by AFF_1:def 2;
        then a',b' // N by A68,A60,AFF_1:37;
        then
A73:    a',b' // K by A69,AFF_1:57;
        a',b' // a,b by A19,AFF_1:13;
        then a,b // K by A71,A73,AFF_1:46;
        then b in K by A36,A34,AFF_1:37;
        hence contradiction by A36,A34,A37,A51,AFF_1:33;
      end;
      not b',p // N
      proof
        assume
        b',p // N;
        then b',p // M by A70,AFF_1:57;
        then p,b' // M by AFF_1:48;
        then
A74:    b' in M by A57,A58,AFF_1:37;
A75:    now
          assume
A76:      b'<>q;
          LIN b',q,c' by A54,AFF_1:15;
          then c' in M by A55,A57,A74,A76,AFF_1:39;
          then a' in N & b' in N by A52,A70,A74,AFF_1:26,59;
          hence contradiction by A68,A52,A72,AFF_1:33;
        end;
        now
          assume
          b'=q;
          then LIN b,b',c by A53,AFF_1:15;
          then c in P by A10,A11,A15,A50,AFF_1:39;
          hence contradiction by A3,A4,A7,A12,A15,A16,A45,AFF_1:30;
        end;
        hence thesis by A75;
      end;
      then consider x such that
A77:  x in N and
A78:  LIN b',p,x by A68,AFF_1:73;
      set A'=Line(x,a);
A79:  a<>a'
      proof
        assume
A80:    a=a';
        ( not LIN o,a,b)& LIN o,b,b' by A3,A10,A11,A15,A21,AFF_1:15,33;
        hence contradiction by A19,A50,A80,AFF_1:23;
      end;
A81:  x<>a
      proof
        assume
        x=a;
        then a' in K by A34,A60,A69,A77,AFF_1:59;
        then A=K by A8,A9,A14,A36,A34,A79,AFF_1:30;
        hence contradiction by A2,A4,A7,A12,A14,A16,A18,A37,AFF_1:30;
      end;
      then
A82:  A' is being_line by AFF_1:def 3;
A83:  c <>c'
      proof
        assume
        c =c';
        then
A84:    c,a // c,a' by A20,AFF_1:13;
        ( not LIN o,c,a)& LIN o,a,a' by A2,A8,A9,A14,A21,AFF_1:15,33;
        hence contradiction by A79,A84,AFF_1:23;
      end;
A85:  not LIN b',c',x
      proof
A86:    now
A87:      now
            assume
            q=c';
            then LIN c,c',b by A53,AFF_1:15;
            then b in C by A12,A13,A16,A83,AFF_1:39;
            hence contradiction by A3,A4,A6,A10,A15,A16,A45,AFF_1:30;
          end;
          assume
          c'=x;
          then
A88:      LIN b',c',p by A78,AFF_1:15;
          LIN b',c',c' by AFF_1:16;
          then c' in M by A66,A54,A55,A57,A58,A61,A88,AFF_1:17,39;
          then
A89:      q in N by A55,A52,A70,AFF_1:59;
          LIN q,c',b' by A54,AFF_1:15;
          then q=c' or b' in N by A68,A52,A89,AFF_1:39;
          hence LIN b',c',a' by A68,A60,A52,A87,AFF_1:33;
        end;
        assume
        LIN b',c',x;
        then
A90:    LIN c',x,b' by AFF_1:15;
A91:    LIN c',x,a' & LIN c',x,c' by A68,A60,A52,A77,AFF_1:33;
        then LIN c',a',b' by A90,A86,AFF_1:15,17;
        then c',a' // c',b' by AFF_1:def 1;
        then a',c' // b',c' by AFF_1:13;
        then
A92:    a,c // b',c' by A20,A67,AFF_1:14;
        c'=x or LIN b',c',a' by A90,A91,AFF_1:17;
        then b',c' // b',a' by A86,AFF_1:def 1;
        then b',c' // a',b' by AFF_1:13;
        then b',c' // a,b by A19,A71,AFF_1:14;
        then a,c // a,b by A66,A92,AFF_1:14;
        then LIN a,c,b by AFF_1:def 1;
        hence contradiction by A51,AFF_1:15;
      end;
A93:  x in A' & a in A' by AFF_1:26;
      A<>K by A2,A4,A7,A12,A14,A16,A18,A37,AFF_1:30;
      then
A94:  A <> N by A8,A34,A69,AFF_1:59;
A95:  not LIN b,c,a by A51,AFF_1:15;
A96:  p in D by A59,AFF_1:def 2;
A97:  D is being_line & b in D by A38,AFF_1:26,def 3;
A98:  LIN b',x,p by A78,AFF_1:15;
      c,a // c',x by A34,A37,A52,A69,A77,AFF_1:53;
      then o in A' by A1,A3,A4,A6,A7,A10,A11,A12,A13,A15,A16,A45,A53,A54,
      A98,A81,A82,A93,A61,A85,A64,A95,A65,Def3;
      then x in A by A2,A5,A8,A14,A82,A93,AFF_1:30;
      then x=a' by A9,A14,A68,A60,A77,A94,AFF_1:30;
      then
A99:  a' in T & p in T by A98,AFF_1:26,def 2;
      D // T by A19,A38,A71,AFF_1:51;
      then a in D & a' in D by A96,A99,AFF_1:26,59;
      then b in A by A8,A9,A14,A79,A97,AFF_1:30;
      hence contradiction by A2,A3,A6,A10,A14,A15,A17,AFF_1:30;
    end;
    b'=o implies thesis
    proof
      assume
A100: b'=o;
      LIN o,a,a' & b,a // b',a' by A2,A8,A9,A14,A19,AFF_1:13,33;
      hence thesis by A21,A28,A100,AFF_1:69;
    end;
    hence thesis by A28,A31,A39,A24,A46;
  end;
  hence thesis by A10,A11,A12,A13,A15,AFF_1:65;
end;

theorem
  AP is satisfying_DES2_1 implies AP is satisfying_DES2
proof
  assume
A1: AP is satisfying_DES2_1;
  let A,P,C,a,a',b,b',c,c',p,q;
  assume that
A2: A is being_line and
A3: P is being_line and
A4: C is being_line and
A5: A<>P and
A6: A<>C and
A7: P<>C and
A8: a in A and
A9: a' in A and
A10: b in P and
A11: b' in P and
A12: c in C and
A13: c' in C and
A14: A // P and
A15: A // C and
A16: not LIN b,a,c and
A17: not LIN b',a',c' and
A18: p<>q and
A19: a<>a' and
A20: LIN b,a,p and
A21: LIN b',a',p and
A22: LIN b,c,q & LIN b',c',q and
A23: a,c // a',c';
A24: P // C by A14,A15,AFF_1:58;
  set P'=Line(b',a');
A25: p in P' by A21,AFF_1:def 2;
  a'<>b' by A17,AFF_1:16;
  then
A26: P' is being_line by AFF_1:def 3;
  set K=Line(a,c), N=Line(a',c'), D=Line(b,c), T=Line(b',c');
A27: q in D & q in T by A22,AFF_1:def 2;
A28: c' in N by AFF_1:26;
  b<>c by A16,AFF_1:16;
  then
A29: D is being_line by AFF_1:def 3;
  b'<>c' by A17,AFF_1:16;
  then
A30: T is being_line by AFF_1:def 3;
A31: a<>c by A16,AFF_1:16;
  then
A32: K is being_line by AFF_1:def 3;
A33: a'<>c' by A17,AFF_1:16;
  then
A34: N is being_line by AFF_1:def 3;
  then consider M such that
A35: p in M and
A36: N // M by AFF_1:63;
A37: K // N by A23,A31,A33,AFF_1:51;
  then
A38: K // M by A36,AFF_1:58;
A39: c in D by AFF_1:26;
A40: b in D by AFF_1:26;
  set A'=Line(b,a);
A41: p in A' by A20,AFF_1:def 2;
  a<>b by A16,AFF_1:16;
  then
A42: A' is being_line by AFF_1:def 3;
A43: c' in T by AFF_1:26;
A44: b' in T by AFF_1:26;
A45: a' in N by AFF_1:26;
A46: a in K by AFF_1:26;
A47: a' in P' by AFF_1:26;
A48: b' in P' by AFF_1:26;
A49: a in A' by AFF_1:26;
A50: b in A' by AFF_1:26;
A51: c in K by AFF_1:26;
  then
A52: K<>A by A6,A12,A15,AFF_1:59;
A53: c <>c'
  proof
    assume
    c =c';
    then K=N by A51,A28,A37,AFF_1:59;
    hence contradiction by A2,A8,A9,A19,A32,A46,A45,A52,AFF_1:30;
  end;
A54: D<>T
  proof
    assume
    D=T;
    then D=C by A4,A12,A13,A29,A39,A43,A53,AFF_1:30;
    hence contradiction by A7,A10,A24,A40,AFF_1:59;
  end;
A55: b<>b'
  proof
A56: A'<>P'
    proof
      assume
      A'=P';
      then A'=A by A2,A8,A9,A19,A42,A49,A47,AFF_1:30;
      hence contradiction by A5,A10,A14,A50,AFF_1:59;
    end;
    assume
A57: b=b';
    then b'=q by A29,A30,A40,A44,A27,A54,AFF_1:30;
    hence contradiction by A18,A42,A26,A50,A41,A48,A25,A57,A56,AFF_1:30;
  end;
A58: M is being_line by A36,AFF_1:50;
A59: now
    assume
    not T // M;
    then consider x such that
A60: x in T and
A61: x in M by A30,A58,AFF_1:72;
A62: p<>x
    proof
      assume
      p=x;
      then p=b' or T=P' by A30,A44,A26,A48,A25,A60,AFF_1:30;
      then LIN b,b',a or c' in P' by A42,A50,A49,A41,AFF_1:26,33;
      then a in P by A3,A10,A11,A17,A55,AFF_1:39,def 2;
      hence contradiction by A5,A8,A14,AFF_1:59;
    end;
    not b,x // C
    proof
      assume
A63:  b,x // C;
      C // P by A14,A15,AFF_1:58;
      then b,x // P by A63,AFF_1:57;
      then x in P by A3,A10,AFF_1:37;
      then b' in M or c' in P by A3,A11,A30,A44,A43,A60,A61,AFF_1:30;
      then b'=p or M=P' by A7,A13,A24,A26,A48,A25,A35,A58,AFF_1:30,59;
      then LIN b,b',a or N // P' by A20,A36,AFF_1:15;
      then a in P or N=P' by A3,A10,A11,A45,A47,A55,AFF_1:39,59;
      hence contradiction by A5,A8,A14,A17,A28,AFF_1:59,def 2;
    end;
    then consider y such that
A64: y in C and
A65: LIN b,x,y by A4,AFF_1:73;
A66: LIN b,y,x by A65,AFF_1:15;
A67: not LIN b,a,y
    proof
A68:  now
        assume
        x=p;
        then p=b' or T=P' by A30,A44,A26,A48,A25,A60,AFF_1:30;
        then LIN b,b',a or c' in P' by A42,A50,A49,A41,AFF_1:26,33;
        then a in P by A3,A10,A11,A17,A55,AFF_1:39,def 2;
        hence contradiction by A5,A8,A14,AFF_1:59;
      end;
      assume
      LIN b,a,y;
      then
A69:  LIN b,y,a by AFF_1:15;
      LIN b,y,b by AFF_1:16;
      then b=y or LIN a,b,x by A66,A69,AFF_1:17;
      then x in A' by A7,A10,A24,A64,AFF_1:59,def 2;
      then x=p or A'=M by A42,A41,A35,A58,A61,AFF_1:30;
      then K=A' by A46,A49,A38,A68,AFF_1:59;
      hence contradiction by A16,A51,AFF_1:def 2;
    end;
    LIN b',c',x & a',c' // p,x by A30,A45,A28,A44,A43,A35,A36,A60,A61,
    AFF_1:33,53;
    then a',c' // a,y by A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A13,A14,A15,A17,
    A20,A21,A64,A66,A67,A62,Def6;
    then a,c // a,y by A23,A33,AFF_1:14;
    then LIN a,c,y by AFF_1:def 1;
    then y in K by AFF_1:def 2;
    then K=C or y=c by A4,A12,A32,A51,A64,AFF_1:30;
    then a in C or LIN b,c,x by A65,AFF_1:15,26;
    then x in D by A6,A8,A15,AFF_1:59,def 2;
    then q in M or c' in D by A29,A30,A43,A27,A60,A61,AFF_1:30;
    then a,c // p,q or LIN c,c',b by A29,A46,A51,A40,A39,A35,A38,AFF_1:
    33 ,53;
    then a,c // p,q or b in C by A4,A12,A13,A53,AFF_1:39;
    hence thesis by A7,A10,A24,AFF_1:59;
  end;
A70: now
    assume
    not M // D;
    then consider x such that
A71: x in M and
A72: x in D by A29,A58,AFF_1:72;
A73: p<>x
    proof
      assume
      p=x;
      then p=b or D=A' by A29,A40,A42,A50,A41,A72,AFF_1:30;
      then LIN b,b',a' or c in A' by A26,A48,A47,A25,AFF_1:26,33;
      then a' in P by A3,A10,A11,A16,A55,AFF_1:39,def 2;
      hence contradiction by A5,A9,A14,AFF_1:59;
    end;
    not b',x // C
    proof
A74:  now
        assume
        b=p;
        then LIN b,b',a' by A26,A48,A47,A25,AFF_1:33;
        then a' in P by A3,A10,A11,A55,AFF_1:39;
        hence contradiction by A5,A9,A14,AFF_1:59;
      end;
      assume
A75:  b',x // C;
      C // P by A14,A15,AFF_1:58;
      then b',x // P by A75,AFF_1:57;
      then x in P by A3,A11,AFF_1:37;
      then x=b or D=P by A3,A10,A29,A40,A72,AFF_1:30;
      then b=p or M=A' by A7,A12,A24,A39,A42,A50,A41,A35,A58,A71,AFF_1:30
      ,59;
      then b in K by A46,A50,A49,A38,A74,AFF_1:59;
      hence contradiction by A16,A32,A46,A51,AFF_1:33;
    end;
    then consider y such that
A76: y in C and
A77: LIN b',x,y by A4,AFF_1:73;
A78: LIN b',y,x by A77,AFF_1:15;
A79: not LIN b',a',y
    proof
A80:  now
        assume
        x=p;
        then p=b or D=A' by A29,A40,A42,A50,A41,A72,AFF_1:30;
        then LIN b,b',a' or c in A' by A26,A48,A47,A25,AFF_1:26,33;
        then a' in P by A3,A10,A11,A16,A55,AFF_1:39,def 2;
        hence contradiction by A5,A9,A14,AFF_1:59;
      end;
      assume
      LIN b',a',y;
      then
A81:  LIN b',y,a' by AFF_1:15;
      LIN b',y,b' by AFF_1:16;
      then b'=y or LIN a',b',x by A78,A81,AFF_1:17;
      then x in P' by A7,A11,A24,A76,AFF_1:59,def 2;
      then x=p or P'=M by A26,A25,A35,A58,A71,AFF_1:30;
      then N=P' by A45,A47,A36,A80,AFF_1:59;
      hence contradiction by A17,A28,AFF_1:def 2;
    end;
    LIN b,c,x & a,c // p,x by A29,A46,A51,A40,A39,A35,A38,A71,A72,AFF_1:
    33,53;
    then a,c // a',y by A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,A14,A15,A16,
    A20,A21,A76,A78,A79,A73,Def6;
    then a',y // a',c' by A23,A31,AFF_1:14;
    then LIN a',y,c' by AFF_1:def 1;
    then LIN a',c',y by AFF_1:15;
    then y in N by AFF_1:def 2;
    then N=C or y=c' by A4,A13,A34,A28,A76,AFF_1:30;
    then a' in C or LIN b',c',x by A77,AFF_1:15,26;
    then x in T by A6,A9,A15,AFF_1:59,def 2;
    then q in M or c' in D by A29,A30,A43,A27,A71,A72,AFF_1:30;
    then a,c // p,q or LIN c,c',b by A29,A46,A51,A40,A39,A35,A38,AFF_1:
    33 ,53;
    then a,c // p,q or b in C by A4,A12,A13,A53,AFF_1:39;
    hence thesis by A7,A10,A24,AFF_1:59;
  end;
  not M // D or not T // M
  proof
    assume
    not thesis;
    then T // D by AFF_1:58;
    hence contradiction by A27,A54,AFF_1:59;
  end;
  hence thesis by A70,A59;
end;

theorem
  AP is satisfying_DES2_1 iff AP is satisfying_DES2_3
proof
A1: AP is satisfying_DES2_1 implies AP is satisfying_DES2_3
  proof
    assume
A2: AP is satisfying_DES2_1;
    thus AP is satisfying_DES2_3
    proof
      let A,P,C,a,a',b,b',c,c',p,q;
      assume that
A3:   A is being_line and
A4:   P is being_line and
A5:   C is being_line and
A6:   A<>P and
A7:   A<>C and
A8:   P<>C and
A9:   a in A and
A10:  a' in A and
A11:  b in P and
A12:  b' in P and
A13:  c in C and
A14:  c' in C and
A15:  A // P and
A16:  not LIN b,a,c and
A17:  not LIN b',a',c' and
A18:  p<>q and
A19:  c <>c' and
A20:  LIN b,a,p and
A21:  LIN b',a',p and
A22:  LIN b,c,q and
A23:  LIN b',c',q and
A24:  a,c // a',c' and
A25:  a,c // p,q;
      now
        set A'=Line(a,c), P'=Line(p,q), C'=Line(a',c');
A26:    LIN p,a',b' by A21,AFF_1:15;
A27:    a<>c by A16,AFF_1:16;
        then
A28:    A' is being_line & a in A' by AFF_1:38;
A29:    q<>c
        proof
          assume
A30:      q=c;
          then c,p // c,a by A25,AFF_1:13;
          then LIN c,p,a by AFF_1:def 1;
          then
A31:      LIN p,a,c by AFF_1:15;
          LIN p,a,b & LIN p,a,a by A20,AFF_1:15,16;
          then a=p by A16,A31,AFF_1:17;
          then LIN a,a',b' by A21,AFF_1:15;
          then b' in A or a=a' by A3,A9,A10,AFF_1:39;
          then a',c' // a',c by A6,A12,A15,A24,AFF_1:13,59;
          then LIN a',c',c by AFF_1:def 1;
          then LIN c,c',a' by AFF_1:15;
          then
A32:      a' in C by A5,A13,A14,A19,AFF_1:39;
          LIN c,c',b' by A23,A30,AFF_1:15;
          then b' in C by A5,A13,A14,A19,AFF_1:39;
          hence contradiction by A5,A14,A17,A32,AFF_1:33;
        end;
A33:    a<>p
        proof
          assume
          a=p;
          then LIN a,c,q by A25,AFF_1:def 1;
          then
A34:      LIN c,q,a by AFF_1:15;
          LIN c,q,b & LIN c,q,c by A22,AFF_1:15,16;
          hence contradiction by A16,A29,A34,AFF_1:17;
        end;
A35:    a<>a'
        proof
A36:      LIN p,a,b & LIN p,a,a by A20,AFF_1:15,16;
          assume
A37:      a=a';
          then LIN a,c,c' by A24,AFF_1:def 1;
          then LIN c,c',a by AFF_1:15;
          then
A38:      a in C by A5,A13,A14,A19,AFF_1:39;
          LIN p,a,b' by A21,A37,AFF_1:15;
          then b=b' or a in P by A4,A11,A12,A33,A36,AFF_1:17,39;
          then
A39:      LIN q,b,c' by A6,A9,A15,A23,AFF_1:15,59;
          LIN q,b,c & LIN q,b,b by A22,AFF_1:15,16;
          then
A40:      q=b or LIN c,c',b by A39,AFF_1:17;
          not b in C by A5,A13,A16,A38,AFF_1:33;
          then LIN p,q,a by A5,A13,A14,A19,A20,A40,AFF_1:15,39;
          then p,q // p,a by AFF_1:def 1;
          then a,c // p,a by A18,A25,AFF_1:14;
          then a,c // a,p by AFF_1:13;
          then LIN a,c,p by AFF_1:def 1;
          then
A41:      p in C by A5,A13,A27,A38,AFF_1:39;
          LIN p,a,b by A20,AFF_1:15;
          then b in C by A5,A33,A38,A41,AFF_1:39;
          hence contradiction by A5,A13,A16,A38,AFF_1:33;
        end;
A42:    b<>b'
        proof
A43:      p,q // c,a by A25,AFF_1:13;
A44:      LIN q,b,c & LIN q,b,b by A22,AFF_1:15,16;
A45:      LIN p,b,a & LIN p,b,b by A20,AFF_1:15,16;
          assume
A46:      b=b';
          then LIN p,b,a' by A21,AFF_1:15;
          then
A47:      p=b or b in A by A3,A9,A10,A35,A45,AFF_1:17,39;
          LIN q,b,c' by A23,A46,AFF_1:15;
          then b=q or LIN c,c',b by A44,AFF_1:17;
          then
A48:      b in C by A5,A6,A11,A13,A14,A15,A18,A19,A47,AFF_1:39 ,59;
          then q in C by A5,A6,A11,A13,A15,A16,A20,A22,A47,AFF_1:39,59;
          then a in C by A5,A6,A11,A13,A15,A18,A47,A48,A43,AFF_1:59,62;
          hence contradiction by A5,A13,A16,A48,AFF_1:33;
        end;
        then
A49:    a',a // b',b by A3,A4,A9,A10,A11,A12,A15,A35,AFF_1:52;
A50:    a'<>c' by A17,AFF_1:16;
        then
A51:    C' is being_line by AFF_1:38;
A52:    c' in C' by A50,AFF_1:38;
A53:    LIN p,a,b by A20,AFF_1:15;
A54:    not LIN p,a',a
        proof
          assume
          LIN p,a',a;
          then
A55:      LIN p,a,a' by AFF_1:15;
          LIN p,a,a by AFF_1:16;
          then b in A by A3,A9,A10,A33,A35,A53,A55,AFF_1:17,39;
          hence contradiction by A6,A11,A15,AFF_1:59;
        end;
A56:    LIN q,c',b' by A23,AFF_1:15;
A57:    a' in C' by A50,AFF_1:38;
A58:    c in A' by A27,AFF_1:38;
        then
A59:    C' // A' by A24,A27,A50,A51,A28,A57,A52,AFF_1:52;
A60:    A'<>C'
        proof
          assume
A61:      A'=C';
          then LIN a,a',c' by A28,A57,A52,AFF_1:33;
          then
A62:      c' in A by A3,A9,A10,A35,AFF_1:39;
          LIN a,a',c by A28,A58,A57,A61,AFF_1:33;
          then c in A by A3,A9,A10,A35,AFF_1:39;
          hence contradiction by A3,A5,A7,A13,A14,A19,A62,AFF_1:30;
        end;
A63:    LIN q,c,b by A22,AFF_1:15;
A64:    p in P' by A18,AFF_1:38;
A65:    A'<>P'
        proof
          assume
          A'=P';
          then
