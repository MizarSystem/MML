      end;
      then consider x such that
A77:  x in N and
A78:  LIN b9,p,x by A68,AFF_1:73;
      set A9=Line(x,a);
A79:  a<>a9
      proof
        assume
A80:    a=a9;
        ( not LIN o,a,b)& LIN o,b,b9 by A3,A10,A11,A15,A21,AFF_1:15,33;
        hence contradiction by A19,A50,A80,AFF_1:23;
