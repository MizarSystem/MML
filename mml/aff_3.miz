        then
A84:    c,a // c,a9 by A20,AFF_1:13;
        ( not LIN o,c,a)& LIN o,a,a9 by A2,A8,A9,A14,A21,AFF_1:15,33;
        hence contradiction by A79,A84,AFF_1:23;
