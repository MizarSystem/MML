A117: A'<>C'
      proof
        assume
        A'=C';
        then LIN a,a',c by A103,A112,A116,AFF_1:33;
        then c in A by A75,A80,A90,AFF_1:39;
        hence contradiction by A78,A83,A86,AFF_1:59;
      end;
A118: q<>c
      proof
        assume
        q=c;
        then LIN c,c',b' by A94,AFF_1:15;
        then b' in C by A76,A83,A84,A106,AFF_1:39;
        hence contradiction by A79,A82,A104,AFF_1:59;
      end;
A119: A'<>P'
      proof
        assume
        A'=P';
        then
A120:   LIN c,q,a & LIN c,q,c by A103,A112,A97,AFF_1:33;
        LIN c,q,b by A93,AFF_1:15;
        hence contradiction by A87,A118,A120,AFF_1:17;
      end;
A121: LIN q,c,b by A93,AFF_1:15;
A122: not LIN q,c,c'
      proof
A123:   LIN q,c,c by AFF_1:16;
        assume
        LIN q,c,c';
        then b in C by A76,A83,A84,A106,A118,A121,A123,AFF_1:17,39;
        hence contradiction by A79,A81,A104,AFF_1:59;
      end;
A124: C' is being_line & c' in C' by A115,AFF_1:38;
      then A' // C' by A95,A102,A115,A103,A112,A116,AFF_1:52;
      then A' // P' by A74,A102,A107,A103,A112,A114,A97,A116,A124,A113,A105
      ,A121,A96,A101,A119,A117,A122,A99,Def7;
      hence thesis by A112,A114,A97,AFF_1:53;
    end;
  end;
  hence thesis by A1;
end;

theorem
  AP is satisfying_DES1_3 implies AP is satisfying_DES2_1
proof
  assume
A1: AP is satisfying_DES1_3;
  let A,P,C,a,a',b,b',c,c',p,q such that
A2: A is being_line and
A3: P is being_line and
A4: C is being_line and
A5: A<>P and
A6: A<>C and
A7: P<>C and
A8: a in A and
A9: a' in A and
A10: b in P and
A11: b' in P and
A12: c in C and
A13: c' in C and
A14: A // P and
A15: A // C and
A16: not LIN b,a,c and
A17: not LIN b',a',c' and
A18: p<>q and
A19: LIN b,a,p and
A20: LIN b',a',p and
A21: LIN b,c,q and
A22: LIN b',c',q and
A23: a,c // p,q;
A24: P // C by A14,A15,AFF_1:58;
  set K=Line(p,q), M=Line(a,c), N=Line(a',c');
A25: a<>c by A16,AFF_1:16;
  then
A26: a in M by AFF_1:38;
A27: c,c' // a,a' & LIN q,c,b by A8,A9,A12,A13,A15,A21,AFF_1:15,53;
A28: LIN p,a',b' by A20,AFF_1:15;
  C // P by A14,A15,AFF_1:58;
  then
A29: c,c' // b,b' by A10,A11,A12,A13,AFF_1:53;
A30: LIN q,c',b' & LIN p,a,b by A19,A22,AFF_1:15;
A31: c in M by A25,AFF_1:38;
  assume
A32: not thesis;
A33: c <>q
  proof
    assume
A34: c =q;
    then c,a // c,p by A23,AFF_1:13;
    then LIN c,a,p by AFF_1:def 1;
    then
A35: LIN p,a,c by AFF_1:15;
    LIN p,a,b & LIN p,a,a by A19,AFF_1:15,16;
    then p=a by A16,A35,AFF_1:17;
    then LIN a,a',b' by A20,AFF_1:15;
    then
A36: a=a' or b' in A by A2,A8,A9,AFF_1:39;
    LIN c,c',b' by A22,A34,AFF_1:15;
    then c =c' or b' in C by A4,A12,A13,AFF_1:39;
    hence contradiction by A5,A7,A11,A14,A32,A24,A36,AFF_1:11,59;
  end;
A37: c <>c'
  proof
A38: now
      assume
A39:  p=b;
      LIN b,q,c by A21,AFF_1:15;
      then b,q // b,c by AFF_1:def 1;
      then a,c // b,c or b=q by A23,A39,AFF_1:14;
      then c,a // c,b by A18,A39,AFF_1:13;
      then LIN c,a,b by AFF_1:def 1;
      hence contradiction by A16,AFF_1:15;
    end;
A40: LIN q,c,b & LIN q,c,c by A21,AFF_1:15,16;
    assume
A41: c =c';
    then LIN q,c,b' by A22,AFF_1:15;
    then b=b' or c in P by A3,A10,A11,A33,A40,AFF_1:17,39;
    then
A42: LIN p,b,a' by A7,A12,A20,A24,AFF_1:15,59;
    LIN p,b,a & LIN p,b,b by A19,AFF_1:15,16;
    then a=a' or b in A by A2,A8,A9,A42,A38,AFF_1:17,39;
    hence contradiction by A5,A10,A14,A32,A41,AFF_1:11,59;
  end;
A43: b<>b'
  proof
    assume
    b=b';
    then
A44: LIN q,b,c' by A22,AFF_1:15;
    LIN q,b,c & LIN q,b,b by A21,AFF_1:15,16;
    then
A45: q=b or b in C by A4,A12,A13,A37,A44,AFF_1:17,39;
    b,a // b,p by A19,AFF_1:def 1;
    then a,b // p,b by AFF_1:13;
    then a,c // a,b or p=b by A7,A10,A23,A24,A45,AFF_1:14,59;
    then LIN a,c,b by A7,A10,A18,A24,A45,AFF_1:59,def 1;
    hence contradiction by A16,AFF_1:15;
  end;
A46: not LIN q,c,c'
  proof
    assume
A47: LIN q,c,c';
    LIN q,c,b & LIN q,c,c by A21,AFF_1:15,16;
    then b in C by A4,A12,A13,A33,A37,A47,AFF_1:17,39;
    hence contradiction by A7,A10,A24,AFF_1:59;
  end;
A48: a'<>c' by A17,AFF_1:16;
  then
A49: N is being_line by AFF_1:38;
A50: p<>a
  proof
    assume
    p=a;
    then LIN a,c,q by A23,AFF_1:def 1;
    then
A51: LIN c,q,a by AFF_1:15;
    LIN c,q,b & LIN c,q,c by A21,AFF_1:15,16;
    hence contradiction by A16,A33,A51,AFF_1:17;
  end;
A52: not LIN p,a,a'
  proof
    assume
A53: LIN p,a,a';
    LIN p,a,b & LIN p,a,a by A19,AFF_1:15,16;
    then a=a' or b in A by A2,A8,A9,A50,A53,AFF_1:17,39;
    then
A54: LIN p,a,b' by A5,A10,A14,A20,AFF_1:15,59;
    LIN p,a,b & LIN p,a,a by A19,AFF_1:15,16;
    then a in P by A3,A10,A11,A43,A50,A54,AFF_1:17,39;
    hence contradiction by A5,A8,A14,AFF_1:59;
  end;
A55: M is being_line by A25,AFF_1:38;
A56: K is being_line & q in K by A18,AFF_1:38;
A57: now
    assume
    M=K;
    then
A58: LIN q,c,a & LIN q,c,c by A56,A26,A31,AFF_1:33;
    LIN q,c,b by A21,AFF_1:15;
    hence contradiction by A16,A33,A58,AFF_1:17;
  end;
A59: c' in N by A48,AFF_1:38;
A60: a' in N by A48,AFF_1:38;
  then consider x such that
A61: x in M and
A62: x in N by A32,A55,A49,A26,A31,A59,AFF_1:53,72;
A63: now
    assume
    x=c;
    then N=C by A4,A12,A13,A37,A49,A59,A62,AFF_1:30;
    hence contradiction by A6,A9,A15,A60,AFF_1:59;
  end;
A64: p in K by A18,AFF_1:38;
  then
A65: M // K by A18,A23,A25,A55,A56,A26,A31,AFF_1:52;
A66: now
    assume
    x=c';
    then M=C by A4,A12,A13,A37,A55,A31,A61,AFF_1:30;
    hence contradiction by A6,A8,A15,A26,AFF_1:59;
  end;
  M<>N by A32,A55,A26,A31,A60,A59,AFF_1:65;
  then x in K by A1,A18,A25,A55,A49,A64,A56,A26,A31,A60,A59,A61,A62,A29,A27,
  A30,A28,A43,A63,A66,A46,A52,Def4;
  hence contradiction by A65,A61,A57,AFF_1:59;
end;

