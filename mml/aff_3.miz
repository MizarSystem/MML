          then a=p by A16,A31,AFF_1:17;
          then LIN a,a9,b9 by A21,AFF_1:15;
          then b9 in A or a=a9 by A3,A9,A10,AFF_1:39;
          then a9,c9 // a9,c by A6,A12,A15,A24,AFF_1:13,59;
          then LIN a9,c9,c by AFF_1:def 1;
          then LIN c,c9,a9 by AFF_1:15;
          then
A32:      a9 in C by A5,A13,A14,A19,AFF_1:39;
          LIN c,c9,b9 by A23,A30,AFF_1:15;
          then b9 in C by A5,A13,A14,A19,AFF_1:39;
          hence contradiction by A5,A14,A17,A32,AFF_1:33;
