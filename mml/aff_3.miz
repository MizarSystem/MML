        then
A49:    a9,a // b9,b by A3,A4,A9,A10,A11,A12,A15,A35,AFF_1:52;
A50:    a9<>c9 by A17,AFF_1:16;
        then
A51:    C9 is being_line by AFF_1:38;
A52:    c9 in C9 by A50,AFF_1:38;
A53:    LIN p,a,b by A20,AFF_1:15;
A54:    not LIN p,a9,a
        proof
          assume LIN p,a9,a;
          then
A55:      LIN p,a,a9 by AFF_1:15;
          LIN p,a,a by AFF_1:16;
          then b in A by A3,A9,A10,A33,A35,A53,A55,AFF_1:17,39;
