A69:  LIN b,y,a by AFF_1:15;
      LIN b,y,b by AFF_1:16;
      then b=y or LIN a,b,x by A66,A69,AFF_1:17;
      then x in A9 by A7,A10,A24,A64,AFF_1:59,def 2;
      then x=p or A9=M by A42,A41,A35,A58,A61,AFF_1:30;
      then K=A9 by A46,A49,A38,A68,AFF_1:59;
      hence contradiction by A16,A51,AFF_1:def 2;
    end;
    LIN b9,c9,x & a9,c9 // p,x by A30,A45,A28,A44,A43,A35,A36,A60,A61,
    AFF_1:33,53;
    then a9,c9 // a,y by A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A13,A14,A15,A17,
    A20,A21,A64,A66,A67,A62,Def6;
    then a,c // a,y by A23,A33,AFF_1:14;
