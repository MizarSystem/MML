canceled 8;

theorem
  AP is satisfying_DES1 implies AP is satisfying_DES1_1
proof
  assume
A1: AP is satisfying_DES1;
  let A,P,C,o,a,a9,b,b9,c,c9,p,q;
  assume that
A2: A is being_line and
A3: P is being_line and
A4: C is being_line and
A5: P<>A and
A6: P<>C and
A7: A<>C and
A8: o in A and
A9: a in A and
A10: a9 in A and
A11: o in P and
A12: b in P & b9 in P and
A13: o in C & c in C and
A14: c9 in C and
A15: o<>a and
A16: o<>b and
A17: o<>c and
A18: p<>q and
A19: c <>q and
A20: not LIN b,a,c and
A21: not LIN b9,a9,c9 and
A22: LIN b,a,p and
A23: LIN b9,a9,p and
A24: LIN b,c,q and
A25: LIN b9,c9,q and
A26: a,c // p,q;
A27: LIN o,a,a9 & LIN b9,p,a9 by A2,A8,A9,A10,A23,AFF_1:15,33;
  set K=Line(b,a);
A28: a in K by AFF_1:26;
  then
A29: K<>P by A2,A3,A5,A8,A9,A11,A15,AFF_1:30;
A30: not LIN o,a,c
  proof
    assume LIN o,a,c;
    then c in A by A2,A8,A9,A15,AFF_1:39;
    hence contradiction by A2,A4,A7,A8,A13,A17,AFF_1:30;
  end;
A31: p in K by A22,AFF_1:def 2;
A32: LIN o,c,c9 & LIN b9,q,c9 by A4,A13,A14,A25,AFF_1:15,33;
A33: b<>c & a <> p by A19,A20,A24,A26,AFF_1:16,23;
A34: a9<>c9 & b<>a by A20,A21,AFF_1:16;
  b<>a by A20,AFF_1:16;
  then
A35: K is being_line by AFF_1:def 3;
  set M=Line(b,c);
A36: c in M by AFF_1:26;
  then
A37: M<>P by A3,A4,A6,A11,A13,A17,AFF_1:30;
  b<>c by A20,AFF_1:16;
  then
A38: M is being_line by AFF_1:def 3;
A39: b in M & q in M by A24,AFF_1:26,def 2;
  q<>b
  proof
    assume
A40: q=b;
    ( not LIN b,c,a)& c,a // q,p by A20,A26,AFF_1:13,15;
    hence contradiction by A18,A22,A40,AFF_1:69;
  end;
  then
A41: q<>b9 by A3,A12,A38,A39,A37,AFF_1:30;
A42: b in K by AFF_1:26;
  p<>b by A18,A20,A24,A26,AFF_1:69;
  then
A43: p<>b9 by A3,A12,A35,A42,A31,A29,AFF_1:30;
A44: not LIN b9,p,q
  proof
    set N=Line(p,q);
A45: N is being_line by A18,AFF_1:def 3;
    assume LIN b9,p,q;
    then LIN p,q,b9 by AFF_1:15;
    then
A46: b9 in N by AFF_1:def 2;
    q in N & LIN q,b9,c9 by A25,AFF_1:15,26;
    then
A47: c9 in N by A41,A45,A46,AFF_1:39;
    p in N & LIN p,b9,a9 by A23,AFF_1:15,26;
    then a9 in N by A43,A45,A46,AFF_1:39;
    hence contradiction by A21,A45,A46,A47,AFF_1:33;
  end;
  K<>M by A20,A35,A42,A28,A36,AFF_1:33;
  hence
  thesis by A1,A3,A11,A12,A16,A26,A35,A38,A42,A28,A36,A31,A39,A37,A29,A34
  ,A30,A44,A33,A27,A32,Def1;
end;

theorem
  AP is satisfying_DES1_1 implies AP is satisfying_DES1
proof
  assume
A1: AP is satisfying_DES1_1;
  let A,P,C,o,a,a9,b,b9,c,c9,p,q;
  assume that
A2: A is being_line and
A3: P is being_line and
A4: C is being_line and
A5: P<>A and
A6: P<>C and
A7: A<>C and
A8: o in A and
A9: a in A and
A10: a9 in A and
A11: o in P and
A12: b in P and
A13: b9 in P and
A14: o in C and
A15: c in C and
A16: c9 in C and
A17: o<>a and
A18: o<>b and
A19: o<>c and
A20: p<>q and
A21: not LIN b,a,c and
A22: not LIN b9,a9,c9 and
A23: a<>a9 and
A24: LIN b,a,p and
A25: LIN b9,a9,p and
A26: LIN b,c,q and
A27: LIN b9,c9,q and
A28: a,c // a9,c9;
A29: a9<>b9 by A22,AFF_1:16;
  set M=Line(b,c);
A30: c in M by AFF_1:26;
  then
A31: M<>P by A3,A4,A6,A11,A14,A15,A19,AFF_1:30;
A32: M<>P by A3,A4,A6,A11,A14,A15,A19,A30,AFF_1:30;
A33: b in M by AFF_1:26;
  set K=Line(b,a);
A34: a in K by AFF_1:26;
  then
A35: K<>P by A2,A3,A5,A8,A9,A11,A17,AFF_1:30;
A36: p in K by A24,AFF_1:def 2;
A37: a9<>c9 & b<>a by A21,A22,AFF_1:16;
A38: b<>c by A21,AFF_1:16;
A39: q in M by A26,AFF_1:def 2;
A40: b9<>c9 by A22,AFF_1:16;
A41: b in K by AFF_1:26;
A42: not LIN o,a,c
  proof
    assume LIN o,a,c;
    then c in A by A2,A8,A9,A17,AFF_1:39;
    hence contradiction by A2,A4,A7,A8,A14,A15,A19,AFF_1:30;
  end;
A43: c <>c9
  proof
    assume c =c9;
    then
A44: c,a // c,a9 by A28,AFF_1:13;
    LIN o,a,a9 & not LIN o,c,a by A2,A8,A9,A10,A42,AFF_1:15,33;
    hence contradiction by A23,A44,AFF_1:23;
  end;
  b<>c by A21,AFF_1:16;
  then
A45: M is being_line by AFF_1:def 3;
  b<>a by A21,AFF_1:16;
  then
A46: K is being_line by AFF_1:def 3;
A47: K<>P by A2,A3,A5,A8,A9,A11,A17,A34,AFF_1:30;
A48: now
    set C9=Line(b9,c9);
    set A9=Line(b9,a9);
A49: c9 in C9 by AFF_1:26;
A50: A9 is being_line & b9 in A9 by A29,AFF_1:26,def 3;
A51: a9 in A9 by AFF_1:26;
    then
A52: A9<>C9 by A22,A50,A49,AFF_1:33;
A53: q in C9 by A27,AFF_1:def 2;
A54: p in A9 by A25,AFF_1:def 2;
A55: C9 is being_line & b9 in C9 by A40,AFF_1:26,def 3;
    assume
A56: LIN b9,p,q;
    then
A57: LIN b9,q,p by AFF_1:15;
A58: now
A59:  C9<>M
      proof
        assume C9=M;
        then LIN c,c9,b by A45,A33,A30,A49,AFF_1:33;
        then b in C by A4,A15,A16,A43,AFF_1:39;
        hence contradiction by A3,A4,A6,A11,A12,A14,A18,AFF_1:30;
      end;
      assume b9<>q;
      then
A60:  p in C9 by A57,A55,A53,AFF_1:39;
      then LIN b,a,b9 by A24,A50,A54,A55,A52,AFF_1:30;
      then b9 in K by AFF_1:def 2;
      then
A61:  b=b9 by A3,A12,A13,A46,A41,A47,AFF_1:30;
      p=b9 by A50,A54,A55,A52,A60,AFF_1:30;
      then p=q by A45,A33,A39,A55,A53,A61,A59,AFF_1:30;
      hence thesis by AFF_1:12;
    end;
    now
A62:  A9<>K
      proof
        assume A9=K;
        then LIN a,a9,b by A46,A41,A34,A51,AFF_1:33;
        then b in A by A2,A9,A10,A23,AFF_1:39;
        hence contradiction by A2,A3,A5,A8,A11,A12,A18,AFF_1:30;
      end;
      assume b9<>p;
      then
A63:  q in A9 by A56,A50,A54,AFF_1:39;
      then LIN b,c,b9 by A26,A50,A55,A53,A52,AFF_1:30;
      then b9 in M by AFF_1:def 2;
      then
A64:  b=b9 by A3,A12,A13,A45,A33,A32,AFF_1:30;
      q=b9 by A50,A55,A53,A52,A63,AFF_1:30;
      then p=q by A46,A41,A36,A50,A54,A64,A62,AFF_1:30;
      hence thesis by AFF_1:12;
    end;
    hence thesis by A20,A58;
  end;
A65: K<>M by A21,A46,A41,A34,A30,AFF_1:33;
  now
A66: LIN o,c,c9 & LIN b9,q,c9 by A4,A14,A15,A16,A27,AFF_1:15,33;
    assume
A67: not LIN b9,p,q;
    LIN o,a,a9 & LIN b9,p,a9 by A2,A8,A9,A10,A25,AFF_1:15,33;
    hence
    thesis by A1,A3,A11,A12,A13,A18,A28,A46,A45,A41,A34,A33,A30,A36,A39,
    A31,A35,A37,A38,A65,A42,A43,A67,A66,Def2;
  end;
  hence thesis by A48;
end;

theorem
  AP is Desarguesian implies AP is satisfying_DES1
proof
  assume
A1: AP is Desarguesian;
  let A,P,C,o,a,a9,b,b9,c,c9,p,q;
  assume that
A2: A is being_line and
A3: P is being_line and
A4: C is being_line and
A5: P<>A and
A6: P<>C and
A7: A<>C and
A8: o in A and
A9: a in A and
A10: a9 in A and
A11: o in P and
A12: b in P and
A13: b9 in P and
A14: o in C and
A15: c in C and
A16: c9 in C and
A17: o<>a and
A18: o<>b and
A19: o<>c and
  p<>q and
A20: not LIN b,a,c and
A21: not LIN b9,a9,c9 and
A22: a<>a9 and
A23: LIN b,a,p and
A24: LIN b9,a9,p and
A25: LIN b,c,q and
A26: LIN b9,c9,q and
A27: a,c // a9,c9;
  set D=Line(b,c);
  b<>c by A20,AFF_1:16;
  then D is being_line by AFF_1:def 3;
  then consider D9 such that
A28: c9 in D9 and
A29: D // D9 by AFF_1:63;
A30: D9 is being_line by A29,AFF_1:50;
  set M=Line(b9,c9);
A31: q in M by A26,AFF_1:def 2;
A32: b in D by AFF_1:26;
A33: c in D by AFF_1:26;
  not D9 // P
  proof
    assume D9 // P;
    then D // P by A29,AFF_1:58;
    then c in P by A12,A32,A33,AFF_1:59;
    hence contradiction by A3,A4,A6,A11,A14,A15,A19,AFF_1:30;
  end;
  then consider d such that
A34: d in D9 and
A35: d in P by A3,A30,AFF_1:72;
A36: q in D by A25,AFF_1:def 2;
  then
A37: d,c9 // b,q by A32,A28,A29,A34,AFF_1:53;
A38: a<>b & b,a // b,p by A20,A23,AFF_1:16,def 1;
A39: c,a // c9,a9 by A27,AFF_1:13;
  c,b // c9,d by A32,A33,A28,A29,A34,AFF_1:53;
  then b,a // d,a9 by A1,A2,A3,A4,A6,A7,A8,A9,A10,A11,A12,A14,A15,A16,A17,A18
  ,A19,A35,A39,AFF_2:def 4;
  then
A40: d,a9 // b,p by A38,AFF_1:14;
  set K=Line(b9,a9);
A41: b9 in K & p in K by A24,AFF_1:26,def 2;
A42: a9<>b9 by A21,AFF_1:16;
  then
A43: K is being_line by AFF_1:def 3;
A44: b9 in M by AFF_1:26;
A45: c9 in M by AFF_1:26;
A46: b9<>c9 by A21,AFF_1:16;
  then
A47: M is being_line by AFF_1:def 3;
A48: not LIN o,a,c
  proof
    assume LIN o,a,c;
    then c in A by A2,A8,A9,A17,AFF_1:39;
    hence contradiction by A2,A4,A7,A8,A14,A15,A19,AFF_1:30;
  end;
A49: c <>c9
  proof
    assume c =c9;
    then
A50: c,a // c,a9 by A27,AFF_1:13;
    LIN o,a,a9 & not LIN o,c,a by A2,A8,A9,A10,A48,AFF_1:15,33;
    hence contradiction by A22,A50,AFF_1:23;
  end;
A51: d<>b9
  proof
    assume d=b9;
    then M=D9 by A46,A47,A44,A45,A28,A30,A34,AFF_1:30;
    then D=M by A31,A36,A29,AFF_1:59;
    then LIN c,c9,b by A47,A45,A32,A33,AFF_1:33;
    then b in C by A4,A15,A16,A49,AFF_1:39;
    hence contradiction by A3,A4,A6,A11,A12,A14,A18,AFF_1:30;
  end;
A52: a9<>c9 by A21,AFF_1:16;
A53: o<>a9
  proof
    assume
A54: o=a9;
    LIN o,c,c9 by A4,A14,A15,A16,AFF_1:33;
    hence contradiction by A27,A52,A48,A54,AFF_1:69;
  end;
  o<>c9
  proof
A55: not LIN o,c,a by A48,AFF_1:15;
    assume
A56: o=c9;
    LIN o,a,a9 & c,a // c9,a9 by A2,A8,A9,A10,A27,AFF_1:13,33;
    hence contradiction by A53,A56,A55,AFF_1:69;
  end;
  then
A57: M<>P by A3,A4,A6,A11,A14,A16,A45,AFF_1:30;
A58: a9 in K by AFF_1:26;
  then K<>P by A2,A3,A5,A8,A10,A11,A53,AFF_1:30;
  then a9,c9 // p,q by A1,A3,A12,A13,A42,A46,A43,A47,A58,A41,A44,A45,A31,
  A57,A35,A51,A40,A37,AFF_2:def 4;
  hence thesis by A27,A52,AFF_1:14;
end;

theorem
  AP is Desarguesian implies AP is satisfying_DES1_2
proof
  assume
A1: AP is Desarguesian;
  then
A2: AP is satisfying_DES_1 by AFF_2:16;
  let A,P,C,o,a,a9,b,b9,c,c9,p,q;
  assume that
A3: A is being_line and
A4: P is being_line and
A5: C is being_line and
A6: P<>A and
A7: P<>C and
  A<>C and
A8: o in A and
A9: a in A & a9 in A and
A10: o in P and
A11: b in P and
A12: b9 in P and
A13: c in C and
A14: c9 in C and
A15: o<>a and
A16: o<>b and
  o<>c and
A17: p<>q and
A18: not LIN b,a,c and
A19: not LIN b9,a9,c9 and
A20: c <>c9 and
A21: LIN b,a,p and
A22: LIN b9,a9,p and
A23: LIN b,c,q and
A24: LIN b9,c9,q and
A25: a,c // a9,c9 and
A26: a,c // p,q;
A27: b<>p by A17,A18,A23,A26,AFF_1:69;
  set K=Line(b9,a9);
A28: p in K by A22,AFF_1:def 2;
  a9<>b9 by A19,AFF_1:16;
  then
A29: K is being_line by AFF_1:def 3;
A30: b<>q
  proof
    assume
A31: b=q;
    ( not LIN b,c,a)& c,a // q,p by A18,A26,AFF_1:13,15;
    hence contradiction by A17,A21,A31,AFF_1:69;
  end;
  set M=Line(b9,c9);
