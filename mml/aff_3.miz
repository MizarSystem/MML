A56:    LIN q,c9,b9 by A23,AFF_1:15;
A57:    a9 in C9 by A50,AFF_1:38;
A58:    c in A9 by A27,AFF_1:38;
        then
A59:    C9 // A9 by A24,A27,A50,A51,A28,A57,A52,AFF_1:52;
A60:    A9<>C9
        proof
          assume
A61:      A9=C9;
