      then CL <> {};
     then consider y being Element of R such that y in CL and
  A8: y is_minimal_wrt CL, iP by BAGORDER:7;
  A9: y is_minimal_in CL by A8,WAYBEL_4:def 26;
  A10: cP = [#]R;
     now
       assume not y is_minimal_in cP;
