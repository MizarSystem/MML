A6: (GoB f)*(len GoB f,j+1)`1 = (GoB f)*(len GoB f,1)`1 by A3,GOBOARD5:3;
    1 <= j+2 by A1,A4,XXREAL_0:2;
    then
A7: (GoB f)*(len GoB f,j+2)`1 = (GoB f)*(len GoB f,1)`1 by A2,A3,GOBOARD5:3;
    (1/2*((GoB f)*(len GoB f,j+1)+(GoB f)*(len GoB f,j+2))+|[1,0]|)`1 = (
1/2*((GoB f)*(len GoB f,j+1)+(GoB f)*(len GoB f,j+2)))`1+|[1,0]|`1 by
TOPREAL3:7
      .= 1/2*((GoB f)*(len GoB f,j+1)+(GoB f)*(len GoB f,j+2))`1+|[1,0]|`1
    by TOPREAL3:9
      .= 1/2*((GoB f)*(len GoB f,1)`1+(GoB f)*(len GoB f,1)`1)+|[1,0]|`1 by A6
,A7,TOPREAL3:7
      .= 1*((GoB f)*(len GoB f,1))`1+1 by EUCLID:56;
    then
A8: 1/2*((GoB f)*(len GoB f,j+1)+(GoB f)*(len GoB f,j+2))+|[1,0]| = |[(
GoB f)*(len GoB f,1)`1+1,(1/2*((GoB f)*(len GoB f,j+1) +(GoB f)*(len GoB f,j+2)
    )+|[1,0]|)`2]| by EUCLID:57;
    (1/2*((GoB f)*(len GoB f,j)+(GoB f)*(len GoB f,j+1))+|[1,0]|)`1 = (1/
2*((GoB f)*(len GoB f,j)+(GoB f)*(len GoB f,j+1)))`1+|[1,0]|`1 by TOPREAL3:7
      .= 1/2*((GoB f)*(len GoB f,j)+(GoB f)*(len GoB f,j+1))`1+|[1,0]|`1 by
TOPREAL3:9
      .= 1/2*((GoB f)*(len GoB f,1)`1+(GoB f)*(len GoB f,1)`1)+|[1,0]|`1 by A5
,A6,TOPREAL3:7
      .= 1*((GoB f)*(len GoB f,1))`1+1 by EUCLID:56;
    then
A9: 1/2*((GoB f)*(len GoB f,j)+(GoB f)*(len GoB f,j+1))+|[1,0]| = |[(GoB
f)*(len GoB f,1)`1+1,(1/2*((GoB f)*(len GoB f,j) +(GoB f)*(len GoB f,j+1))+|[1,
    0]|)`2]| by EUCLID:57;
    let p;
    assume
    p in LSeg(1/2*((GoB f)*(len GoB f,j)+(GoB f)*(len GoB f,j+1))+|[1,
    0]|, 1/2*((GoB f)*(len GoB f,j+1)+(GoB f)*(len GoB f,j+2))+|[1,0]|);
    then p`1 = (GoB f)*(len GoB f,1)`1 + 1 by A9,A8,TOPREAL3:17;
    hence p`1 > (GoB f)*(len GoB f,1)`1 by XREAL_1:31;
  end;
  hence thesis by Th22;
end;

theorem
  LSeg((GoB f)*(len GoB f,1)+|[1,-1]|, 1/2*((GoB f)*(len GoB f,1)+(GoB f
  )*(len GoB f,2))+|[1,0]|) misses L~f
proof
A1: 1 <= len GoB f by GOBOARD7:34;
  now
    1 < width GoB f by GOBOARD7:35;
    then 1+1 <= width GoB f by NAT_1:13;
    then
A2: (GoB f)*(len GoB f,2)`1 = (GoB f)*(len GoB f,1)`1 by A1,GOBOARD5:3;
    (1/2*((GoB f)*(len GoB f,1)+(GoB f)*(len GoB f,2))+|[1,0]|)`1 = (1/2*(
    (GoB f)*(len GoB f,1)+(GoB f)*(len GoB f,2)))`1+|[1,0]|`1 by TOPREAL3:7
      .= 1/2*((GoB f)*(len GoB f,1)+(GoB f)*(len GoB f,2))`1+|[1,0]|`1 by
TOPREAL3:9
      .= 1/2*((GoB f)*(len GoB f,1)`1+(GoB f)*(len GoB f,1)`1)+|[1,0]|`1 by A2,
TOPREAL3:7
      .= 1*((GoB f)*(len GoB f,1))`1+1 by EUCLID:56;
    then
A3: 1/2*((GoB f)*(len GoB f,1)+(GoB f)*(len GoB f,2))+|[1,0]| = |[(GoB f)
*(len GoB f,1)`1+1, (1/2*((GoB f)*(len GoB f,1)+(GoB f)*(len GoB f,2))+|[1,0]|)
    `2]| by EUCLID:57;
    ((GoB f)*(len GoB f,1)+|[1,-1]|)`1 = ((GoB f)*(len GoB f,1))`1+|[1,-1
    ]|`1 by TOPREAL3:7
      .= (GoB f)*(len GoB f,1)`1+1 by EUCLID:56;
    then
A4: (GoB f)*(len GoB f,1)+|[1,-1]| = |[(GoB f)*(len GoB f,1)`1+1,((GoB f)*
    (len GoB f,1)+|[1,-1]|)`2]| by EUCLID:57;
    let p;
    assume
    p in LSeg((GoB f)*(len GoB f,1)+|[1,-1]|, 1/2*((GoB f)*(len GoB f,
    1)+(GoB f)*(len GoB f,2))+|[1,0]|);
    then p`1 = (GoB f)*(len GoB f,1)`1 + 1 by A4,A3,TOPREAL3:17;
    hence p`1 > (GoB f)*(len GoB f,1)`1 by XREAL_1:31;
  end;
  hence thesis by Th22;
end;

theorem
  LSeg(1/2*((GoB f)*(len GoB f,width GoB f -' 1)+ (GoB f)*(len GoB f,
  width GoB f))+|[1,0]|, (GoB f)*(len GoB f,width GoB f)+|[1,1]|) misses L~f
proof
A1: 1 <= len GoB f by GOBOARD7:34;
  now
A2: 1 < width GoB f by GOBOARD7:35;
    then
A3: width GoB f -' 1 +1 = width GoB f by XREAL_1:237;
    then
A4: width GoB f -' 1 <= width GoB f by NAT_1:11;
A5: (GoB f)*(len GoB f,width GoB f)`1 = (GoB f)*(len GoB f,1)`1 by A1,A2,
GOBOARD5:3;
    then
    ((GoB f)*(len GoB f,width GoB f)+|[1,1]|)`1 = (GoB f)*(len GoB f,1)`1
    +|[1,1]|`1 by TOPREAL3:7
      .= (GoB f)*(len GoB f,1)`1+1 by EUCLID:56;
    then
A6: (GoB f)*(len GoB f,width GoB f)+|[1,1]| = |[(GoB f)*(len GoB f,1)`1+1
    , ((GoB f)*(len GoB f,width GoB f)+|[1,1]|)`2 ]| by EUCLID:57;
    1 <= width GoB f -' 1 by A2,A3,NAT_1:13;
    then
A7: (GoB f)*(len GoB f,width GoB f -' 1)`1 = (GoB f)*(len GoB f,1)`1 by A1,A4,
GOBOARD5:3;
    (1/2*((GoB f)*(len GoB f,width GoB f -' 1) +(GoB f)*(len GoB f,width
GoB f))+|[1,0]|)`1 = (1/2*((GoB f)*(len GoB f,width GoB f -' 1) +(GoB f)*(len
    GoB f,width GoB f)))`1+|[1,0]|`1 by TOPREAL3:7
      .= 1/2*((GoB f)*(len GoB f,width GoB f -' 1) +(GoB f)*(len GoB f,width
    GoB f))`1+|[1,0]|`1 by TOPREAL3:9
      .= 1/2*((GoB f)*(len GoB f,1)`1+(GoB f)*(len GoB f,1)`1)+|[1,0]|`1 by A7
,A5,TOPREAL3:7
      .= 1*((GoB f)*(len GoB f,1))`1+1 by EUCLID:56;
    then
A8: 1/2*((GoB f)*(len GoB f,width GoB f -' 1) +(GoB f)*(len GoB f,width
GoB f))+|[1,0]| = |[(GoB f)*(len GoB f,1)`1+1,(1/2*((GoB f)* (len GoB f,width
    GoB f -' 1) +(GoB f)*(len GoB f,width GoB f))+|[1,0]|)`2 ]| by EUCLID:57;
    let p;
    assume
    p in LSeg(1/2*((GoB f)*(len GoB f,width GoB f -' 1)+ (GoB f)*(len
    GoB f,width GoB f))+|[1,0]|, (GoB f)*(len GoB f,width GoB f)+|[1,1]|);
    then p`1 = (GoB f)*(len GoB f,1)`1 + 1 by A8,A6,TOPREAL3:17;
    hence p`1 > (GoB f)*(len GoB f,1)`1 by XREAL_1:31;
  end;
  hence thesis by Th22;
end;

theorem
  1 <= k & k+1 <= len f implies Int left_cell(f,k) misses L~f
proof
  assume that
A1: 1 <= k and
A2: k+1 <= len f;
  k <= k+1 by NAT_1:11;
  then k <= len f by A2,XXREAL_0:2;
  then
A3: k in dom f by A1,FINSEQ_3:27;
  then consider i1,j1 being Element of NAT such that
A4: [i1,j1] in Indices GoB f and
A5: f/.k = (GoB f)*(i1,j1) by GOBOARD2:25;
A6: i1 <= len GoB f by A4,MATRIX_1:39;
  j1 <> 0 by A4,MATRIX_1:39;
  then consider j being Nat such that
A7: j1 = j+1 by NAT_1:6;
  i1 <> 0 by A4,MATRIX_1:39;
  then consider i being Nat such that
A8: i1 = i+1 by NAT_1:6;
  i <= i1 by A8,NAT_1:11;
  then
A9: i <= len GoB f by A6,XXREAL_0:2;
A10: j1 <= width GoB f by A4,MATRIX_1:39;
  k+1 >= 1 by NAT_1:11;
  then
A11: k+1 in dom f by A2,FINSEQ_3:27;
  then consider i2,j2 being Element of NAT such that
A12: [i2,j2] in Indices GoB f and
A13: f/.(k+1) = (GoB f)*(i2,j2) by GOBOARD2:25;
  reconsider i19=i1, i29=i2, j19=j1, j29=j2 as Element of REAL by XREAL_0:def 1
;
  abs(i1-i2)+abs(j1-j2) = 1 by A3,A4,A5,A11,A12,A13,GOBOARD5:13;
  then
A14: abs(i19-i29) = 1 & j1 = j2 or abs(j19-j29)=1 & i1 = i2 by SEQM_3:82;
  reconsider i,j as Element of NAT by ORDINAL1:def 13;
A15: i1 = i+1 by A8;
A16: j1 = j+1 by A7;
A17: j2 <= width GoB f by A12,MATRIX_1:39;
A18: i2 <= len GoB f by A12,MATRIX_1:39;
  j <= j1 by A7,NAT_1:11;
  then
A19: j <= width GoB f by A10,XXREAL_0:2;
  per cases by A14,SEQM_3:81;
  suppose
    i1 = i2 & j1+1 = j2;
    then left_cell(f,k) = cell(GoB f,i,j1) by A1,A2,A4,A5,A8,A12,A13,
GOBOARD5:28;
    hence thesis by A10,A9,GOBOARD7:14;
  end;
  suppose
    i1+1 = i2 & j1 = j2;
    then left_cell(f,k) = cell(GoB f,i1,j1) by A1,A2,A4,A5,A16,A12,A13,
GOBOARD5:29;
    hence thesis by A6,A10,GOBOARD7:14;
  end;
  suppose
    i1 = i2+1 & j1 = j2;
    then left_cell(f,k) = cell(GoB f,i2,j) by A1,A2,A4,A5,A7,A12,A13,
GOBOARD5:30;
    hence thesis by A19,A18,GOBOARD7:14;
  end;
  suppose
    i1 = i2 & j1 = j2+1;
    then left_cell(f,k) = cell(GoB f,i1,j2) by A1,A2,A4,A5,A15,A12,A13,
GOBOARD5:31;
    hence thesis by A6,A17,GOBOARD7:14;
  end;
end;

theorem
  1 <= k & k+1 <= len f implies Int right_cell(f,k) misses L~f
proof
  assume that
A1: 1 <= k and
A2: k+1 <= len f;
  k <= k+1 by NAT_1:11;
  then k <= len f by A2,XXREAL_0:2;
  then
A3: k in dom f by A1,FINSEQ_3:27;
  then consider i1,j1 being Element of NAT such that
A4: [i1,j1] in Indices GoB f and
A5: f/.k = (GoB f)*(i1,j1) by GOBOARD2:25;
A6: i1 <= len GoB f by A4,MATRIX_1:39;
  j1 <> 0 by A4,MATRIX_1:39;
  then consider j being Nat such that
A7: j1 = j+1 by NAT_1:6;
  i1 <> 0 by A4,MATRIX_1:39;
  then consider i being Nat such that
A8: i1 = i+1 by NAT_1:6;
  i <= i1 by A8,NAT_1:11;
  then
A9: i <= len GoB f by A6,XXREAL_0:2;
A10: j1 <= width GoB f by A4,MATRIX_1:39;
  k+1 >= 1 by NAT_1:11;
  then
A11: k+1 in dom f by A2,FINSEQ_3:27;
  then consider i2,j2 being Element of NAT such that
A12: [i2,j2] in Indices GoB f and
A13: f/.(k+1) = (GoB f)*(i2,j2) by GOBOARD2:25;
  reconsider i19=i1, i29=i2, j19=j1, j29=j2 as Element of REAL by XREAL_0:def 1
;
  abs(i1-i2)+abs(j1-j2) = 1 by A3,A4,A5,A11,A12,A13,GOBOARD5:13;
  then
A14: abs(i19-i29) = 1 & j1 = j2 or abs(j19-j29)=1 & i1 = i2 by SEQM_3:82;
  reconsider i,j as Element of NAT by ORDINAL1:def 13;
A15: i1 = i+1 by A8;
A16: j1 = j+1 by A7;
A17: j2 <= width GoB f by A12,MATRIX_1:39;
A18: i2 <= len GoB f by A12,MATRIX_1:39;
  j <= j1 by A7,NAT_1:11;
  then
A19: j <= width GoB f by A10,XXREAL_0:2;
  per cases by A14,SEQM_3:81;
  suppose
    i1 = i2 & j1+1 = j2;
    then right_cell(f,k) = cell(GoB f,i1,j1) by A1,A2,A4,A5,A15,A12,A13,
GOBOARD5:28;
    hence thesis by A6,A10,GOBOARD7:14;
  end;
  suppose
    i1+1 = i2 & j1 = j2;
    then right_cell(f,k) = cell(GoB f,i1,j) by A1,A2,A4,A5,A7,A12,A13,
GOBOARD5:29;
    hence thesis by A6,A19,GOBOARD7:14;
  end;
  suppose
    i1 = i2+1 & j1 = j2;
    then right_cell(f,k) = cell(GoB f,i2,j1) by A1,A2,A4,A5,A16,A12,A13,
GOBOARD5:30;
    hence thesis by A10,A18,GOBOARD7:14;
  end;
  suppose
    i1 = i2 & j1 = j2+1;
    then right_cell(f,k) = cell(GoB f,i,j2) by A1,A2,A4,A5,A8,A12,A13,
GOBOARD5:31;
    hence thesis by A9,A17,GOBOARD7:14;
  end;
end;

