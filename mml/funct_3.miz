A6: dom f1 = [:X,Y:] and
A7: for x,y st x in X & y in Y holds f1.(x,y) = y and
A8: dom f2 = [:X,Y:] and
A9: for x,y st x in X & y in Y holds f2.(x,y) = y;
    for x,y st x in X & y in Y holds f1.(x,y) = f2.(x,y)
    proof
