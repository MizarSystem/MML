A76:  ss' = H.w2 by A74;
      ex z be set st z in dom F & z in Poz & F.s21 = F.z by A5,A16,A23,
      FUNCT_1:def 12;
      then
A77:  s21 in Poz by A15,A67,FUNCT_1:def 8;
      then hh.s21 = g.s2 by A16,A39,A27,FUNCT_1:72;
      then s21 = (hh qua Function)".(g.s2) by A55,A47,A77,FUNCT_1:54;
      then
A78:  s21 = hh".(g.s2) by A43,A55,A48,TOPS_2:def 4;
      s2 in dom g by A40,A41,A60,BORSUK_1:86;
      then s21 = H.w1 by A78,FUNCT_1:23;
      then ss' < s21 by A20,A50,A73,A72,A42,A32,A31,A76,JORDAN5A:16
      ,TOPMETR:27;
      hence thesis by A1,A7,A6,A13,A14,A16,A18,A21,A74,A75,A71,Def1;
    end;
    LSeg (f,i) is closed by SPPOL_1:40;
    then
A79: LSeg (f, i) /\ Q is closed by A2,TOPS_1:35;
    LSeg (f, i) /\ Q <> {} by A5,A8,XBOOLE_0:def 4;
    then
A80: LSeg (f, i) meets Q by XBOOLE_0:def 7;
    LSeg (f,i) = LSeg (f/.i, f/.(i+1)) by A4,TOPREAL1:def 5;
    then
A81: R is_an_arc_of f/.i, f/.(i+1) by A11,TOPREAL1:15;
    FPG in LSeg (f,i) /\ Q by A5,A8,XBOOLE_0:def 4;
    hence thesis by A80,A79,A81,A19,Def1;
  end;
  hence thesis;
end;

