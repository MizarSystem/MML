end;

theorem Th23:
  for f be FinSequence of TOP-REAL 2, i be Element of NAT st f is
being_S-Seq & 1 <= i & i+1 <= len f holds LE f/.i, f/.(i+1), L~f, f/.1, f/.len
  f
proof
  let f be FinSequence of TOP-REAL 2, i be Element of NAT;
  assume that
A1: f is being_S-Seq and
A2: 1 <= i & i+1 <= len f;
  set p1 = f/.1, p2 = f/.len f, q1 = f/.i, q2 = f/.(i+1);
A3: len f >= 2 by A1,TOPREAL1:def 10;
  then reconsider P = L~f as non empty Subset of TOP-REAL 2 by TOPREAL1:29;
  i+1 in dom f by A2,GOBOARD2:3;
  then
A4: q2 in P by A3,GOBOARD1:16;
A5: for g being Function of I[01], (TOP-REAL 2)|P, s1,s2 be Real st g is
being_homeomorphism & g.0=p1 & g.1=p2 & g.s1=q1 & 0<=s1 & s1<=1 & g.s2=q2 & 0<=
  s2 & s2<=1 holds s1<=s2
  proof
    let g be Function of I[01], (TOP-REAL 2)|P, s1,s2 be Real;
    assume that
A6: g is being_homeomorphism and
A7: g.0 = p1 & g.1 = p2 and
A8: g.s1 = q1 and
A9: 0 <= s1 & s1 <= 1 and
A10: g.s2 = q2 and
A11: 0 <= s2 & s2 <= 1;
A12: dom g = [#]I[01] by A6,TOPS_2:def 5
      .= the carrier of I[01];
    then
A13: s1 in dom g by A9,BORSUK_1:86;
A14: s2 in dom g by A11,A12,BORSUK_1:86;
A15: g is one-to-one by A6,TOPS_2:def 5;
    consider r1, r2 be Real such that
A16: r1 < r2 and
A17: 0 <= r1 and
A18: r1 <= 1 and
    0 <= r2 and
A19: r2 <= 1 and
    LSeg (f, i) = g.:[.r1, r2.] and
A20: g.r1 = q1 and
A21: g.r2 = q2 by A1,A2,A6,A7,JORDAN5B:7;
A22: r2 in dom g by A16,A17,A19,A12,BORSUK_1:86;
    r1 in dom g by A17,A18,A12,BORSUK_1:86;
    then s1 = r1 by A8,A20,A13,A15,FUNCT_1:def 8;
    hence thesis by A10,A16,A21,A22,A14,A15,FUNCT_1:def 8;
  end;
  i in dom f by A2,GOBOARD2:3;
  then q1 in P by A3,GOBOARD1:16;
  hence thesis by A4,A5,Def3;
end;

theorem Th24:
  for f be FinSequence of TOP-REAL 2, i, j be Element of NAT st f
is being_S-Seq & 1 <= i & i <= j & j <= len f holds LE f/.i, f/.j, L~f, f/.1, f
  /.len f
proof
  let f be FinSequence of TOP-REAL 2, i, j be Element of NAT such that
A1: f is being_S-Seq and
A2: 1 <= i and
A3: i <= j and
A4: j <= len f;
  consider k being Nat such that
A5: i+k = j by A3,NAT_1:10;
A6: len f >= 2 by A1,TOPREAL1:def 10;
  then reconsider P = L~f as non empty Subset of TOP-REAL 2 by TOPREAL1:29;
  defpred ILE[Element of NAT] means 1 <= i & i+$1 <= len f implies LE f/.i, f
  /.(i+$1), P, f/.1, f/.len f;
A7: for l be Element of NAT st ILE[l] holds ILE[l + 1]
  proof
    let l be Element of NAT;
    assume
A8: ILE[l];
A9: i+l <= i+l+1 by NAT_1:11;
    assume that
A10: 1 <= i and
A11: i+(l+1) <= len f;
A12: i+l+1 <= len f by A11;
    i <= i+l by NAT_1:11;
    then 1 <= i+l by A10,XXREAL_0:2;
    then LE f/.(i+l), f/.(i+(l+1)), P, f/.1,f/.len f by A1,A12,Th23;
    hence thesis by A8,A10,A11,A9,Th13,XXREAL_0:2;
  end;
A13: ILE[0]
  proof
    assume
    1 <= i & i+0 <= len f;
    then i in dom f by FINSEQ_3:27;
    hence thesis by A6,Th9,GOBOARD1:16;
  end;
A14: for l be Element of NAT holds ILE[l] from NAT_1:sch 1(A13, A7);
  k in NAT by ORDINAL1:def 13;
  hence thesis by A2,A4,A5,A14;
end;

Lm1: for f being FinSequence of TOP-REAL 2, Q being Subset of TOP-REAL 2, q
being Point of TOP-REAL 2, i being Element of NAT st LSeg (f,i) meets Q & f is
being_S-Seq & Q is closed & 1<=i & i+1<=len f & q in LSeg(f,i) & q in Q holds
LE First_Point(LSeg(f,i), f/.i,f/.(i+1),Q), q, f/.i, f/.(i+1)

proof

  let f be FinSequence of TOP-REAL 2, Q be Subset of TOP-REAL 2, q be Point of
  TOP-REAL 2, i be Element of NAT;

  assume that
A1: LSeg(f,i) meets Q and
A2: f is being_S-Seq and
A3: Q is closed and
A4: 1<=i & i+1<=len f and
A5: q in LSeg(f,i) and
A6: q in Q;
  reconsider P = LSeg(f,i) as non empty Subset of TOP-REAL 2 by A5;
  set q1 = First_Point(P,f/.i,f/.(i+1),Q), p1 = f/.i, p2 = f/.(i+1);
A7: P /\ Q c= P by XBOOLE_1:17;
A8: i+1 in dom f by A4,GOBOARD2:3;
A9: f is one-to-one & i in dom f by A2,A4,GOBOARD2:3,TOPREAL1:def 10;
A10: p1 <> p2
  proof
    assume
    p1 = p2;
    then i = i+1 by A9,A8,PARTFUN2:17;
    hence thesis;
  end;
  P is closed by SPPOL_1:40;
  then
A11: P /\ Q is closed by A3,TOPS_1:35;
  P is_an_arc_of f/.i,f/.(i+1) by A2,A4,JORDAN5B:15;

  then
  q1 in P /\ Q & for g being Function of I[01], (TOP-REAL 2)|P, s1, s2 be

Real st g is being_homeomorphism & g.0=p1 & g.1=p2 & g.s1=q1 & 0<=s1 & s1<=1 &
  g.s2= q & 0<=s2 & s2<=1 holds s1<=s2 by A1,A6,A11,Def1;

  then
A12: LE q1, q, P, p1, p2 by A5,A7,Def3;
  LSeg(f,i) = LSeg(f/.i, f/.(i+1)) by A4,TOPREAL1:def 5;
  hence thesis by A10,A12,Th17;
end;

Lm2: for f being FinSequence of TOP-REAL 2, Q being Subset of TOP-REAL 2, q
being Point of TOP-REAL 2, i being Element of NAT st L~f meets Q & f is
being_S-Seq & Q is closed & First_Point(L~f,f/.1,f/.len f,Q) in LSeg(f,i) & 1<=
i & i+1<=len f & q in LSeg(f,i) & q in Q holds LE First_Point(L~f,f/.1,f/.len f
,Q), q, f/.i, f/.(i+1)

proof

  let f be FinSequence of TOP-REAL 2, Q be Subset of TOP-REAL 2, q be Point of
  TOP-REAL 2, i be Element of NAT;

  assume that
A1: L~f meets Q and
A2: f is being_S-Seq and
A3: Q is closed and
A4: First_Point(L~f,f/.1,f/.len f,Q) in LSeg(f,i) and
A5: 1<=i & i+1<=len f and
A6: q in LSeg(f,i) & q in Q;
  len f >= 2 by A2,TOPREAL1:def 10;

  then reconsider
  P = L~f, R = LSeg (f,i) as non empty Subset of TOP-REAL 2 by A4,
  TOPREAL1:29;

  LSeg (f,i) /\ Q <> {} by A6,XBOOLE_0:def 4;
  then
A7: LSeg (f,i) meets Q by XBOOLE_0:def 7;

  First_Point (P,f/.1,f/.len f,Q) = First_Point (R, f/.i, f/.(i+1), Q) by
  A1,A2,A3,A4,A5,Th19;

  hence thesis by A2,A3,A5,A6,A7,Lm1;
end;

Lm3: for f being FinSequence of TOP-REAL 2, Q being Subset of TOP-REAL 2, q
being Point of TOP-REAL 2, i being Element of NAT st LSeg (f,i) meets Q & f is
being_S-Seq & Q is closed & 1<=i & i+1<=len f & q in LSeg(f,i) & q in Q holds
LE q, Last_Point(LSeg(f,i),f/.i,f/.(i+1),Q), f/.i, f/.(i+1)

proof

  let f be FinSequence of TOP-REAL 2, Q be Subset of TOP-REAL 2, q be Point of
  TOP-REAL 2, i be Element of NAT;

  assume that
A1: LSeg(f,i) meets Q and
A2: f is being_S-Seq and
A3: Q is closed and
A4: 1<=i & i+1<=len f and
A5: q in LSeg(f,i) and
A6: q in Q;
  reconsider P = LSeg(f,i) as non empty Subset of TOP-REAL 2 by A5;
  set q1 = Last_Point(P,f/.i,f/.(i+1),Q), p1 = f/.i, p2 = f/.(i+1);
A7: P /\ Q c= P by XBOOLE_1:17;
A8: i+1 in dom f by A4,GOBOARD2:3;
A9: f is one-to-one & i in dom f by A2,A4,GOBOARD2:3,TOPREAL1:def 10;
A10: p1 <> p2
  proof
    assume
    p1 = p2;
    then i = i+1 by A9,A8,PARTFUN2:17;
    hence thesis;
  end;
  P is closed by SPPOL_1:40;
  then
A11: P /\ Q is closed by A3,TOPS_1:35;
  P is_an_arc_of f/.i,f/.(i+1) by A2,A4,JORDAN5B:15;

  then
  q1 in P /\ Q & for g being Function of I[01], (TOP-REAL 2)|P, s1, s2 be

Real st g is being_homeomorphism & g.0=p1 & g.1=p2 & g.s1=q & 0<=s1 & s1<=1 & g
  .s2= q1 & 0<=s2 & s2<=1 holds s1<=s2 by A1,A6,A11,Def2;

  then
A12: LE q, q1, P, p1, p2 by A5,A7,Def3;
  LSeg(f,i) = LSeg(f/.i, f/.(i+1)) by A4,TOPREAL1:def 5;
  hence thesis by A10,A12,Th17;
end;

Lm4: for f being FinSequence of TOP-REAL 2, Q being Subset of TOP-REAL 2, q
being Point of TOP-REAL 2, i being Element of NAT st L~f meets Q & f is
being_S-Seq & Q is closed & Last_Point (L~f,f/.1,f/.len f,Q) in LSeg(f,i) & 1
<= i & i+1 <= len f & q in LSeg(f,i) & q in Q holds LE q, Last_Point(L~f,f/.1,f
/.len f,Q), f/.i, f/.(i+1)

proof

  let f be FinSequence of TOP-REAL 2, Q be Subset of TOP-REAL 2, q be Point of
  TOP-REAL 2, i be Element of NAT;

  assume that
A1: L~f meets Q and
A2: f is being_S-Seq and
A3: Q is closed and
A4: Last_Point(L~f,f/.1,f/.len f,Q) in LSeg(f,i) and
A5: 1<=i & i+1<=len f and
A6: q in LSeg(f,i) & q in Q;
  len f >= 2 by A2,TOPREAL1:def 10;

  then reconsider
  P = L~f, R = LSeg (f,i) as non empty Subset of TOP-REAL 2 by A4,
  TOPREAL1:29;

  LSeg (f,i) /\ Q <> {} by A6,XBOOLE_0:def 4;
  then
A7: LSeg (f,i) meets Q by XBOOLE_0:def 7;

  Last_Point(P,f/.1,f/.len f,Q) = Last_Point (R, f/.i, f/.(i+1), Q) by A1
  ,A2,A3,A4,A5,Th20;

  hence thesis by A2,A3,A5,A6,A7,Lm3;
end;

theorem Th25:
  for f being FinSequence of TOP-REAL 2, q being Point of TOP-REAL
  2, i being Element of NAT st f is being_S-Seq & 1 <= i & i+1 <= len f & q in
  LSeg(f,i) holds LE f/.i, q, L~f, f/.1, f/.len f
proof
  let f be FinSequence of TOP-REAL 2, q be Point of TOP-REAL 2, i be Element
  of NAT;
  assume that
A1: f is being_S-Seq and
A2: 1<=i & i+1<=len f and
A3: q in LSeg(f,i);
  set p1 = f/.1, p2 = f/.len f, q1 = f/.i;
A4: 2 <= len f by A1,TOPREAL1:def 10;
  then reconsider P = L~f as non empty Subset of TOP-REAL 2 by TOPREAL1:29;
  i in dom f by A2,GOBOARD2:3;
  then
A5: q1 in P by A4,GOBOARD1:16;
A6: for g being Function of I[01], (TOP-REAL 2)|P, s1,s2 be Real st g is
being_homeomorphism & g.0=p1 & g.1=p2 & g.s1=q1 & 0<=s1 & s1<=1 & g.s2=q & 0<=
  s2 & s2<=1 holds s1<=s2
  proof
    let g be Function of I[01], (TOP-REAL 2)|P, s1,s2 be Real;
    assume that
A7: g is being_homeomorphism and
A8: g.0=p1 & g.1=p2 and
A9: g.s1=q1 and
A10: 0<=s1 & s1<=1 and
A11: g.s2=q and
A12: 0<=s2 & s2<=1;
A13: dom g = [#]I[01] by A7,TOPS_2:def 5
      .= the carrier of I[01];
    then
A14: s1 in dom g by A10,BORSUK_1:86;
    consider r1, r2 be Real such that
    r1 < r2 and
A15: 0 <= r1 & r1 <= 1 and
    0 <= r2 and
    r2 <= 1 and
A16: LSeg (f, i) = g.: [.r1, r2.] and
A17: g.r1 = q1 and
    g.r2 = f/.(i+1) by A1,A2,A7,A8,JORDAN5B:7;
    consider r3' be set such that
A18: r3' in dom g and
A19: r3' in [.r1, r2.] and
A20: g.r3' = q by A3,A16,FUNCT_1:def 12;
    r3' in { l where l is Real : r1 <= l & l <= r2 } by A19,RCOMP_1:def 1;
    then consider r3 be Real such that
A21: r3 = r3' and
A22: r1 <= r3 and
    r3 <= r2;
A23: g is one-to-one by A7,TOPS_2:def 5;
A24: r1 in dom g by A15,A13,BORSUK_1:86;
    s2 in dom g by A12,A13,BORSUK_1:86;
    then s2 = r3 by A11,A18,A20,A21,A23,FUNCT_1:def 8;
    hence thesis by A9,A17,A22,A24,A14,A23,FUNCT_1:def 8;
  end;
  q in P by A3,SPPOL_2:17;
  hence thesis by A5,A6,Def3;
end;

theorem Th26:
  for f being FinSequence of TOP-REAL 2, q being Point of TOP-REAL
2, i being Element of NAT st f is being_S-Seq & 1<=i & i+1<=len f & q in LSeg(f
  ,i) holds LE q, f/.(i+1), L~f, f/.1, f/.len f
proof
  let f be FinSequence of TOP-REAL 2, q be Point of TOP-REAL 2, i be Element
  of NAT;
  assume that
A1: f is being_S-Seq and
A2: 1<=i & i+1<=len f and
A3: q in LSeg(f,i);
  len f >= 2 by A1,TOPREAL1:def 10;
  then reconsider P = L~f as non empty Subset of TOP-REAL 2 by TOPREAL1:29;
  set p1 = f/.1, p2 = f/.len f, q1 = f/.(i+1);
  q1 in LSeg (f,i) by A2,TOPREAL1:27;
  then
A4: q1 in P by SPPOL_2:17;
A5: for g being Function of I[01], (TOP-REAL 2)|P, s1,s2 be Real st g is
being_homeomorphism & g.0=p1 & g.1=p2 & g.s1=q & 0<=s1 & s1<=1 & g.s2=q1 & 0<=
  s2 & s2<=1 holds s1<=s2
  proof
    let g be Function of I[01], (TOP-REAL 2)|P, s1,s2 be Real;
    assume that
A6: g is being_homeomorphism and
A7: g.0=p1 & g.1=p2 and
A8: g.s1=q and
A9: 0<=s1 & s1<=1 and
A10: g.s2=q1 and
A11: 0<=s2 & s2<=1;
A12: dom g = [#]I[01] by A6,TOPS_2:def 5
      .= the carrier of I[01];
    then
A13: s2 in dom g by A11,BORSUK_1:86;
    consider r1, r2 be Real such that
A14: r1 < r2 & 0 <= r1 and
    r1 <= 1 and
    0 <= r2 and
A15: r2 <= 1 and
A16: LSeg (f, i) = g.:[.r1, r2.] and
    g.r1 = f/.i and
A17: g.r2 = q1 by A1,A2,A6,A7,JORDAN5B:7;
A18: r2 in dom g by A14,A15,A12,BORSUK_1:86;
    consider r3' be set such that
A19: r3' in dom g and
A20: r3' in [.r1, r2.] and
A21: g.r3' = q by A3,A16,FUNCT_1:def 12;
    r3' in { l where l is Real : r1 <= l & l <= r2 } by A20,RCOMP_1:def 1;
    then consider r3 be Real such that
A22: r3 = r3' and
    r1 <= r3 and
A23: r3 <= r2;
A24: g is one-to-one by A6,TOPS_2:def 5;
    s1 in dom g by A9,A12,BORSUK_1:86;
    then s1 = r3 by A8,A19,A21,A22,A24,FUNCT_1:def 8;
    hence thesis by A10,A17,A23,A18,A13,A24,FUNCT_1:def 8;
  end;
  q in P by A3,SPPOL_2:17;
  hence thesis by A4,A5,Def3;
end;

theorem
  for f being FinSequence of TOP-REAL 2, Q being Subset of TOP-REAL 2, q
  being Point of TOP-REAL 2, i, j being Element of NAT st L~f meets Q & f is
being_S-Seq & Q is closed & First_Point (L~f,f/.1,f/.len f,Q) in LSeg(f,i) & 1
  <=i & i+1<=len f & q in LSeg(f,j) & 1 <= j & j + 1 <= len f & q in Q &
  First_Point (L~f,f/.1,f/.len f,Q) <> q holds i <= j & (i=j implies LE
  First_Point(L~f,f/.1,f/.len f,Q), q, f/.i, f/.(i+1))
proof
  let f be FinSequence of TOP-REAL 2, Q be Subset of TOP-REAL 2, q be Point of
  TOP-REAL 2, i,j be Element of NAT;
  assume that
A1: L~f meets Q and
A2: f is being_S-Seq and
A3: Q is closed and
A4: First_Point(L~f,f/.1,f/.len f,Q) in LSeg(f,i) and
A5: 1<=i and
A6: i+1<=len f and
A7: q in LSeg(f,j) and
A8: 1<=j & j+1<=len f and
A9: q in Q and
A10: First_Point (L~f,f/.1,f/.len f,Q) <> q;
  reconsider P = L~f as non empty Subset of TOP-REAL 2 by A4,SPPOL_2:17;
  set q1 = First_Point(P,f/.1,f/.len f,Q), p1 = f/.i;
A11: q in L~f by A7,SPPOL_2:17;
  thus i <= j
  proof
    P is closed by SPPOL_1:49;
    then L~f /\ Q is closed by A3,TOPS_1:35;
    then
A12: LE q1, q, P, f/.1, f/.len f by A2,A9,A11,Th15;
A13: LE q, f/.(j+1), P, f/.1, f/.len f by A2,A7,A8,Th26;
    i <= i + 1 by NAT_1:11;
    then
A14: i <= len f by A6,XXREAL_0:2;
    assume
    j < i;
    then
A15: j+1 <= i by NAT_1:13;
    1 <= j+1 by NAT_1:11;
    then LE f/.(j+1), p1, P, f/.1, f/.len f by A2,A15,A14,Th24;
    then
A16: LE q, p1, P, f/.1, f/.len f by A13,Th13;
    LE p1, q1, P, f/.1, f/.len f by A2,A4,A5,A6,Th25;
    then LE q, q1, P, f/.1, f/.len f by A16,Th13;
    hence contradiction by A2,A10,A12,Th12,TOPREAL1:31;
  end;
  assume
  i = j;
  hence thesis by A1,A2,A3,A4,A5,A6,A7,A9,Lm2;
end;

theorem
  for f being FinSequence of TOP-REAL 2, Q being Subset of TOP-REAL 2, q
  being Point of TOP-REAL 2, i, j being Element of NAT st L~f meets Q & f is
being_S-Seq & Q is closed & Last_Point (L~f,f/.1,f/.len f,Q) in LSeg(f,i) & 1<=
i & i+1<=len f & q in LSeg(f,j) & 1 <= j & j + 1 <= len f & q in Q & Last_Point
(L~f,f/.1,f/.len f,Q) <> q holds i >= j & (i=j implies LE q, Last_Point(L~f,f/.
  1,f/.len f,Q), f/.i, f/.(i+1))
proof
  let f be FinSequence of TOP-REAL 2, Q be Subset of TOP-REAL 2, q be Point of
  TOP-REAL 2, i,j be Element of NAT;
  assume that
A1: L~f meets Q and
A2: f is being_S-Seq and
A3: Q is closed and
A4: Last_Point(L~f,f/.1,f/.len f,Q) in LSeg(f,i) and
A5: 1<=i & i+1<=len f and
A6: q in LSeg(f,j) and
A7: 1<=j and
A8: j+1<=len f and
A9: q in Q and
A10: Last_Point (L~f,f/.1,f/.len f,Q) <> q;
  reconsider P = L~f as non empty Subset of TOP-REAL 2 by A4,SPPOL_2:17;
  set q1 = Last_Point(P,f/.1,f/.len f,Q), p2 = f/.(i+1);
A11: q in L~f by A6,SPPOL_2:17;
  thus i >= j
  proof
    assume
    j > i;
    then
A12: i+1 <= j by NAT_1:13;
    j <= j + 1 by NAT_1:11;
    then 1 <= i + 1 & j <= len f by A8,NAT_1:11,XXREAL_0:2;
    then
A13: LE p2, f/.j, P, f/.1, f/.len f by A2,A12,Th24;
    LE f/.j, q, P, f/.1, f/.len f by A2,A6,A7,A8,Th25;
    then
A14: LE p2, q, P, f/.1, f/.len f by A13,Th13;
    L~f is closed by SPPOL_1:49;
    then L~f /\ Q is closed by A3,TOPS_1:35;
    then
A15: LE q, q1, P, f/.1, f/.len f by A2,A9,A11,Th16;
    LE q1, p2, P, f/.1, f/.len f by A2,A4,A5,Th26;
    then LE q1, q, P, f/.1, f/.len f by A14,Th13;
    hence contradiction by A2,A10,A15,Th12,TOPREAL1:31;
  end;
  assume
  i = j;
  hence thesis by A1,A2,A3,A4,A5,A6,A9,Lm4;
end;

theorem Th29:
  for f being FinSequence of TOP-REAL 2, q1, q2 being Point of
TOP-REAL 2, i being Element of NAT st q1 in LSeg(f,i) & q2 in LSeg(f,i) & f is
  being_S-Seq & 1 <= i & i + 1 <= len f holds LE q1, q2, L~f, f/.1, f/.len f
  implies LE q1, q2, LSeg (f,i), f/.i, f/.(i+1)
proof
  let f be FinSequence of TOP-REAL 2, q1, q2 be Point of TOP-REAL 2, i be
  Element of NAT;
  assume that
A1: q1 in LSeg(f,i) and
A2: q2 in LSeg(f,i) and
A3: f is being_S-Seq and
A4: 1 <= i & i + 1 <= len f;
  len f >= 2 by A3,TOPREAL1:def 10;
  then reconsider
  P = L~f, Q = LSeg(f,i) as non empty Subset of TOP-REAL 2 by A1,TOPREAL1:29;
  L~f is_an_arc_of f/.1, f/.len f by A3,TOPREAL1:31;
  then consider F being Function of I[01], (TOP-REAL 2)|P such that
A5: F is being_homeomorphism & F.0 = f/.1 & F.1 = f/.len f by
  TOPREAL1:def 2;
  consider ppi, pi1 be Real such that
A6: ppi < pi1 and
A7: 0 <= ppi and
  ppi <= 1 and
  0 <= pi1 and
A8: pi1 <= 1 and
A9: LSeg (f, i) = F.:[.ppi, pi1.] and
A10: F.ppi = f/.i and
A11: F.pi1 = f/.(i+1) by A3,A4,A5,JORDAN5B:7;
  set Ex = L[01]((#)(ppi,pi1),(ppi,pi1)(#));
A12: Ex is being_homeomorphism by A6,TREAL_1:20;
  then
A13: rng Ex = [#] Closed-Interval-TSpace(ppi,pi1) by TOPS_2:def 5;
A14: ppi in { dd where dd is Real : ppi <= dd & dd <= pi1 } by A6;
  then reconsider Poz = [.ppi,pi1.] as non empty Subset of I[01] by A7,A8,
  BORSUK_1:83,RCOMP_1:def 1,XXREAL_1:34;
  consider G be Function of I[01]|Poz, (TOP-REAL 2)|Q such that
A15: G = F|Poz and
A16: G is being_homeomorphism by A3,A4,A5,A9,JORDAN5B:8;
A17: ppi in [.ppi,pi1.] by A14,RCOMP_1:def 1;
  set H = G * Ex;
A18: dom G = [#](I[01]|Poz) by A16,TOPS_2:def 5
    .= Poz by PRE_TOPC:def 10
    .= [#] Closed-Interval-TSpace(ppi,pi1) by A6,TOPMETR:25;
  then
A19: rng H = rng G by A13,RELAT_1:47
    .= [#] ((TOP-REAL 2)|LSeg(f,i)) by A16,TOPS_2:def 5;
  pi1 in { dd where dd is Real : ppi <= dd & dd <= pi1 } by A6;
  then
A20: pi1 in [.ppi,pi1.] by RCOMP_1:def 1;
A21: Ex.1 = Ex.(0,1)(#) by TREAL_1:def 2
    .= (ppi,pi1)(#) by A6,TREAL_1:12
    .= pi1 by A6,TREAL_1:def 2;
A22: dom H = dom Ex by A13,A18,RELAT_1:46
    .= [#] I[01] by A12,TOPMETR:27,TOPS_2:def 5
    .= the carrier of I[01];
  then reconsider H as Function of I[01], (TOP-REAL 2)|Q by A19,FUNCT_2:4;
  q1 in rng H by A1,A19,PRE_TOPC:def 10;
  then consider x1' be set such that
A23: x1' in dom H and
A24: q1 = H.x1' by FUNCT_1:def 5;
  x1' in { l where l is Real : 0 <= l & l <= 1 } by A22,A23,BORSUK_1:83 ,
  RCOMP_1:def 1;
  then consider x1 be Real such that
A25: x1 = x1' and
A26: 0 <= x1 & x1 <= 1;
  assume
A27: LE q1, q2, L~f, f/.1, f/.len f;
  x1 in the carrier of I[01] by A26,BORSUK_1:86;
  then x1 in dom Ex by A13,A18,A22,RELAT_1:46;
  then Ex.x1 in the carrier of Closed-Interval-TSpace(ppi,pi1) by A13,
  FUNCT_1:def 5;
  then
A28: Ex.x1 in Poz by A6,TOPMETR:25;
  1 in dom H by A22,BORSUK_1:86;
  then
A29: H.1 = G.pi1 by A21,FUNCT_1:22
    .= f/.(i+1) by A11,A20,A15,FUNCT_1:72;
A30: Ex.0 = Ex.(#)(0,1) by TREAL_1:def 1
    .= (#)(ppi,pi1) by A6,TREAL_1:12
    .= ppi by A6,TREAL_1:def 1;
  q2 in rng H by A2,A19,PRE_TOPC:def 10;
  then consider x2' be set such that
A31: x2' in dom H and
A32: q2 = H.x2' by FUNCT_1:def 5;
  x2' in { l where l is Real : 0 <= l & l <= 1 } by A22,A31,BORSUK_1:83 ,
  RCOMP_1:def 1;
  then consider x2 be Real such that
A33: x2 = x2' and
A34: 0 <= x2 and
A35: x2 <= 1;
  reconsider X1 = x1, X2 = x2 as Point of Closed-Interval-TSpace (0,1) by A26,
  A34,A35,BORSUK_1:86,TOPMETR:27;
  x2 in the carrier of I[01] by A34,A35,BORSUK_1:86;
  then x2 in dom Ex by A13,A18,A22,RELAT_1:46;
  then Ex.x2 in the carrier of Closed-Interval-TSpace(ppi,pi1) by A13,
  FUNCT_1:def 5;
  then
A36: Ex.x2 in Poz by A6,TOPMETR:25;
  then reconsider E1 = Ex.X1, E2 = Ex.X2 as Real by A28;
A37: q2 = G.(Ex.x2) by A31,A32,A33,FUNCT_1:22
    .= F.(Ex.x2) by A15,A36,FUNCT_1:72;
  reconsider K1 = Closed-Interval-TSpace(ppi,pi1), K2 = I[01]|Poz as SubSpace
  of I[01] by A6,A7,A8,TOPMETR:27,TREAL_1:6;
A38: Ex is one-to-one continuous by A12,TOPS_2:def 5;
  the carrier of K1 = [.ppi,pi1.] by A6,TOPMETR:25
    .= [#](I[01]|Poz) by PRE_TOPC:def 10
    .= the carrier of K2;
  then I[01]|Poz = Closed-Interval-TSpace(ppi,pi1) by TSEP_1:5;
  then
A39: H is being_homeomorphism by A16,A12,TOPMETR:27,TOPS_2:71;
A40: q1 = G.(Ex.x1) by A23,A24,A25,FUNCT_1:22
    .= F.(Ex.x1) by A15,A28,FUNCT_1:72;
A41: 0 <= E2 & E2 <= 1 by A36,BORSUK_1:86;
  0 in dom H by A22,BORSUK_1:86;
  then
A42: H.0 = G.ppi by A30,FUNCT_1:22
    .= f/.i by A10,A17,A15,FUNCT_1:72;
  E1 <= 1 by A28,BORSUK_1:86;
  then E1 <= E2 by A27,A5,A40,A37,A41,Def3;
  then x1 <= x2 by A6,A30,A21,A38,JORDAN5A:16;
  hence thesis by A3,A4,A39,A42,A29,A24,A32,A25,A26,A33,A35,Th8,JORDAN5B:15;
end;

theorem
  for f being FinSequence of TOP-REAL 2, q1, q2 being Point of TOP-REAL
2 st q1 in L~f & q2 in L~f & f is being_S-Seq & q1 <> q2 holds LE q1, q2, L~f,
  f/.1, f/.len f iff for i, j being Element of NAT st q1 in LSeg(f,i) & q2 in
LSeg(f,j) & 1 <= i & i+1 <= len f & 1 <= j & j+1 <= len f holds i <= j & (i = j
  implies LE q1,q2,f/.i,f/.(i+1))
proof
  let f be FinSequence of TOP-REAL 2, q1,q2 be Point of TOP-REAL 2;
  set p3 = f/.1, p4 = f/.len f;
  assume that
A1: q1 in L~f and
A2: q2 in L~f and
A3: f is being_S-Seq and
A4: q1 <> q2;
  reconsider P = L~f as non empty Subset of TOP-REAL 2 by A1;
  hereby
    assume
A5: LE q1,q2,L~f,f/.1,f/.len f;
    thus for i,j being Element of NAT st q1 in LSeg(f,i) & q2 in LSeg(f,j) & 1
<=i & i+1<=len f & 1<=j & j+1<=len f holds i<=j & (i=j implies LE q1,q2,f/.i,f
    /.(i+1))
    proof
      let i,j be Element of NAT;
      assume that
A6:   q1 in LSeg(f,i) and
A7:   q2 in LSeg(f,j) and
A8:   1<=i and
A9:   i+1<=len f and
A10:  1<=j & j+1<=len f;
      thus i<=j
      proof
        assume
        j < i;
        then j+1 <= i by NAT_1:13;
        then consider m be Nat such that
A11:    j+1+m = i by NAT_1:10;
A12:    LE q2, f/.(j+1), P, p3, p4 by A3,A7,A10,Th26;
        reconsider m as Element of NAT by ORDINAL1:def 13;
A13:    1 <= j+1 & j+1 <= j+1+m by NAT_1:11;
        i <= i + 1 by NAT_1:11;
        then j+1+m <= len f by A9,A11,XXREAL_0:2;
        then LE f/.(j+1), f/.(j+1+m), P, p3, p4 by A3,A13,Th24;
        then
A14:    LE q2, f/.(j+1+m), P, p3, p4 by A12,Th13;
        LE f/.(j+1+m), q1, P, p3, p4 by A3,A6,A8,A9,A11,Th25;
        then LE q2, q1, P, p3, p4 by A14,Th13;
        hence thesis by A3,A4,A5,Th12,TOPREAL1:31;
      end;
      assume
A15:  i = j;
A16:  f is one-to-one by A3,TOPREAL1:def 10;
      set p1 = f/.i, p2 = f/.(i+1);
A17:  i in dom f & i+1 in dom f by A8,A9,GOBOARD2:3;
A18:  p1 <> p2
      proof
        assume
        p1 = p2;
        then i = i+1 by A17,A16,PARTFUN2:17;
        hence thesis;
      end;
      LSeg (f,i) = LSeg (p1, p2) by A8,A9,TOPREAL1:def 5;
      hence thesis by A3,A5,A6,A7,A8,A9,A15,A18,Th17,Th29;
    end;
  end;
  consider i be Element of NAT such that
A19: 1 <= i & i+1 <= len f and
A20: q1 in LSeg(f,i) by A1,SPPOL_2:13;
  consider j be Element of NAT such that
A21: 1 <= j and
A22: j+1 <= len f and
A23: q2 in LSeg(f,j) by A2,SPPOL_2:13;
  assume
A24: for i,j being Element of NAT st q1 in LSeg(f,i) & q2 in LSeg(f,j) &
1<=i & i+1<=len f & 1<=j & j+1<=len f holds i<=j & (i=j implies LE q1,q2,f/.i,f
  /.(i+1));
  then
A25: i<=j by A19,A20,A21,A22,A23;
  per cases by A25,XXREAL_0:1;
  suppose
    i < j;
    then i+1 <= j by NAT_1:13;
    then consider m be Nat such that
A26: j = i+1 + m by NAT_1:10;
    reconsider m as Element of NAT by ORDINAL1:def 13;
A27: 1 <= i+1 & i+1 <= i+1+m by NAT_1:11,12;
A28: LE q1, f/.(i+1), P, f/.1, f/.len f by A3,A19,A20,Th26;
    j <= j + 1 by NAT_1:11;
    then i+1+m <= len f by A22,A26,XXREAL_0:2;
    then LE f/.(i+1), f/.(i+1+m), P, f/.1, f/.len f by A3,A27,Th24;
    then
A29: LE q1, f/.(i+1+m), P, f/.1, f/.len f by A28,Th13;
    LE f/.(i+1+m), q2, P, f/.1, f/.len f by A3,A21,A22,A23,A26,Th25;
    hence thesis by A29,Th13;
  end;
  suppose
A30: i = j;
    reconsider Q = LSeg (f,i) as non empty Subset of TOP-REAL 2 by A20;
    set p1 = f/.i,p2 = f/.(i+1);
A31: f is one-to-one by A3,TOPREAL1:def 10;
A32: i in dom f & i+1 in dom f by A19,GOBOARD2:3;
    p1 <> p2
    proof
      assume
      p1 = p2;
      then i = i+1 by A32,A31,PARTFUN2:17;
      hence thesis;
    end;
    then consider
    H be Function of I[01], (TOP-REAL 2) | LSeg(p1, p2) such that
A33: for x being Real st x in [.0,1.] holds H.x = (1-x)*p1 + x*p2 and
A34: H is being_homeomorphism and
    H.0 = p1 and
    H.1 = p2 by JORDAN5A:4;
A35: LSeg(f,i) = LSeg(p1,p2) by A19,TOPREAL1:def 5;
    then reconsider H as Function of I[01], (TOP-REAL 2) | Q;
A36: LE q1, q2, f/.i, f/.(i+1) by A24,A19,A20,A23,A30;
    q1 in P & q2 in P & for g being Function of I[01], (TOP-REAL 2)|P, s1
,s2 be Real st g is being_homeomorphism & g.0=f/.1 & g.1=f/.len f & g.s1=q1 & 0
    <=s1 & s1<=1 & g.s2=q2 & 0<=s2 & s2<=1 holds s1<=s2
    proof
A37:  rng H = [#] ((TOP-REAL 2) | LSeg(f,i)) by A34,A35,TOPS_2:def 5
        .= LSeg (f,i) by PRE_TOPC:def 10;
      then consider x1' be set such that
A38:  x1' in dom H and
A39:  H.x1' = q1 by A20,FUNCT_1:def 5;
A40:  dom H = [#]I[01] by A34,TOPS_2:def 5
        .= [.0,1.] by BORSUK_1:83;
      then x1' in { l where l is Real : 0 <= l & l <= 1 } by
      A38, RCOMP_1:
      def 1;
      then consider x1 be Real such that
A41:  x1 = x1' and
      0 <= x1 and
A42:  x1 <= 1;
      consider x2' be set such that
A43:  x2' in dom H and
A44:  H.x2' = q2 by A23,A30,A37,FUNCT_1:def 5;
      x2' in { l where l is Real : 0 <= l & l <= 1 } by A40,A43, RCOMP_1:
      def 1;
      then consider x2 be Real such that
A45:  x2 = x2' and
A46:  0 <= x2 & x2 <= 1;
A47:  q2 = (1-x2)*p1 + x2*p2 by A33,A40,A43,A44,A45;
      q1 = (1-x1)*p1 + x1*p2 by A33,A40,A38,A39,A41;
      then
A48:  x1 <= x2 by A36,A42,A46,A47,JORDAN3:def 6;
      0 in { l where l is Real : 0 <= l & l <= 1 };
      then
A49:  0 in [.0,1.] by RCOMP_1:def 1;
      then
A50:  H.0 = (1-0)*p1 + 0 * p2 by A33
        .= p1 + 0 * p2 by EUCLID:33
        .= p1 + 0.TOP-REAL 2 by EUCLID:33
        .= p1 by EUCLID:31;
      thus q1 in P & q2 in P by A1,A2;
      let F be Function of I[01], (TOP-REAL 2)|P, s1, s2 be Real;
      assume that
A51:  F is being_homeomorphism and
A52:  F.0 = f/.1 & F.1 = f/.len f and
A53:  F.s1 = q1 and
A54:  0 <= s1 & s1 <= 1 and
A55:  F.s2 = q2 and
A56:  0 <= s2 & s2 <= 1;
      consider ppi, pi1 be Real such that
A57:  ppi < pi1 and
A58:  0 <= ppi and
      ppi <= 1 and
      0 <= pi1 and
A59:  pi1 <= 1 and
A60:  LSeg (f, i) = F.:[.ppi, pi1.] and
A61:  F.ppi = f/.i and
A62:  F.pi1 = f/.(i+1) by A3,A19,A51,A52,JORDAN5B:7;
A63:  ppi in { dd where dd is Real : ppi <= dd & dd <= pi1 } by A57;
      then reconsider
      Poz = [.ppi,pi1.] as non empty Subset of I[01] by A58,A59,
      BORSUK_1:83,RCOMP_1:def 1,XXREAL_1:34;
      consider G be Function of I[01]|Poz, (TOP-REAL 2)|Q such that
A64:  G = F|Poz and
A65:  G is being_homeomorphism by A3,A19,A51,A52,A60,JORDAN5B:8;
A66:  dom F = [#] I[01] by A51,TOPS_2:def 5
        .= the carrier of I[01];
      reconsider K1 = Closed-Interval-TSpace(ppi,pi1), K2 = I[01]|Poz as
      SubSpace of I[01] by A57,A58,A59,TOPMETR:27,TREAL_1:6;
A67:  the carrier of K1 = [.ppi,pi1.] by A57,TOPMETR:25
        .= [#](I[01]|Poz) by PRE_TOPC:def 10
        .= the carrier of K2;
      then reconsider E=G" * H as Function of Closed-Interval-TSpace(0,1),
      Closed-Interval-TSpace(ppi,pi1) by TOPMETR:27;
A68:  G is one-to-one by A65,TOPS_2:def 5;
      reconsider X1 = x1, X2 = x2 as Point of Closed-Interval-TSpace(0,1) by
      A40,A38,A41,A43,A45,TOPMETR:25;
A69:  G" is being_homeomorphism by A65,TOPS_2:70;
A70:  s2 in the carrier of I[01] by A56,BORSUK_1:86;
A71:  F is one-to-one by A51,TOPS_2:def 5;
      rng G = [#] ((TOP-REAL 2)|LSeg(f,i)) by A65,TOPS_2:def 5;
      then
A72:  G" = (G qua Function)" by A68,TOPS_2:def 4;
A73:  ex x' be set st x' in dom F & x' in [.ppi,pi1.] & q2 = F.x' by A23,
      A30,A60,FUNCT_1:def 12;
      then s2 in Poz by A55,A70,A66,A71,FUNCT_1:def 8;
      then
A74:  G.s2 = q2 by A55,A64,FUNCT_1:72;
      dom G = [#] (I[01]|Poz) by A65,TOPS_2:def 5;
      then
A75:  dom G = Poz by PRE_TOPC:def 10;
      then s2 in dom G by A55,A70,A66,A71,A73,FUNCT_1:def 8;
      then
A76:  s2 = G".(H.x2) by A44,A45,A68,A72,A74,FUNCT_1:54
        .= E.x2 by A43,A45,FUNCT_1:23;
      then
A77:  s2 = E.X2;
      consider x be set such that
A78:  x in dom F and
A79:  x in [.ppi,pi1.] and
A80:  q1 = F.x by A20,A60,FUNCT_1:def 12;
A81:  s1 in the carrier of I[01] by A54,BORSUK_1:86;
      then x = s1 by A53,A66,A71,A78,A80,FUNCT_1:def 8;
      then
A82:  G.s1 = q1 by A64,A79,A80,FUNCT_1:72;
      Closed-Interval-TSpace(ppi,pi1) = I[01]|Poz by A67,TSEP_1:5;
      then E is being_homeomorphism by A34,A35,A69,TOPMETR:27,TOPS_2:71;
      then
A83:  E is continuous one-to-one by TOPS_2:def 5;
      1 in { l where l is Real : 0 <= l & l <= 1 };
      then
A84:  1 in [.0,1.] by RCOMP_1:def 1;
      then
A85:  H.1 = (1-1)*p1 + 1*p2 by A33
        .= 0.TOP-REAL 2 + 1*p2 by EUCLID:33
        .= 0.TOP-REAL 2 + p2 by EUCLID:33
        .= p2 by EUCLID:31;
      s1 in dom G by A53,A81,A66,A71,A75,A79,A80,FUNCT_1:def 8;
      then
A86:  s1 = G".(H.x1) by A39,A41,A68,A72,A82,FUNCT_1:54
        .= E.x1 by A38,A41,FUNCT_1:23;
      then
A87:  s1 = E.X1;
      pi1 in { dd where dd is Real : ppi <= dd & dd <= pi1 } by A57;
      then
A88:  pi1 in [.ppi,pi1.] by RCOMP_1:def 1;
      then G.pi1 = p2 by A62,A64,FUNCT_1:72;
      then
A89:  pi1 = G".(H.1) by A88,A68,A75,A72,A85,FUNCT_1:54
        .= E.1 by A40,A84,FUNCT_1:23;
A90:  ppi in [.ppi,pi1.] by A63,RCOMP_1:def 1;
      then G.ppi = p1 by A61,A64,FUNCT_1:72;
      then
A91:  ppi = G".(H.0) by A90,A68,A75,A72,A50,FUNCT_1:54
        .= E.0 by A40,A49,FUNCT_1:23;
      per cases by A48,XXREAL_0:1;
      suppose
        x1 = x2;
        hence thesis by A86,A76;
      end;
      suppose
        x1 < x2;
        hence thesis by A57,A83,A91,A89,A87,A77,JORDAN5A:16;
      end;
    end;
    hence thesis by Def3;
  end;
end;

