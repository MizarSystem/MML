A24:      sqrt ((p`1 - pa`1)^2 + (p`2 - pa`2)^2) <r by A18,SQUARE_1:77;
          (Pitag_dist 2).(q,qa)=dist(q,qa) by METRIC_1:def 1;
          then dist(q,qa)<r by A24,TOPREAL3:12;
          then pa in Ball(q,r) by METRIC_1:12;
          then P2 /\ Q <> {} by A19,A22,XBOOLE_0:def 4;
          hence thesis by XBOOLE_0:def 7;
        end;
        hence thesis by A13,PRE_TOPC:def 13;
      end;
      suppose
A25:    p`1 <= s2 & p`1 >= s1 & p`2 = t2;
        for Q being Subset of TOP-REAL 2 st Q is open holds
        p in Q implies P2 meets Q
        proof
          let Q be Subset of TOP-REAL 2;
          assume that
A26:      Q is open and
A27:      p in Q;
              reconsider QQ = Q as Subset of TopSpaceMetr Euclid 2 by YY;
