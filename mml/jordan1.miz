      A12: v1 = |[ (1-l)*x2,(1-l)*y0 ]| + l*|[ x1,y0 ]| by A4,A6,A9,EUCLID:62
        .= |[ (1-l)*x2,(1-l)*y0 ]| + |[ l*x1,l*y0 ]| by EUCLID:62
        .= |[ (1-l)*x2+l*x1,(1-l)*y0+l*y0 ]| by EUCLID:60
        .= |[ (1-l)*x2+l*x1,1 * y0 ]|;
           (1-l)*x2+l*x1 >= x0 by A5,A7,A10,A11,XREAL_1:175;
