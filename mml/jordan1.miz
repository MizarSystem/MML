            then not( s1<=pa`1 & pa`1<=s2 & t1<=pa`2 & pa`2<=t2) by A29,A34,
REAL_2:174;
            then A36:pa in P2 by A2;
           reconsider qa=pa as Point of Euclid 2 by TOPREAL3:13;
                 0 <= (p`1 - pa`1)^2 & 0 <= (p`2 - pa`2)^2 by SQUARE_1:72;
           then A37: 0+0 <= (p`1 - pa`1)^2 + (p`2 - pa`2)^2 by REAL_1:55;
           A38:(p`1 - pa`1)^2 =0 by A34,SQUARE_1:60,XCMPLX_1:14;
           A39:pa`2 - p`2 < r by A32,A34,SEQ_2:4;
                (pa`2-p`2)^2=(p`2-pa`2)^2 by Lm9;
           then (p`1 - pa`1)^2 + (p`2 - pa`2)^2 < r^2
 by A34,A35,A38,A39,SQUARE_1:78;
           then (sqrt ((p`1 - pa`1)^2 + (p`2 - pa`2)^2))^2<r^2
                            by A37,SQUARE_1:def 4;
           then A40:sqrt ((p`1 - pa`1)^2 + (p`2 - pa`2)^2) <r
                 by A32,SQUARE_1:77;
                Euclid 2=MetrStruct(#REAL 2,Pitag_dist 2#) by EUCLID:def 7;
           then (Pitag_dist 2).(q,qa)=dist(q,qa) by METRIC_1:def 1;
           then dist(q,qa)<r by A40,TOPREAL3:12;
           then pa in Ball(q,r) by METRIC_1:12;
           then P2 /\ Q <> {} by A33,A36,XBOOLE_0:def 3;
           hence thesis by XBOOLE_0:def 7;
          end;
       hence x in Cl P2 by A16,PRE_TOPC:def 13;
      end;
      suppose A41:    p`1 <= s2 & p`1 >= s1 & p`2 = t1;
              for Q being Subset of TOP-REAL 2 st Q is open holds
          p in Q implies P2 meets Q
          proof let Q be Subset of TOP-REAL 2;assume that
           A42:Q is open and A43:p in Q;
           consider r being real number such that
           A44: r>0 and A45:Ball(q,r) c= Q by A42,A43,Lm10,TOPMETR:22;
           reconsider r as Real by XREAL_0:def 1;
           set pa=|[p`1,p`2-r/2]|;
            A46:pa`1=p`1 & pa`2=p`2-r/2 by EUCLID:56;
            A47: r/2>0 by A44,SEQ_2:3;
            then not( s1<=pa`1 & pa`1<=s2 & t1<=pa`2 & pa`2<=t2)
            by A41,A46,REAL_2:174;
            then A48:pa in P2 by A2;
           reconsider qa=pa as Point of Euclid 2 by TOPREAL3:13;
                 0 <= (p`1 - pa`1)^2 & 0 <= (p`2 - pa`2)^2 by SQUARE_1:72;
           then A49: 0+0 <= (p`1 - pa`1)^2 + (p`2 - pa`2)^2 by REAL_1:55;
           A50:(p`1 - pa`1)^2 =0 by A46,SQUARE_1:60,XCMPLX_1:14;
            p`2 - pa`2 < r by A44,A46,SEQ_2:4;
           then (p`1 - pa`1)^2 + (p`2 - pa`2)^2 < r^2
            by A46,A47,A50,SQUARE_1:78;
           then (sqrt ((p`1 - pa`1)^2 + (p`2 - pa`2)^2))^2<r^2
                            by A49,SQUARE_1:def 4;
           then A51:sqrt ((p`1 - pa`1)^2 + (p`2 - pa`2)^2) <r
                 by A44,SQUARE_1:77;
                Euclid 2=MetrStruct(#REAL 2,Pitag_dist 2#) by EUCLID:def 7;
           then (Pitag_dist 2).(q,qa)=dist(q,qa) by METRIC_1:def 1;
           then dist(q,qa)<r by A51,TOPREAL3:12;
           then pa in Ball(q,r) by METRIC_1:12;
           hence thesis by A45,A48,XBOOLE_0:3;
