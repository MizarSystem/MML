theorem Th24:
for s1,t1,s2,t2,P st P = { |[ s,t ]|:not (s1<=s & s<=s2 & t1<=t & t<=t2)}
   holds P is connected
proof let s1,t1,s2,t2,P;assume
        P={ |[ s,t ]|:not (s1<=s & s<=s2 & t1<=t & t<=t2)};
  then A1: P={ |[ s3,t3 ]|:s3<s1} \/ {|[s4,t4]|:t4<t1}\/ {|[s5,t5]|:s2<s5}
      \/ {|[s6,t6]|:t2<t6} by Th13;
  reconsider A0={ |[ s,t ]|:s<s1}, A1={ |[ s,t ]|:t<t1},
             A2={ |[ s,t ]|:s2<s}, A3={ |[ s,t ]|:t2<t}
   as Subset of TOP-REAL 2 by Lm2,Lm3,Lm4,Lm5;
A2:s1-1<s1 by REAL_2:174;A3:t1-1<t1 by REAL_2:174;
     A4: |[s1-1,t1-1]| in A0 by A2;
           |[s1-1,t1-1]| in A1 by A3;
     then A0 /\ A1 <> {} by A4,XBOOLE_0:def 3;
     then A5: A0 meets A1 by XBOOLE_0:def 7;
     A6: s2< s2+1 by REAL_2:174;
     A7:|[s2+1,t1-1]| in A1 by A3;
           |[s2+1,t1-1]| in A2 by A6;
     then A1 /\ A2 <> {} by A7,XBOOLE_0:def 3;
     then A8: A1 meets A2 by XBOOLE_0:def 7;
     A9: t2< t2+1 by REAL_2:174;
     A10:|[s2+1,t2+1]| in A2 by A6;
           |[s2+1,t2+1]| in A3 by A9;
     then A2 /\ A3 <> {} by A10,XBOOLE_0:def 3;
     then A11: A2 meets A3 by XBOOLE_0:def 7;
     A12:A0 is connected by Th19;
     A13:A1 is connected by Th23;
     A14:A2 is connected by Th17;
           A3 is connected by Th21;
     hence thesis by A1,A5,A8,A11,A12,A13,A14,Th8;
    end;

theorem Th25:
for s1 for P being Subset of TOP-REAL 2 st P = { |[ s,t ]|:s1<s } holds
  P is open
proof let s1; let P be Subset of TOP-REAL 2 such that A1:P= { |[ s,t ]|:s1<s };
       for pe being Point of Euclid 2 st pe in P ex r being real number st r>0
&
 Ball(pe,r) c= P
  proof let pe be Point of Euclid 2;assume pe in P;
   then consider s,t such that A2:|[ s,t ]|=pe & s1<s by A1;
   set r=(s-s1)/2;     s-s1>0 by A2,XREAL_1:52; then A3: r>0 by REAL_2:127;
         Ball(pe,r) c= P
    proof let x be set; assume x in Ball(pe,r);
     then x in {q where q is Element of Euclid 2:dist(pe,q)<r}
        by METRIC_1:18;
     then consider q being Element of Euclid 2
 such that A4: q=x and A5: dist(pe,q)<r;
      reconsider ppe=pe, pq=q as Point of TOP-REAL 2;
       (Pitag_dist 2).(pe,q)=dist(pe,q) by METRIC_1:def 1;
      then A6: sqrt ((ppe`1 - pq`1)^2 + (ppe`2 - pq`2)^2)<r by A5,TOPREAL3:12;
      A7: 0 <= (ppe`1 - pq`1)^2 & 0 <= (ppe`2 - pq`2)^2 by XREAL_1:65;
      then A8: 0+0 <= (ppe`1 - pq`1)^2 + (ppe`2 - pq`2)^2 by XREAL_1:9;

      then 0 <=sqrt ( (ppe`1 - pq`1)^2 + (ppe`2 - pq`2)^2) by SQUARE_1:def 4;
      then (sqrt ((ppe`1 - pq`1)^2 + (ppe`2 - pq`2)^2))^2<r^2
 by A6,SQUARE_1:78;
      then A9:(ppe`1 - pq`1)^2 + (ppe`2 - pq`2)^2 < r^2 by A8,SQUARE_1:def 4;
            (ppe`1 - pq`1)^2 <= (ppe`1 - pq`1)^2 + (ppe`2 - pq`2)^2 by A7,
REAL_2:173
;
      then (ppe`1 - pq`1)^2 <r^2 by A9,XXREAL_0:2;
      then ppe`1 - pq`1 < r by A3,SQUARE_1:77; then ppe`1 <pq`1+r by XREAL_1:21
;
      then ppe`1 - r < pq`1 by XREAL_1:21;
      then A10: s-(s-s1)/2 < pq`1 by A2,EUCLID:56;
            s-(s-s1)/2 =r+s1;
      then s1< s-(s-s1)/2 by A3,REAL_2:174;
      then |[pq`1,pq`2]|=x & s1<pq`1 by A4,A10,EUCLID:57,XXREAL_0:2;
      hence thesis by A1;
    end;
   hence thesis by A3;
     end;
 hence thesis by TOPMETR:22;
end;

