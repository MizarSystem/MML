       P={ |[ s,t ]|:not (s1<=s & s<=s2 & t1<=t & t<=t2)};
  then A1: P={ |[ s3,t3 ]|:s3<s1} \/ {|[s4,t4]|:t4<t1}\/ {|[s5,t5]|:s2<s5}
      \/ {|[s6,t6]|:t2<t6} by Th13;
  reconsider A0={ |[ s,t ]|:s<s1}, A1={ |[ s,t ]|:t<t1},
             A2={ |[ s,t ]|:s2<s}, A3={ |[ s,t ]|:t2<t}
   as Subset of TOP-REAL 2 by Lm2,Lm3,Lm4,Lm5,Lm6;
A2:s1-1<s1 by REAL_2:174;A3:t1-1<t1 by REAL_2:174;
     A4: |[s1-1,t1-1]| in A0 by A2;
          |[s1-1,t1-1]| in A1 by A3;
     then A0 /\ A1 <> {} by A4,XBOOLE_0:def 3;
     then A5: A0 meets A1 by XBOOLE_0:def 7;
     A6: s2< s2+1 by REAL_2:174;
     A7:|[s2+1,t1-1]| in A1 by A3;
          |[s2+1,t1-1]| in A2 by A6;
     then A1 /\ A2 <> {} by A7,XBOOLE_0:def 3;
     then A8: A1 meets A2 by XBOOLE_0:def 7;
     A9: t2< t2+1 by REAL_2:174;
     A10:|[s2+1,t2+1]| in A2 by A6;
          |[s2+1,t2+1]| in A3 by A9;
