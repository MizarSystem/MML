begin  :: Some theorems about R_eal numbers

reserve x,y,a,b,a1,b1,a2,b2 for R_eal;

canceled 7;

theorem
  for a,b,c being R_eal holds (b <> -infty & b <> +infty &
  not (a = -infty & c = -infty) & not (a = +infty & c = +infty)) implies
  (c - b) + (b - a) = c - a
proof
  let a,b,c be R_eal;
  assume
A1: b <> -infty & b <> +infty &
  not (a = -infty & c = -infty) & not (a = +infty & c = +infty);
A2: (a in REAL or a in {-infty,+infty}) & (b in REAL or b in {-infty,+infty
  }) by XBOOLE_0:def 2,XXREAL_0:def 4;
  per cases by A1,A2,TARSKI:def 2;
  suppose
A3: a in REAL & b in REAL;
A4: c in REAL or c in {-infty,+infty} by XBOOLE_0:def 2,XXREAL_0:def 4;
    now per cases by A3,A4,TARSKI:def 2;
      suppose a in REAL & b in REAL & c in REAL;
        then consider x,y,z being Real such that
A5:     x = a & y = b & z = c;
        c - b = z - y & b - a = y - x by A5,SUPINF_2:5;
        hence (c -b) + (b - a) = (z - y) + (y - x) by SUPINF_2:1
          .= z - (0 + x)
          .= c - a by A5,SUPINF_2:5;
      end;
      suppose
A6:     a in REAL & b in REAL & c = -infty;
        then
A7:     (c -b) + (b - a) = -infty + (b - a) by SUPINF_2:7;
        consider x,y being Real such that
A8:     x = a & y = b by A6;
        b - a = y - x by A8,SUPINF_2:5;
        then (c -b) + (b - a) = -infty by A7,SUPINF_2:def 2;
        hence thesis by A6,SUPINF_2:7;
      end;
      suppose
A9:     a in REAL & b in REAL & c = +infty;
        then
A10:    (c -b) + (b - a) = +infty + (b - a) by SUPINF_2:6;
        consider x,y being Real such that
A11:    x = a & y = b by A9;
        b - a = y - x by A11,SUPINF_2:5;
        then (c -b) + (b - a) = +infty by A10,SUPINF_2:def 2;
        hence thesis by A9,SUPINF_2:6;
      end;
    end;
    hence thesis;
  end;
  suppose
A12: a = -infty & b in REAL;
A13: c in REAL or c in {-infty,+infty} by XBOOLE_0:def 2,XXREAL_0:def 4;
    now per cases by A1,A12,A13,TARSKI:def 2;
      suppose
A14:    a = -infty & b in REAL & c in REAL;
        then
A15:    (c -b) + (b - a) = (c - b) + (+infty) by SUPINF_2:7;
        consider x,y being Real such that
A16:    x = c & y = b by A14;
        c - b = x - y by A16,SUPINF_2:5;
        then (c -b) + (b - a) = +infty by A15,SUPINF_2:def 2;
        hence thesis by A14,SUPINF_2:7;
      end;
      suppose
A17:    a = -infty & b in REAL & c = +infty;
        then
A18:    (c -b) + (b - a) = (c - b) + (+infty) by SUPINF_2:7;
        c - b = +infty by A17,SUPINF_2:6;
        then (c -b) + (b - a) = +infty by A18,SUPINF_2:def 2;
        hence thesis by A17,SUPINF_2:7;
      end;
    end;
    hence thesis;
  end;
  suppose
A19: a = +infty & b in REAL;
A20: c in REAL or c in {-infty,+infty} by XBOOLE_0:def 2,XXREAL_0:def 4;
    now per cases by A1,A19,A20,TARSKI:def 2;
      suppose
A21:    a = +infty & b in REAL & c in REAL;
        then
A22:    (c -b) + (b - a) = (c - b) + (-infty) by SUPINF_2:6;
        consider x,y being Real such that
A23:    x = c & y = b by A21;
        c - b = x - y by A23,SUPINF_2:5;
        then (c -b) + (b - a) = -infty by A22,SUPINF_2:def 2;
        hence thesis by A21,SUPINF_2:6;
      end;
      suppose
A24:    a = +infty & b in REAL & c = -infty;
        then
A25:    (c -b) + (b - a) = (c - b) + (-infty) by SUPINF_2:6;
        c - b = -infty by A24,SUPINF_2:7;
        then (c -b) + (b - a) = -infty by A25,SUPINF_2:def 2;
        hence thesis by A24,SUPINF_2:6;
      end;
    end;
    hence thesis;
  end;
end;

:: PROPERTIES OF THE INTERVALS

scheme RSetEq {P[set]} : for X1,X2 being Subset of REAL st
  (for x being R_eal holds x in X1 iff P[x]) &
  (for x being R_eal holds x in X2 iff P[x]) holds X1 = X2
proof
  let A1,A2 be Subset of REAL such that
A1: for x being R_eal holds x in A1 iff P[x] and
A2: for x being R_eal holds x in A2 iff P[x];
  thus A1 c= A2
  proof
    let x be real number;
    assume
A3: x in A1;
    then x in REAL;
    then reconsider x as R_eal by NUMBERS:31;
    P[x] by A1,A3;
    hence thesis by A2;
  end;
  let x be real number;
  assume
A4: x in A2;
  then x in REAL;
  then reconsider x as R_eal by NUMBERS:31;
  P[x] by A2,A4;
  hence thesis by A1;
end;

definition
  let a,b be ext-real number;
  defpred P[R_eal] means a <= $1 & $1 <= b & $1 in REAL;
  func |[.a,b.]| -> Subset of REAL means
  :Def1:
  for x being R_eal holds
  x in it iff a <= x & x <= b & x in REAL;
  existence
  proof
    consider A being Subset of ExtREAL such that
A1: for x being R_eal holds x in A iff P[x] from SUPINF_1:sch 1;
    for x being set holds x in A implies x in REAL by A1;
    then reconsider A as Subset of REAL by TARSKI:def 3;
    take A;
    thus thesis by A1;
  end;
  uniqueness
  proof
    thus for X1,X2 being Subset of REAL st
    (for x being R_eal holds x in X1 iff P[x]) &
    (for x being R_eal holds x in X2 iff P[x]) holds X1 = X2 from RSetEq;
  end;
end;

Lx1:
 for a,b being ext-real number
  holds |[.a,b.]| = [.a,b.] /\ REAL
 proof
   let a,b be ext-real number;
   [.a,b.] /\ REAL c= REAL by XBOOLE_1:17;
   then reconsider Y = [.a,b.] /\ REAL as Subset of REAL;
   for x being R_eal holds
     x in Y iff a <= x & x <= b & x in REAL
   proof let x be R_eal;
      x in [.a,b.] iff a <= x & x <= b by XXREAL_1:1;
    hence x in Y iff a <= x & x <= b & x in REAL by XBOOLE_0:def 3;
   end;
  hence |[.a,b.]| = [.a,b.] /\ REAL by Def1;
 end;

registration
  let a,b be real number;
  identify |[.a,b.]| with [.a,b.];
  compatibility
   proof
    thus |[.a,b.]| = [.a,b.] /\ REAL by Lx1
        .= [.a,b.] by XBOOLE_1:28;
   end;
end;

definition
  let a,b be R_eal;
  redefine func ].a,b.[ -> Subset of REAL means
:Def2: for x being R_eal holds x in it iff a < x & x < b;
  coherence
  proof
    for x being set st x in ]. a,b .[ holds x in REAL;
    hence thesis;
  end;
  compatibility
  proof
    let X be Subset of REAL;
    thus X = ].a,b.[ implies for x being R_eal holds
    x in X iff a < x & x < b by XXREAL_1:4;
    assume
A1: for x being R_eal holds x in X iff a < x & x < b;
    thus X c= ].a,b.[
    proof
      let x be real number;
      assume
A2:   x in X;
      then x in REAL;
      then reconsider t=x as R_eal by NUMBERS:31;
      a < t & t < b & x in REAL by A1,A2;
      hence x in ].a,b.[;
    end;
    let x be real number;
    assume
A3: x in ].a,b.[;
    reconsider t=x as R_eal by XXREAL_0:def 1;
    a < t & t < b by A3,XXREAL_1:4;
    hence x in X by A1;
  end;
end;

definition
  let a,b be ext-real number;
  defpred P[R_eal] means a < $1 & $1 <= b & $1 in REAL;
  func ].a,b.]| -> Subset of REAL means
  :Def3:
  for x being R_eal holds
  x in it iff (a < x & x <= b & x in REAL);
  existence
  proof
    consider A being Subset of ExtREAL such that
A1: for x being R_eal holds x in A iff P[x] from SUPINF_1:sch 1;
    for x being set holds x in A implies x in REAL by A1;
    then reconsider A as Subset of REAL by TARSKI:def 3;
    take A;
    thus thesis by A1;
  end;
  uniqueness
  proof
    thus for X1,X2 being Subset of REAL st
    (for x being R_eal holds x in X1 iff P[x]) &
    (for x being R_eal holds x in X2 iff P[x]) holds X1 = X2 from RSetEq;
  end;
  defpred P[R_eal] means a <= $1 & $1 < b & $1 in REAL;
  func |[.a,b.[ -> Subset of REAL means
  :Def4:
  for x being R_eal holds
  x in it iff (a <= x & x < b & x in REAL);
  existence
  proof
    consider A being Subset of ExtREAL such that
A2: for x being R_eal holds x in A iff P[x] from SUPINF_1:sch 1;
    for x being set holds x in A implies x in REAL by A2;
    then reconsider A as Subset of REAL by TARSKI:def 3;
    take A;
    thus thesis by A2;
  end;
  uniqueness
  proof
    thus for X1,X2 being Subset of REAL st
    (for x being R_eal holds x in X1 iff P[x]) &
    (for x being R_eal holds x in X2 iff P[x]) holds X1 = X2 from RSetEq;
  end;
end;

Lx2:
 for a,b being ext-real number
  holds |[.a,b.[ = [.a,b.[ /\ REAL
 proof
   let a,b be ext-real number;
   [.a,b.[ /\ REAL c= REAL by XBOOLE_1:17;
   then reconsider Y = [.a,b.[ /\ REAL as Subset of REAL;
   for x being R_eal holds
     x in Y iff a <= x & x < b & x in REAL
   proof let x be R_eal;
      x in [.a,b.[ iff a <= x & x < b by XXREAL_1:3;
    hence x in Y iff a <= x & x < b & x in REAL by XBOOLE_0:def 3;
   end;
  hence |[.a,b.[ = [.a,b.[ /\ REAL by Def4;
 end;

Lx3:
 for a,b being ext-real number
  holds ].a,b.]| = ].a,b.] /\ REAL
 proof
   let a,b be ext-real number;
   ].a,b.] /\ REAL c= REAL by XBOOLE_1:17;
   then reconsider Y = ].a,b.] /\ REAL as Subset of REAL;
   for x being R_eal holds
     x in Y iff a < x & x <= b & x in REAL
   proof let x be R_eal;
      x in ].a,b.] iff a < x & x <= b by XXREAL_1:2;
    hence x in Y iff a < x & x <= b & x in REAL by XBOOLE_0:def 3;
   end;
  hence ].a,b.]| = ].a,b.] /\ REAL by Def3;
 end;

registration
  let a be real number, b be ext-real number;
  identify |[.a,b.[ with [.a,b.[;
  compatibility
   proof
X:  a in REAL by XREAL_0:def 1;
A:   [.a,b.[ c= REAL
     proof let x be real number;
      assume x in [.a,b.[;
       then a <= x & x < b by XXREAL_1:3;
      hence x in REAL by X,XXREAL_0:46;
     end;
    thus |[.a,b.[ = [.a,b.[ /\ REAL by Lx2
        .= [.a,b.[ by A,XBOOLE_1:28;
   end;
end;

registration
  let a be ext-real number, b be real number;
  identify ].a,b.]| with ].a,b.];
  compatibility
   proof
X:  b in REAL by XREAL_0:def 1;
A:   ].a,b.] c= REAL
     proof let x be real number;
      assume x in ].a,b.];
       then a < x & x <= b by XXREAL_1:2;
      hence x in REAL by X,XXREAL_0:47;
     end;
    thus ].a,b.]| = ].a,b.] /\ REAL by Lx3
        .= ].a,b.] by A,XBOOLE_1:28;
   end;
end;

definition
  let IT be Subset of REAL;
  attr IT is open_interval means
  :Def5:
  ex a,b being R_eal st a <= b & IT = ].a,b.[;
  attr IT is closed_interval means
  ex a,b being R_eal st a <= b & IT = |[.a,b.]|;
end;

registration
  cluster open_interval Subset of REAL;
  existence
  proof
    consider a,b being R_eal such that
A1: a = -infty & b = +infty;
    take ].a,b.[, a,b;
    thus thesis by A1;
  end;
  cluster closed_interval Subset of REAL;
  existence
  proof
    consider a,b being R_eal such that
A2: a = -infty & b = +infty;
    take |[.a,b.]|,a,b;
    thus thesis by A2;
  end;
end;

definition
  let IT be Subset of REAL;
  attr IT is right_open_interval means
  ex a,b being R_eal st a <= b & IT = |[.a,b.[;
end;

notation
  let IT be Subset of REAL;
  synonym IT is left_closed_interval for IT is right_open_interval;
end;

definition
  let IT be Subset of REAL;
  attr IT is left_open_interval means
  ex a,b being R_eal st a <= b & IT = ].a,b.]|;
end;

notation
  let IT be Subset of REAL;
  synonym IT is right_closed_interval for IT is left_open_interval;
end;

registration
  cluster right_open_interval Subset of REAL;
  existence
  proof
    consider a,b being R_eal such that
A1: a = -infty & b = +infty;
    take |[.a,b.[,a,b;
    thus thesis by A1;
  end;
  cluster left_open_interval Subset of REAL;
  existence
  proof
    consider a,b being R_eal such that
A2: a = -infty & b = +infty;
    take ].a,b.]|,a,b;
    thus thesis by A2;
  end;
end;

definition
  let IT be Subset of REAL;
  attr IT is interval means
  :Def9:
  IT is open_interval or IT is closed_interval or
  IT is right_open_interval or IT is left_open_interval;
end;

registration
  cluster interval Subset of REAL;
  existence
  proof
    consider I being open_interval Subset of REAL;
    take I;
    thus I is open_interval or I is closed_interval
    or I is right_open_interval or I is left_open_interval;
  end;
end;

definition
  mode Interval is interval Subset of REAL;
end;

reserve A,B for Interval;

registration
  cluster open_interval -> interval Subset of REAL;
  coherence by Def9;
  cluster closed_interval -> interval Subset of REAL;
  coherence by Def9;
  cluster right_open_interval -> interval Subset of REAL;
  coherence by Def9;
  cluster left_open_interval -> interval Subset of REAL;
  coherence by Def9;
end;

canceled 2;

theorem Th11:
  for x being set, a,b being R_eal st
  (x in ].a,b.[ or x in |[.a,b.]| or x in |[.a,b.[ or x in ].a,b.]|) holds
  x is R_eal
proof
  let x be set, a,b be R_eal;
  assume x in ].a,b.[ or x in |[.a,b.]| or x in |[.a,b.[ or x in ].a,b.]|;
  then x in REAL;
  hence thesis by NUMBERS:31;
end;

theorem Th12:
  for a,b being R_eal st b < a holds
  |[.a,b.]| = {} & |[.a,b.[ = {} & ].a,b.]| = {}
proof
  let a,b be R_eal;
  assume
A1: b < a;
  thus |[.a,b.]| = {}
  proof
    assume
A2: not thesis;
    consider x being Element of |[.a,b.]|;
    reconsider x as R_eal by A2,Th11;
    b <= a & b <> a & a <= x & x <= b by A1,A2,Def1;
    then b <= a & b <> a & a <= b by XXREAL_0:2;
    hence thesis by XXREAL_0:1;
  end;
  thus |[.a,b.[ = {}
  proof
    assume
A3: not thesis;
    consider x being Element of |[.a,b.[;
    reconsider x as R_eal by A3,Th11;
    b <= a & b <> a & a <= x & x <= b by A1,A3,Def4;
    then b <= a & b <> a & a <= b by XXREAL_0:2;
    hence thesis by XXREAL_0:1;
  end;
  assume
A4: not thesis;
  consider x being Element of ].a,b.]|;
  reconsider x as R_eal by A4,Th11;
  b <= a & b <> a & a <= x & x <= b by A1,A4,Def3;
  then b <= a & b <> a & a <= b by XXREAL_0:2;
  hence thesis by XXREAL_0:1;
end;

theorem Th13:
  for a being R_eal holds |[.a,a.[ = {} & ].a,a.]| = {}
proof
  let a be R_eal;
  thus |[.a,a.[ = {}
  proof
    assume
A1: not thesis;
    consider x being Element of |[.a,a.[;
    reconsider x as R_eal by A1,Th11;
    a <= x & x <= a & x <> a by A1,Def4;
    hence thesis by XXREAL_0:1;
  end;
  assume
A2: not thesis;
  consider x being Element of ].a,a.]|;
  reconsider x as R_eal by A2,Th11;
  a <= x & x <= a & x <> a by A2,Def3;
  hence thesis by XXREAL_0:1;
end;

theorem Th14:
  for a being R_eal holds ((a = -infty or a = +infty) implies |[.a,a.]| = {}) &
  ((a <> -infty & a <> +infty) implies |[.a,a.]| = {a})
proof
  let a be R_eal;
  thus (a = -infty or a = +infty) implies |[.a,a.]| = {}
  proof
    assume
A1: a = -infty or a = +infty;
    assume
A2: not thesis;
    per cases by A1;
    suppose
A3:   a = -infty;
      consider x being Element of |[.a,a.]|;
      reconsider x as R_eal by A2,Th11;
      a <= x & x <= a & x in REAL by A2,Def1;
      hence thesis by A3,XXREAL_0:1;
    end;
    suppose
A4:   a = +infty;
      consider x being Element of |[.a,a.]|;
      reconsider x as R_eal by A2,Th11;
      a <= x & x <= a & x in REAL by A2,Def1;
      hence thesis by A4,XXREAL_0:1;
    end;
  end;
  assume
A5: a <> -infty & a <> +infty;
  for x being set holds x in |[.a,a.]| iff x = a
  proof
    let x be set;
    hereby
      assume
A6:   x in |[.a,a.]|;
      then reconsider x' = x as R_eal by Th11;
      a <= x' & x' <= a & x in REAL by A6,Def1;
      hence x = a by XXREAL_0:1;
    end;
    assume
A7: x = a;
    then reconsider x as R_eal;
    x is Real by A5,A7,MEASURE3:2;
    hence thesis by A7,Def1;
  end;
  hence thesis by TARSKI:def 1;
end;

theorem Th15:
  for a,b being R_eal st b <= a holds
   |[.a,b.[ = {} & ].a,b.]| = {} & |[.a,b.]| c= {a}
   & |[.a,b.]| c= {b}
proof
  let a,b be R_eal;
  assume
A1: b <= a;
  thus  |[.a,b.[ = {} & ].a,b.]| = {}
  proof
    per cases;
    suppose b <> a;
      then b < a by A1,XXREAL_0:1;
      hence thesis by Th12;
    end;
    suppose b = a;
      hence thesis by Th13;
    end;
  end;
  thus |[.a,b.]| c= {a}
  proof
    per cases;
    suppose b <> a;
      then b < a by A1,XXREAL_0:1;
      then |[.a,b.]| = {} by Th12;
      hence thesis by XBOOLE_1:2;
    end;
    suppose
A2:   b = a;
      thus |[.a,b.]| c= {a}
      proof
        per cases;
        suppose a = -infty or a = +infty;
          then |[.a,b.]| = {} by A2,Th14;
          hence thesis by XBOOLE_1:2;
        end;
        suppose a <> -infty & a <> +infty;
          hence thesis by A2,Th14;
        end;
      end;
    end;
  end;
  per cases;
  suppose b <> a;
    then b < a by A1,XXREAL_0:1;
    then |[.a,b.]| = {} by Th12;
    hence thesis by XBOOLE_1:2;
  end;
  suppose
A3: b = a;
    thus |[.a,b.]| c= {b}
    proof
      per cases;
      suppose b = -infty or b = +infty;
        then |[.a,b.]| = {} by A3,Th14;
        hence thesis by XBOOLE_1:2;
      end;
      suppose b <> -infty & b <> +infty;
        hence thesis by A3,Th14;
      end;
    end;
  end;
end;

canceled;

theorem Th17:
  for a,b being R_eal st a < b ex x being R_eal st a < x & x < b & x in REAL
proof
  let a,b be R_eal;
  assume
A1: a < b;
A2: (a in REAL or a in {-infty,+infty}) & (b in REAL or b in {-infty,+infty})
  by XBOOLE_0:def 2,XXREAL_0:def 4;
A3: not a = +infty & not b = -infty by A1,XXREAL_0:4,6;
  per cases by A2,A3,TARSKI:def 2;
  suppose a in REAL & b in REAL;
    then consider x,y being Real such that
A4: x = a & y = b & x<=y by A1;
    consider z being real number such that
A5: x<z & z<y by A1,A4,XREAL_1:7;
    reconsider z as Real by XREAL_0:def 1;
    reconsider o = z as R_eal by XXREAL_0:def 1;
    take o;
    thus thesis by A4,A5;
  end;
  suppose
A6: a in REAL & b = +infty;
    then reconsider x = a as Real;
    consider z being real number such that
A7: x<z by XREAL_1:3;
    reconsider z as Real by XREAL_0:def 1;
    reconsider o = z as R_eal by XXREAL_0:def 1;
    take o;
    thus thesis by A6,A7,XXREAL_0:9;
  end;
  suppose
A8: a = -infty & b in REAL;
    then reconsider y = b as Real;
    consider z being real number such that
A9: z<y by XREAL_1:4;
    reconsider z as Real by XREAL_0:def 1;
    reconsider o = z as R_eal by XXREAL_0:def 1;
    take o;
    thus thesis by A8,A9,XXREAL_0:12;
  end;
  suppose
A10: a = -infty & b = +infty;
    take 0.;
    0. = 0;
    hence thesis by A10;
  end;
end;

theorem Th18:
  for a,b,c being R_eal st a < b & a < c
  ex x being R_eal st a < x & x < b & x < c & x in REAL
proof
  let a,b,c be R_eal;
  assume
A1: a < b & a < c;
X: min(b,c) = min{b,c} by XXREAL_2:14;
  reconsider m = min(b,c) as R_eal by XXREAL_0:def 1;
  a < min(b,c) by A1,XXREAL_0:def 8;
  then consider x being R_eal such that
A2: a < x & x < m & x in REAL by Th17;
  ex o being R_eal st o in {b,c} & o <= b
  proof
    take b;
    thus thesis by TARSKI:def 2;
  end;
  then
A3: min(b,c) <= b by X,XXREAL_2:62;
  ex o being R_eal st (o in {b,c} & o <= c)
  proof
    take c;
    thus thesis by TARSKI:def 2;
  end;
  then
A4: min(b,c) <= c by X,XXREAL_2:62;
  take x;
  thus thesis by A2,A3,A4,XXREAL_0:2;
end;

theorem Th19:
