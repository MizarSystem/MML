theorem Th60:
  vol {} = 0. by Def10;

LX3:
  vol A >= 0
   proof per cases;
    suppose A is empty;
     hence thesis by Def10;
    end;
    suppose A is non empty;
      then inf A <= sup A by XXREAL_2:40;
      then sup A - inf A >= 0 by SUPINF_2:72;
     hence thesis by Def10;
    end;
   end;

LX4:
  A c= B implies vol A <= vol B
 proof assume
Z:  A c= B;
  per cases;
  suppose A = {};
   then vol A = 0 by Th60;
  hence vol A <= vol B by LX3;
  end;
  suppose
S:  A <> {};
   then B <> {} by Z;
   then
B: vol B = sup B - inf B by Def10;
A: vol A = sup A - inf A by S,Def10;
C: sup A <= sup B by Z,XXREAL_2:59;
   inf B <= inf A by Z,XXREAL_2:60;
  hence vol A <= vol B by A,B,C,SUPINF_2:15;
  end;
 end;

theorem
  (A c= B & B =[.a,b.] & b <= a) implies (vol(A) = 0. & vol(B) = 0.)
proof
  assume that
A1: A c= B and
A2: B =[.a,b.] and
A3: b <= a;
  per cases by A3,XXREAL_0:1;
  suppose a = b;
    then
U:   B = {a} by A2,XXREAL_1:17;
    then inf B = a & sup B = a by XXREAL_2:11,13;
    then
X:  vol B = a - a by Def10,U
        .= 0 by SUPINF_2:73;
    then
Y:   vol A <= 0 by A1,LX4;
    0 <= vol A by LX3;
   hence thesis by Y,X;
  end;
  suppose b < a;
    then
X:   B = {} by A2,XXREAL_1:29;
     then A = {} by A1;
   hence thesis by X,Th60;
  end;
end;

theorem
  A c= B implies vol A <= vol B by LX4;

