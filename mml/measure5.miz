     inf [.a,b.] = a & sup [.a,b.] = b by Z,XXREAL_2:25,29;
    hence vol [.a,b.] = b - a by A,Def10;
   end;
   assume b < a;
    then [.a,b.] = {} by XXREAL_1:29;
