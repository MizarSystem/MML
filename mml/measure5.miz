theorem
 for a being ext-real number
  holds ].a,+infty.]| = ].a,+infty.[
proof let a be ext-real number;
A: -infty <= a by XXREAL_0:5;
 thus ].a,+infty.]| = ].a,+infty.] /\ ].-infty,+infty.[ by Lx3, XXREAL_1:224
      .= ].a,+infty.[ by A,XXREAL_1:158;
end;

theorem
 for a being ext-real number st a in REAL
  holds |[.a,+infty.]| = [.a,+infty.[
proof let a be ext-real number;
 assume a in REAL;
  then
A: -infty < a by XXREAL_0:12;
 thus |[.a,+infty.]| = [.a,+infty.] /\ ].-infty,+infty.[ by Lx1, XXREAL_1:224
      .= [.a,+infty.[ by A,XXREAL_1:148;
end;

theorem
 for a being ext-real number st a in REAL
  holds |[.-infty,a.]| = ].-infty,a.]
proof let a be ext-real number;
 assume a in REAL;
  then
A: a < +infty by XXREAL_0:9;
 thus |[.-infty,a.]| = [.-infty,a.] /\ ].-infty,+infty.[ by Lx1, XXREAL_1:224
      .= ].-infty,a.] by A,XXREAL_1:149;
end;

theorem
 for a being ext-real number
  holds |[.-infty,a.[ = ].-infty,a.[
proof let a be ext-real number;
A: a <= +infty by XXREAL_0:3;
 thus |[.-infty,a.[ = [.-infty,a.[ /\ ].-infty,+infty.[ by Lx2, XXREAL_1:224
      .= ].-infty,a.[ by A,XXREAL_1:155;
end;

