A1:a = -infty & b = +infty &
   (A = ].a,b.[ or A = [.a,b.] or A = [.a,b.[ or A = ].a,b.]);
then a <' b by SUPINF_1:29,SUPINF_2:19;
   then vol(A) = b - a by A1,Def10
         .= +infty by A1,SUPINF_1:14,SUPINF_2:6;
   hence thesis;
end;

