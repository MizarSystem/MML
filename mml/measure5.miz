theorem
  for a,b being R_eal holds
  ((a < b implies vol ].a,b.[ = b - a) &
  (b <= a implies vol ].a,b.[ = 0.))
  proof
   let a,b being R_eal;
   hereby
    assume
Z:    a < b;
     then
A:    ].a,b.[ <> {} by XXREAL_1:33;
     inf ].a,b.[ = a & sup ].a,b.[ = b by Z,XXREAL_2:28,32;
    hence vol ].a,b.[ = b - a by A,Def10;
   end;
   assume b <= a;
    then ].a,b.[ = {} by XXREAL_1:28;
   hence vol ].a,b.[ = 0. by Def10;
  end;

theorem
  for a,b being R_eal holds
  ((a <= b implies vol [.a,b.] = b - a) &
  (b < a implies vol [.a,b.] = 0.))
