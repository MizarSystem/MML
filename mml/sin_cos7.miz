environ

 vocabularies NUMBERS, XREAL_0, ORDINAL1, POWER, SQUARE_1, ARYTM_3, RELAT_1,
      CARD_1, XXREAL_0, ARYTM_1, COMPLEX1, SIN_COS, FUNCT_3, POLYEQ_1,
      SIN_COS2, FUNCT_1, NEWTON, SIN_COS7;
 notations ORDINAL1, NUMBERS, XCMPLX_0, XXREAL_0, XREAL_0, SQUARE_1, NEWTON,
      QUIN_1, COMPLEX1, SIN_COS, SEQ_1, BINOP_2, SIN_COS2, POWER, POLYEQ_1;
 constructors REAL_1, SQUARE_1, NAT_1, BINOP_2, SEQ_1, QUIN_1, POWER, POLYEQ_1,
      SIN_COS, SIN_COS2, VALUED_1, NEWTON;
 registrations XXREAL_0, XREAL_0, SQUARE_1, MEMBERED, QUIN_1, SIN_COS,
      XBOOLE_0, VALUED_0, RELSET_1;
 requirements REAL, NUMERALS, SUBSET, BOOLE, ARITHM;
 definitions SQUARE_1, XCMPLX_0;
 theorems SQUARE_1, ABSVALUE, SIN_COS, NEWTON, XCMPLX_1, QUIN_1, SIN_COS2,
      TAYLOR_1, POWER, POLYEQ_1, XREAL_1, COMPLEX1, ASYMPT_1, XXREAL_0;

begin

reserve x,y,t for real number;

Lm1: number_e<>1 by TAYLOR_1:11;

Lm2: x^2<1 & y<1 implies x^2*y<1
proof
  assume that
A1: x^2<1 and
A2: y<1;
  per cases by XREAL_1:65;
  suppose
    0=x^2;
    hence thesis;
  end;
  suppose
    0<x^2;
    then y*x^2<1*x^2 by A2,XREAL_1:70;
    hence thesis by A1,XXREAL_0:2;
  end;
end;

theorem Th1:
  x>0 implies 1/x=x to_power (-1)
proof
  assume x>0;
  then x to_power (-1) =(1/x) to_power 1 by POWER:37
    .=1/x by POWER:30;
  hence thesis;
end;

theorem
  x>1 implies ((sqrt(x^2-1))/x)^2<1
proof
A1: -1+x^2<0+x^2 by XREAL_1:8;
  assume x>1;
  then x^2>1^2+0 by SQUARE_1:78;
  then
A2: x^2-1>0 by XREAL_1:22;
  ((sqrt(x^2-1))/x)^2=(sqrt(x^2-1))^2/x^2 by XCMPLX_1:77
    .=(x^2-1)/x^2 by A2,SQUARE_1:def 4;
  hence thesis by A2,A1,XREAL_1:191;
end;

theorem
  (x/sqrt(x^2+1))^2<1
proof
A1: x^2<x^2+1 by XREAL_1:31;
A2: x^2>=0 by XREAL_1:65;
  then (sqrt(x^2+1))^2=sqrt(x^2+1)^2 by SQUARE_1:97
    .=x^2+1 by A2,SQUARE_1:89;
  then
A3: (x/sqrt(x^2+1))^2=(x^2)/(x^2+1) by XCMPLX_1:77;
  per cases by XREAL_1:65;
  suppose
    x^2>0;
    hence thesis by A1,A3,XREAL_1:191;
  end;
  suppose
    x^2=0;
    hence thesis by A3;
  end;
end;

theorem Th4:
  sqrt(x^2+1)>0
proof
  x^2>=0 by XREAL_1:65;
  hence thesis by SQUARE_1:93;
end;

theorem Th5:
  sqrt(x^2+1)+x>0
proof
  per cases;
  suppose
    x>0;
    hence thesis by Th4,XREAL_1:31;
  end;
  suppose
A1: x<=0;
    x^2<x^2+1 by XREAL_1:31;
    then sqrt(x^2)<sqrt(x^2+1) by SQUARE_1:95,XREAL_1:65;
    then -x<sqrt(x^2+1) by A1,SQUARE_1:90;
    hence thesis by XREAL_1:64;
  end;
end;

Lm3: 1<=x implies x^2-1>=0
proof
  assume 1<=x;
  then x^2>=1^2 by SQUARE_1:77;
  then 1+(-1)<=x^2+(-1) by XREAL_1:8;
  hence thesis;
end;

theorem
  y>=0 & x>=1 implies (x+1)/y>=0;

theorem Th7:
  y>=0 & x>=1 implies (x-1)/y>=0
proof
  assume that
A1: y>=0 and
A2: x>=1;
  x+(-1)>=1+(-1) by A2,XREAL_1:8;
  hence thesis by A1;
end;

theorem Th8:
  x>=1 implies sqrt((x+1)/2)>=1
proof
  assume x>=1;
  then x+1>=1+1 by XREAL_1:8;
  then (x+1)/2>=1 by XREAL_1:183;
  hence thesis by SQUARE_1:83,94;
end;

theorem Th9:
  y>=0 & x>=1 implies (x^2-1)/y>=0
proof
  assume that
A1: y>=0 and
A2: x>=1;
  x^2-1>=0 by A2,Lm3;
  hence thesis by A1;
end;

theorem Th10:
  x>=1 implies sqrt((x+1)/2)+sqrt((x-1)/2)>0
proof
  assume
A1: x>=1;
  then (x-1)/2>=0 by Th7;
  then
A2: sqrt((x-1)/2)>=0 by SQUARE_1:82,94;
  sqrt((x+1)/2)>=1 by A1,Th8;
  hence thesis by A2;
end;

theorem Th11:
  x^2<1 implies x+1>0 & 1-x>0
proof
  assume x^2<1;
  then -1<x & x<1 by SQUARE_1:122;
  hence thesis by XREAL_1:150,151;
end;

Lm4: x^2<1 implies (x+1)/(1-x)>0
proof
  assume x^2<1;
  then x+1>0 & 1-x>0 by Th11;
  hence thesis by XREAL_1:141;
end;

theorem Th12:
  x<>1 implies (1-x)^2>0
proof
  assume x<>1;
  then 0<>1-x;
  hence thesis by SQUARE_1:74;
end;

theorem Th13:
  x^2<1 implies (x^2+1)/(1-x^2)>=0
proof
  assume x^2<1;
  then
A1: 0<1-x^2 by XREAL_1:52;
  x^2>=0 by XREAL_1:65;
  hence thesis by A1;
end;

theorem
  x^2<1 implies ((2*x)/(1+x^2))^2<1
proof
  assume x^2<1;
  then x^2+(-1)<1+(-1) by XREAL_1:10;
  then (x^2-1)*(x^2-1)>0*(x^2-1) by XREAL_1:71;
  then
A1: x^2>=0 & (x^2)^2-2*(x^2)+1+4*x^2>0+4*x^2 by XREAL_1:10,65;
  ((2*x)/(1+x^2))^2 = (2*x)^2/(1+x^2)^2 by XCMPLX_1:77
    .=4*x^2/((x^2)^2+2*(x^2)+1);
  hence thesis by A1,XREAL_1:191;
end;

theorem Th15:
  0<x & x<1 implies (1+x)/(1-x)>0
proof
  assume that
A1: 0<x and
A2: x<1;
  x^2<x by A1,A2,SQUARE_1:75;
  then x^2<1 by A2,XXREAL_0:2;
  hence thesis by Lm4;
end;

theorem
  0<x & x<1 implies x^2<1
proof
  assume that
A1: 0<x and
A2: x<1;
  x^2<x by A1,A2,SQUARE_1:75;
  hence thesis by A2,XXREAL_0:2;
end;

Lm5: 0<x & x<1 implies 0<1-x^2
proof
  assume that
A1: 0<x and
A2: x<1;
  x^2<x by A1,A2,SQUARE_1:75;
  then x^2<1 by A2,XXREAL_0:2;
  then x^2-x^2<1-x^2 by XREAL_1:11;
  hence thesis;
end;

theorem
  0<x & x<1 implies 1/(sqrt(1-x^2))>1
proof
  assume that
A1: 0<x and
A2: x<1;
A3: 0<1-x^2 by A1,A2,Lm5;
  then
A4: 0<sqrt(1-x^2) by SQUARE_1:93;
  x*x>0*x by A1,XREAL_1:70;
  then x^2*(-1)<0*(-1) by XREAL_1:71;
  then -x^2+1<0+1 by XREAL_1:10;
  then sqrt(1-x^2)<1 by A3,SQUARE_1:83,95;
  hence thesis by A4,XREAL_1:189;
end;

theorem Th18:
  0<x & x<1 implies (2*x)/(1-x^2)>0
proof
  assume that
A1: 0<x and
A2: x<1;
  x^2<x by A1,A2,SQUARE_1:75;
  then x^2<1 by A2,XXREAL_0:2;
  then
A3: x^2+(-x^2)<1+(-x^2) by XREAL_1:10;
  0*2<x*2 by A1,XREAL_1:70;
  hence thesis by A3,XREAL_1:141;
end;

theorem Th19:
  0<x & x<1 implies 0<(1-x^2)^2
proof
  assume that
A1: 0<x and
A2: x<1;
  x^2<x by A1,A2,SQUARE_1:75;
  then x^2<1 by A2,XXREAL_0:2;
  then x^2+(-x^2)<1+(-x^2) by XREAL_1:10;
  hence thesis by SQUARE_1:74;
end;

theorem
  0<x & x<1 implies ((1+x^2)/(1-x^2))>1
proof
  assume that
A1: 0<x and
A2: x<1;
  x^2<x by A1,A2,SQUARE_1:75;
  then x^2<1 by A2,XXREAL_0:2;
  then
A3: x^2+(-x^2)<1+(-x^2) by XREAL_1:10;
  0*x<x*x by A1,XREAL_1:70;
  then -x^2+1<x^2+1 by XREAL_1:10;
  hence thesis by A3,XREAL_1:189;
end;

theorem
  1<x^2 implies (1/x)^2<1
proof
  assume
A1: 1<x^2;
  then (1/x^2)<((x^2)/(x^2)) by XREAL_1:76;
  then (1^2/x^2)<1 by A1,XCMPLX_1:60;
  hence thesis by XCMPLX_1:77;
end;

theorem Th22:
  0<x & x<=1 implies 1-x^2>=0
proof
  assume that
A1: 0<x and
A2: x<=1;
A3: x-1<=1-1 by A2,XREAL_1:11;
  per cases by A3;
  suppose
    x-1<0;
    then x-1+1<0+1 by XREAL_1:10;
    hence thesis by A1,Lm5;
  end;
  suppose
    x-1=0;
    hence thesis;
  end;
end;

theorem Th23:
  1<=x implies 0<x+sqrt(x^2-1)
proof
  assume
A1: 1<=x;
  then x^2-1>=0 by Lm3;
  then 0<=sqrt(x^2-1) by SQUARE_1:def 4;
  hence thesis by A1;
end;

theorem Th24:
  1<=x & 1<=y implies 0<=x*sqrt(y^2-1)+y*sqrt(x^2-1)
proof
  assume that
A1: 1<=x and
A2: 1<=y;
  0<=y^2-1 by A2,Lm3;
  then
A3: 0<=sqrt(y^2-1) by SQUARE_1:def 4;
  0<=x^2-1 by A1,Lm3;
  then 0<=sqrt(x^2-1) by SQUARE_1:def 4;
  hence thesis by A1,A2,A3;
end;

theorem Th25:
  1<=y implies 0<y-sqrt(y^2-1)
proof
  assume
A1: 1<=y;
  then 1*y<=y*y by XREAL_1:66;
  then 1<=y^2 by A1,XXREAL_0:2;
  then
A2: 1-1<=y^2-1 by XREAL_1:15;
  -1+y^2<0+y^2 by XREAL_1:10;
  then sqrt(y^2-1)<sqrt(y^2) by A2,SQUARE_1:95;
  then sqrt(y^2-1)<y by A1,SQUARE_1:89;
  then sqrt(y^2-1)-sqrt(y^2-1)<y-sqrt(y^2-1) by XREAL_1:16;
  hence thesis;
end;

theorem Th26:
  1<=x & 1<=y & abs(y)<=abs(x) implies 0<=y*sqrt(x^2-1)-x*sqrt(y^2 -1)
proof
  assume that
A1: 1<=x and
A2: 1<=y and
A3: abs(y)<=abs(x);
A4: 0<=y^2-1 by A2,Lm3;
A5: (abs x)^2=x^2 & (abs y)^2=y^2 by COMPLEX1:161;
  1<=abs(y) by A2,ABSVALUE:def 1;
  then y^2<=x^2 by A3,A5,SQUARE_1:77;
  then (-1)*x^2<=(-1)*y^2 by XREAL_1:67;
  then -x^2+x^2*y^2<=-y^2+x^2*y^2 by XREAL_1:8;
  then 0<=x^2 & sqrt(x^2*(y^2-1))<=sqrt(y^2*(x^2-1)) by A1,A4,SQUARE_1:94
  ;
  then sqrt(x^2)*sqrt(y^2-1)<=sqrt(y^2*(x^2-1)) by A4,SQUARE_1:97;
  then
A6: x*sqrt(y^2-1)<=sqrt(y^2*(x^2-1)) by A1,SQUARE_1:89;
  0<=y^2 & 0<=x^2-1 by A1,Lm3,XREAL_1:65;
  then x*sqrt(y^2-1)<=sqrt(y^2)*sqrt(x^2-1) by A6,SQUARE_1:97;
  then x*sqrt(y^2-1)<=y*sqrt(x^2-1) by A2,SQUARE_1:89;
  then x*sqrt(y^2-1)-x*sqrt(y^2-1)<=y*sqrt(x^2-1)-x*sqrt(y^2-1) by
  XREAL_1
  :15;
  hence thesis;
end;

theorem Th27:
  x^2<1 & y^2<1 implies x*y<>-1
proof
  assume
A1: x^2<1 & y^2<1;
  assume x*y=-1;
  then x*y*(x*y)=1;
  then (x*x)*(y*y)=1;
  hence contradiction by A1,Lm2;
end;

theorem Th28:
  x^2<1 & y^2<1 implies x*y<>1
proof
  assume
A1: x^2<1 & y^2<1;
  assume x*y=1;
  then x*y*(x*y)=1;
  then (x*x)*(y*y)=1;
  hence contradiction by A1,Lm2;
end;

theorem Th29:
  x<>0 implies exp_R(x)<>1
proof
  assume
A1: x<>0;
  assume
A2: exp_R(x)=1;
  x=log(number_e,exp_R(x)) by TAYLOR_1:12
    .=0 by A2,Lm1,POWER:59,TAYLOR_1:11;
  hence contradiction by A1;
end;

theorem Th30:
  0<>x implies (exp_R x)^2-1<>0
proof
  assume
A1: 0<>x;
  assume (exp_R x)^2-1=0;
  then exp_R x=1 or exp_R x= -1 by XCMPLX_1:183;
  hence contradiction by A1,Th29,SIN_COS:60;
end;

Lm6: x^2+1>0
proof
  x^2>=0 by XREAL_1:65;
  hence thesis;
end;

theorem Th31:
  (t^2-1)/(t^2+1)<1
proof
A1: 0<t^2+1 by Lm6;
  -1+t^2<1+t^2 by XREAL_1:10;
  then (-1+t^2)/((1+t^2))<(1+t^2)/((1+t^2)) by A1,XREAL_1:76;
  hence thesis by A1,XCMPLX_1:60;
end;

theorem Th32:
  -1<t & t<1 implies 0<(t+1)/(1-t)
proof
  assume -1<t & t<1;
  then -1+1<t+1 & t-t<1-t by XREAL_1:10;
  then 0/(1-t)<(t+1)/(1-t) by XREAL_1:76;
  hence thesis;
end;

Lm7: 1<t or t< -1 implies 0<(t+1)/(t-1)
proof
  assume
A1: 1<t or t < -1;
  per cases by A1;
  suppose
A2: 1<t;
    then 1-1<t-1 by XREAL_1:16;
    then 0/(t-1)<(t+1)/(t-1) by A2,XREAL_1:76;
    hence thesis;
  end;
  suppose
A3: t< -1;
    then t+1< -1+1 by XREAL_1:10;
    then 0/(t-1)<(t+1)/(t-1) by A3,XREAL_1:77;
    hence thesis;
  end;
end;

Lm8: sqrt(x^2+1)>x
proof
  per cases;
  suppose
A1: x>=0;
    x^2+1>x^2+0 by XREAL_1:10;
    then sqrt(x^2+1)>sqrt(x^2) by SQUARE_1:95,XREAL_1:65;
    hence thesis by A1,SQUARE_1:89;
  end;
  suppose
    x<0;
    hence thesis by Th4;
  end;
end;

Lm9: sqrt(x^2+1)*sqrt(y^2+1)-x*y>0
proof
A1: y^2+1>0 by Lm6;
  assume (sqrt(x^2+1)*sqrt(y^2+1))-x*y<=0;
  then
A2: sqrt(x^2+1)*sqrt(y^2+1)<=x*y by XREAL_1:52;
  sqrt(x^2+1)>0 & sqrt(y^2+1)>0 by Th4;
  then (sqrt(x^2+1)*sqrt(y^2+1))^2<=(x*y)^2 by A2,SQUARE_1:77;
  then
A3: (sqrt(x^2+1))^2*(sqrt(y^2+1))^2<=(x*y)^2;
  x^2+1>0 by Lm6;
  then (x^2+1)*(sqrt(y^2+1))^2<=(x*y)^2 by A3,SQUARE_1:def 4;
  then (x^2+1)*(y^2+1)<=(x*y)^2 by A1,SQUARE_1:def 4;
  then
A4: (x*y)^2+x^2+y^2+1-(x*y)^2<=(x*y)^2-(x*y)^2 by XREAL_1:15;
  0<=x^2 & 0<=y^2 by XREAL_1:65;
  then 0+1<=x^2+y^2+1 by XREAL_1:8;
  hence contradiction by A4;
end;

Lm10: 1<=y implies y+sqrt(y^2-1)>0
proof
  assume
A1: 1<=y;
  then 0<=y^2-1 by Lm3;
  then 0<=sqrt(y^2-1) by SQUARE_1:def 4;
  hence thesis by A1;
end;

Lm11: 0<t implies -1<(t^2-1)/(t^2+1)
proof
A1: 0<t^2+1 by Lm6;
  assume 0<t;
  then 0<t^2 by SQUARE_1:74;
  then 0*2<2*t^2 by XREAL_1:70;
  then 0-(t^2+1)<2*t^2-(t^2+1) by XREAL_1:16;
  then (-(t^2+1))/(t^2+1)<(t^2-1)/(t^2+1) by A1,XREAL_1:76;
  then -(t^2+1)/(t^2+1)<(t^2-1)/(t^2+1);
  hence thesis by A1,XCMPLX_1:60;
end;

Lm12: 0<t & t<>1 implies 1<(t^2+1)/(t^2-1) or (t^2+1)/(t^2-1)< -1
proof
  assume that
A1: 0<t and
A2: t<>1;
  1<t or t<1 by A2,XXREAL_0:1;
  then
A3: 1-1<t-1 or t-1<1-1 by XREAL_1:16;
  per cases by A3;
  suppose
    0<t-1;
    then
A4: 0+1<t-1+1 by XREAL_1:10;
    then t<t^2 by SQUARE_1:76;
    then 1<t^2 by A4,XXREAL_0:2;
    then
A5: 1-1<t^2-1 by XREAL_1:16;
    -1+t^2<1+t^2 by XREAL_1:10;
    hence thesis by A5,XREAL_1:189;
  end;
  suppose
    t-1<0;
    then
A6: t-1+1<0+1 by XREAL_1:10;
    then t^2<t by A1,SQUARE_1:75;
    then t^2<1 by A6,XXREAL_0:2;
    then
A7: t^2-t^2<1-t^2 by XREAL_1:16;
    0<t^2 by A1,SQUARE_1:74;
    then -t^2+1<t^2+1 by XREAL_1:10;
    then 1<(t^2+1)/(1-t^2) by A7,XREAL_1:189;
    then (-1)*((t^2+1)/(1-t^2))<1*(-1) by XREAL_1:71;
    then (-(t^2+1))/(1-t^2)<1*(-1);
    then (t^2+1)/(-(1-t^2))<1*(-1) by XCMPLX_1:193;
    hence thesis;
  end;
end;

Lm13: 0<t implies 0<2*t/(1+t^2)
proof
  assume 0<t;
  then
A1: 0*2<2*t by XREAL_1:70;
  t^2+1>0 by Lm6;
  then 0/(t^2+1)<2*t/(t^2+1) by A1,XREAL_1:76;
  hence thesis;
end;

Lm14: 0<t implies 2*t/(1+t^2)<=1
proof
  0<=(t-1)^2 by XREAL_1:65;
  then
A1: 0+2*t<=t^2-2*t+1+2*t by XREAL_1:8;
  assume 0<t;
  hence thesis by A1,XREAL_1:185;
end;

Lm15: 0<t implies (1-sqrt(1+t^2))/t<0
proof
  assume
A1: 0<t;
  then 0+1<t^2+1 by SQUARE_1:74,XREAL_1:10;
  then 1<sqrt(t^2+1) by SQUARE_1:83,95;
  then -sqrt(t^2+1)< -1 by XREAL_1:26;
  then -sqrt(t^2+1)+1< -1+1 by XREAL_1:10;
  then (1-sqrt(t^2+1))/t<0/t by A1,XREAL_1:76;
  hence thesis;
end;

Lm16: 0<t & t<=1 implies 0<=1-t^2
proof
  assume 0<t & t<=1;
  then t^2<=1^2 by SQUARE_1:77;
  then t^2-t^2<=1-t^2 by XREAL_1:15;
  hence thesis;
end;

Lm17: 0<t & t<=1 implies 0<=4-4*t^2
proof
  assume 0<t & t<=1;
  then 0<=1-t^2 by Lm16;
  then 0*4<=4*(1-t^2);
  hence thesis;
end;

Lm18: 0<t & t<=1 implies 0<1+sqrt(1-t^2)
proof
  assume 0<t & t<=1;
  then 0<=1-t^2 by Lm16;
  then 0<=sqrt(1-t^2) by SQUARE_1:82,94;
  hence thesis;
end;

Lm19: 0<t & t<=1 implies 0<(1+sqrt(1-t^2))/t
proof
  assume that
A1: 0<t and
A2: t<=1;
  0<1+sqrt(1-t^2) by A1,A2,Lm18;
  then 0/t<(1+sqrt(1-t^2))/t by A1,XREAL_1:76;
  hence thesis;
end;

Lm20: 0<t & t<>1 implies 2*t/(t^2-1)<>0
proof
  assume that
A1: 0<t and
A2: t<>1;
  per cases by A2,XXREAL_0:1;
  suppose
A3: 1<t;
    then t<t^2 by SQUARE_1:76;
    then 1<t^2 by A3,XXREAL_0:2;
    then
A4: 1-1<t^2-1 by XREAL_1:16;
    2*1<2*t by A3,XREAL_1:70;
    then 0/(t^2-1)<2*t/(t^2-1) by A4,XREAL_1:76;
    hence thesis;
  end;
  suppose
A5: t<1;
    then t^2<t by A1,SQUARE_1:75;
    then t^2<1 by A5,XXREAL_0:2;
    then
A6: t^2-1<1-1 by XREAL_1:16;
    0*2<2*t by A1,XREAL_1:70;
    then 2*t/(t^2-1)<0/(t^2-1) by A6,XREAL_1:77;
    hence thesis;
  end;
end;

Lm21: t<0 implies (1+sqrt(1+t^2))/t<0
proof
  assume
A1: t<0;
  0+1<(sqrt(1+t^2))+1 by Th4,XREAL_1:10;
  then (1+sqrt(1+t^2))/t<0/t by A1,XREAL_1:77;
  hence thesis;
end;

begin :: Main Part

definition
  let x be real number;
  func sinh"(x) -> real number equals
  log(number_e,(x+sqrt(x^2+1)));
  coherence;
end;

definition
  let x be real number;
  func cosh1"(x) -> real number equals
  log(number_e,x+sqrt(x^2-1));
  coherence;
end;

definition
  let x be real number;
  func cosh2"(x) -> real number equals
  -(log(number_e,x+sqrt(x^2-1)));
  coherence;
end;

definition
  let x be real number;
  func tanh"(x) -> real number equals
  (1/2)*log(number_e,(1+x)/(1-x));
  coherence;
end;

definition
  let x be real number;
  func coth"(x) -> real number equals
  (1/2)*log(number_e,(x+1)/(x-1));
  coherence;
end;

definition
  let x be real number;
  func sech1"(x) -> real number equals
  log(number_e,(1+sqrt(1-x^2))/x);
  coherence;
end;

definition
  let x be real number;
  func sech2"(x) -> real number equals
  -(log(number_e,(1+sqrt(1-x^2))/x));
  coherence;
end;

definition
  let x be real number;
  func csch"(x) -> real number equals
  :Def8:
  log(number_e,(1+sqrt(1+x^2))/x)
  if 0<x, log(number_e,(1-sqrt(1+x^2))/x) if x<0;
  correctness;
end;

theorem
  0<=x implies sinh"(x)=cosh1"(sqrt(x^2+1))
proof
  assume
A1: 0<=x;
  x^2>=0 by XREAL_1:65;
  then cosh1"(sqrt(x^2+1)) =log(number_e,sqrt(x^2+1)+sqrt(x^2+1-1)) by
  SQUARE_1:def 4
    .=log(number_e,(sqrt(x^2+1)+x)) by A1,SQUARE_1:89;
  hence thesis;
end;

theorem
  x<0 implies sinh"(x)=cosh2"(sqrt(x^2+1))
proof
  assume
A1: x<0;
A2: sqrt(x^2+1)+x>0 by Th5;
A3: 1/(sqrt(x^2+1)+x)=(sqrt(x^2+1)+x) to_power (-1) by Th1,Th5;
A4: x^2>=0 by XREAL_1:65;
  then cosh2"(sqrt(x^2+1)) =-log(number_e,sqrt(x^2+1)+sqrt(x^2+1-1)) by
  SQUARE_1:def 4
    .=-log(number_e,(sqrt(x^2+1)+(-x))) by A1,SQUARE_1:90
    .=-log(number_e,(sqrt(x^2+1)-x)*(sqrt(x^2+1)+x)/(sqrt(x^2+1)+x)) by A2,
  XCMPLX_1:90
    .=-log(number_e,((sqrt(x^2+1))^2-x^2)/(sqrt(x^2+1)+x))
    .=-log(number_e,((x^2+1)-x^2)/(sqrt(x^2+1)+x)) by A4,SQUARE_1:def 4
    .=-((-1)*log(number_e,(sqrt(x^2+1)+x))) by A2,A3,Lm1,POWER:63,TAYLOR_1:
  11
    .=log(number_e,(sqrt(x^2+1)+x));
  hence thesis;
end;

theorem
  sinh"(x)=tanh"(x/sqrt(x^2+1))
proof
  set t = sqrt(x^2+1)+x;
A1: sqrt(x^2+1)+x>0 by Th5;
A2: x^2+1>0 by Lm6;
A3: sqrt(x^2+1)>0 by Th4;
  then
  tanh"(x/sqrt(x^2+1)) =(1/2)*log(number_e,((sqrt(x^2+1)*1+x)/sqrt(x^2+1))
  /(1-x/sqrt(x^2+1))) by XCMPLX_1:114
    .=(1/2)*log(number_e,((sqrt(x^2+1)*1+x)/sqrt(x^2+1))/((1*sqrt(x^2+1)-x)
  /sqrt(x^2+1))) by A3,XCMPLX_1:128
    .=(1/2)*log(number_e,t/(sqrt(x^2+1)-x)) by A3,XCMPLX_1:55
    .=(1/2)*log(number_e,t*t/((sqrt(x^2+1)-x)*t)) by A1,XCMPLX_1:92
    .=(1/2)*log(number_e,(t*t)/((sqrt(x^2+1))*(sqrt(x^2+1))-x^2))
    .=(1/2)*log(number_e,(t*t)/((sqrt((x^2+1)^2))-x^2)) by A2,SQUARE_1:97
    .=(1/2)*log(number_e,(t*t)/((x^2+1)-x^2)) by A2,SQUARE_1:89
    .=(1/2)*log(number_e,t^2)
    .=(1/2)*log(number_e,t to_power 2) by POWER:53
    .=(1/2)*(2*log(number_e,t)) by A1,Lm1,POWER:63,TAYLOR_1:11
    .=log(number_e,t);
  hence thesis;
end;

theorem
  x>=1 implies cosh1"(x)=sinh"(sqrt(x^2-1))
proof
  assume
A1: x>=1;
  then x^2-1>=0 by Lm3;
  then sinh"(sqrt(x^2-1)) =log(number_e,((sqrt(x^2-1))+sqrt((x^2-1)+1))) by
  SQUARE_1:def 4
    .=log(number_e,((sqrt(x^2-1))+x)) by A1,SQUARE_1:89;
  hence thesis;
end;

theorem
  x>1 implies cosh1"(x)=tanh"((sqrt(x^2-1))/x)
proof
  assume
A1: x>1;
  then
A2: sqrt(x^2-1)+x>0 by Th23;
  x^2>1^2+0 by A1,SQUARE_1:78;
  then
A3: x^2-1>0 by XREAL_1:22;
  tanh"((sqrt(x^2-1))/x) =(1/2)*log(number_e,((sqrt(x^2-1)+x*1)/x)/(1-(
  sqrt(x^2-1))/x)) by A1,XCMPLX_1:114
    .=(1/2)*log(number_e,((sqrt(x^2-1)+x)/x)/((1*x-(sqrt(x^2-1)))/x)) by A1,
  XCMPLX_1:128
    .=(1/2)*log(number_e,(sqrt(x^2-1)+x)/(x-sqrt(x^2-1))) by A1,XCMPLX_1:55
    .=(1/2)*log(number_e,((sqrt(x^2-1)+x)*(sqrt(x^2-1)+x))/ ((x-sqrt(x^2-1))
  *(sqrt(x^2-1)+x))) by A2,XCMPLX_1:92
    .=(1/2)*log(number_e,((sqrt(x^2-1)+x)*(sqrt(x^2-1)+x))/ (x^2-(sqrt(x^2-1
  ))^2))
    .=(1/2)*log(number_e,((sqrt(x^2-1)+x)*(sqrt(x^2-1)+x))/ (x^2-(x^2-1)))
  by A3,SQUARE_1:def 4
    .=(1/2)*log(number_e,((sqrt(x^2-1)+x)^2))
    .=(1/2)*log(number_e,((sqrt(x^2-1)+x) to_power 2)) by POWER:53
    .=(1/2)*(2*log(number_e,(sqrt(x^2-1)+x))) by A2,Lm1,POWER:63,TAYLOR_1:11
    .=log(number_e,(sqrt(x^2-1)+x));
  hence thesis;
end;

theorem
  x>=1 implies cosh1"(x)=2*cosh1"(sqrt((x+1)/2))
proof
  assume
A1: x>=1;
  then
A2: (x-1)/2>=0 by Th7;
A3: (x^2-1)/4>=0 by A1,Th9;
A4: sqrt((x+1)/2)+sqrt((x-1)/2)>0 by A1,Th10;
  2*cosh1"(sqrt((x+1)/2)) =2*(log(number_e,(sqrt((x+1)/2))+sqrt(((x+1)/2)-
  1))) by A1,SQUARE_1:def 4
    .=log(number_e,((sqrt((x+1)/2))+sqrt((x-1)/2)) to_power 2) by A4,Lm1,
  POWER:63,TAYLOR_1:11
    .=log(number_e,((sqrt((x+1)/2))+sqrt((x-1)/2))^2) by POWER:53
    .=log(number_e,(sqrt((x+1)/2))^2+2*(sqrt((x+1)/2))* (sqrt((x-1)/2))+(
  sqrt((x-1)/2))^2)
    .=log(number_e,((x+1)/2)+2*(sqrt((x+1)/2))* (sqrt((x-1)/2))+(sqrt((x-1)/
  2))^2) by A1,SQUARE_1:def 4
    .=log(number_e,(x+1)/2+2*sqrt((x+1)/2)* sqrt((x-1)/2)+(x-1)/2) by A2,
  SQUARE_1:def 4
    .=log(number_e,x+2*(sqrt((x+1)/2)*sqrt((x-1)/2)))
    .=log(number_e,x+2*(sqrt(((x+1)/2)*((x-1)/2)))) by A1,A2,SQUARE_1:97
    .=log(number_e,x+sqrt(2^2)*(sqrt((x^2-1)/4))) by SQUARE_1:89
    .=log(number_e,x+sqrt(4*((x^2-1)/4))) by A3,SQUARE_1:97
    .=log(number_e,x+sqrt(x^2-1));
  hence thesis;
end;

theorem
  x>=1 implies cosh2"(x)=2*cosh2"(sqrt((x+1)/2))
proof
  assume
A1: x>=1;
  then
A2: (x-1)/2>=0 by Th7;
A3: (x^2-1)/4>=0 by A1,Th9;
A4: sqrt((x+1)/2)+sqrt((x-1)/2)>0 by A1,Th10;
  2*cosh2"(sqrt((x+1)/2)) =2*(-(log(number_e,(sqrt((x+1)/2))+sqrt(((x+1)/2
  )-1)))) by A1,SQUARE_1:def 4
    .=-(2*(log(number_e,(sqrt((x+1)/2))+sqrt((x-1)/2))))
    .=-(log(number_e,((sqrt((x+1)/2))+sqrt((x-1)/2)) to_power 2)) by A4,Lm1,
  POWER:63,TAYLOR_1:11
    .=-log(number_e,((sqrt((x+1)/2))+sqrt((x-1)/2))^2) by POWER:53
    .=-log(number_e,(sqrt((x+1)/2))^2+2*(sqrt((x+1)/2))* (sqrt((x-1)/2))+(
  sqrt((x-1)/2))^2)
    .=-log(number_e,((x+1)/2)+2*(sqrt((x+1)/2))* (sqrt((x-1)/2))+(sqrt((x-1)
  /2))^2) by A1,SQUARE_1:def 4
    .=-log(number_e,(x+1)/2+2*sqrt((x+1)/2)* sqrt((x-1)/2)+(x-1)/2) by A2,
  SQUARE_1:def 4
    .=-log(number_e,(x+2*(sqrt((x+1)/2)*sqrt((x-1)/2))))
    .=-log(number_e,(x+2*(sqrt(((x+1)/2)*((x-1)/2))))) by A1,A2,SQUARE_1:97
    .=-log(number_e,(x+sqrt(2^2)*(sqrt((x^2-1)/4)))) by SQUARE_1:89
    .=-log(number_e,(x+sqrt(4*((x^2-1)/4)))) by A3,SQUARE_1:97
    .=-log(number_e,(x+sqrt(x^2-1)));
  hence thesis;
end;

theorem
  x>=1 implies cosh1"(x)=2*sinh"(sqrt((x-1)/2))
proof
  assume
A1: x>=1;
  then
A2: sqrt((x+1)/2)+sqrt((x-1)/2)>0 by Th10;
A3: (x^2-1)/4>=0 by A1,Th9;
A4: (x-1)/2>=0 by A1,Th7;
  then
  2*sinh"(sqrt((x-1)/2)) =2*log(number_e,((sqrt((x-1)/2))+sqrt(((x-1)/2)+1
  ))) by SQUARE_1:def 4
    .=log(number_e,(sqrt((x-1)/2)+sqrt((x+1)/2)) to_power 2) by A2,Lm1,POWER
  :63,TAYLOR_1:11
    .=log(number_e,(sqrt((x-1)/2)+sqrt((x+1)/2))^2) by POWER:53
    .=log(number_e,(sqrt((x-1)/2))^2+2*(sqrt((x-1)/2)) *(sqrt((x+1)/2))+(
  sqrt((x+1)/2))^2)
    .=log(number_e,((x-1)/2)+2*(sqrt((x-1)/2)) *(sqrt((x+1)/2))+(sqrt((x+1)/
  2))^2) by A4,SQUARE_1:def 4
    .=log(number_e,(x-1)/2+2*(sqrt((x-1)/2)) *(sqrt((x+1)/2))+(x+1)/2) by A1
  ,SQUARE_1:def 4
    .=log(number_e,x+2*((sqrt((x-1)/2))*(sqrt((x+1)/2))))
    .=log(number_e,x+2*(sqrt(((x-1)/2)*((x+1)/2)))) by A1,A4,SQUARE_1:97
    .=log(number_e,x+sqrt(2^2)*(sqrt((x^2-1)/4))) by SQUARE_1:89
    .=log(number_e,x+sqrt(4*((x^2-1)/4))) by A3,SQUARE_1:97
    .=log(number_e,x+sqrt(x^2-1));
  hence thesis;
end;

theorem
  x^2<1 implies tanh"(x)=sinh"(x/sqrt(1-x^2))
proof
  assume
A1: x^2<1;
  then
A2: x+1>0 by Th11;
A3: sqrt(x+1)>0 by A1,Th11,SQUARE_1:93;
A4: (x+1)/(1-x)>0 by A1,Lm4;
A5: 1-x^2>0 by A1,XREAL_1:52;
A6: 1-x>0 by A1,Th11;
  then
A7: sqrt((x+1)/(1-x))=((x+1)/(1-x)) to_power (1/2) by A2,ASYMPT_1:89;
  sinh"(x/sqrt(1-x^2)) =log(number_e,((x/sqrt(1-x^2))+sqrt((x^2/(sqrt(1-x
  ^2))^2)+1))) by XCMPLX_1:77
    .=log(number_e,((x/sqrt(1-x^2))+sqrt((x^2/(1-x^2))+1))) by A5,SQUARE_1:
  def 4
    .=log(number_e,((x/sqrt(1-x^2))+sqrt((x^2+(1-x^2)*1)/(1-x^2)))) by A5,
  XCMPLX_1:114
    .=log(number_e,((x/sqrt(1-x^2))+sqrt(1)/sqrt(1-x^2))) by A5,SQUARE_1:99
    .=log(number_e,((x+1)/sqrt((1-x)*(1+x)))) by SQUARE_1:83
    .=log(number_e,(sqrt((x+1)^2)/sqrt((1-x)*(1+x)))) by A2,SQUARE_1:89
    .=log(number_e,((sqrt(x+1)*sqrt(x+1))/sqrt((1-x)*(1+x)))) by A2,SQUARE_1
  :97
    .=log(number_e,((sqrt(x+1)*sqrt(x+1))/(sqrt(1-x)*sqrt(1+x)))) by A2,A6,
  SQUARE_1:97
    .=log(number_e,(sqrt(x+1)/sqrt(1-x))) by A3,XCMPLX_1:92
    .=log(number_e,(sqrt((x+1)/(1-x)))) by A2,A6,SQUARE_1:99
    .=(1/2)*log(number_e,((1+x)/(1-x))) by A4,A7,Lm1,POWER:63,TAYLOR_1:11;
  hence thesis;
end;

theorem
  0<x & x<1 implies tanh"(x)=cosh1"(1/(sqrt(1-x^2)))
proof
  assume that
A1: 0<x and
A2: x<1;
A3: 1-x^2>0 by A1,A2,Lm5;
A4: (1+x)/(1-x)>0 by A1,A2,Th15;
A5: 1-x>0 by A2,XREAL_1:52;
A6: sqrt(1+x)>0 by A1,SQUARE_1:93;
  (1+x)/(1-x)>0 by A1,A2,Th15;
  then
A7: sqrt((1+x)/(1-x))=((1+x)/(1-x)) to_power (1/2) by ASYMPT_1:89;
A8: x^2>=0 by XREAL_1:65;
  cosh1"(1/(sqrt(1-x^2))) =log(number_e,(1/(sqrt(1-x^2)))+sqrt((1/((sqrt(
  1-x^2))^2))-1^2)) by XCMPLX_1:77
    .=log(number_e,(1/(sqrt(1-x^2)))+sqrt(1/(1-x^2)-1)) by A3,SQUARE_1:def 4
    .=log(number_e,(1/(sqrt(1-x^2)))+sqrt((1-1*(1-x^2))/(1-x^2))) by A3,
  XCMPLX_1:127
    .=log(number_e,(1/(sqrt(1-x^2)))+sqrt(x^2)/sqrt(1-x^2)) by A3,A8,
  SQUARE_1:99
    .=log(number_e,1/(sqrt(1-x^2))+x/sqrt(1-x^2)) by A1,SQUARE_1:89
    .=log(number_e,(1+x)/sqrt((1-x)*(1+x)))
    .=log(number_e,(1+x)/(sqrt(1-x)*sqrt(1+x))) by A1,A5,SQUARE_1:97
    .=log(number_e,(sqrt((1+x)^2))/(sqrt(1-x)*sqrt(1+x))) by A1,SQUARE_1:89
    .=log(number_e,(sqrt(1+x)*sqrt(1+x))/(sqrt(1-x)*sqrt(1+x))) by A1,
  SQUARE_1:97
    .=log(number_e,(sqrt(1+x))/(sqrt(1-x))) by A6,XCMPLX_1:92
    .=log(number_e,sqrt((1+x)/(1-x))) by A1,A5,SQUARE_1:99
    .=(1/2)*log(number_e,((1+x)/(1-x))) by A7,A4,Lm1,POWER:63,TAYLOR_1:11;
  hence thesis;
end;

theorem
  x^2<1 implies tanh"(x)=(1/2)*sinh"((2*x)/(1-x^2))
proof
  assume
A1: x^2<1;
  then
A2: (1-x^2)^2>0 by Th12;
A3: x+1>0 by A1,Th11;
  (1/2)*sinh"((2*x)/(1-x^2)) =(1/2)*log(number_e,(((2*x)/(1-x^2))+sqrt(((2
  *x)^2/(1-x^2)^2)+1))) by XCMPLX_1:77
    .=(1/2)*log(number_e,(((2*x)/(1-x^2))+sqrt(((4*x^2)+((1-x^2)^2)*1)/ ((1-
  x^2)^2)))) by A2,XCMPLX_1:114
    .=(1/2)*log(number_e,(((2*x)/(1-x^2))+sqrt((((x^2)+1)^2)/ ((1-x^2)^2))))
    .=(1/2)*log(number_e,(((2*x)/(1-x^2))+sqrt((((x^2)+1)/(1-x^2))^2))) by
  XCMPLX_1:77
    .=(1/2)*log(number_e,((2*x)/(1-x^2)+((x^2)+1)/(1-x^2))) by A1,Th13,
  SQUARE_1:89
    .=(1/2)*log(number_e,(((2*x)+((x^2)+1))/(1-x^2)))
    .=(1/2)*log(number_e,((x+1)*(x+1)/((1-x)*(1+x))))
    .=(1/2)*log(number_e,((x+1)/(1-x))) by A3,XCMPLX_1:92;
  hence thesis;
end;

theorem
  x>0 & x<1 implies tanh"(x)=(1/2)*cosh1"((1+x^2)/(1-x^2))
proof
  assume that
A1: x>0 and
A2: x<1;
A3: 0<(1-x^2)^2 by A1,A2,Th19;
A4: (2*x)/(1-x^2)>0 by A1,A2,Th18;
  (1/2)*cosh1"((1+x^2)/(1-x^2)) =(1/2)*log(number_e,((1+x^2)/(1-x^2))+sqrt
  ((1+x^2)^2/(1-x^2)^2-1)) by XCMPLX_1:77
    .=(1/2)*log(number_e,((1+x^2)/(1-x^2))+sqrt((((1+x^2)^2)-1* ((1-x^2)^2))
  /((1-x^2)^2))) by A3,XCMPLX_1:127
    .=(1/2)*log(number_e,((1+x^2)/(1-x^2))+sqrt(((2*x)^2)/((1-x^2)^2)))
    .=(1/2)*log(number_e,((1+x^2)/(1-x^2))+sqrt(((2*x)/(1-x^2))^2)) by
  XCMPLX_1:77
    .=(1/2)*log(number_e,(1+x^2)/(1-x^2)+(2*x)/(1-x^2)) by A4,SQUARE_1:89
    .=(1/2)*log(number_e,((1+x^2)+(2*x))/(1-x^2))
    .=(1/2)*log(number_e,(x+1)*(x+1)/((1-x)*(1+x)))
    .=(1/2)*log(number_e,(x+1)/((1-x))) by A1,XCMPLX_1:92;
  hence thesis;
end;

theorem
  x^2<1 implies tanh"(x)=(1/2)*tanh"((2*x)/(1+x^2))
proof
  assume x^2<1;
  then
A1: (x+1)/(1-x)>0 by Lm4;
A2: 1+x^2>0 by Lm6;
  then
  (1/2)*tanh"((2*x)/(1+x^2)) =(1/2)*((1/2)*log(number_e,(((2*x)+(1+x^2)*1)
  /(1+x^2))/(1-(2*x)/(1+x^2)))) by XCMPLX_1:114
    .=(1/2)*((1/2)*log(number_e,((2*x+1+x^2)/(1+x^2))/((1*(1+x^2)-(2*x)) /(1
  +x^2)))) by A2,XCMPLX_1:128
    .=(1/2)*((1/2)*log(number_e,(x+1)^2/(1-x)^2)) by A2,XCMPLX_1:55
    .=(1/2)*((1/2)*log(number_e,((x+1)/(1-x))^2)) by XCMPLX_1:77
    .=(1/2)*((1/2)*log(number_e,((x+1)/(1-x)) to_power 2)) by POWER:53
    .=(1/2)*((1/2)*(2*log(number_e,((x+1)/(1-x))))) by A1,Lm1,POWER:63 ,
  TAYLOR_1:11
    .=(1/2)*log(number_e,(1+x)/(1-x));
  hence thesis;
end;

theorem
  x^2>1 implies coth"(x)=tanh"(1/x)
proof
  assume x^2>1;
  then
A1: x<>0;
  then tanh"(1/x) =(1/2)*log(number_e,((1+x*1)/x)/(1-(1/x))) by
  XCMPLX_1:114
    .=(1/2)*log(number_e,((1+x*1)/x)/((1*x-1)/x)) by A1,XCMPLX_1:128
    .=(1/2)*log(number_e,(1+x)/(x-1)) by A1,XCMPLX_1:55;
  hence thesis;
end;

theorem
  x>0 & x<=1 implies sech1"(x)=cosh1"(1/x)
proof
  assume that
A1: x>0 and
A2: x<=1;
A3: 1-x^2>=0 by A1,A2,Th22;
A4: x^2>0 by A1,SQUARE_1:74;
  cosh1"(1/x)=log(number_e,(1/x)+sqrt(1/x^2-1^2)) by XCMPLX_1:77
    .=log(number_e,(1/x)+sqrt((1-1*(x^2))/x^2)) by A4,XCMPLX_1:127
    .=log(number_e,(1/x)+sqrt(1-(x^2))/sqrt(x^2)) by A1,A3,SQUARE_1:99
    .=log(number_e,(1/x)+sqrt(1-(x^2))/x) by A1,SQUARE_1:89
    .=log(number_e,(1+sqrt(1-x^2))/x);
  hence thesis;
end;

theorem
  x>0 & x<=1 implies sech2"(x)=cosh2"(1/x)
proof
  assume that
A1: x>0 and
A2: x<=1;
A3: 1-x^2>=0 by A1,A2,Th22;
A4: x^2>0 by A1,SQUARE_1:74;
  cosh2"(1/x) =-log(number_e,(1/x)+sqrt(1/x^2-1^2)) by XCMPLX_1:77
    .=-log(number_e,(1/x)+sqrt((1-1*(x^2))/x^2)) by A4,XCMPLX_1:127
    .=-log(number_e,(1/x)+sqrt(1-(x^2))/sqrt(x^2)) by A1,A3,SQUARE_1:99
    .=-log(number_e,(1/x)+sqrt(1-(x^2))/x) by A1,SQUARE_1:89
    .=-log(number_e,(1+sqrt(1-x^2))/x);
  hence thesis;
end;

theorem
  x>0 implies csch"(x)=sinh"(1/x)
proof
  assume
A1: x>0;
  then
A2: x^2>0 by SQUARE_1:74;
  sinh"(1/x)=log(number_e,(1/x+sqrt(1/x^2+1^2))) by XCMPLX_1:77
    .=log(number_e,(1/x+sqrt((1+x^2*1)/(x^2)))) by A2,XCMPLX_1:114
    .=log(number_e,(1/x+sqrt(1+x^2)/sqrt(x^2))) by A1,SQUARE_1:99
    .=log(number_e,(1/x+sqrt(1+x^2)/x)) by A1,SQUARE_1:89
    .=log(number_e,(1+sqrt(1+x^2))/x);
  hence thesis by A1,Def8;
end;

theorem
  (x*y+sqrt(x^2+1)*sqrt(y^2+1))>=0 implies sinh"(x)+sinh"(y)=sinh"(x*
  sqrt(1+y^2)+y*sqrt(1+x^2))
proof
  assume
A1: (x*y+sqrt(x^2+1)*sqrt(y^2+1))>=0;
  sqrt(x^2+1)+x>0 & sqrt(y^2+1)+y>0 by Th5;
  then
A2: sinh"(x)+sinh"(y) =log(number_e,(x+sqrt(x^2+1))*(y+sqrt(y^2+1))) by
  Lm1,POWER:61,TAYLOR_1:11
    .=log(number_e,(x*sqrt(y^2+1)+sqrt(x^2+1)*y+(x*y+sqrt(x^2+1)*sqrt(y^2+1)
  )))
    .=log(number_e,(x*sqrt(y^2+1)+sqrt(x^2+1)*y+ sqrt((x*y+sqrt(x^2+1)*sqrt(
  y^2+1))^2))) by A1,SQUARE_1:89;
A3: y^2+1>=0 by Lm6;
  set p=sqrt(((x*sqrt(1+y^2))+(y*sqrt(1+x^2)))^2+1);
  set t=sqrt((x*y+sqrt(x^2+1)*sqrt(y^2+1))^2);
A4: x^2+1>=0 by Lm6;
A5: p=sqrt((x^2*(sqrt(1+y^2))^2+2*(x*sqrt(1+y^2))*(y*sqrt(1+x^2)) +(y*sqrt(1
  +x^2))^2)+1)
    .=sqrt((x^2*(1+y^2)+2*(x*sqrt(1+y^2))*(y*sqrt(1+x^2)) +(y*sqrt(1+x^2))^2
  )+1) by A3,SQUARE_1:def 4
    .=sqrt((x^2+(x*y)^2+2*(x*sqrt(1+y^2))*(y*sqrt(1+x^2)) +y^2*(sqrt(1+x^2))
  ^2)+1)
    .=sqrt((x^2+(x*y)^2+2*(x*sqrt(1+y^2))*(y*sqrt(1+x^2)) +y^2*(1+x^2))+1)
  by A4,SQUARE_1:def 4
    .=sqrt(x^2+2*(x*y)^2+y^2+1+2*x*y*(sqrt(1+y^2)*sqrt(1+x^2)))
    .=sqrt(x^2+2*(x*y)^2+y^2+1+2*x*y*sqrt((1+y^2)*(1+x^2))) by A4,A3,
  SQUARE_1:97;
  t=sqrt((x*y+sqrt((x^2+1)*(y^2+1)))^2) by A4,A3,SQUARE_1:97
    .=sqrt((x*y)^2+2*(x*y)*(sqrt((x^2+1)*(y^2+1)))+(sqrt((x^2+1)*(y^2+1)))^2
  )
    .=sqrt((x*y)^2+2*(x*y)*(sqrt((x^2+1)*(y^2+1)))+((x*y)^2+x^2+y^2+1)) by
  A4,A3,SQUARE_1:def 4
    .=sqrt(2*(x*y)^2+x^2+y^2+1+2*(x*y)*(sqrt((x^2+1)*(y^2+1))));
  hence thesis by A2,A5;
end;

theorem
  sinh"(x)-sinh"(y)=sinh"(x*sqrt(1+y^2)-y*sqrt(1+x^2))
proof
  set t=(sqrt(x^2+1))*(sqrt(y^2+1))-x*y;
  set q=sqrt((x*sqrt(1+y^2)-y*sqrt(1+x^2))^2+1);
A1: x^2+1>=0 by Lm6;
  y+0<sqrt(y^2+1) by Lm8;
  then
A2: sqrt(y^2+1)-y>0 by XREAL_1:22;
A3: y^2+1>=0 by Lm6;
  (sqrt(x^2+1)*sqrt(y^2+1)-x*y)>=0 by Lm9;
  then
A4: t=sqrt(((sqrt(x^2+1))*(sqrt(y^2+1))-x*y)^2) by SQUARE_1:89
    .=sqrt(((sqrt(x^2+1))^2*(sqrt(y^2+1))^2)-2*((sqrt(x^2+1))* (sqrt(y^2+1))
  )*(x*y)+(x*y)^2)
    .=sqrt(((x^2+1)*(sqrt(y^2+1))^2)-2*((sqrt(x^2+1))* (sqrt(y^2+1)))*(x*y)+
  (x*y)^2) by A1,SQUARE_1:def 4
    .=sqrt(((x^2+1)*(y^2+1))-2*((sqrt(x^2+1))* (sqrt(y^2+1)))*(x*y)+(x*y)^2)
  by A3,SQUARE_1:def 4
    .=sqrt(2*((x*y)^2)+x^2+y^2+1-2*(x*y)*((sqrt(x^2+1))* (sqrt(y^2+1))));
A5: q=sqrt((x^2*(sqrt(1+y^2))^2)-2*(x*sqrt(1+y^2))*(y*sqrt(1+x^2)) +((y*
  sqrt(1+x^2)))^2+1)
    .=sqrt((x^2*(1+y^2))-2*(x*sqrt(1+y^2))*(y*sqrt(1+x^2)) +((y*sqrt(1+x^2))
  )^2+1) by A3,SQUARE_1:def 4
    .=sqrt((x^2+x^2*y^2)-2*(x*sqrt(1+y^2))*(y*sqrt(1+x^2)) +(y^2*(sqrt(1+x^2
  ))^2)+1)
    .=sqrt((x^2+x^2*y^2)-2*(x*sqrt(1+y^2))*(y*sqrt(1+x^2)) +(y^2*(1+x^2))+1)
  by A1,SQUARE_1:def 4
    .=sqrt(x^2+y^2+2*(x*y)^2+1-2*x*y*sqrt(1+y^2)*sqrt(1+x^2));
  sqrt(x^2+1)+x>0 & sqrt(y^2+1)+y>0 by Th5;
  then sinh"(x)-sinh"(y) =log(number_e,(x+sqrt(x^2+1))/(y+sqrt(y^2+1))) by
  Lm1,POWER:62,TAYLOR_1:11
    .=log(number_e,((x+sqrt(x^2+1))*(sqrt(y^2+1)-y))/ ((y+sqrt(y^2+1))*(sqrt
  (y^2+1)-y))) by A2,XCMPLX_1:92
    .=log(number_e,((x+sqrt(x^2+1))*(sqrt(y^2+1)-y))/ ((sqrt(y^2+1))^2-y^2))
    .=log(number_e,((x+sqrt(x^2+1))*(sqrt(y^2+1)-y))/ ((y^2+1)-y^2)) by A3,
  SQUARE_1:def 4
    .=log(number_e,x*(sqrt(y^2+1))-y*(sqrt(x^2+1))+(sqrt(x^2+1)) *(sqrt(y^2+
  1))-x*y);
  hence thesis by A4,A5;
end;

theorem
  1<=x & 1<=y implies cosh1"(x)+cosh1"(y)=cosh1"(x*y+sqrt((x^2-1)*(y^2-1 )))
proof
  assume that
A1: 1<=x and
A2: 1<=y;
A3: y^2-1>=0 by A2,Lm3;
  set t=x*sqrt(y^2-1)+y*sqrt(x^2-1);
A4: x^2-1>=0 by A1,Lm3;
  t=sqrt((x*sqrt(y^2-1)+y*sqrt(x^2-1))^2) by A1,A2,Th24,SQUARE_1:89
    .=sqrt(x^2*(sqrt(y^2-1))^2+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1))+ (y*sqrt(x
  ^2-1))^2)
    .=sqrt(x^2*(y^2-1)+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1))+ (y*sqrt(x^2-1))^2)
  by A3,SQUARE_1:def 4
    .=sqrt(x^2*y^2-x^2+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1))+ y^2*(sqrt(x^2-1))
  ^2)
    .=sqrt(x^2*y^2-x^2+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1))+ y^2*(x^2-1)) by A4
  ,SQUARE_1:def 4
    .=sqrt(2*(x*y)^2-x^2-y^2+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1)));
  then
A5: log(number_e,x*sqrt(y^2-1)+y*sqrt(x^2-1)+sqrt((x^2-1)*(y^2-1))+x*y) =log
(number_e,(sqrt(2*(x*y)^2-x^2-y^2+2*x*y*(sqrt(y^2-1) *sqrt(x^2-1))))+sqrt((x^2-
  1)*(y^2-1))+x*y)
    .=log(number_e,(sqrt(2*(x*y)^2-x^2-y^2+2*x*y*sqrt((y^2-1)*(x^2-1)))) +
  sqrt((x^2-1)*(y^2-1))+x*y) by A4,A3,SQUARE_1:97;
A6: cosh1"(x*y+sqrt((x^2-1)*(y^2-1))) =log(number_e,(x*y+sqrt((x^2-1)*(y^2-
1)))+sqrt((x*y)^2+ 2*(x*y)*(sqrt((x^2-1)*(y^2-1)))+((sqrt((x^2-1)*(y^2-1)))^2)-
  1))
    .=log(number_e,(x*y+sqrt((x^2-1)*(y^2-1)))+sqrt((x*y)^2+ 2*(x*y)*(sqrt((
  x^2-1)*(y^2-1)))+(x^2-1)*(y^2-1)-1)) by A4,A3,SQUARE_1:def 4
    .=log(number_e,((sqrt(2*(x*y)^2-x^2-y^2+2*x*y*sqrt((y^2-1)*(x^2-1))))) +
  (sqrt((x^2-1)*(y^2-1))+x*y));
  0<x+sqrt(x^2-1) & 0<y+sqrt(y^2-1) by A1,A2,Th23;
  then cosh1"(x)+cosh1"(y) =log(number_e,(x+sqrt(x^2-1))*(y+sqrt(y^2-1))) by
  Lm1,POWER:61,TAYLOR_1:11
    .=log(number_e,x*sqrt(y^2-1)+y*sqrt(x^2-1)+(sqrt(x^2-1))*(sqrt(y^2-1))+x
  *y)
    .=log(number_e,x*sqrt(y^2-1)+y*sqrt(x^2-1)+sqrt((x^2-1)*(y^2-1))+x*y) by
  A4,A3,SQUARE_1:97;
  hence thesis by A5,A6;
end;

theorem
  1<=x & 1<=y implies cosh2"(x)+cosh2"(y)=cosh2"(x*y+sqrt((x^2-1)*(y^2-1 )))
proof
  assume that
A1: 1<=x and
A2: 1<=y;
A3: y^2-1>=0 by A2,Lm3;
A4: 0<x+sqrt(x^2-1) & 0<y+sqrt(y^2-1) by A1,A2,Th23;
  set t=x*sqrt(y^2-1)+y*sqrt(x^2-1);
A5: x^2-1>=0 by A1,Lm3;
  t=sqrt((x*sqrt(y^2-1)+y*sqrt(x^2-1))^2) by A1,A2,Th24,SQUARE_1:89
    .=sqrt(x^2*(sqrt(y^2-1))^2+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1))+ (y*sqrt(x
  ^2-1))^2)
    .=sqrt(x^2*(y^2-1)+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1))+ (y*sqrt(x^2-1))^2)
  by A3,SQUARE_1:def 4
    .=sqrt(x^2*y^2-x^2+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1))+ y^2*(sqrt(x^2-1))
  ^2)
    .=sqrt(x^2*y^2-x^2+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1))+ y^2*(x^2-1)) by A5
  ,SQUARE_1:def 4
    .=sqrt(2*(x*y)^2-x^2-y^2+2*(x*sqrt(y^2-1))*(y*sqrt(x^2-1)));
  then
A6: -log(number_e,x*sqrt(y^2-1)+y*sqrt(x^2-1)+sqrt((x^2-1)*(y^2-1))+x*y) =-
log(number_e,(sqrt(2*(x*y)^2-x^2-y^2+2*x*y*(sqrt(y^2-1) *sqrt(x^2-1))))+sqrt((x
  ^2-1)*(y^2-1))+x*y)
    .=-log(number_e,(sqrt(2*(x*y)^2-x^2-y^2+2*x*y*sqrt((y^2-1)*(x^2-1)))) +
  sqrt((x^2-1)*(y^2-1))+x*y) by A5,A3,SQUARE_1:97;
A7: cosh2"(x*y+sqrt((x^2-1)*(y^2-1))) =-log(number_e,(x*y+sqrt((x^2-1)*(y^2
-1)))+sqrt((x*y)^2+ 2*(x*y)*(sqrt((x^2-1)*(y^2-1)))+((sqrt((x^2-1)*(y^2-1)))^2)
  -1))
    .=-log(number_e,(x*y+sqrt((x^2-1)*(y^2-1)))+sqrt((x*y)^2+ 2*(x*y)*(sqrt(
  (x^2-1)*(y^2-1)))+(x^2-1)*(y^2-1)-1)) by A5,A3,SQUARE_1:def 4
    .=-log(number_e,((sqrt(2*(x*y)^2-x^2-y^2+2*x*y*sqrt((y^2-1)*(x^2-1)))))
  +(sqrt((x^2-1)*(y^2-1))+x*y));
  cosh2"(x)+cosh2"(y) =(-1)*(log(number_e,x+sqrt(x^2-1))+log(number_e,y+
  sqrt(y^2-1)))
    .=(-1)*log(number_e,(x+sqrt(x^2-1))*(y+sqrt(y^2-1))) by A4,Lm1,POWER:61
  ,TAYLOR_1:11
    .=-log(number_e,x*sqrt(y^2-1)+y*sqrt(x^2-1)+(sqrt(x^2-1))*(sqrt(y^2-1))+
  x*y)
    .=-log(number_e,x*sqrt(y^2-1)+y*sqrt(x^2-1)+sqrt((x^2-1)*(y^2-1))+x*y)
  by A5,A3,SQUARE_1:97;
  hence thesis by A6,A7;
end;

theorem
  1<=x & 1<=y & abs(y)<=abs(x) implies cosh1"(x)-cosh1"(y)=cosh1"(x*y-
  sqrt((x^2-1)*(y^2-1)))
proof
  assume that
A1: 1<=x and
A2: 1<=y and
A3: abs(y)<=abs(x);
A4: 0<=x^2-1 by A1,Lm3;
  set t=y*sqrt(x^2-1)-x*sqrt(y^2-1);
A5: (y-sqrt(y^2-1))>0 by A2,Th25;
A6: 0<=y^2-1 by A2,Lm3;
  0<x+sqrt(x^2-1) & 0<y+sqrt(y^2-1) by A1,A2,Th23;
  then
A7: cosh1"(x)-cosh1"(y) =log(number_e,((x+sqrt(x^2-1))/(y+sqrt(y^2-1)))) by
  Lm1,POWER:62 ,TAYLOR_1:11
    .=log(number_e,(((x+sqrt(x^2-1))*(y-sqrt(y^2-1)))/ ((y+sqrt(y^2-1))*(y-
  sqrt(y^2-1))))) by A5,XCMPLX_1:92
    .=log(number_e,(((x+sqrt(x^2-1))*(y-sqrt(y^2-1)))/ (y^2-(sqrt(y^2-1))^2)
  ))
    .=log(number_e,(((x+sqrt(x^2-1))*(y-sqrt(y^2-1)))/ (y^2-(y^2-1)))) by A6
  ,SQUARE_1:def 4
    .=log(number_e,(x*y-x*sqrt(y^2-1)+y*sqrt(x^2-1)-sqrt(x^2-1)* (sqrt(y^2-1
  ))))
    .=log(number_e,(x*y-x*sqrt(y^2-1)+y*sqrt(x^2-1)-sqrt((x^2-1)*(y^2-1))))
  by A4,A6,SQUARE_1:97
    .=log(number_e,(x*y-sqrt((x^2-1)*(y^2-1))+y*sqrt(x^2-1)-x*sqrt(y^2-1)));
A8: cosh1"(x*y-sqrt((x^2-1)*(y^2-1))) =log(number_e,(x*y-sqrt((x^2-1)*(y^2-
1)))+sqrt((x*y)^2-2*(x*y)* (sqrt((x^2-1)*(y^2-1)))+(sqrt((x^2-1)*(y^2-1)))^2-1)
  )
    .=log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+sqrt((x*y)^2-2*(x*y)* (sqrt((
  x^2-1)*(y^2-1)))+((x^2-1)*(y^2-1))-1)) by A4,A6,SQUARE_1:def 4
    .=log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+sqrt(2*(x*y)^2-x^2-y^2 -2*x*y
  *(sqrt((x^2-1)*(y^2-1)))));
  t=sqrt((y*sqrt(x^2-1)-x*sqrt(y^2-1))^2) by A1,A2,A3,Th26,SQUARE_1:89
    .=sqrt(y^2*(sqrt(x^2-1))^2-2*(y*sqrt(x^2-1))* (x*sqrt(y^2-1))+(x*sqrt(y
  ^2-1))^2)
    .=sqrt(y^2*(x^2-1)-2*(y*sqrt(x^2-1))* (x*sqrt(y^2-1))+(x*sqrt(y^2-1))^2)
  by A4,SQUARE_1:def 4
    .=sqrt((x*y)^2-y^2-2*(y*sqrt(x^2-1))* (x*sqrt(y^2-1))+x^2*(sqrt(y^2-1))
  ^2)
    .=sqrt((x*y)^2-y^2-2*(y*sqrt(x^2-1))* (x*sqrt(y^2-1))+x^2*(y^2-1)) by A6
  ,SQUARE_1:def 4
    .=sqrt(2*(x*y)^2-x^2-y^2-2*x*y*(sqrt(x^2-1)*sqrt(y^2-1)))
    .=sqrt(2*(x*y)^2-x^2-y^2-2*x*y*(sqrt((x^2-1)*(y^2-1)))) by A4,A6,
  SQUARE_1:97;
  hence thesis by A7,A8;
end;

theorem
  1<=x & 1<=y & abs(y)<=abs(x) implies cosh2"(x)-cosh2"(y)=cosh2"(x*y-
  sqrt((x^2-1)*(y^2-1)))
proof
  assume that
A1: 1<=x and
