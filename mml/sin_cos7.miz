A8: cosh2"(x*y-sqrt((x^2-1)*(y^2-1))) =-log(number_e,(x*y-sqrt((x^2-1)*(y^2
-1)))+sqrt((x*y)^2-2*(x*y)* (sqrt((x^2-1)*(y^2-1)))+(sqrt((x^2-1)*(y^2-1)))^2-1
  ))
    .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+sqrt((x*y)^2-2*(x*y)* (sqrt(
  (x^2-1)*(y^2-1)))+((x^2-1)*(y^2-1))-1)) by A7,A5,SQUARE_1:def 4
    .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+sqrt(2*(x*y)^2-x^2-y^2 -2*x*
  y*(sqrt((x^2-1)*(y^2-1)))));
A9: cosh2"(x)-cosh2"(y) =-(log(number_e,x+sqrt(x^2-1))-log(number_e,y+sqrt(y
  ^2-1)))
    .=-log(number_e,((x+sqrt(x^2-1))/(y+sqrt(y^2-1)))) by A4,Lm1,POWER:62 ,
  TAYLOR_1:11
    .=-log(number_e,(((x+sqrt(x^2-1))*(y-sqrt(y^2-1)))/ ((y+sqrt(y^2-1))*(y-
  sqrt(y^2-1))))) by A6,XCMPLX_1:92
    .=-log(number_e,(((x+sqrt(x^2-1))*(y-sqrt(y^2-1)))/ (y^2-(sqrt(y^2-1))^2
  )))
    .=-log(number_e,(((x+sqrt(x^2-1))*(y-sqrt(y^2-1)))/ (y^2-(y^2-1)))) by
  A5,SQUARE_1:def 4
    .=-log(number_e,(x*y-x*sqrt(y^2-1)+y*sqrt(x^2-1)-sqrt(x^2-1)* (sqrt(y^2-
  1))))
    .=-log(number_e,(x*y-x*sqrt(y^2-1)+y*sqrt(x^2-1)-sqrt((x^2-1)*(y^2-1))))
  by A7,A5,SQUARE_1:97
    .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1))+y*sqrt(x^2-1)-x*sqrt(y^2-1)));
  t=sqrt((y*sqrt(x^2-1)-x*sqrt(y^2-1))^2) by A1,A2,A3,Th26,SQUARE_1:89
    .=sqrt(y^2*(sqrt(x^2-1))^2-2*(y*sqrt(x^2-1))* (x*sqrt(y^2-1))+(x*sqrt(y
  ^2-1))^2)
    .=sqrt(y^2*(x^2-1)-2*(y*sqrt(x^2-1))* (x*sqrt(y^2-1))+(x*sqrt(y^2-1))^2)
  by A7,SQUARE_1:def 4
    .=sqrt((x*y)^2-y^2-2*(y*sqrt(x^2-1))* (x*sqrt(y^2-1))+x^2*(sqrt(y^2-1))
  ^2)
    .=sqrt((x*y)^2-y^2-2*(y*sqrt(x^2-1))* (x*sqrt(y^2-1))+x^2*(y^2-1)) by A5
  ,SQUARE_1:def 4
    .=sqrt(2*(x*y)^2-x^2-y^2-2*x*y*(sqrt(x^2-1)*sqrt(y^2-1)))
    .=sqrt(2*(x*y)^2-x^2-y^2-2*x*y*(sqrt((x^2-1)*(y^2-1)))) by A7,A5,
  SQUARE_1:97;
  hence thesis by A9,A8;
end;

theorem
  x^2<1 & y^2<1 implies tanh"(x)+tanh"(y)=tanh"((x+y)/(1+x*y))
proof
  assume
A1: x^2<1 & y^2<1;
  then
A2: 0<(1+x)/(1-x) & 0<(1+y)/(1-y) by Lm4;
A3: tanh"(x)+tanh"(y) =(1/2)*(log(number_e,(1+x)/(1-x))+log(number_e,(1+y)/(
  1-y)))
    .=(1/2)*(log(number_e,((1+x)/(1-x))*((1+y)/(1-y)))) by A2,Lm1,POWER:61 ,
  TAYLOR_1:11
    .=(1/2)*(log(number_e,(((1+x)*(1+y))/((1-x)*(1-y))))) by XCMPLX_1:77
    .=(1/2)*(log(number_e,((1+x+y+x*y)/(1+x*y-x-y))));
A4: x*y+1<>0 by A1,Th27;
  then
  tanh"((x+y)/(1+x*y)) =(1/2)*log(number_e,(((x+y)+(1+x*y)*1)/(1+x*y))/(1-
  ((x+y)/(1+x*y)))) by XCMPLX_1:114
    .=(1/2)*log(number_e,((x+y+1+x*y)/(1+x*y))/(((1*(1+x*y)-(x+y))/(1+x*y)))
  ) by A4,XCMPLX_1:128
    .=(1/2)*log(number_e,(1+x+y+x*y)/(1+x*y-x-y)) by A4,XCMPLX_1:55;
  hence thesis by A3;
end;

theorem
  x^2<1 & y^2<1 implies tanh"(x)-tanh"(y)=tanh"((x-y)/(1-x*y))
proof
  assume
A1: x^2<1 & y^2<1;
  then
A2: 0<(1+x)/(1-x) & 0<(1+y)/(1-y) by Lm4;
A3: tanh"(x)-tanh"(y) =(1/2)*(log(number_e,(1+x)/(1-x))-log(number_e,(1+y)/(
  1-y)))
    .=(1/2)*(log(number_e,((1+x)/(1-x))/((1+y)/(1-y)))) by A2,Lm1,POWER:62 ,
  TAYLOR_1:11
    .=(1/2)*(log(number_e,((1+x)*(1-y))/((1-x)*(1+y)))) by XCMPLX_1:85
    .=(1/2)*(log(number_e,(1-y+x-x*y)/(1+y-x-x*y)));
A4: 1-x*y<>0 by A1,Th28;
  then
  tanh"((x-y)/(1-x*y)) =(1/2)*log(number_e,(((x-y)+(1-x*y)*1)/(1-x*y))/(1-
  ((x-y)/(1-x*y)))) by XCMPLX_1:114
    .=(1/2)*log(number_e,((x-y+1-x*y)/(1-x*y))/((1*(1-x*y)-(x-y))/(1-x*y)))
  by A4,XCMPLX_1:128
    .=(1/2)*(log(number_e,(1-y+x-x*y)/(1+y-x-x*y))) by A4,XCMPLX_1:55;
  hence thesis by A3;
end;

theorem
  0<x implies log(number_e,x)=2*tanh"((x-1)/(x+1))
proof
  assume
A1: 0<x;
  then
  2*tanh"((x-1)/(x+1)) =log(number_e,(((x-1)+(x+1)*1)/(x+1))/(1-(x-1)/(x+1
  ))) by XCMPLX_1:114
    .=log(number_e,((2*x)/(x+1))/((1*(x+1)-(x-1))/(x+1))) by A1,XCMPLX_1:128
    .=log(number_e,(2*x)/2) by A1,XCMPLX_1:55
    .=log(number_e,x);
  hence thesis;
end;

theorem
  0<x implies log(number_e,x)=tanh"((x^2-1)/(x^2+1))
proof
  assume
A1: 0<x;
A2: x^2+1>0 by Lm6;
  then
  tanh"((x^2-1)/(x^2+1)) =(1/2)*log(number_e,(((x^2-1)+(x^2+1)*1)/(x^2+1))
  /(1-(x^2-1)/(x^2+1))) by XCMPLX_1:114
    .=(1/2)*log(number_e,((2*x^2)/(x^2+1))/((1*(x^2+1)-(x^2-1))/(x^2+1))) by
  A2,XCMPLX_1:128
    .=(1/2)*log(number_e,2*x^2/2) by A2,XCMPLX_1:55
    .=(1/2)*log(number_e,x to_power 2) by POWER:53
    .=(1/2)*(2*log(number_e,x)) by A1,Lm1,POWER:63,TAYLOR_1:11
    .=log(number_e,x);
  hence thesis;
end;

theorem
  1<x implies log(number_e,x)=cosh1"((x^2+1)/(2*x))
proof
  assume
A1: 1<x;
  then x<x^2 by SQUARE_1:76;
  then 1<x^2 by A1,XXREAL_0:2;
  then
A2: 1-1<x^2-1 by XREAL_1:16;
  1*2<2*x by A1,XREAL_1:70;
  then 1<2*x by XXREAL_0:2;
  then
A3: 1^2<(2*x)^2 by SQUARE_1:78;
  cosh1"((x^2+1)/(2*x)) =log(number_e,((x^2+1)/(2*x))+sqrt(((x^2+1))^2/(2*
  x)^2-1)) by XCMPLX_1:77
    .=log(number_e,((x^2+1)/(2*x))+sqrt((((x^2)^2+2*x^2+1)-1*(2*x)^2)/(2*x)
  ^2)) by A3,XCMPLX_1:127
    .=log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)-1)^2)/sqrt((2*x)^2)) by A1,A2
  ,SQUARE_1:99
    .=log(number_e,((x^2+1)/(2*x))+(x^2-1)/sqrt((2*x)^2)) by A2,SQUARE_1:89
    .=log(number_e,((x^2+1)/(2*x))+(x^2-1)/(2*x)) by A1,SQUARE_1:89
    .=log(number_e,((x^2+1)+(x^2-1))/(2*x))
    .=log(number_e,(2*x^2)/(2*x))
    .=log(number_e,(x*x)/x) by XCMPLX_1:92
    .=log(number_e,x/(x/x)) by XCMPLX_1:78
    .=log(number_e,x/1) by A1,XCMPLX_1:60
    .=log(number_e,x);
  hence thesis;
end;

theorem
  0<x & x<1 & 1<=(x^2+1)/(2*x) implies log(number_e,x)=cosh2"((x^2+1)/(2 *x))
proof
  assume that
A1: 0<x and
A2: x<1 and
A3: 1<=(x^2+1)/(2*x);
A4: (1/x)=x to_power (-1) by A1,Th1;
  x^2<x by A1,A2,SQUARE_1:75;
  then x^2<1 by A2,XXREAL_0:2;
  then
A5: x^2-x^2<1-x^2 by XREAL_1:16;
  0*2<x*2 by A1,A3;
  then
A6: 0<(2*x)^2 by SQUARE_1:74;
  cosh2"((x^2+1)/(2*x)) =-log(number_e,((x^2+1)/(2*x))+sqrt(((x^2+1))^2/(
  2*x)^2-1)) by XCMPLX_1:77
    .=-log(number_e,((x^2+1)/(2*x))+sqrt((((x^2)^2+2*x^2+1)-1*(2*x)^2)/(2*x)
  ^2)) by A6,XCMPLX_1:127
    .=-log(number_e,((x^2+1)/(2*x))+sqrt((1-(x^2))^2)/sqrt(2*x)^2) by A1,A5,
  SQUARE_1:99
    .=-log(number_e,((x^2+1)/(2*x))+(1-(x^2))/sqrt(2*x)^2) by A5,SQUARE_1:89
    .=-log(number_e,((x^2+1)/(2*x))+(1-x^2)/(2*x)) by A1,SQUARE_1:89
    .=-log(number_e,((x^2+1)+(1-x^2))/(2*x))
    .=-log(number_e,(2*1)/(2*x))
    .=-log(number_e,1/x) by XCMPLX_1:92
    .=-((-1)*log(number_e,x)) by A1,A4,Lm1,POWER:63,TAYLOR_1:11
    .=log(number_e,x);
  hence thesis;
end;

theorem
  0<x implies log(number_e,x)=sinh"((x^2-1)/(2*x))
proof
A1: x^2>=0 by XREAL_1:65;
  assume
A2: 0<x;
  then 0*2<x*2 by XREAL_1:70;
  then
A3: 0<(2*x)^2 by SQUARE_1:74;
  sinh"((x^2-1)/(2*x)) =log(number_e,(((x^2-1)/(2*x))+sqrt((x^2-1)^2/(2*x)
  ^2+1))) by XCMPLX_1:77
    .=log(number_e,(((x^2-1)/(2*x))+sqrt((((x^2)^2-2*x^2+1)+ (2*x)^2*1)/(2*x
  )^2))) by A3,XCMPLX_1:114
    .=log(number_e,(((x^2-1)/(2*x))+sqrt((x^2+1)^2)/sqrt((2*x)^2))) by A2,
  SQUARE_1:99
    .=log(number_e,(((x^2-1)/(2*x))+(x^2+1)/sqrt((2*x)^2))) by A1,SQUARE_1:
  89
    .=log(number_e,((x^2-1)/(2*x)+(x^2+1)/(2*x))) by A2,SQUARE_1:89
    .=log(number_e,(((x^2-1)+(x^2+1))/(2*x)))
    .=log(number_e,((2*x^2)/(2*x)))
    .=log(number_e,(x*x)/x) by XCMPLX_1:92
    .=log(number_e,x/(x/x)) by XCMPLX_1:78
    .=log(number_e,x/1) by A2,XCMPLX_1:60
    .=log(number_e,x);
  hence thesis;
end;

theorem
  y=(1/2)*(exp_R(x)-exp_R(-x)) implies x=log(number_e,(y+sqrt(y^2+1)))
proof
A1: exp_R(x)>0 by SIN_COS:60;
  set t=exp_R(x);
A2: delta(1,-2*y,-1)=((-2)*y)^2-4*1*(-1) by QUIN_1:def 1
    .=4*(y^2)+4;
A3: 0<=y^2 by XREAL_1:65;
  assume y=(1/2)*(exp_R(x)-exp_R(-x));
  then 2*y*exp_R(x)=(exp_R(x)-1/exp_R(x))*exp_R(x) by TAYLOR_1:4;
  then 2*y*t=t^2-(t*1)/t;
  then 2*y*t-2*y*t=t^2-1-2*y*t by A1,XCMPLX_1:60;
  then 1*t^2+(-2*y)*t+(-1)=0;
  then
  t = (-(-2*y)+sqrt delta(1,-2*y,-1))/(2*1) or t = (-(-2*y)-sqrt delta(1,-
  2*y,-1))/(2*1) by A2,A3,QUIN_1:15;
  then t=(2*y+sqrt(4)*sqrt(y^2+1))/2 or t=(2*y-sqrt(4*(y^2+1)))/2 by A2,A3,
  SQUARE_1:97;
  then t=(2*y+2*sqrt(y^2+1))/2 or t=(2*y-2*sqrt(y^2+1))/2 by A3,SQUARE_1:
  85,97;
  then
A4: exp_R(x)=y+sqrt(y^2+1) or exp_R(x)=y-sqrt(y^2+1);
  y<sqrt(y^2+1)+0 by Lm8;
  hence thesis by A1,A4,TAYLOR_1:12,XREAL_1:21;
end;

theorem
  y=(1/2)*(exp_R(x)+exp_R(-x)) & 1<=y implies x=log(number_e,(y+sqrt(y^2
  -1))) or x=-log(number_e,(y+sqrt(y^2-1)))
proof
  assume that
A1: y=(1/2)*(exp_R(x)+exp_R(-x)) and
A2: 1<=y;
A3: y+sqrt(y^2-1)>0 by A2,Lm10;
  set t=exp_R(x);
  2*y*exp_R(x)=(exp_R(x)+1/exp_R(x))*exp_R(x) by A1,TAYLOR_1:4;
  then 0<exp_R(x) & 2*y*t=t^2+(t*1)/t by SIN_COS:60;
  then 2*y*t-2*y*t=t^2+1-2*y*t by XCMPLX_1:60;
  then
A4: 0=1*t^2+(-2*y)*t+1;
A5: delta(1,-2*y,1)=((-2)*y)^2-4*1*1 by QUIN_1:def 1
    .=4*(y^2)-4;
A6: 0<=(y^2)-1 by A2,Lm3;
  then 0*4<=((y^2)-1)*4;
  then
  t = (-(-2*y)+sqrt delta(1,-2*y,1))/(2*1) or t = (-(-2*y)-sqrt delta(1,-
  2*y,1))/(2*1) by A4,A5,QUIN_1:15;
  then t=(2*y+sqrt(4)*sqrt(y^2-1))/2 or t=(2*y-sqrt(4*(y^2-1)))/2 by A6,A5,
  SQUARE_1:97;
  then t=(2*y+2*sqrt(y^2-1))/2 or t=(2*y-2*sqrt(y^2-1))/2 by A6,SQUARE_1:
  85,97;
  then log(number_e,y+sqrt(y^2-1))=x or log(number_e,y-sqrt(y^2-1))=x by
  TAYLOR_1:12;
  then
  log(number_e,y+sqrt(y^2-1))=x or log(number_e,(y-sqrt(y^2-1))*(y+sqrt(y
  ^2-1))/ (y+sqrt(y^2-1)))=x by A3,XCMPLX_1:90;
  then log(number_e,y+sqrt(y^2-1)) = x or log(number_e,(y^2-(sqrt(y^2-1))^2)/
  (y+sqrt(y^2-1)))=x;
  then
A7: log(number_e,y+sqrt(y^2-1))=x or log(number_e,(y^2-(y^2-1))/(y+sqrt(y^2
  -1)))=x by A6,SQUARE_1:def 4;
  1/(y+sqrt(y^2-1))=(y+sqrt(y^2-1)) to_power (-1) by A3,Th1;
  then log(number_e,y+sqrt(y^2-1))=x or (-1)*log(number_e,(y+sqrt(y^2-1)))=x
  by A3,A7,Lm1,POWER:63,TAYLOR_1:11;
  hence thesis;
end;

theorem
  y=(exp_R(x)-exp_R(-x))/(exp_R(x)+exp_R(-x)) implies x=(1/2)*log(
  number_e,(1+y)/(1-y))
proof
A1: 0<exp_R(x) by SIN_COS:60;
  set t=exp_R(x);
  assume y=(exp_R(x)-exp_R(-x))/(exp_R(x)+exp_R(-x));
  then y=(exp_R(x)-1/exp_R(x))/(exp_R(x)+exp_R(-x)) by TAYLOR_1:4;
  then y=(exp_R(x)-1/exp_R(x))/(exp_R(x)+1/exp_R(x)) by TAYLOR_1:4;
  then y=((exp_R(x)*exp_R(x)-1)/exp_R(x))/(exp_R(x)+1/exp_R(x)) by A1,
  XCMPLX_1:128;
  then y=(((exp_R x)^2-1)/exp_R(x))/((1+exp_R(x)*exp_R(x))/exp_R(x)) by A1,
  XCMPLX_1:114;
  then
A2: y=((exp_R x)^2-1)/(1+(exp_R(x))^2) by A1,XCMPLX_1:55;
  then 1*y+t^2*y=(t^2-1)/(1+t^2)*(1+t^2);
  then y+t^2*y-(t^2-1)=t^2-1-(t^2-1) by A1,XCMPLX_1:88;
  then t^2*(y-1)/(y-1)=(-(y+1))/(y-1);
  then
A3: t^2*(y-1)/(y-1)=(y+1)/(-(y-1)) by XCMPLX_1:193;
  y-1<>0 by A2,Th31;
  then sqrt(t^2)=sqrt((y+1)/(1-y)) by A3,XCMPLX_1:90;
  then
A4: exp_R(x)=sqrt((y+1)/(1-y)) by A1,SQUARE_1:89;
  -1<y by A2,Lm11,SIN_COS:60;
  then
A5: 0<(y+1)/(1-y) by A2,Th31,Th32;
  then sqrt((y+1)/(1-y))=((y+1)/(1-y)) to_power (1/2) by ASYMPT_1:89;
  then log(number_e,((y+1)/(1-y)) to_power (1/2))=x by A4,TAYLOR_1:12;
  hence thesis by A5,Lm1,POWER:63,TAYLOR_1:11;
end;

theorem
  y=(exp_R(x)+exp_R(-x))/(exp_R(x)-exp_R(-x)) & x<>0 implies x=(1/2)*log
  (number_e,(y+1)/(y-1))
proof
A1: 0<exp_R(x) by SIN_COS:60;
  set t=exp_R x;
  assume that
A2: y=(exp_R(x)+exp_R(-x))/(exp_R(x)-exp_R(-x)) and
A3: x<>0;
  y=(exp_R(x)+1/exp_R(x))/(exp_R(x)-exp_R(-x)) by A2,TAYLOR_1:4;
  then y=(exp_R(x)+1/exp_R(x))/(exp_R(x)-1/exp_R(x)) by TAYLOR_1:4;
  then y=(exp_R(x)+1/exp_R(x))/((exp_R(x)*exp_R(x)-1)/exp_R(x)) by A1,
  XCMPLX_1:128;
  then y=((1+exp_R(x)*exp_R(x))/exp_R(x))/(((exp_R(x))^2-1)/exp_R(x)) by A1,
  XCMPLX_1:114;
  then
A4: y=(1+(exp_R x)^2)/((exp_R x)^2-1) by A1,XCMPLX_1:55;
  then y*(t^2-1)=1+t^2 by A3,Th30,XCMPLX_1:88;
  then
A5: t^2*(y-1)/(y-1)=(y+1)/(y-1);
  exp_R(x)<>1 by A3,Th29;
  then
A6: 1<y or y< -1 by A4,Lm12,SIN_COS:60;
  then 1-1<y-1 or y-1< -1-1 by XREAL_1:16;
  then sqrt(t^2)=sqrt((y+1)/(y-1)) by A5,XCMPLX_1:90;
  then
A7: exp_R(x)=sqrt((y+1)/(y-1)) by A1,SQUARE_1:89;
A8: 0<(y+1)/(y-1) by A6,Lm7;
  then sqrt((y+1)/(y-1))=((y+1)/(y-1)) to_power (1/2) by ASYMPT_1:89;
  then log(number_e,((y+1)/(y-1)) to_power (1/2))=x by A7,TAYLOR_1:12;
  hence thesis by A8,Lm1,POWER:63,TAYLOR_1:11;
end;

theorem
  y=1/((exp_R(x)+exp_R(-x))/2) implies x=log(number_e,(1+sqrt(1-y^2))/y)
  or x=-log(number_e,(1+sqrt(1-y^2))/y)
proof
  set t=exp_R(x);
A1: delta(y,-2,y) =(-2)^2-4*y*y by QUIN_1:def 1
    .=4-4*y^2;
  assume y=1/((exp_R(x)+exp_R(-x))/2);
  then y=(1*2)/(2*((exp_R(x)+exp_R(-x))/2)) by XCMPLX_1:92;
  then 0<exp_R(x) & y=2/(exp_R(x)+1/exp_R(x)) by SIN_COS:60,TAYLOR_1:4;
  then y=2/((1+exp_R(x)*exp_R(x))/exp_R(x)) by XCMPLX_1:114;
  then y=2*(exp_R(x)/(1+(exp_R(x))^2)) by XCMPLX_1:80;
  then
A2: y=2*t/(1+t^2);
  then
A3: 0<y by Lm13,SIN_COS:60;
  1+t^2>0 by Lm6;
  then
A4: y*(1+t^2)=(2*t) by A2,XCMPLX_1:88;
A5: y<=1 by A2,Lm14,SIN_COS:60;
  then
A6: 0<=1-y^2 by A3,Lm16;
  Polynom(y,-2,y,t)=y*t^2+(-2)*t+y by POLYEQ_1:def 2;
  then
  t=(-(-2)+sqrt delta(y,-2,y))/(2*y) or t=(-(-2)-sqrt delta(y,-2,y))/(2*y
  ) by A3,A5,A4,A1,Lm17,QUIN_1:15;
  then t=(2+sqrt(4*(1-y^2)))/(2*y) or t=(2-sqrt(4*(1-y^2)))/(2*y) by A1;
  then t=(2+2*sqrt(1-y^2))/(2*y) or t=(2-2*sqrt(1-y^2))/(2*y) by A6,
  SQUARE_1:85,97;
  then t=(2*(1+sqrt(1-y^2)))/(2*y) or t=(2*(1-sqrt(1-y^2)))/(2*y);
  then
A7: t=(1+sqrt(1-y^2))/y or t=(1-sqrt(1-y^2))/y by XCMPLX_1:92;
  0<1+sqrt(1-y^2) by A3,A5,Lm18;
  then
  t=(1+sqrt(1-y^2))/y or t=((1-sqrt(1-y^2))*(1+sqrt(1-y^2)))/(y*(1+sqrt(1
  -y^2))) by A7,XCMPLX_1:92;
  then t=(1+sqrt(1-y^2))/y or t=(1-(sqrt(1-y^2))^2)/(y*(1+sqrt(1-y^2)));
  then t=(1+sqrt(1-y^2))/y or t=(1-(1-y^2))/(y*(1+sqrt(1-y^2))) by A6,
  SQUARE_1:def 4;
  then
A8: t=(1+sqrt(1-y^2))/y or t=y/(1+sqrt(1-y^2)) by A3,XCMPLX_1:92;
  log(number_e,exp_R(x))=x & 1/((1+sqrt(1-y^2))/y)=((1+sqrt(1-y^2))/y)
  to_power (-1) by A3,A5,Lm19,Th1,TAYLOR_1:12;
  then
A9: log(number_e,(1+sqrt(1-y^2))/y)=x or log(number_e,((1+sqrt(1-y^2))/y)
  to_power (-1))=x by A8,XCMPLX_1:57;
  0<(1+sqrt(1-y^2))/y by A3,A5,Lm19;
  then
  log(number_e,(1+sqrt(1-y^2))/y)=x or (-1)*log(number_e,((1+sqrt(1-y^2))
  /y))=x by A9,Lm1,POWER:63 ,TAYLOR_1:11;
  hence thesis;
end;

theorem
  y=1/((exp_R(x)-exp_R(-x))/2) & x<>0 implies x=log(number_e,(1+sqrt(1+y
  ^2))/y) or x=log(number_e,(1-sqrt(1+y^2))/y)
proof
A1: 0<exp_R(x) by SIN_COS:60;
  set t=exp_R x;
  assume that
A2: y=1/((exp_R(x)-exp_R(-x))/2) and
A3: x<>0;
A4: delta(y,-2,-y)=(-2)^2-4*y*(-y) by QUIN_1:def 1
    .=4+4*y^2;
  y=(1*2)/(2*((exp_R(x)-exp_R(-x))/2)) by A2,XCMPLX_1:92;
  then y=2/(exp_R(x)-1/exp_R(x)) by TAYLOR_1:4;
  then y=2/((exp_R(x)*exp_R(x)-1)/exp_R(x)) by A1,XCMPLX_1:128;
  then
A5: y=2*(exp_R(x)/((exp_R(x))^2-1)) by XCMPLX_1:80;
  then
A6: y=2*exp_R(x)/((exp_R(x))^2-1);
  then y*(t^2-1)=2*t by A3,Th30,XCMPLX_1:88;
  then
A7: y*t^2+(-2)*t+(-y)=0;
A8: exp_R(x)<>1 by A3,Th29;
  then
A9: 2*exp_R(x)/((exp_R(x))^2-1)<>0 by Lm20,SIN_COS:60;
  per cases by A9,A5;
  suppose
A10: 0<y;
A11: 0<1+y^2 by Lm6;
    then 0*4<4*(1+y^2) by XREAL_1:70;
    then
    t=(2+sqrt delta(y,-2,-y))/(2*y) or t=(-(-2)-sqrt delta(y,-2,-y))/(2*y
    ) by A1,A8,A6,A7,A4,Lm20, QUIN_1:15;
    then
    t=(2+sqrt(4)*sqrt(1+y^2))/(2*y) or t=(2-sqrt(4*(1+y^2)))/(2*y) by A4
    ,A11,SQUARE_1:97;
    then t=(2*(1+sqrt(1+y^2)))/(2*y) or t=(2-2*sqrt(1+y^2))/(2*y) by A11,
    SQUARE_1:85,97;
    then
A12: t=(1+sqrt(1+y^2))/y or t=(2*(1-sqrt(1+y^2)))/(2*y) by
    XCMPLX_1:92;
    (1-sqrt(1+y^2))/y<0 by A10,Lm15;
    hence thesis by A1,A12,TAYLOR_1:12,XCMPLX_1:92;
  end;
  suppose
A13: y<0;
A14: 0<1+y^2 by Lm6;
    then 0*4<4*(1+y^2) by XREAL_1:70;
    then
    t=(2+sqrt delta(y,-2,-y))/(2*y) or t=(-(-2)-sqrt delta(y,-2,-y))/(2*y
    ) by A1,A8,A6,A7,A4,Lm20, QUIN_1:15;
    then
    t=(2+sqrt(4)*sqrt(1+y^2))/(2*y) or t=(2-sqrt(4*(1+y^2)))/(2*y) by A4
    ,A14,SQUARE_1:97;
    then t=(2*(1+sqrt(1+y^2)))/(2*y) or t=(2-2*sqrt(1+y^2))/(2*y) by A14,
    SQUARE_1:85,97;
    then
A15: t=(1+sqrt(1+y^2))/y or t=(2*(1-sqrt(1+y^2)))/(2*y) by
    XCMPLX_1:92;
    (1+sqrt(1+y^2))/y<0 by A13,Lm21;
    then exp_R(x)=(1-sqrt(1+y^2))/y by A15,SIN_COS:60,XCMPLX_1:92;
    hence thesis by TAYLOR_1:12;
  end;
end;

theorem Th69:
  cosh.(2*x)=1+2*(sinh.x)^2
proof
  2*(sinh.x)^2=2*(1/2*(cosh.(2*x)-1)) by SIN_COS2:18;
  hence thesis;
end;

theorem Th70:
  (cosh.x)^2=1+(sinh.x)^2
proof
  cosh.(2*x)=cosh.(x+x) .=(cosh.x)^2+(sinh.x)^2 by SIN_COS2:20;
  then 1+2*(sinh.x)^2-(sinh.x)^2=(cosh.x)^2+(sinh.x)^2-(sinh.x)^2 by
  Th69;
  hence thesis;
end;

theorem Th71:
  (sinh.x)^2=(cosh.x)^2-1
proof
  (cosh.x)^2-(sinh.x)^2-1=1-1 by SIN_COS2:14;
  hence thesis;
end;

theorem
  sinh(5*x)=5*sinh(x)+20*(sinh(x))|^3+16*(sinh(x))|^5
proof
  set t = sinh.x;
  sinh(5*x) =sinh.(4*x+x) by SIN_COS2:def 2
    .=sinh.(2*(2*x))*(cosh.x)+(cosh.(4*x))*t by SIN_COS2:21
    .=2*(sinh.(2*x))*(cosh.(2*x))*(cosh.x)+(cosh.(4*x))*t by SIN_COS2:23
    .=2*(2*t*(cosh.x))*(cosh.(2*x))*(cosh.x)+(cosh.(4*x))*t by SIN_COS2:23
    .=4*t*((cosh.x)^2)*(cosh.(2*x))+(cosh.(4*x))*t
    .=4*t*((cosh.x)^2)*(1+2*t^2)+(cosh.(2*(2*x)))*t by Th69
    .=4*t*((cosh.x)^2)*(1+2*t^2)+(1+2*(sinh.(2*x))^2)*t by Th69
    .=4*t*((cosh.x)^2)*(1+2*t^2)+(1+2*(2*t* (cosh.x))^2)*t by SIN_COS2:23
    .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*t^2*t*(cosh.x)^2
    .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*(t|^1*t)*t*(cosh.x)^2 by NEWTON:10
    .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*t|^(1+1)*t*(cosh.x)^2 by NEWTON:11
    .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*(t|^2*t)*(cosh.x)^2
    .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*(t|^(2+1))*(cosh.x)^2 by NEWTON:11
    .=4*t*((cosh.x)^2)*1+4*t*((cosh.x)^2)*2*t^2+t+8*t|^3*(cosh.x)^2
    .=4*t*(1+t^2)*1+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2 by Th70
    .=4*t+4*t*t*t+8*t*((cosh.x)^2)*t^2+sinh.x+8*t|^3*(cosh.x)^2
    .=4*t+4*t|^1*t*t+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2 by NEWTON:10
    .=4*t+4*(t|^1*t)*t+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2
    .=4*t+4*t|^(1+1)*t+8*t*((cosh.x)^2)* t^2+t+8*t|^3*(cosh.x)^2 by NEWTON:
  11
    .=4*t+4*(t|^2*t)+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2
    .=4*t+4*(t|^(2+1))+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2 by NEWTON:11
    .=4*t+4*t|^3+8*t*t*t*((cosh.x)^2)+t+8*t|^3*(cosh.x)^2
    .=4*t+4*t|^3+8*t|^1*t*t* ((cosh.x)^2)+t+8*t|^3*(cosh.x)^2 by NEWTON:10
    .=4*t+4*t|^3+8*(t|^1*t)*t*((cosh.x)^2)+t+8*t|^3*(cosh.x)^2
    .=4*t+4*t|^3+8*(t|^(1+1))*t* ((cosh.x)^2)+t+8*t|^3*(cosh.x)^2 by NEWTON:
  11
    .=4*t+4*t|^3+8*(t|^2*t)*((cosh.x)^2)+t+8*t|^3*(cosh.x)^2
    .=4*t+4*t|^3+8*(t|^(2+1))* ((cosh.x)^2)+t+8*t|^3*(cosh.x)^2 by NEWTON:11
    .=4*t+4*t|^3+8*t|^3*(1+t^2)+t+8*t|^3*(cosh.x)^2 by Th70
    .=4*t+4*t|^3+8*t|^3+8*(t|^3*t)*t+t+8*t|^3*(cosh.x)^2
    .=4*t+4*t|^3+8*t|^3+8*(t|^(3+1))*t+t+8*t|^3*(cosh.x)^2 by NEWTON:11
    .=4*t+4*t|^3+8*t|^3+8*(t|^4*t)+t+8*t|^3*(cosh.x)^2
    .=4*t+4*t|^3+8*t|^3+8*(t|^(4+1))+t+8*t|^3*(cosh.x)^2 by NEWTON:11
    .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3*(1+t^2) by Th70
    .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^3*t)*t
    .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^(3+1))*t by NEWTON:11
    .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^4*t)
    .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^(4+1)) by NEWTON:11
    .=5*t+20*t|^3+(8*t|^5+8*t|^5)
    .=5*sinh(x)+20*t|^3+16*t|^5 by SIN_COS2:def 2
    .=5*sinh(x)+20*(sinh x)|^3+16*t|^5 by SIN_COS2:def 2
    .=5*sinh(x)+20*(sinh x)|^3+16*(sinh x)|^5 by SIN_COS2:def 2;
  hence thesis;
end;

theorem
  cosh(5*x)=5*cosh(x)-20*(cosh(x))|^3+16*(cosh(x))|^5
proof
  set t = cosh.x, u = sinh.x;
  cosh(5*x) = cosh.(4*x+x) by SIN_COS2:def 4
    .=(cosh.(2*(2*x)))*t+(sinh.(4*x))*u by SIN_COS2:20
    .=(1+2*(sinh.(2*x))^2)*t+(sinh.(4*x))*u by Th69
    .=(1+2*((2*u)*t)^2)*t+(sinh.(2*(2*x)))*u by SIN_COS2:23
    .=t+8*u^2*(t^2*t)+(sinh.(2*(2*x)))*u
    .=t+8*u^2*(t|^1*t*t)+(sinh.(2*(2*x)))*u by NEWTON:10
    .=t+8*u^2*(t|^(1+1)*t)+(sinh.(2*(2*x)))*u by NEWTON:11
    .=t+8*(sinh.x)^2*(t|^(2+1))+(sinh.(2*(2*x)))*u by NEWTON:11
    .=t+8*(t^2-1)*(t|^3)+(sinh.(2*(2*x)))*u by Th71
    .=t+8*(t|^3*t*t)-8*t|^3+(sinh.(2*(2*x)))*u
    .=t+8*(t|^(3+1)*t)-8*t|^3+(sinh.(2*(2*x)))*u by NEWTON:11
    .=t+8*(t|^(4+1))-8*t|^3+(sinh.(2*(2*x)))*u by NEWTON:11
    .=t+8*(t|^5)-8*t|^3+(2*(sinh.(2*x))*(cosh.(2*x)))*u by SIN_COS2:23
    .=t+8*(t|^5)-8*t|^3+(2*(2*u*t)*(cosh.(2*x)))*u by SIN_COS2:23
    .=t+8*(t|^5)-8*t|^3+4*u^2*t*(cosh.(2*x))
    .=t+8*(t|^5)-8*t|^3+4*(t^2-1)*t*(cosh.(2*x)) by Th71
    .=t+8*(t|^5)-8*t|^3+4*(t*t*t)*(cosh.(2*x))-4*t*(cosh.(2*x))
    .=t+8*(t|^5)-8*t|^3+4*(t|^1*t*t)*(cosh.(2*x))-4*t*(cosh.(2*x)) by NEWTON
  :10
    .=t+8*(t|^5)-8*t|^3+4*(t|^(1+1)*t)*(cosh.(2*x))-4*t*(cosh.(2*x)) by
  NEWTON:11
    .=t+8*(t|^5)-8*t|^3+4*(t|^(2+1)) *(cosh.(2*x))-4*t*(cosh.(2*x)) by
  NEWTON:11
    .=t+8*(t|^5)-8*t|^3+4*t|^3*(2*t^2-1)-4*t*(cosh.(2*x)) by SIN_COS2:23
    .=t+8*(t|^5)-8*t|^3+8*(t|^3*t*t)-4*t|^3-4*t*(cosh.(2*x))
    .=t+8*(t|^5)-8*t|^3+8*(t|^(3+1)*t)-4*t|^3-4*t*(cosh.(2*x)) by NEWTON:11
    .=t+8*(t|^5)-8*t|^3+8*(t|^(4+1))-4*t|^3-4*t*(cosh.(2*x)) by NEWTON:11
    .=t+16*(t|^5)-12*t|^3-4*t*(2*t^2-1) by SIN_COS2:23
    .=5*t+16*(t|^5)-12*t|^3-8*(t*t*t)
    .=5*t+16*(t|^5)-12*t|^3-8*(t|^1*t*t) by NEWTON:10
    .=5*t+16*(t|^5)-12*t|^3-8*(t|^(1+1)*t) by NEWTON:11
    .=5*t+16*(t|^5)-12*t|^3-8*(t|^(2+1)) by NEWTON:11
    .=5*t-20*t|^3+16*(t|^5)
    .=5*(cosh x)-20*t|^3+16*(t|^5) by SIN_COS2:def 4
    .=5*(cosh x)-20*(cosh x)|^3+16*(t|^5) by SIN_COS2:def 4
    .=5*cosh(x)-20*(cosh x)|^3+16*(cosh x)|^5 by SIN_COS2:def 4;
  hence thesis;
end;
