A7:     0*(y^2-1)<=(x^2-1)*(y^2-1) by A4,AXIOMS:25;
    B1:cosh2"(x)-cosh2"(y)
    =-(log(number_e,x+sqrt(x^2-1)))-cosh2"(y) by Def22
   .=-(log(number_e,x+sqrt(x^2-1)))-(-(log(number_e,y+sqrt(y^2-1)))) 
     by Def22
   .=-(log(number_e,x+sqrt(x^2-1))-log(number_e,y+sqrt(y^2-1)))
   .=-log(number_e,((x+sqrt(x^2-1))/(y+sqrt(y^2-1)))) by A3,Lm2,POWER:62
   .=-log(number_e,(((x+sqrt(x^2-1))*(y-sqrt(y^2-1)))/
     ((y+sqrt(y^2-1))*(y-sqrt(y^2-1))))) by A2,XCMPLX_1:92
   .=-log(number_e,(((x+sqrt(x^2-1))*(y-sqrt(y^2-1)))/
     (y^2-(sqrt(y^2-1))^2))) by SQUARE_1:67 
   .=-log(number_e,(((x+sqrt(x^2-1))*(y-sqrt(y^2-1)))/
     (y^2-(y^2-1)))) by A4,SQUARE_1:def 4
   .=-log(number_e,(x*y-x*sqrt(y^2-1)+y*sqrt(x^2-1)-sqrt(x^2-1)*
     (sqrt(y^2-1))))
   .=-log(number_e,(x*y-x*sqrt(y^2-1)+y*sqrt(x^2-1)-sqrt((x^2-1)*(y^2-1))))
      by A4,SQUARE_1:97
   .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1))+y*sqrt(x^2-1)-x*sqrt(y^2-1)));
   set t=y*sqrt(x^2-1)-x*sqrt(y^2-1);
   A8:    t=sqrt((y*sqrt(x^2-1)-x*sqrt(y^2-1))^2) by SQUARE_1:89,A5
       .=sqrt((y*sqrt(x^2-1))^2-2*(y*sqrt(x^2-1))*
         (x*sqrt(y^2-1))+(x*sqrt(y^2-1))^2) by SQUARE_1:64
       .=sqrt(y^2*(sqrt(x^2-1))^2-2*(y*sqrt(x^2-1))*
         (x*sqrt(y^2-1))+(x*sqrt(y^2-1))^2) by SQUARE_1:68
       .=sqrt(y^2*(x^2-1)-2*(y*sqrt(x^2-1))*
         (x*sqrt(y^2-1))+(x*sqrt(y^2-1))^2) by A4,SQUARE_1:def 4
       .=sqrt(y^2*x^2-y^2-2*(y*sqrt(x^2-1))*
         (x*sqrt(y^2-1))+(x*sqrt(y^2-1))^2)
       .=sqrt((x*y)^2-y^2-2*(y*sqrt(x^2-1))*
         (x*sqrt(y^2-1))+(x*sqrt(y^2-1))^2) by SQUARE_1:68
       .=sqrt((x*y)^2-y^2-2*(y*sqrt(x^2-1))*
         (x*sqrt(y^2-1))+x^2*(sqrt(y^2-1))^2) by SQUARE_1:68
       .=sqrt((x*y)^2-y^2-2*(y*sqrt(x^2-1))*
         (x*sqrt(y^2-1))+x^2*(y^2-1)) by A4,SQUARE_1:def 4
       .=sqrt((x*y)^2-y^2-2*(y*sqrt(x^2-1))*(x*sqrt(y^2-1))+(x^2*y^2-x^2))
       .=sqrt((x*y)^2-y^2-2*(y*sqrt(x^2-1))*(x*sqrt(y^2-1))+((x*y)^2-x^2))
          by SQUARE_1:68
       .=sqrt(2*(x*y)^2-x^2-y^2-2*x*y*(sqrt(x^2-1)*sqrt(y^2-1)))
       .=sqrt(2*(x*y)^2-x^2-y^2-2*x*y*(sqrt((x^2-1)*(y^2-1))))
         by A4,SQUARE_1:97;
  B2:-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1))+y*sqrt(x^2-1)-x*sqrt(y^2-1)))
    =-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1))+t))
   .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1))+sqrt(2*(x*y)^2-x^2-y^2-
      2*x*y*(sqrt((x^2-1)*(y^2-1)))))) by A8;
   cosh2"(x*y-sqrt((x^2-1)*(y^2-1)))
    =-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+
     sqrt((x*y-sqrt((x^2-1)*(y^2-1)))^2-1)) by Def22
  .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+sqrt((x*y)^2-2*(x*y)*
    (sqrt((x^2-1)*(y^2-1)))+(sqrt((x^2-1)*(y^2-1)))^2-1)) by SQUARE_1:64
  .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+sqrt((x*y)^2-2*(x*y)*
    (sqrt((x^2-1)*(y^2-1)))+((x^2-1)*(y^2-1))-1)) by A7,SQUARE_1:def 4     
  .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+sqrt((x*y)^2-2*(x*y)*
    (sqrt((x^2-1)*(y^2-1)))+x^2*y^2-x^2-y^2))
  .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+sqrt((x*y)^2-2*(x*y)*
    (sqrt((x^2-1)*(y^2-1)))+(x*y)^2-x^2-y^2)) by SQUARE_1:68
  .=-log(number_e,(x*y-sqrt((x^2-1)*(y^2-1)))+sqrt(2*(x*y)^2-x^2-y^2
     -2*x*y*(sqrt((x^2-1)*(y^2-1)))));
  hence thesis by B1,B2;
 end;

theorem 
  x^2<1 & y^2<1 implies tanh"(x)+tanh"(y)=tanh"((x+y)/(1+x*y))
   proof
     assume A1:x^2<1 & y^2<1;then
     A2:0<(1+x)/(1-x) & 0<(1+y)/(1-y) by Th36;
     A4:x*y+1<>0 by A1,Th58;
 B1:tanh"(x)+tanh"(y)
   =(1/2)*log(number_e,(1+x)/(1-x))+tanh"(y) by Def3
  .=(1/2)*log(number_e,(1+x)/(1-x))+((1/2)*log(number_e,(1+y)/(1-y)))
     by Def3
  .=(1/2)*(log(number_e,(1+x)/(1-x))+log(number_e,(1+y)/(1-y)))
  .=(1/2)*(log(number_e,((1+x)/(1-x))*((1+y)/(1-y)))) by A2,Lm2,POWER:61
  .=(1/2)*(log(number_e,(((1+x)*(1+y))/((1-x)*(1-y))))) by XCMPLX_1:77
  .=(1/2)*(log(number_e,((1+x+y+x*y)/(1+x*y-x-y))));
 tanh"((x+y)/(1+x*y))
   =(1/2)*log(number_e,(1+((x+y)/(1+x*y)))/(1-((x+y)/(1+x*y)))) by Def3
  .=(1/2)*log(number_e,(((x+y)+(1+x*y)*1)/(1+x*y))/(1-((x+y)/(1+x*y))))
      by A4,XCMPLX_1:114
  .=(1/2)*log(number_e,((x+y+1+x*y)/(1+x*y))/(((1*(1+x*y)-(x+y))/(1+x*y))))
      by A4,XCMPLX_1:128 
  .=(1/2)*log(number_e,(1+x+y+x*y)/(1+x*y-x-y)) by A4,XCMPLX_1:55;
 hence thesis by B1;
end;

theorem 
  x^2<1 & y^2<1 implies tanh"(x)-tanh"(y)=tanh"((x-y)/(1-x*y))
  proof
    assume A1:x^2<1 & y^2<1;then
    A2:0<(1+x)/(1-x) & 0<(1+y)/(1-y) by Th36;
    A4:1-x*y<>0 by A1,Th62;
  B1:  tanh"(x)-tanh"(y)
   =(1/2)*log(number_e,(1+x)/(1-x))-tanh"(y) by Def3
  .=(1/2)*log(number_e,(1+x)/(1-x))-(1/2)*log(number_e,(1+y)/(1-y)) by Def3
  .=(1/2)*(log(number_e,(1+x)/(1-x))-log(number_e,(1+y)/(1-y)))
  .=(1/2)*(log(number_e,((1+x)/(1-x))/((1+y)/(1-y)))) by Lm2,A2,POWER:62
  .=(1/2)*(log(number_e,((1+x)*(1-y))/((1-x)*(1+y)))) by XCMPLX_1:85
  .=(1/2)*(log(number_e,(1-y+x-x*y)/(1+y-x-x*y)));
    tanh"((x-y)/(1-x*y))
   =(1/2)*log(number_e,(1+((x-y)/(1-x*y)))/(1-((x-y)/(1-x*y)))) by Def3
  .=(1/2)*log(number_e,(((x-y)+(1-x*y)*1)/(1-x*y))/(1-((x-y)/(1-x*y))))
     by A4,XCMPLX_1:114
  .=(1/2)*log(number_e,((x-y+1-x*y)/(1-x*y))/((1*(1-x*y)-(x-y))/(1-x*y)))
     by A4,XCMPLX_1:128
  .=(1/2)*(log(number_e,(1-y+x-x*y)/(1+y-x-x*y))) by A4,XCMPLX_1:55;
   hence thesis by B1;
  end;

theorem
  0<x & ((x-1)/(x+1))^2<1 implies log(number_e,x)=2*tanh"((x-1)/(x+1))
  proof
    assume 0<x & ((x-1)/(x+1))^2<1;then
    A2:x+1>0 by XREAL_1:36;
    2*tanh"((x-1)/(x+1))
   =2*((1/2)*log(number_e,(1+((x-1)/(x+1)))/(1-((x-1)/(x+1))))) by Def3
  .=log(number_e,(((x-1)+(x+1)*1)/(x+1))/(1-(x-1)/(x+1))) by A2,XCMPLX_1:114
  .=log(number_e,((2*x)/(x+1))/((1*(x+1)-(x-1))/(x+1))) by A2,XCMPLX_1:128
  .=log(number_e,(2*x)/2) by A2, XCMPLX_1:55
  .=log(number_e,x);
  hence thesis;
  end;

theorem
  0<x & ((x^2-1)/(x^2+1))^2<1 implies log(number_e,x)=tanh"((x^2-1)/(x^2+1))
  proof
    assume A1:0<x & ((x^2-1)/(x^2+1))^2<1;
    A2:x^2+1>0 by Lm16;
    tanh"((x^2-1)/(x^2+1))
   =(1/2)*log(number_e,(1+((x^2-1)/(x^2+1)))/(1-((x^2-1)/(x^2+1)))) by Def3
  .=(1/2)*log(number_e,(((x^2-1)+(x^2+1)*1)/(x^2+1))/(1-(x^2-1)/(x^2+1)))
     by A2,XCMPLX_1:114
  .=(1/2)*log(number_e,((2*x^2)/(x^2+1))/((1*(x^2+1)-(x^2-1))/(x^2+1)))
     by A2,XCMPLX_1:128
  .=(1/2)*log(number_e,2*x^2/2) by A2,XCMPLX_1:55
  .=(1/2)*log(number_e,x to_power 2) by POWER:53
  .=(1/2)*(2*log(number_e,x)) by Lm2,A1,POWER:63
  .=log(number_e,x);
  hence thesis;
 end;

theorem
  1<x & 1<=(x^2+1)/(2*x) implies log(number_e,x)=cosh1"((x^2+1)/(2*x))
   proof
     assume A1:1<x & 1<=(x^2+1)/(2*x);
     1*2<2*x by A1,REAL_1:70;then
  A5:1<2*x by AXIOMS:22;then
  A3:1<(2*x)^2 by SQUARE_1:59,78;
     x<x^2 by A1,SQUARE_1:76;then
     1<x^2 by A1,AXIOMS:22;then
  A6:1-1<x^2-1 by REAL_1:92;then
  A4:0<(x^2-1)^2 by SQUARE_1:74;
     cosh1"((x^2+1)/(2*x))
    =log(number_e,((x^2+1)/(2*x))+sqrt(((x^2+1)/(2*x))^2-1)) by Def21
   .=log(number_e,((x^2+1)/(2*x))+sqrt(((x^2+1))^2/(2*x)^2-1)) by SQUARE_1:69
   .=log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)^2+2*x^2+1)/(2*x)^2-1)) 
     by SQUARE_1:65
   .=log(number_e,((x^2+1)/(2*x))+sqrt((((x^2)^2+2*x^2+1)-1*(2*x)^2)/(2*x)^2)) 
      by A3,XCMPLX_1:127
   .=log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)^2+2*x^2+1-(2^2*x^2))/(2*x)^2))
      by SQUARE_1:68
   .=log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)^2+2*x^2+1-(2*2*x^2))/(2*x)^2))
      by SQUARE_1:def 3
   .=log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)^2-2*x^2+1)/(2*x)^2))
   .=log(number_e,((x^2+1)/(2*x))+sqrt((((x^2)-1)^2)/(2*x)^2)) by SQUARE_1:66
   .=log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)-1)^2)/sqrt((2*x)^2)) 
      by A3,A4,SQUARE_1:99
   .=log(number_e,((x^2+1)/(2*x))+(x^2-1)/sqrt((2*x)^2)) by A6,SQUARE_1:89
   .=log(number_e,((x^2+1)/(2*x))+(x^2-1)/(2*x)) by A5,SQUARE_1:89
   .=log(number_e,((x^2+1)+(x^2-1))/(2*x)) by XCMPLX_1:63
   .=log(number_e,(2*x^2)/(2*x))
   .=log(number_e,(x^2)/x) by XCMPLX_1:92
   .=log(number_e,(x*x)/x) by SQUARE_1:def 3
   .=log(number_e,x/(x/x)) by XCMPLX_1:78 
   .=log(number_e,x/1) by XCMPLX_1:60,A1
   .=log(number_e,x);
   hence thesis;
   end;

theorem
  0<x & x<1 & 1<=(x^2+1)/(2*x) implies log(number_e,x)=cosh2"((x^2+1)/(2*x))
   proof
     assume A1:0<x & x<1 & 1<=(x^2+1)/(2*x);then
  A2:   0*2<x*2 by REAL_1:70;then
  A3:0<(2*x)^2 by SQUARE_1:74;
     x^2<x by A1,SQUARE_1:75;then
     x^2<1 by A1,AXIOMS:22;then
  A4:x^2-x^2<1-x^2 by REAL_1:92;then
  A5:0<(1-x^2)^2 by SQUARE_1:74;
  A6:(1/x)=x to_power (-1) by A1,Th4;
     cosh2"((x^2+1)/(2*x))
    =-log(number_e,((x^2+1)/(2*x))+sqrt(((x^2+1)/(2*x))^2-1)) by Def22
   .=-log(number_e,((x^2+1)/(2*x))+sqrt(((x^2+1))^2/(2*x)^2-1)) by SQUARE_1:69
   .=-log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)^2+2*x^2+1)/(2*x)^2-1)) 
     by SQUARE_1:65
   .=-log(number_e,((x^2+1)/(2*x))+sqrt((((x^2)^2+2*x^2+1)-1*(2*x)^2)/(2*x)^2)) 
      by A3,XCMPLX_1:127
   .=-log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)^2+2*x^2+1-(2^2*x^2))/(2*x)^2))
      by SQUARE_1:68
   .=-log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)^2+2*x^2+1-(2*2*x^2))/(2*x)^2))
      by SQUARE_1:def 3
   .=-log(number_e,((x^2+1)/(2*x))+sqrt(((x^2)^2-2*x^2+1^2)/(2*x)^2))
      by SQUARE_1:59
   .=-log(number_e,((x^2+1)/(2*x))+sqrt((1^2-2*1*x^2+(x^2)^2)/(2*x)^2))
   .=-log(number_e,((x^2+1)/(2*x))+sqrt(((1-(x^2))^2)/(2*x)^2)) by SQUARE_1:64
   .=-log(number_e,((x^2+1)/(2*x))+sqrt((1-(x^2))^2)/sqrt(2*x)^2) 
      by A3,A5,SQUARE_1:99
   .=-log(number_e,((x^2+1)/(2*x))+(1-(x^2))/sqrt(2*x)^2) by A4,SQUARE_1:89
   .=-log(number_e,((x^2+1)/(2*x))+(1-x^2)/(2*x)) by A2,SQUARE_1:89
   .=-log(number_e,((x^2+1)+(1-x^2))/(2*x)) by XCMPLX_1:63
   .=-log(number_e,(2*1)/(2*x))
   .=-log(number_e,1/x) by XCMPLX_1:92
   .=-((-1)*log(number_e,x)) by POWER:63,Lm2,A1,A6
   .=log(number_e,x);
    hence thesis;
   end; 

theorem
  0<x implies log(number_e,x)=sinh"((x^2-1)/(2*x))
   proof
     assume A1:0<x;then
  A2:0*2<x*2 by REAL_1:70;then
  A3:0<(2*x)^2 by SQUARE_1:74;
     x^2>=0 by SQUARE_1:72;then
  A5:x^2+1>=0+1 by REAL_1:55;then
  A4:(x^2+1)^2>0 by SQUARE_1:74; 
     sinh"((x^2-1)/(2*x))
    =log(number_e,(((x^2-1)/(2*x))+sqrt(((x^2-1)/(2*x))^2+1))) by Def1
   .=log(number_e,(((x^2-1)/(2*x))+sqrt((x^2-1)^2/(2*x)^2+1))) 
      by SQUARE_1:69
   .=log(number_e,(((x^2-1)/(2*x))+sqrt(((x^2)^2-2*x^2+1)/(2*x)^2+1))) 
      by SQUARE_1:66
   .=log(number_e,(((x^2-1)/(2*x))+sqrt((((x^2)^2-2*x^2+1)+
        (2*x)^2*1)/(2*x)^2))) by A3,XCMPLX_1:114      
   .=log(number_e,(((x^2-1)/(2*x))+sqrt(((x^2)^2-2*x^2+1+
        (2^2*x^2))/(2*x)^2))) by SQUARE_1:68
   .=log(number_e,(((x^2-1)/(2*x))+sqrt(((x^2)^2-2*x^2+1+
        (2*2*x^2))/(2*x)^2))) by SQUARE_1:def 3
   .=log(number_e,(((x^2-1)/(2*x))+sqrt(((x^2)^2+2*x^2+1)/(2*x)^2)))
   .=log(number_e,(((x^2-1)/(2*x))+sqrt(((x^2+1)^2)/(2*x)^2))) by SQUARE_1:65
   .=log(number_e,(((x^2-1)/(2*x))+sqrt((x^2+1)^2)/sqrt((2*x)^2)))
       by A3,A4,SQUARE_1:99
   .=log(number_e,(((x^2-1)/(2*x))+(x^2+1)/sqrt((2*x)^2))) by A5,SQUARE_1:89
   .=log(number_e,((x^2-1)/(2*x)+(x^2+1)/(2*x))) by A2,SQUARE_1:89
   .=log(number_e,(((x^2-1)+(x^2+1))/(2*x))) by XCMPLX_1:63
   .=log(number_e,((2*x^2)/(2*x)))
   .=log(number_e,((x^2)/x)) by XCMPLX_1:92
   .=log(number_e,(x*x)/x) by SQUARE_1:def 3
   .=log(number_e,x/(x/x)) by XCMPLX_1:78
   .=log(number_e,x/1) by XCMPLX_1:60,A1
   .=log(number_e,x);
   hence thesis;
 end;

theorem
  y=(1/2)*(exp(x)-exp(-x)) implies x=log(number_e,(y+sqrt(y^2+1)))
  proof
    assume A1:y=(1/2)*(exp(x)-exp(-x));
    A2:exp(x)>0 by SIN_COS:60;
    A3:2*y*exp(x)=(exp(x)-1/exp(x))*exp(x) by A1,TAYLOR_1:4;
    set t=exp(x);
    2*y*t=t*t-(1/t)*t by A3;then
    2*y*t=t^2-(1/t)*t by SQUARE_1:def 3;then
    2*y*t=t^2-(t*1)/t by XCMPLX_1:75;then
    2*y*t-2*y*t=t^2-1-2*y*t by A2,XCMPLX_1:60;then
    C1:1*t^2+(-2*y)*t+(-1)=0 by XCMPLX_1:14;
    A7:delta(1,-2*y,-1)=((-2)*y)^2-4*1*(-1) by QUIN_1:def 1
         .=(-2)^2*(y^2)-4*1*(-1) by SQUARE_1:68
         .=(-2)*(-2)*(y^2)-4*1*(-1) by SQUARE_1:def 3
         .=4*(y^2)+4;
    A9:0<=y^2 by SQUARE_1:72;then
    A10:0+1<=y^2+1 by AXIOMS:24;
    4*0<=4*y^2 by A9,AXIOMS:25;then
    0+4<=4*y^2+4 by AXIOMS:24;then
    t = (-(-2*y)+sqrt delta(1,-2*y,-1))/(2*1)
        or t = (-(-2*y)-sqrt delta(1,-2*y,-1))/(2*1)
             by QUIN_1:15,A7,C1;then
    t=(2*y+sqrt(4)*sqrt(y^2+1))/2
           or t=(2*y-sqrt(4*(y^2+1)))/2 by A7,SQUARE_1:97,A10;then
    t=(2*y+2*sqrt(y^2+1))/2
           or t=(2*y-2*sqrt(y^2+1))/2 by SQUARE_1:85,97,A10;then
T1: exp(x)=y+sqrt(y^2+1) or exp(x)=y-sqrt(y^2+1);
    y<sqrt(y^2+1)+0 by Lm22;
    hence thesis by A2,T1,TAYLOR_1:12,REAL_1:84;
  end;

theorem
  y=(1/2)*(exp(x)+exp(-x)) & 1<=y implies 
  x=log(number_e,(y+sqrt(y^2-1))) or x=-log(number_e,(y+sqrt(y^2-1))) 
  proof
    assume A1:y=(1/2)*(exp(x)+exp(-x)) & 1<=y;then       
    A10:0<=(y^2)-1 by Th29;then
B1: 0*4<=((y^2)-1)*4 by AXIOMS:25;
    A12:y+sqrt(y^2-1)>0 by A1,Lm25;then
    B2:1/(y+sqrt(y^2-1))=(y+sqrt(y^2-1)) to_power (-1) by Th4;
    A2:0<exp(x)by SIN_COS:60;
    A3:2*y*exp(x)=(exp(x)+1/exp(x))*exp(x) by A1,TAYLOR_1:4;
    set t=exp(x);
    2*y*t=t*t+(1/t)*t by A3;then
    2*y*t=t^2+(1/t)*t by SQUARE_1:def 3;then
    2*y*t=t^2+(t*1)/t by XCMPLX_1:75;then
    2*y*t-2*y*t=t^2+1-2*y*t by A2,XCMPLX_1:60;then
    C1:0=1*t^2+(-2*y)*t+1 by XCMPLX_1:14;
    A7:delta(1,-2*y,1)=((-2)*y)^2-4*1*1 by QUIN_1:def 1
         .=(-2)^2*(y^2)-4*1*1 by SQUARE_1:68
         .=(-2)*(-2)*(y^2)-4*1*1 by SQUARE_1:def 3
         .=4*(y^2)-4;then
    A8:0<=delta(1,-2*y,1) by B1;
    t = (-(-2*y)+sqrt delta(1,-2*y,1))/(2*1)
        or t = (-(-2*y)-sqrt delta(1,-2*y,1))/(2*1)
              by A7,B1,C1,QUIN_1:15;then
              t=(2*y+sqrt(4)*sqrt(y^2-1))/2
           or t=(2*y-sqrt(4*(y^2-1)))/2 by A7,SQUARE_1:97,A10;then
A11:          t=(2*y+2*sqrt(y^2-1))/2
           or t=(2*y-2*sqrt(y^2-1))/2 by SQUARE_1:85,97,A10;
    log(number_e,y+sqrt(y^2-1))=x
           or log(number_e,y-sqrt(y^2-1))=x by A11,TAYLOR_1:12;then
              log(number_e,y+sqrt(y^2-1))=x 
           or log(number_e,(y-sqrt(y^2-1))*(y+sqrt(y^2-1))/
              (y+sqrt(y^2-1)))=x by A12,XCMPLX_1:90;then      
              log(number_e,y+sqrt(y^2-1)) = x 
           or log(number_e,(y^2-(sqrt(y^2-1))^2)/
              (y+sqrt(y^2-1)))=x by SQUARE_1:67;then          
              log(number_e,y+sqrt(y^2-1))=x 
           or log(number_e,(y^2-(y^2-1))/(y+sqrt(y^2-1)))=x 
       by A10,SQUARE_1:def 4;then               
              log(number_e,y+sqrt(y^2-1))=x 
           or (-1)*log(number_e,(y+sqrt(y^2-1)))=x by POWER:63,Lm2,A12,B2;          
     hence thesis;
  end;    

theorem
  y=(exp(x)-exp(-x))/(exp(x)+exp(-x)) implies
  x=(1/2)*log(number_e,(1+y)/(1-y))
 proof
  A1:0<exp(x)by SIN_COS:60;then
     0*exp(x)<exp(x)*exp(x) by REAL_1:70;then
     0<(exp x)^2 by SQUARE_1:def 3;then
A2:  0+1<(exp x)^2+1 by REAL_1:67;
  assume y=(exp(x)-exp(-x))/(exp(x)+exp(-x));then
     y=(exp(x)-1/exp(x))/(exp(x)+exp(-x)) by TAYLOR_1:4;then
     y=(exp(x)-1/exp(x))/(exp(x)+1/exp(x)) by TAYLOR_1:4;then
     y=((exp(x)*exp(x)-1)/exp(x))/(exp(x)+1/exp(x))
        by A1,XCMPLX_1:128;then
     y=((exp(x)*exp(x)-1)/exp(x))/((1+exp(x)*exp(x))/exp(x))
        by A1,XCMPLX_1:114;then
     y=(((exp x)^2-1)/exp(x))/((1+exp(x)*exp(x))/exp(x))
        by SQUARE_1:def 3;then
     y=(((exp x)^2-1)/exp(x))/((1+(exp(x))^2)/exp(x))
        by SQUARE_1:def 3;then
  A4:y=((exp x)^2-1)/(1+(exp(x))^2) by A1,XCMPLX_1:55;then
  A5:-1<y & y<1 by A1,Lm26,Th65;then
  A6:y-1<>0 by XCMPLX_1:15;
  A7:0<(y+1)/(1-y) by A5,Th66;then
  A8:sqrt((y+1)/(1-y))=((y+1)/(1-y)) to_power (1/2) by ASYMPT_1:89;
   set t=exp(x);
   1*y+t^2*y=(t^2-1)/(1+t^2)*(1+t^2) by A4;then
   y+t^2*y-(t^2-1)=t^2-1-(t^2-1) by A2,XCMPLX_1:88;then
   y+t^2*y-t^2+1=0 by XCMPLX_1:37;then
   t^2*(y-1)=0-(y+1);then
   t^2*(y-1)/(y-1)=(-(y+1))/(y-1);then
   t^2*(y-1)/(y-1)=(y+1)/(-(y-1)) by XCMPLX_1:193;then
   sqrt(t^2)=sqrt((y+1)/(1-y)) by A6,XCMPLX_1:90;then
A9:exp(x)=sqrt((y+1)/(1-y)) by A1,SQUARE_1:89;
log(number_e,((y+1)/(1-y)) to_power (1/2))=x by A9,TAYLOR_1:12,A8;
  hence thesis by Lm2,A7,POWER:63;
 end;

theorem
  y=(exp(x)+exp(-x))/(exp(x)-exp(-x)) & x<>0 implies 
  x=(1/2)*log(number_e,(y+1)/(y-1))
   proof
     A1:0<exp(x)by SIN_COS:60;
     assume A2:y=(exp(x)+exp(-x))/(exp(x)-exp(-x)) & x<>0;then
     A3:(exp(x))^2-1<>0 by Th64;
     B1:exp(x)<>1 by A2,Th63;
     y=(exp(x)+1/exp(x))/(exp(x)-exp(-x)) by A2,TAYLOR_1:4;then
     y=(exp(x)+1/exp(x))/(exp(x)-1/exp(x)) by TAYLOR_1:4;then
     y=(exp(x)+1/exp(x))/((exp(x)*exp(x)-1)/exp(x))
       by A1,XCMPLX_1:128;then
     y=((1+exp(x)*exp(x))/exp(x))/((exp(x)*exp(x)-1)/exp(x))
       by A1,XCMPLX_1:114;then
     y=((1+exp(x)*exp(x))/exp(x))/(((exp(x))^2-1)/exp(x))
                   by SQUARE_1:def 3;then
     y=((1+(exp x)^2)/exp(x))/(((exp x)^2-1)/exp(x))
                   by SQUARE_1:def 3;then
     A4:y=(1+(exp x)^2)/((exp x)^2-1) by A1,XCMPLX_1:55;
     set t=exp x;
     B2:1<y or y< -1 by A4,A1,B1,Lm29;then
A5:     1-1<y-1 or y-1< -1-1 by REAL_1:92;
     A6:0<(y+1)/(y-1) by B2,Lm30;then
     A8:sqrt((y+1)/(y-1))=((y+1)/(y-1)) to_power (1/2) by ASYMPT_1:89;
        y*(t^2-1)=1+t^2 by A3,A4,XCMPLX_1:88;then
        y*t^2-y*1=1+t^2 by XCMPLX_1:40;then
        y*t^2-y-t^2=1+t^2-t^2;then
        y*t^2-t^2*1-y=1 by XCMPLX_1:21;then
        t^2*(y-1)/(y-1)=(y+1)/(y-1) by XCMPLX_1:40;then
        sqrt(t^2)=sqrt((y+1)/(y-1)) by A5,XCMPLX_1:90;then
      A9:exp(x)=sqrt((y+1)/(y-1)) by A1,SQUARE_1:89;
        log(number_e,((y+1)/(y-1)) to_power (1/2))=x by A8,TAYLOR_1:12,A9;
     hence thesis by Lm2,A6,POWER:63;
   end;

theorem
  y=1/((exp(x)+exp(-x))/2) implies x=log(number_e,(1+sqrt(1-y^2))/y)
  or x=-log(number_e,(1+sqrt(1-y^2))/y)
  proof
   A1:0<exp(x)by SIN_COS:60;
   C5:log(number_e,exp(x))=x by TAYLOR_1:12;
   assume y=1/((exp(x)+exp(-x))/2);then
   y=(1*2)/(2*((exp(x)+exp(-x))/2)) by XCMPLX_1:92;then
   y=2/(exp(x)+1/exp(x)) by TAYLOR_1:4;then
   y=2/((1+exp(x)*exp(x))/exp(x)) by A1,XCMPLX_1:114;then
   y=2/((1+(exp(x))^2)/exp(x)) by SQUARE_1:def 3;then
   A3:y=2*(exp(x)/(1+(exp(x))^2)) by XCMPLX_1:80;
   set t=exp(x);
   C1:y=2*t/(1+t^2) by A3,XCMPLX_1:75;
   C2: 0<y & y<=1 by C1,A1,Lm31,Lm32;then
   B2:0<=1-y^2 & 0<=4-4*y^2 & 0<1+sqrt(1-y^2) & 0<(1+sqrt(1-y^2))/y
       by Lm35,Lm36,Lm37,Lm38;then
   B3:1/((1+sqrt(1-y^2))/y)=((1+sqrt(1-y^2))/y) to_power (-1) by Th4;
   1+t^2>0 by Lm16;then
   y*(1+t^2)=(2*t) by C1,XCMPLX_1:88;then
   y*1+y*t^2=2*t by XCMPLX_1:8;then
   y+y*t^2-2*t=2*t-2*t;then
D1:y+y*t^2-2*t=0;
   A6:Poly2(y,-2,y,t)=y*t^2+(-2)*t+y by POLYEQ_1:def 2;
   A7:delta(y,-2,y)
     =(-2)^2-4*y*y by QUIN_1:def 1
    .=(-2)^2-4*(y*y)
    .=(-2)^2-4*y^2 by SQUARE_1:def 3
    .=(-2)*(-2)-4*y^2 by SQUARE_1:def 3
    .=4-4*y^2;
     t=(-(-2)+sqrt delta(y,-2,y))/(2*y)
  or t=(-(-2)-sqrt delta(y,-2,y))/(2*y)
       by A6,C2,D1,B2,A7,QUIN_1:15;then
     t=(2+sqrt(4*(1-y^2)))/(2*y)
  or t=(2-sqrt(4*(1-y^2)))/(2*y) by A7;then
     t=(2+2*sqrt(1-y^2))/(2*y)
  or t=(2-2*sqrt(1-y^2))/(2*y) by B2,SQUARE_1:85,97;then
     t=(2*(1+sqrt(1-y^2)))/(2*y)
  or t=(2*(1-sqrt(1-y^2)))/(2*y);then
     t=(1+sqrt(1-y^2))/y
  or t=(1-sqrt(1-y^2))/y by XCMPLX_1:92;then
     t=(1+sqrt(1-y^2))/y
  or t=((1-sqrt(1-y^2))*(1+sqrt(1-y^2)))/(y*(1+sqrt(1-y^2)))
      by B2,XCMPLX_1:92;then
     t=(1+sqrt(1-y^2))/y
  or t=(1-(sqrt(1-y^2))^2)/(y*(1+sqrt(1-y^2))) by SQUARE_1:59,67;then
     t=(1+sqrt(1-y^2))/y
  or t=(1-(1-y^2))/(y*(1+sqrt(1-y^2))) by B2,SQUARE_1:def 4;then
     t=(1+sqrt(1-y^2))/y
  or t=(y*y)/(y*(1+sqrt(1-y^2))) by SQUARE_1:def 3;then
     t=(1+sqrt(1-y^2))/y
  or t=y/(1+sqrt(1-y^2)) by C2,XCMPLX_1:92;then
     log(number_e,(1+sqrt(1-y^2))/y)=x
  or log(number_e,((1+sqrt(1-y^2))/y) to_power (-1))=x
       by B3,C5,XCMPLX_1:57;then
     log(number_e,(1+sqrt(1-y^2))/y)=x
  or (-1)*log(number_e,((1+sqrt(1-y^2))/y))=x by Lm2,B2,POWER:63;
    hence thesis;
  end;

theorem
  y=1/((exp(x)-exp(-x))/2) & x<>0 implies x=log(number_e,(1+sqrt(1+y^2))/y)
  or x=log(number_e,(1-sqrt(1+y^2))/y)
   proof
   A1:0<exp(x)by SIN_COS:60;
      assume A2:y=1/((exp(x)-exp(-x))/2) & x<>0;then
   A3:(exp(x))^2-1<>0 by Th64;
      exp(x)<>1 by A2,Th63;then
   B2:2*exp(x)/((exp(x))^2-1)<>0 by A1,Lm39;
      y=(1*2)/(2*((exp(x)-exp(-x))/2)) by A2,XCMPLX_1:92;then
      y=2/(exp(x)-1/exp(x)) by TAYLOR_1:4;then
      y=2/((exp(x)*exp(x)-1)/exp(x)) by A1,XCMPLX_1:128;then
      y=2/(((exp(x))^2-1)/exp(x)) by SQUARE_1:def 3;then
      y=2*(exp(x)/((exp(x))^2-1)) by XCMPLX_1:80;then
   A4:y=2*exp(x)/((exp(x))^2-1) by XCMPLX_1:75;
      set t=exp x;
      y*(t^2-1)=2*t by A3,A4,XCMPLX_1:88;then
      y*t^2-y*1-2*t=2*t-2*t by XCMPLX_1:40;then
      y*t^2-y*1-2*t=0;then
      D1:y*t^2+(-2)*t+(-y)=0;
      A7:delta(y,-2,-y)=(-2)^2-4*y*(-y) by QUIN_1:def 1
              .=(-2)^2+4*(y*y)
              .=(-2)^2+4*y^2 by SQUARE_1:def 3
              .=(-2)*(-2)+4*y^2 by SQUARE_1:def 3
              .=4+4*y^2;
      C2:y<>0 by B2,A4;
               per cases by C2;
                suppose 0<y;then
            C3:(1-sqrt(1+y^2))/y<0 & 0<(1+sqrt(1+y^2))/y by Lm33,Lm34;
            D2:0<1+y^2 by Lm16;then
               0*4<4*(1+y^2) by REAL_1:70;then
               t=(2+sqrt delta(y,-2,-y))/(2*y) 
            or t=(-(-2)-sqrt delta(y,-2,-y))/(2*y) by A7,C2,D1,QUIN_1:15;then
               t=(2+sqrt(4)*sqrt(1+y^2))/(2*y) 
            or t=(2-sqrt(4*(1+y^2)))/(2*y) by A7,D2,SQUARE_1:97;then
               t=(2*(1+sqrt(1+y^2)))/(2*y) 
            or t=(2-2*sqrt(1+y^2))/(2*y) by D2,SQUARE_1:85,97;then
               t=(1+sqrt(1+y^2))/y 
            or t=(2*(1-sqrt(1+y^2)))/(2*y) by XCMPLX_1:92;
           hence thesis by TAYLOR_1:12,A1,C3,XCMPLX_1:92;
           end;
           suppose y<0;then
            C3':0<(1-sqrt(1+y^2))/y & (1+sqrt(1+y^2))/y<0 by Lm40,Lm41;
            D2':0<1+y^2 by Lm16;then
               0*4<4*(1+y^2) by REAL_1:70;then
               t=(2+sqrt delta(y,-2,-y))/(2*y)
            or t=(-(-2)-sqrt delta(y,-2,-y))/(2*y) by A7,C2,D1,QUIN_1:15;then
               t=(2+sqrt(4)*sqrt(1+y^2))/(2*y)
            or t=(2-sqrt(4*(1+y^2)))/(2*y) by A7,D2',SQUARE_1:97;then
               t=(2*(1+sqrt(1+y^2)))/(2*y) 
            or t=(2-2*sqrt(1+y^2))/(2*y) by D2',SQUARE_1:85,97;then
               t=(1+sqrt(1+y^2))/y 
            or t=(2*(1-sqrt(1+y^2)))/(2*y) by XCMPLX_1:92;then
               exp(x)=(1-sqrt(1+y^2))/y by XCMPLX_1:92,A1,C3';
           hence thesis by TAYLOR_1:12;   
           end;
      end;

theorem M1:
  cosh.(2*x)=1+2*(sinh.x)^2
  proof
    2*(sinh.x)^2=2*(1/2*(cosh.(2*x)-1)) by SIN_COS2:18;
    hence thesis;
  end;

theorem M2:
  (cosh.x)^2=1+(sinh.x)^2
  proof
    cosh.(2*x)=cosh.(x+x)
      .=(cosh.x)*(cosh.x)+(sinh.x)*(sinh.x) by SIN_COS2:20
      .=(cosh.x)^2+(sinh.x)*(sinh.x) by SQUARE_1:def 3
      .=(cosh.x)^2+(sinh.x)^2 by SQUARE_1:def 3;then
    1+2*(sinh.x)^2-(sinh.x)^2=(cosh.x)^2+(sinh.x)^2-(sinh.x)^2 by M1;then
    1+(sinh.x)^2+(sinh.x)^2-(sinh.x)^2=(cosh.x)^2+(sinh.x)^2-(sinh.x)^2;
    hence thesis by XCMPLX_1:26;
  end;

theorem M3:
  (sinh.x)^2=(cosh.x)^2-1
  proof
    (cosh.x)^2-(sinh.x)^2-1=1-1 by SIN_COS2:14;then
    (cosh.x)^2-(sinh.x)^2-1=0;then
    (cosh.x)^2-(sinh.x)^2+(sinh.x)^2-1=0+(sinh.x)^2;
    hence thesis;
  end;

theorem
  sinh(5*x)=5*sinh(x)+20*(sinh(x))|^3+16*(sinh(x))|^5
  proof
    set t = sinh.x;
    sinh(5*x) =sinh.(4*x+x) by SIN_COS2:def 2
  .=sinh.(2*(2*x))*(cosh.x)+(cosh.(4*x))*t by SIN_COS2:21
  .=2*(sinh.(2*x))*(cosh.(2*x))*(cosh.x)+(cosh.(4*x))*t by SIN_COS2:23
  .=2*(2*t*(cosh.x))*(cosh.(2*x))*(cosh.x)+(cosh.(4*x))*t by SIN_COS2:23
  .=2*2*t*((cosh.x)*(cosh.x))*(cosh.(2*x))+(cosh.(4*x))*(sinh.x)
  .=4*t*((cosh.x)^2)*(cosh.(2*x))+(cosh.(4*x))*t by SQUARE_1:def 3
  .=4*t*((cosh.x)^2)*(1+2*t^2)+(cosh.(2*(2*x)))*t by M1
  .=4*t*((cosh.x)^2)*(1+2*t^2)+(1+2*(sinh.(2*x))^2)*t by M1
  .=4*t*((cosh.x)^2)*(1+2*t^2)+(1+2*(2*t*
     (cosh.x))^2)*t by SIN_COS2:23
  .=4*t*((cosh.x)^2)*(1+2*t^2)+(1+2*((2*t)^2*
     (cosh.x)^2))*t by SQUARE_1:68
  .=4*t*((cosh.x)^2)*(1+2*t^2)+(1+2*(2^2*t^2*
     (cosh.x)^2))*t by SQUARE_1:68
  .=4*t*((cosh.x)^2)*(1+2*t^2)+(1+2*(2*2*t^2*
     (cosh.x)^2))*t by SQUARE_1:def 3
  .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*t^2*t*(cosh.x)^2
  .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*(t*t)*t*(cosh.x)^2 by SQUARE_1:def 3
  .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*(t|^1*t)*t*(cosh.x)^2 by NEWTON:10
  .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*t|^(1+1)*t*(cosh.x)^2 by NEWTON:11
  .=4*t*((cosh.x)^2)*(1+2*t^2)+sinh.x+8*t|^2*t*(cosh.x)^2
  .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*(t|^2*t)*(cosh.x)^2
  .=4*t*((cosh.x)^2)*(1+2*t^2)+t+8*(t|^(2+1))*(cosh.x)^2 by NEWTON:11
  .=4*t*((cosh.x)^2)*1+4*t*((cosh.x)^2)*2*t^2+t+8*t|^3*(cosh.x)^2
  .=4*t*(1+t^2)*1+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2 by M2
  .=4*t+4*t*t^2+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t*(t*t)+8*t*((cosh.x)^2)*
     t^2+t+8*t|^3*(cosh.x)^2 by SQUARE_1:def 3
  .=4*t+4*t*t*t+8*t*((cosh.x)^2)*t^2+sinh.x+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^1*t*t+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2 by NEWTON:10
  .=4*t+4*(t|^1*t)*t+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^(1+1)*t+8*t*((cosh.x)^2)*
     t^2+t+8*t|^3*(cosh.x)^2 by NEWTON:11
  .=4*t+4*t|^2*t+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2
  .=4*t+4*(t|^2*t)+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2
  .=4*t+4*(t|^(2+1))+8*t*((cosh.x)^2)*t^2+t+8*t|^3*(cosh.x)^2 by NEWTON:11
  .=4*t+4*t|^3+8*t*t^2*((cosh.x)^2)+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^3+8*t*(t*t)*((cosh.x)^2)+t+8*t|^3*(cosh.x)^2 by SQUARE_1:def 3
  .=4*t+4*t|^3+8*t*t*t*((cosh.x)^2)+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^3+8*t|^1*t*t*
     ((cosh.x)^2)+t+8*t|^3*(cosh.x)^2 by NEWTON:10
  .=4*t+4*t|^3+8*(t|^1*t)*t*((cosh.x)^2)+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^3+8*(t|^(1+1))*t*
     ((cosh.x)^2)+t+8*t|^3*(cosh.x)^2 by NEWTON:11
  .=4*t+4*t|^3+8*t|^2*t*((cosh.x)^2)+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^3+8*(t|^2*t)*((cosh.x)^2)+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^3+8*(t|^(2+1))*
     ((cosh.x)^2)+t+8*t|^3*(cosh.x)^2 by NEWTON:11
  .=4*t+4*t|^3+8*t|^3*(1+t^2)+t+8*t|^3*(cosh.x)^2 by M2
  .=4*t+4*t|^3+8*t|^3+8*t|^3*t^2+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^3+8*t|^3+8*t|^3*(t*t)+t+8*t|^3*(cosh.x)^2 by SQUARE_1:def 3
  .=4*t+4*t|^3+8*t|^3+8*(t|^3*t)*t+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^3+8*t|^3+8*(t|^(3+1))*t+t+8*t|^3*(cosh.x)^2 by NEWTON:11
  .=4*t+4*t|^3+8*t|^3+8*t|^4*t+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^3+8*t|^3+8*(t|^4*t)+t+8*t|^3*(cosh.x)^2
  .=4*t+4*t|^3+8*t|^3+8*(t|^(4+1))+t+8*t|^3*(cosh.x)^2 by NEWTON:11
  .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3*(1+t^2) by M2
  .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^3)*(t^2)
  .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^3)*(t*t) by SQUARE_1:def 3
  .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^3*t)*t
  .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^(3+1))*t by NEWTON:11
  .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*t|^4*t
  .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^4*t)
  .=4*t+4*t|^3+8*t|^3+8*t|^5+t+8*t|^3+8*(t|^(4+1)) by NEWTON:11
  .=4*t+t+4*t|^3+8*t|^3+8*t|^3+8*t|^5+8*t|^5
  .=5*t+20*t|^3+(8*t|^5+8*t|^5)
  .=5*sinh(x)+20*t|^3+16*t|^5 by SIN_COS2:def 2
  .=5*sinh(x)+20*(sinh x)|^3+16*t|^5 by SIN_COS2:def 2
  .=5*sinh(x)+20*(sinh x)|^3+16*(sinh x)|^5 by SIN_COS2:def 2;
   hence thesis;
  end;

theorem
  cosh(5*x)=5*cosh(x)-20*(cosh(x))|^3+16*(cosh(x))|^5
  proof
    set t = cosh.x, u = sinh.x;
    cosh(5*x) = cosh.(4*x+x) by SIN_COS2:def 4
   .=(cosh.(2*(2*x)))*t+(sinh.(4*x))*u by SIN_COS2:20
   .=(1+2*(sinh.(2*x))^2)*t+(sinh.(4*x))*u by M1
   .=(1+2*((2*u)*t)^2)*t+(sinh.(2*(2*x)))*u by SIN_COS2:23
   .=(1+2*((2*u)^2*t^2))*t+(sinh.(2*(2*x)))*u by SQUARE_1:68
   .=(1+2*((2^2*u^2)*t^2))*t+(sinh.(2*(2*x)))*u by SQUARE_1:68
   .=(1+2*((2*2*u^2)*t^2))*t+(sinh.(2*(2*x)))*u by SQUARE_1:def 3
   .=t+8*u^2*(t^2*t)+(sinh.(2*(2*x)))*u
   .=t+8*u^2*(t*t*t)+(sinh.(2*(2*x)))*u by SQUARE_1:def 3
   .=t+8*u^2*(t|^1*t*t)+(sinh.(2*(2*x)))*u by NEWTON:10
   .=t+8*u^2*(t|^(1+1)*t)+(sinh.(2*(2*x)))*u by NEWTON:11
   .=t+8*(sinh.x)^2*(t|^(2+1))+(sinh.(2*(2*x)))*u by NEWTON:11
   .=t+8*(t^2-1)*(t|^3)+(sinh.(2*(2*x)))*u by M3
   .=t+8*(t|^3*t^2)-8*t|^3+(sinh.(2*(2*x)))*u
   .=t+8*(t|^3*(t*t))-8*t|^3+(sinh.(2*(2*x)))*u by SQUARE_1:def 3
   .=t+8*(t|^3*t*t)-8*t|^3+(sinh.(2*(2*x)))*u
   .=t+8*(t|^(3+1)*t)-8*t|^3+(sinh.(2*(2*x)))*u by NEWTON:11
   .=t+8*(t|^(4+1))-8*t|^3+(sinh.(2*(2*x)))*u by NEWTON:11
   .=t+8*(t|^5)-8*t|^3+(2*(sinh.(2*x))*(cosh.(2*x)))*u by SIN_COS2:23
   .=t+8*(t|^5)-8*t|^3+(2*(2*u*t)*(cosh.(2*x)))*u by SIN_COS2:23
   .=t+8*(t|^5)-8*t|^3+4*(u*u)*t*(cosh.(2*x))
   .=t+8*(t|^5)-8*t|^3+4*u^2*t*(cosh.(2*x)) by SQUARE_1:def 3
   .=t+8*(t|^5)-8*t|^3+4*(t^2-1)*t*(cosh.(2*x)) by M3
   .=t+8*(t|^5)-8*t|^3+4*(t^2*t)*(cosh.(2*x))-4*t*(cosh.(2*x))
   .=t+8*(t|^5)-8*t|^3+4*(t*t*t)*(cosh.(2*x))-4*t*(cosh.(2*x))
      by SQUARE_1:def 3
   .=t+8*(t|^5)-8*t|^3+4*(t|^1*t*t)*(cosh.(2*x))-4*t*(cosh.(2*x))
      by NEWTON:10
   .=t+8*(t|^5)-8*t|^3+4*(t|^(1+1)*t)*(cosh.(2*x))-4*t*(cosh.(2*x))
      by NEWTON:11
   .=t+8*(t|^5)-8*t|^3+4*(t|^(2+1))
      *(cosh.(2*x))-4*t*(cosh.(2*x)) by NEWTON:11
   .=t+8*(t|^5)-8*t|^3+4*t|^3*(2*t^2-1)-4*t*(cosh.(2*x)) by SIN_COS2:23
   .=t+8*(t|^5)-8*t|^3+4*t|^3*2*t^2-4*t|^3-4*t*(cosh.(2*x))
   .=t+8*(t|^5)-8*t|^3+4*t|^3*2*(t*t)-4*t|^3-4*t*(cosh.(2*x))
      by SQUARE_1:def 3
   .=t+8*(t|^5)-8*t|^3+8*(t|^3*t*t)-4*t|^3-4*t*(cosh.(2*x))
   .=t+8*(t|^5)-8*t|^3+8*(t|^(3+1)*t)-4*t|^3-4*t*(cosh.(2*x)) by NEWTON:11
   .=t+8*(t|^5)-8*t|^3+8*(t|^(4+1))-4*t|^3-4*t*(cosh.(2*x)) by NEWTON:11
   .=t+8*(t|^5)-8*t|^3+8*(t|^5)-4*t|^3-4*t*(cosh.(2*x))
   .=t+16*(t|^5)-12*t|^3-4*t*(2*t^2-1) by SIN_COS2:23
   .=5*t+16*(t|^5)-12*t|^3-4*2*t*t^2
   .=5*t+16*(t|^5)-12*t|^3-8*t*(t*t) by SQUARE_1:def 3
   .=5*t+16*(t|^5)-12*t|^3-8*(t*t*t)
   .=5*t+16*(t|^5)-12*t|^3-8*(t|^1*t*t) by NEWTON:10
   .=5*t+16*(t|^5)-12*t|^3-8*(t|^(1+1)*t) by NEWTON:11
   .=5*t+16*(t|^5)-12*t|^3-8*(t|^(2+1)) by NEWTON:11
   .=5*t+16*(t|^5)-12*t|^3-8*t|^3
   .=5*t-20*t|^3+16*(t|^5)
   .=5*(cosh x)-20*t|^3+16*(t|^5) by SIN_COS2:def 4
   .=5*(cosh x)-20*(cosh x)|^3+16*(t|^5) by SIN_COS2:def 4
   .=5*cosh(x)-20*(cosh x)|^3+16*(cosh x)|^5 by SIN_COS2:def 4;
  hence thesis;
  end;
