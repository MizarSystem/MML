 definitions COMPTS_1, TARSKI, ORDINAL1, XBOOLE_0, STRUCT_0, FINSET_1;
 theorems METRIC_1, SUBSET_1, FUNCT_1, PCOMPS_1, PRE_TOPC, MEMBERED, TOPS_2,
      TARSKI, COMPTS_1, NAT_1, SEQ_2, ZFMISC_1, POWER, SERIES_1, ABSVALUE,
