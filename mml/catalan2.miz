theorem
  for Fr1,Fr2 st dom Fr1=dom Fr2 & for n st n in len Fr1 holds Fr1.n=Fr2
  .(len Fr1-'(1+n)) holds Sum Fr1 = Sum Fr2 by Lm9;

theorem Th49:
  for Fr1,Fr2 st dom Fr1=dom Fr2 & for n st n in len Fr1 holds Fr1
  .n=r*Fr2.n holds Sum Fr1 = r*Sum Fr2
proof
  let Fr1,Fr2 such that
A1: dom Fr1=dom Fr2 and
A2: for n st n in len Fr1 holds Fr1.n=r*Fr2.n;
A3: Fr1|(dom Fr1)=Fr1 & Fr2|(dom Fr1)=Fr2 by A1,RELAT_1:98;
  defpred P[Nat] means $1 <= len Fr1 implies Sum(Fr1|$1)=r*Sum(Fr2|
  $1);
A4: for i st P[i] holds P[i+1]
  proof
    let i such that
A5: P[i];
    assume
A6: i+1 <= len Fr1;
    then i< len Fr1 by NAT_1:13;
    then
A7: i in len Fr1 by NAT_1:45;
    then
A8: Fr1.i=r*Fr2.i by A2;
    Sum(Fr1|(i+1))=Fr1.i+Sum(Fr1|i) & Sum(Fr2|(i+1))=Fr2.i+Sum(Fr2|i) by
    A1,A7,AFINSQ_2:77;
    hence thesis by A5,A6,A8,NAT_1:13;
  end;
A9: P[0];
  for i holds P[i] from NAT_1:sch 2(A9,A4);
  hence thesis by A3;
end;

theorem
  seq1 (##) (r(#)seq2) = r(#)(seq1 (##) seq2)
proof
  set RS=r(#)seq2;
  set S=seq1 (##) seq2;
  now
    let x;
    assume x in NAT;
    then reconsider k=x as Element of NAT;
    consider Fr1 such that
A1: dom Fr1 = k+1 and
A2: for n st n in k+1 holds Fr1.n = seq1.n * RS.(k-'n) and
A3: Sum Fr1 = (seq1(##)RS).k by Def5;
    consider Fr2 such that
A4: dom Fr2 = k+1 and
A5: for n st n in k+1 holds Fr2.n = seq1.n * seq2.(k-'n) and
A6: Sum Fr2 = S.k by Def5;
    now
      let n;
      assume n in len Fr1;
      then
A7:   Fr1.n = seq1.n * RS.(k-'n) & Fr2.n = seq1.n * seq2.(k-'n) by A1,A2,
      A5;
      RS.(k-'n)= r * seq2.(k-'n) by SEQ_1:13;
      hence Fr1.n=r*Fr2.n by A7;
    end;
    then Sum Fr1 = r*Sum Fr2 by A1,A4,Th49;
    hence (seq1(##)RS).x=(r(#)S).x by A3,A6,SEQ_1:13;
  end;
  hence thesis by FUNCT_2:18;
end;

theorem
  seq1 (##) (seq2 + seq3) = (seq1 (##) seq2) + (seq1 (##) seq3)
proof
  set S=seq2 + seq3;
  set S2=seq1 (##) seq2;
  set S3=seq1 (##) seq3;
  now
    let x;
    assume x in NAT;
    then reconsider k=x as Element of NAT;
    consider Fr be XFinSequence of REAL such that
A1: dom Fr = k+1 and
A2: for n st n in k+1 holds Fr.n = seq1.n * S.(k-'n) and
A3: Sum Fr = (seq1(##)S).k by Def5;
    consider Fr1 be XFinSequence of REAL such that
A4: dom Fr1 = k+1 and
A5: for n st n in k+1 holds Fr1.n = seq1.n * seq2.(k-'n) and
A6: Sum Fr1 = S2.k by Def5;
A7: len Fr1=len Fr by A1,A4;
    consider Fr2 be XFinSequence of REAL such that
A8: dom Fr2 = k+1 and
A9: for n st n in k+1 holds Fr2.n = seq1.n * seq3.(k-'n) and
A10: Sum Fr2 = S3.k by Def5;
A11: for n be Nat st n in dom Fr holds Fr.n=addreal.(Fr1.n,Fr2.n)
    proof
      let n be Nat such that
A12:  n in dom Fr;
A13:  Fr.n = seq1.n * S.(k-'n) by A1,A2,A12;
A14:  S.(k-'n)=seq2.(k-'n) + seq3.(k-'n) by SEQ_1:11;
      Fr1.n = seq1.n * seq2.(k-'n) & Fr2.n = seq1.n * seq3.(k-'n) by A1,
      A5,A9,A12;
      then Fr.n=Fr1.n+Fr2.n by A13,A14;
      hence thesis by BINOP_2:def 9;
    end;
    len Fr1=len Fr2 by A4,A8;
  then addreal "**" Fr1^Fr2 = addreal "**" Fr by A11,A7,AFINSQ_2:58;
   then Sum Fr=addreal "**" Fr1^Fr2 by AFINSQ_2:60;
  then Sum Fr=Sum (Fr1^Fr2) by  AFINSQ_2:60;
then Sum Fr=Sum (Fr1)+Sum(Fr2) by  AFINSQ_2:67;
    hence (seq1(##)S).x=(S2+S3).x by A3,A6,A10,SEQ_1:11;
  end;
  hence thesis by FUNCT_2:18;
end;

theorem Th52:
  (seq1 (##) seq2).0=seq1.0*seq2.0
proof
  set S=seq1.0 * seq2.0;
  consider Fr such that
A1: dom Fr = 0+1 and
A2: for n st n in 0+1 holds Fr.n = seq1.n * seq2.(0-'n) and
A3: Sum Fr = (seq1 (##) seq2).0 by Def5;
A4: 0-'0=0 & len Fr=1 by A1,XREAL_1:234;
  0 in 1 by NAT_1:45;
  then Fr.0=seq1.0 * seq2.(0-'0) by A2;
  then Fr=<%S%> by A4,AFINSQ_1:38;
  hence thesis by A3,AFINSQ_2:65;
end;

theorem Th53:
  for seq1, seq2, n ex Fr st Partial_Sums(seq1 (##) seq2).n = Sum
Fr & dom Fr=n+1 & for i st i in n+1 holds Fr.i=seq1.i * Partial_Sums(seq2).(n-'
  i)
proof
  let seq1,seq2,n;
  set S=seq1 (##) seq2;
  set P=Partial_Sums(seq2);
  defpred P[Nat] means ex Fr st Partial_Sums(S).$1 = Sum Fr & dom Fr=$1+1 &
  for i st i in $1+1 holds Fr.i=seq1.i * P.($1-'i);
A1: for n st P[n] holds P[n+1]
  proof
    set A=addreal;
    let n;
    set n1=n+1;
    defpred Q[set,set] means for i st i=$1 holds $2=seq1.i * P.(n1-'i);
A2: n1-'n1=0 & P.0=seq2.0 by SERIES_1:def 1,XREAL_1:234;
A3: for i st i in n1+1 ex x be Element of REAL st Q[i,x]
    proof
      let i such that
      i in n1+1;
      take seq1.i * Partial_Sums(seq2).(n1-'i);
      thus thesis;
    end;
    consider Fr2 such that
A4: dom Fr2 = n1+1 and
A5: for i st i in n1+1 holds Q[i,Fr2.i] from STIRL2_1:sch 5(A3);
    assume P[n];
    then consider Fr such that
A6: Partial_Sums(S).n = Sum Fr and
A7: dom Fr=n1 and
A8: for i st i in n+1 holds Fr.i=seq1.i * P.(n-'i);
    consider Fr1 such that
A9: dom Fr1 = n1+1 and
A10: for i st i in n1+1 holds Fr1.i = seq1.i * seq2.(n1-'i) and
A11: Sum Fr1 = S.n1 by Def5;
A12: Fr1|(n1+1)=Fr1 by A9,RELAT_1:98;
A13: for i be Nat
    st i in dom (Fr2|n1) holds (Fr2|n1).i=addreal.(Fr.i,(Fr1|n1).i)
    proof
      let i be Nat such that
A14:  i in dom (Fr2|n1);
A15:  i in dom Fr2 /\ n1 by A14,RELAT_1:90;
      then i in dom(Fr1|n1) by A9,A4,RELAT_1:90;
      then
A16:  Fr1.i=(Fr1|n1).i by FUNCT_1:70;
A17:  i in n1 by A15,XBOOLE_0:def 4;
      then
A18:  i < n1 by NAT_1:45;
      then i <=n by NAT_1:13;
      then
A19:  n-'i=n-i by XREAL_1:235;
      i in n1+1 & i in NAT by A4,A15,XBOOLE_0:def 4;
      then
A20:  Fr1.i = seq1.i * seq2.(n1-'i) & Fr2.i=seq1.i * P.(n1-'i) by A10,A5;
A21:  Fr2.i=(Fr2|n1).i by A14,FUNCT_1:70;
      n1-'i=n1-i by A18,XREAL_1:235;
      then (n-'i)+1=n1-'i by A19;
      then
A22:  P.(n1-'i)=P.(n-'i)+seq2.(n1-'i) by SERIES_1:def 1;
      Fr.i=seq1.i * P.(n-'i) by A8,A17;
      then Fr2.i=Fr.i+Fr1.i by A20,A22;
      hence thesis by A16,A21,BINOP_2:def 9;
    end;
    n1<=n1+1 by NAT_1:11;
    then
A23: n1 c= n1+1 by NAT_1:40;
    then
A24: len (Fr1|n1)=len Fr by A7,A9,RELAT_1:91;
    n1 < n1+1 by NAT_1:13;
    then
A25: n1 in n1+1 by NAT_1:45;
    then
A26: Fr1.n1=seq1.n1 * seq2.(n1-'n1) & Sum(Fr1|(n1+1)) = Fr1.n1 + Sum(Fr1|
    n1) by A9,A10,AFINSQ_2:77;
    len (Fr2|n1)=len Fr by A7,A4,A23,RELAT_1:91;
    then A"**"(Fr2|n1)=A"**"(Fr^(Fr1|n1)) by A13,A24,AFINSQ_2:58
 .= Sum(Fr^(Fr1|n1)) by AFINSQ_2:60
.= Sum(Fr)+Sum(Fr1|n1) by AFINSQ_2:67;
    then
A27: Sum(Fr2|n1)=Sum Fr+Sum(Fr1|n1) by AFINSQ_2:60;
    take Fr2;
    Fr2.n1=seq1.n1 * P.(n1-'n1) & Sum(Fr2|(n1+1)) = Fr2.n1 + Sum(Fr2|n1)
    by A4,A5,A25,AFINSQ_2:77;
    then Sum Fr2=Partial_Sums(S).n + S.n1 & n in NAT & n1 in NAT
      by A6,A11,A4,A27,A2,A26,A12,
    RELAT_1:98,ORDINAL1:def 13;
    hence thesis by A4,A5,SERIES_1:def 1;
  end;
A28: P[0]
  proof
    set Fr=1--> (seq1.0*seq2.0);
    reconsider Fr as XFinSequence of REAL;
    take Fr;
A29: dom Fr=1 by FUNCOP_1:19;
    then
A30: dom (Fr|(0 qua Ordinal))=0 & Fr|1=Fr by RELAT_1:98;
A31: 0 in 1 by NAT_1:45;
    then
A32: Fr.0=seq1.0*seq2.0 by FUNCOP_1:13;
    Sum(Fr|(0 qua Ordinal))+Fr.0=Sum (Fr|(0+1)) by A29,A31,AFINSQ_2:77;
    then Sum Fr=S.0 by Th52,A32,A30;
    hence Partial_Sums(S).0 = Sum Fr & dom Fr=0+1 by FUNCOP_1:19,SERIES_1:
    def 1;
    let i such that
A33: i in 0+1;
    i<1 by A33,NAT_1:45;
    then
A34: i=0 by NAT_1:14;
    then (0-'i)=0 by XREAL_1:234;
    then P.(0-'i)= seq2.0 by SERIES_1:def 1;
    hence thesis by A33,A34,FUNCOP_1:13;
  end;
  for i holds P[i] from NAT_1:sch 2(A28,A1);
  hence thesis;
end;

theorem Th54:
  for seq1, seq2, n st seq2 is summable ex Fr st Partial_Sums(seq1
(##) seq2).n= Sum seq2 * Partial_Sums(seq1).n- Sum Fr & dom Fr=n+1 & for i st i
  in n+1 holds Fr.i=seq1.i * Sum (seq2^\(n-'i+1))
proof
  let seq1, seq2, n such that
A1: seq2 is summable;
  defpred Q[set,set] means for i st i=$1 holds $2=seq1.i * Sum (seq2^\(n-'i+1)
  );
  set P2=Partial_Sums(seq2);
  set P1=Partial_Sums(seq1);
  set S=seq1 (##) seq2;
A2: for i st i in n+1 ex x be Element of REAL st Q[i,x]
  proof
    let i such that
    i in n+1;
    take seq1.i * Sum (seq2^\(n-'i+1));
    thus thesis;
  end;
  consider Fr such that
A3: dom Fr = n+1 and
A4: for i st i in n+1 holds Q[i,Fr.i] from STIRL2_1:sch 5(A2);
  consider Fr1 such that
A5: Partial_Sums(S).n = Sum Fr1 and
A6: dom Fr1=n+1 and
A7: for i st i in n+1 holds Fr1.i=seq1.i * P2.(n-'i) by Th53;
A8: 0 in n+1 by NAT_1:45;
  then
A9: Fr1.0=seq1.0 * P2.(n-'0) &
    Sum (Fr1|(0+1))=Fr1.0 + Sum (Fr1|(0 qua Ordinal)) by A6,A7,AFINSQ_2:77;
  defpred P[Nat] means $1+1 <= n+1 implies Sum (Fr1|($1+1)) + Sum (
  Fr|($1+1))= Sum seq2 * P1.$1;
A10: for k st P[k] holds P[k+1]
  proof
    let k such that
A11: P[k];
    reconsider k1=k+1 as Element of NAT;
    assume
 BB:    k+1+1<=n+1;
    then k1<n+1 by NAT_1:13;
    then
A13: k1 in n+1 by NAT_1:45;
    then
A14: Fr.k1=seq1.k1*Sum(seq2^\(n-'k1+1)) & Sum(Fr1|(k1+1))=Fr1.k1+Sum (Fr1|
    k1) by A4,A6,AFINSQ_2:77;
B15:Sum (Fr1|k1) + Sum (Fr|k1)= Sum seq2 * P1.k by BB,NAT_1:13,A11;
B:k in NAT by ORDINAL1:def 13;
    Sum(Fr|(k1+1))=Fr.k1 + Sum (Fr|k1) & Fr1.k1=seq1.k1 * P2.(n-'k1) by
    A3,A7,A13,AFINSQ_2:77;
    then Sum(Fr|(k1+1)) + Sum(Fr1|(k1+1))
       =seq1.k1 * ( Sum(seq2^\(n-'k1+1)) + P2.(n-'k1) )
      + Sum seq2 * P1.k by B15,A14
      .=seq1.k1*Sum seq2 +Sum seq2 * P1.k by A1,SERIES_1:18
      .=Sum seq2*(P1.k+seq1.k1)
      .=P1.k1*Sum seq2 by SERIES_1:def 1,B;
    hence thesis;
  end;
  Sum (Fr|(0+1))=Fr.0 + Sum (Fr|(0 qua Ordinal)) &
  Fr.0=seq1.0 * Sum (seq2^\(n-'0+1))
  by A3,A4,A8,AFINSQ_2:77;
  then Sum (Fr|(0+1))+Sum (Fr1|(0+1))=seq1.0*(Sum(seq2^\(n-'0+1))+P2.(n-'0))
  by A9
    .=seq1.0*Sum seq2 by A1,SERIES_1:18;
  then
A16: P[0] by SERIES_1:def 1;
A17: for k holds P[k] from NAT_1:sch 2(A16,A10);
  take Fr;
A18: Fr1|(n+1)=Fr1 by A6,RELAT_1:98;
  Fr|(n+1)=Fr by A3,RELAT_1:98;
  then Sum Fr1 + Sum Fr= Sum seq2 * P1.n by A17,A18;
  hence thesis by A3,A4,A5;
end;

theorem Th55:
  for Fr ex absFr be XFinSequence of REAL st dom absFr=dom Fr &
  abs(Sum Fr) <= Sum absFr & for i st i in dom absFr holds absFr.i=abs(Fr.i)
proof
  let Fr;
  defpred P[set,set] means $2=abs(Fr.$1);
A1: Fr|(dom Fr)=Fr by RELAT_1:98;
A2: for i st i in len Fr ex x be Element of REAL st P[i,x];
  consider absFr be XFinSequence of REAL such that
A3: dom absFr = len Fr and
A4: for i st i in len Fr holds P[i,absFr.i] from STIRL2_1:sch 5(A2);
  defpred Q[Nat] means $1 <= len Fr implies abs Sum (Fr|$1) <= Sum
  (absFr|$1);
A5: for i st Q[i] holds Q[i+1]
  proof
    let i such that
A6: Q[i];
    set i1=i+1;
    assume
A7: i1<=len Fr;
    then i < len Fr by NAT_1:13;
    then
A8: i in dom Fr by NAT_1:45;
    then Sum(Fr|i1)=Fr.i+Sum(Fr|i) & absFr.i=abs(Fr.i) by A4,AFINSQ_2:77;
    then
A9: abs(Sum(Fr|i1))<=absFr.i +abs(Sum (Fr|i)) by COMPLEX1:142;
    Sum(absFr|i1)=absFr.i+Sum (absFr|i) by A3,A8,AFINSQ_2:77;
    then absFr.i + abs(Sum (Fr|i)) <= Sum(absFr|i1) by A6,A7,NAT_1:13 ,
    XREAL_1:9;
    hence thesis by A9,XXREAL_0:2;
  end;
  take absFr;
A10: Q[0] by COMPLEX1:130;
  for i holds Q[i] from NAT_1:sch 2(A10,A5);
  then abs Sum (Fr|(len Fr)) <= Sum (absFr|(len Fr));
  hence thesis by A3,A4,A1,RELAT_1:98;
end;

theorem Th56:
  for seq1 st seq1 is summable ex r st 0 < r & for k holds abs(Sum
  (seq1^\k)) < r
proof
  let seq1 such that
A1: seq1 is summable;
  defpred P[Nat] means ex r st r>=0 & for i st i <= $1 holds abs(Sum (seq1^\i)
  )<=r;
A2: for k st P[k] holds P[k+1]
  proof
    let k;
    assume P[k];
    then consider r such that
A3: r>=0 and
A4: for i st i <= k holds abs(Sum (seq1^\i) )<=r;
    take M=max(r,abs(Sum (seq1^\(k+1))));
    thus M >=0 by A3,XXREAL_0:25;
    let i such that
A5: i <= k+1;
    now
      per cases by A5,NAT_1:8;
      suppose
        i=k+1;
        hence thesis by XXREAL_0:25;
      end;
      suppose
A6:     i<=k;
A7:     r<=M by XXREAL_0:25;
        abs(Sum (seq1^\i) )<=r by A4,A6;
        hence thesis by A7,XXREAL_0:2;
      end;
    end;
    hence thesis;
  end;
  set P=Partial_Sums(seq1);
A8: lim P= Sum seq1 by SERIES_1:def 3;
  P is convergent by A1,SERIES_1:def 2;
  then consider n be Element of NAT such that
A9: for m be Element of NAT
st n<=m holds abs(P.m-Sum seq1)<1 by A8,SEQ_2:def 7;
A10: P[0]
  proof
    take abs(Sum(seq1));
    thus abs(Sum(seq1))>=0 by COMPLEX1:132;
    let i;
    assume i<=0;
    then i=0;
    hence thesis by NAT_1:48;
  end;
  for k holds P[k] from NAT_1:sch 2(A10,A2);
  then consider r such that
A11: r>=0 and
A12: for i st i <= n holds abs(Sum (seq1^\i) )<=r;
  take r1=r+1;
  thus r1>0 by A11;
  let k;
  now
    per cases;
    suppose
A13:  k <= n;
A14:  0 qua Nat+r< r1 by XREAL_1:10;
      abs(Sum (seq1^\k) )<=r by A12,A13;
      hence thesis by A14,XXREAL_0:2;
    end;
    suppose
A15:  k > n;
      then reconsider k1=k-1 as Element of NAT by NAT_1:20;
      k1+1>n by A15;
      then k1>=n by NAT_1:13;
      then
A16:  abs(P.k1-Sum seq1)<1 by A9;
      Sum seq1 = P.k1+Sum(seq1^\(k1+1)) by A1,SERIES_1:18;
      then abs(-Sum(seq1^\(k1+1)))<1 by A16;
      then
A17:  abs( Sum(seq1^\(k1+1)) )<1 by COMPLEX1:138;
      1+0<=r1 by A11,XREAL_1:8;
      hence thesis by A17,XXREAL_0:2;
    end;
  end;
  hence thesis;
end;

theorem Th57:
  for seq1, n, m st n <= m & for i holds seq1.i>=0 holds (
  Partial_Sums seq1).n <= (Partial_Sums seq1).m
proof
  let seq1, n, m such that
A1: n <= m and
A2: for i holds seq1.i>=0;
  set S=Partial_Sums seq1;
  defpred P[Nat] means S.n <= S.(n+$1);
A3: for i st P[i] holds P[i+1]
  proof
    let i such that
A4: P[i];
    set ni=n+i;
    S.(ni+1)=S.ni+seq1.(ni+1) & seq1.(ni+1)>=0 by A2,SERIES_1:def 1;
    then S.(ni+1) >= S.ni + (0 qua Nat) by XREAL_1:8;
    hence thesis by A4,XXREAL_0:2;
  end;
A5: P[0];
A6: for i holds P[i] from NAT_1:sch 2(A5,A3);
  reconsider m9=m, n9=n as Nat;
A7: n9+(m9-n9)=m9;
  m9-n9 is Element of NAT by A1,NAT_1:21;
  hence thesis by A6,A7;
end;
 
:: CAUCHY PRODUCT THEOREM

theorem Th58:
  for seq1, seq2 st seq1 is absolutely_summable & seq2 is summable
  holds seq1 (##) seq2 is summable & Sum(seq1 (##) seq2) = Sum seq1 * Sum seq2
proof
  let seq1, seq2 such that
A1: seq1 is absolutely_summable and
A2: seq2 is summable;
  set S2=Sum seq2;
  set S1=Sum seq1;
  set PA=Partial_Sums(abs(seq1));
  set P2=Partial_Sums(seq2);
  set P1=Partial_Sums(seq1);
  set S=seq1 (##) seq2;
  set P=Partial_Sums(S);
A3: for e st 0<e ex n be Element of NAT st
   for m be Element of NAT st n<=m holds abs(P.m-S1*S2)<e
  proof
    seq1 is summable by A1,SERIES_1:40;
    then
A4: P1 is convergent by SERIES_1:def 2;
    let e such that
A5: 0<e;
    set e1=e/(3*(abs(S2)+1));
    (abs(S2)+1)> 0+0 by COMPLEX1:132,XREAL_1:10;
    then
A6: 3*(abs(S2)+1)>3*0 by XREAL_1:70;
    then lim P1= S1 & e1>0 by A5,SERIES_1:def 3,XREAL_1:141;
    then consider n0 be Element of NAT such that
A7: for n be Element of NAT st n0 <= n holds abs(P1.n-S1) < e1
      by A4,SEQ_2:def 7;
    set e3=e/(3*(Sum abs(seq1)+1));
A8: max(1,n0) = 1 or max(1,n0) = n0 by XXREAL_0:16;
    abs(S2)+1>0+0 by COMPLEX1:132,XREAL_1:10;
    then
A9: e1*(abs(S2)+1)=e/3 by XCMPLX_1:93;
A10: P2 is convergent & lim P2= S2 by A2,SERIES_1:def 2,def 3;
    consider r such that
A11: 0 < r and
A12: for k holds abs(Sum(seq2^\k)) < r by A2,Th56;
    set e2=e/(3*r);
A13: abs(S2)+1 > abs(S2)+(0 qua Nat) by XREAL_1:10;
A14: now
      let n;n in NAT by ORDINAL1:def 13;then
      abs(seq1.n)=abs(seq1).n by SEQ_1:16;
      hence abs(seq1).n>=0 by COMPLEX1:132;
    end;
then B14:for n be Element of NAT holds abs(seq1).n>=0;
A15: abs seq1 is summable by A1,SERIES_1:def 5;
    then Sum abs(seq1)>=0 by SERIES_1:21,B14;
    then
A16: (Sum abs(seq1)+1)*e3=e/3 by XCMPLX_1:93;
A17: Sum abs seq1>=0 by A15,SERIES_1:21,B14;
    then 3*(Sum abs(seq1)+1)>0*3 by XREAL_1:70;
    then e3>0 by A5,XREAL_1:141;
    then consider n2 be Element of NAT such that
A18: for n be Element of NAT st n2<=n holds abs(P2.n-S2)<e3 by A10,SEQ_2:def 7;
    3*r>0*3 by A11,XREAL_1:70;
    then e2>0 by A5,XREAL_1:141;
    then consider n1 be Element of NAT such that
A19: for n be Element of NAT
     st n1 <= n holds abs(PA.n-PA.n1) < e2 by A15,SERIES_1:
    25;
    max(n1+1,n2) = n1+1 or max(n1+1,n2) = n2 by XXREAL_0:16;
    then reconsider M=max(max(1,n0),max(n1+1,n2)) as Element of NAT by A8,
    XXREAL_0:16;
A20: max(n1+1,n2)<=M by XXREAL_0:25;
    take 2M=M*2;
    let m be Element of NAT such that
A21: 2M <= m;
A22: max(1,n0)<=M by XXREAL_0:25;
    then 0<M by XXREAL_0:25;
    then reconsider M1=M-1 as Element of NAT by NAT_1:20;
A23: M=M1+1;
A24: n1+1 <= max(n1+1,n2) by XXREAL_0:25;
    then M1+1>=n1+1 by A20,XXREAL_0:2;
    then M1>= n1 by XREAL_1:10;
    then PA.M1>=PA.n1 by A14,Th57;
    then PA.m-PA.M1<=PA.m-PA.n1 by XREAL_1:12;
    then
A25: r*(PA.m-PA.M1)<=r*(PA.m-PA.n1) by A11,XREAL_1:66;
    consider Fr such that
A26: P.m= S2 * P1.m- Sum Fr and
A27: dom Fr=m+1 and
A28: for i st i in m+1 holds Fr.i=seq1.i*Sum(seq2^\(m-'i+1))by A2,Th54;
    consider absFr be XFinSequence of REAL such that
A29: dom absFr=dom Fr and
A30: abs(Sum Fr) <= Sum absFr and
A31: for i st i in dom absFr holds absFr.i=abs(Fr.i) by Th55;
A32: M<=M+M by NAT_1:11;
    then
A33: M<=m by A21,XXREAL_0:2;
    then M < len absFr by A27,A29,NAT_1:13;
    then
A34: len (absFr|M)=M by AFINSQ_1:14;
    n1+1 <= M by A24,A20,XXREAL_0:2;
    then n1+1<=m by A33,XXREAL_0:2;
    then
A35: n1 <= m by NAT_1:13;
    then PA.m>=PA.n1 by A14,Th57;
    then PA.m-PA.n1>=PA.n1-PA.n1 by XREAL_1:11;
    then
A36: abs(PA.m-PA.n1)=(PA.m-PA.n1) by ABSVALUE:def 1;
    consider Fr1 such that
A37: absFr=absFr|M^Fr1 by Th5;
A38: m+1=len (absFr|M)+ len Fr1 by A27,A29,A37,AFINSQ_1:def 4;
    then
A39: Fr1|((m-M)+1)=Fr1 by A34,RELAT_1:98;
A40: n2 <= max(n1+1,n2) by XXREAL_0:25;
    then n2 <= M by A20,XXREAL_0:2;
    then n2 <=2M by A32,XXREAL_0:2;
    then n2 <= m & m in NAT by A21,XXREAL_0:2;
    then
A41: abs(P2.m-S2)<e3 by A18;
    defpred S[Nat] means M+$1+1<=m+1 implies Sum(Fr1|($1+1))<=r*(PA
    .(M+$1)-PA.M1);
A42: for k st S[k] holds S[k+1]
    proof
      let k such that
A43:  S[k];
      set k1=k+1;
      set Mk1=M+k1;
A44:  abs(seq1.Mk1)=abs(seq1).Mk1 by SEQ_1:16;
      assume
A45:  Mk1+1<=m+1;
      then
A46:  Mk1 <m+1 by NAT_1:13;
      then
A47:  Mk1 in m+1 by NAT_1:45;
      then Fr.Mk1=seq1.Mk1*Sum(seq2^\(m-'Mk1+1)) by A28;
      then
A48:  abs(Fr.Mk1)=abs(seq1.Mk1)*abs(Sum(seq2^\(m-'Mk1+1))) by
      COMPLEX1:151;
      Mk1<m+1 by A45,NAT_1:13;
      then k1 < len Fr1 by A38,A34,XREAL_1:9;
      then k1 in len Fr1 by NAT_1:45;
      then
A49:  Sum(Fr1|(k1+1))=Fr1.k1+Sum(Fr1|k1) by AFINSQ_2:77;
      m+1=len absFr by A27,A29;
      then absFr.Mk1=Fr1.(Mk1-M) by A37,A34,A46,AFINSQ_1:22,NAT_1:11;
      then
A50:  Fr1.k1=abs(Fr.Mk1) by A27,A29,A31,A47;
      abs(seq1.Mk1)>=0 & abs(Sum(seq2^\(m-'Mk1+1)))<r by A12,COMPLEX1:132;
      then Fr1.k1<=r*abs(seq1.Mk1) by A50,A48,XREAL_1:66;
      then Sum(Fr1|(k1+1))<=r*abs(seq1).Mk1+r*(PA.(M+k)-PA.M1)by A43,A45,
      A49,A44,NAT_1:13,XREAL_1:9;
      then Sum(Fr1|(k1+1))<=r*((abs(seq1).Mk1+PA.(M+k))-PA.M1);
      then Sum(Fr1|(k1+1))<=r*(PA.(M+k+1)-PA.M1) by SERIES_1:def 1;
      hence thesis;
    end;
    abs(PA.m - PA.n1) < e2 by A19,A35;
    then r*(PA.m-PA.n1)<=r*e2 by A11,A36,XREAL_1:66;
    then
A51: r*(PA.m-PA.M1)<=r*e2 by A25,XXREAL_0:2;
A52: m=M+(m-M) & m-M=m-'M by A21,A32,XREAL_1:235,XXREAL_0:2;
A53: S[0]
    proof
      assume
A54:  M+0+1<=m+1;
      then
A55:  M <m+1 by NAT_1:13;
      then
A56:  M in m+1 by NAT_1:45;
      then
A57:  Fr.M=seq1.M*Sum(seq2^\(m-'M+1)) by A28;
      M+1-M<=m+1-M by A54,XREAL_1:11;
      then 1 c= len Fr1 by A38,A34,NAT_1:40;
      then
A58:  dom (Fr1|1)=1 by RELAT_1:91;
      m+1=len absFr by A27,A29;
      then absFr.M=Fr1.(M-M) by A37,A34,A55,AFINSQ_1:22;
      then Fr1.0=abs(Fr.M) by A27,A29,A31,A56;
      then
A59:  Fr1.0=abs(seq1.M)*abs(Sum(seq2^\(m-'M+1))) by A57, COMPLEX1:
      151;
A60:  abs(seq1.M)>=0 & r>abs(Sum(seq2^\(m-'M+1)) ) by A12,COMPLEX1:132;
      0 in 1 by NAT_1:45;
      then
A61:  (Fr1|1).0=Fr1.0 by A58,FUNCT_1:70;
      PA.M1+abs(seq1).(M1+1)=PA.(M1+1) by SERIES_1:def 1;
      then
A62:  PA.M-PA.M1=abs(seq1.M) by SEQ_1:16;
      Sum (Fr1|1)=(Fr1|1).0 by A58,Lm7;
      hence thesis by A61,A59,A62,A60,XREAL_1:66;
    end;
    for k holds S[k] from NAT_1:sch 2(A53,A42);
    then Sum(Fr1)<=r*(PA.m-PA.M1) by A39,A52;
    then Sum(Fr1)<=r*e2 by A51,XXREAL_0:2;
    then
A63: Sum(Fr1)<=e/3 by A11,XCMPLX_1:93;
    abs(seq1).0>=0 by A14;
    then
A64: abs(seq1).0*abs(P2.m-S2)<=e3*abs(seq1).0 by A41,XREAL_1:66;
A65: 0 in m+1 by NAT_1:45;
    then
A66: Fr.0=seq1.0*Sum(seq2^\(m-'0+1)) by A28;
    PA.M1 <= Sum abs(seq1) by B14, A15,RSSPACE2:4;
    then
A67: e3* PA.M1 <= e3* Sum abs(seq1) by A5,A17,XREAL_1:66;
    S2=P2.(m-'0)+Sum(seq2^\(m-'0+1)) & m-'0=m by A2,NAT_D:40,SERIES_1:18;
    then
A68: Sum(seq2^\(m-'0+1))=S2-P2.m;
    n0<= max(1,n0) by XXREAL_0:25;
    then n0 <= M by A22,XXREAL_0:2;
    then n0 <= m & m in NAT by A33,XXREAL_0:2;
    then
A69: abs(P1.m-S1)<e1 by A7;
    abs(S2*(P1.m-S1))=abs(S2)*abs(P1.m-S1) & abs(S2)>=0 by COMPLEX1:132,
    151;
    then
A70: abs(S2*(P1.m-S1))<=abs(S2)*e1 by A69,XREAL_1:66;
A71: Sum absFr=Sum(absFr|M)+Sum Fr1 by A37,AFINSQ_2:67;
    defpred Q[Nat] means $1+1 <= M implies Sum(absFr|($1+1))<= e3 *
    PA.$1;
A72: n2 <= M by A40,A20,XXREAL_0:2;
A73: for k st Q[k] holds Q[k+1]
    proof
      let k such that
A74:  Q[k];
      reconsider k1=k+1 as Element of NAT;
A75:  abs(seq1.k1)=abs(seq1).k1 by SEQ_1:16;
A76:  m-M >= 2M-M by A21,XREAL_1:11;
      assume
A77:  k+1+1<=M;
      then
A78:  k1<M by NAT_1:13;
      then m-k1 >= m-M by XREAL_1:12;
      then m-k1 >= M by A76,XXREAL_0:2;
      then
A79:  m-k1 >= n2 by A72,XXREAL_0:2;
      e3*abs(seq1.k1)+Sum(absFr|k1) <= e3*abs(seq1.k1) + e3 * PA.k by A74
      ,A77, NAT_1:13,XREAL_1:8;
      then e3*abs(seq1.k1)+Sum(absFr|k1)<=e3*(abs(seq1).k1+PA.k) & k in NAT
        by A75,ORDINAL1:def 13;
      then
A80:  e3*abs(seq1.k1)+Sum(absFr|k1)<= e3* PA.k1 by SERIES_1:def 1;
      k1<m by A33,A78,XXREAL_0:2;
      then k1 < m+1 by NAT_1:13;
      then
A81:  k1 in m+1 by NAT_1:45;
      then
A82:  Sum(absFr|(k1+1))=absFr.k1 + Sum(absFr|k1) by A27,A29,AFINSQ_2:77;
      m-k1=m-'k1 by A33,A78,XREAL_1:235,XXREAL_0:2;
      then abs(P2.(m-'k1)-S2)<e3 by A18,A79;
      then
A83:  abs(S2-P2.(m-'k1))<e3 by COMPLEX1:146;
A84:  S2=P2.(m-'k1)+Sum(seq2^\(m-'k1+1)) by A2,SERIES_1:18;
      abs(seq1.k1)>=0 by COMPLEX1:132;
      then abs(S2-P2.(m-'k1))*abs(seq1.k1) <= e3*abs(seq1.k1) by A83,
      XREAL_1:66;
      then
A85:  abs
(seq1.k1 * Sum(seq2^\(m-'k1+1))) <= e3 * abs(seq1.k1) by A84,COMPLEX1:151;
      Fr.k1=seq1.k1*Sum(seq2^\(m-'k1+1)) & abs(Fr.k1)=absFr.k1 by A27,A28
      ,A29,A31,A81;
      then Sum(absFr|(k1+1))<=e3*abs(seq1.k1)+Sum(absFr|k1) by A82,A85,
      XREAL_1:8;
      hence thesis by A80,XXREAL_0:2;
    end;
    Sum(absFr|(0+1))=absFr.0 + Sum(absFr|(0 qua Ordinal)) & absFr.0=abs(Fr.0)
    by A27,A29,A31,A65,AFINSQ_2:77;
    then Sum(absFr|(0+1))=abs(seq1.0)*abs(Sum(seq2^\(m-'0+1))) by A66
    ,COMPLEX1:151
      .=abs(seq1).0*abs(Sum(seq2^\(m-'0+1))) by SEQ_1:16
      .=abs(seq1).0*abs(P2.m-S2) by A68,COMPLEX1:146;
    then
A87: Q[0] by A64,SERIES_1:def 1;
    for k holds Q[k] from NAT_1:sch 2(A87,A73);
    then
A88: Sum(absFr|M)<= e3 * PA.M1 by A23;
    Sum abs(seq1)+1>Sum abs(seq1)+(0 qua Nat) by XREAL_1:10;
    then e3*(Sum abs(seq1)+1) >= e3*Sum abs(seq1) by A5,A17,XREAL_1:66;
    then e3* PA.M1 <=e/3 by A67,A16,XXREAL_0:2;
    then Sum(absFr|M)<=e/3 by A88,XXREAL_0:2;
    then Sum(absFr|M)+Sum Fr1 <=e/3+e/3 by A63,XREAL_1:9;
    then
A89: abs(Sum Fr)<= e/3+e/3 by A30,A71,XXREAL_0:2;
    P.m-S1*S2=S2*(P1.m-S1)-Sum Fr by A26;
