A90: abs(P.m-S1*S2)<=abs(S2*(P1.m-S1))+abs(Sum Fr) by COMPLEX1:143;
    e1>0 by A5,A6,XREAL_1:141;
    then e1*(abs(S2)+1) > e1*abs(S2) by A13,XREAL_1:70;
    then abs(S2*(P1.m-S1))<e/3 by A9,A70,XXREAL_0:2;
    then abs(S2*(P1.m-S1))+abs(Sum Fr) <e/3+(e/3+e/3) by
    A89,XREAL_1:10;
    hence thesis by A90,XXREAL_0:2;
  end;
  then
A91: P is convergent by SEQ_2:def 6;
  hence S is summable by SERIES_1:def 2;
  lim P = S1*S2 by A3,A91,SEQ_2:def 7;
  hence thesis by SERIES_1:def 3;
end;

