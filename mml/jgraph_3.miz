        end;
A42:    now
          assume (q`1)^2<=0;
          then (q`1)^2=0 by XREAL_1:65;
          then
A43:      q`1=0 by XCMPLX_1:6;
          then q`2=0 by A31;
          hence contradiction by A31,A43,EUCLID:57,58;
        end;
        then (q`2)^2/(q`1)^2<=(q`1)^2/(q`1)^2 by A36,XREAL_1:74;
        then (q`2/q`1)^2<=(q`1)^2/(q`1)^2 by XCMPLX_1:77;
        then (q`2/q`1)^2 <=1 by A42,XCMPLX_1:60;
        then
A44:    1+(q`2/q`1)^2<=1+1 by XREAL_1:9;
        then
A45:    (q`1)^2*(1+(q`2/q`1)^2)<=(q`1)^2*2 by A26,XREAL_1:66;
A46:    (q`2)^2*(1+(q`2/q`1)^2)<=(q`2)^2*2 by A27,A44,XREAL_1:66;
A47:    1+(q`2/q`1)^2>0 by Lm1;
        (qz`1)^2=(q`1)^2*(sqrt(1+(q`2/q`1)^2))^2 by A35;
        then
A48:    (qz`1)^2<=(q`1)^2*2 by A45,A47,SQUARE_1:def 4;
A49:    (qz`1)^2>=0 by XREAL_1:65;
A50:    (qz`2)^2=(q`2)^2*(sqrt(1+(q`2/q`1)^2))^2 by A35;
A51:    (qz`2)^2>=0 by XREAL_1:65;
        (qz`2)^2<=(q`2)^2*2 by A46,A47,A50,SQUARE_1:def 4;
        then
A52:    (qz`1)^2+(qz`2)^2<=(q`1)^2*2+(q`2)^2*2 by A48,XREAL_1:9;
        0+0<= (qz`1)^2+(qz`2)^2 by A49,A51;
        then sqrt((qz`1)^2+(qz`2)^2) <= sqrt((q`1)^2*2+(q`2)^2*2)
        by A52,SQUARE_1:94;
        then sqrt((((0.REAL 2) - qz)`1)^2+(((0.REAL 2) - qz)`2)^2)<r
        by A30,A33,A34,XXREAL_0:2;
        then |.(0.REAL 2) - qz.|<r by JGRAPH_1:47;
        then dist(u0,pz)<r by JGRAPH_1:45;
        hence thesis by METRIC_1:12;
      end;
      suppose
A53:    q<>0.REAL 2 & not (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
        then
A54:    (Sq_Circ").q=|[q`1*sqrt(1+(q`1/q`2)^2),q`2*sqrt(1+(q`1/q`2 ) ^2)]|
        by Th38;
A55:    ((0.REAL 2) - qz)`1=(0.REAL 2)`1-qz`1 by TOPREAL3:8
          .= -qz`1 by JGRAPH_2:11;
A56:    ((0.REAL 2) - qz)`2=(0.REAL 2)`2-qz`2 by TOPREAL3:8
          .= -qz`2 by JGRAPH_2:11;
A57:    qz`1=q`1*sqrt(1+(q`1/q`2)^2) & qz`2=q`2*sqrt(1+(q`1/q`2)^2)
        by A24,A54,EUCLID:56;
A59:    now per cases by A53,JGRAPH_2:23;
          case
A60:        q`1<=q`2 & -q`2<=q`1;
            now per cases;
              case 0<=q`1;
                hence (q`1)^2<=(q`2)^2 by A60,SQUARE_1:77;
              end;
              case
A61:            0>q`1;
A62:            --q`2>=-q`1 by A60,XREAL_1:26;
                -0<-q`1 by A61,XREAL_1:26;
                then (-q`1)^2<=(q`2)^2 by A62,SQUARE_1:77;
                hence (q`1)^2<=(q`2)^2;
              end;
            end;
            hence (q`1)^2<=(q`2)^2;
          end;
          case
A63:        q`1>=q`2 & q`1<=-q`2;
            now per cases;
              case 0>=q`1;
                then
A64:            -q`1>= -0;
                -q`1<=-q`2 by A63,XREAL_1:26;
                then (-q`1)^2<=(-q`2)^2 by A64,SQUARE_1:77;
                hence (q`1)^2<=(q`2)^2;
              end;
              case 0<q`1;
                then (q`1)^2<=(-q`2)^2 by A63,SQUARE_1:77;
                hence (q`1)^2<=(q`2)^2;
              end;
            end;
            hence (q`1)^2<=(q`2)^2;
          end;
        end;
A65:    now
          assume (q`2)^2<=0;
          then (q`2)^2=0 by XREAL_1:65;
          then q`2=0 by XCMPLX_1:6;
          hence contradiction by A53;
        end;
        then (q`1)^2/(q`2)^2<=(q`2)^2/(q`2)^2 by A59,XREAL_1:74;
        then (q`1/q`2)^2<=(q`2)^2/(q`2)^2 by XCMPLX_1:77;
        then (q`1/q`2)^2 <=1 by A65,XCMPLX_1:60;
        then
A66:    1+(q`1/q`2)^2<=1+1 by XREAL_1:9;
        then
A67:    (q`2)^2*(1+(q`1/q`2)^2)<=(q`2)^2*2 by A27,XREAL_1:66;
A68:    (q`1)^2*(1+(q`1/q`2)^2)<=(q`1)^2*2 by A26,A66,XREAL_1:66;
        1+(q`1/q`2)^2>0 by Lm1;
        then
A69:    (sqrt(1+(q`1/q`2)^2))^2=1+(q`1/q`2)^2 by SQUARE_1:def 4;
        then
A70:    (qz`2)^2<=(q`2)^2*2 by A57,A67,SQUARE_1:68;
A71:    (qz`2)^2>=0 by XREAL_1:65;
A72:    (qz`1)^2>=0 by XREAL_1:65;
        (qz`1)^2<=(q`1)^2*2 by A57,A68,A69,SQUARE_1:68;
        then
A73:    (qz`2)^2+(qz`1)^2<=(q`2)^2*2+(q`1)^2*2 by A70,XREAL_1:9;
        0+0<= (qz`2)^2+(qz`1)^2 by A71,A72;
        then sqrt((qz`2)^2+(qz`1)^2) <= sqrt((q`2)^2*2+(q`1)^2*2)
        by A73,SQUARE_1:94;
        then sqrt((((0.REAL 2) - qz)`2)^2+(((0.REAL 2) - qz)`1)^2)<r
        by A30,A55,A56,XXREAL_0:2;
        then |.(0.REAL 2) - qz.|<r by JGRAPH_1:47;
        then dist(u0,pz)<r by JGRAPH_1:45;
        hence thesis by METRIC_1:12;
      end;
    end;
    hence ex W being Subset of TOP-REAL 2 st 0.REAL 2 in W
    & W is open & f.:W c= V by A20,A22,A23,XBOOLE_1:1;
  end;
  then f is continuous by A1,A2,A3,A18,Th13;
  hence thesis;
end;

