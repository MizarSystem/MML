  rng f0 = (Sq_Circ|Kb).: (the carrier of ((TOP-REAL 2)|Kb)) by FUNCT_2:45
    .= Sq_Circ.:Kb by A5,RELAT_1:162
    .= Cb by A3,Th33;
  then
  ex f1 being Function of (TOP-REAL 2)|Kb,(TOP-REAL 2)|Cbb st f00=f1 & f1
  is being_homeomorphism by A8,Th31,JGRAPH_1:64,TOPMETR:10;
  hence thesis by Th34;
end;

begin :: The Fashoda Meet Theorem for the Circle

theorem
  for K0,C0 being Subset of TOP-REAL 2 st K0={p: -1<=p`1 & p`1<=1 & -1<=
  p`2 & p`2<=1} & C0={p1 where p1 is Point of TOP-REAL 2: |.p1.|<=1} holds
  Sq_Circ"(C0) c= K0
proof
  let K0,C0 be Subset of TOP-REAL 2;
  assume
A1: K0={p: -1<=p`1 & p`1<=1 & -1<=p`2 & p`2<=1} & C0={p1 where p1 is
  Point of TOP-REAL 2: |.p1.|<=1};
  let x be set;
  assume
A2: x in Sq_Circ"(C0);
  then reconsider px=x as Point of TOP-REAL 2;
  set q=px;
A3: Sq_Circ.x in C0 by A2,FUNCT_1:def 13;
  now
    per cases;
    case
      q=0.TOP-REAL 2;
      hence -1<=px`1 & px`1<=1 & -1<=px`2 & px`2<=1 by JGRAPH_2:11;
    end;
    case
A4:   q<>0.TOP-REAL 2 & (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
A5:   now
        assume ((px`1)^2+(px`2)^2)=0;
        then px`1=0 & px`2=0 by COMPLEX1:2;
        hence contradiction by A4,EUCLID:57,58;
      end;
A6:   (px`1)^2 >=0 by XREAL_1:65;
A7:   now
        assume
A8:     px`1=0;
        then px`2=0 by A4;
        hence contradiction by A4,A8,EUCLID:57,58;
      end;
A9:   (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`1 = q`1/sqrt(1
+(q `2/q `1)^2) & (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`2 = q`2
      /sqrt(1+ (q `2/q`1)^2) by EUCLID:56;
      consider p1 being Point of TOP-REAL 2 such that
A10:  p1=Sq_Circ.q and
A11:  |.p1.|<=1 by A1,A3;
      (|.p1.|)^2<= |.p1.| by A11,SQUARE_1:111;
      then
A12:  (|.p1.|)^2<=1 by A11,XXREAL_0:2;
A13:  1+(q`2/q`1)^2>0 by Lm1;
      Sq_Circ.q=|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]| by A4,Def1;
      then (|.p1.|)^2= (q`1/sqrt(1+(q`2/q`1)^2))^2+(q`2/sqrt(1+(q`2/q`1)^2))
      ^2 by A9,A10,JGRAPH_1:46
        .= (q`1)^2/(sqrt(1+(q`2/q`1)^2))^2+(q`2/sqrt(1+(q`2/q`1)^2))^2 by
XCMPLX_1:77
        .= (q`1)^2/(sqrt(1+(q`2/q`1)^2))^2+(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
      by XCMPLX_1:77
        .= (q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2 by A13,
SQUARE_1:def 4
        .= (q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(1+(q`2/q`1)^2) by A13,
SQUARE_1:def 4
        .= ((q`1)^2+(q`2)^2)/(1+(q`2/q`1)^2) by XCMPLX_1:63;
      then
      ((q`1)^2+(q`2)^2)/(1+(q`2/q`1)^2)*(1+(q`2/q`1)^2)<=1 *(1+(q`2/q`1 )
      ^2) by A13,A12,XREAL_1:66;
      then ((q`1)^2+(q`2)^2)<=(1+(q`2/q`1)^2) by A13,XCMPLX_1:88;
      then (px`1)^2+(px`2)^2<=1+(px`2)^2/(px`1)^2 by XCMPLX_1:77;
      then (px`1)^2+(px`2)^2-1<=1+(px`2)^2/(px`1)^2-1 by XREAL_1:11;
      then ((px`1)^2+(px`2)^2-1)*(px`1)^2<=(px`2)^2/(px`1)^2*(px`1)^2 by A6,
XREAL_1:66;
      then (px`1)^2*(px`1)^2+((px`2)^2-1)*(px`1)^2<=(px`2)^2 by A7,XCMPLX_1:6
,88;
      then (px`1)^2*(px`1)^2-(px`1)^2*1+(px`1)^2*(px`2)^2-1 *(px`2)^2<=0 by
XREAL_1:49;
      then
A14:  ((px`1)^2-1)*((px`1)^2+(px`2)^2)<=0;
      (px`2)^2>=0 by XREAL_1:65;
      then
A15:  (px`1)^2-1<=0 by A6,A14,A5,XREAL_1:131;
      then
A16:  px`1<=1 by SQUARE_1:112;
A17:  -1<=px`1 by A15,SQUARE_1:112;
      then q`2<=1 & --q`1>=-q`2 or q`2>=-1 & -q`2>=--q`1 by A4,A16,XREAL_1:26
,XXREAL_0:2;
      then q`2<=1 & 1>=-q`2 or q`2>=-1 & -q`2>=q`1 by A16,XXREAL_0:2;
      then q`2<=1 & -1<=--q`2 or q`2>=-1 & -q`2>=-1 by A17,XREAL_1:26
,XXREAL_0:2;
      hence -1<=px`1 & px`1<=1 & -1<=px`2 & px`2<=1 by A15,SQUARE_1:112
,XREAL_1:26;
    end;
    case
A18:  q<>0.TOP-REAL 2 & not (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<= -q`1);
A19:  now
        assume ((px`2)^2+(px`1)^2)=0;
        then px`2=0 by COMPLEX1:2;
        hence contradiction by A18;
      end;
A20:  (px`2)^2 >=0 by XREAL_1:65;
A21:  px`2<>0 by A18;
A22:  (|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|)`2 = q`2/sqrt(
1+(q `1/q `2)^2) & (|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|)`1 = q
      `1/sqrt(1+ (q `1/q`2)^2) by EUCLID:56;
      consider p1 being Point of TOP-REAL 2 such that
A23:  p1=Sq_Circ.q and
A24:  |.p1.|<=1 by A1,A3;
      (|.p1.|)^2<= |.p1.| by A24,SQUARE_1:111;
      then
A25:  (|.p1.|)^2<=1 by A24,XXREAL_0:2;
A26:  1+(q`1/q`2)^2>0 by Lm1;
      Sq_Circ.q=|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]| by A18,Def1
;
      then (|.p1.|)^2= (q`1/sqrt(1+(q`1/q`2)^2))^2+(q`2/sqrt(1+(q`1/q`2)^2))
      ^2 by A22,A23,JGRAPH_1:46
        .= (q`2)^2/(sqrt(1+(q`1/q`2)^2))^2+(q`1/sqrt(1+(q`1/q`2)^2))^2 by
XCMPLX_1:77
        .= (q`2)^2/(sqrt(1+(q`1/q`2)^2))^2+(q`1)^2/(sqrt(1+(q`1/q`2)^2))^2
      by XCMPLX_1:77
        .= (q`2)^2/(1+(q`1/q`2)^2)+(q`1)^2/(sqrt(1+(q`1/q`2)^2))^2 by A26,
SQUARE_1:def 4
        .= (q`2)^2/(1+(q`1/q`2)^2)+(q`1)^2/(1+(q`1/q`2)^2) by A26,
SQUARE_1:def 4
        .= ((q`2)^2+(q`1)^2)/(1+(q`1/q`2)^2) by XCMPLX_1:63;
      then
      ((q`2)^2+(q`1)^2)/(1+(q`1/q`2)^2)*(1+(q`1/q`2)^2)<=1 *(1+(q`1/q`2 )
      ^2) by A26,A25,XREAL_1:66;
      then ((q`2)^2+(q`1)^2)<=(1+(q`1/q`2)^2) by A26,XCMPLX_1:88;
      then (px`2)^2+(px`1)^2<=1+(px`1)^2/(px`2)^2 by XCMPLX_1:77;
      then (px`2)^2+(px`1)^2-1<=1+(px`1)^2/(px`2)^2-1 by XREAL_1:11;
      then ((px`2)^2+(px`1)^2-1)*(px`2)^2<=(px`1)^2/(px`2)^2*(px`2)^2 by A20,
XREAL_1:66;
      then (px`2)^2*(px`2)^2+((px`1)^2-1)*(px`2)^2<=(px`1)^2 by A21,XCMPLX_1:6
,88;
      then (px`2)^2*(px`2)^2-(px`2)^2*1+(px`2)^2*(px`1)^2-1 *(px`1)^2<=0 by
XREAL_1:49;
      then
A27:  ((px`2)^2-1)*((px`2)^2+(px`1)^2)<=0;
      (px`1)^2>=0 by XREAL_1:65;
      then
A28:  (px`2)^2-1<=0 by A20,A27,A19,XREAL_1:131;
      then -1<=px`2 & px`2<=1 by SQUARE_1:112;
      then q`1<=1 & 1>=-q`1 or q`1>=-1 & -q`1>=-1 by A18,XXREAL_0:2;
      then q`1<=1 & -1<=--q`1 or q`1>=-1 & q`1<=1 by XREAL_1:26;
      hence -1<=px`1 & px`1<=1 & -1<=px`2 & px`2<=1 by A28,SQUARE_1:112;
    end;
  end;
  hence thesis by A1;
end;

theorem Th38:
  for p holds (p=0.TOP-REAL 2 implies Sq_Circ".p=0.TOP-REAL 2) & (
  (p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1) & p<>0.TOP-REAL 2 implies
Sq_Circ".p=|[p`1*sqrt(1+(p`2/p`1)^2),p`2*sqrt(1+(p`2/p`1)^2)]|)& (not(p`2<=p`1
& -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1) implies Sq_Circ".p=|[p`1*sqrt(1+(p`1/p`2)
  ^2),p`2*sqrt(1+(p`1/p`2)^2)]|)
proof
  let p;
  set q=p;
  set px=|[q`1*sqrt(1+(q`1/q`2)^2),q`2*sqrt(1+(q`1/q`2)^2)]|;
A1: px`2 = q`2*sqrt(1+(q`1/q`2)^2) by EUCLID:56;
A2: dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
  hereby
    assume
A3: p=0.TOP-REAL 2;
    then Sq_Circ.p=p by Def1;
    hence Sq_Circ".p=0.TOP-REAL 2 by A2,A3,FUNCT_1:56;
  end;
  hereby
A4: dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
    set q=p;
    assume that
A5: p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1 and
A6: p<>0.TOP-REAL 2;
    set px=|[q`1*sqrt(1+(q`2/q`1)^2),q`2*sqrt(1+(q`2/q`1)^2)]|;
A7: px`1 = q`1*sqrt(1+(q`2/q`1)^2) by EUCLID:56;
A8: sqrt(1+(q`2/q`1)^2)>0 by Lm1,SQUARE_1:93;
A9: px`2 = q`2*sqrt(1+(q`2/q`1)^2) by EUCLID:56;
    then
A10: px`2/px`1=q`2/q`1 by A7,A8,XCMPLX_1:92;
    then
A11: px`2/sqrt(1+(px`2/px`1)^2)=q`2 by A9,A8,XCMPLX_1:90;
A12: now
      assume px`1=0 & px`2=0;
      then q`1=0 & q`2=0 by A7,A9,A8,XCMPLX_1:6;
      hence contradiction by A6,EUCLID:57,58;
    end;
    q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2*sqrt(1+(q`2/q`1)^2) <= (-q`1)*
    sqrt(1+(q`2/q`1)^2) by A5,A8,XREAL_1:66;
    then q`2<=q`1 & (-q`1)*sqrt(1+(q`2/q`1)^2) <= q`2*sqrt(1+(q`2/q`1)^2) or
    px`2>=px`1 & px`2<=-px`1 by A7,A9,A8,XREAL_1:66;
    then q`2*sqrt(1+(q`2/q`1)^2) <= q`1*sqrt(1+(q`2/q`1)^2) & -px`1<=px`2 or
    px`2>=px`1 & px`2<=-px`1 by A7,A8,EUCLID:56,XREAL_1:66;
    then
A13: Sq_Circ.px=|[px`1/sqrt(1+(px`2/px`1)^2),px`2/sqrt(1+(px`2/px`1)^2 )
    ]| by A7,A9,A12,Def1,JGRAPH_2:11;
    px`1/sqrt(1+(px`2/px`1)^2)=q`1 by A7,A8,A10,XCMPLX_1:90;
    then q=Sq_Circ.px by A13,A11,EUCLID:57;
    hence (Sq_Circ").p=|[p`1*sqrt(1+(p`2/p`1)^2),p`2*sqrt(1+(p`2/p`1)^2)]| by
A4,FUNCT_1:56;
  end;
A14: dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
A15: sqrt(1+(q`1/q`2)^2)>0 by Lm1,SQUARE_1:93;
A16: px`1 = q`1*sqrt(1+(q`1/q`2)^2) by EUCLID:56;
  then
A17: px`1/px`2=q`1/q`2 by A1,A15,XCMPLX_1:92;
  then
A18: px`1/sqrt(1+(px`1/px`2)^2)=q`1 by A16,A15,XCMPLX_1:90;
  assume
A19: not(p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1);
A20: now
    assume that
A21: px`2=0 and
    px`1=0;
    q`2=0 by A1,A15,A21,XCMPLX_1:6;
    hence contradiction by A19;
  end;
  p`1<=p`2 & -p`2<=p`1 or p`1>=p`2 & p`1<=-p`2 by A19,JGRAPH_2:23;
  then q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 & q`1*sqrt(1+(q`1/q`2)^2) <= (-q`2)*
  sqrt(1+(q`1/q`2)^2) by A15,XREAL_1:66;
  then q`1<=q`2 & (-q`2)*sqrt(1+(q`1/q`2)^2) <= q`1*sqrt(1+(q`1/q`2)^2) or px
  `1>=px`2 & px`1<=-px`2 by A1,A16,A15,XREAL_1:66;
  then q`1*sqrt(1+(q`1/q`2)^2) <= q`2*sqrt(1+(q`1/q`2)^2) & -px`2<=px`1 or px
  `1>=px`2 & px`1<=-px`2 by A1,A15,EUCLID:56,XREAL_1:66;
  then
A22: Sq_Circ.px=|[px`1/sqrt(1+(px`1/px`2)^2),px`2/sqrt(1+(px`1/px`2)^2) ]|
  by A1,A16,A20,Th14,JGRAPH_2:11;
  px`2/sqrt(1+(px`1/px`2)^2)=q`2 by A1,A15,A17,XCMPLX_1:90;
  then q=Sq_Circ.px by A22,A18,EUCLID:57;
  hence thesis by A14,FUNCT_1:56;
end;

theorem Th39:
  Sq_Circ" is Function of TOP-REAL 2,TOP-REAL 2
proof
A1: the carrier of TOP-REAL 2 c= rng Sq_Circ
  proof
    let y be set;
    assume y in the carrier of TOP-REAL 2;
    then reconsider py=y as Point of TOP-REAL 2;
A2: dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
    now
      per cases;
      case
        py=0.TOP-REAL 2;
        then Sq_Circ.py=py by Def1;
        hence ex x being set st x in dom Sq_Circ & y=Sq_Circ.x by A2;
      end;
      case
A3:     (py`2<=py`1 & -py`1<=py`2 or py`2>=py`1 & py`2<=-py`1) & py<>
        0.TOP-REAL 2;
        set q=py;
        set px=|[q`1*sqrt(1+(q`2/q`1)^2),q`2*sqrt(1+(q`2/q`1)^2)]|;
A4:     sqrt(1+(q`2/q`1)^2)>0 by Lm1,SQUARE_1:93;
A5:     now
          assume that
A6:       px`1=0 and
A7:       px`2=0;
          q`2*sqrt(1+(q`2/q`1)^2)=0 by A7,EUCLID:56;
          then
A8:       q`2=0 by A4,XCMPLX_1:6;
          q`1*sqrt(1+(q`2/q`1)^2)=0 by A6,EUCLID:56;
          then q`1=0 by A4,XCMPLX_1:6;
          hence contradiction by A3,A8,EUCLID:57,58;
        end;
A9:     dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
A10:    px`1 = q`1*sqrt(1+(q`2/q`1)^2) by EUCLID:56;
A11:    px`2 = q`2*sqrt(1+(q`2/q`1)^2) by EUCLID:56;
        then
A12:    px`2/px`1=q`2/q`1 by A10,A4,XCMPLX_1:92;
        then
A13:    px`2/sqrt(1+(px`2/px`1)^2)=q`2 by A11,A4,XCMPLX_1:90;
        q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2*sqrt(1+(q`2/q`1)^2) <= (-q
        `1)*sqrt(1+(q`2/q`1)^2) by A3,A4,XREAL_1:66;
        then q`2<=q`1 & (-q`1)*sqrt(1+(q`2/q`1)^2) <= q`2*sqrt(1+(q`2/q`1)^2)
        or px`2>=px`1 & px`2<=-px`1 by A10,A11,A4,XREAL_1:66;
        then q`2*sqrt(1+(q`2/q`1)^2) <= q`1*sqrt(1+(q`2/q`1)^2) & -px`1<=px`2
        or px`2>=px`1 & px`2<=-px`1 by A10,A4,EUCLID:56,XREAL_1:66;
        then
A14:    Sq_Circ.px=|[px`1/sqrt(1+(px`2/px`1)^2),px`2/sqrt(1+(px`2/ px`1)
        ^2) ]| by A10,A11,A5,Def1,JGRAPH_2:11;
        px`1/sqrt(1+(px`2/px`1)^2)=q`1 by A10,A4,A12,XCMPLX_1:90;
        hence ex x being set st x in dom Sq_Circ & y=Sq_Circ.x by A14,A13,A9,
EUCLID:57;
      end;
      case
A15:    not(py`2<=py`1 & -py`1<=py`2 or py`2>=py`1 & py`2<=-py`1)& py
        <>0.TOP-REAL 2;
        set q=py;
        set px=|[q`1*sqrt(1+(q`1/q`2)^2),q`2*sqrt(1+(q`1/q`2)^2)]|;
A16:    sqrt(1+(q`1/q`2)^2)>0 by Lm1,SQUARE_1:93;
A17:    px`2 = q`2*sqrt(1+(q`1/q`2)^2) by EUCLID:56;
A18:    now
          assume that
A19:      px`2=0 and
          px`1=0;
          q`2=0 by A17,A16,A19,XCMPLX_1:6;
          hence contradiction by A15;
        end;
A20:    px`1 = q`1*sqrt(1+(q`1/q`2)^2) by EUCLID:56;
        then
A21:    px`1/px`2=q`1/q`2 by A17,A16,XCMPLX_1:92;
        then
A22:    px`1/sqrt(1+(px`1/px`2)^2)=q`1 by A20,A16,XCMPLX_1:90;
        py`1<=py`2 & -py`2<=py`1 or py`1>=py`2 & py`1<=-py`2 by A15,JGRAPH_2:23
;
        then
        q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 & q`1*sqrt(1+(q`1/q`2)^2) <= (-q
        `2)*sqrt(1+(q`1/q`2)^2) by A16,XREAL_1:66;
        then q`1<=q`2 & (-q`2)*sqrt(1+(q`1/q`2)^2) <= q`1*sqrt(1+(q`1/q`2)^2)
        or px`1>=px`2 & px`1<=-px`2 by A17,A20,A16,XREAL_1:66;
        then q`1*sqrt(1+(q`1/q`2)^2) <= q`2*sqrt(1+(q`1/q`2)^2) & -px`2<=px`1
        or px`1>=px`2 & px`1<=-px`2 by A17,A16,EUCLID:56,XREAL_1:66;
        then
A23:    Sq_Circ.px=|[px`1/sqrt(1+(px`1/px`2)^2),px`2/sqrt(1+(px`1/ px`2)
        ^2 )]| by A17,A20,A18,Th14,JGRAPH_2:11;
A24:    dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
        px`2/sqrt(1+(px`1/px`2)^2)=q`2 by A17,A16,A21,XCMPLX_1:90;
        hence ex x being set st x in dom Sq_Circ & y=Sq_Circ.x by A23,A22,A24,
EUCLID:57;
      end;
    end;
    hence thesis by FUNCT_1:def 5;
  end;
A25: rng (Sq_Circ")=dom (Sq_Circ) by FUNCT_1:55
    .=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
  dom (Sq_Circ")=rng (Sq_Circ) by FUNCT_1:55;
  then dom (Sq_Circ")=the carrier of TOP-REAL 2 by A1,XBOOLE_0:def 10;
  hence thesis by A25,FUNCT_2:3;
end;

theorem Th40:
  for p being Point of TOP-REAL 2 st p<>0.TOP-REAL 2 holds ((p`1<=
p`2 & -p`2<=p`1 or p`1>=p`2 & p`1<=-p`2) implies (Sq_Circ").p=|[p`1*sqrt(1+(p`1
/p`2)^2),p`2*sqrt(1+(p`1/p`2)^2)]|) & (not(p`1<=p`2 & -p`2<=p`1 or p`1>=p`2 & p
`1<=-p`2) implies (Sq_Circ").p=|[p`1*sqrt(1+(p`2/p`1)^2),p`2*sqrt(1+(p`2/p`1)^2
  )]|)
proof
  let p be Point of TOP-REAL 2;
A1: -p`2<p`1 implies --p`2>-p`1 by XREAL_1:26;
  assume
A2: p<>0.TOP-REAL 2;
  hereby
    assume
A3: p`1<=p`2 & -p`2<=p`1 or p`1>=p`2 & p`1<=-p`2;
    now
      per cases by A3;
      case
A4:     p`1<=p`2 & -p`2<=p`1;
        now
          assume
A5:       p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1;
A6:       now
            per cases by A5;
            case
              p`2<=p`1 & -p`1<=p`2;
              hence p`1=p`2 or p`1=-p`2 by A4,XXREAL_0:1;
            end;
            case
              p`2>=p`1 & p`2<=-p`1;
              then -p`2>=--p`1 by XREAL_1:26;
              hence p`1=p`2 or p`1=-p`2 by A4,XXREAL_0:1;
            end;
          end;
          now
            per cases by A6;
            case
              p`1=p`2;
              hence
              (Sq_Circ").p =|[p`1*sqrt(1+(p`1/p`2)^2),p`2*sqrt(1+(p`1/p`2
              )^2)]| by A2,A5,Th38;
            end;
            case
              p`1=-p`2;
              then p`1<>0 & -p`1=p`2 by A2,EUCLID:57,58;
              then p`1/p`2=-1 & p`2/p`1=-1 by XCMPLX_1:198,199;
              hence
              (Sq_Circ").p=|[p`1*sqrt(1+(p`1/p`2)^2),p`2*sqrt(1+(p`1/p`2)
              ^2) ]| by A2,A5,Th38;
            end;
          end;
          hence (Sq_Circ").p=|[p`1*sqrt(1+(p`1/p`2)^2),p`2*sqrt(1+(p`1/p`2)^2)
          ]|;
        end;
        hence (Sq_Circ").p=|[p`1*sqrt(1+(p`1/p`2)^2),p`2*sqrt(1+(p`1/p`2)^2)]|
        by Th38;
      end;
      case
A7:     p`1>=p`2 & p`1<=-p`2;
        now
          assume
A8:       p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1;
A9:       now
            per cases by A8;
            case
              p`2<=p`1 & -p`1<=p`2;
              then --p`1>=-p`2 by XREAL_1:26;
              hence p`1=p`2 or p`1=-p`2 by A7,XXREAL_0:1;
            end;
            case
              p`2>=p`1 & p`2<=-p`1;
              hence p`1=p`2 or p`1=-p`2 by A7,XXREAL_0:1;
            end;
          end;
          now
            per cases by A9;
            case
              p`1=p`2;
              hence
              (Sq_Circ").p=|[p`1*sqrt(1+(p`1/p`2)^2),p`2*sqrt(1+(p`1/p`2)
              ^2) ]| by A2,A8,Th38;
            end;
            case
A10:          p`1=-p`2;
              then p`1<>0 & -p`1=p`2 by A2,EUCLID:57,58;
              then
A11:          p`2/p`1=-1 by XCMPLX_1:198;
              p`2<>0 by A2,A10,EUCLID:57,58;
              then p`1/p`2=-1 by A10,XCMPLX_1:198;
              hence
              (Sq_Circ").p=|[p`1*sqrt(1+(p`1/p`2)^2),p`2*sqrt(1+(p`1/p`2)
              ^2) ]| by A2,A8,A11,Th38;
            end;
          end;
          hence (Sq_Circ").p=|[p`1*sqrt(1+(p`1/p`2)^2),p`2*sqrt(1+(p`1/p`2)^2)
          ]|;
        end;
        hence (Sq_Circ").p=|[p`1*sqrt(1+(p`1/p`2)^2),p`2*sqrt(1+(p`1/p`2)^2)]|
        by Th38;
      end;
    end;
    hence (Sq_Circ").p=|[p`1*sqrt(1+(p`1/p`2)^2),p`2*sqrt(1+(p`1/p`2)^2)]|;
  end;
A12: -p`2>p`1 implies --p`2<-p`1 by XREAL_1:26;
  assume not(p`1<=p`2 & -p`2<=p`1 or p`1>=p`2 & p`1<=-p`2);
  hence thesis by A2,A1,A12,Th38;
end;

theorem Th41:
  for X being non empty TopSpace, f1,f2 being Function of X,R^1 st
  f1 is continuous & f2 is continuous & (for q being Point of X holds f2.q<>0)
holds ex g being Function of X,R^1 st (for p being Point of X,r1,r2 being real
  number st f1.p=r1 & f2.p=r2 holds g.p=r1*sqrt(1+(r1/r2)^2)) & g is continuous
proof
  let X be non empty TopSpace, f1,f2 be Function of X,R^1;
  assume that
A1: f1 is continuous and
A2: f2 is continuous & for q being Point of X holds f2.q<>0;
  consider g2 being Function of X,R^1 such that
A3: for p being Point of X,r1,r2 being real number st f1.p=r1 & f2.p=r2
  holds g2.p=sqrt(1+(r1/r2)^2) and
A4: g2 is continuous by A1,A2,Th18;
  consider g3 being Function of X,R^1 such that
A5: for p being Point of X,r1,r0 being real number st f1.p=r1 & g2.p=r0
  holds g3.p=r1*r0 and
A6: g3 is continuous by A1,A4,JGRAPH_2:35;
  for p being Point of X,r1,r2 being real number st f1.p=r1 & f2.p=r2
  holds g3.p=r1*sqrt(1+(r1/r2)^2)
  proof
    let p be Point of X,r1,r2 be real number;
    assume that
A7: f1.p=r1 and
A8: f2.p=r2;
    g2.p=sqrt(1+(r1/r2)^2) by A3,A7,A8;
    hence thesis by A5,A7;
  end;
  hence thesis by A6;
end;

theorem Th42:
  for X being non empty TopSpace, f1,f2 being Function of X,R^1 st
f1 is continuous & f2 is continuous & (for q being Point of X holds f2.q<>0) ex
g being Function of X,R^1 st (for p being Point of X,r1,r2 being real number st
  f1.p=r1 & f2.p=r2 holds g.p=r2*sqrt(1+(r1/r2)^2)) & g is continuous
proof
  let X be non empty TopSpace, f1,f2 be Function of X,R^1;
  assume that
A1: f1 is continuous and
A2: f2 is continuous and
A3: for q being Point of X holds f2.q<>0;
  consider g2 being Function of X,R^1 such that
A4: for p being Point of X,r1,r2 being real number st f1.p=r1 & f2.p=r2
  holds g2.p=sqrt(1+(r1/r2)^2) and
A5: g2 is continuous by A1,A2,A3,Th18;
  consider g3 being Function of X,R^1 such that
A6: for p being Point of X,r2,r0 being real number st f2.p=r2 & g2.p=r0
  holds g3.p=r2*r0 and
A7: g3 is continuous by A2,A5,JGRAPH_2:35;
  for p being Point of X,r1,r2 being real number st f1.p=r1 & f2.p=r2
  holds g3.p=r2*sqrt(1+(r1/r2)^2)
  proof
    let p be Point of X,r1,r2 be real number;
    assume that
A8: f1.p=r1 and
A9: f2.p=r2;
    g2.p=sqrt(1+(r1/r2)^2) by A4,A8,A9;
    hence thesis by A6,A9;
  end;
  hence thesis by A7;
end;

theorem Th43:
  for K1 being non empty Subset of TOP-REAL 2, f being Function of
(TOP-REAL 2)|K1,R^1 st (for p being Point of TOP-REAL 2 st p in the carrier of
  (TOP-REAL 2)|K1 holds f.p=p`1*sqrt(1+(p`2/p`1)^2)) & (for q being Point of
  TOP-REAL 2 st q in the carrier of (TOP-REAL 2)|K1 holds q`1<>0 ) holds f is
  continuous
proof
  let K1 be non empty Subset of TOP-REAL 2, f be Function of (TOP-REAL 2)|K1,
  R^1;
  reconsider g1=proj1|K1 as continuous Function of (TOP-REAL 2)|K1,R^1 by Lm7;
  reconsider g2=proj2|K1 as continuous Function of (TOP-REAL 2)|K1,R^1 by Lm5;
