A5:       sqrt(1+(q`2/q`1)^2)>0 by Lm1,SQUARE_1:93;
A6:       now
            assume that
A7:         px`1=0 and
A8:         px`2=0;
            q`2*sqrt(1+(q`2/q`1)^2 )=0 by A8,EUCLID:56;
            then
A9:         q`2=0 by A5,XCMPLX_1:6;
            q`1*sqrt(1+(q`2/q`1)^2)=0 by A7,EUCLID:56;
            then q`1=0 by A5,XCMPLX_1:6;
            hence contradiction by A4,A9,EUCLID:57,58;
          end;
A10:      dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
A11:      px`1 = q`1*sqrt(1+(q`2/q`1)^2) by EUCLID:56;
A12:      px`2 = q`2*sqrt(1+(q`2/q`1)^2) by EUCLID:56;
          then
A13:      px`2/px`1=q`2/q`1 by A11,A5,XCMPLX_1:92;
          then
A14:      px`2/sqrt(1+(px`2/px`1)^2)=q`2 by A12,A5,XCMPLX_1:90;
          q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2*sqrt(1+(q`2/q`1)^2) <= (
          -q`1)*sqrt(1+(q`2/q`1)^2) by A4,A5,XREAL_1:66;
          then
          q`2<=q`1 & (-q`1)*sqrt(1+(q`2/q`1)^2) <= q`2*sqrt(1+(q`2/q`1)^2
          ) or px`2>=px`1 & px`2<=-px`1 by A11,A12,A5,XREAL_1:66;
          then q`2*sqrt(1+(q`2/q`1)^2) <= q`1*sqrt(1+(q`2/q`1)^2) & -px`1<=px
          `2 or px`2>=px`1 & px`2<=-px`1 by A11,A5,EUCLID:56,XREAL_1:66;
          then
A15:      Sq_Circ.px=|[px`1/sqrt(1+(px`2/px`1)^2),px`2/sqrt(1+( px`2/px`1
          )^2) ]| by A11,A12,A6,Def1,JGRAPH_2:11;
          px`1/sqrt(1+(px`2/px`1)^2)=q`1 by A11,A5,A13,XCMPLX_1:90;
          hence ex x being set st x in dom Sq_Circ & y=Sq_Circ.x by A15,A14,A10
,EUCLID:57;
        end;
        case
A16:      q<>0.TOP-REAL 2 & not(q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q `2<=-q`1);
          set px=|[q`1*sqrt(1+(q`1/q`2)^2),q`2*sqrt(1+(q`1/q`2)^2)]|;
A17:      sqrt(1+(q`1/q`2)^2)>0 by Lm1,SQUARE_1:93;
A18:      now
            assume that
A19:        px`2=0 and
            px`1=0;
            q`2*sqrt(1+(q`1/q`2)^2)=0 by A19,EUCLID:56;
            then q`2=0 by A17,XCMPLX_1:6;
            hence contradiction by A16;
          end;
A20:      px`2 = q`2*sqrt(1+(q`1/q`2)^2) by EUCLID:56;
A21:      px`1 = q`1*sqrt(1+(q`1/q`2)^2) by EUCLID:56;
          then
A22:      px`1/px`2=q`1/q`2 by A20,A17,XCMPLX_1:92;
          then
A23:      px`1/sqrt(1+(px`1/px`2)^2)=q`1 by A21,A17,XCMPLX_1:90;
          q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 & q`1<=-q`2 by A16,JGRAPH_2:23;
          then
          q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 & q`1*sqrt(1+(q`1/q`2)^2) <= (
          -q`2)*sqrt(1+(q`1/q`2)^2) by A17,XREAL_1:66;
          then
          q`1<=q`2 & (-q`2)*sqrt(1+(q`1/q`2)^2) <= q`1*sqrt(1+(q`1/q`2)^2
          ) or px`1>=px`2 & px`1<=-px`2 by A20,A21,A17,XREAL_1:66;
          then q`1*sqrt(1+(q`1/q`2)^2) <= q`2*sqrt(1+(q`1/q`2)^2) & -px`2<=px
          `1 or px`1>=px`2 & px`1<=-px`2 by A20,A17,EUCLID:56,XREAL_1:66;
          then
A24:      Sq_Circ.px=|[px`1/sqrt(1+(px`1/px`2)^2),px`2/sqrt(1+( px`1/px`2
          )^2 )]| by A20,A21,A18,Th14,JGRAPH_2:11;
A25:      dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
          px`2/sqrt(1+(px`1/px`2)^2)=q`2 by A20,A17,A22,XCMPLX_1:90;
          hence ex x being set st x in dom Sq_Circ & y=Sq_Circ.x by A24,A23,A25
,EUCLID:57;
        end;
      end;
      hence thesis by A2,FUNCT_1:def 5;
    end;
    then
A26: rng f=the carrier of TOP-REAL 2 by XBOOLE_0:def 10;
A27: rng f=dom ((f qua Function)") by A2,FUNCT_1:55
      .=dom (f/") by A2,A26,TOPS_2:def 4
      .=[#](TOP-REAL 2) by FUNCT_2:def 1;
    then g=Sq_Circ" by A2,TOPS_2:def 4;
    hence thesis by A2,A3,A27,Th31,Th52,TOPS_2:def 5;
  end;
  hence rng Sq_Circ=the carrier of TOP-REAL 2;
  thus thesis by A1;
end;

Lm19: now
  let pz2, pz1 be real number;
  assume ((pz2)^2+(pz1)^2-1)*(pz2)^2<=(pz1)^2;
  then (pz2)^2*(pz2)^2+(pz2)^2*((pz1)^2-1)-(pz1)^2 <=(pz1)^2-(pz1)^2 by
XREAL_1:11;
  hence ((pz2)^2-1)*((pz2)^2+(pz1)^2)<=0;
end;

Lm20: now
  let px1 be real number;
  assume (px1)^2-1=0;
  then (px1-1)*(px1+1)=0;
  then px1-1=0 or px1+1=0 by XCMPLX_1:6;
  hence px1=1 or px1=-1;
end;

theorem
  for f,g being Function of I[01],TOP-REAL 2, C0,KXP,KXN,KYP,KYN being
  Subset of TOP-REAL 2, O,I being Point of I[01] st O=0 & I=1 & f is continuous
one-to-one & g is continuous one-to-one & C0={p: |.p.|<=1}& KXP={q1 where q1 is
Point of TOP-REAL 2: |.q1.|=1 & q1`2<=q1`1 & q1`2>=-q1`1} & KXN={q2 where q2 is
Point of TOP-REAL 2: |.q2.|=1 & q2`2>=q2`1 & q2`2<=-q2`1} & KYP={q3 where q3 is
Point of TOP-REAL 2: |.q3.|=1 & q3`2>=q3`1 & q3`2>=-q3`1} & KYN={q4 where q4 is
Point of TOP-REAL 2: |.q4.|=1 & q4`2<=q4`1 & q4`2<=-q4`1} & f.O in KXN & f.I in
KXP & g.O in KYN & g.I in KYP & rng f c= C0 & rng g c= C0 holds rng f meets rng
  g
proof
A1: dom (Sq_Circ")=the carrier of TOP-REAL 2 by Th39,FUNCT_2:def 1;
  let f,g be Function of I[01],TOP-REAL 2, C0,KXP,KXN,KYP,KYN be Subset of
  TOP-REAL 2, O,I be Point of I[01];
  assume
A2: O=0 & I=1 & f is continuous one-to-one & g is continuous one-to-one
& C0={p: |.p.|<=1}& KXP={q1 where q1 is Point of TOP-REAL 2: |.q1.|=1 & q1`2<=
q1`1 & q1`2>=-q1`1} & KXN={q2 where q2 is Point of TOP-REAL 2: |.q2.|=1 & q2`2
>=q2`1 & q2`2<=-q2`1} & KYP={q3 where q3 is Point of TOP-REAL 2: |.q3.|=1 & q3
  `2>=q3`1 & q3`2>=-q3`1} & KYN={q4 where q4 is Point of TOP-REAL 2: |.q4.|=1 &
q4`2<=q4`1 & q4`2<=-q4`1} & f.O in KXN & f.I in KXP & g.O in KYN & g.I in KYP &
  rng f c= C0 & rng g c= C0;
  then consider p1 being Point of TOP-REAL 2 such that
A3: f.O=p1 and
A4: |.p1.|=1 and
A5: p1`2>=p1`1 and
A6: p1`2<=-p1`1;
  reconsider gg=Sq_Circ"*g as Function of I[01],TOP-REAL 2 by Th39,FUNCT_2:19;
A7: dom g=the carrier of I[01] by FUNCT_2:def 1;
  reconsider ff=Sq_Circ"*f as Function of I[01],TOP-REAL 2 by Th39,FUNCT_2:19;
A8: dom gg=the carrier of I[01] by FUNCT_2:def 1;
A9: dom ff=the carrier of I[01] by FUNCT_2:def 1;
  then
A10: (ff.O)=(Sq_Circ").(f.O) by FUNCT_1:22;
A11: dom f=the carrier of I[01] by FUNCT_2:def 1;
A12: for r being Point of I[01] holds -1<=(ff.r)`1 & (ff.r)`1<=1 & -1<=(gg.
r)`1 & (gg.r)`1<=1 & -1 <=(ff.r)`2 & (ff.r)`2<=1 & -1 <=(gg.r)`2 & (gg.r)`2<=1
  proof
    let r be Point of I[01];
    f.r in rng f by A11,FUNCT_1:12;
    then f.r in C0 by A2;
    then consider p1 being Point of TOP-REAL 2 such that
A13: f.r=p1 and
A14: |.p1.|<=1 by A2;
    g.r in rng g by A7,FUNCT_1:12;
    then g.r in C0 by A2;
    then consider p2 being Point of TOP-REAL 2 such that
A15: g.r=p2 and
A16: |.p2.|<=1 by A2;
A17: (gg.r)=(Sq_Circ").(g.r) by A8,FUNCT_1:22;
A18: now
      per cases;
      case
        p2=0.TOP-REAL 2;
        hence
        -1<=(gg.r)`1 & (gg.r)`1<=1 & -1 <=(gg.r)`2 & (gg.r)`2<=1 by A17,A15
,Th38,JGRAPH_2:11;
      end;
      case
A19:    p2<>0.TOP-REAL 2 & (p2`2<=p2`1 & -p2`1<=p2`2 or p2`2>=p2`1 &
        p2`2<=-p2`1);
        set px=gg.r;
A20:    Sq_Circ".p2=|[p2`1*sqrt(1+(p2`2/p2`1)^2),p2`2*sqrt(1+(p2`2 /p2`1
        )^2 )]| by A19,Th38;
        then
A21:    px`1 = p2`1*sqrt(1+(p2`2/p2`1)^2) by A17,A15,EUCLID:56;
        (|.p2.|)^2<=|.p2.| by A16,SQUARE_1:111;
        then
A22:    (|.p2.|)^2<=1 by A16,XXREAL_0:2;
A23:    (px`2)^2>=0 by XREAL_1:65;
A24:    (px`1)^2 >=0 by XREAL_1:65;
A25:    px`2 = p2`2*sqrt(1+(p2`2/p2`1)^2) by A17,A15,A20,EUCLID:56;
A26:    sqrt(1+(p2`2/p2`1)^2)>0 by Lm1,SQUARE_1:93;
        then p2`2<=p2`1 & -p2`1<=p2`2 or p2`2>=p2`1 & p2`2*sqrt(1+(p2`2/p2`1)
        ^2) <= (-p2`1)*sqrt(1+(p2`2/p2`1)^2) by A19,XREAL_1:66;
        then
A27:    p2`2<=p2`1 & (-p2`1)*sqrt(1+(p2`2/p2`1)^2) <= p2`2*sqrt(1+(p2`2/
        p2`1 )^2) or px`2>=px`1 & px`2<=-px`1 by A21,A25,A26,XREAL_1:66;
        then
A28:    p2`2*sqrt( 1+(p2`2/p2`1)^2) <= p2`1*sqrt(1+(p2`2/p2`1)^2) & -px
        `1<= px `2 or px`2>=px`1 & px`2<=-px`1 by A17,A15,A20,A21,A26,EUCLID:56
,XREAL_1:66;
A29:    now
          assume px`1=0 & px`2=0;
          then p2`1=0 & p2`2=0 by A21,A25,A26,XCMPLX_1:6;
          hence contradiction by A19,EUCLID:57,58;
        end;
        then
A30:    px`1<>0 by A21,A25,A26,A27,XREAL_1:66;
        set q=px;
A31:    (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`2 = q`2/
        sqrt(1+( q`2/q`1)^2) by EUCLID:56;
A32:    1+(q`2/q`1)^2>0 by Lm1;
A33:    p2=Sq_Circ.px & (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)
^2)]|)`1 = q `1/sqrt(1+( q`2/q`1)^2) by A17,A15,Th54,EUCLID:56,FUNCT_1:54;
        Sq_Circ.q=|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]| by A21
,A25,A29,A28,Def1,JGRAPH_2:11;
        then (|.p2.|)^2= (q`1/sqrt(1+(q`2/q`1)^2))^2+(q`2/sqrt(1+(q`2/q`1)^2)
        ) ^2 by A33,A31,JGRAPH_1:46
          .= (q`1)^2/(sqrt(1+(q`2/q`1)^2))^2+(q`2/sqrt(1+(q`2/q`1)^2))^2 by
XCMPLX_1:77
          .= (q`1)^2/(sqrt(1+(q`2/q`1)^2))^2+(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
        by XCMPLX_1:77
          .= (q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2 by A32,
SQUARE_1:def 4
          .= (q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(1+(q`2/q`1)^2) by A32,
SQUARE_1:def 4
          .= ((q`1)^2+(q`2)^2)/(1+(q`2/q`1)^2) by XCMPLX_1:63;
        then ((q`1)^2+(q`2)^2)/(1+(q`2/q`1)^2)*(1+(q`2/q`1)^2)<=1 *(1+(q`2/q
        `1 )^2) by A32,A22,XREAL_1:66;
        then ((q`1)^2+(q`2)^2)<=(1+(q`2/q`1)^2) by A32,XCMPLX_1:88;
        then (px`1)^2+(px`2)^2<=1+(px`2)^2/(px`1)^2 by XCMPLX_1:77;
        then (px`1)^2+(px`2)^2-1<=(px`2)^2/(px`1)^2 by XREAL_1:22;
        then ((px`1)^2+(px`2)^2-1)*(px`1)^2<=(px`2)^2/(px`1)^2*(px`1)^2 by A24,
XREAL_1:66;
        then ((px`1)^2+(px`2)^2-1)*(px`1)^2<=(px`2)^2 by A30,XCMPLX_1:6,88;
        then
A34:    ((px`1)^2-1)*((px`1)^2+(px`2)^2)<=0 by Lm19;
        ((px`1)^2+(px`2)^2)<>0 by A29,COMPLEX1:2;
        then
A35:    ((px`1)^2-1)<=0 by A24,A34,A23,XREAL_1:131;
        then
A36:    px`1>=-1 by SQUARE_1:112;
A37:    px`1<=1 by A35,SQUARE_1:112;
        then q`2<=1 & --q`1>=-q`2 or q`2>=-1 & q`2<=-q`1 by A21,A25,A28,A36,
XREAL_1:26,XXREAL_0:2;
        then q`2<=1 & q`1>=-q`2 or q`2>=-1 & -q`2>=--q`1 by XREAL_1:26;
        then q`2<=1 & 1>=-q`2 or q`2>=-1 & -q`2>=q`1 by A37,XXREAL_0:2;
        then q`2<=1 & -1<=--q`2 or q`2>=-1 & -q`2>=-1 by A36,XREAL_1:26
,XXREAL_0:2;
        hence
        -1<=(gg.r)`1 & (gg.r)`1<=1 & -1 <=(gg.r)`2 & (gg.r)`2<=1 by A35,
SQUARE_1:112,XREAL_1:26;
      end;
      case
A38:    p2<>0.TOP-REAL 2 & not(p2`2<=p2`1 & -p2`1<=p2`2 or p2`2>=p2
        `1 & p2`2<=-p2`1);
        set pz=gg.r;
A39:    Sq_Circ".p2=|[p2`1*sqrt(1+(p2`1/p2`2)^2),p2`2*sqrt(1+(p2`1 /p2`2
        )^2 )]| by A38,Th38;
        then
A40:    pz`2 = p2`2*sqrt(1+(p2`1/p2`2)^2) by A17,A15,EUCLID:56;
A41:    pz`1 = p2`1*sqrt(1+(p2`1/p2`2)^2) by A17,A15,A39,EUCLID:56;
A42:    sqrt(1+(p2`1/p2`2)^2)>0 by Lm1,SQUARE_1:93;
        p2`1<=p2`2 & -p2`2<=p2`1 or p2`1>=p2`2 & p2`1<=-p2`2 by A38,JGRAPH_2:23
;
        then p2`1<=p2`2 & -p2`2<=p2`1 or p2`1>=p2`2 & p2`1*sqrt(1+(p2`1/p2`2)
        ^2) <= (-p2`2)*sqrt(1+(p2`1/p2`2)^2) by A42,XREAL_1:66;
        then
A43:    p2`1<=p2`2 & (-p2`2)*sqrt(1+(p2`1/p2`2)^2) <= p2`1*sqrt(1+(p2`1/
        p2`2) ^2) or pz`1>=pz`2 & pz`1<=-pz`2 by A40,A41,A42,XREAL_1:66;
        then
A44:    p2`1*sqrt(1+(p2`1/p2`2)^2) <= p2`2*sqrt(1+(p2`1/p2`2)^2) & -pz
        `2<=pz `1 or pz`1>=pz`2 & pz`1<=-pz`2 by A17,A15,A39,A40,A42,EUCLID:56
,XREAL_1:66;
A45:    now
          assume that
A46:      pz`2=0 and
          pz`1=0;
          p2`2=0 by A40,A42,A46,XCMPLX_1:6;
          hence contradiction by A38;
        end;
        then
A47:    pz`2<>0 by A40,A41,A42,A43,XREAL_1:66;
A48:    p2=Sq_Circ.pz & (|[pz`1/sqrt(1+(pz`1/pz`2)^2),pz`2/sqrt(1+(pz`1/
        pz`2)^2)]|) `2 = pz`2/ sqrt(1+(pz`1/pz`2)^2) by A17,A15,Th54,EUCLID:56
,FUNCT_1:54;
A49:    (pz`2)^2 >=0 by XREAL_1:65;
        (|.p2.|)^2<=|.p2.| by A16,SQUARE_1:111;
        then
A50:    (|.p2.|)^2<=1 by A16,XXREAL_0:2;
A51:    (pz`1)^2>=0 by XREAL_1:65;
A52:    (|[pz`1/sqrt(1+(pz`1/pz`2)^2),pz`2/sqrt(1+(pz`1/pz`2)^2)]|)`1 =
        pz`1/ sqrt(1+(pz`1/pz`2)^2) by EUCLID:56;
A53:    1+(pz`1/pz`2)^2>0 by Lm1;
        Sq_Circ.pz=|[pz`1/sqrt(1+(pz`1/pz`2)^2),pz`2/sqrt(1+(pz`1/pz`2)
        ^2)]| by A40,A41,A45,A44,Th14,JGRAPH_2:11;
        then (|.p2.|)^2= (pz`2/sqrt(1+(pz`1/pz`2)^2))^2+(pz`1/sqrt(1+(pz`1/pz
        `2)^2))^2 by A48,A52,JGRAPH_1:46
          .= (pz`2)^2/(sqrt(1+(pz`1/pz`2)^2))^2+(pz`1/sqrt(1+(pz`1/pz`2)^2))
        ^2 by XCMPLX_1:77
          .= (pz`2)^2/(sqrt(1+(pz`1/pz`2)^2))^2 +(pz`1)^2/(sqrt(1+(pz`1/pz`2
        )^2))^2 by XCMPLX_1:77
          .= (pz`2)^2/(1+(pz`1/pz`2)^2)+(pz`1)^2/(sqrt(1+(pz`1/pz`2)^2))^2
        by A53,SQUARE_1:def 4
          .= (pz`2)^2/(1+(pz`1/pz`2)^2)+(pz`1)^2/(1+(pz`1/pz`2)^2) by A53,
SQUARE_1:def 4
          .= ((pz`2)^2+(pz`1)^2)/(1+(pz`1/pz`2)^2) by XCMPLX_1:63;
        then ((pz`2)^2+(pz`1)^2)/(1+(pz`1/pz`2)^2)*(1+(pz`1/pz`2)^2) <=1 *(1+
        (pz`1/pz`2)^2) by A53,A50,XREAL_1:66;
        then ((pz`2)^2+(pz`1)^2)<=(1+(pz`1/pz`2)^2) by A53,XCMPLX_1:88;
        then (pz`2)^2+(pz`1)^2<=1+(pz`1)^2/(pz`2)^2 by XCMPLX_1:77;
        then (pz`2)^2+(pz`1)^2-1<=(pz`1)^2/(pz`2)^2 by XREAL_1:22;
        then ((pz`2)^2+(pz`1)^2-1)*(pz`2)^2<=(pz`1)^2/(pz`2)^2*(pz`2)^2 by A49,
XREAL_1:66;
        then ((pz`2)^2+(pz`1)^2-1)*(pz`2)^2<=(pz`1)^2 by A47,XCMPLX_1:6,88;
        then
A54:    ((pz`2)^2-1)*((pz`2)^2+(pz`1)^2)<=0 by Lm19;
        (pz`2)^2+(pz`1)^2<>0 by A45,COMPLEX1:2;
        then
A55:    (pz`2)^2-1<=0 by A49,A54,A51,XREAL_1:131;
        then
A56:    pz`2>=-1 by SQUARE_1:112;
A57:    pz`2<=1 by A55,SQUARE_1:112;
        then pz`1<=1 & --pz`2>=-pz`1 or pz`1>=-1 & pz`1<=-pz`2 by A40,A41,A44
,A56,XREAL_1:26,XXREAL_0:2;
