      end;
    end;
    hence ex W being Subset of TOP-REAL 2 st 0.REAL 2 in W
    & W is open & f.:W c= V by A22,A23,A24,XBOOLE_1:1;
  end;
  then f is continuous by A1,A2,A3,A20,Th13;
  hence thesis;
end;

theorem Th32:
  Sq_Circ is one-to-one
proof
  let x1,x2 be set;
  assume
A1: x1 in dom Sq_Circ & x2 in dom Sq_Circ & Sq_Circ.x1=Sq_Circ.x2;
  then reconsider p1=x1 as Point of TOP-REAL 2;
  reconsider p2=x2 as Point of TOP-REAL 2 by A1;
  set q=p1,p=p2;
  per cases;
  suppose
A2: q=0.REAL 2;
    then
A3: Sq_Circ.q=0.REAL 2 by Def1;
    now per cases;
      case p=0.REAL 2;
        hence x1=x2 by A2;
      end;
      case
A4:     p<>0.REAL 2 & (p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1);
        then Sq_Circ.p=|[p`1/sqrt(1+(p`2/p`1)^2),p`2/sqrt(1+(p`2/p`1)^2)]|
        by Def1;
        then
A5:     p`1/sqrt(1+(p`2/p`1)^2)=0 & p`2/sqrt(1+(p`2/p`1)^2)=0
        by A1,A3,EUCLID:56,JGRAPH_2:11;
        (p`2/p`1)^2 >=0 by XREAL_1:65;
        then 1+(p`2/p`1)^2>=1+0 by XREAL_1:9;
        then
A6:     sqrt(1+(p`2/p`1)^2)>=1 by SQUARE_1:83,94;
        then
A7:     p`1= 0 *sqrt(1+(p`2/p`1)^2) by A5,XCMPLX_1:88
          .=0;
        p`2= 0 *sqrt(1+(p`2/p`1)^2) by A5,A6,XCMPLX_1:88
          .=0;
        hence contradiction by A4,A7,EUCLID:57,58;
      end;
      case
A8:     p<>0.REAL 2 & not (p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1);
        then Sq_Circ.p=|[p`1/sqrt(1+(p`1/p`2)^2),p`2/sqrt(1+(p`1/p`2)^2)]|
        by Def1;
        then
A9:     p`1/sqrt(1+(p`1/p`2)^2)=0 & p`2/sqrt(1+(p`1/p`2)^2)=0
        by A1,A3,EUCLID:56,JGRAPH_2:11;
        (p`1/p`2)^2 >=0 by XREAL_1:65;
        then 1+(p`1/p`2)^2>=1+0 by XREAL_1:9;
        then
A10:    sqrt(1+(p`1/p`2)^2)>=1 by SQUARE_1:83,94;
        p`2= 0 *sqrt(1+(p`1/p`2)^2) by A9,A10,XCMPLX_1:88
          .=0;
        hence contradiction by A8;
      end;
    end;
    hence x1=x2;
  end;
  suppose
A12: q<>0.REAL 2 & (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
    then
A13: Sq_Circ.q=|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]| by Def1;
A14: (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`1
    = q`1/sqrt(1+(q`2/q`1)^2) &
    (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`2
    = q`2/sqrt(1+(q`2/q`1)^2) by EUCLID:56;
A15: 1+(q`2/q`1)^2>0 by Lm1;
A16: sqrt(1+(q`2/q`1)^2)>0 by Lm1,SQUARE_1:93;
    now per cases;
      case p=0.REAL 2;
        then Sq_Circ.p=0.REAL 2 by Def1;
        then
A17:    q`1/sqrt(1+(q`2/q`1)^2)=0 & q`2/sqrt(1+(q`2/q`1)^2)=0
        by A1,A13,EUCLID:56,JGRAPH_2:11;
        (q`2/q`1)^2 >=0 by XREAL_1:65;
        then 1+(q`2/q`1)^2>=1+0 by XREAL_1:9;
        then
A18:    sqrt(1+(q`2/q`1)^2)>=1 by SQUARE_1:83,94;
        then
A19:    q`1= 0 *sqrt(1+(q`2/q`1)^2) by A17,XCMPLX_1:88
          .=0;
        q`2= 0 *sqrt(1+(q`2/q`1)^2) by A17,A18,XCMPLX_1:88
          .=0;
        hence contradiction by A12,A19,EUCLID:57,58;
      end;
      case
A20:    p<>0.REAL 2 & (p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1);
        then
A21:    Sq_Circ.p=|[p`1/sqrt(1+(p`2/p`1)^2),p`2/sqrt(1+(p`2/p`1)^2
        )]| by Def1;
        then
A22:    p`1/sqrt(1+(p`2/p`1)^2)=q`1/sqrt(1+(q`2/q`1)^2) &
        p`2/sqrt(1+(p`2/p`1)^2)=q`2/sqrt(1+(q`2/q`1)^2)
        by A1,A13,A14,EUCLID:56;
A23:    1+(p`2/p`1)^2>0 by Lm1;
A24:    sqrt(1+(p`2/p`1)^2)>0 by Lm1,SQUARE_1:93;
        (p`1)^2/(sqrt(1+(p`2/p`1)^2))^2=(q`1/sqrt(1+(q`2/q`1)^2))^2
        by A22,XCMPLX_1:77;
        then (p`1)^2/(sqrt(1+(p`2/p`1)^2))^2=(q`1)^2/(sqrt(1+(q`2/q`1)^2))^2
        by XCMPLX_1:77;
        then (p`1)^2/(1+(p`2/p`1)^2)=(q`1)^2/(sqrt(1+(q`2/q`1)^2))^2
        by A23,SQUARE_1:def 4;
        then
A25:    (p`1)^2/(1+(p`2/p`1)^2)=(q`1)^2/(1+(q`2/q`1)^2) by A15,SQUARE_1:def 4;
        (p`2)^2/(sqrt(1+(p`2/p`1)^2))^2=(q`2/sqrt(1+(q`2/q`1)^2))^2
        by A22,XCMPLX_1:77;
        then (p`2)^2/(sqrt(1+(p`2/p`1)^2))^2=(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
        by XCMPLX_1:77;
        then (p`2)^2/(1+(p`2/p`1)^2)=(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
        by A23,SQUARE_1:def 4;
        then
A26:    (p`2)^2/(1+(p`2/p`1)^2)=(q`2)^2/(1+(q`2/q`1)^2) by A15,SQUARE_1:def 4;
        now
          assume
A27:      p`1=0;
          then p`2=0 by A20;
          hence contradiction by A20,A27,EUCLID:57,58;
        end;
        then
A28:    (p`1)^2>0 by SQUARE_1:74;
        (p`1)^2/(1+(p`2/p`1)^2)/(p`1)^2=(q`1)^2/(p`1)^2/(1+(q`2/q`1)^2)
        by A25,XCMPLX_1:48;
        then (p`1)^2/(p`1)^2/(1+(p`2/p`1)^2)=(q`1)^2/(p`1)^2/(1+(q`2/q`1)^2)
        by XCMPLX_1:48;
        then 1/(1+(p`2/p`1)^2)=(q`1)^2/(p`1)^2/(1+(q`2/q`1)^2)
        by A28,XCMPLX_1:60;
        then
A29:    1/(1+(p`2/p`1)^2)*(1+(q`2/q`1)^2)=(q`1)^2/(p`1)^2 by A15,XCMPLX_1:88;
        now
          assume
A30:      q`1=0;
          then q`2=0 by A12;
          hence contradiction by A12,A30,EUCLID:57,58;
        end;
        then
A31:    (q`1)^2>0 by SQUARE_1:74;
        now per cases;
          case
A32:        p`2=0;
            then (q`2)^2=0 by A15,A26,XCMPLX_1:50;
            then
A33:        q`2=0 by XCMPLX_1:6;
            then p=|[q`1,0]|by A1,A13,A21,A32,EUCLID:57,SQUARE_1:83;
            hence x1=x2 by A33,EUCLID:57;
          end;
          case p`2<>0;
            then
A34:        (p`2)^2>0 by SQUARE_1:74;
            (p`2)^2/(1+(p`2/p`1)^2)/(p`2)^2=(q`2)^2/(p`2)^2/(1+(q`2/q`1)^2)
            by A26,XCMPLX_1:48;

then (p`2)^2/(p`2)^2/(1+(p`2/p`1)^2)=(q`2)^2/(p`2)^2/(1+(q`2/q`1)^2)
            by XCMPLX_1:48;
            then 1/(1+(p`2/p`1)^2)=(q`2)^2/(p`2)^2/(1+(q`2/q`1)^2)
            by A34,XCMPLX_1:60;
            then 1/(1+(p`2/p`1)^2)*(1+(q`2/q`1)^2)=(q`2)^2/(p`2)^2
            by A15,XCMPLX_1:88;
            then (q`1)^2/(q`1)^2/(p`1)^2=(q`2)^2/(p`2)^2/(q`1)^2
            by A29,XCMPLX_1:48;
            then 1/(p`1)^2=(q`2)^2/(p`2)^2/(q`1)^2 by A31,XCMPLX_1:60;
            then 1/(p`1)^2*(p`2)^2=(p`2)^2*((q`2)^2/(p`2)^2)/(q`1)^2
            by XCMPLX_1:75;
            then 1/(p`1)^2*(p`2)^2=(q`2)^2/(q`1)^2 by A34,XCMPLX_1:88;
            then (p`2)^2/(p`1)^2=(q`2)^2/(q`1)^2 by XCMPLX_1:100;
            then (p`2/p`1)^2=(q`2)^2/(q`1)^2 by XCMPLX_1:77;
            then
A35:        (1+(p`2/p`1)^2)=(1+(q`2/q`1)^2) by XCMPLX_1:77;
            then p`1=q`1/sqrt(1+(q`2/q`1)^2)*sqrt(1+(q`2/q`1)^2)
            by A22,A24,XCMPLX_1:88;
            then
A36:        p`1=q`1 by A16,XCMPLX_1:88;
            p`2=q`2/sqrt(1+(q`2/q`1)^2)*sqrt(1+(q`2/q`1)^2)
            by A22,A24,A35,XCMPLX_1:88;
            then p`2=q`2 by A16,XCMPLX_1:88;
            then p=|[q`1,q`2]|by A36,EUCLID:57;
            hence x1=x2 by EUCLID:57;
          end;
        end;
        hence x1=x2;
      end;
      case
A37:    p<>0.REAL 2 & not (p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1);
        then
A38:    p<>0.REAL 2 & p`1<=p`2 & -p`2<=p`1 or p`1>=p`2 & p`1<=-p`2
        by JGRAPH_2:23;
        (|[p`1/sqrt(1+(p`1/p`2)^2),p`2/sqrt(1+(p`1/p`2)^2)]|)`1
        = p`1/sqrt(1+(p`1/p`2)^2) &
        (|[p`1/sqrt(1+(p`1/p`2)^2),p`2/sqrt(1+(p`1/p`2)^2)]|)`2
        = p`2/sqrt(1+(p`1/p`2)^2) by EUCLID:56;
        then
A39:    p`1/sqrt(1+(p`1/p`2)^2)=q`1/sqrt(1+(q`2/q`1)^2) &
        p`2/sqrt(1+(p`1/p`2)^2)=q`2/sqrt(1+(q`2/q`1)^2)
        by A1,A13,A14,A37,Def1;
A40:    1+(p`1/p`2)^2>0 by Lm1;
A41:    sqrt(1+(p`1/p`2)^2)>0 by Lm1,SQUARE_1:93;
        (p`1)^2/(sqrt(1+(p`1/p`2)^2))^2=(q`1/sqrt(1+(q`2/q`1)^2))^2
        by A39,XCMPLX_1:77;
        then (p`1)^2/(sqrt(1+(p`1/p`2)^2))^2=(q`1)^2/(sqrt(1+(q`2/q`1)^2))^2
        by XCMPLX_1:77;
        then (p`1)^2/(1+(p`1/p`2)^2)=(q`1)^2/(sqrt(1+(q`2/q`1)^2))^2
        by A40,SQUARE_1:def 4;
        then
A42:    (p`1)^2/(1+(p`1/p`2)^2)=(q`1)^2/(1+(q`2/q`1)^2) by A15,SQUARE_1:def 4;
        (p`2)^2/(sqrt(1+(p`1/p`2)^2))^2=(q`2/sqrt(1+(q`2/q`1)^2))^2
        by A39,XCMPLX_1:77;
        then (p`2)^2/(sqrt(1+(p`1/p`2)^2))^2=(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
        by XCMPLX_1:77;
        then (p`2)^2/(1+(p`1/p`2)^2)=(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
        by A40,SQUARE_1:def 4;
        then
A43:    (p`2)^2/(1+(p`1/p`2)^2)=(q`2)^2/(1+(q`2/q`1)^2) by A15,SQUARE_1:def 4;
        p`2<>0 by A37;
        then
A44:    (p`2)^2>0 by SQUARE_1:74;
        (p`2)^2/(1+(p`1/p`2)^2)/(p`2)^2=(q`2)^2/(p`2)^2/(1+(q`2/q`1)^2)
        by A43,XCMPLX_1:48;
        then (p`2)^2/(p`2)^2/(1+(p`1/p`2)^2)=(q`2)^2/(p`2)^2/(1+(q`2/q`1)^2)
        by XCMPLX_1:48;
        then 1/(1+(p`1/p`2)^2)=(q`2)^2/(p`2)^2/(1+(q`2/q`1)^2)
        by A44,XCMPLX_1:60;
        then
A45:    1/(1+(p`1/p`2)^2)*(1+(q`2/q`1)^2)=(q`2)^2/(p`2)^2 by A15,XCMPLX_1:88;
A46:    now
          assume
A47:      q`1=0;
          then q`2=0 by A12;
          hence contradiction by A12,A47,EUCLID:57,58;
        end;
        then
A48:    (q`1)^2>0 by SQUARE_1:74;
        now per cases;
          case p`1=0;
            then (q`1)^2=0 by A15,A42,XCMPLX_1:50;
            then
A49:        q`1=0 by XCMPLX_1:6;
            then q`2=0 by A12;
            hence contradiction by A12,A49,EUCLID:57,58;
          end;
          case
A50:        p`1<>0;
            then
A51:        (p`1)^2>0 by SQUARE_1:74;
            (p`1)^2/(1+(p`1/p`2)^2)/(p`1)^2=(q`1)^2/(p`1)^2/(1+(q`2/q`1)^2)
            by A42,XCMPLX_1:48;

then (p`1)^2/(p`1)^2/(1+(p`1/p`2)^2)=(q`1)^2/(p`1)^2/(1+(q`2/q`1)^2)
            by XCMPLX_1:48;
            then 1/(1+(p`1/p`2)^2)=(q`1)^2/(p`1)^2/(1+(q`2/q`1)^2)
            by A51,XCMPLX_1:60;
            then 1/(1+(p`1/p`2)^2)*(1+(q`2/q`1)^2)=(q`1)^2/(p`1)^2
            by A15,XCMPLX_1:88;
            then (q`1)^2/(q`1)^2/(p`1)^2=(q`2)^2/(p`2)^2/(q`1)^2 by A45,
XCMPLX_1:48;
            then 1/(p`1)^2=(q`2)^2/(p`2)^2/(q`1)^2 by A48,XCMPLX_1:60;
            then 1/(p`1)^2*(p`2)^2=(p`2)^2*((q`2)^2/(p`2)^2)/(q`1)^2
            by XCMPLX_1:75;
            then 1/(p`1)^2*(p`2)^2=(q`2)^2/(q`1)^2 by A44,XCMPLX_1:88;
            then (p`2)^2/(p`1)^2=(q`2)^2/(q`1)^2 by XCMPLX_1:100;
            then (p`2/p`1)^2=(q`2)^2/(q`1)^2 by XCMPLX_1:77;
            then
A52:        (p`2/p`1)^2=(q`2/q`1)^2 by XCMPLX_1:77;
            set a=q`2/q`1;
A53:        p`2/p`1*p`1=a*p`1 or p`2/p`1*p`1=(-a)*p`1 by A52,SQUARE_1:109;
A54:        now per cases by A50,A53,XCMPLX_1:88;
              case
A55:            p`2=a*p`1;
                now per cases by A50;
                  case p`1>0;
                    then p`1/p`1<= a*p`1/p`1 & (-(a*p`1))/p`1<=p`1/p`1 or
                    p`1/p`1>=(a*p`1)/p`1 & p`1/p`1<=(-(a*p`1))/p`1
                    by A38,A55,XREAL_1:74;
                    then
A56:                1<= a*p`1/p`1 & (-(a*p`1))/p`1<=1 or
                    1>=(a*p`1)/p`1 & 1<=(-(a*p`1))/p`1 by A50,XCMPLX_1:60;
                    (a*p`1)/p`1=a by A50,XCMPLX_1:90;
                    hence 1<=a & -a<=1 or 1>=a & 1<=-a by A56,XCMPLX_1:188;
                  end;
                  case p`1<0;
                    then p`1/p`1>= a*p`1/p`1 & (-(a*p`1))/p`1>=p`1/p`1 or
                    p`1/p`1<=(a*p`1)/p`1 & p`1/p`1>=(-(a*p`1))/p`1
                    by A38,A55,XREAL_1:75;
                    then
A57:                1>= a*p`1/p`1 & (-(a*p`1))/p`1>=1 or
                    1<=(a*p`1)/p`1 & 1>=(-(a*p`1))/p`1 by A50,XCMPLX_1:60;
                    (a*p`1)/p`1=a by A50,XCMPLX_1:90;
                    hence 1<=a & -a<=1 or 1>=a & 1<=-a by A57,XCMPLX_1:188;
                  end;
                end;
                then 1<=a & -a<=1 or 1>=a & -1>=--a by XREAL_1:26;
                hence 1<=a or -1>=a;
              end;
              case
A58:            p`2=(-a)*p`1;
                now per cases by A50;
                  case p`1>0;
                    then p`1/p`1<= (-a)*p`1/p`1 & (-((-a)*p`1))/p`1<=p`1/p`1
                    or p`1/p`1>=((-a)*p`1)/p`1 & p`1/p`1<=(-((-a)*p`1))/p`1
                    by A38,A58,XREAL_1:74;
                    then
A59:                1<= (-a)*p`1/p`1 & (-((-a)*p`1))/p `1<=1 or

1>=((-a)*p`1)/p`1 & 1<=(-((-a)*p`1))/p`1 by A50,XCMPLX_1:60;
A60:                ((-a)*p`1)/p`1=(-a) by A50,XCMPLX_1:90;
                    1<= (-a) & -(((-a)*p`1)/p`1)<=1 or
                    1>=(-a) & 1<=-(((-a)*p`1)/p`1) by A50,A59,XCMPLX_1:90,188;
                    hence 1<=a & -a<=1 or 1>=a & 1<=-a by A60;
                  end;
                  case p`1<0;
                    then p`1/p`1>= (-a)*p`1/p`1 & (-((-a)*p`1))/p`1>=p`1/p`1
                    or p`1/p`1<=((-a)*p`1)/p`1 & p`1/p`1>=(-((-a)*p`1))/p`1
                    by A38,A58,XREAL_1:75;
                    then
A61:                1>= (-a)*p`1/p`1 & (-((-a)*p`1))/p `1>=1 or

1<=((-a)*p`1)/p`1 & 1>=(-((-a)*p`1))/p`1 by A50,XCMPLX_1:60;
A62:                ((-a)*p`1)/p`1=(-a) by A50,XCMPLX_1:90;
                    1>= (-a) & -(((-a)*p`1)/p`1)>=1 or
                    1<=(-a) & 1>=-(((-a)*p`1)/p`1) by A50,A61,XCMPLX_1:90,188;
                    hence 1<=a & -a<=1 or 1>=a & 1<=-a by A62;
                  end;
                end;
                then 1<=a & -a<=1 or 1>=a & -1>=--a by XREAL_1:26;
                hence 1<=a or -1>=a;
              end;
            end;
A63:        q`1*a<=q`1 & -q`1<=q`1*a or q`1*a>=q`1 & q`1*a<=-q`1
            by A12,A46,XCMPLX_1:88;
A64:        now per cases by A46;
              case
A65:            q`1>0;
                then a*q`1/q`1<=q`1/q`1 & (-q`1)/q`1<=a*q`1/q`1
                or a*q`1/q`1>=q`1/q`1 & a*q`1/q`1<=(-q`1)/q`1
                 by A63,XREAL_1:74;
                then
A66:            a<=q`1/q`1 & (-q`1)/q`1<=a
                or a>=q`1/q`1 & a<=(-q`1)/q`1 by A65,XCMPLX_1:90;
                q`1/q`1=1 by A65,XCMPLX_1:60;
                hence a<=1 & -1<=a or a>=1 & a<=-1 by A66,XCMPLX_1:188;
              end;
              case
A67:            q`1<0;
                then
A68:            a*q`1/q`1>=q`1/q`1 & (-q`1)/q`1>=a*q`1/q`1
                or a*q`1/q`1<=q`1/q`1 & a*q`1/q`1>=(-q`1)/q`1
                 by A63,XREAL_1:75;
A69:            q`1/q`1=1 by A67,XCMPLX_1:60;
                (-q`1)/q`1=-1 by A67,XCMPLX_1:198;
                hence a<=1 & -1<=a or a>=1 & a<=-1 by A67,A68,A69,XCMPLX_1:90;
              end;
            end;
A70:        now per cases by A54,A64,XXREAL_0:1;
              case a=1;
                then (p`2)^2/(p`1)^2=1 by A52,XCMPLX_1:77;
                then
A71:            (p`2)^2=(p`1)^2 by XCMPLX_1:58;
                (p`1/p`2)^2=(p`1)^2/(p`2)^2 by XCMPLX_1:77;
                hence (p`1/p`2)^2=(q`2/q`1)^2 by A52,A71,XCMPLX_1:77;
              end;
              case a=-1;
                then (p`2)^2/(p`1)^2=1 by A52,XCMPLX_1:77;
                then
A72:            (p`2)^2=(p`1)^2 by XCMPLX_1:58;
                (p`1/p`2)^2=(p`1)^2/(p`2)^2 by XCMPLX_1:77;
                hence (p`1/p`2)^2=(q`2/q`1)^2 by A52,A72,XCMPLX_1:77;
              end;
            end;
            then p`1=q`1/sqrt(1+(q`2/q`1)^2)*sqrt(1+(q`2/q`1)^2)
            by A39,A41,XCMPLX_1:88;
            then
A73:        p`1=q`1 by A16,XCMPLX_1:88;
            p`2=q`2/sqrt(1+(q`2/q`1)^2)*sqrt(1+(q`2/q`1)^2)
            by A39,A41,A70,XCMPLX_1:88;
            then p`2=q`2 by A16,XCMPLX_1:88;
            then p=|[q`1,q`2]| by A73,EUCLID:57;
            hence x1=x2 by EUCLID:57;
          end;
        end;
        hence x1=x2;
      end;
    end;
    hence x1=x2;
  end;
  suppose
A74: q<>0.REAL 2 & not (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
    then
A75: Sq_Circ.q=|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]| by Def1;
A76: (|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|)`1
    = q`1/sqrt(1+(q`1/q`2)^2) &
    (|[q`1/sqrt(1+(q`1/q`2)^2),q`2/sqrt(1+(q`1/q`2)^2)]|)`2
    = q`2/sqrt(1+(q`1/q`2)^2) by EUCLID:56;
A77: 1+(q`1/q`2)^2>0 by Lm1;
A78: sqrt(1+(q`1/q`2)^2)>0 by Lm1,SQUARE_1:93;
A79: (q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 & q`1<=-q`2) by A74,JGRAPH_2:23;
    now per cases;
      case p=0.REAL 2;
        then Sq_Circ.p=0.REAL 2 by Def1;
        then
A80:    q`1/sqrt(1+(q`1/q`2)^2)=0 & q`2/sqrt(1+(q`1/q`2)^2)=0
        by A1,A75,EUCLID:56,JGRAPH_2:11;
        (q`1/q`2)^2 >=0 by XREAL_1:65;
        then 1+(q`1/q`2)^2>=1+0 by XREAL_1:9;
        then
A81:    sqrt(1+(q`1/q`2)^2)>=1 by SQUARE_1:83,94;
        q`2= 0 *sqrt(1+(q`1/q`2)^2) by A80,A81,XCMPLX_1:88
          .=0;
        hence contradiction by A74;
      end;
      case
A83:    p<>0.REAL 2 & (p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1);
        then Sq_Circ.p=|[p`1/sqrt(1+(p`2/p`1)^2),p`2/sqrt(1+(p`2/p`1)^2)]|
        by Def1;
        then
A84:    p`1/sqrt(1+(p`2/p`1)^2)=q`1/sqrt(1+(q`1/q`2)^2) &
        p`2/sqrt(1+(p`2/p`1)^2)=q`2/sqrt(1+(q`1/q`2)^2)
        by A1,A75,A76,EUCLID:56;
A85:    1+(p`2/p`1)^2>0 by Lm1;
A86:    sqrt(1+(p`2/p`1)^2)>0 by Lm1,SQUARE_1:93;
        (p`1)^2/(sqrt(1+(p`2/p`1)^2))^2=(q`1/sqrt(1+(q`1/q`2)^2))^2
        by A84,XCMPLX_1:77;
        then (p`1)^2/(sqrt(1+(p`2/p`1)^2))^2=(q`1)^2/(sqrt(1+(q`1/q`2)^2))^2
        by XCMPLX_1:77;
        then (p`1)^2/(1+(p`2/p`1)^2)=(q`1)^2/(sqrt(1+(q`1/q`2)^2))^2
        by A85,SQUARE_1:def 4;
        then
A87:    (p`1)^2/(1+(p`2/p`1)^2)=(q`1)^2/(1+(q`1/q`2)^2) by A77,SQUARE_1:def 4;
        (p`2)^2/(sqrt(1+(p`2/p`1)^2))^2=(q`2/sqrt(1+(q`1/q`2)^2))^2
        by A84,XCMPLX_1:77;
        then (p`2)^2/(sqrt(1+(p`2/p`1)^2))^2=(q`2)^2/(sqrt(1+(q`1/q`2)^2))^2
        by XCMPLX_1:77;
        then (p`2)^2/(1+(p`2/p`1)^2)=(q`2)^2/(sqrt(1+(q`1/q`2)^2))^2
        by A85,SQUARE_1:def 4;
        then
A88:    (p`2)^2/(1+(p`2/p`1)^2)=(q`2)^2/(1+(q`1/q`2)^2) by A77,SQUARE_1:def 4;
        now
          assume
A89:      p`1=0;
          then p`2=0 by A83;
          hence contradiction by A83,A89,EUCLID:57,58;
        end;
        then
A90:    (p`1)^2>0 by SQUARE_1:74;
        (p`1)^2/(1+(p`2/p`1)^2)/(p`1)^2=(q`1)^2/(p`1)^2/(1+(q`1/q`2)^2)
        by A87,XCMPLX_1:48;
        then (p`1)^2/(p`1)^2/(1+(p`2/p`1)^2)=(q`1)^2/(p`1)^2/(1+(q`1/q`2)^2)
        by XCMPLX_1:48;
        then 1/(1+(p`2/p`1)^2)=(q`1)^2/(p`1)^2/(1+(q`1/q`2)^2)
        by A90,XCMPLX_1:60;
        then
A91:    1/(1+(p`2/p`1)^2)*(1+(q`1/q`2)^2)=(q`1)^2/(p`1)^2 by A77,XCMPLX_1:88;
A92:    q`2<>0 by A74;
        then
A93:    (q`2)^2>0 by SQUARE_1:74;
        now per cases;
          case p`2=0;
            then (q`2)^2=0 by A77,A88,XCMPLX_1:50;
            then q`2=0 by XCMPLX_1:6;
            hence contradiction by A74;
          end;
          case
A94:        p`2<>0;
            then
A95:        (p`2)^2>0 by SQUARE_1:74;
            (p`2)^2/(1+(p`2/p`1)^2)/(p`2)^2=(q`2)^2/(p`2)^2/(1+(q`1/q`2)^2)
            by A88,XCMPLX_1:48;

then (p`2)^2/(p`2)^2/(1+(p`2/p`1)^2)=(q`2)^2/(p`2)^2/(1+(q`1/q`2)^2)
            by XCMPLX_1:48;
            then 1/(1+(p`2/p`1)^2)=(q`2)^2/(p`2)^2/(1+(q`1/q`2)^2)
            by A95,XCMPLX_1:60;
            then 1/(1+(p`2/p`1)^2)*(1+(q`1/q`2)^2)=(q`2)^2/(p`2)^2
            by A77,XCMPLX_1:88;
            then (q`2)^2/(q`2)^2/(p`2)^2=(q`1)^2/(p`1)^2/(q`2)^2
            by A91,XCMPLX_1:48;
            then 1/(p`2)^2=(q`1)^2/(p`1)^2/(q`2)^2 by A93,XCMPLX_1:60;
            then 1/(p`2)^2*(p`1)^2=(p`1)^2*((q`1)^2/(p`1)^2)/(q`2)^2
            by XCMPLX_1:75;
            then 1/(p`2)^2*(p`1)^2=(q`1)^2/(q`2)^2 by A90,XCMPLX_1:88;
            then (p`1)^2/(p`2)^2=(q`1)^2/(q`2)^2 by XCMPLX_1:100;
            then (p`1/p`2)^2=(q`1)^2/(q`2)^2 by XCMPLX_1:77;
            then
A96:        (p`1/p`2)^2=(q`1/q`2)^2 by XCMPLX_1:77;
            then
A97:        p`1/p`2=q`1/q`2 or p`1/p`2=-q`1/q`2 by SQUARE_1:109;
            set a=q`1/q`2;
A98:        now per cases by A94,A97,XCMPLX_1:88;
              case
A99:            p`1=a*p`2;
                now per cases by A94;
                  case p`2>0;
                    then p`2/p`2<= a*p`2/p`2 & (-(a*p`2))/p`2<=p`2/p`2 or
                    p`2/p`2>=(a*p`2)/p`2 & p`2/p`2<=(-(a*p`2))/p`2
                    by A83,A99,XREAL_1:74;
                    then
A100:               1<= a*p`2/p`2 & (-(a*p`2))/p`2<=1 or
                    1>=(a*p`2)/p`2 & 1<=(-(a*p`2))/p`2 by A94,XCMPLX_1:60;
                    (a*p`2)/p`2=a by A94,XCMPLX_1:90;
                    hence 1<=a & -a<=1 or 1>=a & 1<=-a by A100,XCMPLX_1:188;
                  end;
                  case p`2<0;
                    then p`2/p`2>= a*p`2/p`2 & (-(a*p`2))/p`2>=p`2/p`2 or
                    p`2/p`2<=(a*p`2)/p`2 & p`2/p`2>=(-(a*p`2))/p`2
                    by A83,A99,XREAL_1:75;
                    then
A101:               1>= a*p`2/p`2 & (-(a*p`2))/p`2>=1 or
                    1<=(a*p`2)/p`2 & 1>=(-(a*p`2))/p`2 by A94,XCMPLX_1:60;
                    (a*p`2)/p`2=a by A94,XCMPLX_1:90;
                    hence 1<=a & -a<=1 or 1>=a & 1<=-a by A101,XCMPLX_1:188;
                  end;
                end;
                then 1<=a & -a<=1 or 1>=a & -1>=--a by XREAL_1:26;
                hence 1<=a or -1>=a;
              end;
              case
A102:           p`1=(-a)*p`2;
                now per cases by A94;
                  case p`2>0;
                    then p`2/p`2<= (-a)*p`2/p`2 & (-((-a)*p`2))/p`2<=p`2/p`2
                    or p`2/p`2>=((-a)*p`2)/p`2 & p`2/p`2<=(-((-a)*p`2))/p`2
                    by A83,A102,XREAL_1:74;
                    then
A103:               1<= (-a)*p`2/p`2 & (-((-a)*p`2))/p `2<=1 or

1>=((-a)*p`2)/p`2 & 1<=(-((-a)*p`2))/p`2 by A94,XCMPLX_1:60;
A104:               ((-a)*p`2)/p`2=(-a) by A94,XCMPLX_1:90;
                    1<= (-a) & -(((-a)*p`2)/p`2)<=1 or
                    1>=(-a) & 1<=-(((-a)*p`2)/p`2) by A94,A103,XCMPLX_1:90,188;
                    hence 1<=a & -a<=1 or 1>=a & 1<=-a by A104;
                  end;
                  case p`2<0;
                    then p`2/p`2>= (-a)*p`2/p`2 & (-((-a)*p`2))/p`2>=p`2/p`2
                    or p`2/p`2<=((-a)*p`2)/p`2 & p`2/p`2>=(-((-a)*p`2))/p`2
                    by A83,A102,XREAL_1:75;
                    then
A105:               1>= (-a)*p`2/p`2 & (-((-a)*p`2))/p `2>=1 or

1<=((-a)*p`2)/p`2 & 1>=(-((-a)*p`2))/p`2 by A94,XCMPLX_1:60;
A106:               ((-a)*p`2)/p`2=(-a) by A94,XCMPLX_1:90;
                    1>= -a & -(((-a)*p`2)/p`2)>=1 or
                    1<=-a & 1>=-(((-a)*p`2)/p`2) by A94,A105,XCMPLX_1:90,188;
                    hence 1<=a & -a<=1 or 1>=a & 1<=-a by A106;
                  end;
                end;
                then 1<=a & -a<=1 or 1>=a & -1>=--a by XREAL_1:26;
                hence 1<=a or -1>=a;
              end;
            end;
A107:       q`2*a<=q`2 & -q`2<=q`2*a or q`2*a>=q`2 & q`2*a<=-q`2
            by A79,A92,XCMPLX_1:88;
A108:       now per cases by A92;
              case
A109:           q`2>0;
                then
A110:           a*q`2/q`2<=q`2/q`2 & (-q`2)/q`2<=a*q`2/q`2

or a*q`2/q`2>=q`2/q`2 & a*q`2/q`2<=(-q`2)/q`2 by A107,XREAL_1:74;
A111:           q`2/q`2=1 by A109,XCMPLX_1:60;
                (-q`2)/q`2=-1 by A109,XCMPLX_1:198;

hence a<=1 & -1<=a or a>=1 & a<=-1 by A109,A110,A111,XCMPLX_1:90;
              end;
              case
A112:           q`2<0;
                then a*q`2/q`2>=q`2/q`2 & (-q`2)/q`2>=a*q`2/q`2

or a*q`2/q`2<=q`2/q`2 & a*q`2/q`2>=(-q`2)/q`2 by A107,XREAL_1:75;
                then a>=q`2/q`2 & (-q`2)/q`2>=a
                or a<=q`2/q`2 & a>=(-q`2)/q`2 by A112,XCMPLX_1:90;
                hence a<=1 & -1<=a or a>=1 & a<=-1 by A112,XCMPLX_1:60,198;
              end;
            end;
A113:       now per cases by A98,A108,XXREAL_0:1;
              case a=1;
                then (p`1)^2/(p`2)^2=1 by A96,XCMPLX_1:77;
                then
A114:           (p`1)^2=(p`2)^2 by XCMPLX_1:58;
                (p`2/p`1)^2=(p`2)^2/(p`1)^2 by XCMPLX_1:77;
                hence (p`2/p`1)^2=(q`1/q`2)^2 by A96,A114,XCMPLX_1:77;
              end;
              case a=-1;
                then (p`1)^2/(p`2)^2=1 by A96,XCMPLX_1:77;
                then
A115:           (p`1)^2=(p`2)^2 by XCMPLX_1:58;
                (p`2/p`1)^2=(p`2)^2/(p`1)^2 by XCMPLX_1:77;
                hence (p`2/p`1)^2=(q`1/q`2)^2 by A96,A115,XCMPLX_1:77;
              end;
            end;
            then p`2=q`2/sqrt(1+(q`1/q`2)^2)*sqrt(1+(q`1/q`2)^2)
            by A84,A86,XCMPLX_1:88;
            then
A116:       p`2=q`2 by A78,XCMPLX_1:88;
            p`1=q`1/sqrt(1+(q`1/q`2)^2)*sqrt(1+(q`1/q`2)^2)
            by A84,A86,A113,XCMPLX_1:88;
            then p`1=q`1 by A78,XCMPLX_1:88;
            then p=|[q`1,q`2]|by A116,EUCLID:57;
