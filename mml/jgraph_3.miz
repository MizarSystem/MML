A67: x in D`` by SUBSET_1:def 5;
    per cases;
    suppose
A68:  x in K0;
A69:  Sq_Circ|D.p=Sq_Circ.p by A67,FUNCT_1:72
        .=f.p by A68,FUNCT_1:72;
      h.p=(g+*f).p by A56,A64,FUNCT_4:35
        .=f.p by A61,A68,FUNCT_4:14;
      hence thesis by A69;
    end;
    suppose not x in K0;
      then not (p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1) by A66;
      then (p`1<=p`2 & -p`2<=p`1 or p`1>=p`2 & p`1<=-p`2)by REAL_2:110;
