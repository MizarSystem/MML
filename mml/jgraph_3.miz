theorem Th54:
  Sq_Circ is Function of TOP-REAL 2,TOP-REAL 2 &
  rng Sq_Circ = the carrier of TOP-REAL 2 &
  for f being Function of TOP-REAL 2,TOP-REAL 2 st f=Sq_Circ holds
  f is_homeomorphism
proof thus Sq_Circ is Function of TOP-REAL 2,TOP-REAL 2;
A1: for f being Function of TOP-REAL 2,TOP-REAL 2 st f=Sq_Circ holds
  rng Sq_Circ=the carrier of TOP-REAL 2 & f is_homeomorphism
  proof
    let f be Function of TOP-REAL 2,TOP-REAL 2;
    assume
A2: f=Sq_Circ;
A3: dom f=the carrier of TOP-REAL 2 by FUNCT_2:def 1;
    consider h being Function of (TOP-REAL 2),(TOP-REAL 2) such that
A4: h=Sq_Circ & h is continuous by Th31;
    consider h2 being Function of (TOP-REAL 2),(TOP-REAL 2) such that
A5: h2=Sq_Circ" & h2 is continuous by Th52;
    the carrier of TOP-REAL 2 c= rng f
    proof
      let y be set;
      assume y in the carrier of TOP-REAL 2;
      then reconsider p2=y as Point of TOP-REAL 2;
      set q=p2;
      now per cases;
        case q=0.REAL 2;
          then q in dom Sq_Circ & y=Sq_Circ.q by A2,A3,Def1;
          hence ex x being set st x in dom Sq_Circ & y=Sq_Circ.x;
        end;
        case
A6:       q<>0.REAL 2 & (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
          set px=|[q`1*sqrt(1+(q`2/q`1)^2),q`2*sqrt(1+(q`2/q`1)^2)]|;
A7:       px`1 = q`1*sqrt(1+(q`2/q`1)^2) &
          px`2 = q`2*sqrt(1+(q`2/q`1)^2) by EUCLID:56;
A8:       sqrt(1+(q`2/q`1)^2)>0 by Lm1,SQUARE_1:93;
          then
A9:       px`2/px`1=q`2/q`1 by A7,XCMPLX_1:92;
A10:      now
            assume px`1=0 & px`2=0;
            then
A11:        q`1*sqrt(1+(q`2/q`1)^2)=0 & q`2*sqrt(1+(q`2/q`1)^2 )=0
            by EUCLID:56;
            then
A12:        q`1=0 by A8,XCMPLX_1:6;
            q`2=0 by A8,A11,XCMPLX_1:6;
            hence contradiction by A6,A12,EUCLID:57,58;
          end;
          q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 &
          q`2*sqrt(1+(q`2/q`1)^2) <= (-q`1)*sqrt(1+(q`2/q`1)^2)
          by A6,A8,XREAL_1:66;
          then q`2<=q`1 & (-q`1)*sqrt(1+(q`2/q`1)^2) <= q`2*sqrt(1+(q`2/q`1)^2
          ) or px`2>=px`1 & px`2<=-px`1 by A7,A8,XREAL_1:66;
          then q`2*sqrt(1+(q`2/q`1)^2) <= q`1*sqrt(1+(q`2/q`1)^2) & -px`1<=px
          `2 or px`2>=px`1 & px`2<=-px`1 by A7,A8,XREAL_1:66;
          then
A13:      Sq_Circ.px=|[px`1/sqrt(1+(px`2/px`1)^2),px`2/sqrt(1+( px`2/px`1)^2)
          ]| by A7,A10,Def1,JGRAPH_2:11;
A14:      px`1/sqrt(1+(px`2/px`1)^2)=q`1 by A7,A8,A9,XCMPLX_1:90;
A15:      px`2/sqrt(1+(px`2/px`1)^2)=q`2 by A7,A8,A9,XCMPLX_1:90;
          dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;

hence ex x being set st x in dom Sq_Circ & y=Sq_Circ.x by A13,A14,A15,EUCLID:57
          ;
        end;
        case
A16:      q<>0.REAL 2 & not(q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
          set px=|[q`1*sqrt(1+(q`1/q`2)^2),q`2*sqrt(1+(q`1/q`2)^2)]|;
A17:      q<>0.REAL 2 & (q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 & q`1<=-q`2)
          by A16,JGRAPH_2:23;
A18:      px`2 = q`2*sqrt(1+(q`1/q`2)^2) &
          px`1 = q`1*sqrt(1+(q`1/q`2)^2) by EUCLID:56;
A19:      sqrt(1+(q`1/q`2)^2)>0 by Lm1,SQUARE_1:93;
          then
A20:      px`1/px`2=q`1/q`2 by A18,XCMPLX_1:92;
A21:      now
            assume px`2=0 & px`1=0;
            then
        q`2*sqrt(1+(q`1/q`2)^2)=0 & q`1*sqrt(1+(q`1/q`2)^2 )=0
            by EUCLID:56;
            then q`2=0 by A19,XCMPLX_1:6;
            hence contradiction by A16;
          end;
          q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 &
          q`1*sqrt(1+(q`1/q`2)^2) <= (-q`2)*sqrt(1+(q`1/q`2)^2)
          by A17,A19,XREAL_1:66;
          then q`1<=q`2 & (-q`2)*sqrt(1+(q`1/q`2)^2) <= q`1*sqrt(1+(q`1/q`2)^2
          ) or px`1>=px`2 & px`1<=-px`2 by A18,A19,XREAL_1:66;
          then q`1*sqrt(1+(q`1/q`2)^2) <= q`2*sqrt(1+(q`1/q`2)^2) & -px`2<=px
          `1 or px`1>=px`2 & px`1<=-px`2 by A18,A19,XREAL_1:66;
          then
A23:      Sq_Circ.px=|[px`1/sqrt(1+(px`1/px`2)^2),px`2/sqrt(1+( px`1/px`2)^2
          )]| by A18,A21,Th14,JGRAPH_2:11;
A24:      px`2/sqrt(1+(px`1/px`2)^2)=q`2 by A18,A19,A20,XCMPLX_1:90;
A25:      px`1/sqrt(1+(px`1/px`2)^2)=q`1 by A18,A19,A20,XCMPLX_1:90;
          dom Sq_Circ=the carrier of TOP-REAL 2 by FUNCT_2:def 1;

hence ex x being set st x in dom Sq_Circ & y=Sq_Circ.x by A23,A24,A25,EUCLID:57
          ;
        end;
      end;
      hence y in rng f by A2,FUNCT_1:def 5;
    end;
    then
A26: rng f=the carrier of TOP-REAL 2 by XBOOLE_0:def 10;
A27: rng f=dom ((f qua Function)") by A2,FUNCT_1:55
      .=dom (f/") by A2,A26,TOPS_2:def 4
      .=[#](TOP-REAL 2) by FUNCT_2:def 1;
    reconsider g=f/" as Function of TOP-REAL 2,TOP-REAL 2;
    g=Sq_Circ" by A2,A27,TOPS_2:def 4;
    hence rng Sq_Circ=the carrier of TOP-REAL 2 &
    f is_homeomorphism by A2,A3,A4,A5,A27,TOPS_2:def 5;
  end;
  hence rng Sq_Circ=the carrier of TOP-REAL 2;
  thus thesis by A1;
end;

Lm14: now
  let pz2, pz1 be real number;
  assume ((pz2)^2+(pz1)^2-1)*(pz2)^2<=(pz1)^2;
  then (pz2)^2*(pz2)^2+(pz2)^2*((pz1)^2-1)-(pz1)^2
  <=(pz1)^2-(pz1)^2 by XREAL_1:11;
  hence ((pz2)^2-1)*((pz2)^2+(pz1)^2)<=0;
end;

Lm15: now
  let px1 be real number;
  assume (px1)^2-1=0;
  then (px1-1)*(px1+1)=0;
  then px1-1=0 or px1+1=0 by XCMPLX_1:6;
  hence px1=1 or px1=-1;
end;

theorem
  for f,g being Function of I[01],TOP-REAL 2,
  C0,KXP,KXN,KYP,KYN being Subset of TOP-REAL 2,
  O,I being Point of I[01] st O=0 & I=1 & f is continuous one-to-one &
  g is continuous one-to-one & C0={p: |.p.|<=1}&
  KXP={q1 where q1 is Point of TOP-REAL 2:
  |.q1.|=1 & q1`2<=q1`1 & q1`2>=-q1`1} &
  KXN={q2 where q2 is Point of TOP-REAL 2:
  |.q2.|=1 & q2`2>=q2`1 & q2`2<=-q2`1} &
  KYP={q3 where q3 is Point of TOP-REAL 2:
  |.q3.|=1 & q3`2>=q3`1 & q3`2>=-q3`1} &
  KYN={q4 where q4 is Point of TOP-REAL 2:
  |.q4.|=1 & q4`2<=q4`1 & q4`2<=-q4`1} & f.O in KXN & f.I in KXP &
  g.O in KYN & g.I in KYP & rng f c= C0 & rng g c= C0 holds rng f meets rng g
proof
  let f,g be Function of I[01],TOP-REAL 2,
  C0,KXP,KXN,KYP,KYN be Subset of TOP-REAL 2, O,I be Point of I[01];
  assume
A1: O=0 & I=1 & f is continuous one-to-one & g is continuous one-to-one &
  C0={p: |.p.|<=1}& KXP={q1 where q1 is Point of TOP-REAL 2:
  |.q1.|=1 & q1`2<=q1`1 & q1`2>=-q1`1} &
  KXN={q2 where q2 is Point of TOP-REAL 2:
  |.q2.|=1 & q2`2>=q2`1 & q2`2<=-q2`1} &
  KYP={q3 where q3 is Point of TOP-REAL 2:
  |.q3.|=1 & q3`2>=q3`1 & q3`2>=-q3`1} &
  KYN={q4 where q4 is Point of TOP-REAL 2:
  |.q4.|=1 & q4`2<=q4`1 & q4`2<=-q4`1} & f.O in KXN & f.I in KXP &
  g.O in KYN & g.I in KYP & rng f c= C0 & rng g c= C0;
  reconsider ff=Sq_Circ"*f as Function of I[01],TOP-REAL 2 by Th39,FUNCT_2:19;
  reconsider gg=Sq_Circ"*g as Function of I[01],TOP-REAL 2 by Th39,FUNCT_2:19;
  consider h1 being Function of (TOP-REAL 2),(TOP-REAL 2) such that
A2: h1=(Sq_Circ") & h1 is continuous by Th52;
A3: dom ff=the carrier of I[01] by FUNCT_2:def 1;
A4: dom gg=the carrier of I[01] by FUNCT_2:def 1;
A5: dom f=the carrier of I[01] by FUNCT_2:def 1;
A6: dom g=the carrier of I[01] by FUNCT_2:def 1;
A7: ff is continuous by A1,A2;
A9: gg is continuous by A1,A2;
A11: (ff.O)=(Sq_Circ").(f.O) by A3,FUNCT_1:22;
  consider p1 being Point of TOP-REAL 2 such that
A12: f.O=p1 &( |.p1.|=1 & p1`2>=p1`1 & p1`2<=-p1`1) by A1;
A13: p1<>0.REAL 2 & (p1`2<=p1`1 & -p1`1<=p1`2 or p1`2>=p1`1 & p1`2<=-p1`1)
  by A12,TOPRNS_1:24;
  then
A14: Sq_Circ".p1=|[p1`1*sqrt(1+(p1`2/p1`1)^2),p1`2*sqrt(1+(p1`2/p1`1)^2 )]|
  by Th38;
A15: (ff.O)`1=-1 & (ff.I)`1=1 & (gg.O)`2=-1 & (gg.I)`2=1
  proof
    set px=ff.O;
    set q=px;
A16: Sq_Circ".p1=q by A3,A12,FUNCT_1:22;
A17: px`1 = p1`1*sqrt(1+(p1`2/p1`1)^2) &
    px`2 = p1`2*sqrt(1+(p1`2/p1`1)^2) by A11,A12,A14,EUCLID:56;
A18: sqrt(1+(p1`2/p1`1)^2)>0 by Lm1,SQUARE_1:93;
A19: now
      assume
A20:  px`1=0 & px`2=0;
      then
A21:  p1`1=0 by A17,A18,XCMPLX_1:6;
      p1`2=0 by A17,A18,A20,XCMPLX_1:6;
      hence contradiction by A13,A21,EUCLID:57,58;
    end;
    p1`2<=p1`1 & -p1`1<=p1`2 or p1`2>=p1`1 &
    p1`2*sqrt(1+(p1`2/p1`1)^2) <= (-p1`1)*sqrt(1+(p1`2/p1`1)^2)
    by A12,A18,XREAL_1:66;
    then
A22: p1`2<=p1`1 & (-p1`1)*sqrt(1+(p1`2/p1`1)^2) <= p1`2*sqrt(1+(p1`2/ p1`1
    )^2) or px`2>=px`1 & px`2<=-px`1 by A17,A18,XREAL_1:66;
    then
A23: px`2<=px`1 & -px`1<=px`2 or px`2>=px`1 & px`2<=-px`1
    by A17,A18,XREAL_1:66;
A24: p1=Sq_Circ.px by A16,Th54,FUNCT_1:54;
A25: Sq_Circ.q=|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|
    by A19,A23,Def1,JGRAPH_2:11;
A26: (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`1
    = q`1/sqrt(1+(q`2/q`1)^2) &
    (|[q`1/sqrt(1+(q`2/q`1)^2),q`2/sqrt(1+(q`2/q`1)^2)]|)`2
    = q`2/sqrt(1+(q`2/q`1)^2) by EUCLID:56;
A27: 1+(q`2/q`1)^2>0 by Lm1;
A28: sqrt(1+(q`2/q`1)^2)>0 by Lm1,SQUARE_1:93;
A29: px`1<>0 by A17,A18,A19,A22,XREAL_1:66;
    (|.p1.|)^2= (q`1/sqrt(1+(q`2/q`1)^2))^2+(q`2/sqrt(1+(q`2/q`1)^2)) ^2
    by A24,A25,A26,JGRAPH_1:46
      .= (q`1)^2/(sqrt(1+(q`2/q`1)^2))^2+(q`2/sqrt(1+(q`2/q`1)^2))^2
    by XCMPLX_1:77
      .= (q`1)^2/(sqrt(1+(q`2/q`1)^2))^2+(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
    by XCMPLX_1:77
      .= (q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(sqrt(1+(q`2/q`1)^2))^2
    by A27,SQUARE_1:def 4
      .= (q`1)^2/(1+(q`2/q`1)^2)+(q`2)^2/(1+(q`2/q`1)^2)
    by A27,SQUARE_1:def 4
      .= ((q`1)^2+(q`2)^2)/(1+(q`2/q`1)^2) by XCMPLX_1:63;
    then ((q`1)^2+(q`2)^2)/(1+(q`2/q`1)^2)*(1+(q`2/q`1)^2)=1 *(1+(q`2/q`1) ^2)
    by A12;
    then (q`1)^2+(q`2)^2=1+(q`2/q`1)^2 by A27,XCMPLX_1:88;
    then (px`1)^2+(px`2)^2-1=(px`2)^2/(px`1)^2 by XCMPLX_1:77;
    then ((px`1)^2+(px`2)^2-1)*(px`1)^2=(px`2)^2 by A29,XCMPLX_1:6,88;
    then
A30: ((px`1)^2-1)*((px`1)^2+(px`2)^2)=0;
    ((px`1)^2+(px`2)^2)<>0 by A19,COMPLEX1:2;
    then
A31: ((px`1)^2-1)=0 by A30,XCMPLX_1:6;
A32: now
      assume
A33:  px`1=1;
      -p1`2>=--p1`1 by A12,XREAL_1:26;
      then -p1`2>0 by A24,A25,A26,A28,A33,REAL_2:127;
      then --p1`2<-0;
      hence contradiction by A12,A24,A25,A26,A28,A33;
    end;
A34: (ff.I)=(Sq_Circ").(f.I) by A3,FUNCT_1:22;
    consider p2 being Point of TOP-REAL 2 such that
A35: f.I=p2 &(|.p2.|=1 & p2`2<=p2`1 & p2`2>=-p2`1) by A1;
A36: p2<>0.REAL 2 & (p2`2<=p2`1 & -p2`1<=p2`2 or p2`2>=p2`1 & p2`2<=-p2`1)
    by A35,TOPRNS_1:24;
    then
A37: Sq_Circ".p2=|[p2`1*sqrt(1+(p2`2/p2`1)^2),p2`2*sqrt(1+(p2`2/p2`1) ^2 )]|
    by Th38;
    set py=ff.I;
A38: py`1 = p2`1*sqrt(1+(p2`2/p2`1)^2) &
    py`2 = p2`2*sqrt(1+(p2`2/p2`1)^2) by A34,A35,A37,EUCLID:56;
A39: sqrt(1+(p2`2/p2`1)^2)>0 by Lm1,SQUARE_1:93;
A40: now
      assume
A41:  py`1=0 & py`2=0;
      then
A42:  p2`1=0 by A38,A39,XCMPLX_1:6;
      p2`2=0 by A38,A39,A41,XCMPLX_1:6;
      hence contradiction by A36,A42,EUCLID:57,58;
    end;
A43: now
      p2`2<=p2`1 & (-p2`1)*sqrt(1+(p2`2/p2`1)^2) <= p2`2*sqrt(1+(p2`2/p2`1 )
      ^2 ) or py`2>=py`1 & py`2<=-py`1 by A35,A39,XREAL_1:66;
      hence p2`2*sqrt(1+(p2`2/p2`1)^2) <= p2`1*sqrt(1+(p2`2/p2`1)^2) & -py
      `1<= py `2 or py`2>=py`1 & py`2<=-py`1 by A38,A39,XREAL_1:66;
    end;
A44: p2=Sq_Circ.py by A34,A35,Th54,FUNCT_1:54;
A45: Sq_Circ.py=|[py`1/sqrt(1+(py`2/py`1)^2),py`2/sqrt(1+(py`2/py`1)^2)]|
    by A38,A40,A43,Def1,JGRAPH_2:11;
A46: (|[py`1/sqrt(1+(py`2/py`1)^2),py`2/sqrt(1+(py`2/py`1)^2)]|)`1
    = py`1/sqrt(1+(py`2/py`1)^2) &
    (|[py`1/sqrt(1+(py`2/py`1)^2),py`2/sqrt(1+(py`2/py`1)^2)]|)`2
    = py`2/sqrt(1+(py`2/py`1)^2) by EUCLID:56;
A47: 1+(py`2/py`1)^2>0 by Lm1;
A48: sqrt(1+(py`2/py`1)^2)>0 by Lm1,SQUARE_1:93;
A49: py`1<>0 by A38,A40,A43;
    (|.p2.|)^2= (py`1/sqrt(1+(py`2/py`1)^2))^2+(py`2/sqrt(1+(py`2/py`1)^2 ) )
    ^2 by A44,A45,A46,JGRAPH_1:46
      .= (py`1)^2/(sqrt(1+(py`2/py`1)^2))^2+(py`2/sqrt(1+(py`2/py`1)^2))^2
    by XCMPLX_1:77
      .= (py`1)^2/(sqrt(1+(py`2/py`1)^2))^2
    +(py`2)^2/(sqrt(1+(py`2/py`1)^2))^2 by XCMPLX_1:77
      .= (py`1)^2/(1+(py`2/py`1)^2)+(py`2)^2/(sqrt(1+(py`2/py`1)^2))^2
