A62: K0=[#](((TOP-REAL 2)|D)|K0) by PRE_TOPC:def 10;
A63: dom g=K1 by A30,FUNCT_2:def 1;
  K1=[#](((TOP-REAL 2)|D)|K1) by PRE_TOPC:def 10;
