        hence thesis by A20,GOBOARD6:4;
      end;
      suppose
A31:    q<>0.REAL 2 & (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
        then
A32:    (Sq_Circ").q=|[q`1*sqrt(1+(q`2/q`1)^2),q`2*sqrt(1+(q`2/q`1 ) ^2)]|
        by Th38;
A33:    ((0.REAL 2) - qz)`1=(0.REAL 2)`1-qz`1 by TOPREAL3:8
          .= -qz`1 by JGRAPH_2:11;
A34:    ((0.REAL 2) - qz)`2=(0.REAL 2)`2-qz`2 by TOPREAL3:8
          .= -qz`2 by JGRAPH_2:11;
A35:    qz`1=q`1*sqrt(1+(q`2/q`1)^2) & qz`2=q`2*sqrt(1+(q`2/q`1)^2)
        by A24,A32,EUCLID:56;
A36:    now per cases by A31;
          case
A37:        q`2<=q`1 & -q`1<=q`2;
            now per cases;
              case 0<=q`2;
                hence (q`2)^2<=(q`1)^2 by A37,SQUARE_1:77;
              end;
              case
A38:            0>q`2;
A39:            --q`1>=-q`2 by A37,XREAL_1:26;
                -0<-q`2 by A38,XREAL_1:26;
                then (-q`2)^2<=(q`1)^2 by A39,SQUARE_1:77;
                hence (q`2)^2<=(q`1)^2;
              end;
            end;
            hence (q`2)^2<=(q`1)^2;
          end;
          case
A40:        q`2>=q`1 & q`2<=-q`1;
            now per cases;
              case 0>=q`2;
                then
A41:            -q`2>= -0;
                -q`2<=-q`1 by A40,XREAL_1:26;
