A22:    -1=py`2 & -1<=py`1 & py`1<=1;
        then 0-1<=py`1;
        then 0<= py`1+1 by XREAL_1:22;
        then
A23:    0/2 <= (py`1-B)/2;
        2-1>=py`1 by A22;
        then 2>= py`1+1 by XREAL_1:21;
        then 2/2 >= (py`1-B)/2 by XREAL_1:74;
        hence t`1 = 0 & t`2 <= 1 & t`2 >= 0 or
        t`1 <= 1 & t`1 >= 0 & t`2 = 1 or t`1 <= 1 & t`1 >= 0 & t`2 = 0 or
        t`1 = 1 & t`2 <= 1 & t`2 >= 0 by A22,A23,EUCLID:56;
      end;
      case
A24:    1=py`2 & -1<=py`1 & py`1<=1;
        then 0-1<=py`1;
        then 0<= py`1+1 by XREAL_1:22;
        then
A25:    0/2 <= (py`1-B)/2;
        2-1>=py`1 by A24;
        then 2>= py`1+1 by XREAL_1:21;
        then 2/2 >= (py`1-B)/2 by XREAL_1:74;
        hence t`1 = 0 & t`2 <= 1 & t`2 >= 0 or
        t`1 <= 1 & t`1 >= 0 & t`2 = 1 or t`1 <= 1 & t`1 >= 0 & t`2 = 0 or
        t`1 = 1 & t`2 <= 1 & t`2 >= 0 by A24,A25,EUCLID:56;
      end;
    end;
    then
A26: t in (R^2-unit_square) by TOPREAL1:20;
    py=|[A*(t`1)+B,C*(t`2)+D]| by A16,EUCLID:57;
    then py=ff.t by A6
      .=f11.t by A26,FUNCT_1:72;
