  then K0=K1 /\ [#]((TOP-REAL 2)|B0) by A1,PRE_TOPC:def 10;
hence thesis by A1,A2,A14,Th26,PRE_TOPC:43;
end;

theorem Th29:for D being non empty Subset of TOP-REAL 2
st D`={0.REAL 2} holds
