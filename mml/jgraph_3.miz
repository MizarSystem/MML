theorem Th52:
ex h being map of TOP-REAL 2,TOP-REAL 2 st h=Sq_Circ" & h is continuous
proof
 reconsider f=(Sq_Circ") as map of (TOP-REAL 2),(TOP-REAL 2) by Th39;
 reconsider D=(the carrier of TOP-REAL 2) \{0.REAL 2}
   as non empty Subset of TOP-REAL 2 by JGRAPH_2:19,XBOOLE_1:36;
 A1: f.(0.REAL 2)=0.REAL 2 by Th38;
 A2: D`= {0.REAL 2} by Th30;
 A3: for p being Point of (TOP-REAL 2)|D holds f.p<>f.(0.REAL 2)
  proof let p be Point of (TOP-REAL 2)|D;
    A4:the carrier of (TOP-REAL 2)|D=[#]((TOP-REAL 2)|D) by PRE_TOPC:12;
    A5: [#]((TOP-REAL 2)|D)=D by PRE_TOPC:def 10;
    then A6:p in the carrier of TOP-REAL 2 & not p in {0.REAL 2}
                         by A4,XBOOLE_0:def 4;
    reconsider q=p as Point of TOP-REAL 2 by A4,A5,XBOOLE_0:def 4;
    A7:not p=0.REAL 2 by A6,TARSKI:def 1;
    per cases;
    suppose A8:not(q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
      then A9:q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 & q`1<=-q`2 by JGRAPH_2:23;
      A10:now assume A11:q`2=0;
        then q`1=0 by A9;
       hence contradiction by A7,A11,EUCLID:57,58;
      end;
      set q9=|[q`1*sqrt(1+(q`1/q`2)^2),q`2*sqrt(1+(q`1/q`2)^2)]|;
      A12:q9`1=q`1*sqrt(1+(q`1/q`2)^2) & q9`2=q`2*sqrt(1+(q`1/q`2)^2)
                                  by EUCLID:56;
         1+(q`1/q`2)^2>0 by Lm1;
      then A13:sqrt(1+(q`1/q`2)^2)>0 by SQUARE_1:93;
         now assume q9=0.REAL 2;
       then 0 *q`2=q`2*sqrt(1+(q`1/q`2)^2) by A12,EUCLID:56,58;
       then 0 *sqrt(1+(q`1/q`2)^2)=q`2*sqrt(1+(q`1/q`2)^2)/sqrt(1+(q`1/q`2)^2)
;
      hence contradiction by A10,A13,XCMPLX_1:90;
     end;
     hence thesis by A1,A7,A8,Th38;
    end;
    suppose A14:(q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
      A15:now assume A16:q`1=0;
        then q`2=0 by A14;
       hence contradiction by A7,A16,EUCLID:57,58;
      end;
      set q9=|[q`1*sqrt(1+(q`2/q`1)^2),q`2*sqrt(1+(q`2/q`1)^2)]|;
      A17:q9`1=q`1*sqrt(1+(q`2/q`1)^2) & q9`2=q`2*sqrt(1+(q`2/q`1)^2)
                                  by EUCLID:56;
         1+(q`2/q`1)^2>0 by Lm1;
      then A18:sqrt(1+(q`2/q`1)^2)>0 by SQUARE_1:93;
         now assume q9=0.REAL 2;
       then 0/sqrt(1+(q`2/q`1)^2)=q`1*sqrt(1+(q`2/q`1)^2)/sqrt(1+(q`2/q`1)^2)
                                by A17,EUCLID:56,58;
      hence contradiction by A15,A18,XCMPLX_1:90;
     end;
     hence thesis by A1,A7,A14,Th38;
  end;
  end;
 A19:ex h being map of (TOP-REAL 2)|D,(TOP-REAL 2)|D
  st h=(Sq_Circ")|D & h is continuous by A2,Th51;
    for V being Subset of TOP-REAL 2 st f.(0.REAL 2) in V & V is open
 ex W being Subset of TOP-REAL 2 st 0.REAL 2 in W
 & W is open & f.:W c= V
  proof let V be Subset of TOP-REAL 2;
    assume A20:f.(0.REAL 2) in V & V is open;
A21:  Euclid 2=MetrStruct(#REAL 2,Pitag_dist 2#) by EUCLID:def 7;
     then reconsider u0=0.REAL 2 as Point of Euclid 2 by EUCLID:25;
     consider r being real number such that A22:r>0 & Ball(u0,r) c= V
       by A1,A20,Lm2,TOPMETR:22;
     reconsider r as Real by XREAL_0:def 1;
        sqrt(2)>0 by SQUARE_1:93;
     then A23: r/sqrt(2)>0 by A22,REAL_2:127;
     reconsider W1=Ball(u0,r), V1=Ball(u0,r/sqrt(2)) as Subset of TOP-REAL 2
                       by A21,EUCLID:25;
     A24:u0 in V1 by A23,GOBOARD6:4;
     A25:V1 is open by GOBOARD6:6;
        f.:V1 c= W1
      proof let z be set;assume z in f.:V1;
        then consider y being set such that
        A26: y in dom f & y in V1 & z=f.y by FUNCT_1:def 12;
        reconsider q=y as Point of TOP-REAL 2 by A26;
        reconsider qy=q as Point of Euclid 2 by A21,EUCLID:25;
           z in rng f by A26,FUNCT_1:def 5;
        then reconsider qz=z as Point of TOP-REAL 2;
        reconsider pz=qz as Point of Euclid 2 by A21,EUCLID:25;
           dist(u0,qy)<r/sqrt(2) by A26,METRIC_1:12;
        then |.(0.REAL 2) - q.|<r/sqrt(2) by JGRAPH_1:45;
        then sqrt((((0.REAL 2) - q)`1)^2+(((0.REAL 2) - q)`2)^2)
                                < r/sqrt(2) by JGRAPH_1:47;
        then sqrt(((0.REAL 2)`1 - q`1)^2+(((0.REAL 2) - q)`2)^2)<r/sqrt(2)
                                  by TOPREAL3:8;
        then sqrt(((0.REAL 2)`1 - q`1)^2+((0.REAL 2)`2 - q`2)^2)<r/sqrt(2)
                                  by TOPREAL3:8;
        then sqrt((- q`1)^2+(0 - q`2)^2)<r/sqrt(2) by JGRAPH_2:11,XCMPLX_1:150
;
        then sqrt((- q`1)^2+(- q`2)^2)<r/sqrt(2) by XCMPLX_1:150;
        then sqrt((q`1)^2+(- q`2)^2)<r/sqrt(2) by SQUARE_1:61;
        then A27:sqrt((q`1)^2+(q`2)^2)<r/sqrt(2) by SQUARE_1:61;
        A28:(q`1)^2 >=0 by SQUARE_1:72;
        A29:(q`2)^2>=0 by SQUARE_1:72;
        then A30:(q`1)^2+(q`2)^2 >=0+0 by A28,REAL_1:55;
        A31:sqrt(2)>0 by SQUARE_1:93;
        then sqrt((q`1)^2+(q`2)^2)*sqrt(2)< r/sqrt(2)*sqrt(2) by A27,REAL_1:70
;
        then sqrt(((q`1)^2+(q`2)^2)*2)< r/sqrt(2)*sqrt(2) by A30,SQUARE_1:97;
        then A32:sqrt(((q`1)^2+(q`2)^2)*2)< r by A31,XCMPLX_1:88;
        per cases;
        suppose q=0.REAL 2; then z=0.REAL 2 by A26,Th38;
         hence thesis by A22,GOBOARD6:4;
        end;
        suppose A33:q<>0.REAL 2 &
          (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
          then A34:(Sq_Circ").q=|[q`1*sqrt(1+(q`2/q`1)^2),q`2*sqrt(1+(q`2/q`1)
^2)]|
                                 by Th38;
          A35:((0.REAL 2) - qz)`1=(0.REAL 2)`1-qz`1 by TOPREAL3:8
                      .= -qz`1 by JGRAPH_2:11,XCMPLX_1:150;
          A36:((0.REAL 2) - qz)`2=(0.REAL 2)`2-qz`2 by TOPREAL3:8
                      .= -qz`2 by JGRAPH_2:11,XCMPLX_1:150;
          A37:qz`1=q`1*sqrt(1+(q`2/q`1)^2) & qz`2=q`2*sqrt(1+(q`2/q`1)^2)
                                 by A26,A34,EUCLID:56;
          A38:now per cases by A33;
          case A39:q`2<=q`1 & -q`1<=q`2;
               now per cases;
            case 0<=q`2;
             hence (q`2)^2<=(q`1)^2 by A39,SQUARE_1:77;
            end;
            case A40:0>q`2;
A41:           --q`1>=-q`2 by A39,REAL_1:50;
                 -0<-q`2 by A40,REAL_1:50;
              then (-q`2)^2<=(q`1)^2 by A41,SQUARE_1:77;
             hence (q`2)^2<=(q`1)^2 by SQUARE_1:61;
            end;
            end;
           hence (q`2)^2<=(q`1)^2;
          end;
          case A42: q`2>=q`1 & q`2<=-q`1;
               now per cases;
            case 0>=q`2;
              then A43: -q`2>= -0 by REAL_1:50;
                 -q`2<=-q`1 by A42,REAL_1:50;
              then (-q`2)^2<=(-q`1)^2 by A43,SQUARE_1:77;
              then (q`2)^2<=(-q`1)^2 by SQUARE_1:61;
             hence (q`2)^2<=(q`1)^2 by SQUARE_1:61;
            end;
            case 0<q`2;
              then (q`2)^2<=(-q`1)^2 by A42,SQUARE_1:77;
             hence (q`2)^2<=(q`1)^2 by SQUARE_1:61;
            end;
            end;
           hence (q`2)^2<=(q`1)^2;
          end;
          end;
          A44:now assume (q`1)^2<=0;
            then (q`1)^2=0 by SQUARE_1:72;
            then A45:q`1=0 by SQUARE_1:73;
            then q`2=0 by A33;
           hence contradiction by A33,A45,EUCLID:57,58;
          end;
          then (q`2)^2/(q`1)^2<=(q`1)^2/(q`1)^2 by A38,REAL_1:73;
          then (q`2/q`1)^2<=(q`1)^2/(q`1)^2 by SQUARE_1:69;
          then (q`2/q`1)^2 <=1 by A44,XCMPLX_1:60;
          then A46:1+(q`2/q`1)^2<=1+1 by REAL_1:55;
          then A47:(q`1)^2*(1+(q`2/q`1)^2)<=(q`1)^2*2 by A28,AXIOMS:25;
          A48:(q`2)^2*(1+(q`2/q`1)^2)<=(q`2)^2*2 by A29,A46,AXIOMS:25;
          A49:1+(q`2/q`1)^2>0 by Lm1;
            (qz`1)^2=(q`1)^2*(sqrt(1+(q`2/q`1)^2))^2 by A37,SQUARE_1:68;
          then A50:(qz`1)^2<=(q`1)^2*2 by A47,A49,SQUARE_1:def 4;
          A51:(qz`1)^2>=0 by SQUARE_1:72;
          A52:(qz`2)^2=(q`2)^2*(sqrt(1+(q`2/q`1)^2))^2 by A37,SQUARE_1:68;
          A53:(qz`2)^2>=0 by SQUARE_1:72;
          A54:(qz`2)^2<=(q`2)^2*2 by A48,A49,A52,SQUARE_1:def 4;
          A55:sqrt((((0.REAL 2) - qz)`1)^2+(((0.REAL 2) - qz)`2)^2)
          =sqrt((qz`1)^2+(-qz`2)^2) by A35,A36,SQUARE_1:61
          .=sqrt((qz`1)^2+(qz`2)^2) by SQUARE_1:61;
          A56:(qz`1)^2+(qz`2)^2<=(q`1)^2*2+(q`2)^2*2 by A50,A54,REAL_1:55;
             0+0<= (qz`1)^2+(qz`2)^2 by A51,A53,REAL_1:55;
          then sqrt((qz`1)^2+(qz`2)^2) <= sqrt((q`1)^2*2+(q`2)^2*2)
                          by A56,SQUARE_1:94;
          then sqrt((qz`1)^2+(qz`2)^2) <= sqrt(((q`1)^2+(q`2)^2)*2)
                          by XCMPLX_1:8;
          then sqrt((((0.REAL 2) - qz)`1)^2+(((0.REAL 2) - qz)`2)^2)<r
                           by A32,A55,AXIOMS:22;
          then |.(0.REAL 2) - qz.|<r by JGRAPH_1:47;
          then dist(u0,pz)<r by JGRAPH_1:45;
          hence thesis by METRIC_1:12;
        end;
        suppose A57:q<>0.REAL 2 &
         not (q`2<=q`1 & -q`1<=q`2 or q`2>=q`1 & q`2<=-q`1);
          then A58:(Sq_Circ").q=|[q`1*sqrt(1+(q`1/q`2)^2),q`2*sqrt(1+(q`1/q`2)
^2)]|
                        by Th38;
          A59:((0.REAL 2) - qz)`1=(0.REAL 2)`1-qz`1 by TOPREAL3:8
                      .= -qz`1 by JGRAPH_2:11,XCMPLX_1:150;
          A60:((0.REAL 2) - qz)`2=(0.REAL 2)`2-qz`2 by TOPREAL3:8
                      .= -qz`2 by JGRAPH_2:11,XCMPLX_1:150;
          A61:qz`1=q`1*sqrt(1+(q`1/q`2)^2) & qz`2=q`2*sqrt(1+(q`1/q`2)^2)
                                 by A26,A58,EUCLID:56;
          A62: q`1<=q`2 & -q`2<=q`1 or q`1>=q`2 & q`1<=-q`2 by A57,JGRAPH_2:23;
          A63:now per cases by A57,JGRAPH_2:23;
          case A64:q`1<=q`2 & -q`2<=q`1;
               now per cases;
            case 0<=q`1;
             hence (q`1)^2<=(q`2)^2 by A64,SQUARE_1:77;
            end;
            case A65:0>q`1;
              A66: --q`2>=-q`1 by A64,REAL_1:50;
                 -0<-q`1 by A65,REAL_1:50;
              then (-q`1)^2<=(q`2)^2 by A66,SQUARE_1:77;
             hence (q`1)^2<=(q`2)^2 by SQUARE_1:61;
            end;
            end;
           hence (q`1)^2<=(q`2)^2;
          end;
          case A67: q`1>=q`2 & q`1<=-q`2;
               now per cases;
            case 0>=q`1;
              then A68: -q`1>= -0 by REAL_1:50;
                 -q`1<=-q`2 by A67,REAL_1:50;
              then (-q`1)^2<=(-q`2)^2 by A68,SQUARE_1:77;
              then (q`1)^2<=(-q`2)^2 by SQUARE_1:61;
             hence (q`1)^2<=(q`2)^2 by SQUARE_1:61;
            end;
            case 0<q`1;
              then (q`1)^2<=(-q`2)^2 by A67,SQUARE_1:77;
             hence (q`1)^2<=(q`2)^2 by SQUARE_1:61;
            end;
            end;
           hence (q`1)^2<=(q`2)^2;
          end;
          end;
          A69:now assume (q`2)^2<=0;
            then (q`2)^2=0 by SQUARE_1:72;
            then q`2=0 by SQUARE_1:73;
           hence contradiction by A57,A62;
          end;
          then (q`1)^2/(q`2)^2<=(q`2)^2/(q`2)^2 by A63,REAL_1:73;
          then (q`1/q`2)^2<=(q`2)^2/(q`2)^2 by SQUARE_1:69;
          then (q`1/q`2)^2 <=1 by A69,XCMPLX_1:60;
          then A70:1+(q`1/q`2)^2<=1+1 by REAL_1:55;
          then A71:(q`2)^2*(1+(q`1/q`2)^2)<=(q`2)^2*2 by A29,AXIOMS:25;
          A72:(q`1)^2*(1+(q`1/q`2)^2)<=(q`1)^2*2 by A28,A70,AXIOMS:25;
            1+(q`1/q`2)^2>0 by Lm1;
          then A73:(sqrt(1+(q`1/q`2)^2))^2=1+(q`1/q`2)^2 by SQUARE_1:def 4;
          then A74:(qz`2)^2<=(q`2)^2*2 by A61,A71,SQUARE_1:68;
          A75:(qz`2)^2>=0 by SQUARE_1:72;
          A76:(qz`1)^2>=0 by SQUARE_1:72;
          A77:(qz`1)^2<=(q`1)^2*2 by A61,A72,A73,SQUARE_1:68;
          A78:sqrt((((0.REAL 2) - qz)`2)^2+(((0.REAL 2) - qz)`1)^2)
           =sqrt((qz`2)^2+(-qz`1)^2) by A59,A60,SQUARE_1:61
          .=sqrt((qz`2)^2+(qz`1)^2) by SQUARE_1:61;
          A79:(qz`2)^2+(qz`1)^2<=(q`2)^2*2+(q`1)^2*2 by A74,A77,REAL_1:55;
             0+0<= (qz`2)^2+(qz`1)^2 by A75,A76,REAL_1:55;
          then sqrt((qz`2)^2+(qz`1)^2) <= sqrt((q`2)^2*2+(q`1)^2*2)
                          by A79,SQUARE_1:94;
          then sqrt((qz`2)^2+(qz`1)^2) <= sqrt(((q`2)^2+(q`1)^2)*2)
                          by XCMPLX_1:8;
          then sqrt((((0.REAL 2) - qz)`2)^2+(((0.REAL 2) - qz)`1)^2)<r
                           by A32,A78,AXIOMS:22;
          then |.(0.REAL 2) - qz.|<r by JGRAPH_1:47;
          then dist(u0,pz)<r by JGRAPH_1:45;
         hence thesis by METRIC_1:12;
      end;
      end;
     then f.:V1 c= V by A22,XBOOLE_1:1;
   hence ex W being Subset of TOP-REAL 2 st 0.REAL 2 in W
       & W is open & f.:W c= V by A24,A25;
  end;
  then f is continuous by A1,A2,A3,A19,Th13;
 hence thesis;
end;

