Lm1: {} is_a_proper_prefix_of <*m*>
proof
  {} is_a_prefix_of <*m*> by XBOOLE_1:2;
  hence thesis by XBOOLE_0:def 8;
end;

definition
  let Z be Tree;
  func Root Z -> Element of Z equals
  {};
  coherence by TREES_1:47;
end;

definition
  let D;
  let T be DecoratedTree of D;
  func Root T -> Element of D equals
  T.(Root dom T);
  coherence;
end;

