proof
  let p,q be FinSequence;
  assume q = p|Seg n;
then A2: dom q = dom p /\ Seg n by RELAT_1:90;
 Seg len q = dom q by Def3;
then  Seg len q c= Seg n by A2,XBOOLE_1:17;
