               LSeg(g,m1)=lq by A209,A255,A256,A257,A258,TOPREAL3:16;
              then p1 in LSeg(g,m1) & LSeg(g,m1) in lg
              by A240,A245,A249,A251,A253,A256;
              hence x in L~g by A240,TARSKI:def 4;
             end;
             suppose ma<>len g;
              then ma<len g by A244,REAL_1:def 5;
              then A259: 1<=ma & ma+1 <= len g & len g1<=ma+1
                by A13,A14,A23,A244,NAT_1:13;
              reconsider qa=g/.ma, qa1=g/.(ma+1) as Point of TOP-REAL 2;
              A260: p1`1<=qa`1 by A244;
               A261: now assume p1`1<=qa1`1;
                 then for q holds q=g/.(ma+1) implies p1`1<=q`1;
                then ma+1<=ma by A244,A259;
                hence contradiction by REAL_2:174;
               end;
              then A262: qa1`1<qa`1 & qa1`1<=p1`1 & p1`1<=qa`1 &
              qa`2=ppi`2 & qa1 `2 = ppi`2 by A212,A244,A259,A260,XXREAL_0:2;
              set lma = {p2: p2`2=ppi`2 & qa1`1<=p2`1 & p2`1<=qa`1};
              A263: qa=|[qa`1,qa`2]| & qa1=|[qa1 `1, qa1 `2]| by EUCLID:57;
                  LSeg(g,ma)=LSeg(qa1,qa) by A259,TOPREAL1:def 5
              .= lma by A262,A263,TOPREAL3:16;
              then x in LSeg(g,ma) & LSeg(g,ma) in lg by A240,A259,A260,A261;
              hence x in L~g by TARSKI:def 4;
             end;
             end;
            hence x in L~g;
           end;
           end;
          hence x in L~g;
         end;
            1<=len g1 by A13,A14,A23,XXREAL_0:2;
        then 1 in dom g1 by A30,FINSEQ_1:3;
        hence g/.1=f1/.1 by A23,FINSEQ_4:83
        .=f/.1 by A14,FINSEQ_4:86;
            j1+1<=i1 by A193,NAT_1:13;
        then A264: 1<=l by XREAL_1:21;
        then A265: l in dom g2 & len g=len g1 + len g2 & len g2 = l
         by A198,FINSEQ_1:3,35,def 3;
        reconsider m1=i1-l as Element of NAT;
        thus g/.len g=g2/.l by A265,FINSEQ_4:84
        .=G*(m1,i2) by A197,A198,A265
        .=f/.len f by A5,A26,A192;
        thus len f<=len g by A5,A14,A23,A264,A265,XREAL_1:9;
       end;
       case i1=j1;
        hence contradiction by A8,A14,A24,A25,A26,A192;
       end;
       case A266: i1<j1;
        then reconsider l=j1-i1 as Element of NAT by INT_1:18;
        deffunc F(Element of NAT) = G*(i1+$1,i2);
        consider g2 such that
        A267: len g2 = l & for n st n in dom g2 holds g2/.n = F(n)
          from PiLambdaD;
A268:    dom g2 = Seg l by A267,FINSEQ_1:def 3;
        take g=g1^g2;
        A269: now let n; assume n in Seg l;
          then A270: 1<=n & n<=l by FINSEQ_1:3;
          then n<=n+i1 & i1+n<=l+i1 by NAT_1:11,XREAL_1:9;
          then n<=i1+n & i1+n<=j1 & j1<=len G
            by A28,FINSEQ_3:27;
          then 1<=i1+n & i1+n<=len G by A270,XXREAL_0:2;
          hence i1+n in dom G by FINSEQ_3:27;
          hence [i1+n,i2] in Indices G by A27,A28,ZFMISC_1:106;
         end;
        A271: dom g2 = Seg len g2 by FINSEQ_1:def 3;
             now let n such that A272: n in dom g2;
          take m=i1+n,k=i2;
          thus [m,k] in Indices G & g2/.n=G*(m,k) by A267,A269,A271,A272;
         end;
        then A273: for n st n in dom g ex i,j st [i,j] in Indices G &
         g/.n=G*(i,j) by A29,GOBOARD1:39;
        A274: now let n; assume
              n in dom g2 & n+1 in dom g2;
          then A275: g2/.n=G*(i1+n,i2) & [i1+n,i2] in Indices G &
          g2/.(n+1)=G*(i1+(n+1),i2) & [i1+(n+1),i2] in Indices G
          by A267,A269,A271;
          let l1,l2,l3,l4 be Element of NAT; assume
              [l1,l2] in Indices G & [l3,l4] in Indices G & g2/.n=G*(l1,l2) &
           g2/.(n+1)=G*(l3,l4);
           then l1=i1+n & l2=i2 & l3=i1+(n+1) & l4=i2 & 0<=1 by A275,
GOBOARD1:21;
          hence abs(l1-l3)+abs(l2-l4)= abs(i1+n-(i1+(n+1)))+0 by ABSVALUE:7
           .= abs(-1)
           .= abs(1) by COMPLEX1:138
           .= 1 by ABSVALUE:def 1;
         end;
          now let l1,l2,l3,l4 be Element of NAT; assume
A276: [l1,l2] in Indices G & [l3,l4] in Indices G & g1/.len g1=G*(l1,l2) &
          g2/.1=G*(l3,l4) & len g1 in dom g1 & 1 in dom g2;
          then g2/.1=G*(i1+1,i2) & [i1+1,i2] in Indices G & f1/.len f1=f/.k
            by A14,A267,A269,A271,FINSEQ_4:86;
           then l1=i1 & l2=i2 & l3=i1+1 & l4=i2 & 0<=1 by A23,A24,A276,
GOBOARD1:21;
          hence abs(l1-l3)+abs(l2-l4)=abs(i1-(i1+1))+0 by ABSVALUE:7
          .=abs(i1-i1+-1)
          .=abs(1) by COMPLEX1:138
          .=1 by ABSVALUE:def 1;
         end;
        then for n st n in dom g & n+1 in dom g holds
         for m,k,i,j st [m,k] in Indices G &
         [i,j] in Indices G & g/.n=G*(m,k) &
          g/.(n+1)=G*(i,j) holds abs(m-i)+abs(k-j)=1 by A29,A274,GOBOARD1:40;
        hence g is_sequence_on G by A273,GOBOARD1:def 11;
        set
        lk={w where w is Point of TOP-REAL 2: w`2=ppi`2 & ppi`1<=w`1 & w`1<=
pj`1};
            c1/.i1 = c1.i1 & c1/.j1 = c1.j1 by A28,A42,PARTFUN1:def 8;
        then A277: c1/.i1=ppi & c1/.j1=pj by A28,MATRIX_1:def 9;
        then A278: y2.i1=ppi`2 & y2.j1=pj`2 & x2.i1=ppi`1 & x2.j1=pj`1
          by A28,A42,A43,GOBOARD1:def 3,def 4;
  then A279: ppi`1<pj`1 & ppi`2=pj`2 & ppi=|[ppi`1,ppi`2]| & pj=|[pj`1,pj`2]|
        by A28,A33,A34,A42,A43,A266,EUCLID:57,GOBOARD1:def 1,def 2;
        A280: LSeg(f,k)=LSeg(ppi,pj) by A13,A24,A26,A192,TOPREAL1:def 5
        .= lk by A279,TOPREAL3:16;
        thus L~g=L~f
         proof
          set lg = {LSeg(g,i): 1<=i & i+1<=len g},
              lf = {LSeg(f,j): 1<=j & j+1<=len f};
          A281: len g = len g1 + len g2 by FINSEQ_1:35;
          A282: now let j; assume
            A283: len g1<=j & j<=len g;
            then reconsider w = j-len g1 as Element of NAT by INT_1:18;
            let p such that
            A284: p=g/.j;
            set u=i1+w;
                 now per cases;
             suppose A285: j=len g1;
                  1<=len g1 by A13,A14,A23,XXREAL_0:2;
              then len g1 in dom g1 by FINSEQ_3:27;
              then A286: g/.len g1 = f1/.len f1 by A23,FINSEQ_4:83
              .= G*(i1,i2) by A14,A24,FINSEQ_4:86;
              hence p`2=G*(i1,i2)`2 by A284,A285;
              thus G*(i1,i2)`1<=p`1 & p`1<=G*
(j1,i2)`1 by A28,A33,A42,A43,A266,A278,A284,A285,A286,GOBOARD1:def 1;
              thus p in rng c1 by A28,A42,A277,A284,A285,A286,PARTFUN2:4;
             end;
             suppose A287: j<>len g1;
then A288:           len g1 < j by A283,REAL_1:def 5;
              A289: j - len g1 <> 0 by A287;
then A290:           w >= 1 by NAT_1:14;
A291:           w + len g1 = j;
              then A292: w <= len g2 by A281,A283,XREAL_1:8;
              then A293: len g1 + 1<=j & g/.j=g2/.w by A288,A290,A291,Th5,
NAT_1:13;
                  w in dom g2 by A290,A292,FINSEQ_3:27;
              then A294: g2/.w=G*(u,i2) & u in dom G by A267,A268,A269;
              then c1/.u = c1.u by A42,PARTFUN1:def 8;
              then A295: c1/.u=G*(u,i2) by A294,MATRIX_1:def 9;
              then A296: x2.i1=G*(i1,i2)`1 & x2.u=G*(u,i2)`1 & x2.j1=G*
(j1,i2)`1 &
              y2.i1=G*(i1,i2)`2 &
              y2.u=G*(u,i2)`2 & y2.j1=G*(j1,i2)`2
       by A28,A42,A43,A277,A294,GOBOARD1:def 3,def 4;
              hence p`2=G*(i1,i2)`2
               by A28,A34,A42,A43,A284,A293,A294,GOBOARD1:def 2;
                  0+1<=w by A289,NAT_1:14;
              then i1<u by REAL_2:174;
              hence G*(i1,i2)`1<=p`1 by A28,A33,A42,A43,A284,A293,A294,A296,
GOBOARD1:def 1;
                   now per cases;
               suppose u=j1;
                hence p`1<=G*(j1,i2)`1 by A284,A290,A291,A292,A294,Th5;
               end;
               suppose A297: u<>j1;
                    u<=i1+l by A267,A292,XREAL_1:9;
                then u<j1 by A297,REAL_1:def 5;
                hence p`1<=G*(j1,i2)`1 by A28,A33,A42,A43,A284,A293,A294,A296,
GOBOARD1:def 1;
               end;
               end;
              hence p`1<=G*(j1,i2)`1;
              thus p in rng c1 by A42,A284,A293,A294,A295,PARTFUN2:4;
