theorem Th1:
 for R being finite Subset of REAL holds
  R <> {} implies R is bounded_above & upper_bound(R) in R &
                             R is bounded_below & lower_bound(R) in R
 proof
  let R be finite Subset of REAL;
  assume
  A1: R <> {};
      P[card R] by Lm3;
  hence thesis by A1;
 end;

begin

canceled;

theorem Th3:
for n being Nat
for f being FinSequence
  holds 1 <= n & n+1 <= len f iff n in dom f & n+1 in dom f
 proof let n be Nat; let f be FinSequence;
  thus 1<=n & n+1 <= len f implies n in dom f & n+1 in dom f
   proof assume
    A1: 1<=n & n+1 <= len f;
        n<=n+1 by NAT_1:11;
    then 1<=n+1 & n<=len f by A1,XXREAL_0:2;
    hence thesis by A1,FINSEQ_3:27;
   end;
  thus thesis by FINSEQ_3:27;
 end;

theorem Th4:
for n being Nat
for f being FinSequence
  holds 1 <= n & n+2 <= len f iff n in dom f & n+1 in dom f & n+2 in dom f
 proof let n be Nat;let f be FinSequence;
  thus 1<=n & n+2 <= len f implies n in dom f & n+1 in dom f & n+2 in dom f
   proof assume
    A1: 1<=n & n+2 <= len f;
        then n<=n+1 & n<=n+2 & n+1<=n+1+1 & n+1+1=n+(1+1) & n+2<=len f
      by NAT_1:11;
    then 1<=n+1 & 1<=n+2 & n+1<=len f & n<=len f by A1,XXREAL_0:2;
    hence thesis by A1,FINSEQ_3:27;
   end;
  assume n in dom f & n+1 in dom f & n+2 in dom f;
  hence thesis by FINSEQ_3:27;
 end;

theorem Th5:
 for D being non empty set, f1,f2 being FinSequence of D, n
  st 1 <= n & n <= len f2 holds (f1^f2)/.(n + len f1) = f2/.n
proof let D be non empty set, f1,f2 be FinSequence of D, n such that
A1: 1 <= n and
A2: n <= len f2;
A3: len f1 < n + len f1 by A1,NAT_1:19;
        len(f1^f2) = len f1 + len f2 by FINSEQ_1:35;
then A4: n + len f1 <= len(f1^f2) by A2,XREAL_1:8;
      n + len f1 >= n by NAT_1:11;
  then n + len f1 >= 1 by A1,XXREAL_0:2;
 hence (f1^f2)/.(n + len f1) = (f1^f2).(n + len f1) by A4,FINSEQ_4:24
   .= f2.(n + len f1 - len f1) by A3,A4,FINSEQ_1:37
   .= f2/.n by A1,A2,FINSEQ_4:24;
end;

theorem
    (for n,m st m>n+1 & n in dom f & n+1 in dom f & m in dom f & m+1 in dom f
holds
  LSeg(f,n) misses LSeg(f,m)) implies f is s.n.c.
 proof assume
  A1: for n,m st m>n+1 & n in dom f & n+1 in dom f & m in dom f & m+1 in dom f
        holds LSeg(f,n) misses LSeg(f,m);
  let n,m be Nat such that
  A2: m>n+1;
A3:   n <= n+1 & m <= m+1 by NAT_1:11;
   per cases;
   suppose n in dom f & n+1 in dom f & m in dom f & m+1 in dom f;
    hence thesis by A1,A2;
   end;
   suppose not(n in dom f & n+1 in dom f & m in dom f & m+1 in dom f);
    then not(1 <= n & n <= len f & 1 <= n+1 & n+1<= len f &
        1 <= m & m <= len f & 1 <= m+1 & m+1<= len f) by FINSEQ_3:27;
     then not(1 <= n & n+1 <= len f & 1 <= m & m+1 <= len f)
       by A3,XXREAL_0:2;
     then LSeg(f,m)={} or LSeg(f,n)={} by TOPREAL1:def 5;
    hence thesis by XBOOLE_1:65;
 end;
 end;

theorem
     f is unfolded s.n.c. one-to-one &
  f/.len f in LSeg(f,i) & i in dom f & i+1 in dom f implies i+1=len f
 proof assume that
  A1: f is unfolded and
  A2: f is s.n.c. and
  A3: f is one-to-one &
      f/.len f in LSeg(f,i) & i in dom f & i+1 in dom f and
  A4: i+1<>len f;
  A5: 1<=i & i<=len f & 1<=i+1 & i+1<=len f by A3,FINSEQ_3:27;
  then i+1<len f by A4,REAL_1:def 5;
  then A6: i+1+1<=len f & i+1+1=i+(1+1) by NAT_1:13;
  then A7: i+1<=len f - 1 & 1<=len f & i+2 <= len f by A5,XREAL_1:21,XXREAL_0:2
;
  then reconsider l=len f - 1 as Element of NAT by INT_1:18;
      i<=l & 0<=1 by A5,XREAL_1:21;
  then A8: 1<=l & l<=len f & l+1=len f by A5,XREAL_1:45,XXREAL_0:2;
  then A9: l in dom f & l+1 in dom f by A7,FINSEQ_3:27;
A10:  f/.(l+1) in LSeg(f,l) by A8,TOPREAL1:27;
      l<>l+1;
  then A11: f/.l<>f/.(l+1) by A3,A9,PARTFUN2:17;
  A12: LSeg(f,i)/\ LSeg(f,i+1)={f/.(i+1)} & i+2 in dom f
   by A1,A5,A6,Th4,TOPREAL1:def 8;
  A13: f/.(i+2) in LSeg(f,i+1) by A5,A6,TOPREAL1:27;
       now per cases;
   suppose A14: l=i+1;
    then f/.len f in LSeg(f,i) /\ LSeg(f,i+1) by A3,A13,XBOOLE_0:def 3;
    hence contradiction by A11,A12,A14,TARSKI:def 1;
   end;
   suppose l<>i+1; then i+1<l by A7,REAL_1:def 5;
    then LSeg(f,i) misses LSeg(f,l) by A2,TOPREAL1:def 9;
    then LSeg(f,i) /\ LSeg(f,l) = {} by XBOOLE_0:def 7;
    hence contradiction by A3,A10,XBOOLE_0:def 3;
   end;
   end;
  hence contradiction;
 end;

theorem Th8:
k<>0 & len f = k+1 implies L~f = L~(f|k) \/ LSeg(f,k)
 proof assume
  A1: k<>0 & len f = k+1;
then A2:  0<k & k<=k+1 by NAT_1:11;
  then A3: 0+1<=k & k<=len f by A1,NAT_1:13;
  then A4: k in Seg k & k in dom f & len(f|k)=k & dom(f|k) = dom(f|k) &
  dom f=Seg len f by FINSEQ_1:3,80,def 3,FINSEQ_3:27;
  set f1 = f|k,
      lf = {LSeg(f,i): 1<=i & i+1 <= len f},
      l1 = {LSeg(f1,j): 1<=j & j+1 <= len f1};
  thus L~f c= L~(f|k) \/ LSeg(f,k)
   proof
    let x; assume x in L~f;
    then consider X be set such that
    A5: x in X & X in lf by TARSKI:def 4;
    consider n such that
    A6: X=LSeg(f,n) & 1<=n & n+1 <= len f by A5;
         now per cases;
     suppose n+1 = len f;
      hence thesis by A1,A5,A6,XBOOLE_0:def 2;
     end;
     suppose n+1 <> len f;
      then A7: n+1 < len f by A6,REAL_1:def 5;
      then A8: n+1<=k & n<=k & n<=n+1 by A1,NAT_1:13,XREAL_1:8;
           n+1 <= k & 1<=n+1 by A1,A6,A7,NAT_1:13;
      then n in dom f1 & n+1 in dom f1 by A4,A6,A8,FINSEQ_3:27;
      then X=LSeg(f1,n) by A4,A6,TOPREAL3:24;
      then X in l1 by A4,A6,A8;
      then x in union l1 by A5,TARSKI:def 4;
      hence thesis by XBOOLE_0:def 2;
     end;
     end;
    hence thesis;
   end;
  let x such that A9: x in L~f1 \/ LSeg(f,k);
       now per cases by A9,XBOOLE_0:def 2;
   suppose x in L~f1;
    then consider X be set such that
    A10: x in X & X in l1 by TARSKI:def 4;
    consider n such that
    A11: X=LSeg(f1,n) & 1<=n & n+1 <= len f1 by A10;
         n<=n+1 & n+1<=len f1 by A11,NAT_1:11;
then A12:    1<=n+1 & n<=len f1 & n+1<=len f by A1,A2,A4,A11,XXREAL_0:2;
     then n in dom f1 & n+1 in dom f1 & n+1 <= len f by A11,FINSEQ_3:27;
    then X=LSeg(f,n) by A4,A11,TOPREAL3:24;
    then X in lf by A11,A12;
    hence thesis by A10,TARSKI:def 4;
   end;
   suppose A13: x in LSeg(f,k);
        LSeg(f,k) in lf by A1,A3;
    hence thesis by A13,TARSKI:def 4;
   end;
   end;
  hence thesis;
 end;

theorem
     1 < k & len f = k+1 & f is unfolded s.n.c. implies
  L~(f|k) /\ LSeg(f,k) = {f/.k}
 proof assume that
  A1: 1<k & len f = k+1 and
  A2: f is unfolded and
  A3: f is s.n.c.;
  set f1 = f|k;
  A4: 1<=k & dom f=Seg len f & dom f1=Seg len f1 & k<=k+1
    by A1,FINSEQ_1:def 3,NAT_1:11;
  then A5: k in dom f & k in Seg k by A1,FINSEQ_1:3;
  reconsider k1=k-1 as Element of NAT by A1,INT_1:18;
  set f2 = f1|k1,
      l2 = {LSeg(f2,m): 1<=m & m+1<=len f2};
      1+1<=k & 0<=1 & 0<1 by A1,NAT_1:13;
  then A6: 1<=k1 & k1<=k & k1<k & k1<=k1+1 by XREAL_1:21,46;
  then A7: 0+1<=k1 & k1+1<=k & 1<=k1+1 & Seg k1 c= Seg k & dom f2=Seg len f2
     by FINSEQ_1:7,def 3,NAT_1:13;
  A8: 0<k1 & k1 in Seg k & k1+1 in Seg k & len f1=k
   by A1,A4,A6,FINSEQ_1:3,80;
  then A9: k1+1=k & 0<>k1 & len f2 = k1 by A6,FINSEQ_1:80;
  A10: L~f1=L~f2 \/ LSeg(f1,k1) & k+1=k1+(1+1) by A8,Th8;
      L~f2 misses LSeg(f,k)
   proof
    assume not thesis;
    then consider x be set such that
    A11: x in L~f2 & x in LSeg(f,k) by XBOOLE_0:3;
    consider X be set such that
    A12: x in X & X in l2 by A11,TARSKI:def 4;
    consider n such that
    A13: X=LSeg(f2,n) & 1<=n & n+1<=len f2 by A12;
    A14:  n+1<k by A6,A9,A13,XXREAL_0:2;
     then n in dom f2 & n+1 in dom f2 & 1<k-n by A13,Th3,XREAL_1:22;
    then LSeg(f2,n)=LSeg(f1,n) & n in dom f1 & n+1 in dom f1
      by A4,A7,A8,A9,TOPREAL3:24;
    then LSeg(f2,n)=LSeg(f,n) by A5,TOPREAL3:24;
    then LSeg(f,n) meets LSeg(f,k) & LSeg(f,n) misses LSeg(f,k)
      by A3,A11,A12,A13,A14,TOPREAL1:def 9,XBOOLE_0:3;
    hence contradiction;
   end;
  then L~f2 /\ LSeg(f,k) = {} by XBOOLE_0:def 7;
  hence L~f1 /\ LSeg(f,k) = {} \/ LSeg(f1,k1) /\ LSeg(f,k)
     by A10,XBOOLE_1:23
  .= LSeg(f,k1) /\ LSeg(f,k1+1) by A4,A5,A8,TOPREAL3:24
  .= {f/.k} by A1,A2,A6,A10,TOPREAL1:def 8;
 end;

theorem
    len f1 < n & n+1 <= len (f1^f2) & m+len f1 = n implies
LSeg(f1^f2,n) = LSeg(f2,m)
 proof
  set f = f1^f2; assume
  A1: len f1 < n & n+1 <= len f & m+len f1 = n;
  then A2: 1<=m & n<=len f & len f1 <n+1
    by NAT_1:13,19;
A3: n+1 <= len f & len f=len f1+len f2
    by A1,FINSEQ_1:35;
A4:  n = m + len f1 & n+1 = m+1+len f1 by A1;
then A5:  m+1<=len f2 by A3,XREAL_1:8;
       m <= m + 1 by NAT_1:11;
   then m <= len f2 & m+1 >= 1 by A5,NAT_1:11,XXREAL_0:2;
  then A6: f/.n=f2/.m & f/.(n+1)=f2/.(m+1) by A2,A4,A5,Th5;
A7:  0+1<=n by A1,NAT_1:13;
  reconsider p=f/.n, q=f/.(n+1) as Point of TOP-REAL 2;
  thus LSeg(f,n)=LSeg(p,q) by A1,A7,TOPREAL1:def 5
  .=LSeg(f2,m) by A2,A5,A6,TOPREAL1:def 5;
 end;

theorem Th11:
L~f c= L~(f^g)
 proof
  set f1 = f^g,
      lf = {LSeg(f,i): 1<=i & i+1 <= len f},
      l1 = {LSeg(f1,j): 1<=j & j+1 <= len f1};
  let x; assume x in L~f;
  then consider X be set such that
  A1: x in X & X in lf by TARSKI:def 4;
  consider n such that
  A2: X=LSeg(f,n) & 1<=n & n+1 <= len f by A1;
  A3: dom f = Seg len f & len f1=len f +len g by FINSEQ_1:35,def 3;
       n<=n+1 & n+1<=len f & 0<=len g by A2,NAT_1:11;
  then 1<=n+1 & n<=len f & len f<=len f1 by A3,REAL_2:173,XXREAL_0:2;
  then n in dom f & n+1 in dom f & len f <= len f1 by A2,FINSEQ_3:27;
  then X=LSeg(f1,n) & n+1 <= len f1 by A2,TOPREAL3:25,XXREAL_0:2;
  then X in l1 by A2;
  hence thesis by A1,TARSKI:def 4;
 end;

theorem
     f is s.n.c. implies f|i is s.n.c.
 proof assume
  A1: f is s.n.c.;
  set f1 = f|i;
  A2: dom f=Seg len f & dom f1=Seg len f1 & f1=f|(Seg i) & 0<=i
    by FINSEQ_1:def 3,def 15;
   per cases;
   suppose len f<i;
    then Seg len f c= Seg i by FINSEQ_1:7;
    hence thesis by A1,A2,RELAT_1:97;
   end;
   suppose A3: i<=len f;
    let n,m be Nat; assume
         m>n+1;
    then A4: LSeg(f,n) misses LSeg(f,m) & dom f1=dom f /\ Seg i
     by A1,A2,FUNCT_1:68,TOPREAL1:def 9;
    then A5: LSeg(f,n) /\ LSeg(f,m) = {} & dom f1=dom f /\ Seg i by
XBOOLE_0:def 7;
         now per cases;
     suppose i=0;
      then not(1 <= n+1 & n+1<= len f1) by A4,FINSEQ_1:4,FINSEQ_3:27;
      then LSeg(f1,n)={} by NAT_1:11,TOPREAL1:def 5;
      hence thesis by XBOOLE_1:65;
     end;
     suppose i<>0;
      then 0+1<=i by A2,NAT_1:13;
      then A6: i in dom f by A3,FINSEQ_3:27;
A7:     n <= n+1 & m <= m+1 by NAT_1:11;
           now per cases;
       suppose A8: n in dom f1;
             now per cases;
         suppose n+1 in dom f1;
          then A9: LSeg(f,n)=LSeg(f1,n) by A6,A8,TOPREAL3:24;
               now per cases;
           suppose A10: m in dom f1;
                 now per cases;
             suppose m+1 in dom f1;
              hence LSeg(f1,n) /\ LSeg(f1,m) = {}
                by A5,A6,A9,A10,TOPREAL3:24;
             end;
             suppose not m+1 in dom f1;
              then not(1 <= m+1 & m+1<= len f1) by FINSEQ_3:27;
              then LSeg(f1,m)={} by NAT_1:11,TOPREAL1:def 5;
              hence LSeg(f1,n) /\ LSeg(f1,m) = {};
             end;
             end;
            hence thesis by XBOOLE_0:def 7;
           end;
           suppose not m in dom f1;
            then not(1 <= m & m <= len f1) by FINSEQ_3:27;
            then not(1 <= m & m+1 <= len f1) by A7,XXREAL_0:2;
            then LSeg(f1,m)={} by TOPREAL1:def 5;
            hence thesis by XBOOLE_1:65;
           end;
           end;
          hence thesis;
         end;
         suppose not n+1 in dom f1;
          then not(1 <= n+1 & n+1<= len f1) by FINSEQ_3:27;
          then LSeg(f1,n)={} by NAT_1:11,TOPREAL1:def 5;
          hence thesis by XBOOLE_1:65;
         end;
         end;
        hence thesis;
       end;
       suppose not n in dom f1;
        then not(1 <= n & n <= len f1) by FINSEQ_3:27;
        then not(1 <= n & n+1 <= len f1) by A7,XXREAL_0:2;
        then LSeg(f1,n)={} by TOPREAL1:def 5;
        hence thesis by XBOOLE_1:65;
       end;
       end;
      hence thesis;
     end;
     end;
  hence thesis;
 end;
 end;

theorem
     f1 is special & f2 is special &
  ((f1/.len f1)`1=(f2/.1)`1 or (f1/.len f1)`2=(f2/.1)`2) implies
     f1^f2 is special
 proof assume that
  A1: f1 is special and
  A2: f2 is special and
  A3: (f1/.len f1)`1=(f2/.1)`1 or (f1/.len f1)`2=(f2/.1)`2;
  set f = f1^f2;
  let n be Nat;
  assume that
  A4: 1 <= n & n+1 <= len f;
  reconsider n as Element of NAT by ORDINAL1:def 13;
   set p =f/.n, q =f/.(n+1);
  A5: dom f1=Seg len f1 & dom f2=Seg len f2 & len f=len f1+len f2 &
  n+1-len f1=n-len f1+1 by FINSEQ_1:35,def 3;
   per cases;
   suppose A6: n+1 <= len f1;
    then n in dom f1 & n+1 in dom f1 by A4,Th3;
    then f1/.n=p & f1/.(n+1)=q by FINSEQ_4:83;
    hence thesis by A1,A4,A6,TOPREAL1:def 7;
   end;
   suppose len f1 < n+1;
    then A7: len f1<=n by NAT_1:13;
    then reconsider n1=n-len f1 as Element of NAT by INT_1:18;
         now per cases;
     suppose A8: n=len f1;
      then n in dom f1 by A4,FINSEQ_3:27;
      then A9: p=f1/.n by FINSEQ_4:83;
          len f2 >= 1 by A4,A5,A8,XREAL_1:8;
      hence p`1=q`1 or p`2=q`2 by A3,A8,A9,Th5;
     end;
     suppose n<>len f1;
      then A10: len f1<n & n<=n+1 by A7,NAT_1:11,REAL_1:def 5;
  then len f1+1<=n & len f1<n+1 & n+1<=len f by A4,NAT_1:13;
      then A11: 1<=n1 & n<=len f by A10,XREAL_1:21,XXREAL_0:2;
A12:   n = n1 + len f1;
A13:   n+1 = n1 + 1 + len f1;
      then A14: n1+1<=len f2 by A4,A5,XREAL_1:8;
          n1 + 1 >= n1 by NAT_1:11;
      then n1 <= len f2 by A14,XXREAL_0:2;
      then A15: f2/.n1=p by A11,A12,Th5;
          n1 + 1 >= 1 by NAT_1:11;
      then f2/.(n1+1)=q by A13,A14,Th5;
      hence p`1=q`1 or p`2=q`2 by A2,A11,A14,A15,TOPREAL1:def 7;
     end;
     end;
  hence thesis;
 end;
 end;

theorem Th14:
f <> {} implies X_axis(f) <> {}
 proof assume
      f <> {} & X_axis(f) = {};
  then len X_axis(f) = len f & len f <> 0 & len X_axis(f) = 0
   by FINSEQ_1:25,GOBOARD1:def 3;
  hence contradiction;
 end;

theorem Th15:
f <> {} implies Y_axis(f) <> {}
 proof assume
      f <> {} & Y_axis(f) = {};
  then len Y_axis(f) = len f & len f <> 0 & len Y_axis(f) = 0
   by FINSEQ_1:25,GOBOARD1:def 4;
  hence contradiction;
 end;

registration let f be non empty FinSequence of TOP-REAL 2;
 cluster X_axis f -> non empty;
 coherence by Th14;
 cluster Y_axis f -> non empty;
 coherence by Th15;
end;

theorem Th16:
 f is special implies
for n st n in dom f & n+1 in dom f holds
 for i,j,m,k st [i,j] in Indices G & [m,k] in Indices G & f/.n=G*(i,j) &
                f/.(n+1)=G*(m,k) holds i=m or k=j
 proof assume
  A1: f is special;
  let n such that
  A2: n in dom f & n+1 in dom f;
  let i,j,m,k such that
  A3: [i,j] in Indices G & [m,k] in Indices G &
  f/.n=G*(i,j) & f/.(n+1)=G*(m,k);
  assume
  A4: i<>m & k<>j;
  reconsider cj = Col(G,j), lm = Line(G,m) as FinSequence of TOP-REAL 2;
  set xj = X_axis(cj),
      yj = Y_axis(cj),
      xm = X_axis(lm),
      ym = Y_axis(lm);
      Indices G = [:dom G,Seg width G:] by MATRIX_1:def 5;
  then A5: i in dom G & j in Seg width G & m in dom G & k in Seg width G &
   len cj=len G & len xj=len cj & dom xj=Seg len xj &
   len lm=width G & len xm=len lm & dom xm=Seg len xm &
   len yj=len cj & dom yj=Seg len yj & len ym=len lm & dom ym=Seg len ym
    by A3,FINSEQ_1:def 3,GOBOARD1:def 3,def 4,MATRIX_1:def 8,def 9,ZFMISC_1:106
;
then A6: dom cj = dom G & dom lm = Seg width G &
     dom xj = dom cj & dom xm = dom lm & dom yj = dom cj & dom ym = dom lm
               by FINSEQ_3:31;
  then cj/.i = cj.i & cj/.m = cj.m & lm/.j = lm.j & lm/.k = lm.k
   by A5,PARTFUN1:def 8;
  then G*(i,j)=cj/.i & G*(m,j)=cj/.m & G*(m,j)=lm/.j & G*(m,k)=lm/.k
     by A5,MATRIX_1:def 8,def 9;
  then A7: xj is increasing & xj.i=G*(i,j)`1 & xj.m=G*(m,j)`1 &
   xm is constant & xm.j=G*(m,j)`1 & xm.k=G*(m,k)`1 &
   ym is increasing & ym.j=G*(m,j)`2 & ym.k=G*(m,k)`2 &
   yj is constant & yj.i=G*(i,j)`2 & yj.m=G*(m,j)`2
     by A5,A6,GOBOARD1:def 3,def 4,def 6,def 7,def 8,def 9;
A8: 1 <= n & n +1 <= len f by A2,FINSEQ_3:27;
       now per cases by A1,A3,A8,TOPREAL1:def 7;
   suppose A9: G*(i,j)`1=G*(m,k)`1;
         now per cases by A4,XXREAL_0:1;
     suppose i>m;
      then G*(m,j)`1<G*(i,j)`1 by A5,A6,A7,GOBOARD1:def 1;
      hence contradiction by A5,A7,A9,GOBOARD1:def 2;
     end;
     suppose i<m;
      then G*(m,j)`1>G*(i,j)`1 by A5,A6,A7,GOBOARD1:def 1;
      hence contradiction by A5,A7,A9,GOBOARD1:def 2;
     end;
     end;
    hence contradiction;
   end;
   suppose A10: G*(i,j)`2=G*(m,k)`2;
         now per cases by A4,XXREAL_0:1;
     suppose k>j;
      then G*(m,j)`2<G*(m,k)`2 by A5,A7,GOBOARD1:def 1;
      hence contradiction by A5,A6,A7,A10,GOBOARD1:def 2;
     end;
     suppose k<j;
      then G*(m,j)`2>G*(m,k)`2 by A5,A7,GOBOARD1:def 1;
      hence contradiction by A5,A6,A7,A10,GOBOARD1:def 2;
     end;
     end;
    hence contradiction;
   end;
   end;
  hence contradiction;
 end;

theorem
    (for n st n in dom f ex i,j st [i,j] in Indices G & f/.n=G*(i,j)) &
 f is special &
 (for n st n in dom f & n+1 in dom f holds f/.n <> f/.(n+1))
 implies
 ex g st g is_sequence_on G & L~f = L~g & g/.1=f/.1 &
        g/.len g=f/.len f & len f<=len g
proof
defpred P[Element of NAT] means
for f st len f=$1 &
 (for n st n in dom f ex i,j st [i,j] in Indices G & f/.n=G*(i,j)) &
 f is special &
 for n st n in dom f & n+1 in dom f holds f/.n <> f/.(n+1)
ex g st g is_sequence_on G & L~f = L~g & g/.1=f/.1 & g/.len g=f/.len f
      & len f<=len g;
A1: P[0]
 proof let f such that
  A2: len f=0 &
  (for n st n in dom f ex i,j st [i,j] in Indices G & f/.n=G*(i,j)) &
  f is special &
 for n st n in dom f & n+1 in dom f holds f/.n <> f/.(n+1);
  take g=f;
      f={} by A2,FINSEQ_1:25;
   then for n holds n in dom g & n+1 in dom g implies (
 for m,k,i,j st [m,k] in Indices G & [i,j] in Indices G & g/.n=G*(m,k) &
       g/.(n+1)=G*(i,j) holds abs(m-i)+abs(k-j)=1) by FINSEQ_1:26;
  hence g is_sequence_on G & L~f = L~g & g/.1=f/.1 & g/.len g=f/.len f
        & len f<=len g by A2,GOBOARD1:def 11;
 end;
A3: for k st P[k] holds P[k+1]
 proof let k such that
  A4: P[k];
  let f such that
A5: len f=k+1 and
A6: for n st n in dom f ex i,j st [i,j] in Indices G & f/.n=G*(i,j) and
A7: f is special and
A8: for n st n in dom f & n+1 in dom f holds f/.n <> f/.(n+1);
  A9: dom f = Seg len f by FINSEQ_1:def 3;
       now per cases;
   suppose k=0;
    then A10: dom f = {1} by A5,FINSEQ_1:4,def 3;
    take g=f;
         now let n; assume
          n in dom g & n+1 in dom g;
      then n=1 & n+1=1 by A10,TARSKI:def 1;
      hence for i1,i2,j1,j2 be Element of NAT st
      [i1,i2] in Indices G & [j1,j2] in Indices G &
       g/.n=G*(i1,i2) & g/.(n+1)=G*(j1,j2) holds abs(i1-j1)+abs(i2-j2)=1;
     end;
    hence g is_sequence_on G & L~f=L~g & g/.1=f/.1 & g/.len g=f/.len f
          & len f<=len g by A6,GOBOARD1:def 11;
   end;
   suppose A11: k<>0;
    then A12: 0<k & k<=k+1 by NAT_1:11;
    then A13: 0+1<=k & k<=len f & k+1 <= len f by A5,NAT_1:13;
    then A14: k in dom f & len (f|k)=k &
    dom(f|k)=Seg len(f|k) & k in Seg k & 1 in Seg k
       by A9,FINSEQ_1:3,80,def 3;
    set f1=f|k;
    A15: now let n; assume A16: n in dom f1;
       then f1/.n=f/.n & n in dom f by A14,FINSEQ_4:86;
      then consider i,j such that
      A17: [i,j] in Indices G & f/.n=G*(i,j) by A6;
      take i,j;
      thus [i,j] in Indices G & f1/.n=G*(i,j) by A14,A16,A17,FINSEQ_4:86;
     end;
     A18: f1 is special
      proof let n be Nat;
       set p =f1/.n, q =f1/.(n+1);
      assume
A19:   1 <= n & n+1 <= len f1;
          n <= n+1 by NAT_1:11;
      then 1 <= n+1 & n <= len f1 by A19,XXREAL_0:2;
       then n in dom f1 & n+1 in dom f1 by A19,FINSEQ_3:27;
then A20:  f1/.n=f/.n & f1/.(n+1)=f/.(n+1) by A14,FINSEQ_4:86;
           n+1 <= len f by A5,A12,A14,A19,XXREAL_0:2;
      hence p`1=q`1 or p`2=q`2 by A7,A19,A20,TOPREAL1:def 7;
     end;
         now let n; assume
A21:   n in dom f1 & n+1 in dom f1;
then A22:  n in dom f & n+1 in dom f by A14,FINSEQ_4:86;
          f1/.n=f/.n & f1/.(n+1)=f/.(n+1) by A14,A21,FINSEQ_4:86;
      hence f1/.n <> f1/.(n+1) by A8,A22;
     end;
    then consider g1 such that
    A23: g1 is_sequence_on G & L~g1=L~f1 & g1/.1=f1/.1 &
       g1/.len g1=f1/.len f1 & len f1<=len g1 by A4,A14,A15,A18;
    consider i1,i2 be Element of NAT such that
    A24: [i1,i2] in Indices G & f/.k=G*(i1,i2) by A6,A14;
        1<=len f by A13,XXREAL_0:2;
    then A25: k+1 in dom f by A5,FINSEQ_3:27;
    then consider j1,j2 be Element of NAT such that
    A26: [j1,j2] in Indices G & f/.(k+1)=G*(j1,j2) by A6;
    A27: Indices G = [:dom G,Seg width G:] by MATRIX_1:def 5;
    then A28: i1 in dom G & i2 in Seg width G &
        j1 in dom G & j2 in Seg width G by A24,A26,ZFMISC_1:106;
    A29: (for n st n in dom g1 ex m,k st [m,k] in Indices G &
    g1/.n=G*(m,k)) &
    for n st n in dom g1 & n+1 in dom g1 holds
     for m,k,i,j st [m,k] in Indices G & [i,j] in Indices G &
     g1/.n = G*(m,k) &
      g1/.(n+1) = G*(i,j) holds abs(m-i)+abs(k-j) = 1 by A23,GOBOARD1:def 11;
    A30: dom g1 = Seg len g1 by FINSEQ_1:def 3;
    reconsider l1 = Line(G,i1), c1 = Col(G,i2) as FinSequence of TOP-REAL 2;
    set x1 = X_axis(l1),
        y1 = Y_axis(l1),
        x2 = X_axis(c1),
        y2 = Y_axis(c1);
A31: x1 is constant
      by A28,GOBOARD1:def 6;
A32: y1 is increasing
      by A28,GOBOARD1:def 8;
A33: x2 is increasing
      by A28,GOBOARD1:def 9;
A34: y2 is constant
      by A28,GOBOARD1:def 7;
A35: dom x1=Seg len x1 by FINSEQ_1:def 3;
A36: dom y1=Seg len y1 by FINSEQ_1:def 3;
A37: len y1=len l1 by GOBOARD1:def 4;
A38: len x1=len l1 by GOBOARD1:def 3;
A39: len l1=width G by MATRIX_1:def 8;
A40: len x2=len c1 by GOBOARD1:def 3;
A41: len y2=len c1 by GOBOARD1:def 4;
     len c1 = len G by MATRIX_1:def 9;
then A42: dom c1 = dom G by FINSEQ_3:31;
A43: dom y1 = dom l1 & dom x1 = dom l1 & dom x2 = dom c1 & dom y2 = dom c1
    by A37,A38,A40,A41,FINSEQ_3:31;
         now per cases by A7,A14,A24,A25,A26,Th16;
     suppose A44: i1=j1;
      set ppi = G*(i1,i2),
          pj = G*(i1,j2);
           now per cases by XXREAL_0:1;
       case A45: i2>j2;
        then reconsider l=i2-j2 as Element of NAT by INT_1:18;
        A46: now let n; assume n in Seg l;
          then A47: 1<=n & n<=l & 0<=j2 by FINSEQ_1:3;
           l<=i2 by XREAL_1:45;
          then n<=i2 by A47,XXREAL_0:2;
          then reconsider w=i2-n as Element of NAT by INT_1:18;
              0<=n & i2-l<=i2-n by A47,REAL_1:92;
          then i2-n<=i2 & i2<=width G & j2<=w & 1<=j2
            by A28,FINSEQ_1:3,XREAL_1:45;
          then 1<=w & w<=width G by XXREAL_0:2;
          then w in Seg width G by FINSEQ_1:3;
          hence i2-n is Element of NAT &
          [i1,i2-n] in Indices G & i2-n in Seg width G
          by A27,A28,ZFMISC_1:106;
         end;
        defpred P1[Element of NAT,set] means
            for m st m=i2-$1 holds $2=G*(i1,m);
        A48: now let n; assume n in Seg l;
          then reconsider m=i2-n as Element of NAT by A46;
          take p=G*(i1,m);
          thus P1[n,p];
         end;
        consider g2 such that
        A49: len g2 = l & for n st n in Seg l holds P1[n,g2/.n]
         from FINSEQ_4:sch 1(A48);
A50: dom g2 = Seg l by A49,FINSEQ_1:def 3;
        take g=g1^g2;
             now let n; assume A51: n in dom g2;
          then reconsider m=i2-n as Element of NAT by A46,A50;
          take k=i1,m;
          thus [k,m] in Indices G & g2/.n=G*(k,m) by A46,A49,A50,A51;
         end;
        then A52: for n st n in dom g ex i,j st [i,j] in Indices G & g/.n=G*(i,
j)
           by A29,GOBOARD1:39;
        A53: now let n; assume
A54:          n in dom g2 & n+1 in dom g2;
          then A55: i2-n is Element of NAT & [i1,i2-n] in Indices G & P1[n,g2/.
n] &
           i2-(n+1) is Element of NAT &
           [i1,i2-(n+1)] in Indices G & P1[n+1,g2/.(n+1)]
            by A46,A49,A50;
          reconsider m1=i2-n ,m2=i2-(n+1) as Element of NAT by A46,A50,A54;
          A56: g2/.n=G*(i1,m1) & g2/.(n+1)=G*(i1,m2) by A49,A50,A54;
          let l1,l2,l3,l4 be Element of NAT; assume
              [l1,l2] in Indices G & [l3,l4] in Indices G & g2/.n=G*(l1,l2) &
           g2/.(n+1)=G*(l3,l4);
           then l1=i1 & l2=m1 & l3=i1 & l4=m2 & 0<=1 by A55,A56,GOBOARD1:21;
          hence abs(l1-l3)+abs(l2-l4)= 0+abs(i2-n-(i2-(n+1))) by ABSVALUE:7
           .= 1 by ABSVALUE:def 1;
         end;
          now let l1,l2,l3,l4 be Element of NAT; assume
          A57: [l1,l2] in Indices G & [l3,l4] in Indices G &
          g1/.len g1=G*(l1,l2) &
          g2/.1=G*(l3,l4) & len g1 in dom g1 & 1 in dom g2;
          then A58: i2-1 is Element of NAT & [i1,i2-1] in Indices G & P1[1,g2/.
1] &
              f1/.len f1=f/.k by A14,A46,A49,A50,FINSEQ_4:86;
          reconsider m1=i2-1 as Element of NAT by A46,A50,A57;
              g2/.1=G*(i1,m1) by A49,A50,A57;
           then l1=i1 & l2=i2 & l3=i1 & l4=m1 & 0<=1 by A23,A24,A57,A58,
GOBOARD1:21;
          hence abs(l1-l3)+abs(l2-l4)=0+abs(i2-(i2-1)) by ABSVALUE:7
          .=1 by ABSVALUE:def 1;
         end;
        then for n st n in dom g & n+1 in dom g holds
         for m,k,i,j st [m,k] in Indices G &
         [i,j] in Indices G & g/.n=G*(m,k) &
          g/.(n+1)=G*(i,j) holds abs(m-i)+abs(k-j)=1 by A29,A53,GOBOARD1:40;
        hence g is_sequence_on G by A52,GOBOARD1:def 11;
        set
        lk={w where w is Point of TOP-REAL 2: w`1=ppi`1 & pj`2<=w`2 &
         w`2<=ppi`2};
        i2 in dom l1 & j2 in dom l1 by A28,A39,FINSEQ_1:def 3;
        then l1/.i2 = l1.i2 & l1/.j2 = l1.j2 by PARTFUN1:def 8;
        then A59: l1/.i2=ppi & l1/.j2=pj by A28,MATRIX_1:def 8;
        then A60: y1.i2=ppi`2 & y1.j2=pj`2 & x1.i2=ppi`1 & x1.j2=pj`1
          by A28,A35,A36,A37,A38,A39,GOBOARD1:def 3,def 4;
        then A61: pj`2<ppi`2 & ppi`1=pj`1 & ppi=|[ppi`1,ppi`2]| &
         pj=|[pj`1,pj`2]|
        by A28,A31,A32,A35,A36,A37,A38,A39,A45,EUCLID:57,GOBOARD1:def 1,def 2;
        A62: LSeg(f,k)=LSeg(pj,ppi) by A13,A24,A26,A44,TOPREAL1:def 5
        .= lk by A61,TOPREAL3:15;
        thus L~g=L~f
         proof
          set lg = {LSeg(g,i): 1<=i & i+1<=len g},
              lf = {LSeg(f,j): 1<=j & j+1<=len f};
          A63: len g = len g1 + len g2 by FINSEQ_1:35;
          A64: now let j; assume
            A65: len g1<=j & j<=len g;
            then reconsider w = j-len g1 as Element of NAT by INT_1:18;
            let p such that
            A66: p=g/.j;
A67:        dom l1 = Seg len l1 by FINSEQ_1:def 3;
                 now per cases;
             suppose A68: j=len g1;
                  1<=len g1 by A13,A14,A23,XXREAL_0:2;
              then len g1 in dom g1 by FINSEQ_3:27;
              then A69: g/.len g1 = f1/.len f1 by A23,FINSEQ_4:83
              .= G*(i1,i2) by A14,A24,FINSEQ_4:86;
              hence p`1=G*(i1,i2)`1 by A66,A68;
              thus G*(i1,j2)`2<=p`2 & p`2<=G*(i1,i2)`2 by A28,A32,A36,A37,A39
,A45,A60,A66,A68,A69,GOBOARD1:def 1;
              thus p in rng l1
               by A28,A39,A59,A66,A67,A68,A69,PARTFUN2:4;
             end;
             suppose A70: j<>len g1;
then A71:           len g1 < j by A65,REAL_1:def 5;
                  j - len g1 <> 0 by A70;
then A72:           w >= 1 by NAT_1:14;
A73:           w + len g1 = j;
              then A74: w <= len g2 by A63,A65,XREAL_1:8;
              then A75: len g1 + 1<=j & g/.j=g2/.w
               by A71,A72,A73,Th5,NAT_1:13;
A76:        w in dom g2 by A72,A74,FINSEQ_3:27;
              then A77: i2-w is Element of NAT & i2-w in Seg width G & P1[w,g2
/.w]
                by A46,A49,A50;
              reconsider u=i2-w as Element of NAT by A46,A50,A76;
                  u in dom l1 by A39,A77,FINSEQ_1:def 3;
              then g2/.w = g2.w & l1/.u = l1.u by A76,PARTFUN1:def 8;
              then A78: g2/.w=G*(i1,u) & l1/.u=G*(i1,u) by A77,MATRIX_1:def 8;
              then A79: x1.i2=ppi`1 & x1.u=G*(i1,u)`1 &
              y1.i2=ppi`2 & y1.u=G*(i1,u)`2 & y1.j2=pj`2
                by A28,A35,A36,A37,A38,A39,A59,A77,GOBOARD1:def 3,def 4;
              hence p`1=G*(i1,i2)`1
               by A28,A31,A35,A38,A39,A66,A75,A77,A78,GOBOARD1:def 2;
                   now per cases;
               suppose u=j2;
                hence G*(i1,j2)`2<=p`2 by A66,A72,A73,A74,A78,Th5;
               end;
               suppose A80: u<>j2;
                    i2-len g2<=u & len g2=l by A49,A74,REAL_1:92;
                then j2<u by A80,REAL_1:def 5;
                hence G*(i1,j2)`2<=
p`2 by A28,A32,A36,A37,A39,A66,A75,A77,A78,A79,GOBOARD1:def 1;
               end;
               end;
              hence G*(i1,j2)`2<=p`2;
               u<i2 by A72,XREAL_1:46;
              hence p`2<=
G*(i1,i2)`2 by A28,A32,A36,A37,A39,A66,A75,A77,A78,A79,GOBOARD1:def 1;
              thus p in rng l1
               by A39,A66,A67,A75,A77,A78,PARTFUN2:4;
             end;
             end;
            hence p`1=ppi`1 & pj`2<=p`2 & p`2<=ppi`2 & p in rng l1;
           end;
          thus L~g c= L~f
           proof let x; assume x in L~g;
            then consider X be set such that
            A81: x in X & X in lg by TARSKI:def 4;
            consider i such that
            A82: X=LSeg(g,i) & 1<=i & i+1<=len g by A81;
                 now per cases;
             suppose A83: i+1 <= len g1;
              then i<=i+1 & i+1<=len g1 by NAT_1:11;
              then 1<=i & i<=len g1 & 1<=i+1 & i+1<=len g1 by A82,XXREAL_0:2;
              then i in dom g1 & i+1 in dom g1 by FINSEQ_3:27;
              then X=LSeg(g1,i) by A82,TOPREAL3:25;
              then X in {LSeg(g1,j): 1<=j & j+1<=len g1} by A82,A83;
              then x in L~f1 & L~f1 c= L~f by A23,A81,TARSKI:def 4,TOPREAL3:27
;
              hence thesis;
             end;
             suppose A84: i+1 > len g1;
              then A85: len g1<=i & i<=i+1 & i+1<=len g by A82,NAT_1:13;
                A86: 1<=i+1 & len g1<=i+1 & i<=len g by A82,A84,NAT_1:13;
              reconsider q1=g/.i, q2=g/.(i+1)
                as Point of TOP-REAL 2;
         A87: q1`1=ppi`1 & pj`2<=q1`2 & q1`2<=ppi`2 & q2`1=ppi`1 & pj`2<=
q2`2 &
              q2`2<=ppi`2 by A64,A85,A86;
              then A88: q1=|[q1`1,q1`2]| & q2=|[q1`1,q2`2]| by EUCLID:57;
              A89: LSeg(g,i)=LSeg(q2,q1) by A82,TOPREAL1:def 5;
                   now per cases by XXREAL_0:1;
               suppose q1`2>q2`2;
                then LSeg(g,i)={p2: p2`1=q1`1 & q2`2<=p2`2 & p2`2<=q1`2}
                  by A88,A89,TOPREAL3:15;
                then consider p2 such that
                A90: p2=x & p2`1=q1`1 & q2`2<=p2`2 & p2`2<=q1`2 by A81,A82;
                    pj`2<=p2`2 & p2`2<=ppi`2 by A87,A90,XXREAL_0:2;
                then x in LSeg(f,k) & LSeg(f,k) in lf by A13,A62,A87,A90;
                hence thesis by TARSKI:def 4;
               end;
               suppose q1`2=q2`2;
                then LSeg(g,i)={q1} by A88,A89,TOPREAL1:7;
                then x=q1 by A81,A82,TARSKI:def 1;
                then x in LSeg(f,k) & LSeg(f,k) in lf by A13,A62,A87;
                hence thesis by TARSKI:def 4;
               end;
               suppose q1`2<q2`2;
                then LSeg(g,i)= {p1: p1`1=q1`1 & q1`2<=p1`2 & p1`2<=q2`2}
                  by A88,A89,TOPREAL3:15;
                then consider p2 such that
                A91: p2=x & p2`1=q1`1 & q1`2<=p2`2 & p2`2<=q2`2 by A81,A82;
                    pj`2<=p2`2 & p2`2<=ppi`2 by A87,A91,XXREAL_0:2;
                then x in LSeg(f,k) & LSeg(f,k) in lf by A13,A62,A87,A91;
                hence thesis by TARSKI:def 4;
               end;
               end;
              hence thesis;
             end;
             end;
            hence thesis;
           end;
          let x; assume x in L~f;
          then A92: x in L~f1 \/ LSeg(f,k) by A5,A11,Th8;
               now per cases by A92,XBOOLE_0:def 2;
           suppose x in L~f1; then x in L~g1 & L~g1 c= L~g by A23,Th11;
            hence x in L~g;
           end;
           suppose x in LSeg(f,k);
            then consider p1 such that
            A93: p1=x & p1`1=ppi`1 & pj`2<=p1`2 & p1`2<=ppi`2 by A62;
            defpred P2[Nat] means
            len g1<=$1 & $1<=len g & for q st q=g/.$1 holds q`2>=p1`2;
            A94: now
            reconsider n=len g1 as Nat;
            take n;
              thus P2[n]
              proof
               thus len g1<=n & n<=len g by A63,REAL_2:173;
               let q such that
               A95: q=g/.n;
                   1<=len g1 by A13,A14,A23,XXREAL_0:2;
               then len g1 in dom g1 by FINSEQ_3:27;
               then q=f1/.len f1 by A23,A95,FINSEQ_4:83
               .=G*(i1,i2) by A14,A24,FINSEQ_4:86;
               hence p1`2<=q`2 by A93;
              end;
             end;
            A96: for n be Nat holds P2[n] implies n<=len g;
            consider ma be Nat such that
            A97: P2[ma] & for n be Nat st P2[n] holds n<=ma
            from NAT_1:sch 6(A96,A94);
            reconsider ma as Element of NAT by ORDINAL1:def 13;
                 now per cases;
             suppose A98: ma=len g;
                  j2+1<=i2 by A45,NAT_1:13;
              then A99: 1<=l & l=len g2 by A49,XREAL_1:21;
              then A100: l in dom g2 & i2-l=j2 by A50,FINSEQ_1:3;
              then A101: g/.ma=g2/.l by A49,A63,A98,FINSEQ_4:84
               .= pj by A49,A50,A100;
              then p1`2<=pj`2 by A97;
              then A102: p1`2=pj`2 & p1=|[p1`1,p1`2]| by A93,EUCLID:57
,XXREAL_0:1;
              A103: ma <= len g + 1 & len g1+1<=ma & ma <= len g
                by A63,A98,A99,NAT_1:11,XREAL_1:9;
           0+1<=ma by A63,A98,A99,XREAL_1:9;
              then reconsider m1=ma-1 as Element of NAT by INT_1:18;
A104:           m1 + 1 = ma;
then A105:           m1 >= len g1 by A103,XREAL_1:8;
                  1<=len g1 by A13,A14,A23,XXREAL_0:2;
              then A106: 1<=m1 by A105,XXREAL_0:2;
A107:           m1 <= len g by A98,A104,NAT_1:11;
              reconsider q=g/.m1 as Point of TOP-REAL 2;
A108:          q`1=ppi`1 by A64,A105,A107;
A109:          pj`2<=q`2 by A64,A105,A107;
A110:          q=|[q`1,q`2]| by EUCLID:57;
              A111: LSeg(g,m1)=LSeg(pj,q) by A98,A101,A104,A106,TOPREAL1:def 5;
              set lq={e where e is Point of TOP-REAL 2:
              e`1=ppi`1 & pj`2<=e`2 &
                      e`2<=q`2};
               LSeg(g,m1)=lq by A61,A108,A109,A110,A111,TOPREAL3:15;
              then p1 in LSeg(g,m1) & LSeg(g,m1) in lg
              by A93,A98,A102,A104,A106,A109;
              hence x in L~g by A93,TARSKI:def 4;
             end;
             suppose ma<>len g;
              then ma<len g by A97,REAL_1:def 5;
              then A112: 1<=ma & ma+1 <= len g & len g1<=ma+1
                by A13,A14,A23,A97,NAT_1:13;
              reconsider qa=g/.ma, qa1=g/.(ma+1) as Point of TOP-REAL 2;
              A113: p1`2<=qa`2 by A97;
               A114: now assume p1`2<=qa1`2;
                 then for q holds q=g/.(ma+1) implies p1`2<=q`2;
                then ma+1<=ma by A97,A112;
                hence contradiction by REAL_2:174;
               end;
              then A115: qa1`2<qa`2 & qa1`2<=p1`2 & p1`2<=qa`2 &
              qa`1=ppi`1 & qa1 `1 = ppi`1 by A64,A97,A112,A113,XXREAL_0:2;
              set lma = {p2: p2`1=ppi`1 & qa1`2<=p2`2 & p2`2<=qa`2};
              A116: qa=|[qa`1,qa`2]| & qa1=|[qa1 `1, qa1 `2]| by EUCLID:57;
                  LSeg(g,ma)=LSeg(qa1,qa) by A112,TOPREAL1:def 5
              .= lma by A115,A116,TOPREAL3:15;
              then x in LSeg(g,ma) & LSeg(g,ma) in lg by A93,A112,A113,A114;
              hence x in L~g by TARSKI:def 4;
             end;
             end;
            hence x in L~g;
           end;
           end;
          hence x in L~g;
         end;
            1<=len g1 by A13,A14,A23,XXREAL_0:2;
        then 1 in dom g1 by FINSEQ_3:27;
        hence g/.1=f1/.1 by A23,FINSEQ_4:83
        .=f/.1 by A14,FINSEQ_4:86;
            j2+1<=i2 by A45,NAT_1:13;
        then A117: 1<=l by XREAL_1:21;
        then A118: l in dom g2 & len g=len g1 + len g2 & len g2 = l
         by A50,FINSEQ_1:3,35,def 3;
        reconsider m1=i2-l as Element of NAT;
        thus g/.len g=g2/.l by A118,FINSEQ_4:84
        .=G*(i1,m1) by A49,A50,A118
        .=f/.len f by A5,A26,A44;
        thus len f<=len g by A5,A14,A23,A117,A118,XREAL_1:9;
       end;
       case i2=j2;
        hence contradiction by A8,A14,A24,A25,A26,A44;
       end;
       case A119: i2<j2;
        then reconsider l=j2-i2 as Element of NAT by INT_1:18;
        deffunc F(Element of NAT) = G*(i1,i2+$1);
        consider g2 such that
        A120: len g2=l & for n st n in dom g2 holds g2/.n=F(n)
         from PiLambdaD;
A121:    dom g2 = Seg l by A120,FINSEQ_1:def 3;
        take g=g1^g2;
        A122: now let n; assume n in Seg l;
          then A123: 1<=n & n<=l by FINSEQ_1:3;
          then n<=n+i2 & i2+n<=l+i2 by NAT_1:11,XREAL_1:9;
          then n<=i2+n & i2+n<=j2 & j2<=width G
            by A28,FINSEQ_1:3;
          then 1<=i2+n & i2+n<=width G by A123,XXREAL_0:2;
          hence i2+n in Seg width G by FINSEQ_1:3;
          hence [i1,i2+n] in Indices G by A27,A28,ZFMISC_1:106;
         end;
        A124: dom g2 = Seg len g2 by FINSEQ_1:def 3;
             now let n such that A125: n in dom g2;
          take m=i1,k=i2+n;
          thus [m,k] in Indices G & g2/.n=G*(m,k) by A120,A122,A124,A125;
         end;
        then A126: for n st n in dom g ex i,j st [i,j] in Indices G &
        g/.n=G*(i,j) by A29,GOBOARD1:39;
        A127: now let n; assume
              n in dom g2 & n+1 in dom g2;
          then A128: g2/.n=G*(i1,i2+n) & [i1,i2+n] in Indices G &
          g2/.(n+1)=G*(i1,i2+(n+1)) & [i1,i2+(n+1)] in Indices G
          by A120,A122,A124;
          let l1,l2,l3,l4 be Element of NAT; assume
              [l1,l2] in Indices G & [l3,l4] in Indices G & g2/.n=G*(l1,l2) &
           g2/.(n+1)=G*(l3,l4);
           then l1=i1 & l2=i2+n & l3=i1 & l4=i2+(n+1) & 0<=1 by A128,
GOBOARD1:21;
          hence abs(l1-l3)+abs(l2-l4)= 0+abs(i2+n-(i2+(n+1))) by ABSVALUE:7
           .= abs(-1)
           .= abs(1) by COMPLEX1:138
           .= 1 by ABSVALUE:def 1;
         end;
          now let l1,l2,l3,l4 be Element of NAT; assume
          A129: [l1,l2] in Indices G & [l3,l4] in Indices G & g1/.len g1=G*(l1,
l2) &
          g2/.1=G*(l3,l4) & len g1 in dom g1 & 1 in dom g2;
          then g2/.1=G*
(i1,i2+1) & [i1,i2+1] in Indices G & f1/.len f1=f/.k
            by A14,A120,A122,A124,FINSEQ_4:86;
           then l1=i1 & l2=i2 & l3=i1 & l4=i2+1 & 0<=1 by A23,A24,A129,
GOBOARD1:21;
          hence abs(l1-l3)+abs(l2-l4)=0+abs(i2-(i2+1)) by ABSVALUE:7
          .=abs(i2-i2+-1)
          .=abs(1) by COMPLEX1:138
          .=1 by ABSVALUE:def 1;
         end;
        then for n st n in dom g & n+1 in dom g holds
         for m,k,i,j st [m,k] in Indices G & [i,j] in Indices G &
         g/.n=G*(m,k) &
          g/.(n+1)=G*(i,j) holds abs(m-i)+abs(k-j)=1 by A29,A127,GOBOARD1:40;
        hence g is_sequence_on G by A126,GOBOARD1:def 11;
        set
        lk={w where w is Point of TOP-REAL 2: w`1=ppi`1 & ppi`2<=w`2 &
        w`2<= pj`2};
            i2 in dom l1 & j2 in dom l1 by A28,A39,FINSEQ_1:def 3;
        then l1/.i2 = l1.i2 & l1/.j2 = l1.j2 by PARTFUN1:def 8;
        then A130: l1/.i2=ppi & l1/.j2=pj by A28,MATRIX_1:def 8;
        then A131: y1.i2=ppi`2 & y1.j2=pj`2 & x1.i2=ppi`1 & x1.j2=pj`1
          by A28,A35,A36,A37,A38,A39,GOBOARD1:def 3,def 4;
   then A132: ppi`2<pj`2 & ppi`1=pj`1 & ppi=|[ppi`1,ppi`2]| & pj=|[pj`1,pj`2]|
        by A28,A31,A32,A35,A36,A37,A38,A39,A119,EUCLID:57,GOBOARD1:def 1,def 2;
        A133: LSeg(f,k)=LSeg(ppi,pj) by A13,A24,A26,A44,TOPREAL1:def 5
        .= lk by A132,TOPREAL3:15;
        thus L~g=L~f
         proof
          set lg = {LSeg(g,i): 1<=i & i+1<=len g},
              lf = {LSeg(f,j): 1<=j & j+1<=len f};
          A134: len g = len g1 + len g2 by FINSEQ_1:35;
          A135: now let j; assume
            A136: len g1<=j & j<=len g;
            then reconsider w = j-len g1 as Element of NAT by INT_1:18;
            let p such that
            A137: p=g/.j;
            set u=i2+w;
A138:        dom l1 = Seg len l1 by FINSEQ_1:def 3;
                 now per cases;
             suppose A139: j=len g1;
                  1<=len g1 by A13,A14,A23,XXREAL_0:2;
              then len g1 in dom g1 by FINSEQ_3:27;
              then A140: g/.len g1 = f1/.len f1 by A23,FINSEQ_4:83
              .= G*(i1,i2) by A14,A24,FINSEQ_4:86;
              hence p`1=G*(i1,i2)`1 by A137,A139;
              thus G*(i1,i2)`2<=p`2 & p`2<=G*
(i1,j2)`2 by A28,A32,A36,A37,A39,A119,A131,A137,A139,A140,GOBOARD1:def 1;
              thus p in rng l1
               by A28,A39,A130,A137,A138,A139,A140,PARTFUN2:4;
             end;
             suppose A141: j<>len g1;
then A142:           len g1 < j by A136,REAL_1:def 5;
              A143: j - len g1 <> 0 by A141;
then A144:           w >= 1 by NAT_1:14;
A145:           w + len g1 = j;
              then A146: w <= len g2 by A134,A136,XREAL_1:8;
              then A147: len g1 + 1<=j & g/.j=g2/.w by A142,A144,A145,Th5,
NAT_1:13;
A148:         w in dom g2 by A144,A146,FINSEQ_3:27;
              then u in Seg width G by A121,A122;
              then u in dom l1 by A39,FINSEQ_1:def 3;
then A149:           g2/.w = g2.w & l1/.u = l1.u by A148,PARTFUN1:def 8;
              A150: g2/.w=G*
(i1,u) & u in Seg width G by A120,A121,A122,A148;
              then A151: l1/.u=G*(i1,u) by A149,MATRIX_1:def 8;
        then A152: x1.i2=ppi`1 & x1.u=G*(i1,u)`1 & y1.i2=ppi`2 & y1.u=G*(i1,u)
`2
               & y1.j2=pj`2
                by A28,A35,A36,A37,A38,A39,A130,A150,GOBOARD1:def 3,def 4;
              hence p`1=G*(i1,i2)`1
               by A28,A31,A35,A38,A39,A137,A147,A150,GOBOARD1:def 2;
                  0+1<=w by A143,NAT_1:14;
              then i2<u by REAL_2:174;
              hence G*(i1,i2)`2<=
p`2 by A28,A32,A36,A37,A39,A137,A147,A150,A152,GOBOARD1:def 1;
                   now per cases;
               suppose u=j2;
                hence p`2<=G*(i1,j2)`2 by A137,A144,A145,A146,A150,Th5;
               end;
               suppose A153: u<>j2;
                    u<=i2+l by A120,A146,XREAL_1:9;
                then u<j2 by A153,REAL_1:def 5;
                hence p`2<=G*(i1,j2)`2 by A28,A32,A36,A37,A39,A137,A147,A150
,A152,GOBOARD1:def 1;
               end;
               end;
              hence p`2<=G*(i1,j2)`2;
              thus p in rng l1
               by A39,A137,A138,A147,A150,A151,PARTFUN2:4;
             end;
             end;
            hence p`1=ppi`1 & ppi`2<=p`2 & p`2<=pj`2 & p in rng l1;
           end;
          thus L~g c= L~f
           proof let x; assume x in L~g;
            then consider X be set such that
            A154: x in X & X in lg by TARSKI:def 4;
            consider i such that
            A155: X=LSeg(g,i) & 1<=i & i+1 <= len g by A154;
                 now per cases;
             suppose A156: i+1 <= len g1;
              then i<=i+1 & i+1<=len g1 by NAT_1:11;
              then 1<=i & i<=len g1 & 1<=i+1 & i+1<=len g1 by A155,XXREAL_0:2;
              then i in dom g1 & i+1 in dom g1 by FINSEQ_3:27;
              then X=LSeg(g1,i) by A155,TOPREAL3:25;
              then X in {LSeg(g1,j): 1<=j & j+1<=len g1} by A155,A156;
              then x in L~f1 & L~f1 c= L~f by A23,A154,TARSKI:def 4,TOPREAL3:27
;
              hence thesis;
             end;
             suppose A157: i+1 > len g1;
              then A158: len g1<=i & i<=i+1 & i+1<=len g by A155,NAT_1:13;
                A159: 1<=i+1 & len g1<=i+1 & i<=len g by A155,A157,NAT_1:13;
              reconsider q1=g/.i, q2=g/.(i+1)
                as Point of TOP-REAL 2;
        A160: q1`1=ppi`1 & ppi`2<=q1`2 & q1`2<=pj`2 & q2`1=ppi`1 & ppi`2<=
q2`2 &
                  q2`2<=pj`2 by A135,A158,A159;
              then A161: q1=|[q1`1,q1`2]| & q2=|[q1`1,q2`2]| by EUCLID:57;
              A162: LSeg(g,i)=LSeg(q2,q1) by A155,TOPREAL1:def 5;
                   now per cases by XXREAL_0:1;
               suppose q1`2>q2`2;
                then LSeg(g,i)={p2: p2`1=q1`1 & q2`2<=p2`2 & p2`2<=q1`2}
                  by A161,A162,TOPREAL3:15;
                then consider p2 such that
                A163: p2=x & p2`1=q1`1 & q2`2<=p2`2 & p2`2<=q1`2 by A154,A155;
