    then
A2: iG2 in NAT by INT_1:16;
    then ind G1+0<=iG1 & iG1<=iG1+iG2 by A1,NAT_1:11,XREAL_1:8;
    then
A3: ind G1<=iG1+iG2 by XXREAL_0:2;
    ind G2+0<=iG2 & iG2<=iG2+iG1 by A2,A1,NAT_1:11,XREAL_1:8;
    then ind G2<=iG1+iG2 by XXREAL_0:2;
