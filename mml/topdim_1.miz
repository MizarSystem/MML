    assume A in G\/G1;
    then A in G or A in G1 by XBOOLE_0:def 3;
    hence thesis by A1,A2,A3,A4,Th11;
  end;
  -1<=i by A1,A3,Th11;
  hence thesis by A5,Th11;
end;

registration
