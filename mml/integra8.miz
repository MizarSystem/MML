theorem
  A = [.-sqrt 2/2,sqrt 2/2.] & dom ((arcsin)`|].-1,1.[) = dom f2 &
  (for x holds x in ].-1,1.[ & f2.x = 1/sqrt(1-x^2)) &
  f2 is_continuous_on A implies
  integral(f2,A) = PI/2
proof
    assume
A1: A = [.-sqrt 2/2,sqrt 2/2.] & dom ((arcsin)`|].-1,1.[) = dom f2 &
    (for x holds x in ].-1,1.[ & f2.x = 1/sqrt(1-x^2)) &
    f2 is_continuous_on A;
A2: sup A = sqrt 2/2 & inf A = -sqrt 2/2 by A1,Th45;
    integral(f2,A)
     = arcsin.(sqrt 2/2) - arcsin.(-sqrt 2/2) by A2,A1,Th40,Th10
    .= arcsin(sqrt 2/2) - arcsin.(-sqrt 2/2) by SIN_COS6:def 2
    .= arcsin(sqrt 2/2) - arcsin(-sqrt 2/2) by SIN_COS6:def 2
    .=(2*PI)/4 by Th11,Th12;
    hence thesis;
end;

theorem
  A = [.-sqrt 2/2,sqrt 2/2.] & dom ((arccos)`|].-1,1.[) = dom f2 &
  (for x holds x in ].-1,1.[ & f2.x = -1/sqrt(1-x^2)) &
  f2 is_continuous_on A implies
  integral(f2,A) = -PI/2
proof
    assume
A1: A = [.-sqrt 2/2,sqrt 2/2.] & dom ((arccos)`|].-1,1.[) = dom f2 &
    (for x holds x in ].-1,1.[ & f2.x = -1/sqrt(1-x^2)) &
    f2 is_continuous_on A;
A2: sup A = sqrt 2/2 & inf A = -sqrt 2/2 by A1,Th45;
    integral(f2,A)
     = arccos.(sqrt 2/2)-arccos.(-sqrt 2/2) by A2,Th41,A1,Th10
    .= arccos(sqrt 2/2) - arccos.(-sqrt 2/2) by SIN_COS6:def 4
    .= arccos(sqrt 2/2) - arccos(-sqrt 2/2) by SIN_COS6:def 4
    .=(-2*PI)/4 by Th17,Th18;
    hence thesis;
end;

theorem Th73:
  f is_differentiable_on Z & g is_differentiable_on Z & A c= Z &
  f`|Z is_integrable_on A & f`|Z is_bounded_on A & g`|Z is_integrable_on A &
  g`|Z is_bounded_on A implies
  integral(f`|Z+g`|Z,A) = f.(sup A) - f.(inf A) + g.(sup A) - g.(inf A)
proof
    assume that
A1: f is_differentiable_on Z and
A2: g is_differentiable_on Z and
A3: A c= Z and
A4: f`|Z is_integrable_on A and
A5: f`|Z is_bounded_on A and
A6: g`|Z is_integrable_on A and
A7: g`|Z is_bounded_on A;
    f is_continuous_on Z & g is_continuous_on Z by A1,A2,FDIFF_1:33;then
    f is_continuous_on A & g is_continuous_on A by A3,FCONT_1:17;
    then f is_integrable_on A & g is_integrable_on A by INTEGRA5:11;then
A10:f||A is_integrable_on A & g||A is_integrable_on A &
    (f`|Z)||A is_integrable_on A & (g`|Z)||A is_integrable_on A
     by A4,A6,INTEGRA5:def 2;
    A c= dom(f`|Z) & A c= dom(g`|Z) by A1,A2,A3,FDIFF_1:def 8;
    then (f`|Z)||A is total & (g`|Z)||A is total by INTEGRA5:6;then
A16: (f`|Z)||A is Function of A,REAL & (g`|Z)||A is Function of A,REAL
     by FUNCT_2:131;
B1: (f`|Z)||A is_bounded_on A & (g`|Z)||A is_bounded_on A
         by A5,A7,INTEGRA5:9;
    integral(f`|Z+g`|Z,A)
    = integral((f`|Z)||A+(g`|Z)||A) by INTEGRA5:5
   .= integral(f`|Z,A) + integral(g`|Z,A) by A10,A16,B1,INTEGRA1:59
   .= f.(sup A) - f.(inf A)+ integral(g`|Z,A) by A1,A3,A4,A5,INTEGRA5:13
   .= f.(sup A) - f.(inf A)+ (g.(sup A) - g.(inf A))
      by A2,A3,A6,A7,INTEGRA5:13;
   hence thesis;
end;

theorem Th74:
  f is_differentiable_on Z & g is_differentiable_on Z & A c= Z &
  f`|Z is_integrable_on A & f`|Z is_bounded_on A & g`|Z is_integrable_on A &
  g`|Z is_bounded_on A implies
  integral(f`|Z-g`|Z,A) = f.(sup A) - f.(inf A) - (g.(sup A) - g.(inf A))
proof
    assume that
A1: f is_differentiable_on Z and
A2: g is_differentiable_on Z and
A3: A c= Z and
A4: f`|Z is_integrable_on A and
A5: f`|Z is_bounded_on A and
A6: g`|Z is_integrable_on A and
A7: g`|Z is_bounded_on A;
    f is_continuous_on Z & g is_continuous_on Z by A1,A2,FDIFF_1:33;then
    f is_continuous_on A & g is_continuous_on A by A3,FCONT_1:17;
    then f is_integrable_on A & g is_integrable_on A by INTEGRA5:11;then
A10:f||A is_integrable_on A & g||A is_integrable_on A &
    (f`|Z)||A is_integrable_on A & (g`|Z)||A is_integrable_on A
     by A4,A6,INTEGRA5:def 2;
    A c= dom(f`|Z) & A c= dom(g`|Z) by A1,A2,A3,FDIFF_1:def 8;
    then (f`|Z)||A is total & (g`|Z)||A is total by INTEGRA5:6;then
A16: (f`|Z)||A is Function of A,REAL & (g`|Z)||A is Function of A,REAL
      by FUNCT_2:131;
B1: (f`|Z)||A is_bounded_on A & (g`|Z)||A is_bounded_on A
         by A5,A7,INTEGRA5:9;
    integral(f`|Z-g`|Z,A) = integral((f`|Z)||A-(g`|Z)||A) by RFUNCT_1:63
   .= integral(f`|Z,A) - integral((g`|Z)||A) by A10,A16,B1,INTEGRA2:33
   .= f.(sup A) - f.(inf A)- integral(g`|Z,A) by A1,A3,A4,A5,INTEGRA5:13
   .= f.(sup A) - f.(inf A)- (g.(sup A) - g.inf A) by A2,A3,A6,A7,INTEGRA5:13;
   hence thesis;
end;

theorem Th75:
  f is_differentiable_on Z & A c= Z &
  f`|Z is_integrable_on A & f`|Z is_bounded_on A implies
  integral((r(#)(f`|Z)),A) = r*f.(sup A) - r*f.(inf A)
proof
    assume that
A1: f is_differentiable_on Z and
A2: A c= Z and
A3: f`|Z is_integrable_on A and
A4: f`|Z is_bounded_on A;
    f is_continuous_on Z by A1,FDIFF_1:33;then
    f is_continuous_on A by A2,FCONT_1:17;
    then f is_integrable_on A by INTEGRA5:11;then
A10:f||A is_integrable_on A &
    (f`|Z)||A is_integrable_on A by A3,INTEGRA5:def 2;
    A c= dom(f`|Z) by A1,A2,FDIFF_1:def 8;
    then (f`|Z)||A is total by INTEGRA5:6;then
A16: (f`|Z)||A is Function of A,REAL by FUNCT_2:131;
B1: (f`|Z)||A is_bounded_on A by A4,INTEGRA5:9;
    integral((r(#)(f`|Z)),A) = integral (r(#)((f`|Z)||A)) by RFUNCT_1:65
    .= r * integral ((f`|Z),A) by A10,A16,B1,INTEGRA2:31
    .= r*(f.(sup A) - f.(inf A)) by A1,A2,A3,A4,INTEGRA5:13;
    hence thesis;
end;

Lm8:sin is_continuous_on A
proof
  sin is_continuous_on REAL by SIN_COS:73,FDIFF_1:33;
  hence thesis by FCONT_1:17;
end;

Lm8A:
  sin is_integrable_on A & sin is_bounded_on A
proof
  sin is_continuous_on A by Lm8;
  hence thesis by INTEGRA5:10,11;
end;

theorem Th76:
  integral(sin+cos,A) =
    (-cos).(sup A) - (-cos).(inf A) + sin.(sup A) - sin.(inf A)
proof
A1: cos is_integrable_on A & cos is_bounded_on A by Lm3A; 
A2: sin is_integrable_on A & sin is_bounded_on A by Lm8A;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
    hence thesis by A1,A2,Th32,Th33,Th35,SIN_COS:73,Th73;
end;

theorem
  A = [.0,PI/2.] implies integral(sin+cos,A) = 2
proof
    assume A = [.0,PI/2.];then
A2: sup A=PI/2 & inf A=0 by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin+cos,A)
     = (-cos).(PI/2) - (-cos).0 + sin.(PI/2) - sin.0 by A2,Th76
    .= -(cos.(PI/2)) - (-cos).0 + sin.(PI/2) - sin.0 by A3,SEQ_1:def 7
    .= -0 - (-cos.0) + 1 - sin.0 by SIN_COS:81,A3,SEQ_1:def 7
    .= - (-cos.0) + 1 - sin.(0+2 * PI) by SIN_COS:83
    .= - (-cos.(0+2*PI)) + 1 - 0 by SIN_COS:81,83
    .= 2 by SIN_COS:81;
    hence thesis;
end;

theorem
  A = [.0,PI.] implies integral(sin+cos,A) = 2
proof
    assume A = [.0,PI.];then
A2: sup A=PI & inf A=0 by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin+cos,A)
     = (-cos).(PI) - (-cos).0 + sin.(PI) - sin.0 by A2,Th76
    .= -(cos.(PI)) - (-cos).0 + sin.(PI) - sin.0 by A3,SEQ_1:def 7
    .= -(cos.(PI)) - (-cos.0) + sin.(PI) - sin.0 by A3,SEQ_1:def 7
    .= - (-cos.0) + 1 - sin.(0+2 * PI) by SIN_COS:81,83
    .= - (-cos.(0+2*PI)) + 1 - 0 by SIN_COS:81,83
    .= 2 by SIN_COS:81;
    hence thesis;
end;

theorem
  A = [.0,PI*3/2.] implies integral(sin+cos,A) = 0
proof
    assume A = [.0,PI*3/2.];then
A2: sup A=PI*3/2 & inf A=0 by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin+cos,A)
     = (-cos).(PI*3/2) - (-cos).0 + sin.(PI*3/2) - sin.0 by A2,Th76
    .= -(cos.(PI*3/2)) - (-cos).0 + sin.(PI*3/2) - sin.0 by A3,SEQ_1:def 7
    .= -(cos.(PI*3/2)) - (-cos.0) + sin.(PI*3/2) - sin.0 by A3,SEQ_1:def 7
    .= -(cos.(PI*3/2)) - (-cos.(0+2*PI)) + sin.(PI*3/2) - sin.0 by SIN_COS:83
    .= -0 +1 + -1 - 0 by SIN_COS:81,83;
    hence thesis;
end;

theorem
  A = [.0,PI*2.] implies integral(sin+cos,A) = 0
proof
    assume A = [.0,PI*2.];then
A2: sup A=PI*2 & inf A=0 by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin+cos,A)
     = (-cos).(PI*2) - (-cos).0 + sin.(PI*2) - sin.0 by A2,Th76
    .= -(cos.(PI*2)) - (-cos).0 + sin.(PI*2) - sin.0 by A3,SEQ_1:def 7
    .= -(cos.(PI*2)) - (-cos.0) + sin.(PI*2) - sin.0 by A3,SEQ_1:def 7
    .= -(cos.(PI*2)) - (-cos.(0+2*PI)) + sin.(PI*2) - sin.0 by SIN_COS:83
    .= -1+1 + 0 - 0 by SIN_COS:81,83;
    hence thesis;
end;

theorem
  A = [.2*n*PI,(2*n+1)*PI.] implies integral(sin+cos,A) = 2
proof
    assume A = [.2*n*PI,(2*n+1)*PI.];then
A2: sup A=(2*n+1)*PI & inf A=(2*n*PI) by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin+cos,A)
     = (-cos).((2*n+1)*PI) - (-cos).(2*n*PI) + sin.((2*n+1)*PI) - sin.(2*n*PI)
        by A2,Th76
    .= -(cos.((2*n+1)*PI)) - (-cos).(2*n*PI) + sin.((2*n+1)*PI) - sin.(2*n*PI)
        by A3,SEQ_1:def 7
    .= -(cos.((2*n+1)*PI)) - -(cos(0+2*n*PI)) +
       sin.((2*n+1)*PI) - sin.(0+2*n*PI) by A3,SEQ_1:def 7
    .= -(cos.((2*n+1)*PI)) - -(cos(0)) +
       sin.((2*n+1)*PI) - sin.(0+2*n*PI) by Th3
    .= -(cos.((2*n+1)*PI)) + (cos(0)) +
       sin.((2*n+1)*PI) - sin.(0+2*n*PI)
    .= -(cos.((2*n+1)*PI)) + (cos(0+2*PI)) +
       sin.((2*n+1)*PI) - sin.(0+2*n*PI) by SIN_COS:84
    .= -(cos.((2*n+1)*PI)) + 1 +
       sin.((2*n+1)*PI) - sin(0+2*n*PI) by SIN_COS:82
    .= -(cos.((2*n+1)*PI)) + 1 +
       sin.((2*n+1)*PI) - sin(0) by Th1
    .= -(cos.((2*n+1)*PI)) + 1 +
       sin.((2*n+1)*PI) - sin(0+2*PI) by SIN_COS:84
    .= -(cos(0+(2*n+1)*PI)) + 1 + sin(0+(2*n+1)*PI) by SIN_COS:82
    .= -(cos(0+(2*n+1)*PI)) + 1 + -sin(0) by Th2
    .= - -cos(0) + 1 + -sin(0) by Th4
    .= cos(0) + 1 - sin(0)
    .= cos(0+2*PI) + 1 - sin(0) by SIN_COS:84
    .= cos(0+2*PI) + 1 - sin(0+2*PI) by SIN_COS:84
    .= 2 by SIN_COS:82;
    hence thesis;
end;

theorem
  A = [.x+2*n*PI,x+(2*n+1)*PI.] implies
    integral(sin+cos,A) = 2*cos x - 2*sin x
proof
    assume A = [.x+2*n*PI,x+(2*n+1)*PI.];then
A2: sup A=x+(2*n+1)*PI & inf A=x+2*n*PI by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin+cos,A)
     = (-cos).(x+(2*n+1)*PI) - (-cos).(x+2*n*PI) +
       sin.(x+(2*n+1)*PI) - sin.(x+2*n*PI) by A2,Th76
    .= -(cos.(x+(2*n+1)*PI)) - (-cos).(x+2*n*PI) +
       sin.(x+(2*n+1)*PI) - sin.(x+2*n*PI) by A3,SEQ_1:def 7
    .= -(cos(x+(2*n+1)*PI)) - -(cos(x+2*n*PI)) +
       sin(x+(2*n+1)*PI) - sin(x+2*n*PI) by A3,SEQ_1:def 7
    .= -(-cos x) - -(cos(x+2*n*PI)) +
       sin(x+(2*n+1)*PI) - sin(x+2*n*PI) by Th4
    .= -(-cos x) - -cos x + sin(x+(2*n+1)*PI) - sin(x+2*n*PI) by Th3
    .= -(-cos x) - -cos x + -sin x - sin(x+2*n*PI) by Th2
    .= cos x + cos x  -sin x - sin x by Th1
    .= 2* cos x - 2*sin x;
    hence thesis;
end;

theorem Th83:
  integral(sinh+cosh,A) =
    cosh.(sup A) - cosh.(inf A) + sinh.(sup A) - sinh.(inf A)
proof
    cosh is_continuous_on A by Lm7;then
A1: cosh is_integrable_on A & cosh is_bounded_on A by INTEGRA5:10,11;
    sinh is_continuous_on A by Lm6;then
A2: sinh is_integrable_on A & sinh is_bounded_on A by INTEGRA5:10,11;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
    hence thesis by A1,A2,Th36,Th37,SIN_COS2:34,35,Th73;
end;

theorem
  A = [.0,1.] implies integral(sinh+cosh,A) = number_e -1
proof
    assume A = [.0,1.];then
A1: sup A=1 & inf A=0 by Th45;
A2: number_e > 0 by SIN_COS:60,IRRAT_1:def 7;
    integral(sinh+cosh,A)
    = cosh.1 - cosh.0 + sinh.1 - sinh.0 by A1,Th83
   .= (number_e^2 + 1)/(2*number_e) + (number_e^2 - 1)/(2*number_e) - 1
       by Th20,Th21,Th19,SIN_COS2:16
   .= ((number_e^2 + 1) + (number_e^2 - 1))/(2*number_e) -1 by XCMPLX_1:63
   .= (2*number_e^2)/(2*number_e) -1
   .= number_e^2/number_e -1 by XCMPLX_1:92
   .= number_e*number_e/number_e -1 by SQUARE_1:def 3
   .= number_e -1 by XCMPLX_1:90,A2;
   hence thesis;
end;

theorem Th85:
  integral(sin-cos,A) =
    (-cos).(sup A) - (-cos).(inf A) - (sin.(sup A) - sin.(inf A))
proof
A1: cos is_integrable_on A & cos is_bounded_on A by Lm3A; 
A2: sin is_integrable_on A & sin is_bounded_on A by Lm8A;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
    hence thesis by A1,A2,Th32,Th33,Th35,SIN_COS:73,Th74;
end;

theorem
  A = [.0,PI/2.] implies integral(sin-cos,A) = 0
proof
    assume A = [.0,PI/2.];then
A2: sup A=PI/2 & inf A=0 by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin-cos,A)
     = (-cos).(PI/2) - (-cos).0 - (sin.(PI/2) - sin.0) by A2,Th85
    .= -(cos.(PI/2)) - (-cos).0 - (sin.(PI/2) - sin.0) by A3,SEQ_1:def 7
    .= -0 - (-cos.0) - (1 - sin.0) by SIN_COS:81,A3,SEQ_1:def 7
    .= - (-cos.0) - (1 - sin.(0+2 * PI)) by SIN_COS:83
    .= - (-cos.(0+2*PI)) - (1 - 0) by SIN_COS:81,83
    .= 0 by SIN_COS:81;
    hence thesis;
end;

theorem
  A = [.0,PI.] implies integral(sin-cos,A) = 2
proof
    assume A = [.0,PI.];then
A2: sup A=PI & inf A=0 by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin-cos,A)
     = (-cos).(PI) - (-cos).0 - (sin.(PI) - sin.0) by A2,Th85
    .= -(cos.(PI)) - (-cos).0 - (sin.(PI) - sin.0) by A3,SEQ_1:def 7
    .= -(cos.(PI)) - (-cos.0) - (sin.(PI) - sin.0) by A3,SEQ_1:def 7
    .= - (-cos.0) + 1 - (sin.(0+2 * PI)-0) by SIN_COS:81,83
    .= - (-cos.(0+2*PI)) + 1 - 0 by SIN_COS:81,83
    .= 2 by SIN_COS:81;
    hence thesis;
end;

theorem
  A = [.0,PI*3/2.] implies integral(sin-cos,A) = 2
proof
    assume A = [.0,PI*3/2.];then
A2: sup A=PI*3/2 & inf A=0 by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin-cos,A)
     = (-cos).(PI*3/2) - (-cos).0 - (sin.(PI*3/2) - sin.0) by A2,Th85
    .= -(cos.(PI*3/2)) - (-cos).0 - (sin.(PI*3/2) - sin.0) by A3,SEQ_1:def 7
    .= -(cos.(PI*3/2)) - (-cos.0) - (sin.(PI*3/2) - sin.0) by A3,SEQ_1:def 7
    .= -(cos.(PI*3/2)) - (-cos.(0+2*PI)) - (sin.(PI*3/2) - sin.0) by SIN_COS:83
    .= -0 +1 - (-1 - 0) by SIN_COS:81,83;
    hence thesis;
end;

theorem
  A = [.0,PI*2.] implies integral(sin-cos,A) = 0
proof
    assume A = [.0,PI*2.];then
A2: sup A=PI*2 & inf A=0 by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin-cos,A)
     = (-cos).(PI*2) - (-cos).0 - (sin.(PI*2) - sin.0) by A2,Th85
    .= -(cos.(PI*2)) - (-cos).0 - (sin.(PI*2) - sin.0) by A3,SEQ_1:def 7
    .= -(cos.(PI*2)) - (-cos.0) - (sin.(PI*2) - sin.0) by A3,SEQ_1:def 7
    .= -(cos.(PI*2)) - (-cos.(0+2*PI)) - (sin.(PI*2) - sin.0) by SIN_COS:83
    .= -1+1 - (0 - 0) by SIN_COS:81,83;
    hence thesis;
end;

theorem
  A = [.2*n*PI,(2*n+1)*PI.] implies integral(sin-cos,A) = 2
proof
    assume A = [.2*n*PI,(2*n+1)*PI.];then
A2: sup A=(2*n+1)*PI & inf A=(2*n*PI) by Th45;
A3: dom (-cos) = REAL by LmX0;
     integral(sin-cos,A)
   = (-cos).((2*n+1)*PI) - (-cos).(2*n*PI) - (sin.((2*n+1)*PI) - sin.(2*n*PI))
       by A2,Th85
  .= -(cos.((2*n+1)*PI)) - (-cos).(2*n*PI) - (sin.((2*n+1)*PI) - sin.(2*n*PI))
        by A3,SEQ_1:def 7
  .= -(cos.((2*n+1)*PI)) - -(cos(0+2*n*PI)) -
       (sin.((2*n+1)*PI) - sin.(0+2*n*PI)) by A3,SEQ_1:def 7
  .= -(cos.((2*n+1)*PI)) - -(cos(0)) -
      (sin.((2*n+1)*PI) - sin.(0+2*n*PI)) by Th3
  .= -(cos.((2*n+1)*PI)) + (cos(0)) -
      (sin.((2*n+1)*PI) - sin.(0+2*n*PI))
  .= -(cos.((2*n+1)*PI)) + (cos(0+2*PI)) -
      (sin.((2*n+1)*PI) - sin.(0+2*n*PI)) by SIN_COS:84
  .= -(cos.((2*n+1)*PI)) + 1 -
      (sin.((2*n+1)*PI) - sin(0+2*n*PI)) by SIN_COS:82
  .= -(cos.((2*n+1)*PI)) + 1 -
      (sin.((2*n+1)*PI) - sin(0)) by Th1
  .= -(cos.((2*n+1)*PI)) + 1 -
      (sin.((2*n+1)*PI) - sin(0+2*PI)) by SIN_COS:84
  .= -(cos(0+(2*n+1)*PI)) + 1 - sin(0+(2*n+1)*PI) by SIN_COS:82
  .= -(cos(0+(2*n+1)*PI)) + 1 - -sin(0) by Th2
  .= - -cos(0) + 1 - -sin(0) by Th4
  .= cos(0) + 1 + sin(0)
  .= cos(0+2*PI) + 1 + sin(0) by SIN_COS:84
  .= cos(0+2*PI) + 1 + sin(0+2*PI) by SIN_COS:84
  .= 2 by SIN_COS:82;
    hence thesis;
end;

theorem
  A = [.x+2*n*PI,x+(2*n+1)*PI.] implies
    integral(sin-cos,A) = 2*cos x + 2*sin x
proof
    assume A = [.x+2*n*PI,x+(2*n+1)*PI.];then
A2: sup A=x+(2*n+1)*PI & inf A=x+2*n*PI by Th45;
A3: dom (-cos) = REAL by LmX0;
    integral(sin-cos,A)
     = (-cos).(x+(2*n+1)*PI) - (-cos).(x+2*n*PI) -
       (sin.(x+(2*n+1)*PI) - sin.(x+2*n*PI)) by A2,Th85
    .= -(cos.(x+(2*n+1)*PI)) - (-cos).(x+2*n*PI) -
       (sin.(x+(2*n+1)*PI) - sin.(x+2*n*PI)) by A3,SEQ_1:def 7
    .= -(cos(x+(2*n+1)*PI)) - -(cos(x+2*n*PI)) -
       (sin(x+(2*n+1)*PI) - sin(x+2*n*PI)) by A3,SEQ_1:def 7
    .= -(-cos x) - -(cos(x+2*n*PI)) -
       (sin(x+(2*n+1)*PI) - sin(x+2*n*PI)) by Th4
    .= -(-cos x) - -cos x -
       (sin(x+(2*n+1)*PI) - sin(x+2*n*PI)) by Th3
    .= -(-cos x) - -cos x - (-sin x - sin(x+2*n*PI)) by Th2
    .= cos x + cos x - (-sin x - sin x) by Th1
    .= 2* cos x + 2*sin x;
    hence thesis;
end;

theorem
  integral(r(#)sin,A) = r*(-cos).(sup A) - r*(-cos).(inf A)
proof
    sin is_continuous_on A by Lm8;then
A2: sin is_integrable_on A & sin is_bounded_on A by INTEGRA5:10,11;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
    hence thesis by A2,Th32,Th35,Th75;
end;

theorem
  integral(r(#)cos,A) = r*sin.(sup A) - r*sin.(inf A)
proof
A2: cos is_integrable_on A & cos is_bounded_on A by Lm3A;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
    hence thesis by A2,Th33,SIN_COS:73,Th75;
end;

theorem
  integral(r(#)sinh,A) = r*cosh.(sup A) - r*cosh.(inf A)
proof
    sinh is_continuous_on A by Lm6;then
A2: sinh is_integrable_on A & sinh is_bounded_on A by INTEGRA5:10,11;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
    hence thesis by A2,SIN_COS2:35,Th37,Th75;
end;

theorem
  integral(r(#)cosh,A) = r*sinh.(sup A) - r*sinh.(inf A)
proof
    cosh is_continuous_on A by Lm7;then
A2: cosh is_integrable_on A & cosh is_bounded_on A by INTEGRA5:10,11;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
    hence thesis by A2,SIN_COS2:34,Th36,Th75;
end;

theorem
  integral(r(#)exp_R,A) = r*exp_R.(sup A) - r*exp_R.(inf A)
proof
    exp_R is_continuous_on A by Lm5;then
A2: exp_R is_integrable_on A & exp_R is_bounded_on A by INTEGRA5:10,11;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
    hence thesis by A2,SIN_COS:71,Th38,Th75;
end;

theorem Th457:
  integral(sin(#)cos,A)
    = 1/2*(cos.(inf A)*cos.(inf A)-cos.(sup A)*cos.(sup A))
proof
    sin is_continuous_on A by Lm8;then
B3: (-cos)`|REAL is_integrable_on A & (-cos)`|REAL is_bounded_on A
      by Th35,INTEGRA5:10,11;
    -sin is_continuous_on A by Lm4;then
B5: cos`|REAL is_integrable_on A & cos`|REAL is_bounded_on A
     by Th34,INTEGRA5:10,11;
B6: dom (-cos) = REAL by LmX0;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;then
    integral(sin(#)cos,A)
    =(-cos).(sup A)*cos.(sup A)-(-cos).(inf A)*cos.(inf A)
     -integral((-cos)(#)(-sin),A)
      by Th32,SIN_COS:72,B3,B5,INTEGRA5:21,Th35,Th34
   .=(-cos).(sup A)*cos.(sup A)-(-cos).(inf A)*cos.(inf A)
    -integral(sin(#)cos,A) by Lm3B
   .=(-cos.(sup A))*cos.(sup A)-(-cos).(inf A)*cos.(inf A)
    -integral(sin(#)cos,A) by B6,SEQ_1:def 7
   .=(-cos.(sup A))*cos.(sup A)-(-cos.(inf A))*cos.(inf A)
    -integral(sin(#)cos,A) by B6,SEQ_1:def 7
   .=cos.(inf A)*cos.(inf A)-cos.(sup A)*cos.(sup A)
     -integral(sin(#)cos,A);
    hence thesis;
end;

theorem
  A = [.0,PI/2.] implies integral(sin(#)cos,A) = 1/2
proof
  assume A = [.0,PI/2.];then
  sup A=PI/2 & inf A=0 by Th45; then
  integral(sin(#)cos,A)
    =1/2*(cos.0*cos.0-cos.(PI/2)*cos.(PI/2)) by Th457
   .=1/2*(cos.(0+2*PI)*cos.0-cos.(PI/2)*cos.(PI/2)) by SIN_COS:83
   .=1/2*(1*1-0*0) by SIN_COS:81,83;
   hence thesis;
end;

theorem
  A = [.0,PI.] implies integral(sin(#)cos,A) = 0
proof
    assume A = [.0,PI.];then
    sup A=PI & inf A=0 by Th45; then
    integral(sin(#)cos,A)
    =1/2*(cos.0*cos.0-cos.PI*cos.PI) by Th457
   .=1/2*(cos.(0+2*PI)*cos.0-cos.PI*cos.PI) by SIN_COS:83
   .=1/2*(1*1-(-1)*(-1)) by SIN_COS:81,83;
   hence thesis;
end;

theorem
  A = [.0,PI*(3/2).] implies integral(sin(#)cos,A) = 1/2
proof
   assume A = [.0,PI*(3/2).];then
   sup A=PI*(3/2) & inf A=0 by Th45; then
    integral(sin(#)cos,A)
    =1/2*(cos.0*cos.0-cos.(PI*(3/2))*cos.(PI*(3/2))) by Th457
   .=1/2*(cos.(0+2*PI)*cos.0-cos.(PI*(3/2))*cos.(PI*(3/2))) by SIN_COS:83
   .=1/2*(1*1-0*0) by SIN_COS:81,83;
   hence thesis;
end;

theorem
  A = [.0,PI*2.] implies integral(sin(#)cos,A) = 0
proof
  assume A = [.0,PI*2.];then
  sup A=PI*2 & inf A=0 by Th45; then
    integral(sin(#)cos,A)
    =1/2*(cos.0*cos.0-cos.(PI*2)*cos.(PI*2)) by Th457
   .=1/2*(cos.(0+2*PI)*cos.0-cos.(PI*2)*cos.(PI*2)) by SIN_COS:83
   .=1/2*(1*1-1*1) by SIN_COS:81,83;
   hence thesis;
end;

theorem
  A = [.2*n*PI,(2*n+1)*PI.] implies integral(sin(#)cos,A) = 0
proof
    assume A = [.2*n*PI,(2*n+1)*PI.];then
    sup A=(2*n+1)*PI & inf A=2*n*PI by Th45; then
      integral(sin(#)cos,A)
    =1/2*(cos.(2*n*PI)*cos.(2*n*PI)-cos.((2*n+1)*PI)*cos.((2*n+1)*PI))
       by Th457
   .=1/2*(cos(0)*cos(0+2*n*PI)-cos.((2*n+1)*PI)*cos.((2*n+1)*PI)) by Th3
   .=1/2*(cos(0)*cos(0)-cos.((2*n+1)*PI)*cos.((2*n+1)*PI)) by Th3
   .=1/2*(cos(0+2*PI)*cos(0)-cos.((2*n+1)*PI)*cos.((2*n+1)*PI)) by SIN_COS:84
   .=1/2*(1*1-cos.((2*n+1)*PI)*cos.((2*n+1)*PI)) by SIN_COS:82,84
   .=1/2*(1*1- (-cos(0))*cos(0+(2*n+1)*PI)) by Th4
   .=1/2*(1*1- (-cos(0))*(-cos(0))) by Th4
   .=1/2*(1*1- cos(0)*cos(0))
   .=1/2*(1*1- cos(0+2*PI)*cos(0)) by SIN_COS:84
   .=1/2*(1*1- cos(0+2*PI)*cos(0+2*PI)) by SIN_COS:84;
   hence thesis by SIN_COS:82;
end;

theorem
  A = [.x+2*n*PI,x+(2*n+1)*PI.] implies integral(sin(#)cos,A) = 0
proof
    assume A = [.x+2*n*PI,x+(2*n+1)*PI.];then
    sup A=x+(2*n+1)*PI & inf A=x+2*n*PI by Th45; then
    integral(sin(#)cos,A)
    = 1/2*(cos.(x+2*n*PI)*cos.(x+2*n*PI)-cos.(x+(2*n+1)*PI)*cos.(x+(2*n+1)*PI))
         by Th457
   .= 1/2*(cos(x)*cos(x+2*n*PI)-cos(x+(2*n+1)*PI)*cos(x+(2*n+1)*PI)) by Th3
   .= 1/2*(cos(x)*cos(x)-cos(x+(2*n+1)*PI)*cos(x+(2*n+1)*PI)) by Th3
   .= 1/2*(cos(x)*cos(x)- (-cos(x))*cos(x+(2*n+1)*PI)) by Th4
   .= 1/2*(cos(x)*cos(x)- (-cos(x))*(-cos(x))) by Th4
   .= 0;
   hence thesis;
end;

Lm11:cos is_continuous_on A
proof
   cos is_continuous_on REAL by SIN_COS:72,FDIFF_1:33;
   hence thesis by FCONT_1:17;
end;

Lm11A:cos(#)cos is_integrable_on A & cos(#)cos is_bounded_on A
proof
  cos is_continuous_on A by Lm11;then
  cos(#)cos is_continuous_on A by FCONT_1:19;
  hence thesis by INTEGRA5:10,11;
end;

LmX1: for f, g being Function of REAL, REAL holds
  dom (f(#)g) = REAL & dom (f-g) = REAL & dom (f+g) = REAL
proof
  let f, g be Function of REAL, REAL;
A1:dom f = REAL & dom g = REAL by FUNCT_2:def 1;
  thus dom (f(#)g) = dom f /\ dom g by SEQ_1:def 5
    .= REAL by A1;
  thus dom (f-g) = dom f /\ dom g by SEQ_1:def 4
    .= REAL by A1;
  thus dom (f+g) = dom f /\ dom g by SEQ_1:def 3
    .= REAL by A1;
end;

theorem
  integral(sin(#)sin,A)=cos.(inf A)*sin.(inf A)-
  cos.(sup A)*sin.(sup A)+integral((cos)(#)(cos),A)
proof
    sin is_continuous_on A by Lm8;then
A1: (-cos)`|REAL is_integrable_on A & (-cos)`|REAL is_bounded_on A
     by Th35,INTEGRA5:10,11;
    cos is_continuous_on A by Lm11;then
A2: sin`|REAL is_integrable_on A & sin`|REAL is_bounded_on A
     by Th33,INTEGRA5:10,11;
A3: dom (-cos) = REAL by LmX0;
    cos(#)cos is_integrable_on A & cos(#)cos is_bounded_on A by Lm11A;then
A4: (cos(#)cos)||A is_integrable_on A &
    (cos(#)cos)||A is_bounded_on A by INTEGRA5:def 2,INTEGRA5:9;
    dom(cos(#)cos) = REAL by LmX1;then
    (cos(#)cos)||A is total by INTEGRA5:6;then
A5: (cos(#)cos)||A is Function of A,REAL by FUNCT_2:131;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;then
      integral(sin(#)sin,A)
    =(-cos).(sup A)*sin.(sup A)-(-cos).(inf A)*sin.(inf A)
     -integral((-cos)(#)(cos),A)
       by Th32,SIN_COS:73,A1,A2,INTEGRA5:21,Th33,Th35
   .=(-cos.(sup A))*sin.(sup A)-(-cos).(inf A)*sin.(inf A)
     -integral((-cos)(#)(cos),A) by A3,SEQ_1:def 7
   .=(-cos.(sup A))*sin.(sup A)-(-(cos.(inf A)))*sin.(inf A)
     -integral((-cos)(#)(cos),A) by A3,SEQ_1:def 7
   .=cos.(inf A)*sin.(inf A)-cos.(sup A)*sin.(sup A)
     -integral(-((cos)(#)(cos)),A) by Lm3C
   .=cos.(inf A)*sin.(inf A)-cos.(sup A)*sin.(sup A)
     -integral((-1)(#)((cos)(#)(cos)),A) by RFUNCT_1:38
   .=cos.(inf A)*sin.(inf A) - cos.(sup A)*sin.(sup A)
     -integral((-1)(#)(((cos)(#)(cos))||A)) by RFUNCT_1:65
   .=cos.(inf A)*sin.(inf A)-cos.(sup A)*sin.(sup A)
     -(-1)*integral((cos)(#)(cos),A) by A4,A5,INTEGRA2:31
   .=cos.(inf A)*sin.(inf A)-cos.(sup A)*sin.(sup A)
     + integral((cos)(#)(cos),A);
    hence thesis;
end;

theorem
  integral(sinh(#)sinh,A)=cosh.(sup A)*sinh.(sup A)-
  cosh.(inf A)*sinh.(inf A)-integral((cosh)(#)(cosh),A)
proof
A1: (cosh)`|REAL is_integrable_on A & (cosh)`|REAL is_bounded_on A
     by Lm6A,Th37;
A2: sinh`|REAL is_integrable_on A & sinh`|REAL is_bounded_on A
     by Lm7A,Th36;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
    hence thesis by A1,A2,INTEGRA5:21,SIN_COS2:34,35,Th36,Th37;
end;

theorem
  integral(sinh(#)cosh,A)
  =1/2*((cosh.(sup A))*cosh.(sup A)-(cosh).(inf A)*cosh.(inf A))
proof
    sinh is_continuous_on A by Lm6;then
B3: (cosh)`|REAL is_integrable_on A & (cosh)`|REAL is_bounded_on A
      by Th37,INTEGRA5:10,11;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;then
    integral(sinh(#)cosh,A)
    =(cosh.(sup A))*cosh.(sup A)-(cosh).(inf A)*cosh.(inf A)
    -integral(sinh(#)cosh,A) by SIN_COS2:35,B3,INTEGRA5:21,Th37;
    hence thesis;
end;

theorem
  integral(exp_R(#)exp_R,A)
  =1/2*((exp_R.(sup A))^2-((exp_R).(inf A))^2)
proof
    exp_R is_continuous_on A by Lm5;then
B5: exp_R`|REAL is_integrable_on A & exp_R`|REAL is_bounded_on A
      by Th38,INTEGRA5:10,11;
    REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;then
    integral(exp_R(#)exp_R,A)
    =(exp_R).(sup A)*exp_R.(sup A)-(exp_R).(inf A)*exp_R.(inf A)
     -integral((exp_R)(#)(exp_R),A) by SIN_COS:71,B5,INTEGRA5:21,Th38
   .= (exp_R.(sup A))^2 - ((exp_R).(inf A))*exp_R.(inf A)
    -integral(exp_R(#)exp_R,A) by SQUARE_1:def 3
   .= (exp_R.(sup A))^2 - ((exp_R).(inf A))^2
    -integral(exp_R(#)exp_R,A) by SQUARE_1:def 3;
    hence thesis;
end;

Lm12:sin is_continuous_on A
proof 
   sin is_continuous_on REAL by SIN_COS:73,FDIFF_1:33;
   hence thesis by FCONT_1:17;
end;

Lm13:exp_R(#)(sin + cos) is_integrable_on A &
      exp_R(#)(sin + cos) is_bounded_on A
proof
A1: exp_R is_continuous_on A by Lm5;
A2: cos is_continuous_on A by Lm11;
    sin is_continuous_on A by Lm12; then
    sin + cos is_continuous_on A by A2,FCONT_1:19;then
    exp_R(#)(sin + cos) is_continuous_on A by A1,FCONT_1:19;
    hence thesis by INTEGRA5:10,11;
end;

Lm14:exp_R(#)(cos - sin) is_integrable_on A &
     exp_R(#)(cos - sin) is_bounded_on A
proof
A1: exp_R is_continuous_on A by Lm5;
A2: cos is_continuous_on A by Lm11;
    sin is_continuous_on A by Lm12; then
    cos - sin is_continuous_on A by A2,FCONT_1:19;then
    exp_R(#)(cos - sin) is_continuous_on A by A1,FCONT_1:19;
    hence thesis by INTEGRA5:10,11;
end;

theorem
  integral(exp_R(#)(sin+cos),A) = (exp_R(#)sin).(sup A)-(exp_R(#)sin).(inf A)
proof
A1: dom (exp_R(#)sin) = REAL by LmX1;
A2: dom (sin+cos) = REAL by LmX1;
A3: dom (exp_R(#)(sin+cos)) = dom exp_R /\ dom (sin+cos) by SEQ_1:def 5
    .= REAL /\ dom (sin+cos) by SIN_COS:def 26
    .= REAL by A2;
A8: exp_R(#)(sin + cos) is_integrable_on A &
    exp_R(#)(sin + cos) is_bounded_on A by Lm13;
A9: REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
A10:(exp_R(#)sin) is_differentiable_on REAL by A9,A1,FDIFF_7:44;
A11:for x st x in dom ((exp_R(#)sin)`|REAL) holds
   ((exp_R(#)sin)`|REAL).x = (exp_R(#)(sin + cos)).x
   proof
     let x;
     assume x in dom ((exp_R(#)sin)`|REAL);
     (exp_R(#)(sin + cos)).x = (exp_R.x) * ((sin + cos).x)
       by A3,SEQ_1:def 5
         .= (exp_R.x) * (sin.x + cos.x) by A2,SEQ_1:def 3;
     hence thesis by A9,A1,FDIFF_7:44;
   end;
   dom ((exp_R(#)sin)`|REAL) = dom (exp_R(#)(sin + cos))
       by A10,A3,FDIFF_1:def 8;then
   ((exp_R(#)sin)`|REAL) = exp_R(#)(sin + cos) by A11,PARTFUN1:34;
   hence thesis by A8,A10,INTEGRA5:13;
end;

theorem
  integral(exp_R(#)(cos-sin),A) = (exp_R(#)cos).(sup A)-(exp_R(#)cos).(inf A)
proof
A1: dom (exp_R(#)cos) = REAL by LmX1;
A2: dom (cos - sin) = REAL by LmX1;
A3: dom (exp_R(#)(cos - sin)) = dom exp_R /\ dom (cos - sin) by SEQ_1:def 5
    .= REAL /\ dom (cos - sin) by SIN_COS:def 26
    .= REAL by A2;
A8: exp_R(#)(cos - sin) is_integrable_on A &
    exp_R(#)(cos - sin) is_bounded_on A by Lm14;
A9: REAL is open Subset of REAL by FCONT_3:4,SUBSET_1:def 4;
A10: (exp_R(#)cos) is_differentiable_on REAL by A9,A1,FDIFF_7:45;
A11:for x st x in dom ((exp_R(#)cos)`|REAL) holds
   ((exp_R(#)cos)`|REAL).x = (exp_R(#)(cos - sin)).x
   proof
     let x;
     assume x in dom ((exp_R(#)cos)`|REAL);
     (exp_R(#)(cos - sin)).x = (exp_R.x) * ((cos - sin).x)
        by A3,SEQ_1:def 5
         .= (exp_R.x) * (cos.x - sin.x) by A2,SEQ_1:def 4;
     hence thesis by A9,A1,FDIFF_7:45;
   end;
   dom ((exp_R(#)cos)`|REAL) = dom (exp_R(#)(cos - sin))
     by A10,A3,FDIFF_1:def 8;then
   ((exp_R(#)cos)`|REAL) = exp_R(#)(cos - sin) by A11,PARTFUN1:34;
   hence thesis by A8,A10,INTEGRA5:13;
end;
