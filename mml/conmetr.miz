environ

 vocabularies ANALMETR, SUBSET_1, SYMSP_1, ANALOAF, AFF_1, DIRAF, INCSP_1,
      AFF_2, CONAFFM, STRUCT_0, CONMETR;
 notations SUBSET_1, STRUCT_0, ANALOAF, AFF_1, ANALMETR, AFF_2, CONAFFM;
 constructors AFF_1, AFF_2, CONAFFM;
 registrations ANALMETR;
 requirements SUBSET;
 definitions AFF_2;
 theorems AFF_1, AFF_2, ANALMETR, TRANSLAC, CONAFFM, DIRAF;

begin

reserve X for OrtAfPl;
reserve o,a,a1,a2,a3,a4,b,b1,b2,b3,b4,c,c1,c2,c3,d,d1,d2,d3,d4,e1,e2 for
  Element of X;
reserve a29,a39,b29,x9 for Element of Af(X);
reserve A,K,M,N for Subset of X;
reserve A9,K9 for Subset of Af(X);

definition
  let X;
  attr X is satisfying_OPAP means
  :Def1:
  for o,a1,a2,a3,b1,b2,b3,M,N st o in M
  & a1 in M & a2 in M & a3 in M & o in N & b1 in N & b2 in N & b3 in N & not b2
in M & not a3 in N & M _|_ N & o<>a1 & o<>a2 & o<>a3 & o<>b1 & o<>b2 & o<>b3 &
  a3,b2 // a2,b1 & a3,b3 // a1,b1 holds a1,b2 // a2,b3;
  attr X is satisfying_PAP means
  for o,a1,a2,a3,b1,b2,b3,M,N st M is
being_line & N is being_line & o in M & a1 in M & a2 in M & a3 in M & o in N &
b1 in N & b2 in N & b3 in N & not b2 in M & not a3 in N & o<>a1 & o<>a2 & o<>a3
& o<>b1 & o<>b2 & o<>b3 & a3,b2 // a2,b1 & a3,b3 // a1,b1 holds a1,b2 // a2,b3;
  attr X is satisfying_MH1 means
  :Def3:
  for a1,a2,a3,a4,b1,b2,b3,b4,M,N st M
  _|_ N & a1 in M & a3 in M & b1 in M & b3 in M & a2 in N & a4 in N & b2 in N &
b4 in N & not a2 in M & not a4 in M & a1,a2 _|_ b1,b2 & a2,a3 _|_ b2,b3 & a3,a4
  _|_ b3,b4 holds a1,a4 _|_ b1,b4;
  attr X is satisfying_MH2 means
  :Def4:
  for a1,a2,a3,a4,b1,b2,b3,b4,M,N st M
  _|_ N & a1 in M & a3 in M & b2 in M & b4 in M & a2 in N & a4 in N & b1 in N &
b3 in N & not a2 in M & not a4 in M & a1,a2 _|_ b1,b2 & a2,a3 _|_ b2,b3 & a3,a4
  _|_ b3,b4 holds a1,a4 _|_ b1,b4;
  attr X is satisfying_TDES means
  :Def5:
  for o,a,a1,b,b1,c,c1 st o<>a & o<>a1
& o<>b & o<>b1 & o<>c & o<>c1 & not LIN b,b1,a & not LIN b,b1,c & LIN o,a,a1 &
LIN o,b,b1 & LIN o,c,c1 & a,b // a1,b1 & a,b // o,c & b,c // b1,c1 holds a,c //
  a1,c1;
  attr X is satisfying_SCH means
  for a1,a2,a3,a4,b1,b2,b3,b4,M,N st M is
being_line & N is being_line & a1 in M & a3 in M & b1 in M & b3 in M & a2 in N
  & a4 in N & b2 in N & b4 in N & not a4 in M & not a2 in M & not b2 in M & not
b4 in M & not a1 in N & not a3 in N & not b1 in N & not b3 in N & a3,a2 // b3,
  b2 & a2,a1 // b2,b1 & a1,a4 // b1,b4 holds a3,a4 // b3,b4;
  attr X is satisfying_OSCH means
  :Def7:
  for a1,a2,a3,a4,b1,b2,b3,b4,M,N st M
  _|_ N & a1 in M & a3 in M & b1 in M & b3 in M & a2 in N & a4 in N & b2 in N &
b4 in N & not a4 in M & not a2 in M & not b2 in M & not b4 in M & not a1 in N &
not a3 in N & not b1 in N & not b3 in N & a3,a2 // b3,b2 & a2,a1 // b2,b1 & a1,
  a4 // b1,b4 holds a3,a4 // b3,b4;
  attr X is satisfying_des means
  :Def8:
  for a,a1,b,b1,c,c1 st not LIN a,a1,b &
  not LIN a,a1,c & a,a1 // b,b1 & a,a1 // c,c1 & a,b // a1,b1 & a,c // a1,c1
  holds b,c // b1,c1;
end;

theorem Th1:
  ex a,b,c st LIN a,b,c & a<>b & b<>c & c <>a
proof
  consider a,p being Element of X such that
A1: a<>p by ANALMETR:51;
  consider b such that
A2: a,p _|_ p,b and
A3: p<>b by ANALMETR:51;
  reconsider a9=a,b9=b,p9=p as Element of Af(X) by ANALMETR:47;
  consider c such that
A4: p,c _|_ a,b and
A5: LIN a,b,c by ANALMETR:91;
  take a,b,c;
  thus LIN a,b,c by A5;
  thus a<>b
  proof
    assume not thesis;
    then a,p _|_ a,p by A2,ANALMETR:83;
    hence contradiction by A1,ANALMETR:51;
  end;
  thus b<>c
  proof
    assume not thesis;
    then a,p // a,b by A2,A3,A4,ANALMETR:85;
    then LIN a,p,b by ANALMETR:def 11;
    then LIN a9,p9,b9 by ANALMETR:55;
    then LIN p9,a9,b9 by AFF_1:15;
    then LIN p,a,b by ANALMETR:55;
    then p,a // p,b by ANALMETR:def 11;
    then a,p _|_ p,a by A2,A3,ANALMETR:84;
    then a,p _|_ a,p by ANALMETR:83;
    hence contradiction by A1,ANALMETR:51;
  end;
  assume not thesis;
  then a,p _|_ a,b by A4,ANALMETR:83;
  then p,b // a,b by A1,A2,ANALMETR:85;
  then b,p // b,a by ANALMETR:81;
  then LIN b,p,a by ANALMETR:def 11;
  then LIN b9,p9,a9 by ANALMETR:55;
  then LIN p9,a9,b9 by AFF_1:15;
  then LIN p,a,b by ANALMETR:55;
  then p,a // p,b by ANALMETR:def 11;
  then a,p _|_ p,a by A2,A3,ANALMETR:84;
  then a,p _|_ a,p by ANALMETR:83;
  hence contradiction by A1,ANALMETR:51;
end;

theorem Th2:
  for a,b ex c st LIN a,b,c & a<>c & b<>c
proof
  let a,b;
  consider p,q,r being Element of X such that
A1: LIN p,q,r and
A2: p<>q and
A3: q<>r and
A4: r<>p by Th1;
  reconsider a9=a,b9=b,p9=p,q9=q,r9=r as Element of Af(X) by ANALMETR:47;
  LIN p9,q9,r9 by A1,ANALMETR:55;
  then consider c9 being Element of Af(X) such that
A5: LIN a9,b9,c9 and
A6: a9<>c9 and
A7: b9<>c9 by A2,A3,A4,TRANSLAC:2;
  reconsider c =c9 as Element of X by ANALMETR:47;
  LIN a,b,c by A5,ANALMETR:55;
  hence thesis by A6,A7;
end;

theorem Th3:
  for A,a st A is being_line ex K st a in K & A _|_ K
proof
  let A,a;
  assume A is being_line;
  then consider b,c such that
A1: b<>c and
A2: A = Line(b,c) by ANALMETR:def 13;
  consider d such that
A3: b,c _|_ a,d and
A4: a<>d by ANALMETR:51;
  reconsider a9=a,d9=d as Element of Af(X) by ANALMETR:47;
  take K = Line(a,d);
  K = Line(a9,d9) by ANALMETR:56;
  hence a in K by AFF_1:26;
  thus thesis by A1,A2,A3,A4,ANALMETR:63;
end;

theorem Th4:
  A is being_line & a in A & b in A & c in A implies LIN a,b,c
proof
  assume that
A1: A is being_line and
A2: a in A and
A3: b in A and
A4: c in A;
  reconsider a9=a,b9=b,c9=c as Element of Af(X) by ANALMETR:47;
  reconsider A9=A as Subset of Af(X) by ANALMETR:57;
  A9 is being_line by A1,ANALMETR:58;
  then LIN a9,b9,c9 by A2,A3,A4,AFF_1:33;
  hence thesis by ANALMETR:55;
end;

theorem Th5:
  A is being_line & M is being_line & a in A & b in A & a in M & b
  in M implies a=b or A=M
proof
  assume that
A1: A is being_line and
A2: M is being_line and
A3: a in A and
A4: b in A and
A5: a in M and
A6: b in M;
  reconsider A9=A,M9=M as Subset of Af X by ANALMETR:57;
A7: M9 is being_line by A2,ANALMETR:58;
  assume
A8: a<>b;
  A9 is being_line by A1,ANALMETR:58;
  hence thesis by A3,A4,A5,A6,A8,A7,AFF_1:30;
end;

theorem Th6:
  for a,b,c,d,M holds for M9 being Subset of Af(X), c9,d9 being
Element of Af(X) st c =c9 & d=d9 & M=M9 & a in M & b in M & c9,d9 // M9 holds c
  ,d // a,b
proof
  let a,b,c,d,M;
  let M9 be Subset of Af(X),c9,d9 be Element of Af(X) such that
A1: c =c9 and
A2: d=d9 and
A3: M=M9 and
A4: a in M and
A5: b in M and
A6: c9,d9 // M9;
  reconsider a9=a,b9=b as Element of Af(X) by ANALMETR:47;
A:  M9 is being_line by A6,AFF_1:40;
  then a9,b9 // M9 by A3,A4,A5,AFF_1:66;
  then c9,d9 // a9,b9 by A,A6,AFF_1:40,45;
  hence thesis by A1,A2,ANALMETR:48;
end;

theorem Th7:
  X is satisfying_TDES implies Af(X) is Moufangian
proof
  assume
A1: X is satisfying_TDES;
  let K9 be Subset of Af(X);
  let o9,a9,b9,c9,a19,b19,c19 be Element of Af(X);
  assume that
A2: K9 is being_line and
A3: o9 in K9 and
A4: c9 in K9 and
A5: c19 in K9 and
A6: not a9 in K9 and
A7: o9<>c9 and
A8: a9<>b9 and
A9: LIN o9,a9,a19 and
A10: LIN o9,b9,b19 and
A11: a9,b9 // a19,b19 and
A12: a9,c9 // a19,c19 and
A13: a9,b9 // K9;
  reconsider K=K9 as Subset of X by ANALMETR:57;
  reconsider o=o9,a=a9,a1=a19,b=b9,b1=b19,c =c9,c1=c19 as Element
 of X by ANALMETR:47;
A14: a,c // a1,c1 by A12,ANALMETR:48;
A15: a,b // a1,b1 by A11,ANALMETR:48;
  now
A16: now
      assume
A17:  o<>a1;
A18:  o<>a & o<>b & o<>c & o<>c1 & o<>b1
      proof
A19:    now
          o9,c9 // K9 by A2,A3,A4,AFF_1:37;
          then
A20:      a9,b9 // o9,c9 by A2,A13,AFF_1:45;
          assume o=b1;
          then a19,o9 // o9,c9 by A8,A11,A20,DIRAF:47;
          then o9,c9 // o9,a19 by AFF_1:13;
          then LIN o9,c9,a19 by AFF_1:def 1;
          then
A21:      a19 in K9 by A2,A3,A4,A7,AFF_1:39;
          LIN o9,a19,a9 by A9,AFF_1:15;
          hence contradiction by A2,A3,A6,A17,A21,AFF_1:39;
        end;
A22:    now
          o9,a9 // o9,a19 by A9,AFF_1:def 1;
          then
A23:      o9,a19 // a9, o9 by AFF_1:13;
          assume o=c1;
          then o9,a19 // a9,c9 by A12,AFF_1:13;
          then a9,c9 // a9,o9 by A17,A23,DIRAF:47;
          then LIN a9,c9,o9 by AFF_1:def 1;
          then LIN o9,c9,a9 by AFF_1:15;
          hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
        end;
        assume o=a or o=b or o=c or o=c1 or o=b1;
        hence contradiction by A3,A6,A7,A13,A22,A19,AFF_1:49;
      end;
A24:  now
        assume
A25:    a<>a19;
A26:    not LIN a,a1,b & not LIN a,a1,c
        proof
A27:      now
            LIN a9,a19,o9 by A9,AFF_1:15;
            then a9,a19 // a9,o9 by AFF_1:def 1;
            then
A28:        a,a1 // a,o by ANALMETR:48;
            assume LIN a,a1,b;
            then a,a1 // a,b by ANALMETR:def 11;
            then a,b // a,o by A25,A28,ANALMETR:82;
            then
A29:        a,b // o,a by ANALMETR:81;
            a9,b9 // o9,c9 by A2,A3,A4,A7,A13,AFF_1:41;
            then a,b // o,c by ANALMETR:48;
            then o,c // o,a by A8,A29,ANALMETR:82;
            then LIN o,c,a by ANALMETR:def 11;
            then LIN o9,c9,a9 by ANALMETR:55;
            hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
          end;
A30:      now
            LIN a9,a19,o9 by A9,AFF_1:15;
            then a9,a19 // a9,o9 by AFF_1:def 1;
            then
A31:        a,a1 // a,o by ANALMETR:48;
            assume LIN a,a1,c;
            then a,a1 // a,c by ANALMETR:def 11;
            then a,c // a,o by A25,A31,ANALMETR:82;
            then LIN a,c,o by ANALMETR:def 11;
            then LIN a9,c9,o9 by ANALMETR:55;
            then LIN c9,o9,a9 by AFF_1:15;
            hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
          end;
          assume LIN a,a1,b or LIN a,a1,c;
          hence contradiction by A27,A30;
        end;
A32:    LIN o,b,b1 by A10,ANALMETR:55;
        o9,c9 // o9,c19 by A2,A3,A4,A5,AFF_1:53,55;
        then o,c // o,c1 by ANALMETR:48;
        then
A33:    LIN o,c,c1 by ANALMETR:def 11;
        o9,c9 // K9 by A2,A3,A4,AFF_1:54,55;
        then a9,b9 // o9,c9 by A2,A13,AFF_1:45;
        then b9,a9 // o9,c9 by AFF_1:13;
        then
A34:    b,a // o,c by ANALMETR:48;
A35:    b,a // b1,a1 by A15,ANALMETR:81;
        LIN o,a,a1 by A9,ANALMETR:55;
        then b,c // b1,c1 by A1,A14,A17,A18,A26,A32,A33,A35,A34,Def5;
        hence thesis by ANALMETR:48;
      end;
      now
A36:    not LIN o9,a9,c9
        proof
          assume LIN o9,a9,c9;
          then LIN o9,c9,a9 by AFF_1:15;
          hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
        end;
        assume
A37:    a=a1;
        o9,c9 // o9,c19 by A2,A3,A4,A5,AFF_1:53,55;
        then LIN o9,c9,c19 by AFF_1:def 1;
        then
A38:    c9=c19 by A12,A37,A36,AFF_1:23;
        not LIN o9,a9,b9
        proof
          assume LIN o9,a9,b9;
          then LIN a9,b9,o9 by AFF_1:15;
          then
A39:      a9,b9 // a9,o9 by AFF_1:def 1;
          o9,c9 // K9 by A2,A3,A4,AFF_1:37;
          then a9,b9 // o9,c9 by A2,A13,AFF_1:45;
          then a9,o9 // o9,c9 by A8,A39,DIRAF:47;
          then o9,c9 // o9,a9 by AFF_1:13;
          then LIN o9,c9,a9 by AFF_1:def 1;
          hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
        end;
        then b9=b19 by A10,A11,A37,AFF_1:23;
        hence thesis by A38,AFF_1:11;
      end;
      hence thesis by A24;
    end;
    assume
A40: not b in K;
A41: o=a1 implies o=b1 & o=c1
    proof
      assume that
A42:  o=a1 and
A43:  o<>b1 or o<>c1;
A44:  now
A45:    o9,c19 // a9,c9 by A12,A42,AFF_1:13;
        assume
A46:    o<>c1;
        o9,c19 // o9,c9 by A2,A3,A4,A5,AFF_1:53,55;
        then a9,c9 // o9,c9 by A46,A45,DIRAF:47;
        then c9,a9 // c9,o9 by AFF_1:13;
        then LIN c9,a9,o9 by AFF_1:def 1;
        then LIN o9,c9,a9 by AFF_1:15;
        hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
      end;
      now
        o9,c9 // K9 by A2,A3,A4,AFF_1:37;
        then a9,b9 // o9,c9 by A2,A13,AFF_1:45;
        then o9,c9 // o9,b19 by A8,A11,A42,DIRAF:47;
        then LIN o9,c9,b19 by AFF_1:def 1;
        then
A47:    b19 in K9 by A2,A3,A4,A7,AFF_1:39;
        assume
A48:    o<>b1;
        LIN o9,b19,b9 by A10,AFF_1:15;
        hence contradiction by A2,A3,A40,A48,A47,AFF_1:39;
      end;
      hence contradiction by A43,A44;
    end;
    now
      assume o=a1;
      then b,c // b1,c1 by A41,ANALMETR:51;
      hence thesis by ANALMETR:48;
    end;
    hence thesis by A16;
  end;
  hence thesis by A6,A13,AFF_1:49;
end;

theorem Th8:
  Af(X) is translational implies X is satisfying_des
proof
  assume
A1: Af(X) is translational;
  let a,a1,b,b1,c,c1;
  assume that
A2: not LIN a,a1,b and
A3: not LIN a,a1,c and
A4: a,a1 // b,b1 and
A5: a,a1 // c,c1 and
A6: a,b // a1,b1 and
A7: a,c // a1,c1;
  reconsider a9=a,a19=a1,b9=b,b19=b1,c9=c,c19=c1 as Element of Af(X) by
  ANALMETR:47;
  LIN a9,a19,a19 by AFF_1:16;
  then consider A9 be Subset of Af(X) such that
A8: A9 is being_line and
A9: a9 in A9 and
A10: a19 in A9 and
  a19 in A9 by AFF_1:33;
A11: a9,b9 // a19,b19 by A6,ANALMETR:48;
  b,b1 // a,a1 by A4,ANALMETR:81;
  then
A12: b9,b19 // a9,a19 by ANALMETR:48;
A13: a9<>a19
  proof
    assume a9=a19;
    then LIN a9,a19,b9 by AFF_1:16;
    hence contradiction by A2,ANALMETR:55;
  end;
  then for x9 holds x9 in A9 iff LIN a9,a19,x9 by A8,A9,A10,AFF_1:33,39;
  then A9=Line(a9,a19) by AFF_1:def 2;
  then
A14: b9,b19 // A9 by A13,A12,AFF_1:def 4;
A15: a9,c9 // a19,c19 by A7,ANALMETR:48;
  c,c1 // a,a1 by A5,ANALMETR:81;
  then
A16: c9,c19 // a9,a19 by ANALMETR:48;
A17: a9<>a19
  proof
    assume a9=a19;
    then LIN a9,a19,b9 by AFF_1:16;
    hence contradiction by A2,ANALMETR:55;
  end;
  then for x9 holds x9 in A9 iff LIN a9,a19,x9 by A8,A9,A10,
  AFF_1:33 ,39;
  then A9=Line(a9,a19) by AFF_1:def 2;
  then
A18: c9,c19 // A9 by A17,A16,AFF_1:def 4;
  LIN c9,c19,c19 by AFF_1:16;
  then consider N9 be Subset of Af(X) such that
A19: N9 is being_line and
A20: c9 in N9 and
A21: c19 in N9 and
  c19 in N9 by AFF_1:33;
  LIN b9,b19,b19 by AFF_1:16;
  then consider M9 be Subset of Af(X) such that
A22: M9 is being_line and
A23: b9 in M9 and
A24: b19 in M9 and
  b19 in M9 by AFF_1:33;
A25: A9<>M9 & A9<>N9
  proof
    assume A9=M9 or A9=N9;
    then LIN a9,a19,b9 or LIN a9,a19,c9 by A8,A9,A10,A23,A20,AFF_1:33;
    hence contradiction by A2,A3,ANALMETR:55;
  end;
A26: c9<>c19
  proof
    assume c9=c19;
    then c,a // c,a1 by A7,ANALMETR:81;
    then LIN c,a,a1 by ANALMETR:def 11;
    then LIN c9,a9,a19 by ANALMETR:55;
    then LIN a9,a19,c9 by AFF_1:15;
    hence contradiction by A3,ANALMETR:55;
  end;
  then for x9 holds x9 in N9 iff LIN c9,c19,x9 by A19,A20,A21,
  AFF_1:33 ,39;
  then N9=Line(c9,c19) by AFF_1:def 2;
  then
A27: A9 // N9 by A18,A26,AFF_1:def 5;
A28: b9<>b19
  proof
    assume b9=b19;
    then b,a // b,a1 by A6,ANALMETR:81;
    then LIN b,a,a1 by ANALMETR:def 11;
    then LIN b9,a9,a19 by ANALMETR:55;
