theorem
  X is satisfying_OPAP & X is satisfying_DES implies X is satisfying_PAP
proof
  assume that
A1: X is satisfying_OPAP and
A2: X is satisfying_DES;
  let o,a1,a2,a3,b1,b2,b3,M,N;
  assume that
A3: M is being_line and
A4: N is being_line and
A5: o in M and
A6: a1 in M and
A7: a2 in M and
A8: a3 in M and
A9: o in N and
A10: b1 in N and
A11: b2 in N and
A12: b3 in N and
A13: not b2 in M and
A14: not a3 in N and
A15: o<>a1 and
A16: o<>a2 and
  o<>a3 and
A17: o<>b1 and
  o<>b2 and
