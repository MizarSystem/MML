environ

 vocabularies ANALMETR, SYMSP_1, ANALOAF, AFF_1, DIRAF, INCSP_1, AFF_2,
      CONAFFM, CONMETR;
 notations SUBSET_1, STRUCT_0, ANALOAF, AFF_1, ANALMETR, AFF_2, CONAFFM;
 constructors AFF_1, AFF_2, CONAFFM;
 registrations ANALMETR;
 requirements SUBSET;
 definitions AFF_2;
 theorems AFF_1, AFF_2, ANALMETR, TRANSLAC, CONAFFM, DIRAF;

begin

reserve X for OrtAfPl;

reserve o,a,a1,a2,a3,a4,b,b1,b2,b3,b4,c,c1,c2,c3,d,d1,d2,d3,d4,e1,e2 for
  Element of X;

reserve a2',a3',b2',x' for Element of Af(X);
reserve A,K,M,N for Subset of X;
reserve A',K' for Subset of Af(X);

definition
  let X;
  attr X is satisfying_OPAP means
  :Def1:
  for o,a1,a2,a3,b1,b2,b3,M,N st o in M
  & a1 in M & a2 in M & a3 in M & o in N & b1 in N & b2 in N & b3 in N & not b2
in M & not a3 in N & M _|_ N & o<>a1 & o<>a2 & o<>a3 & o<>b1 & o<>b2 & o<>b3 &
  a3,b2 // a2,b1 & a3,b3 // a1,b1 holds a1,b2 // a2,b3;
  attr X is satisfying_PAP means
  for o,a1,a2,a3,b1,b2,b3,M,N st M is
being_line & N is being_line & o in M & a1 in M & a2 in M & a3 in M & o in N &
b1 in N & b2 in N & b3 in N & not b2 in M & not a3 in N & o<>a1 & o<>a2 & o<>a3
& o<>b1 & o<>b2 & o<>b3 & a3,b2 // a2,b1 & a3,b3 // a1,b1 holds a1,b2 // a2,b3;
  attr X is satisfying_MH1 means
  :Def3:
  for a1,a2,a3,a4,b1,b2,b3,b4,M,N st M
  _|_ N & a1 in M & a3 in M & b1 in M & b3 in M & a2 in N & a4 in N & b2 in N &
b4 in N & not a2 in M & not a4 in M & a1,a2 _|_ b1,b2 & a2,a3 _|_ b2,b3 & a3,a4
  _|_ b3,b4 holds a1,a4 _|_ b1,b4;
  attr X is satisfying_MH2 means
  :Def4:
  for a1,a2,a3,a4,b1,b2,b3,b4,M,N st M
  _|_ N & a1 in M & a3 in M & b2 in M & b4 in M & a2 in N & a4 in N & b1 in N &
b3 in N & not a2 in M & not a4 in M & a1,a2 _|_ b1,b2 & a2,a3 _|_ b2,b3 & a3,a4
  _|_ b3,b4 holds a1,a4 _|_ b1,b4;
  attr X is satisfying_TDES means
  :Def5:
  for o,a,a1,b,b1,c,c1 st o<>a & o<>a1
& o<>b & o<>b1 & o<>c & o<>c1 & not LIN b,b1,a & not LIN b,b1,c & LIN o,a,a1 &
LIN o,b,b1 & LIN o,c,c1 & a,b // a1,b1 & a,b // o,c & b,c // b1,c1 holds a,c //
  a1,c1;
  attr X is satisfying_SCH means
  for a1,a2,a3,a4,b1,b2,b3,b4,M,N st M is
being_line & N is being_line & a1 in M & a3 in M & b1 in M & b3 in M & a2 in N
  & a4 in N & b2 in N & b4 in N & not a4 in M & not a2 in M & not b2 in M & not
b4 in M & not a1 in N & not a3 in N & not b1 in N & not b3 in N & a3,a2 // b3,
  b2 & a2,a1 // b2,b1 & a1,a4 // b1,b4 holds a3,a4 // b3,b4;
  attr X is satisfying_OSCH means
  :Def7:
  for a1,a2,a3,a4,b1,b2,b3,b4,M,N st M
  _|_ N & a1 in M & a3 in M & b1 in M & b3 in M & a2 in N & a4 in N & b2 in N &
b4 in N & not a4 in M & not a2 in M & not b2 in M & not b4 in M & not a1 in N &
not a3 in N & not b1 in N & not b3 in N & a3,a2 // b3,b2 & a2,a1 // b2,b1 & a1,
  a4 // b1,b4 holds a3,a4 // b3,b4;
  attr X is satisfying_des means
  :Def8:
  for a,a1,b,b1,c,c1 st not LIN a,a1,b &
  not LIN a,a1,c & a,a1 // b,b1 & a,a1 // c,c1 & a,b // a1,b1 & a,c // a1,c1
  holds b,c // b1,c1;
end;

theorem Th1:
  ex a,b,c st LIN a,b,c & a<>b & b<>c & c <>a
proof
  consider a,p being Element of X such that
A1: a<>p by ANALMETR:51;
  consider b such that
A2: a,p _|_ p,b and
A3: p<>b by ANALMETR:51;
  reconsider a'=a,b'=b,p'=p as Element of Af(X) by ANALMETR:47;
  consider c such that
A4: p,c _|_ a,b and
A5: LIN a,b,c by ANALMETR:91;
  take a,b,c;
  thus LIN a,b,c by A5;
  thus a<>b
  proof
    assume
    not thesis;
    then a,p _|_ a,p by A2,ANALMETR:83;
    hence contradiction by A1,ANALMETR:51;
  end;
  thus b<>c
  proof
    assume
    not thesis;
    then a,p // a,b by A2,A3,A4,ANALMETR:85;
    then LIN a,p,b by ANALMETR:def 11;
    then LIN a',p',b' by ANALMETR:55;
    then LIN p',a',b' by AFF_1:15;
    then LIN p,a,b by ANALMETR:55;
    then p,a // p,b by ANALMETR:def 11;
    then a,p _|_ p,a by A2,A3,ANALMETR:84;
    then a,p _|_ a,p by ANALMETR:83;
    hence contradiction by A1,ANALMETR:51;
  end;
  assume
  not thesis;
  then a,p _|_ a,b by A4,ANALMETR:83;
  then p,b // a,b by A1,A2,ANALMETR:85;
  then b,p // b,a by ANALMETR:81;
  then LIN b,p,a by ANALMETR:def 11;
  then LIN b',p',a' by ANALMETR:55;
  then LIN p',a',b' by AFF_1:15;
  then LIN p,a,b by ANALMETR:55;
  then p,a // p,b by ANALMETR:def 11;
  then a,p _|_ p,a by A2,A3,ANALMETR:84;
  then a,p _|_ a,p by ANALMETR:83;
  hence contradiction by A1,ANALMETR:51;
end;

theorem Th2:
  for a,b ex c st LIN a,b,c & a<>c & b<>c
proof
  let a,b;
  consider p,q,r being Element of X such that
A1: LIN p,q,r and
A2: p<>q and
A3: q<>r and
A4: r<>p by Th1;
  reconsider a'=a,b'=b,p'=p,q'=q,r'=r as Element of Af(X) by ANALMETR:47;
  LIN p',q',r' by A1,ANALMETR:55;
  then consider c' being Element of Af(X) such that
A5: LIN a',b',c' and
A6: a'<>c' and
A7: b'<>c' by A2,A3,A4,TRANSLAC:2;
  reconsider c =c' as Element of X by ANALMETR:47;
  LIN a,b,c by A5,ANALMETR:55;
  hence thesis by A6,A7;
end;

theorem Th3:
  for A,a st A is being_line ex K st a in K & A _|_ K
proof
  let A,a;
  assume
  A is being_line;
  then consider b,c such that
A1: b<>c and
A2: A = Line(b,c) by ANALMETR:def 13;
  consider d such that
A3: b,c _|_ a,d and
A4: a<>d by ANALMETR:51;
  reconsider a'=a,d'=d as Element of Af(X) by ANALMETR:47;
  take K = Line(a,d);
  K = Line(a',d') by ANALMETR:56;
  hence a in K by AFF_1:26;
  thus thesis by A1,A2,A3,A4,ANALMETR:63;
end;

theorem Th4:
  A is being_line & a in A & b in A & c in A implies LIN a,b,c
proof
  assume that
A1: A is being_line and
A2: a in A and
A3: b in A and
A4: c in A;
  reconsider a'=a,b'=b,c'=c as Element of Af(X) by ANALMETR:47;
  reconsider A'=A as Subset of Af(X) by ANALMETR:57;
  A' is being_line by A1,ANALMETR:58;
  then LIN a',b',c' by A2,A3,A4,AFF_1:33;
  hence thesis by ANALMETR:55;
end;

theorem Th5:
  A is being_line & M is being_line & a in A & b in A & a in M & b
  in M implies a=b or A=M
proof
  assume that
A1: A is being_line and
A2: M is being_line and
A3: a in A and
A4: b in A and
A5: a in M and
A6: b in M;
  reconsider A'=A,M'=M as Subset of Af X by ANALMETR:57;
A7: M' is being_line by A2,ANALMETR:58;
  assume
A8: a<>b;
  A' is being_line by A1,ANALMETR:58;
  hence thesis by A3,A4,A5,A6,A8,A7,AFF_1:30;
end;

theorem Th6:
  for a,b,c,d,M holds for M' being Subset of Af(X), c',d' being
Element of Af(X) st c =c' & d=d' & M=M' & a in M & b in M & c',d' // M' holds c
  ,d // a,b
proof
  let a,b,c,d,M;
  let M' be Subset of Af(X),c',d' be Element of Af(X) such that
A1: c =c' and
A2: d=d' and
A3: M=M' and
A4: a in M and
A5: b in M and
A6: c',d' // M';
  reconsider a'=a,b'=b as Element of Af(X) by ANALMETR:47;
  M' is being_line by A6,AFF_1:40;
  then a',b' // M' by A3,A4,A5,AFF_1:66;
  then c',d' // a',b' by A6,AFF_1:40,45;
  hence thesis by A1,A2,ANALMETR:48;
end;

theorem Th7:
  X is satisfying_TDES implies Af(X) is Moufangian
proof
  assume
A1: X is satisfying_TDES;
  let K' be Subset of Af(X);
  let o',a',b',c',a1',b1',c1' be Element of Af(X);
  assume that
A2: K' is being_line and
A3: o' in K' and
A4: c' in K' and
A5: c1' in K' and
A6: not a' in K' and
A7: o'<>c' and
A8: a'<>b' and
A9: LIN o',a',a1' and
A10: LIN o',b',b1' and
A11: a',b' // a1',b1' and
A12: a',c' // a1',c1' and
A13: a',b' // K';
  reconsider K=K' as Subset of X by ANALMETR:57;
  reconsider o=o',a=a',a1=a1',b=b',b1=b1',c =c',c1=c1' as Element
 of X by ANALMETR:47;
A14: a,c // a1,c1 by A12,ANALMETR:48;
A15: a,b // a1,b1 by A11,ANALMETR:48;
  now
A16: now
      assume
A17:  o<>a1;
A18:  o<>a & o<>b & o<>c & o<>c1 & o<>b1
      proof
A19:    now
          o',c' // K' by A2,A3,A4,AFF_1:37;
          then
A20:      a',b' // o',c' by A2,A13,AFF_1:45;
          assume
          o=b1;
          then a1',o' // o',c' by A8,A11,A20,DIRAF:47;
          then o',c' // o',a1' by AFF_1:13;
          then LIN o',c',a1' by AFF_1:def 1;
          then
A21:      a1' in K' by A2,A3,A4,A7,AFF_1:39;
          LIN o',a1',a' by A9,AFF_1:15;
          hence contradiction by A2,A3,A6,A17,A21,AFF_1:39;
        end;
A22:    now
          o',a' // o',a1' by A9,AFF_1:def 1;
          then
A23:      o',a1' // a', o' by AFF_1:13;
          assume
          o=c1;
          then o',a1' // a',c' by A12,AFF_1:13;
          then a',c' // a',o' by A17,A23,DIRAF:47;
          then LIN a',c',o' by AFF_1:def 1;
          then LIN o',c',a' by AFF_1:15;
          hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
        end;
        assume
        o=a or o=b or o=c or o=c1 or o=b1;
        hence contradiction by A3,A6,A7,A13,A22,A19,AFF_1:49;
      end;
A24:  now
        assume
A25:    a<>a1';
A26:    not LIN a,a1,b & not LIN a,a1,c
        proof
A27:      now
            LIN a',a1',o' by A9,AFF_1:15;
            then a',a1' // a',o' by AFF_1:def 1;
            then
A28:        a,a1 // a,o by ANALMETR:48;
            assume
            LIN a,a1,b;
            then a,a1 // a,b by ANALMETR:def 11;
            then a,b // a,o by A25,A28,ANALMETR:82;
            then
A29:        a,b // o,a by ANALMETR:81;
            a',b' // o',c' by A2,A3,A4,A7,A13,AFF_1:41;
            then a,b // o,c by ANALMETR:48;
            then o,c // o,a by A8,A29,ANALMETR:82;
            then LIN o,c,a by ANALMETR:def 11;
            then LIN o',c',a' by ANALMETR:55;
            hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
          end;
A30:      now
            LIN a',a1',o' by A9,AFF_1:15;
            then a',a1' // a',o' by AFF_1:def 1;
            then
A31:        a,a1 // a,o by ANALMETR:48;
            assume
            LIN a,a1,c;
            then a,a1 // a,c by ANALMETR:def 11;
            then a,c // a,o by A25,A31,ANALMETR:82;
            then LIN a,c,o by ANALMETR:def 11;
            then LIN a',c',o' by ANALMETR:55;
            then LIN c',o',a' by AFF_1:15;
            hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
          end;
          assume
          LIN a,a1,b or LIN a,a1,c;
          hence contradiction by A27,A30;
        end;
A32:    LIN o,b,b1 by A10,ANALMETR:55;
        o',c' // o',c1' by A2,A3,A4,A5,AFF_1:53,55;
        then o,c // o,c1 by ANALMETR:48;
        then
A33:    LIN o,c,c1 by ANALMETR:def 11;
        o',c' // K' by A2,A3,A4,AFF_1:54,55;
        then a',b' // o',c' by A2,A13,AFF_1:45;
        then b',a' // o',c' by AFF_1:13;
        then
A34:    b,a // o,c by ANALMETR:48;
A35:    b,a // b1,a1 by A15,ANALMETR:81;
        LIN o,a,a1 by A9,ANALMETR:55;
        then b,c // b1,c1 by A1,A14,A17,A18,A26,A32,A33,A35,A34,Def5;
        hence thesis by ANALMETR:48;
      end;
      now
A36:    not LIN o',a',c'
        proof
          assume
          LIN o',a',c';
          then LIN o',c',a' by AFF_1:15;
          hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
        end;
        assume
A37:    a=a1;
        o',c' // o',c1' by A2,A3,A4,A5,AFF_1:53,55;
        then LIN o',c',c1' by AFF_1:def 1;
        then
A38:    c'=c1' by A12,A37,A36,AFF_1:23;
        not LIN o',a',b'
        proof
          assume
          LIN o',a',b';
          then LIN a',b',o' by AFF_1:15;
          then
A39:      a',b' // a',o' by AFF_1:def 1;
          o',c' // K' by A2,A3,A4,AFF_1:37;
          then a',b' // o',c' by A2,A13,AFF_1:45;
          then a',o' // o',c' by A8,A39,DIRAF:47;
          then o',c' // o',a' by AFF_1:13;
          then LIN o',c',a' by AFF_1:def 1;
          hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
        end;
        then b'=b1' by A10,A11,A37,AFF_1:23;
        hence thesis by A38,AFF_1:11;
      end;
      hence thesis by A24;
    end;
    assume
A40: not b in K;
A41: o=a1 implies o=b1 & o=c1
    proof
      assume that
A42:  o=a1 and
A43:  o<>b1 or o<>c1;
A44:  now
A45:    o',c1' // a',c' by A12,A42,AFF_1:13;
        assume
A46:    o<>c1;
        o',c1' // o',c' by A2,A3,A4,A5,AFF_1:53,55;
        then a',c' // o',c' by A46,A45,DIRAF:47;
        then c',a' // c',o' by AFF_1:13;
        then LIN c',a',o' by AFF_1:def 1;
        then LIN o',c',a' by AFF_1:15;
        hence contradiction by A2,A3,A4,A6,A7,AFF_1:39;
      end;
      now
        o',c' // K' by A2,A3,A4,AFF_1:37;
        then a',b' // o',c' by A2,A13,AFF_1:45;
        then o',c' // o',b1' by A8,A11,A42,DIRAF:47;
        then LIN o',c',b1' by AFF_1:def 1;
        then
A47:    b1' in K' by A2,A3,A4,A7,AFF_1:39;
        assume
A48:    o<>b1;
        LIN o',b1',b' by A10,AFF_1:15;
        hence contradiction by A2,A3,A40,A48,A47,AFF_1:39;
      end;
      hence contradiction by A43,A44;
    end;
    now
      assume
      o=a1;
      then b,c // b1,c1 by A41,ANALMETR:51;
      hence thesis by ANALMETR:48;
    end;
    hence thesis by A16;
  end;
  hence thesis by A6,A13,AFF_1:49;
end;

theorem Th8:
  Af(X) is translational implies X is satisfying_des
proof
  assume
A1: Af(X) is translational;
  let a,a1,b,b1,c,c1;
  assume that
A2: not LIN a,a1,b and
A3: not LIN a,a1,c and
A4: a,a1 // b,b1 and
A5: a,a1 // c,c1 and
A6: a,b // a1,b1 and
A7: a,c // a1,c1;
  reconsider a'=a,a1'=a1,b'=b,b1'=b1,c'=c,c1'=c1 as Element of Af(X) by
  ANALMETR:47;
  LIN a',a1',a1' by AFF_1:16;
  then consider A' be Subset of Af(X) such that
A8: A' is being_line and
A9: a' in A' and
A10: a1' in A' and
  a1' in A' by AFF_1:33;
A11: a',b' // a1',b1' by A6,ANALMETR:48;
  b,b1 // a,a1 by A4,ANALMETR:81;
  then
A12: b',b1' // a',a1' by ANALMETR:48;
A13: a'<>a1'
  proof
    assume
    a'=a1';
    then LIN a',a1',b' by AFF_1:16;
    hence contradiction by A2,ANALMETR:55;
  end;
  then for x' holds x' in A' iff LIN a',a1',x' by A8,A9,A10,AFF_1:33,39;
  then A'=Line(a',a1') by AFF_1:def 2;
  then
A14: b',b1' // A' by A13,A12,AFF_1:def 4;
A15: a',c' // a1',c1' by A7,ANALMETR:48;
  c,c1 // a,a1 by A5,ANALMETR:81;
  then
A16: c',c1' // a',a1' by ANALMETR:48;
A17: a'<>a1'
  proof
    assume
    a'=a1';
    then LIN a',a1',b' by AFF_1:16;
    hence contradiction by A2,ANALMETR:55;
  end;
  then for x' holds x' in A' iff LIN a',a1',x' by A8,A9,A10,
  AFF_1:33 ,39;
  then A'=Line(a',a1') by AFF_1:def 2;
  then
A18: c',c1' // A' by A17,A16,AFF_1:def 4;
  LIN c',c1',c1' by AFF_1:16;
  then consider N' be Subset of Af(X) such that
A19: N' is being_line and
A20: c' in N' and
A21: c1' in N' and
  c1' in N' by AFF_1:33;
  LIN b',b1',b1' by AFF_1:16;
  then consider M' be Subset of Af(X) such that
A22: M' is being_line and
A23: b' in M' and
A24: b1' in M' and
  b1' in M' by AFF_1:33;
A25: A'<>M' & A'<>N'
  proof
    assume
    A'=M' or A'=N';
    then LIN a',a1',b' or LIN a',a1',c' by A8,A9,A10,A23,A20,AFF_1:33;
    hence contradiction by A2,A3,ANALMETR:55;
  end;
A26: c'<>c1'
  proof
    assume
    c'=c1';
    then c,a // c,a1 by A7,ANALMETR:81;
    then LIN c,a,a1 by ANALMETR:def 11;
    then LIN c',a',a1' by ANALMETR:55;
    then LIN a',a1',c' by AFF_1:15;
    hence contradiction by A3,ANALMETR:55;
  end;
  then for x' holds x' in N' iff LIN c',c1',x' by A19,A20,A21,
  AFF_1:33 ,39;
  then N'=Line(c',c1') by AFF_1:def 2;
  then
A27: A' // N' by A18,A26,AFF_1:def 5;
A28: b'<>b1'
  proof
    assume
    b'=b1';
    then b,a // b,a1 by A6,ANALMETR:81;
    then LIN b,a,a1 by ANALMETR:def 11;
    then LIN b',a',a1' by ANALMETR:55;
    then LIN a',a1',b' by AFF_1:15;
    hence contradiction by A2,ANALMETR:55;
  end;
  then for x' holds x' in M' iff LIN b',b1',x' by A22,A23,A24,AFF_1:33,39;
  then M'=Line(b',b1') by AFF_1:def 2;
  then A' // M' by A14,A28,AFF_1:def 5;
  then b',c' // b1',c1' by A1,A8,A9,A10,A22,A23,A24,A19,A20,A21,
  A27,A25,
  A11,A15,AFF_2:def 11;
  hence thesis by ANALMETR:48;
end;

theorem
  X is satisfying_MH1 implies X is satisfying_OSCH
proof
  assume
A1: X is satisfying_MH1;
  let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
  assume that
A2: M _|_ N and
A3: a1 in M and
A4: a3 in M and
A5: b1 in M and
A6: b3 in M and
A7: a2 in N and
A8: a4 in N and
A9: b2 in N and
A10: b4 in N and
A11: not a4 in M and
A12: not a2 in M and
A13: not b2 in M and
A14: not b4 in M and
A15: not a1 in N and
A16: not a3 in N and
A17: not b1 in N and
  not b3 in N and
A18: a3,a2 // b3,b2 and
A19: a2,a1 // b2,b1 and
A20: a1,a4 // b1,b4;
  reconsider M'=M,N'=N as Subset of Af(X) by ANALMETR:57;
  N is being_line by A2,ANALMETR:62;
  then
A21: N' is being_line by ANALMETR:58;
  reconsider b4'=b4,b1'=b1,b2'=b2,b3'=b3,a1'=a1,a2'=a2,a3'=a3,a4'=a4 as
  Element of Af(X) by ANALMETR:47;
  M is being_line by A2,ANALMETR:62;
  then
A22: M' is being_line by ANALMETR:58;
  not M' // N'
  proof
    assume
    M' // N';
    then M // N by ANALMETR:64;
    hence contradiction by A2,ANALMETR:73;
  end;
  then ex o' be Element of Af X st o' in M' & o' in N' by A22,A21,
  AFF_1:72;
  then consider o such that
A23: o in M and
A24: o in N;
  reconsider o'=o as Element of Af(X) by ANALMETR:47;
A25: now
    assume
A26: b2<>b4;
A27: now
      consider e1' be Element of Af(X) such that
A28:  o'<>e1' and
A29:  e1' in M' by A22,AFF_1:32;
      reconsider e1=e1' as Element of X by ANALMETR:47;
      ex d4' be Element of Af(X) st o'<>d4' & d4' in N' by A21,AFF_1:32;
      then consider d4 such that
A30:  d4 in N and
A31:  d4 <> o;
      reconsider d4'=d4 as Element of Af(X) by ANALMETR:47;
      consider e2 such that
A32:  a1,a4 _|_ d4,e2 and
A33:  e2<>d4 by ANALMETR:51;
      reconsider e2'=e2 as Element of Af(X) by ANALMETR:47;
      assume
A34:  b1<>b3;
      not o',e1' // d4',e2'
      proof
A35:    a4'<>a2'
        proof
          assume
          a4'=a2';
          then a2,a1 // b1,b4 by A20,ANALMETR:81;
          then b1,b4 // b2,b1 by A3,A12,A19,ANALMETR:82;
          then b1,b4 // b1,b2 by ANALMETR:81;
          then LIN b1,b4,b2 by ANALMETR:def 11;
          then LIN b1',b4',b2' by ANALMETR:55;
          then LIN b2',b4',b1' by AFF_1:15;
          hence contradiction by A9,A10,A17,A21,A26,AFF_1:39;
        end;
A36:    a1<>a3
        proof
          assume
          a1=a3;
          then a3,a2 // b2,b1 by A19,ANALMETR:81;
          then b3,b2 // b2,b1 by A4,A12,A18,ANALMETR:82;
          then b2,b3 // b2,b1 by ANALMETR:81;
          then LIN b2,b3,b1 by ANALMETR:def 11;
          then LIN b2',b3',b1' by ANALMETR:55;
          then LIN b1',b3',b2' by AFF_1:15;
          hence contradiction by A5,A6,A13,A22,A34,AFF_1:39;
        end;
        assume
        o',e1' // d4',e2';
        then o,e1 // d4,e2 by ANALMETR:48;
        then
A37:    o,e1 _|_ a1,a4 by A32,A33,ANALMETR:84;
        o',e1' // a1',a3' by A3,A4,A22,A23,A29,AFF_1:53,55;
        then o,e1 // a1,a3 by ANALMETR:48;
        then
A38:    a1,a3 _|_ a1,a4 by A28,A37,ANALMETR:84;
        a1,a3 _|_ a2,a4 by A2,A3,A4,A7,A8,ANALMETR:78;
        then a1,a4 // a2,a4 by A38,A36,ANALMETR:85;
        then a4,a2 // a4,a1 by ANALMETR:81;
        then LIN a4,a2,a1 by ANALMETR:def 11;
        then LIN a4',a2',a1' by ANALMETR:55;
        hence contradiction by A7,A8,A15,A21,A35,AFF_1:39;
      end;
      then consider d1' be Element of Af(X) such that
A39:  LIN o',e1',d1' and
A40:  LIN d4',e2',d1' by AFF_1:74;
      reconsider d1=d1' as Element of X by ANALMETR:47;
      consider e1' be Element of Af(X) such that
A41:  o'<>e1' and
A42:  e1' in N' by A21,AFF_1:32;
A43:  d1 in M by A22,A23,A28,A29,A39,AFF_1:39;
      LIN d4,e2,d1 by A40,ANALMETR:55;
      then d4,e2 // d4,d1 by ANALMETR:def 11;
      then
A44:  d1,d4 // d4,e2 by ANALMETR:81;
      then a1,a4 _|_ d1,d4 by A32,A33,ANALMETR:84;
      then
A45:  b1,b4 _|_ d1,d4 by A3,A11,A20,ANALMETR:84;
      reconsider e1=e1' as Element of X by ANALMETR:47;
      consider e2 such that
A46:  a2,a1 _|_ d1,e2 and
A47:  e2<>d1 by ANALMETR:51;
      reconsider e2'=e2 as Element of Af(X) by ANALMETR:47;
      not o',e1' // e2',d1'
      proof
A48:    a1'<>a3'
        proof
          assume
          a1'=a3';
          then a3,a2 // b2,b1 by A19,ANALMETR:81;
          then b3,b2 // b2,b1 by A4,A12,A18,ANALMETR:82;
          then b2,b3 // b2,b1 by ANALMETR:81;
          then LIN b2,b3,b1 by ANALMETR:def 11;
          then LIN b2',b3',b1' by ANALMETR:55;
          then LIN b1',b3',b2' by AFF_1:15;
          hence contradiction by A5,A6,A13,A22,A34,AFF_1:39;
        end;
        o',e1' // a2',a4' by A7,A8,A21,A24,A42,AFF_1:53,55;
        then
A49:    o,e1 // a2,a4 by ANALMETR:48;
A50:    a2<>a4
        proof
          assume
          a2=a4;
          then a2,a1 // b1,b4 by A20,ANALMETR:81;
          then b1,b4 // b2,b1 by A3,A12,A19,ANALMETR:82;
          then b1,b4 // b1,b2 by ANALMETR:81;
          then LIN b1,b4,b2 by ANALMETR:def 11;
          then LIN b1',b4',b2' by ANALMETR:55;
          then LIN b2',b4',b1' by AFF_1:15;
          hence contradiction by A9,A10,A17,A21,A26,AFF_1:39;
        end;
        assume
        o',e1' // e2',d1';
        then
A51:    o,e1 // e2,d1 by ANALMETR:48;
        a2,a1 _|_ e2,d1 by A46,ANALMETR:83;
        then o,e1 _|_ a2,a1 by A47,A51,ANALMETR:84;
        then
A52:    a2,a1 _|_ a2,a4 by A41,A49,ANALMETR:84;
        a3,a1 _|_ a2,a4 by A2,A3,A4,A7,A8,ANALMETR:78;
        then a3,a1 // a2,a1 by A52,A50,ANALMETR:85;
        then a1,a3 // a1,a2 by ANALMETR:81;
        then LIN a1,a3,a2 by ANALMETR:def 11;
        then LIN a1',a3',a2' by ANALMETR:55;
        hence contradiction by A3,A4,A12,A22,A48,AFF_1:39;
      end;
      then consider d2' be Element of Af(X) such that
A53:  LIN o',e1',d2' and
A54:  LIN e2',d1',d2' by AFF_1:74;
      reconsider d2=d2' as Element of X by ANALMETR:47;
A55:  d2 in N by A21,A24,A41,A42,A53,AFF_1:39;
      LIN d1',d2',e2' by A54,AFF_1:15;
      then LIN d1,d2,e2 by ANALMETR:55;
      then d1,d2 // d1,e2 by ANALMETR:def 11;
      then
A56:  d2,d1 // e2,d1 by ANALMETR:81;
A57:  a2,a1 _|_ e2,d1 by A46,ANALMETR:83;
      then
A58:  a2,a1 _|_ d2,d1 by A47,A56,ANALMETR:84;
      consider e1' be Element of Af(X) such that
A59:  o'<>e1' and
A60:  e1' in M' by A22,AFF_1:32;
      reconsider e1=e1' as Element of X by ANALMETR:47;
      consider e2 such that
A61:  a3,a2 _|_ d2,e2 and
A62:  e2<>d2 by ANALMETR:51;
      reconsider e2'=e2 as Element of Af(X) by ANALMETR:47;
      not o',e1' // e2',d2'
      proof
A63:    a2'<>a4'
        proof
          assume
          a2'=a4';
          then a2,a1 // b1,b4 by A20,ANALMETR:81;
          then b1,b4 // b2,b1 by A3,A12,A19,ANALMETR:82;
          then b1,b4 // b1,b2 by ANALMETR:81;
          then LIN b1,b4,b2 by ANALMETR:def 11;
          then LIN b1',b4',b2' by ANALMETR:55;
          then LIN b2',b4',b1' by AFF_1:15;
          hence contradiction by A9,A10,A17,A21,A26,AFF_1:39;
        end;
        assume
        o',e1' // e2',d2';
        then o,e1 // e2,d2 by ANALMETR:48;
        then o,e1 // d2,e2 by ANALMETR:81;
        then
A64:    a3,a2 _|_ o,e1 by A61,A62,ANALMETR:84;
        o,e1 _|_ a2,a4 by A2,A7,A8,A23,A60,ANALMETR:78;
        then a3,a2 // a2,a4 by A59,A64,ANALMETR:85;
        then a2,a4 // a2,a3 by ANALMETR:81;
        then LIN a2,a4,a3 by ANALMETR:def 11;
        then LIN a2',a4',a3' by ANALMETR:55;
        hence contradiction by A7,A8,A16,A21,A63,AFF_1:39;
      end;
      then consider d3' be Element of Af(X) such that
A65:  LIN o',e1',d3' and
A66:  LIN e2',d2',d3' by AFF_1:74;
      reconsider d3=d3' as Element of X by ANALMETR:47;
A67:  d3 in M by A22,A23,A59,A60,A65,AFF_1:39;
      then
A68:  d3<>d4 by A2,A22,A21,A23,A24,A30,A31,AFF_1:30;
      a2,a1 _|_ d2,d1 by A47,A56,A57,ANALMETR:84;
      then
A69:  b2,b1 _|_ d2,d1 by A3,A12,A19,ANALMETR:84;
      LIN d2',d3',e2' by A66,AFF_1:15;
      then LIN d2,d3,e2 by ANALMETR:55;
      then d2,d3 // d2,e2 by ANALMETR:def 11;
      then d3,d2 // d2,e2 by ANALMETR:81;
      then
A70:  a3,a2 _|_ d3,d2 by A61,A62,ANALMETR:84;
      then b3,b2 _|_ d3,d2 by A4,A12,A18,ANALMETR:84;
      then
A71:  b3,b4 _|_ d3,d4 by A1,A2,A5,A6,A9,A10,A13,A14,A30,A43,A55,A67,
      A45,A69,Def3;
      a1,a4 _|_ d1,d4 by A32,A33,A44,ANALMETR:84;
      then a3,a4 _|_ d3,d4 by A1,A2,A3,A4,A7,A8,A11,A12,A30,A43,
      A55,A58
      ,A70,A67,Def3;
      hence thesis by A68,A71,ANALMETR:85;
    end;
    now
A72:  not LIN o',a2',a1'
      proof
        assume
        LIN o',a2',a1';
        then o',a2' // o',a1' by AFF_1:def 1;
        hence contradiction by A7,A12,A15,A21,A23,A24,AFF_1:62;
      end;
A73:  LIN o',a1',a3' by A3,A4,A22,A23,AFF_1:33;
      assume
A74:  b1=b3;
      then a2,a3 // b2,b1 by A18,ANALMETR:81;
      then
A75:  a2',a3' // b2',b1' by ANALMETR:48;
      a2',a1' // b2',b1' by A19,ANALMETR:48;
      then a2',a1' // a2',a3' by A5,A13,A75,AFF_1:14;
      hence thesis by A20,A74,A72,A73,AFF_1:23;
    end;
    hence thesis by A27;
  end;
  now
A76: not LIN o',a1',a4'
    proof
      assume
      LIN o',a1',a4';
      then
      ex A' st A' is being_line & o' in A' & a1' in A' & a4' in A' by AFF_1:33;
      hence contradiction by A3,A11,A15,A22,A23,A24,AFF_1:30;
    end;
    assume
A77: b2=b4;
    b1,b2 // a1,a2 by A19,ANALMETR:81;
    then a1,a4 // a1,a2 by A5,A13,A20,A77,ANALMETR:82;
    then
A78: a1',a4' // a1',a2' by ANALMETR:48;
    LIN o',a4',a2' by A7,A8,A21,A24,AFF_1:33;
    hence thesis by A18,A77,A78,A76,AFF_1:23;
  end;
  hence thesis by A25;
end;

theorem
  X is satisfying_MH2 implies X is satisfying_OSCH
proof
  assume
A1: X is satisfying_MH2;
  let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
  assume that
A2: M _|_ N and
A3: a1 in M and
A4: a3 in M and
A5: b1 in M and
A6: b3 in M and
A7: a2 in N and
A8: a4 in N and
A9: b2 in N and
A10: b4 in N and
A11: not a4 in M and
A12: not a2 in M and
A13: not b2 in M and
A14: not b4 in M and
A15: not a1 in N and
A16: not a3 in N and
A17: not b1 in N and
  not b3 in N and
A18: a3,a2 // b3,b2 and
A19: a2,a1 // b2,b1 and
A20: a1,a4 // b1,b4;
  reconsider M'=M,N'=N as Subset of Af(X) by ANALMETR:57;
  N is being_line by A2,ANALMETR:62;
  then
A21: N' is being_line by ANALMETR:58;
  reconsider b4'=b4,b1'=b1,b2'=b2,b3'=b3,a1'=a1,a2'=a2,a3'=a3,a4'=a4 as
  Element of Af(X) by ANALMETR:47;
  M is being_line by A2,ANALMETR:62;
  then
A22: M' is being_line by ANALMETR:58;
  not M' // N'
  proof
    assume
    M' // N';
    then M // N by ANALMETR:64;
    hence contradiction by A2,ANALMETR:73;
  end;
  then ex o' be Element of Af(X) st o' in M' & o' in N' by A22,A21,AFF_1:
  72;
  then consider o such that
A23: o in M and
A24: o in N;
  reconsider o'=o as Element of Af(X) by ANALMETR:47;
A25: now
    assume
A26: b2<>b4;
A27: now
      ex d3' be Element of Af(X) st o'<>d3' & d3' in N' by A21,AFF_1:
      32;
      then consider d3 such that
A28:  d3 in N and
A29:  d3 <>o;
      reconsider d3'=d3 as Element of Af(X) by ANALMETR:47;
      consider e2 such that
A30:  a3,a2 _|_ d3,e2 and
A31:  d3<>e2 by ANALMETR:def 10;
      consider e1 such that
A32:  a3,a1 // a3,e1 and
A33:  a3<>e1 by ANALMETR:51;
      reconsider e1'=e1,e2'=e2 as Element of Af(X) by ANALMETR:47;
      assume
A34:  b1<>b3;
A35:  a1<>a3 & a2<>a4
      proof
        assume
        a1=a3 or a2=a4;
        then a2,a1 // b3,b2 or a1,a4 // b2,b1 by A18,A19,ANALMETR:81;
        then b3,b2 // b2,b1 or b2,b1 // b1,b4 by A3,A11,A12,A19,A20,
        ANALMETR:82;
        then b2,b3 // b2,b1 or b1,b2 // b1,b4 by ANALMETR:81;
        then LIN b2,b3,b1 or LIN b1,b2,b4 by ANALMETR:def 11;
        then LIN b2',b3',b1' or LIN b1',b2',b4' by ANALMETR:55;
        then LIN b1',b3',b2' or LIN b2',b4',b1' by AFF_1:15;
        hence contradiction by A5,A6,A9,A10,A13,A17,A22,A21,A26,A34, AFF_1:
        39;
      end;
      not a3',e1' // d3',e2'
      proof
        assume
        a3',e1' // d3',e2';
        then a3,e1 // d3,e2 by ANALMETR:48;
        then a3,a1 // d3,e2 by A32,A33,ANALMETR:82;
        then
A36:    a3,a2 _|_ a3,a1 by A30,A31,ANALMETR:84;
        a3,a1 _|_ a2,a4 by A2,A3,A4,A7,A8,ANALMETR:78;
        then a3,a2 // a2,a4 by A35,A36,ANALMETR:85;
        then a2,a4 // a2,a3 by ANALMETR:81;
        then LIN a2,a4,a3 by ANALMETR:def 11;
        then LIN a2',a4',a3' by ANALMETR:55;
        hence contradiction by A7,A8,A16,A21,A35,AFF_1:39;
      end;
      then consider d2' be Element of Af(X) such that
A37:  LIN a3',e1',d2' and
A38:  LIN d3',e2',d2' by AFF_1:74;
      reconsider d2=d2' as Element of X by ANALMETR:47;
      LIN d3,e2,d2 by A38,ANALMETR:55;
      then d3,e2 // d3,d2 by ANALMETR:def 11;
      then
A39:  a3,a2 _|_ d3,d2 by A30,A31,ANALMETR:84;
      LIN a3,e1,d2 by A37,ANALMETR:55;
      then a3,e1 // a3,d2 by ANALMETR:def 11;
      then a3,a1 // a3,d2 by A32,A33,ANALMETR:82;
      then LIN a3,a1,d2 by ANALMETR:def 11;
      then LIN a3',a1',d2' by ANALMETR:55;
      then consider d2 such that
A40:  d2 in M and
A41:  a3,a2 _|_ d3,d2 by A3,A4,A22,A35,A39,AFF_1:39;
      reconsider d2'=d2 as Element of Af(X) by ANALMETR:47;
      consider e1 such that
A42:  a2,a4 // a2,e1 and
A43:  a2<>e1 by ANALMETR:51;
      consider e2 such that
A44:  a2,a1 _|_ d2,e2 and
A45:  d2<>e2 by ANALMETR:def 10;
      reconsider e1'=e1,e2'=e2 as Element of Af(X) by ANALMETR:47;
      not a2',e1' // d2',e2'
      proof
        assume
        a2',e1' // d2',e2';
        then a2,e1 // d2,e2 by ANALMETR:48;
        then a2,a4 // d2,e2 by A42,A43,ANALMETR:82;
        then
A46:    a2,a4 _|_ a2,a1 by A44,A45,ANALMETR:84;
        a1,a3 _|_ a2,a4 by A2,A3,A4,A7,A8,ANALMETR:78;
        then a1,a3 // a2,a1 by A35,A46,ANALMETR:85;
        then a1,a3 // a1,a2 by ANALMETR:81;
        then LIN a1,a3,a2 by ANALMETR:def 11;
        then LIN a1',a3',a2' by ANALMETR:55;
        hence contradiction by A3,A4,A12,A22,A35,AFF_1:39;
      end;
      then consider d1' be Element of Af(X) such that
A47:  LIN a2',e1',d1' and
A48:  LIN d2',e2',d1' by AFF_1:74;
      reconsider d1=d1' as Element of X by ANALMETR:47;
A49:  b3,b2 _|_ d3,d2 by A4,A12,A18,A41,ANALMETR:84;
      LIN a2,e1,d1 by A47,ANALMETR:55;
      then a2,e1 // a2,d1 by ANALMETR:def 11;
      then a2,a4 // a2,d1 by A42,A43,ANALMETR:82;
      then LIN a2,a4,d1 by ANALMETR:def 11;
      then LIN a2',a4',d1' by ANALMETR:55;
      then
A50:  d1 in N by A7,A8,A21,A35,AFF_1:39;
      LIN d2,e2,d1 by A48,ANALMETR:55;
      then d2,e2 // d2,d1 by ANALMETR:def 11;
      then
A51:  a2,a1 _|_ d2,d1 by A44,A45,ANALMETR:84;
      then
A52:  b2,b1 _|_ d2,d1 by A3,A12,A19,ANALMETR:84;
      consider e2 such that
A53:  a1,a4 _|_ d1,e2 and
A54:  d1<>e2 by ANALMETR:51;
      consider e1 such that
A55:  a1,a3 // a1,e1 and
A56:  a1<>e1 by ANALMETR:51;
      reconsider e1'=e1,e2'=e2 as Element of Af(X) by ANALMETR:47;
      not a1',e1' // d1',e2'
      proof
        assume
        a1',e1' // d1',e2';
        then a1,e1 // d1,e2 by ANALMETR:48;
        then a1,a3 // d1,e2 by A55,A56,ANALMETR:82;
        then
A57:    a1,a3 _|_ a1,a4 by A53,A54,ANALMETR:84;
        a1,a3 _|_ a2,a4 by A2,A3,A4,A7,A8,ANALMETR:78;
        then a2,a4 // a1,a4 by A35,A57,ANALMETR:85;
        then a4,a2 // a4,a1 by ANALMETR:81;
        then LIN a4,a2,a1 by ANALMETR:def 11;
        then LIN a4',a2',a1' by ANALMETR:55;
        hence contradiction by A7,A8,A15,A21,A35,AFF_1:39;
      end;
      then consider d4' be Element of Af(X) such that
A58:  LIN a1',e1',d4' and
A59:  LIN d1',e2',d4' by AFF_1:74;
      reconsider d4=d4' as Element of X by ANALMETR:47;
      LIN a1,e1,d4 by A58,ANALMETR:55;
      then a1,e1 // a1,d4 by ANALMETR:def 11;
      then a1,a3 // a1,d4 by A55,A56,ANALMETR:82;
      then LIN a1,a3,d4 by ANALMETR:def 11;
      then LIN a1',a3',d4' by ANALMETR:55;
      then
A60:  d4 in M by A3,A4,A22,A35,AFF_1:39;
      then
A61:  d3<>d4 by A2,A22,A21,A23,A24,A28,A29,AFF_1:30;
      LIN d1,e2,d4 by A59,ANALMETR:55;
      then d1,e2 // d1,d4 by ANALMETR:def 11;
      then
A62:  a1,a4 _|_ d1,d4 by A53,A54,ANALMETR:84;
      then b1,b4 _|_ d1,d4 by A3,A11,A20,ANALMETR:84;
      then
A63:  b3,b4 _|_ d3,d4 by A1,A2,A5,A6,A9,A10,A13,A14,A28,A40,A50,A60,
      A49,A52,Def4;
      a3,a4 _|_ d3,d4 by A1,A2,A3,A4,A7,A8,A11,A12,A28,A40,A41,A50,A51
      ,A60,A62,Def4;
      hence thesis by A63,A61,ANALMETR:85;
    end;
    now
      o',a1' // o',a3' by A3,A4,A22,A23,AFF_1:53,55;
      then
A64:  LIN o',a1',a3' by AFF_1:def 1;
      assume
A65:  b1=b3;
      then a2,a1 // b3,b2 by A19,ANALMETR:81;
      then a2,a1 // a3,a2 by A6,A13,A18,ANALMETR:82;
      then a2,a1 // a2,a3 by ANALMETR:81;
      then a2',a1' // a2',a3' by ANALMETR:48;
      hence thesis by A7,A12,A15,A20,A21,A23,A24,A65,A64,AFF_1:23,39;
    end;
    hence thesis by A27;
  end;
  now
    o',a2' // o',a4' by A7,A8,A21,A24,AFF_1:53,55;
    then
A66: LIN o',a2',a4' by AFF_1:def 1;
    assume
A67: b2=b4;
    then a1,a4 // b2,b1 by A20,ANALMETR:81;
    then a2,a1 // a1,a4 by A5,A13,A19,ANALMETR:82;
    then a1,a2 // a1,a4 by ANALMETR:81;
    then a1',a2' // a1',a4' by ANALMETR:48;
    hence thesis by A3,A12,A15,A18,A22,A23,A24,A67,A66,AFF_1:23,39;
  end;
  hence thesis by A25;
end;

theorem
  X is satisfying_AH implies X is satisfying_TDES
proof
  assume
A1: X is satisfying_AH;
  let o,a,a1,b,b1,c,c1;
  assume that
A2: o<>a and
A3: o<>a1 and
A4: o<>b and
A5: o<>b1 and
A6: o<>c and
A7: o<>c1 and
A8: not LIN b,b1,a and
A9: not LIN b,b1,c and
A10: LIN o,a,a1 and
A11: LIN o,b,b1 and
A12: LIN o,c,c1 and
A13: a,b // a1,b1 and
A14: a,b // o,c and
A15: b,c // b1,c1;
  consider e1 such that
A16: o,b _|_ o,e1 and
A17: o<>e1 by ANALMETR:def 10;
  consider c2 such that
A18: o,c _|_ o,c2 and
A19: o<>c2 by ANALMETR:def 10;
  consider e2 such that
A20: b,c _|_ c2,e2 and
A21: c2<>e2 by ANALMETR:def 10;
  reconsider o'=o,a'=a,a1'=a1,b'=b,b1'=b1,c' = c,c1'= c1,c2'= c2,e1'=e1, e2'=
  e2 as Element of Af(X) by ANALMETR:47;
A22: b1<>a1
  proof
    assume
    b1=a1;
    then LIN o',b',a1' by A11,ANALMETR:55;
    then o',b' // o',a1' by AFF_1:def 1;
    then o',a1' // o',b' by AFF_1:13;
    then
A23: o,a1 // o,b by ANALMETR:48;
    o,a // o,a1 by A10,ANALMETR:def 11;
    then o',a' // o',a1' by ANALMETR:48;
    then o',a1' // o',a' by AFF_1:13;
    then o,a1 // o,a by ANALMETR:48;
    then o,a // o,b by A3,A23,ANALMETR:82;
    then LIN o,a,b by ANALMETR:def 11;
    then
A24: LIN o',a',b' by ANALMETR:55;
    then LIN b',o',a' by AFF_1:15;
    then b',o' // b',a' by AFF_1:def 1;
    then o',b' // a',b' by AFF_1:13;
    then
A25: o,b // a,b by ANALMETR:48;
A26: a'<>b'
    proof
      assume
      a'=b';
      then LIN b',b1',a' by AFF_1:16;
      hence contradiction by A8,ANALMETR:55;
    end;
    o,b // o,b1 by A11,ANALMETR:def 11;
    then a,b // o,b1 by A4,A25,ANALMETR:82;
    then
A27: a',b' // o',b1' by ANALMETR:48;
    LIN a',b',o' by A24,AFF_1:15;
    then LIN a',b',b1' by A27,A26,AFF_1:18;
    then LIN b',b1',a' by AFF_1:15;
    hence contradiction by A8,ANALMETR:55;
  end;
A28: now
    not o',e1' // c2',e2'
    proof
      assume
      o',e1' // c2',e2';
      then o,e1 // c2,e2 by ANALMETR:48;
      then c2,e2 _|_ o,b by A16,A17,ANALMETR:84;
      then c2,e2 _|_ b,o by ANALMETR:83;
      then b,c // b,o by A20,A21,ANALMETR:85;
      then LIN b,c,o by ANALMETR:def 11;
      then LIN b',c',o' by ANALMETR:55;
      then LIN b',o',c' by AFF_1:15;
      then LIN b,o,c by ANALMETR:55;
      then
A29:  b,o // b,c by ANALMETR:def 11;
      LIN o',b',b1' by A11,ANALMETR:55;
      then LIN b',o',b1' by AFF_1:15;
      then LIN b,o,b1 by ANALMETR:55;
      then b,o // b,b1 by ANALMETR:def 11;
      then b,b1 // b,c by A4,A29,ANALMETR:82;
      hence contradiction by A9,ANALMETR:def 11;
    end;
    then consider b2' such that
A30: LIN o',e1',b2' and
A31: LIN c2',e2',b2' by AFF_1:74;
    reconsider b2=b2' as Element of X by ANALMETR:47;
    LIN o,e1,b2 by A30,ANALMETR:55;
    then o,e1 // o,b2 by ANALMETR:def 11;
    then
A32: o,b _|_ o,b2 by A16,A17,ANALMETR:84;
    o,b // o,b1 by A11,ANALMETR:def 11;
    then
A33: o,b1 _|_ o,b2 by A4,A32,ANALMETR:84;
    assume
A34: not LIN a,b,c;
A35: b<>a
    proof
      assume
      b=a;
      then LIN a',b',c' by AFF_1:16;
      hence contradiction by A34,ANALMETR:55;
    end;
A36: not o,c // o,b
    proof
      assume
      o,c // o,b;
      then LIN o,c,b by ANALMETR:def 11;
      then LIN o',c',b' by ANALMETR:55;
      then LIN c',o',b' by AFF_1:15;
      then c',o' // c',b' by AFF_1:def 1;
      then o',c' // b',c' by AFF_1:13;
      then
A37:  o,c // b,c by ANALMETR:48;
      a',b' // o',c' by A14,ANALMETR:48;
      then o',c' // b',a' by AFF_1:13;
      then o,c // b,a by ANALMETR:48;
      then b,a // b,c by A6,A37,ANALMETR:82;
      then LIN b,a,c by ANALMETR:def 11;
      then LIN b',a',c' by ANALMETR:55;
      then LIN a',b',c' by AFF_1:15;
      hence contradiction by A34,ANALMETR:55;
    end;
A38: not o,a // o,c
    proof
      assume
      o,a // o,c;
      then LIN o,a,c by ANALMETR:def 11;
      then LIN o',a',c' by ANALMETR:55;
      then LIN c',o',a' by AFF_1:15;
      then LIN c,o,a by ANALMETR:55;
      then
A39:  c,o // c,a by ANALMETR:def 11;
      a',b' // o',c' by A14,ANALMETR:48;
      then c',o' // a',b' by AFF_1:13;
      then c,o // a,b by ANALMETR:48;
      then a,b // c,a by A6,A39,ANALMETR:82;
      then a',b' // c',a' by ANALMETR:48;
      then a',b' // a',c' by AFF_1:13;
      then a,b // a,c by ANALMETR:48;
      hence contradiction by A34,ANALMETR:def 11;
    end;
    LIN c2,e2,b2 by A31,ANALMETR:55;
    then c2,e2 // c2,b2 by ANALMETR:def 11;
    then
A40: b,c _|_ c2,b2 by A20,A21,ANALMETR:84;
    consider e1 such that
A41: o,a _|_ o,e1 and
A42: o<>e1 by ANALMETR:def 10;
    consider e2 such that
A43: a,c _|_ c2,e2 and
A44: c2<>e2 by ANALMETR:def 10;
    reconsider e1'=e1,e2'=e2 as Element of Af(X) by ANALMETR:47;
    not o',e1' // c2',e2'
    proof
      assume
      o',e1' // c2',e2';
      then o,e1 // c2,e2 by ANALMETR:48;
      then c2,e2 _|_ o,a by A41,A42,ANALMETR:84;
      then a,c // o,a by A43,A44,ANALMETR:85;
      then a',c' // o',a' by ANALMETR:48;
      then a',c' // a',o' by AFF_1:13;
      then LIN a',c',o' by AFF_1:def 1;
      then LIN c',o',a' by AFF_1:15;
      then LIN c,o,a by ANALMETR:55;
      then
A45:  c,o // c,a by ANALMETR:def 11;
      a',b' // o',c' by A14,ANALMETR:48;
      then c',o' // a',b' by AFF_1:13;
      then c,o // a,b by ANALMETR:48;
      then a,b // c,a by A6,A45,ANALMETR:82;
      then a',b' // c',a' by ANALMETR:48;
      then a',b' // a',c' by AFF_1:13;
      then a,b // a,c by ANALMETR:48;
      hence contradiction by A34,ANALMETR:def 11;
    end;
    then consider a2' such that
A46: LIN o',e1',a2' and
A47: LIN c2',e2',a2' by AFF_1:74;
    reconsider a2=a2' as Element of X by ANALMETR:47;
    LIN o,e1,a2 by A46,ANALMETR:55;
    then o,e1 // o,a2 by ANALMETR:def 11;
    then
A48: o,a _|_ o,a2 by A41,A42,ANALMETR:84;
A49: c2<>a2
    proof
      assume
      c2=a2;
      then o,c // o,a by A18,A19,A48,ANALMETR:85;
      then LIN o,c,a by ANALMETR:def 11;
      then LIN o',c',a' by ANALMETR:55;
      then LIN c',a',o' by AFF_1:15;
      then c',a' // c',o' by AFF_1:def 1;
      then o',c' // a',c' by AFF_1:13;
      then o,c // a,c by ANALMETR:48;
      then a,b // a,c by A6,A14,ANALMETR:82;
      hence contradiction by A34,ANALMETR:def 11;
    end;
    LIN c2,e2,a2 by A47,ANALMETR:55;
    then c2,e2 // c2,a2 by ANALMETR:def 11;
    then a,c _|_ c2,a2 by A43,A44,ANALMETR:84;
    then
A50: c,a _|_ c2,a2 by ANALMETR:83;
A51: not LIN o',b2',a2'
    proof
A52:  o<>b2
      proof
        a',b' // o',c' by A14,ANALMETR:48;
        then o',c' // b',a' by AFF_1:13;
        then
A53:    o,c // b,a by ANALMETR:48;
        assume
        o=b2;
        then o,c2 _|_ b,c by A40,ANALMETR:83;
        then o,c // b,c by A18,A19,ANALMETR:85;
        then b,a // b,c by A6,A53,ANALMETR:82;
        then LIN b,a,c by ANALMETR:def 11;
        then LIN b',a',c' by ANALMETR:55;
        then LIN a',b',c' by AFF_1:15;
        hence contradiction by A34,ANALMETR:55;
      end;
A54:  o<>a2
      proof
        assume
A55:    o=a2;
        c2,o _|_ o,c by A18,ANALMETR:83;
        then o,c // c,a by A19,A50,A55,ANALMETR:85;
        then a,b // c,a by A6,A14,ANALMETR:82;
        then a',b' // c',a' by ANALMETR:48;
        then a',b' // a',c' by AFF_1:13;
        then a,b // a,c by ANALMETR:48;
        hence contradiction by A34,ANALMETR:def 11;
      end;
      assume
      LIN o',b2',a2';
      then LIN o,b2,a2 by ANALMETR:55;
      then o,b2 // o,a2 by ANALMETR:def 11;
      then o,a2 _|_ o,b by A32,A52,ANALMETR:84;
      then o,a // o,b by A48,A54,ANALMETR:85;
      then LIN o,a,b by ANALMETR:def 11;
      then LIN o',a',b' by ANALMETR:55;
      then
A56:  LIN a',b',o' by AFF_1:15;
A57:  a'<>b'
      proof
        assume
        a'=b';
        then LIN a',b',c' by AFF_1:16;
        hence contradiction by A34,ANALMETR:55;
      end;
      a',b' // o',c' by A14,ANALMETR:48;
      then LIN a',b',c' by A56,A57,AFF_1:18;
      hence contradiction by A34,ANALMETR:55;
    end;
    consider e1 such that
A58: o,a1 _|_ o,e1 and
A59: o<>e1 by ANALMETR:def 10;
    consider e2 such that
A60: a1,c1 _|_ c2,e2 and
A61: c2<>e2 by ANALMETR:def 10;
    reconsider e1'=e1,e2'=e2 as Element of Af(X) by ANALMETR:47;
    not o',e1' // c2',e2'
    proof
      assume
      o',e1' // c2',e2';
      then o,e1 // c2,e2 by ANALMETR:48;
      then c2,e2 _|_ o,a1 by A58,A59,ANALMETR:84;
      then a1,c1 // o,a1 by A60,A61,ANALMETR:85;
      then a1',c1' // o',a1' by ANALMETR:48;
      then a1',c1' // a1',o' by AFF_1:13;
      then LIN a1',c1',o' by AFF_1:def 1;
      then LIN o',c1',a1' by AFF_1:15;
      then o',c1' // o',a1' by AFF_1:def 1;
      then
A62:  o,c1 // o,a1 by ANALMETR:48;
      LIN o',a',a1' by A10,ANALMETR:55;
      then o',a' // o',a1' by AFF_1:def 1;
      then o',a1' // o',a' by AFF_1:13;
      then
A63:  o,a1 // o,a by ANALMETR:48;
      LIN o',c',c1' by A12,ANALMETR:55;
      then o',c' // o',c1' by AFF_1:def 1;
      then o',c1' // o',c' by AFF_1:13;
      then o,c1 // o,c by ANALMETR:48;
      then o,a1 // o,c by A7,A62,ANALMETR:82;
      then o,a // o,c by A3,A63,ANALMETR:82;
      then LIN o,a,c by ANALMETR:def 11;
      then LIN o',a',c' by ANALMETR:55;
      then LIN c',o',a' by AFF_1:15;
      then c',o' // c',a' by AFF_1:def 1;
      then o',c' // a',c' by AFF_1:13;
      then o,c // a,c by ANALMETR:48;
      then a,b // a,c by A6,A14,ANALMETR:82;
      hence contradiction by A34,ANALMETR:def 11;
    end;
    then consider a3' such that
A64: LIN o',e1',a3' and
A65: LIN c2',e2',a3' by AFF_1:74;
    reconsider a3=a3' as Element of X by ANALMETR:47;
    LIN o,e1,a3 by A64,ANALMETR:55;
    then o,e1 // o,a3 by ANALMETR:def 11;
    then
A66: o,a1 _|_ o,a3 by A58,A59,ANALMETR:84;
    b<>c
    proof
      assume
      b= c;
      then LIN a',b',c' by AFF_1:16;
      hence contradiction by A34,ANALMETR:55;
    end;
    then c2,b2 _|_ b1,c1 by A15,A40,ANALMETR:84;
    then
A67: c1,b1 _|_ c2,b2 by ANALMETR:83;
A68: not o,a1 // o,c1
    proof
      o,a // o,a1 by A10,ANALMETR:def 11;
      then o',a' // o',a1' by ANALMETR:48;
      then o',a1' // o',a' by AFF_1:13;
      then
A69:  o,a1 // o,a by ANALMETR:48;
      assume
      o,a1 // o,c1;
      then o',a1' // o',c1' by ANALMETR:48;
      then o',c1' // o',a1' by AFF_1:13;
      then
A70:  o,c1 // o,a1 by ANALMETR:48;
      o,c // o,c1 by A12,ANALMETR:def 11;
      then o',c' // o',c1' by ANALMETR:48;
      then o',c1' // o',c' by AFF_1:13;
      then o,c1 // o,c by ANALMETR:48;
      then o,a1 // o,c by A7,A70,ANALMETR:82;
      then o,a // o,c by A3,A69,ANALMETR:82;
      then LIN o,a,c by ANALMETR:def 11;
      then LIN o',a',c' by ANALMETR:55;
      then LIN c',a',o' by AFF_1:15;
      then c',a' // c',o' by AFF_1:def 1;
      then o',c'// a',c' by AFF_1:13;
      then o,c // a,c by ANALMETR:48;
      then a,b // a,c by A6,A14,ANALMETR:82;
      hence contradiction by A34,ANALMETR:def 11;
    end;
    a',b' // o',c' by A14,ANALMETR:48;
    then o',c' // b',a' by AFF_1:13;
    then
A71: o,c // b,a by ANALMETR:48;
A72: not o,c1 // o,b1
    proof
      o,b // o,b1 by A11,ANALMETR:def 11;
      then o',b' // o',b1' by ANALMETR:48;
      then o',b1' // o',b' by AFF_1:13;
      then
A73:  o,b1 // o,b by ANALMETR:48;
      o,c // o,c1 by A12,ANALMETR:def 11;
      then o',c' // o',c1' by ANALMETR:48;
      then o',c1' // o',c' by AFF_1:13;
      then
A74:  o,c1 // o,c by ANALMETR:48;
      assume
      o,c1 // o,b1;
      then o,b1 // o,c by A7,A74,ANALMETR:82;
      then o,b // o,c by A5,A73,ANALMETR:82;
      then LIN o,b,c by ANALMETR:def 11;
      then LIN o',b',c' by ANALMETR:55;
      then LIN c',o',b' by AFF_1:15;
      then c',o' // c',b' by AFF_1:def 1;
      then o',c' // b',c' by AFF_1:13;
      then
A75:  o,c // b,c by ANALMETR:48;
      a',b' // o',c' by A14,ANALMETR:48;
      then o',c' // b',a' by AFF_1:13;
      then o,c // b,a by ANALMETR:48;
      then b,a // b,c by A6,A75,ANALMETR:82;
      then LIN b,a,c by ANALMETR:def 11;
      then LIN b',a',c' by ANALMETR:55;
      then LIN a',b',c' by AFF_1:15;
      hence contradiction by A34,ANALMETR:55;
    end;
    a<>b
    proof
      assume
      a=b;
      then LIN b',b1',a' by AFF_1:16;
      hence contradiction by A8,ANALMETR:55;
    end;
    then o,c // a1,b1 by A13,A14,ANALMETR:82;
    then o',c' // a1',b1' by ANALMETR:48;
    then o',c' // b1',a1' by AFF_1:13;
    then
A76: o,c // b1,a1 by ANALMETR:48;
    o,c // o,c1 by A12,ANALMETR:def 11;
    then
A77: o,c1 // b1,a1 by A6,A76,ANALMETR:82;
    o,c // o,c1 by A12,ANALMETR:def 11;
    then
A78: o,c1 _|_ o,c2 by A6,A18,ANALMETR:84;
    c,b _|_ c2,b2 by A40,ANALMETR:83;
    then b,a _|_ b2,a2 by A1,A18,A32,A48,A71,A50,A38,A36, CONAFFM:
    def 2;
    then b2,a2 _|_ a,b by ANALMETR:83;
    then b2,a2 _|_ a1,b1 by A13,A35,ANALMETR:84;
    then
A79: b1,a1 _|_ b2,a2 by ANALMETR:83;
    o,a // o,a1 by A10,ANALMETR:def 11;
    then o,a1 _|_ o,a2 by A2,A48,ANALMETR:84;
    then o,a2 // o,a3 by A3,A66,ANALMETR:85;
    then LIN o,a2,a3 by ANALMETR:def 11;
    then
A80: LIN o',a2',a3' by ANALMETR:55;
    LIN c2,e2,a3 by A65,ANALMETR:55;
    then c2,e2 // c2,a3 by ANALMETR:def 11;
    then
A81: a1,c1 _|_ c2,a3 by A60,A61,ANALMETR:84;
    then c1,a1 _|_ c2,a3 by ANALMETR:83;
    then b1,a1 _|_ b2,a3 by A1,A66,A78,A33,A67,A77,A68,A72,
    CONAFFM:def 2;
    then b2,a3 // b2,a2 by A22,A79,ANALMETR:85;
    then b2',a3' // b2',a2' by ANALMETR:48;
    then b2',a2' // b2',a3' by AFF_1:13;
    then a2'=a3' by A51,A80,AFF_1:23;
    then c,a // a1,c1 by A50,A81,A49,ANALMETR:85;
    then c',a' // a1',c1' by ANALMETR:48;
    then a',c' // a1',c1' by AFF_1:13;
    hence thesis by ANALMETR:48;
  end;
  now
    assume
A82: LIN a,b,c;
    then
A83: LIN a',b',c' by ANALMETR:55;
A84: a<>b
    proof
      assume
      a=b;
      then LIN b',b1',a' by AFF_1:16;
      hence contradiction by A8,ANALMETR:55;
    end;
A85: now
      assume
      a1,c1 // a,b;
      then a1',c1' // a',b' by ANALMETR:48;
      then a',b' // a1',c1' by AFF_1:13;
      then
A86:  a,b // a1,c1 by ANALMETR:48;
      a,b // a,c by A82,ANALMETR:def 11;
      hence thesis by A84,A86,ANALMETR:82;
    end;
A87: b<>c
    proof
      assume
      b=c;
      then LIN b',b1',c' by AFF_1:16;
      hence contradiction by A9,ANALMETR:55;
    end;
A88: now
      LIN c',b',a' by A83,AFF_1:15;
      then c',b' // c',a' by AFF_1:def 1;
      then b',c' // a',c' by AFF_1:13;
      then
A89:  b,c // a,c by ANALMETR:48;
      assume
      a1=b1;
      hence thesis by A15,A87,A89,ANALMETR:82;
    end;
    LIN b',a',c' by A83,AFF_1:15;
    then b',a' // b',c' by AFF_1:def 1;
    then a',b' // b',c' by AFF_1:13;
    then a,b // b,c by ANALMETR:48;
    then b,c // a1,b1 by A13,A84,ANALMETR:82;
    then b1,c1 // a1,b1 by A15,A87,ANALMETR:82;
    then b1',c1' // a1',b1' by ANALMETR:48;
    then b1',a1' // b1',c1' by AFF_1:13;
    then LIN b1',a1',c1' by AFF_1:def 1;
    then LIN a1',b1',c1' by AFF_1:15;
    then LIN a1,b1,c1 by ANALMETR:55;
    then
A90: a1,b1 // a1,c1 by ANALMETR:def 11;
    a',b' // a1',b1' by A13,ANALMETR:48;
    then a1',b1' // a',b' by AFF_1:13;
    then a1,b1 // a,b by ANALMETR:48;
    hence thesis by A90,A85,A88,ANALMETR:82;
  end;
  hence thesis by A28;
end;

theorem
  X is satisfying_OSCH & X is satisfying_TDES implies X is
  satisfying_SCH
proof
  assume that
A1: X is satisfying_OSCH and
A2: X is satisfying_TDES;
  let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
  assume that
A3: M is being_line and
A4: N is being_line and
A5: a1 in M and
A6: a3 in M and
A7: b1 in M and
A8: b3 in M and
A9: a2 in N and
A10: a4 in N and
A11: b2 in N and
A12: b4 in N and
A13: not a4 in M and
A14: not a2 in M and
A15: not b2 in M and
A16: not b4 in M and
A17: not a1 in N and
A18: not a3 in N and
A19: not b1 in N and
A20: not b3 in N and
A21: a3,a2 // b3,b2 and
A22: a2,a1 // b2,b1 and
A23: a1,a4 // b1,b4;
  reconsider a1'=a1,a2'=a2,a3'=a3,a4'=a4,b1'=b1,b2'=b2,b3'=b3,b4'=b4 as
  Element of Af(X) by ANALMETR:47;
  reconsider M'=M,N'=N as Subset of Af(X) by ANALMETR:57;
A24: M' is being_line by A3,ANALMETR:58;
A25: N' is being_line by A4,ANALMETR:58;
A26: a1<>b1 implies a2<>b2 & a3<>b3 & a4<>b4
  proof
    assume
A27: a1<>b1;
A28: now
      assume
      a4=b4;
      then a4,a1 // a4,b1 by A23,ANALMETR:81;
      then LIN a4,a1,b1 by ANALMETR:def 11;
      then LIN a4',a1',b1' by ANALMETR:55;
      then LIN a1',b1',a4' by AFF_1:15;
      hence contradiction by A5,A7,A13,A24,A27,AFF_1:39;
    end;
A29: now
      assume
      a2=b2;
      then LIN a2,a1,b1 by A22,ANALMETR:def 11;
      then LIN a2',a1',b1' by ANALMETR:55;
      then LIN a1',b1',a2' by AFF_1:15;
      hence contradiction by A5,A7,A14,A24,A27,AFF_1:39;
    end;
A30: now
      assume
      a3=b3;
      then LIN a3,a2,b2 by A21,ANALMETR:def 11;
      then LIN a3',a2',b2' by ANALMETR:55;
      then LIN a2',b2',a3' by AFF_1:15;
      hence contradiction by A9,A11,A18,A25,A29,AFF_1:39;
    end;
    assume
    a2=b2 or a3=b3 or a4=b4;
    hence contradiction by A29,A28,A30;
  end;
A31: now
    assume
A32: a1<>b1;
A33: now
A34:  not LIN a2',b1',b2'
      proof
        assume
        LIN a2',b1',b2';
        then LIN a2',b2',b1' by AFF_1:15;
        hence contradiction by A9,A11,A19,A25,A26,A32,AFF_1:39;
      end;
      assume
A35:  a2=a4;
      then a2,a1 // b1,b4 by A23,ANALMETR:81;
      then b2,b1 // b1,b4 by A5,A14,A22,ANALMETR:82;
      then b1,b2 // b1,b4 by ANALMETR:81;
      then
A36:  b1',b2' // b1',b4' by ANALMETR:48;
      a2',b2' // a2',b4' by A9,A11,A12,A25,AFF_1:53,55;
      then LIN a2',b2',b4' by AFF_1:def 1;
      hence thesis by A21,A35,A34,A36,AFF_1:23;
    end;
A37: now
      assume that
A38:  a2<>a4 and
A39:  a1<>a3;
A40:  now
        consider e1 be Element of X such that
A41:    b1,b2 // b1,e1 and
A42:    b1<>e1 by ANALMETR:51;
        consider x be Element of X such that
A43:    LIN a1,a2,x and
A44:    a1<>x and
A45:    a2<>x by A5,A14,Th2;
        reconsider x'=x as Element of Af(X) by ANALMETR:47;
        consider e2 be Element of X such that
A46:    a1,b1 // x,e2 and
A47:    x<>e2 by ANALMETR:51;
        reconsider e1'=e1,e2'=e2 as Element of Af(X) by ANALMETR:47;
        not b1',e1' // x',e2'
        proof
          assume
          b1',e1' // x',e2';
          then b1,e1 // x,e2 by ANALMETR:48;
          then b1,b2 // x,e2 by A41,A42,ANALMETR:82;
          then b1,b2 // a1,b1 by A46,A47,ANALMETR:82;
          then b1,a1 // b1,b2 by ANALMETR:81;
          then LIN b1,a1,b2 by ANALMETR:def 11;
          then LIN b1',a1',b2' by ANALMETR:55;
          hence contradiction by A5,A7,A15,A24,A32,AFF_1:39;
        end;
        then consider y' be Element of Af(X) such that
A48:    LIN b1',e1',y' and
A49:    LIN x',e2',y' by AFF_1:74;
        reconsider y=y' as Element of X by ANALMETR:47;
A50:    a1,a2 // a1,x by A43,ANALMETR:def 11;
A51:    not LIN a2,b2,x
        proof
A52:      a2<>b2
          proof
            assume
            a2=b2;
            then LIN a2,a1,b1 by A22,ANALMETR:def 11;
            then LIN a2',a1',b1' by ANALMETR:55;
            then LIN a1',b1',a2' by AFF_1:15;
            hence contradiction by A5,A7,A14,A24,A32,AFF_1:39;
          end;
          LIN a1',a2',x' by A43,ANALMETR:55;
          then
A53:      LIN a2',x',a1' by AFF_1:15;
          assume
          LIN a2,b2,x;
          then LIN a2',b2',x' by ANALMETR:55;
          then x' in N' by A9,A11,A25,A52,AFF_1:39;
          hence contradiction by A9,A17,A25,A45,A53,AFF_1:39;
        end;
A54:    not LIN a1,b1,a4
        proof
          assume
          LIN a1,b1,a4;
          then LIN a1',b1',a4' by ANALMETR:55;
          hence contradiction by A5,A7,A13,A24,A32,AFF_1:39;
        end;
A55:    not LIN a1,b1,x
        proof
          assume
          LIN a1,b1,x;
          then LIN a1',b1',x' by ANALMETR:55;
          then
A56:      x' in M' by A5,A7,A24,A32,AFF_1:39;
          LIN a1',a2',x' by A43,ANALMETR:55;
          then LIN a1',x',a2' by AFF_1:15;
          hence contradiction by A5,A14,A24,A44,A56,AFF_1:39;
        end;
        assume
A57:    M // N;
        then M' // N' by ANALMETR:64;
        then a3',b3' // a2', b2' by A6,A8,A9,A11,AFF_1:53;
        then a3,b3 // a2,b2 by ANALMETR:48;
        then
A58:    a2,b2 // a3,b3 by ANALMETR:81;
        LIN x,e2,y by A49,ANALMETR:55;
        then x,e2 // x,y by ANALMETR:def 11;
        then
A59:    a1,b1 // x,y by A46,A47,ANALMETR:82;
A60:    b1 <> y
        proof
          assume
          b1=y;
          then b1,a1 // b1,x by A59,ANALMETR:81;
          then LIN b1,a1,x by ANALMETR:def 11;
          then LIN b1',a1',x' by ANALMETR:55;
          then
A61:      x' in M' by A5,A7,A24,A32,AFF_1:39;
          LIN a1',a2',x' by A43,ANALMETR:55;
          then LIN a1',x',a2' by AFF_1:15;
          hence contradiction by A5,A14,A24,A44,A61,AFF_1:39;
        end;
        LIN b1,e1,y by A48,ANALMETR:55;
        then b1,e1 // b1,y by ANALMETR:def 11;
        then
A62:    b1,b2 // b1,y by A41,A42,ANALMETR:82;
        then LIN b1,b2,y by ANALMETR:def 11;
        then LIN b1',b2',y' by ANALMETR:55;
        then LIN y',b1',b2' by AFF_1:15;
        then LIN y,b1,b2 by ANALMETR:55;
        then y,b1 // y,b2 by ANALMETR:def 11;
        then
A63:    b1,y // b2,y by ANALMETR:81;
        a1,a2 // b1, b2 by A22,ANALMETR:81;
        then a1,a2 // b1,y by A7,A15,A62,ANALMETR:82;
        then
A64:    a1,x // b1,y by A5,A14,A50,ANALMETR:82;
A65:    not LIN x,y,a3
        proof
A66:      x<>y
          proof
            assume
            x=y;
            then x,a1 // x,b1 by A64,ANALMETR:81;
            then LIN x,a1,b1 by ANALMETR:def 11;
            then LIN x',a1',b1' by ANALMETR:55;
            then LIN a1',b1',x' by AFF_1:15;
            then
A67:        x' in M' by A5,A7,A24,A32,AFF_1:39;
            LIN a1',a2',x' by A43,ANALMETR:55;
            then LIN x',a1', a2' by AFF_1:15;
            hence contradiction by A5,A14,A24,A44,A67,AFF_1:39;
          end;
          a1',a3' // a1',b1' by A5,A6,A7,A24,AFF_1:53,55;
          then a1,a3 // a1,b1 by ANALMETR:48;
          then
A68:      x,y // a1,a3 by A32,A59,ANALMETR:82;
          assume
          LIN x,y,a3;
          then x,y // x,a3 by ANALMETR:def 11;
          then x,a3 // a1,a3 by A66,A68,ANALMETR:82;
          then a3,a1 // a3,x by ANALMETR:81;
          then LIN a3,a1,x by ANALMETR:def 11;
          then LIN a3',a1',x' by ANALMETR:55;
          then
A69:      x' in M' by A5,A6,A24,A39,AFF_1:39;
          LIN a1',a2',x' by A43,ANALMETR:55;
          then LIN a1',x', a2' by AFF_1:15;
          hence contradiction by A5,A14,A24,A44,A69,AFF_1:39;
        end;
A70:    not LIN x,y,a4
        proof
A71:      x<>y
          proof
            assume
            x=y;
            then x,a1 // x,b1 by A64,ANALMETR:81;
            then LIN x,a1,b1 by ANALMETR:def 11;
            then LIN x',a1',b1' by ANALMETR:55;
            then LIN a1',b1',x' by AFF_1:15;
            then
A72:        x' in M' by A5,A7,A24,A32,AFF_1:39;
            LIN a1',a2',x' by A43,ANALMETR:55;
            then LIN x',a1', a2' by AFF_1:15;
            hence contradiction by A5,A14,A24,A44,A72,AFF_1:39;
          end;
          M' // N' by A57,ANALMETR:64;
          then a1',b1' // a2',a4' by A5,A7,A9,A10,AFF_1:53;
          then a1,b1 // a2,a4 by ANALMETR:48;
          then
A73:      a2,a4 // x,y by A32,A59,ANALMETR:82;
          assume
          LIN x,y,a4;
          then x,y // x,a4 by ANALMETR:def 11;
          then a2,a4 // x,a4 by A71,A73,ANALMETR:82;
          then a4,a2 // a4,x by ANALMETR:81;
          then LIN a4,a2,x by ANALMETR:def 11;
          then LIN a4',a2',x' by ANALMETR:55;
          then
A74:      x' in N' by A9,A10,A25,A38,AFF_1:39;
          LIN a1',a2',x' by A43,ANALMETR:55;
          then LIN a2',x', a1' by AFF_1:15;
          hence contradiction by A9,A17,A25,A45,A74,AFF_1:39;
        end;
A75:    not LIN a2,b2,a3
        proof
A76:      a2<>b2
          proof
            assume
            a2=b2;
            then LIN a2,a1,b1 by A22,ANALMETR:def 11;
            then LIN a2',a1',b1' by ANALMETR:55;
            then LIN a1',b1',a2' by AFF_1:15;
            hence contradiction by A5,A7,A14,A24,A32,AFF_1:39;
          end;
          assume
          LIN a2,b2,a3;
          then LIN a2',b2',a3' by ANALMETR:55;
          hence contradiction by A9,A11,A18,A25,A76,AFF_1:39;
        end;
A77:    X is satisfying_TDES implies X is satisfying_des
        proof
          assume
          X is satisfying_TDES;
          then Af(X) is Moufangian by Th7;
          then Af(X) is translational by AFF_2:28;
          hence thesis by Th8;
        end;
        LIN a1',a2',x' by A43,ANALMETR:55;
        then LIN x',a1', a2' by AFF_1:15;
        then LIN x,a1,a2 by ANALMETR:55;
        then x,a1 // x,a2 by ANALMETR:def 11;
        then a1,x // a2,x by ANALMETR:81;
        then a2,x // b1,y by A44,A64,ANALMETR:82;
        then
A78:    a2,x // b2,y by A60,A63,ANALMETR:82;
        a1',b1' // a3',b3' by A5,A6,A7,A8,A24,AFF_1:53,55;
        then a1,b1 // a3,b3 by ANALMETR:48;
        then
A79:    x,y // a3,b3 by A32,A59,ANALMETR:82;
        M' // N' by A57,ANALMETR:64;
        then a1',b1' // a4',b4' by A5,A7,A10,A12,AFF_1:53;
        then a1,b1 // a4,b4 by ANALMETR:48;
        then
A80:    x,y // a4,b4 by A32,A59,ANALMETR:82;
        M' // N' by A57,ANALMETR:64;
        then a1',b1' // a2',b2' by A5,A7,A9,A11,AFF_1:53;
        then a1,b1 // a2,b2 by ANALMETR:48;
        then
A81:    a2,b2 // x,y by A32,A59,ANALMETR:82;
        a2,a3 // b2,b3 by A21,ANALMETR:81;
        then
A82:    x,a3 // y,b3 by A2,A77,A51,A75,A81,A58,A78,Def8;
        M' // N' by A57,ANALMETR:64;
        then a1',b1' // a4', b4' by A5,A7,A10,A12,AFF_1:53;
        then a1,b1 // a4,b4 by ANALMETR:48;
        then x,a4 // y,b4 by A2,A23,A59,A77,A55,A54,A64,Def8;
        hence thesis by A2,A77,A82,A65,A70,A79,A80,Def8;
      end;
      now
        assume
        not M // N;
        then not M' // N' by ANALMETR:64;
        then consider o' being Element of Af X such that
A83:    o' in M' and
A84:    o' in N' by A24,A25,AFF_1:72;
        reconsider o=o' as Element of X by ANALMETR:47;
        consider K such that
A85:    o in K and
A86:    N _|_ K by A4,Th3;
        reconsider K'=K as Subset of Af(X) by ANALMETR:57;
A87:    K is being_line by A86,ANALMETR:62;
        then
A88:    K' is being_line by ANALMETR:58;
        now
A89:      not a2 in K & not a4 in K & not b2 in K & not b4 in K
          proof
A90:        now
              let x be Element of X;
              assume that
A91:          x in N and
A92:          o<>x;
              assume
              x in K;
              then o,x _|_ o,x by A84,A85,A86,A91,ANALMETR:78;
              hence contradiction by A92,ANALMETR:51;
            end;
            assume
            not thesis;
            hence contradiction by A9,A10,A11,A12,A13,A14,A15,A16,A83,A90;
          end;
A93:      LIN o,a2,b2 by A4,A9,A11,A84,Th4;
A94:      now
            assume
            LIN a1,b1,a4;
            then LIN a1',b1',a4' by ANALMETR:55;
            hence contradiction by A5,A7,A13,A24,A32,AFF_1:39;
          end;
A95:      LIN o,a3,b3 by A3,A6,A8,A83,Th4;
A96:      now
            assume
            LIN a3,b3,a2;
            then LIN a3',b3',a2' by ANALMETR:55;
            hence contradiction by A6,A8,A14,A24,A26,A32,AFF_1:39;
          end;
A97:      LIN o,a1,b1 by A3,A5,A7,A83,Th4;
A98:      now
            let x' be Element of Af(X);
            consider D' being Subset of Af(X) such that
A99:        x' in D' and
A100:       N' // D' by A25,AFF_1:63;
            reconsider D=D' as Subset of the carrier of X by ANALMETR:57;
            N // D by A100,ANALMETR:64;
            then K _|_ D by A86,ANALMETR:73;
            then consider y being Element of X such that
A101:       y in K and
A102:       y in D by ANALMETR:88;
            reconsider y'=y as Element of Af(X) by ANALMETR:47;
            take y';
            thus y' in K' & x',y' // N' by A99,A100,A101,A102,AFF_1:54;
          end;
          then consider d1' be Element of Af(X) such that
A103:     d1' in K' and
A104:     a1',d1' // N';
          consider e1' be Element of Af(X) such that
A105:     e1' in K' and
A106:     b1',e1' // N' by A98;
          consider d3' be Element of Af(X) such that
A107:     d3' in K' and
A108:     a3',d3' // N' by A98;
          consider e3' be Element of Af(X) such that
A109:     e3' in K' and
A110:     b3',e3' // N' by A98;
          reconsider d1=d1',d3=d3',e1=e1',e3=e3' as Element of X by ANALMETR:
          47;
A111:     LIN o,d1,e1 by A85,A87,A103,A105,Th4;
A112:     o<>e1 & o<>e3 & o<>d1 & o<>d3
          proof
A113:       now
              let x,y be Element of X, x',y' be Element of Af(X
              );
              assume that
A114:         x',y' // N' and
A115:         x=x' and
A116:         y=y' and
              x in M and
              y in K and
A117:         not x in N;
              assume
              o=y;
              then o',x' // N' by A114,A116,AFF_1:48;
              hence contradiction by A25,A84,A115,A117,AFF_1:37;
            end;
            assume
            not thesis;
            hence
            contradiction by A5,A6,A7,A8,A17,A18,A19,A20,A103,A104,A107,
            A108,A109,A110,A105,A106,A113;
          end;
          a1,d1 // o,a2 by A9,A84,A104,Th6;
          then
A118:     d1,a1 // o,a2 by ANALMETR:81;
A119:     not e1 in N by A19,A106,AFF_1:49;
A120:     not d3 in N by A18,A108,AFF_1:49;
          a1',d1' // b1',e1' by A25,A104,A106,AFF_1:45;
          then a1,d1 // b1,e1 by ANALMETR:48;
          then
A121:     d1,a1 // e1,b1 by ANALMETR:81;
          assume
A122:     K<>M;
A123:     now
            assume
            LIN a3,b3,d3;
            then LIN a3',b3',d3' by ANALMETR:55;
            then d3 in M by A6,A8,A24,A26,A32,AFF_1:39;
            hence contradiction by A3,A83,A85,A87,A122,A107,A112,Th5;
          end;
A124:     now
            assume
            LIN a1,b1,d1;
            then LIN a1',b1',d1' by ANALMETR:55;
            then d1 in M by A5,A7,A24,A32,AFF_1:39;
            hence contradiction by A3,A83,A85,A87,A122,A103,A112,Th5;
          end;
          a3',d3' // b3',e3' by A25,A108,A110,AFF_1:45;
          then
A125:     a3,d3 // b3,e3 by ANALMETR:48;
          then
A126:     d3,a3 // e3,b3 by ANALMETR:81;
A127:     not LIN d3,e3,a3 & not LIN d3,e3,a4
          proof
A128:       d3<>e3
            proof
              assume
              not thesis;
              then LIN e3,a3,b3 by A126,ANALMETR:def 11;
              then LIN e3',a3',b3' by ANALMETR:55;
              then LIN a3',b3',e3' by AFF_1:15;
              then e3' in M' by A6,A8,A24,A26,A32,AFF_1:39;
              hence contradiction by A24,A83,A85,A88,A122,A109,A112,
              AFF_1:30;
            end;
            assume
            not thesis;
            then LIN d3',e3',a3' or LIN d3',e3',a4' by ANALMETR:55;
            then a3' in K' or a4' in K' by A88,A107,A109,A128,AFF_1:39;
            hence
            contradiction by A6,A10,A13,A18,A24,A25,A83,A84,A85,A86,
            A88,A122,AFF_1:30;
          end;
A129:     now
            assume
            LIN a1,b1,a2;
            then LIN a1',b1',a2' by ANALMETR:55;
            hence contradiction by A5,A7,A14,A24,A32,AFF_1:39;
          end;
A130:     LIN o,a4,b4 by A4,A10,A12,A84,Th4;
          a1,a2 // b1,b2 by A22,ANALMETR:81;
          then d1,a2 // e1,b2 by A2,A14,A15,A17,A19,A83,A84,A112,A93,A97
          ,A111,A124,A129,A121,A118,Def5;
          then
A131:     a2,d1 // b2,e1 by ANALMETR:81;
A132:     not e3 in N by A20,A110,AFF_1:49;
A133:     LIN o,d3,e3 by A85,A87,A107,A109,Th4;
          a1,d1 // o,a4 by A10,A84,A104,Th6;
          then d1,a1 // o,a4 by ANALMETR:81;
          then
A134:     d1,a4 // e1,b4 by A2,A13,A16,A17,A19,A23,A83,A84,A112,A97,
          A130,A111,A124,A94,A121,Def5;
A135:     a3,d3 // o,a4 by A10,A84,A108,Th6;
A136:     not d1 in N by A17,A104,AFF_1:49;
          a3,d3 // o,a2 by A9,A84,A108,Th6;
          then d3,a3 // o,a2 by ANALMETR:81;
          then d3,a2 // e3,b2 by A2,A14,A15,A18,A20,A21,A83,A84,A112,A93,
          A95,A133,A123,A96,A126,Def5;
          then d3,a4 // e3,b4 by A1,A9,A10,A11,A12,A86,A103,A107,A109,A105,
          A131,A134,A136,A120,A119,A132,A89,Def7;
          hence
          thesis by A2,A13,A16,A18,A20,A83,A84,A112,A130,A95,A133,A125
          ,A127,A135,Def5;
        end;
        hence
        thesis by A1,A5,A6,A7,A8,A9,A10,A11,A12,A13,A14,A15,A16,A17,A18,
        A19,A20,A21,A22,A23,A86,Def7;
      end;
      hence thesis by A40;
    end;
    now
A137: not LIN a1',b2',b1'
      proof
        assume
        LIN a1',b2',b1';
        then LIN a1',b1',b2' by AFF_1:15;
        hence contradiction by A5,A7,A15,A24,A32,AFF_1:39;
      end;
      a1',b1' // a1',b3' by A5,A7,A8,A24,AFF_1:53,55;
      then
A138: LIN a1',b1',b3' by AFF_1:def 1;
      assume
A139: a1=a3;
      then a2,a1 // b2,b3 by A21,ANALMETR:81;
      then b2,b1 // b2,b3 by A5,A14,A22,ANALMETR:82;
      then b2',b1' // b2',b3' by ANALMETR:48;
      hence thesis by A23,A139,A137,A138,AFF_1:23;
    end;
    hence thesis by A33,A37;
  end;
A140: a1=b1 implies a2=b2 & a3=b3 & a4=b4
  proof
    assume
A141: a1=b1;
A142: now
      LIN a1,a4,b4 by A23,A141,ANALMETR:def 11;
      then LIN a1',a4',b4' by ANALMETR:55;
      then
A143: LIN a4',b4',a1' by AFF_1:15;
      assume
      a4<>b4;
      hence contradiction by A10,A12,A17,A25,A143,AFF_1:39;
    end;
A144: now
      a1,a2 // a1,b2 by A22,A141,ANALMETR:81;
      then LIN a1,a2,b2 by ANALMETR:def 11;
      then LIN a1',a2',b2' by ANALMETR:55;
      then
A145: LIN a2',b2',a1' by AFF_1:15;
      assume
      a2<>b2;
      hence contradiction by A9,A11,A17,A25,A145,AFF_1:39;
    end;
A146: now
      a2,a3 // a2,b3 by A21,A144,ANALMETR:81;
      then LIN a2,a3,b3 by ANALMETR:def 11;
      then LIN a2',a3',b3' by ANALMETR:55;
      then
A147: LIN a3',b3',a2' by AFF_1:15;
      assume
      a3<>b3;
      hence contradiction by A6,A8,A14,A24,A147,AFF_1:39;
    end;
    assume
    a2<>b2 or a3<>b3 or a4<>b4;
    hence contradiction by A144,A142,A146;
  end;
  now
    assume
    a1=b1;
    then a3',a4' // b3',b4' by A140,AFF_1:11;
    hence thesis by ANALMETR:48;
  end;
  hence thesis by A31;
end;

theorem
  X is satisfying_OPAP & X is satisfying_DES implies X is satisfying_PAP
proof
  assume that
A1: X is satisfying_OPAP and
A2: X is satisfying_DES;
  let o,a1,a2,a3,b1,b2,b3,M,N;
  assume that
A3: M is being_line and
A4: N is being_line and
A5: o in M and
A6: a1 in M and
A7: a2 in M and
A8: a3 in M and
A9: o in N and
A10: b1 in N and
A11: b2 in N and
A12: b3 in N and
A13: not b2 in M and
A14: not a3 in N and
A15: o<>a1 and
A16: o<>a2 and
  o<>a3 and
A17: o<>b1 and
  o<>b2 and
A18: o<>b3 and
A19: a3,b2 // a2,b1 and
A20: a3,b3 // a1,b1;
  reconsider o'=o,a1'=a1,a2'=a2,a3'=a3,b1'=b1,b2'=b2,b3'=b3 as Element of Af(X
  ) by ANALMETR:47;
  reconsider M'=M,N'=N as Subset of Af(X) by ANALMETR:57;
A21: M' is being_line by A3,ANALMETR:58;
A22: N' is being_line by A4,ANALMETR:58;
  now
    assume
A23: not M _|_ N;
A24: now
      assume that
A25:  a1<>a2 and
A26:  a2<>a3 and
A27:  a1<>a3;
A28:  b1<>b2 & b2<>b3 & b1<>b3
      proof
A29:    now
A30:      not LIN o',b1',a1'
          proof
            o',a1' // o',a3' by A5,A6,A8,A21,AFF_1:53,55;
            then
A31:        LIN o',a1',a3' by AFF_1:def 1;
            assume
            LIN o',b1',a1';
            then a1' in N' by A9,A10,A17,A22,AFF_1:39;
            hence contradiction by A9,A14,A15,A22,A31,AFF_1:39;
          end;
          assume
          b1=b3;
          then b1,a1 // b1,a3 by A20,ANALMETR:81;
          then
A32:      b1',a1' // b1',a3' by ANALMETR:48;
          o',a1' // o',a3' by A5,A6,A8,A21,AFF_1:53,55;
          then LIN o',a1',a3' by AFF_1:def 1;
          hence contradiction by A27,A30,A32,AFF_1:23;
        end;
A33:    now
A34:      not LIN o',b1',a1'
          proof
            o',a1' // o',a3' by A5,A6,A8,A21,AFF_1:53,55;
            then
A35:        LIN o',a1',a3' by AFF_1:def 1;
            assume
            LIN o',b1',a1';
            then a1' in N' by A9,A10,A17,A22,AFF_1:39;
            hence contradiction by A9,A14,A15,A22,A35,AFF_1:39;
          end;
          assume
          b2=b3;
          then a1,b1 // a2,b1 by A8,A13,A19,A20,ANALMETR:82;
          then b1,a1 // b1,a2 by ANALMETR:81;
          then
A36:      b1',a1' // b1',a2' by ANALMETR:48;
          o',a1' // o',a2' by A5,A6,A7,A21,AFF_1:53,55;
          then LIN o',a1',a2' by AFF_1:def 1;
          hence contradiction by A25,A34,A36,AFF_1:23;
        end;
A37:    now
A38:      not LIN o',b2',a2'
          proof
            assume
            LIN o',b2',a2';
            then LIN o',a2',b2' by AFF_1:15;
            hence contradiction by A5,A7,A13,A16,A21,AFF_1:39;
          end;
          assume
          b1=b2;
          then b2,a2 // b2,a3 by A19,ANALMETR:81;
          then
A39:      b2',a2' // b2',a3' by ANALMETR:48;
          o',a2' // o',a3' by A5,A7,A8,A21,AFF_1:53,55;
          then LIN o',a2',a3' by AFF_1:def 1;
          hence contradiction by A26,A38,A39,AFF_1:23;
        end;
        assume
        b1=b2 or b2=b3 or b1=b3;
        hence contradiction by A37,A33,A29;
      end;
A40:  now
A41:    not LIN b3,b1,a3
        proof
          assume
          LIN b3,b1,a3;
          then LIN b3',b1',a3' by ANALMETR:55;
          hence contradiction by A10,A12,A14,A22,A28,AFF_1:39;
        end;
        o',b2' // o',b3' by A9,A11,A12,A22,AFF_1:53,55;
        then LIN o',b2',b3' by AFF_1:def 1;
        then
A42:    LIN o,b2,b3 by ANALMETR:55;
A43:    not LIN b2,b1,a3
        proof
          assume
          LIN b2,b1,a3;
          then LIN b2',b1',a3' by ANALMETR:55;
          hence contradiction by A10,A11,A14,A22,A28,AFF_1:39;
        end;
        o',a3' // o',a1' by A5,A6,A8,A21,AFF_1:53,55;
        then LIN o',a3',a1' by AFF_1:def 1;
        then
A44:    LIN o,a3,a1 by ANALMETR:55;
        consider x be Element of X such that
A45:    o,b2 _|_ o,x and
A46:    o<>x by ANALMETR:51;
A47:    o,x _|_ N by A4,A5,A9,A11,A13,A45,ANALMETR:77;
        consider e1' be Element of Af(X) such that
A48:    a3',b3' // b2',e1' and
A49:    b2'<> e1' by DIRAF:47;
        reconsider x'=x as Element of Af(X) by ANALMETR:47;
        LIN o',x',x' by AFF_1:16;
        then consider A' be Subset of Af(X) such that
A50:    A' is being_line and
A51:    o' in A' and
A52:    x' in A' and
        x' in A' by AFF_1:33;
        reconsider A=A' as Subset of X by ANALMETR:57;
A53:    for e1 holds e1 in A implies LIN o,x,e1
        proof
          let e1 such that
A54:      e1 in A;
          reconsider e1'=e1 as Element of Af(X) by ANALMETR:47;
          o',x' // o',e1' by A50,A51,A52,A54,AFF_1:53,55;
          then LIN o',x',e1' by AFF_1:def 1;
          hence thesis by ANALMETR:55;
        end;
        for e1 holds LIN o,x,e1 implies e1 in A
        proof
          let e1 such that
A55:      LIN o,x,e1;
          reconsider e1'=e1 as Element of Af(X) by ANALMETR:47;
          LIN o',x',e1' by A55,ANALMETR:55;
          hence thesis by A46,A50,A51,A52,AFF_1:39;
        end;
        then A=Line(o,x) by A53,ANALMETR:def 12;
        then
A56:    A _|_ N by A46,A47,ANALMETR:def 15;
A57:    not b2 in A
        proof
A58:      o',b2' // o',b2' by AFF_1:11;
          assume
          b2 in A;
          then A' // N' by A5,A9,A11,A13,A22,A50,A51,A58,AFF_1:52;
          then A // N by ANALMETR:64;
          hence contradiction by A56,ANALMETR:73;
        end;
        assume
A59:    not a3,b3 _|_ o,b2;
        not b2',e1' // A'
        proof
          assume
A60:      b2',e1' // A';
          consider d1',d2' be Element of Af(X) such that
A61:      d1'<>d2' and
A62:      A'=Line(d1',d2') by A50,AFF_1:def 3;
A63:      d2' in A' by A62,AFF_1:26;
          reconsider d1=d1',d2=d2' as Element of X by ANALMETR:47;
          d1',d2' // d1',d2' by AFF_1:11;
          then d1',d2' // A' by A61,A62,AFF_1:def 4;
          then b2',e1' // d1',d2' by A50,A60,AFF_1:45;
          then a3',b3' // d1',d2' by A48,A49,AFF_1:14;
          then
A64:      a3,b3 // d1,d2 by ANALMETR:48;
          d1' in A' by A62,AFF_1:26;
          then d1,d2 _|_ o,b2 by A9,A11,A56,A63,ANALMETR:78;
          hence contradiction by A59,A61,A64,ANALMETR:84;
        end;
        then consider c3' be Element of Af(X) such that
A65:    c3' in A' and
A66:    LIN b2',e1',c3' by A50,AFF_1:73;
        b2',e1' // b2',c3' by A66,AFF_1:def 1;
        then
A67:    a3',b3' // b2',c3' by A48,A49,AFF_1:14;
        consider e1' be Element of Af(X) such that
A68:    c3',a3' // a1',e1' and
A69:    a1'<> e1' by DIRAF:47;
        not a1',e1' // A'
        proof
A70:      c3'<>o'
          proof
            assume
            c3'=o';
            then
A71:        a3,b3 // b2,o by A67,ANALMETR:48;
            b2',o' // b2',b3' by A9,A11,A12,A22,AFF_1:53,55;
            then b2,o // b2,b3 by ANALMETR:48;
            then a3,b3 // b2,b3 by A5,A13,A71,ANALMETR:82;
            then b3,b2 // b3,a3 by ANALMETR:81;
            then LIN b3,b2,a3 by ANALMETR:def 11;
            then LIN b3',b2',a3' by ANALMETR:55;
            hence contradiction by A11,A12,A14,A22,A28,AFF_1:39;
          end;
          assume
A72:      a1',e1' // A';
A73:      o',a3' // o',a3' by AFF_1:11;
          o',c3' // A' by A50,A51,A65,AFF_1:54,55;
          then a1',e1' // o',c3' by A50,A72,AFF_1:45;
          then c3',a3' // o',c3' by A68,A69,AFF_1:14;
          then c3',o' // c3',a3' by AFF_1:13;
          then LIN c3',o',a3' by AFF_1:def 1;
          then a3' in A' by A50,A51,A65,A70,AFF_1:39;
          then A' // M' by A5,A8,A9,A14,A21,A50,A51,A73,AFF_1:52;
          then A // M by ANALMETR:64;
          hence contradiction by A23,A56,ANALMETR:73;
        end;
        then consider c1' be Element of Af(X) such that
A74:    c1' in A' and
A75:    LIN a1',e1',c1' by A50,AFF_1:73;
        reconsider c1=c1' as Element of X by ANALMETR:47;
        reconsider c3=c3' as Element of X by ANALMETR:47;
        a1',e1' // a1',c1' by A75,AFF_1:def 1;
        then
A76:    c3',a3' // a1',c1' by A68,A69,AFF_1:14;
        then
A77:    c3,a3 // a1,c1 by ANALMETR:48;
        consider e1' be Element of Af(X) such that
A78:    c3',a3' // a2',e1' and
A79:    a2'<> e1' by DIRAF:47;
        not a2',e1' // A'
        proof
A80:      c3'<>o'
          proof
            assume
            c3'=o';
            then
A81:        a3,b3 // b2,o by A67,ANALMETR:48;
            b2',o' // b2',b3' by A9,A11,A12,A22,AFF_1:53,55;
            then b2,o // b2,b3 by ANALMETR:48;
            then a3,b3 // b2,b3 by A5,A13,A81,ANALMETR:82;
            then b3,b2 // b3,a3 by ANALMETR:81;
            then LIN b3,b2,a3 by ANALMETR:def 11;
            then LIN b3',b2',a3' by ANALMETR:55;
            hence contradiction by A11,A12,A14,A22,A28,AFF_1:39;
          end;
          assume
A82:      a2',e1' // A';
A83:      o',a3' // o',a3' by AFF_1:11;
          o',c3' // A' by A50,A51,A65,AFF_1:54,55;
          then a2',e1' // o',c3' by A50,A82,AFF_1:45;
          then c3',a3' // o',c3' by A78,A79,AFF_1:14;
          then c3',o' // c3',a3' by AFF_1:13;
          then LIN c3',o',a3' by AFF_1:def 1;
          then a3' in A' by A50,A51,A65,A80,AFF_1:39;
          then A' // M' by A5,A8,A9,A14,A21,A50,A51,A83,AFF_1:52;
          then A // M by ANALMETR:64;
          hence contradiction by A23,A56,ANALMETR:73;
        end;
        then consider c2' be Element of Af(X) such that
A84:    c2' in A' and
A85:    LIN a2',e1',c2' by A50,AFF_1:73;
        reconsider c2=c2' as Element of X by ANALMETR:47;
        a2',e1' // a2',c2' by A85,AFF_1:def 1;
        then
A86:    c3',a3' // a2',c2' by A78,A79,AFF_1:14;
        then
A87:    c3,a3 // a2,c2 by ANALMETR:48;
A88:    o<>c3 & o<>c2 & o<>c1
        proof
A89:      now
            assume
A90:        o=c3;
            b2',o' // b3',b2' by A9,A11,A12,A22,AFF_1:53,55;
            then a3',b3' // b3',b2' by A5,A13,A67,A90,AFF_1:14;
            then b3',b2' // b3',a3' by AFF_1:13;
            then LIN b3',b2',a3' by AFF_1:def 1;
            hence contradiction by A11,A12,A14,A22,A28,AFF_1:39;
          end;
A91:      now
            a1',o' // a1',a3' by A5,A6,A8,A21,AFF_1:53,55;
            then
A92:        a1,o // a1,a3 by ANALMETR:48;
            assume
            o=c2 or o=c1;
            then
A93:        c3,a3 // a1,o or c3,a3 // a2,o by A76,A86, ANALMETR:
            48;
            a2',o' // a1',a3' by A5,A6,A7,A8,A21,AFF_1:53,55;
            then a2,o // a1,a3 by ANALMETR:48;
            then c3,a3 // a1,a3 by A15,A16,A93,A92,ANALMETR:82;
            then a3,a1 // a3,c3 by ANALMETR:81;
            then LIN a3,a1,c3 by ANALMETR:def 11;
            then LIN a3',a1',c3' by ANALMETR:55;
            then
A94:        c3' in M' by A6,A8,A21,A27,AFF_1:39;
            o',c3' // o',c3' by AFF_1:11;
            then A' // M' by A5,A21,A50,A51,A65,A89,A94,AFF_1:52;
            then A // M by ANALMETR:64;
            hence contradiction by A23,A56,ANALMETR:73;
          end;
          assume
          o=c3 or o=c2 or o=c1;
          hence contradiction by A89,A91;
        end;
A95:    not c3 in N
        proof
A96:      o',c3' // o',c3' by AFF_1:11;
          assume
          c3 in N;
          then A' // N' by A9,A22,A50,A51,A65,A88,A96,AFF_1:52;
          then A // N by ANALMETR:64;
          hence contradiction by A56,ANALMETR:73;
        end;
A97:    not LIN a3,a2,c3
        proof
          assume
          LIN a3,a2,c3;
          then LIN a3',a2',c3' by ANALMETR:55;
          then
A98:      c3' in M' by A7,A8,A21,A26,AFF_1:39;
          o',c3' // o',c3' by AFF_1:11;
          then A' // M' by A5,A21,A50,A51,A65,A88,A98,AFF_1:52;
          then A // M by ANALMETR:64;
          hence contradiction by A23,A56,ANALMETR:73;
        end;
A99:    not LIN a3,a1,c3
        proof
