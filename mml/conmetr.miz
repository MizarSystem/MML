        assume o9,e19 // d49,e29;
        then o,e1 // d4,e2 by ANALMETR:36;
        then
A37:    o,e1 _|_ a1,a4 by A32,A33,ANALMETR:62;
        o9,e19 // a19,a39 by A3,A4,A22,A23,A29,AFF_1:39,41;
