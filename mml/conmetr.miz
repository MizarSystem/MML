then LIN o',b1',b2' by AFF_1:def 1;
hence contradiction by A2,A3,A76,AFF_1:39;end;
      then a3,b2 _|_ c2,a1 by A2,A48,ANALMETR:84; then c2,a1 // c3,a1 by A2,A63
