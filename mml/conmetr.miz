            then
A35:        LIN o9,a19,a39 by AFF_1:def 1;
            assume LIN o9,b19,a19;
            then a19 in N9 by A9,A10,A17,A22,AFF_1:25;
            hence contradiction by A9,A14,A15,A22,A35,AFF_1:25;
          end;
          assume b2=b3;
          then a1,b1 // a2,b1 by A8,A13,A19,A20,ANALMETR:60;
