    then o,a2 // o,a3 by A3,A66,ANALMETR:85;
    then LIN o,a2,a3 by ANALMETR:def 11;
    then
A80: LIN o9,a29,a39 by ANALMETR:55;
    LIN c2,e2,a3 by A65,ANALMETR:55;
    then c2,e2 // c2,a3 by ANALMETR:def 11;
    then
A81: a1,c1 _|_ c2,a3 by A60,A61,ANALMETR:84;
    then c1,a1 _|_ c2,a3 by ANALMETR:83;
    then b1,a1 _|_ b2,a3 by A1,A66,A78,A33,A67,A77,A68,A72,
    CONAFFM:def 2;
    then b2,a3 // b2,a2 by A22,A79,ANALMETR:85;
    then b29,a39 // b29,a29 by ANALMETR:48;
    then b29,a29 // b29,a39 by AFF_1:13;
    then a29=a39 by A51,A80,AFF_1:23;
    then c,a // a1,c1 by A50,A81,A49,ANALMETR:85;
    then c9,a9 // a19,c19 by ANALMETR:48;
    then a9,c9 // a19,c19 by AFF_1:13;
    hence thesis by ANALMETR:48;
  end;
  now
    assume
A82: LIN a,b,c;
    then
A83: LIN a9,b9,c9 by ANALMETR:55;
A84: a<>b
    proof
      assume a=b;
      then LIN b9,b19,a9 by AFF_1:16;
      hence contradiction by A8,ANALMETR:55;
    end;
A85: now
      assume a1,c1 // a,b;
      then a19,c19 // a9,b9 by ANALMETR:48;
      then a9,b9 // a19,c19 by AFF_1:13;
      then
A86:  a,b // a1,c1 by ANALMETR:48;
      a,b // a,c by A82,ANALMETR:def 11;
      hence thesis by A84,A86,ANALMETR:82;
    end;
A87: b<>c
    proof
      assume b=c;
      then LIN b9,b19,c9 by AFF_1:16;
      hence contradiction by A9,ANALMETR:55;
    end;
A88: now
      LIN c9,b9,a9 by A83,AFF_1:15;
      then c9,b9 // c9,a9 by AFF_1:def 1;
      then b9,c9 // a9,c9 by AFF_1:13;
      then
A89:  b,c // a,c by ANALMETR:48;
      assume a1=b1;
      hence thesis by A15,A87,A89,ANALMETR:82;
    end;
    LIN b9,a9,c9 by A83,AFF_1:15;
    then b9,a9 // b9,c9 by AFF_1:def 1;
    then a9,b9 // b9,c9 by AFF_1:13;
    then a,b // b,c by ANALMETR:48;
    then b,c // a1,b1 by A13,A84,ANALMETR:82;
    then b1,c1 // a1,b1 by A15,A87,ANALMETR:82;
    then b19,c19 // a19,b19 by ANALMETR:48;
    then b19,a19 // b19,c19 by AFF_1:13;
    then LIN b19,a19,c19 by AFF_1:def 1;
    then LIN a19,b19,c19 by AFF_1:15;
    then LIN a1,b1,c1 by ANALMETR:55;
    then
A90: a1,b1 // a1,c1 by ANALMETR:def 11;
    a9,b9 // a19,b19 by A13,ANALMETR:48;
    then a19,b19 // a9,b9 by AFF_1:13;
    then a1,b1 // a,b by ANALMETR:48;
    hence thesis by A90,A85,A88,ANALMETR:82;
  end;
  hence thesis by A28;
end;

theorem
  X is satisfying_OSCH & X is satisfying_TDES implies X is satisfying_SCH
proof
  assume that
A1: X is satisfying_OSCH and
A2: X is satisfying_TDES;
  let a1,a2,a3,a4,b1,b2,b3,b4,M,N;
  assume that
A3: M is being_line and
A4: N is being_line and
A5: a1 in M and
A6: a3 in M and
A7: b1 in M and
A8: b3 in M and
A9: a2 in N and
A10: a4 in N and
A11: b2 in N and
A12: b4 in N and
A13: not a4 in M and
A14: not a2 in M and
A15: not b2 in M and
A16: not b4 in M and
A17: not a1 in N and
A18: not a3 in N and
A19: not b1 in N and
A20: not b3 in N and
A21: a3,a2 // b3,b2 and
A22: a2,a1 // b2,b1 and
A23: a1,a4 // b1,b4;
  reconsider a19=a1,a29=a2,a39=a3,a49=a4,b19=b1,b29=b2,b39=b3,b49=b4 as
  Element of Af(X) by ANALMETR:47;
  reconsider M9=M,N9=N as Subset of Af(X) by ANALMETR:57;
A24: M9 is being_line by A3,ANALMETR:58;
A25: N9 is being_line by A4,ANALMETR:58;
A26: a1<>b1 implies a2<>b2 & a3<>b3 & a4<>b4
  proof
    assume
A27: a1<>b1;
A28: now
      assume a4=b4;
      then a4,a1 // a4,b1 by A23,ANALMETR:81;
      then LIN a4,a1,b1 by ANALMETR:def 11;
      then LIN a49,a19,b19 by ANALMETR:55;
      then LIN a19,b19,a49 by AFF_1:15;
      hence contradiction by A5,A7,A13,A24,A27,AFF_1:39;
    end;
A29: now
      assume a2=b2;
      then LIN a2,a1,b1 by A22,ANALMETR:def 11;
      then LIN a29,a19,b19 by ANALMETR:55;
      then LIN a19,b19,a29 by AFF_1:15;
      hence contradiction by A5,A7,A14,A24,A27,AFF_1:39;
    end;
A30: now
      assume a3=b3;
      then LIN a3,a2,b2 by A21,ANALMETR:def 11;
      then LIN a39,a29,b29 by ANALMETR:55;
      then LIN a29,b29,a39 by AFF_1:15;
      hence contradiction by A9,A11,A18,A25,A29,AFF_1:39;
    end;
    assume a2=b2 or a3=b3 or a4=b4;
    hence contradiction by A29,A28,A30;
  end;
A31: now
    assume
A32: a1<>b1;
A33: now
A34:  not LIN a29,b19,b29
      proof
        assume LIN a29,b19,b29;
        then LIN a29,b29,b19 by AFF_1:15;
        hence contradiction by A9,A11,A19,A25,A26,A32,AFF_1:39;
      end;
      assume
A35:  a2=a4;
      then a2,a1 // b1,b4 by A23,ANALMETR:81;
      then b2,b1 // b1,b4 by A5,A14,A22,ANALMETR:82;
      then b1,b2 // b1,b4 by ANALMETR:81;
      then
A36:  b19,b29 // b19,b49 by ANALMETR:48;
      a29,b29 // a29,b49 by A9,A11,A12,A25,AFF_1:53,55;
      then LIN a29,b29,b49 by AFF_1:def 1;
      hence thesis by A21,A35,A34,A36,AFF_1:23;
    end;
A37: now
      assume that
A38:  a2<>a4 and
A39:  a1<>a3;
A40:  now
        consider e1 be Element of X such that
A41:    b1,b2 // b1,e1 and
A42:    b1<>e1 by ANALMETR:51;
        consider x be Element of X such that
A43:    LIN a1,a2,x and
A44:    a1<>x and
A45:    a2<>x by Th2;
        reconsider x9=x as Element of Af(X) by ANALMETR:47;
        consider e2 be Element of X such that
A46:    a1,b1 // x,e2 and
A47:    x<>e2 by ANALMETR:51;
        reconsider e19=e1,e29=e2 as Element of Af(X) by ANALMETR:47;
        not b19,e19 // x9,e29
        proof
          assume b19,e19 // x9,e29;
          then b1,e1 // x,e2 by ANALMETR:48;
          then b1,b2 // x,e2 by A41,A42,ANALMETR:82;
          then b1,b2 // a1,b1 by A46,A47,ANALMETR:82;
          then b1,a1 // b1,b2 by ANALMETR:81;
          then LIN b1,a1,b2 by ANALMETR:def 11;
          then LIN b19,a19,b29 by ANALMETR:55;
          hence contradiction by A5,A7,A15,A24,A32,AFF_1:39;
        end;
        then consider y9 be Element of Af(X) such that
A48:    LIN b19,e19,y9 and
A49:    LIN x9,e29,y9 by AFF_1:74;
        reconsider y=y9 as Element of X by ANALMETR:47;
A50:    a1,a2 // a1,x by A43,ANALMETR:def 11;
A51:    not LIN a2,b2,x
        proof
A52:      a2<>b2
          proof
            assume a2=b2;
            then LIN a2,a1,b1 by A22,ANALMETR:def 11;
            then LIN a29,a19,b19 by ANALMETR:55;
            then LIN a19,b19,a29 by AFF_1:15;
            hence contradiction by A5,A7,A14,A24,A32,AFF_1:39;
          end;
          LIN a19,a29,x9 by A43,ANALMETR:55;
          then
A53:      LIN a29,x9,a19 by AFF_1:15;
          assume LIN a2,b2,x;
          then LIN a29,b29,x9 by ANALMETR:55;
          then x9 in N9 by A9,A11,A25,A52,AFF_1:39;
          hence contradiction by A9,A17,A25,A45,A53,AFF_1:39;
        end;
A54:    not LIN a1,b1,a4
        proof
          assume LIN a1,b1,a4;
          then LIN a19,b19,a49 by ANALMETR:55;
          hence contradiction by A5,A7,A13,A24,A32,AFF_1:39;
        end;
A55:    not LIN a1,b1,x
        proof
          assume LIN a1,b1,x;
          then LIN a19,b19,x9 by ANALMETR:55;
          then
A56:      x9 in M9 by A5,A7,A24,A32,AFF_1:39;
          LIN a19,a29,x9 by A43,ANALMETR:55;
          then LIN a19,x9,a29 by AFF_1:15;
          hence contradiction by A5,A14,A24,A44,A56,AFF_1:39;
        end;
        assume
A57:    M // N;
        then M9 // N9 by ANALMETR:64;
        then a39,b39 // a29, b29 by A6,A8,A9,A11,AFF_1:53;
        then a3,b3 // a2,b2 by ANALMETR:48;
        then
A58:    a2,b2 // a3,b3 by ANALMETR:81;
        LIN x,e2,y by A49,ANALMETR:55;
        then x,e2 // x,y by ANALMETR:def 11;
        then
A59:    a1,b1 // x,y by A46,A47,ANALMETR:82;
A60:    b1 <> y
        proof
          assume b1=y;
          then b1,a1 // b1,x by A59,ANALMETR:81;
          then LIN b1,a1,x by ANALMETR:def 11;
          then LIN b19,a19,x9 by ANALMETR:55;
          then
A61:      x9 in M9 by A5,A7,A24,A32,AFF_1:39;
          LIN a19,a29,x9 by A43,ANALMETR:55;
          then LIN a19,x9,a29 by AFF_1:15;
          hence contradiction by A5,A14,A24,A44,A61,AFF_1:39;
        end;
        LIN b1,e1,y by A48,ANALMETR:55;
        then b1,e1 // b1,y by ANALMETR:def 11;
        then
A62:    b1,b2 // b1,y by A41,A42,ANALMETR:82;
        then LIN b1,b2,y by ANALMETR:def 11;
        then LIN b19,b29,y9 by ANALMETR:55;
        then LIN y9,b19,b29 by AFF_1:15;
        then LIN y,b1,b2 by ANALMETR:55;
        then y,b1 // y,b2 by ANALMETR:def 11;
        then
A63:    b1,y // b2,y by ANALMETR:81;
        a1,a2 // b1, b2 by A22,ANALMETR:81;
        then a1,a2 // b1,y by A7,A15,A62,ANALMETR:82;
        then
A64:    a1,x // b1,y by A5,A14,A50,ANALMETR:82;
A65:    not LIN x,y,a3
        proof
A66:      x<>y
          proof
            assume x=y;
            then x,a1 // x,b1 by A64,ANALMETR:81;
            then LIN x,a1,b1 by ANALMETR:def 11;
            then LIN x9,a19,b19 by ANALMETR:55;
            then LIN a19,b19,x9 by AFF_1:15;
            then
A67:        x9 in M9 by A5,A7,A24,A32,AFF_1:39;
            LIN a19,a29,x9 by A43,ANALMETR:55;
            then LIN x9,a19, a29 by AFF_1:15;
            hence contradiction by A5,A14,A24,A44,A67,AFF_1:39;
          end;
          a19,a39 // a19,b19 by A5,A6,A7,A24,AFF_1:53,55;
          then a1,a3 // a1,b1 by ANALMETR:48;
          then
A68:      x,y // a1,a3 by A32,A59,ANALMETR:82;
          assume LIN x,y,a3;
          then x,y // x,a3 by ANALMETR:def 11;
          then x,a3 // a1,a3 by A66,A68,ANALMETR:82;
          then a3,a1 // a3,x by ANALMETR:81;
          then LIN a3,a1,x by ANALMETR:def 11;
          then LIN a39,a19,x9 by ANALMETR:55;
          then
A69:      x9 in M9 by A5,A6,A24,A39,AFF_1:39;
          LIN a19,a29,x9 by A43,ANALMETR:55;
          then LIN a19,x9, a29 by AFF_1:15;
          hence contradiction by A5,A14,A24,A44,A69,AFF_1:39;
        end;
A70:    not LIN x,y,a4
        proof
A71:      x<>y
          proof
            assume x=y;
            then x,a1 // x,b1 by A64,ANALMETR:81;
            then LIN x,a1,b1 by ANALMETR:def 11;
            then LIN x9,a19,b19 by ANALMETR:55;
            then LIN a19,b19,x9 by AFF_1:15;
            then
A72:        x9 in M9 by A5,A7,A24,A32,AFF_1:39;
            LIN a19,a29,x9 by A43,ANALMETR:55;
            then LIN x9,a19, a29 by AFF_1:15;
            hence contradiction by A5,A14,A24,A44,A72,AFF_1:39;
          end;
          M9 // N9 by A57,ANALMETR:64;
          then a19,b19 // a29,a49 by A5,A7,A9,A10,AFF_1:53;
          then a1,b1 // a2,a4 by ANALMETR:48;
          then
A73:      a2,a4 // x,y by A32,A59,ANALMETR:82;
          assume LIN x,y,a4;
          then x,y // x,a4 by ANALMETR:def 11;
          then a2,a4 // x,a4 by A71,A73,ANALMETR:82;
          then a4,a2 // a4,x by ANALMETR:81;
          then LIN a4,a2,x by ANALMETR:def 11;
          then LIN a49,a29,x9 by ANALMETR:55;
          then
A74:      x9 in N9 by A9,A10,A25,A38,AFF_1:39;
          LIN a19,a29,x9 by A43,ANALMETR:55;
          then LIN a29,x9, a19 by AFF_1:15;
          hence contradiction by A9,A17,A25,A45,A74,AFF_1:39;
        end;
A75:    not LIN a2,b2,a3
        proof
A76:      a2<>b2
          proof
            assume a2=b2;
            then LIN a2,a1,b1 by A22,ANALMETR:def 11;
            then LIN a29,a19,b19 by ANALMETR:55;
            then LIN a19,b19,a29 by AFF_1:15;
            hence contradiction by A5,A7,A14,A24,A32,AFF_1:39;
          end;
          assume LIN a2,b2,a3;
          then LIN a29,b29,a39 by ANALMETR:55;
          hence contradiction by A9,A11,A18,A25,A76,AFF_1:39;
        end;
A77:    X is satisfying_TDES implies X is satisfying_des
        proof
          assume X is satisfying_TDES;
          then Af(X) is Moufangian by Th7;
          then Af(X) is translational by AFF_2:28;
          hence thesis by Th8;
        end;
        LIN a19,a29,x9 by A43,ANALMETR:55;
        then LIN x9,a19, a29 by AFF_1:15;
        then LIN x,a1,a2 by ANALMETR:55;
        then x,a1 // x,a2 by ANALMETR:def 11;
        then a1,x // a2,x by ANALMETR:81;
        then a2,x // b1,y by A44,A64,ANALMETR:82;
        then
A78:    a2,x // b2,y by A60,A63,ANALMETR:82;
        a19,b19 // a39,b39 by A5,A6,A7,A8,A24,AFF_1:53,55;
        then a1,b1 // a3,b3 by ANALMETR:48;
        then
A79:    x,y // a3,b3 by A32,A59,ANALMETR:82;
        M9 // N9 by A57,ANALMETR:64;
        then a19,b19 // a49,b49 by A5,A7,A10,A12,AFF_1:53;
        then a1,b1 // a4,b4 by ANALMETR:48;
        then
A80:    x,y // a4,b4 by A32,A59,ANALMETR:82;
        M9 // N9 by A57,ANALMETR:64;
        then a19,b19 // a29,b29 by A5,A7,A9,A11,AFF_1:53;
        then a1,b1 // a2,b2 by ANALMETR:48;
        then
A81:    a2,b2 // x,y by A32,A59,ANALMETR:82;
        a2,a3 // b2,b3 by A21,ANALMETR:81;
        then
A82:    x,a3 // y,b3 by A2,A77,A51,A75,A81,A58,A78,Def8;
        M9 // N9 by A57,ANALMETR:64;
        then a19,b19 // a49, b49 by A5,A7,A10,A12,AFF_1:53;
        then a1,b1 // a4,b4 by ANALMETR:48;
        then x,a4 // y,b4 by A2,A23,A59,A77,A55,A54,A64,Def8;
        hence thesis by A2,A77,A82,A65,A70,A79,A80,Def8;
      end;
      now
        assume not M // N;
        then not M9 // N9 by ANALMETR:64;
        then consider o9 being Element of Af X such that
A83:    o9 in M9 and
A84:    o9 in N9 by A24,A25,AFF_1:72;
        reconsider o=o9 as Element of X by ANALMETR:47;
        consider K such that
A85:    o in K and
A86:    N _|_ K by A4,Th3;
        reconsider K9=K as Subset of Af(X) by ANALMETR:57;
A87:    K is being_line by A86,ANALMETR:62;
        then
A88:    K9 is being_line by ANALMETR:58;
        now
A89:      not a2 in K & not a4 in K & not b2 in K & not b4 in K
          proof
A90:        now
              let x be Element of X;
              assume that
A91:          x in N and
A92:          o<>x;
              assume x in K;
              then o,x _|_ o,x by A84,A85,A86,A91,ANALMETR:78;
              hence contradiction by A92,ANALMETR:51;
            end;
            assume not thesis;
            hence contradiction by A9,A10,A11,A12,A13,A14,A15,A16,A83,A90;
          end;
A93:      LIN o,a2,b2 by A4,A9,A11,A84,Th4;
A94:      now
            assume LIN a1,b1,a4;
            then LIN a19,b19,a49 by ANALMETR:55;
            hence contradiction by A5,A7,A13,A24,A32,AFF_1:39;
          end;
A95:      LIN o,a3,b3 by A3,A6,A8,A83,Th4;
A96:      now
            assume LIN a3,b3,a2;
            then LIN a39,b39,a29 by ANALMETR:55;
            hence contradiction by A6,A8,A14,A24,A26,A32,AFF_1:39;
          end;
A97:      LIN o,a1,b1 by A3,A5,A7,A83,Th4;
A98:      now
            let x9 be Element of Af(X);
            consider D9 being Subset of Af(X) such that
A99:        x9 in D9 and
A100:       N9 // D9 by A25,AFF_1:63;
            reconsider D=D9 as Subset of the carrier of X by ANALMETR:57;
            N // D by A100,ANALMETR:64;
            then K _|_ D by A86,ANALMETR:73;
            then consider y being Element of X such that
A101:       y in K and
A102:       y in D by ANALMETR:88;
            reconsider y9=y as Element of Af(X) by ANALMETR:47;
            take y9;
            thus y9 in K9 & x9,y9 // N9 by A99,A100,A101,A102,AFF_1:54;
          end;
          then consider d19 be Element of Af(X) such that
A103:     d19 in K9 and
A104:     a19,d19 // N9;
          consider e19 be Element of Af(X) such that
A105:     e19 in K9 and
A106:     b19,e19 // N9 by A98;
          consider d39 be Element of Af(X) such that
A107:     d39 in K9 and
A108:     a39,d39 // N9 by A98;
          consider e39 be Element of Af(X) such that
A109:     e39 in K9 and
A110:     b39,e39 // N9 by A98;
          reconsider d1=d19,d3=d39,e1=e19,e3=e39 as Element of X by ANALMETR:
          47;
A111:     LIN o,d1,e1 by A85,A87,A103,A105,Th4;
A112:     o<>e1 & o<>e3 & o<>d1 & o<>d3
          proof
A113:       now
              let x,y be Element of X, x9,y9 be Element of Af(X
              );
              assume that
A114:         x9,y9 // N9 and
A115:         x=x9 and
A116:         y=y9 and
              x in M and
              y in K and
A117:         not x in N;
              assume o=y;
              then o9,x9 // N9 by A114,A116,AFF_1:48;
              hence contradiction by A25,A84,A115,A117,AFF_1:37;
            end;
            assume not thesis;
            hence
            contradiction by A5,A6,A7,A8,A17,A18,A19,A20,A103,A104,A107,
            A108,A109,A110,A105,A106,A113;
          end;
          a1,d1 // o,a2 by A9,A84,A104,Th6;
          then
A118:     d1,a1 // o,a2 by ANALMETR:81;
A119:     not e1 in N by A19,A106,AFF_1:49;
A120:     not d3 in N by A18,A108,AFF_1:49;
          a19,d19 // b19,e19 by A25,A104,A106,AFF_1:45;
          then a1,d1 // b1,e1 by ANALMETR:48;
          then
A121:     d1,a1 // e1,b1 by ANALMETR:81;
          assume
A122:     K<>M;
A123:     now
