            hence contradiction by A9,A14,A15,A22,A31,AFF_1:25;
          end;
          assume b1=b3;
          then b1,a1 // b1,a3 by A20,ANALMETR:59;
          then
A32:      b19,a19 // b19,a39 by ANALMETR:36;
          o9,a19 // o9,a39 by A5,A6,A8,A21,AFF_1:39,41;
          then LIN o9,a19,a39 by AFF_1:def 1;
