        assume o9,e19 // e29,d29;
        then o,e1 // e2,d2 by ANALMETR:36;
        then o,e1 // d2,e2 by ANALMETR:59;
        then
A64:    a3,a2 _|_ o,e1 by A61,A62,ANALMETR:62;
