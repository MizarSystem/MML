    .= -(1/(1/4))*((sin(2*x-h)*sin(h))/(cos(2*x-h)-cos(h))^2) by XCMPLX_1:77
    .= -4*sin(2*x-h)*sin(h)/(cos(2*x-h)-cos(h))^2;
