    .= sin.x*tan.x*1-sin.(x-h)*tan.(x-h)*1 by A1,FDIFF_8:1,XCMPLX_1:107
    .= (tan(#)sin).x-tan.(x-h)*sin.(x-h) by VALUED_1:5
    .= (tan(#)sin).x-(tan(#)sin).(x-h) by VALUED_1:5;
