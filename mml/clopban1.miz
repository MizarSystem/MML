then A20:   ||.f1.t.||  <= ||.f.|| by A19,AXIOMS:22;
        0<= |.c.| by COMPLEX1:132;
       hence ||.h1.t.|| <= |.c.|*||.f.|| by A17,A18,A20,AXIOMS:25;
     end;
A21: now let r be Real such that
A22:   r in PreNorms(h1);
        r in {||.h1.t.|| where t is VECTOR of X : ||.t.|| <= 1 }
         by A22,Def11;
       then consider t be VECTOR of X such that
A23:   r=||.h1.t.|| & ||.t.|| <= 1;
