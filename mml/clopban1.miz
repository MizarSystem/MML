     end;

A24: (for s be real number st s in PreNorms(h1) holds s <= |.c.|*||.f.|| )
       implies sup PreNorms(h1) <=  |.c.|*||.f.|| by SEQ_4:62;
      BoundedLinearOperatorsNorm(X,Y).(c*f)
      = sup PreNorms(h1) by Th34;
then A25: ||.c*f.|| <= |.c.|*||.f.|| by A1,A21,A24,CLVECT_1:def 9;
      now per cases;
       case
A26:     c <> 0c;
A27:     now let t be VECTOR of X such that
A28:       ||.t.|| <= 1;
            h1.t=c*f1.t by Th40;
then A29:       c"*h1.t =( c"* c)*f1.t by CLVECT_1:def 2
               .=1r*f1.t by A26,COMPLEX1:def 7,XCMPLX_0:def 7
               .=f1.t by CLVECT_1:def 2;
A30:       ||.c"*h1.t.|| =|.c".|*||.h1.t.|| by CLVECT_1:def 10;
A31:       ||.h1.t.||<= ||.c*f.||*||.t.|| by Th36;
            0 <= ||.c*f.|| by Th37;
           then ||.c*f.||*||.t.|| <= ||.c*f.||*1 by A28,AXIOMS:25;
then A32:       ||.h1.t.|| <= ||.c*f.|| by A31,AXIOMS:22;
A33:       0<= |.c".| by COMPLEX1:132;
            |.c".| =|.c.|" by COMPLEX1:152;
           hence ||.f1.t.|| <= |.c.|"*||.c*f.|| by A29,A30,A32,A33,AXIOMS:25;
         end;
A34:     now let r be Real such that
A35:       r in PreNorms(f1);
            r in {||.f1.t.|| where t is VECTOR of X : ||.t.|| <= 1 }
             by A35,Def11;
           then consider t be VECTOR of X such that
A36:       r=||.f1.t.|| & ||.t.|| <= 1;
           thus r <= |.c.|"*||.c*f.|| by A27,A36;
         end;
A37:     |.c.| <>0 by A26,COMPLEX1:133;
A38:     (for s be real number st s in PreNorms(f1) holds
            s <= |.c.|"*||.c*f.|| )
           implies sup PreNorms(f1) <= |.c.|"*||.c*f.|| by SEQ_4:62;
          BoundedLinearOperatorsNorm(X,Y).(f)
          = sup PreNorms(f1) by Th34;
then A39:     ||.f.|| <=|.c.|"*||.c*f.|| by A1,A34,A38,CLVECT_1:def 9;
          0 <= |.c.| by COMPLEX1:132;
         then |.c.|*||.f.|| <=|.c.|*(|.c.|"*||.c*f.||) by A39,AXIOMS:25;
         then |.c.|*||.f.|| <=(|.c.|*|.c.|")*||.c*f.||;
         then |.c.|*||.f.|| <=1*||.c*f.|| by A37,XCMPLX_0:def 7;
         hence |.c.|* ||.f.|| <=||.c*f.||;
       end;
       case
A40:     c = 0c;
         reconsider fz=f as VECTOR
           of C_VectorSpace_of_BoundedLinearOperators(X,Y) by A1,A2;
     c*f =Mult_(BoundedLinearOperators(X,Y),
            C_VectorSpace_of_LinearOperators(X,Y)).[c,f] by A1,CLVECT_1:def 1
            .=c*fz by A2,CLVECT_1:def 1
            .=0.C_VectorSpace_of_BoundedLinearOperators(X,Y)
