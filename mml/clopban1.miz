     =0.C_VectorSpace_of_BoundedLinearOperators(X,Y) by Th29
    .=Zero_(BoundedLinearOperators(X,Y),
         C_VectorSpace_of_LinearOperators(X,Y)) by A2,RLVECT_1:def 2
    .=0.C_NormSpace_of_BoundedLinearOperators(X,Y) by A1,RLVECT_1:def 2;
