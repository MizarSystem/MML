   end;

A5:now assume
A6:  f = 0.C_NormSpace_of_BoundedLinearOperators(X,Y);
     thus ||.f.|| = 0
     proof
       set z = (the carrier of X) --> 0.Y;
       reconsider z as Function of the carrier of X, the carrier of Y
         by FUNCOP_1:58;
       reconsider g=f as bounded LinearOperator of X,Y by A1,Def8;
A7:    z=g by A6,Th35;
A8:    now let r be Real such that
A9:      r in PreNorms(g);
          r in {||.g.t.|| where t is VECTOR of X : ||.t.|| <= 1 }
           by A9,Def11;
         then consider t be VECTOR of X such that
A10:      r=||.g.t.|| & ||.t.|| <= 1;
          ||.g.t.|| = ||.0.Y.|| by A7,FUNCOP_1:13
              .= 0 by CLVECT_1:def 10;
         hence 0 <= r & r <=0 by A10;
       end;
A11:   PreNorms(g) is non empty bounded_above by Th30;
       consider r0 be set such that
A12:   r0 in PreNorms(g) by XBOOLE_0:def 1;
       reconsider r0 as Real by A12;
A13:   0<=r0 by A8,A12;
