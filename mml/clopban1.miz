theorem Th36:
for X,Y be ComplexNormSpace,
 f being Point of C_NormSpace_of_BoundedLinearOperators(X,Y),
   g be bounded LinearOperator of X,Y st g=f holds
    for t be VECTOR of X holds ||.g.t.|| <= ||.f.|| * ||.t.||
proof
   let X,Y be ComplexNormSpace;
   let f being Point of C_NormSpace_of_BoundedLinearOperators(X,Y);
   let g be bounded LinearOperator of X,Y such that
A1:g=f;
A2:C_NormSpace_of_BoundedLinearOperators(X,Y)
     = CNORMSTR (# BoundedLinearOperators(X,Y),
         Zero_(BoundedLinearOperators(X,Y),
         C_VectorSpace_of_LinearOperators(X,Y)),
          Add_(BoundedLinearOperators(X,Y),
          C_VectorSpace_of_LinearOperators(X,Y)),
           Mult_(BoundedLinearOperators(X,Y),
           C_VectorSpace_of_LinearOperators(X,Y)),
           BoundedLinearOperatorsNorm(X,Y) #) by Def13;
A3:PreNorms(g) is non empty bounded_above by Th30;
    now let t be VECTOR of X;
      now per cases;
       case
A4:      t  = 0.X;
then A5:      g.t =g.(0c * 0.X) by CLVECT_1:2
            .=0c * g.(0.X) by Def4
            .=0.Y by CLVECT_1:2;
          ||.t.|| = 0 by A4,CLVECT_1:def 10;
         hence ||.g.t.|| <= ||.f.||*||.t.|| by A5,CLVECT_1:def 10;
       end;
       case t <> 0.X;
then A6:      ||.t.|| <> 0 by CLVECT_1:def 10;
then A7:      ||.t.|| > 0 by CLVECT_1:106;
         reconsider t0 = ||.t.||"+0*<i> as Element of COMPLEX
                  by XCMPLX_0:def 2;
          Im t0 = 0 by COMPLEX1:28;
then A8:      |. t0 .| = abs Re t0 by COMPLEX1:136
          .=abs( 1*||.t.||" ) by COMPLEX1:28
          .=abs( 1/||.t.||) by XCMPLX_0:def 9
          .=1/abs( ||.t.||) by ABSVALUE:15
          .=1/||.t.|| by A7,ABSVALUE:def 1
          .=1*||.t.||" by XCMPLX_0:def 9
          .=||.t.||";
         reconsider t1= t0*t as VECTOR of X;
A9:      ||.t1.|| = |.t0.| * ||.t.|| by CLVECT_1:def 10
                 .=1 by A6,A8,XCMPLX_0:def 7;
          ||.g.t.||/||.t.|| =||.g.t.|| * |. t0 .| by A8,
XCMPLX_0:def 9
            .=||.t0*g.t.|| by CLVECT_1:def 10
            .=||.g.t1.|| by Def4;
         then ||.g.t.||/||.t.|| in
           {||.g.s.|| where s is VECTOR of X : ||.s.|| <= 1 } by A9;
         then ||.g.t.||/||.t.|| in PreNorms(g) by Def11;
         then ||.g.t.||/||.t.|| <= sup PreNorms(g) by A3,SEQ_4:def 4;
         then ||.g.t.||/||.t.||
              <= BoundedLinearOperatorsNorm(X,Y).g by Th34;
then A10:     ||.g.t.||/||.t.|| <= ||.f.|| by A1,A2,CLVECT_1:def 9;
          ||.g.t.||/||.t.||*||.t.||
           = ||.g.t.||*||.t.||"*||.t.|| by XCMPLX_0:def 9
          .=||.g.t.||*(||.t.||"*||.t.||)
          .=||.g.t.||*1 by A6,XCMPLX_0:def 7
          .=||.g.t.||;
         hence ||.g.t.|| <= ||.f.||*||.t.|| by A7,A10,AXIOMS:25;
       end;
     end;
     hence ||.g.t.|| <= ||.f.||*||.t.||;
   end;
   hence thesis;
end;

theorem Th37:
for X,Y be ComplexNormSpace,
 f being Point of C_NormSpace_of_BoundedLinearOperators(X,Y) holds
   0 <= ||.f.||
proof
   let X,Y be ComplexNormSpace;
   let f being Point of C_NormSpace_of_BoundedLinearOperators(X,Y);
A1:C_NormSpace_of_BoundedLinearOperators(X,Y)
    = CNORMSTR (# BoundedLinearOperators(X,Y),
         Zero_(BoundedLinearOperators(X,Y),
         C_VectorSpace_of_LinearOperators(X,Y)),
          Add_(BoundedLinearOperators(X,Y),
          C_VectorSpace_of_LinearOperators(X,Y)),
           Mult_(BoundedLinearOperators(X,Y),
           C_VectorSpace_of_LinearOperators(X,Y)),
           BoundedLinearOperatorsNorm(X,Y)
      #) by Def13;
   then reconsider g=f as bounded LinearOperator of X,Y by Def8;
A2:now let r be Real such that
A3:  r in PreNorms(g);
      r in {||.g.t.|| where t is VECTOR of X : ||.t.|| <= 1 }
       by A3,Def11;
     then consider t be VECTOR of X such that
A4:  r=||.g.t.|| & ||.t.|| <= 1;
     thus 0 <= r by A4,CLVECT_1:106;
   end;
A5:PreNorms(g) is non empty bounded_above by Th30;
   consider r0 be set such that
A6:r0 in PreNorms(g) by XBOOLE_0:def 1;
   reconsider r0 as Real by A6;
    0 <= r0 by A2,A6;
then A7:0 <=sup PreNorms(g) by A5,A6,SEQ_4:def 4;
    BoundedLinearOperatorsNorm(X,Y).f = sup PreNorms(g) by Th34;
   hence thesis by A1,A7,CLVECT_1:def 9;
end;

theorem Th38:
for X,Y be ComplexNormSpace,
 f being Point of C_NormSpace_of_BoundedLinearOperators(X,Y)
  st f = 0.C_NormSpace_of_BoundedLinearOperators(X,Y) holds
   0 = ||.f.||
proof
   let X,Y be ComplexNormSpace;
   let f being Point of
       C_NormSpace_of_BoundedLinearOperators(X,Y) such that
A1:f = 0.C_NormSpace_of_BoundedLinearOperators(X,Y);
A2:C_NormSpace_of_BoundedLinearOperators(X,Y)
    = CNORMSTR (# BoundedLinearOperators(X,Y),
         Zero_(BoundedLinearOperators(X,Y),
         C_VectorSpace_of_LinearOperators(X,Y)),
          Add_(BoundedLinearOperators(X,Y),
          C_VectorSpace_of_LinearOperators(X,Y)),
           Mult_(BoundedLinearOperators(X,Y),
           C_VectorSpace_of_LinearOperators(X,Y)),
           BoundedLinearOperatorsNorm(X,Y) #) by Def13;
   thus ||.f.|| = 0
   proof
     set z = (the carrier of X) --> 0.Y;
     reconsider z as Function of the carrier of X, the carrier of Y
       by FUNCOP_1:58;
     reconsider g=f as bounded LinearOperator of X,Y by A2,Def8;
A3:  z=g by A1,Th35;
A4:  now let r be Real such that
A5:    r in PreNorms(g);
        r in {||.g.t.|| where t is VECTOR of X : ||.t.|| <= 1 }
         by A5,Def11;
       then consider t be VECTOR of X such that
A6:    r=||.g.t.|| & ||.t.|| <= 1;
        ||.g.t.|| = ||.0.Y.|| by A3,FUNCOP_1:13
                .= 0 by CLVECT_1:def 10;
       hence 0 <= r & r <=0 by A6;
     end;
A7:  PreNorms(g) is non empty bounded_above by Th30;
     consider r0 be set such that
A8:  r0 in PreNorms(g) by XBOOLE_0:def 1;
     reconsider r0 as Real by A8;
A9:  0 <= r0 & r0<=0 by A4,A8;
      (for s be real number st s in PreNorms(g) holds s <= 0)
         implies sup PreNorms(g) <= 0 by SEQ_4:62;
     then sup PreNorms(g) = 0 by A4,A7,A8,A9,SEQ_4:def 4;
     then BoundedLinearOperatorsNorm(X,Y).f=0 by Th34;
     hence thesis by A2,CLVECT_1:def 9;
   end;
end;

