    then A13: (r+abs(a))+(r+abs(a))<= dist(z1,z2) by ABSVALUE:11,SQUARE_1:83;
      (r+abs(a))+0<(r+abs(a))+(r+abs(a)) by A12,REAL_1:53;
    then (r+abs(a))<dist(z1,z2) by A13,AXIOMS:22;
    then r<dist(z1,z2) by A11,AXIOMS:22;
