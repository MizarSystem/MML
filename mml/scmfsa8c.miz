   .= if>0(a,I,J).insloc (card J + 2) by A27,A30,GRFUNC_1:8
   .= goto insloc (card I + card J + 3) by Th64;
      if>0(a,I,J) +* Start-At insloc 0 c= s3 by FUNCT_4:26;
