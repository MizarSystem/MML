by AMI_1:123;
A37: CurInstr(ProgramPart (s3),Comput(ProgramPart(s3),s3,k))
 = s3.IC Comput(ProgramPart( s3), s3,k) by Y,TX3,AMI_1:54
      .= if=0(a,I,J).IC Comput(ProgramPart( s3), s3,k) by A36,Th26;
    Directed J1 c= if=0(a,I,J) by A21,SCMFSA6A:55;
