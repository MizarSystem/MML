  set s1 = s +* (I +* Start-At( 0,SCM+FSA));
  set s2 = s +* (loop I +* Start-At( 0,SCM+FSA));
  assume that
A1: I is_closed_on s and
A2: I is_halting_on s;
  let m be Element of NAT;
A3: IC Comput(ProgramPart(s1),s1,m) in dom I by A1,SCMFSA7B:def 7;
