registration
  let A be lattice-wise category;
  cluster -> lattice-wise (non empty subcategory of A);
  coherence
  proof
    let B be non empty subcategory of A;
    thus B is semi-functional set-id-inheriting;
    hereby
      let b be object of B;
      reconsider a = b as object of A by ALTCAT_2:30;
      a is LATTICE by Def4;
      hence b is LATTICE;
    end;
    let a,b be object of B;
    reconsider a9 = a, b9 = b as object of A by ALTCAT_2:30;
    let A,B be LATTICE;
    assume A = a & B = b;
    then <^a,b^> c= <^a9,b9^> & <^a9,b9^> c= MonFuncs(A, B) by
    Def4,ALTCAT_2
    :32;
    hence thesis by XBOOLE_1:1;
  end;
end;

registration
  let A be with_all_isomorphisms (lattice-wise category);
