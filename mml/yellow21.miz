      FUNCT_2, CARD_1, REALSET1, STRUCT_0, ORDERS_2, LATTICE3, YELLOW_0,
      ALTCAT_2, FUNCTOR0, WAYBEL_0, WAYBEL_3, WAYBEL_8, WAYBEL10, FUNCTOR2,
      ALTCAT_4, WAYBEL17, YELLOW_9, YELLOW18;
 requirements SUBSET, BOOLE;
 definitions TARSKI, RELAT_2, WELLORD1, WELLORD2, ORDERS_2, LATTICE3, ORDERS_3,
      ALTCAT_1, ALTCAT_3, YELLOW_0, WAYBEL_0, WAYBEL_3, YELLOW18, XBOOLE_0,
      BINOP_1, STRUCT_0;
 theorems ZFMISC_1, RELAT_1, FUNCT_1, PBOOLE, SETWISEO, STRUCT_0, WELLORD1,
      WELLORD2, ORDERS_2, ORDERS_1, FUNCT_2, ALTCAT_1, ALTCAT_2, ORDERS_3,
      ALTCAT_3, ORDINAL1, CARD_1, CARD_5, WAYBEL_0, WAYBEL13, ORDINAL3, TARSKI,
