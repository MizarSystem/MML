      .=(1+1)(#)(f(#)(diff(f,Z).2))+2(#)((f`|Z)(#)(f`|Z)) by ThB2
      .=2(#)(f(#)(diff(f,Z).2))+2(#)((f`|Z)(#)(f`|Z))
