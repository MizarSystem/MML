    .=(1(#)((exp_R(#)cos)|Z)+(-1)(#)((exp_R(#)cos)|Z)+2(#)((exp_R(#)-sin)|Z)).x
                                                            by RFUNCT_1:33
       .=((1+(-1))(#)((exp_R(#)cos)|Z)+ 2(#)((exp_R(#)-sin)|Z)).x by ThB2
       .=(0(#)((exp_R(#)cos)|Z)).x+(2(#)((exp_R(#)-sin)|Z)).x
                                                by A6,B1,A17,VALUED_1:def 1
       .=0*((exp_R(#)cos)|Z).x+(2(#)((exp_R(#)-sin)|Z)).x
                                                by A4,B1,A17,VALUED_1:def 5
       .=(2(#)((exp_R(#)-sin)|Z)).x;
