:: from HEINE, 2007.07.27, A.T.
defpred P[Nat] means 2|^$1 >= $1 + 1;

Lm2: P[0] by FINSEQ_2:72,RVSUM_1:124;

Lm3: for n st P[n] holds P[n+1]
proof
  let n;
  assume 2|^n >= n+1;
  then 2|^n - n >= 1 & 2 >= 0 by XREAL_1:21;
  then
A1: 2*(2|^n - n) >= 2*1 by XREAL_1:66;
  2|^(n+1) - (n + 1 + 1) = 2*2|^n - (2*n - n + 2) by Th11
    .= 2*(2|^n - n) + (n - 2);
  then 2|^(n+1) - (n + 1 + 1) >= 2 + (n - 2) by A1,XREAL_1:8;
  then 2|^(n+1) >= 0 + (n + 1 + 1) by XREAL_1:21;
  hence 2|^(n+1) >= n + 1 + 1;
end;

theorem Th104:
  2|^n >= n + 1
proof for n holds P[n] from NAT_1:sch 2(Lm2,Lm3);
  hence thesis;
end;

theorem
  2|^n > n
proof
  2|^n >= n + 1 & n+1 > n by Th104,NAT_1:13;
  hence thesis by XXREAL_0:2;
end;

:: from AMI_4, 2008.02.14, A.T.

reserve k,i for Element of NAT;

scheme Euklides' { F(Nat)->Element of NAT,G(Nat)->
  Element of NAT,a()->Element of NAT,b()->Element of NAT } :
  ex k st F(k) = a() hcf b() & G(k) = 0
provided
A1: 0 < b()
and
A2: b() < a()
and
A3: F(0) = a()
and
A4: G(0) = b()
and
A5: for k st G(k) > 0 holds F(k+1) = G(k) & G(k+1) = F(k) mod G(k)
proof
A6: 0 < b() & b() < a() by A1,A2;
  deffunc F(Nat,set) = IFEQ($2,0,0,G($1));
  consider q being Function of NAT,NAT such that
A7: q.0 = a() and
A8: for i being Nat holds q.(i+1) = F(i,q.i) from NAT_1:sch 12;
  deffunc Q(Element of NAT) = q.$1;
  q.(0+1) = IFEQ(q.0,0,0,G(0)) by A8
    .= G(0) by A2,A7,FUNCOP_1:def 8;
  then
A9: Q(0) = a() & Q(1 qua Element of NAT) = b() by A4,A7;
A10: for k st q.k qua Element of NAT > 0 holds q.k = F(k)
  proof
    let k such that
A11: q.k qua Element of NAT > 0;
    now per cases;
      case k = 0;
        hence q.k = F(k) by A3,A7;
      end;
      case k <> 0;
        then consider i being Nat such that
A12:    k = i+1 by NAT_1:6;
        reconsider i as Element of NAT by ORDINAL1:def 13;
A13:    now
          assume
A14:      q.i qua Element of NAT = 0;
          q.k = IFEQ(q.i,0,0,G(i)) by A8,A12
            .= 0 by A14,FUNCOP_1:def 8;
          hence contradiction by A11;
        end;
        q.k = IFEQ(q.i,0,0,G(i)) by A8,A12
          .= G(i) by A13,FUNCOP_1:def 8;
        hence q.k = F(k) by A5,A11,A12;
      end;
    end;
    hence q.k = F(k);
  end;
A15: for k holds Q(k+2) = Q(k) mod Q(k+1)
  proof
    let k;
    now per cases;
      case
A16:    q.(k+1) <> 0;
        now
          assume
A17:      G(k) = 0;
A18:      q.k = 0 or q.k <> 0;
          q.(k+1) = IFEQ(q.k,0,0,G(k)) by A8
            .= 0 by A17,A18,FUNCOP_1:def 8;
          hence contradiction by A16;
        end;
        then
A19:    G(k) > 0;
A20:    q.(k+(1+1)) = q.((k+1)+1)
          .= IFEQ(q.(k+1),0,0,G(k+1)) by A8
          .= G(k+1) by A16,FUNCOP_1:def 8
          .= F(k) mod G(k) by A5,A19;
A21:    now
          assume
A22:      q.k = 0;
          q.(k+1) = IFEQ(q.k,0,0,G(k)) by A8
            .= 0 by A22,FUNCOP_1:def 8;
          hence contradiction by A16;
        end;
        then
A23:    q.k qua Element of NAT > 0;
        q.(k+1) = IFEQ(q.k,0,0,G(k)) by A8
          .= G(k) by A21,FUNCOP_1:def 8;
        hence q.(k+2) = q.(k) mod q.(k+1) by A10,A20,A23;
      end;
      case
A24:    q.(k+1) = 0;
        thus q.(k+2) = q.((k+1)+1)
          .= IFEQ(q.(k+1),0,0,G(k+1)) by A8
          .= 0 by A24,FUNCOP_1:def 8
          .= q.(k) mod q.(k+1) by A24,NAT_D:def 2;
      end;
    end;
    hence q.(k+2) = q.(k) mod q.(k+1);
  end;
  consider k such that
A25: Q(k) = a() hcf b() & Q(k + 1) = 0 from NAT_D:sch 1(A6,A9,A15);
  take k;
A26: a() hcf b() > 0 by A1,Th71;
  thus F(k) = a() hcf b() by A1,A10,A25,Th71;
  thus G(k) = IFEQ(q.k,0,0,G(k)) by A25,A26,FUNCOP_1:def 8
    .= 0 by A8,A25;
end;
