      FUNCT_1, INT_1, XREAL_0, XBOOLE_0, XCMPLX_1, REAL_2, INT_2, FINSET_1,
      CARD_2, CARD_1, XBOOLE_1, XREAL_1, XCMPLX_0, XXREAL_0, RELAT_1, FINSOP_1,
