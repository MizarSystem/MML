 schemes NAT_1, FINSEQ_1, SUBSET_1, NAT_D;

begin

reserve i,j,k,n,m,l,s,t,r for Nat;
reserve a,b for real number;
reserve F,G,H for FinSequence of REAL;

canceled 2;

theorem Th3:
  for F,G being FinSequence st
  len F = len G & (for i st i in dom F holds F.i = G.i) holds F = G
proof
  let F,G be FinSequence;
  assume
A1: len F = len G & for i st i in dom F holds F.i = G.i;
A2: dom F = Seg len F by FINSEQ_1:def 3;
  dom G = Seg len F by A1,FINSEQ_1:def 3;
  hence thesis by A1,A2,FINSEQ_1:17;
end;

canceled;

theorem Th5:
  n>=1 implies Seg n = {1} \/ {k where k is Element of NAT: 1<k & k<n} \/ {n}
proof
  assume
A1: n>=1;
A2: n in NAT by ORDINAL1:def 13;
A3: Seg n c= {1} \/ {k where k is Element of NAT: 1<k & k<n} \/ {n}
  proof
    let d be set;
    assume
A4: d in Seg n;
    per cases by A1,REAL_1:def 5;
    suppose
A5:   n>1;
      reconsider l = d as Element of NAT by A4;
      1 <= l & l <= n by A4,FINSEQ_1:3;
      then 1 = l or (1<l & l<n) or l = n or 1 = n by REAL_1:def 5;
      then d in {1} or d in {k where k is Element of NAT: 1<k & k<n} or
      d in {n} by A5,TARSKI:def 1;
      then d in {1} \/ {k where k is Element of NAT: 1<k & k<n} or d in {n}
      by XBOOLE_0:def 2;
      hence thesis by XBOOLE_0:def 2;
    end;
    suppose n = 1;
      hence thesis by A4,FINSEQ_1:4,XBOOLE_0:def 2;
    end;
  end;
  {1} \/ {k where k is Element of NAT: 1<k & k<n} \/ {n} c= Seg n
  proof
A6: for d be set holds d in {1} implies d in Seg n
    proof
      let d be set;
      assume d in {1};
      then d = 1 by TARSKI:def 1;
      hence d in Seg n by A1;
    end;
A7: for d be set holds d in {k where k is Element of NAT: 1<k & k<n}
    implies d in Seg n
    proof
      let d be set;
      assume d in {k where k is Element of NAT: 1<k & k<n};
      then consider k being Element of NAT such that
A8:   d = k & 1<k & k<n;
      thus d in Seg n by A8;
    end;
A9: for d be set holds d in {n} implies d in Seg n
    proof
      let d be set;
      assume d in {n};
      then d = n by TARSKI:def 1;
      hence d in Seg n by A1,A2;
    end;
    let d be set;
    assume d in ({1} \/ {k where k is Element of NAT: 1<k & k<n} \/ {n});
    then d in ({1} \/ {k where k is Element of NAT: 1<k & k<n}) or
    d in {n} by XBOOLE_0:def 2;
    then d in {1} or d in {k where k is Element of NAT: 1<k & k<n} or
    d in {n} by XBOOLE_0:def 2;
    hence thesis by A6,A7,A9;
  end;
  hence thesis by A3,XBOOLE_0:def 10;
end;

theorem Th6:
  len (a*F) = len F
proof
  reconsider G = F as Element of (len F)-tuples_on REAL by FINSEQ_2:110;
  len (a*G) = len G by FINSEQ_2:109;
  hence thesis;
end;

theorem Th7:
  dom G = dom (a*G)
proof
  len (a*G) = len G by Th6;
  hence thesis by FINSEQ_3:31;
end;

::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
::    x |^ n  Function                                                      ::
::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::

definition
  let i be Nat;
  let x be real number;
  redefine func i |-> x -> FinSequence of REAL;
  coherence
  proof
    let z be set;
    assume z in rng (i |-> x);
    hence thesis;
  end;
end;

registration
  let x be complex number, n be Nat;
  cluster n |-> x -> complex-yielding;
  coherence;
end;

definition
  let x be complex number, n be Nat;
  func x |^ n equals

  Product (n |-> x);
  coherence;
end;

registration
  let x be real number, n be Nat;
  cluster x |^ n -> real;
  coherence;
end;

definition
  let x be Real, n be Nat;
  redefine func x |^ n -> Real;
  coherence by XREAL_0:def 1;
end;

reserve z for complex number;

canceled;

theorem
  z|^0 = 1 by FINSEQ_2:72,RVSUM_1:124;

registration
  let z be complex number, n be Nat;
  cluster z|^n -> complex;
  coherence;
end;

theorem Th10:
  z|^1 = z
proof
  reconsider z as Element of COMPLEX by XCMPLX_0:def 2;
  z|^1 = Product(<*z*>) by FINSEQ_2:73
    .= z by RVSUM_1:125;
  hence thesis;
end;

theorem Th11:
  z|^(s+1) = z|^s*z
proof
  defpred P[Nat] means z|^($1+1) = z|^$1*z;
A1: P[0]
  proof
    reconsider z as Element of COMPLEX by XCMPLX_0:def 2;
    z|^(0+1) = Product(<*z*>) by FINSEQ_2:73
      .= 1*z by RVSUM_1:125;
    hence thesis by FINSEQ_2:72,RVSUM_1:124;
  end;
A2: for s st P[s] holds P[s+1]
  proof
    let s;
    assume z|^(s+1) = z|^s*z;
    reconsider z as Element of COMPLEX by XCMPLX_0:def 2;
    z|^(s+1+1) = Product(((s+1) |-> z) ^ <*z*>) by FINSEQ_2:74
      .= Product((s+1) |-> z)*z by RVSUM_1:126;
    hence thesis;
  end;
  for s holds P[s] from NAT_1:sch 2(A1,A2);
  hence thesis;
end;

registration
  let x, n be Nat;
  cluster x|^n -> natural;
  coherence
  proof
    defpred P[Nat] means x|^$1 is natural;
A1: P[0] by FINSEQ_2:72,RVSUM_1:124;
A2: for a being Nat st P[a] holds P[a+1]
    proof
      let a be Nat;
      assume P[a];
      then reconsider b = x|^a as Nat;
      x|^(a+1) = b*x by Th11;
      hence thesis;
    end;
    for a being Nat holds P[a] from NAT_1:sch 2(A1,A2);
    hence thesis;
  end;
end;

reserve x,y for complex number;

theorem
  (x*y)|^s = x|^s * y|^s
proof
  reconsider x, y as Element of COMPLEX by XCMPLX_0:def 2;
  x|^s*y|^s = Product(s |-> (x*y)) by RVSUM_1:143;
  hence thesis;
end;

theorem
  x|^(s+t) = x|^s * x|^t
proof
  reconsider x as Element of COMPLEX by XCMPLX_0:def 2;
  x|^s*x|^t = Product((s+t) |-> x) by RVSUM_1:141;
  hence thesis;
end;

theorem
  (x|^s)|^t = x|^(s*t)
proof
  reconsider x as Element of COMPLEX by XCMPLX_0:def 2;
  x|^(s*t) = Product(t |-> x|^s) by RVSUM_1:142;
  hence thesis;
end;

theorem Th15:
  1|^s = 1
proof
  defpred P[Nat] means 1|^$1 = 1;
A1: P[0] by FINSEQ_2:72,RVSUM_1:124;
A2: now
    let s;
    assume P[s];
    then 1|^(s+1) = 1 * 1 by Th11;
    hence P[s+1];
  end;
  for s holds P[s] from NAT_1:sch 2(A1,A2);
  hence thesis;
end;

theorem Th16:
  s >= 1 implies 0|^s = 0
proof
  defpred P[Nat] means 0|^$1 = 0;
A1: P[1] by Th10;
A2: now
    let n be Nat;
    assume n>=1 & P[n];
    0|^(n+1) = 0|^n * 0 by Th11
      .= 0;
    hence P[n+1];
  end;
  for n be Nat st n >=1 holds P[n] from NAT_1:sch 8(A1,A2);
  hence thesis;
end;

