                    .= {} \/ ((E9\E) \ E) by XBOOLE_1:37
                    .= E9 \ (E \/ E) by XBOOLE_1:41;
                  then i in E9 \ (E \/ E) by A54,A58,XBOOLE_0:def 5;
                  then i in dom g /\ (E9\E) by A52,XBOOLE_0:def 4;
