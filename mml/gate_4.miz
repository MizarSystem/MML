  ($b11 iff $XOR2(a10,AND2(g11,a15)))&
  ($b12 iff $XOR2(a11,AND2(g12,a15)))&
  ($b13 iff $XOR2(a12,AND2(g13,a15)))&
  ($b14 iff $XOR2(a13,AND2(g14,a15)))&
