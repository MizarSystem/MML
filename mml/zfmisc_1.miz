theorem
 not A in [:A,B:]
  proof
   assume A in [:A,B:];
    then consider x,y such that
W:   x in A & y in B & A = [x,y] by Th103;
    x = {x} or x = {x,y} by W, TARSKI:def 2;
    then x in x by TARSKI:def 1,def 2;
   hence contradiction;
  end;

theorem
 [x,{x}] in [:{x},[x,{x}]:]
  proof
    [:{x},[x,{x}]:] = { [x,{x}], [x,{x,{x}}] } by Th36;
   hence thesis by TARSKI:def 2;
  end;

theorem
 B in [:A,B:] implies ex x st x in A & B = [x,{x}]
  proof
   assume B in [:A,B:];
    then consider x,y such that
W:   x in A & y in B & B = [x,y] by Th103;
   take x;
   thus x in A by W;
pc:  y in [x,y] by W;
   per cases by pc,TARSKI:def 2;
   suppose y = {x};
    hence thesis by W;
   end;
   suppose y = {x,y};
     then y in y by TARSKI:def 2;
    hence thesis;
   end;
  end;
