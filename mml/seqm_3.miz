    then r*seq.n<r*seq.(n+1) by A2,XREAL_1:70;
    then (r(#)seq).n<r*seq.(n+1) by SEQ_1:13;
    hence thesis by SEQ_1:13;
  end;
  thus 0=r implies r(#)seq is constant
