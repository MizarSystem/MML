  assume abs seq is bounded;
  then consider r be real number such that
A2: 0<r and
A3: for n be Element of NAT holds abs(abs seq.n)<r by SEQ_2:15;
  now
    let n be Element of NAT;
    abs(abs seq .n)=abs(abs(seq.n)) by SEQ_1:16
