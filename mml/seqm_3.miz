  proof
    assume
A1: seq is bounded;
    let r;
    per cases;
    suppose
      0<r;
      hence thesis by A1;
    end;
    suppose
      0=r;
      hence thesis by Th68;
    end;
    suppose
      r<0;
      hence thesis by A1;
    end;
  end;
