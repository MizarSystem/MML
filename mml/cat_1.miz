A1: Hom(a,b) <> {} and
A2: Hom(b,a) <> {};
  let g1,g2 be Morphism of b,a;
  assume
A3: f*g1=id b;
  assume g2*f=id a;
  hence g1 = (g2*f)*g1 by A2,Th57
    .= g2*(id b) by A1,A2,A3,Th54
    .= g2 by A2,Th58;
end;

definition
