    end;
 for n holds X[n] from NAT_1:sch 1(A12,A13);
    hence thesis;
  end;
  hence thesis by A4,A7,PREPOWER:2,SEQ_4:29;
end;

definition
  func tan -> PartFunc of REAL, REAL equals
  sin/cos;
  coherence;
  func cot -> PartFunc of REAL, REAL equals
  cos/sin;
  coherence;
end;

theorem Th75:
  [.0,1 .] c= dom tan & ].0,1 .[ c= dom tan
proof
A1: [.0,1 .] \ cos"{0} c=dom cos \ cos"{0} by Th27,XBOOLE_1:33;
 [.0,1 .] /\ cos"{0}={}
  proof
    assume [.0,1 .] /\ cos"{0}<>{};
    then consider rr such that
A4: rr in [.0,1 .] /\ cos"{0} by XBOOLE_0:def 1;
A5: rr in [.0,1 .] by A4,XBOOLE_0:def 4;
A6: rr in cos"{0} by A4,XBOOLE_0:def 4;
A7: cos.(rr) <>0 by A5,Th74;
 cos.(rr) in {0} by A6,FUNCT_1:def 13;
    hence contradiction by A7,TARSKI:def 1;
  end;
then  [.0,1 .] misses cos"{0} by XBOOLE_0:def 7;
then  [.0,1 .] c= dom cos \ cos"{0} by A1,XBOOLE_1:83;
then  [.0,1 .] c= dom sin /\ (dom cos \ cos"{0}) by Th27,XBOOLE_1:19;
then A12: [.0,1 .] c= dom tan by RFUNCT_1:def 4;
 ].0,1 .[c=[.0,1 .] by XXREAL_1:25;
  hence thesis by A12,XBOOLE_1:1;
end;

Lm15: dom(tan|[.0,1 .]) = [.0,1 .] &
for th st th in [.0,1 .] holds tan|[.0,1 .].th=tan.th
proof
 dom(tan|[.0,1 .])=dom tan /\ [.0,1 .] by RELAT_1:90;
then  dom(tan|[.0,1 .]) = [.0,1 .] by Th75,XBOOLE_1:28;
  hence thesis by FUNCT_1:70;
end;

Lm16: tan is_differentiable_on ].0,1 .[ & for th st th in ].0,1 .[ holds
diff(tan,th)>0
proof
  A1: sin is_differentiable_on ].0,1 .[ & cos is_differentiable_on ].0,1 .[ by
  Th72,Th73,FDIFF_1:34;
A2: for th be Real st th in ].0,1 .[ holds cos.th<>0
  proof
    let th be Real such that
A3: th in ].0,1 .[;
 ].0,1 .[ c=[.0,1 .] by XXREAL_1:25;
    hence thesis by A3,Th74;
  end;
 for th st th in ].0,1 .[ holds diff(tan,th)>0
  proof
    let th such that
A6: th in ].0,1 .[;
A7: th is Real & cos is_differentiable_in th by Th68,XREAL_0:def 1;
A8: sin is_differentiable_in th by Th69;
A9: cos.th<>0 by A2,A6;
then A10: diff(tan,th)=(diff(sin,th)*cos.th-diff(cos,th)*sin.th)/(cos.th) ^2
    by A7,A8,FDIFF_2:14
      .=(cos.th*cos.th-diff(cos,th)*sin.th)/(cos.th)^2 by Th69
      .=(cos.th*cos.th-(-sin.th)*sin.th)/(cos.th)^2 by Th68
      .=((cos.th)^2+(sin.th)*sin.th)/(cos.th)^2
      .=1/(cos.th)^2 by Th31;
 (cos.th)^2>0 by A9,SQUARE_1:74;
    hence thesis by A10;
  end;
  hence thesis by A1,A2,FDIFF_2:21;
end;

theorem Th76:
  tan|[.0,1 .] is continuous
proof
 for th be real number st th in dom(tan|[.0,1 .]) holds
  tan|[.0,1 .] is_continuous_in th
  proof
    let th be real number;
    assume
A2: th in dom(tan|[.0,1 .]);
then A3: th in [.0,1 .] by RELAT_1:86;
 dom sin = REAL by FUNCT_2:def 1;
then A5: th in dom sin by XREAL_0:def 1;
A6: th in [#]REAL by XREAL_0:def 1;
then  sin is_differentiable_in th by Th73,FDIFF_1:16;
then A8: sin is_continuous_in th by FDIFF_1:32;
 cos is_differentiable_in th by A6,Th72,FDIFF_1:16;
then A10: cos is_continuous_in th by FDIFF_1:32;
 cos.(th) <> 0 by A3,Th74;
then A12: tan is_continuous_in th by A5,A8,A10,FCONT_1:11;
 now
      let rseq;
      assume that
A14:  rng rseq c= dom (tan|[.0,1 .]) and
A15:  rseq is convergent & lim rseq = th;
A16:  rng rseq c= dom tan by A14,Lm15,Th75,XBOOLE_1:1;
then A17:  tan.th = lim (tan/*rseq) by A12,A15,FCONT_1:def 1;
  now
        let k1 be Element of NAT;
    dom (rseq) = NAT by SEQ_1:3;
then     rseq.k1 in rng rseq by FUNCT_1:def 5;
then A21:    (tan|[.0,1 .]).(rseq.k1)=tan.(rseq.k1) by A14,Lm15;
    (tan|[.0,1 .]).(rseq.k1)=((tan|[.0,1 .])/*rseq).k1 by A14,FUNCT_2:185;
        hence ((tan|[.0,1 .])/*rseq).k1=(tan/*rseq).k1
        by A14,A21,Lm15,Th75,FUNCT_2:185,XBOOLE_1:1;
      end;
then   (tan|[.0,1 .])/*rseq =tan/*rseq by FUNCT_2:113;
      hence (tan|[.0,1 .])/*rseq is convergent
      & (tan|[.0,1 .]).th = lim ((tan|[.0,1 .])/*rseq)
      by A2,A12,A15,A16,A17,Lm15,FCONT_1:def 1;
    end;
    hence thesis by FCONT_1:def 1;
  end;
  hence thesis by FCONT_1:def 2;
end;

theorem Th77:
  th1 in ].0,1 .[ & th2 in ].0,1 .[ & tan.th1=tan.th2 implies th1=th2
proof
  assume that
A1: th1 in ].0,1 .[ and
A2: th2 in ].0,1 .[ and
A3: tan.(th1)=tan.(th2);
A4: 0<th1 by A1,XXREAL_1:4;
A5: th1<1 by A1,XXREAL_1:4;
A6: 0<th2 by A2,XXREAL_1:4;
A7: th2<1 by A2,XXREAL_1:4;
  assume
A8: th1<>th2;
 now per cases by A8,XXREAL_0:1;
    suppose
A10:  th1<th2;
A11:  for th be Real st th in ].th1,th2.[ holds th in ].0,1 .[
      proof
        let th be Real;
        assume
A12:    th in ].th1,th2.[;
then A13:    th1<th by XXREAL_1:4;
    th< th2 by A12,XXREAL_1:4;
then     th <1 by A7,XXREAL_0:2;
        hence thesis by A4,A13,XXREAL_1:4;
      end;
A16:  for th be Real st th in [.th1,th2.] holds th in [.0,1 .]
      proof
        let th be Real;
        assume
A17:    th in [.th1,th2.];
then A18:    th1<=th by XXREAL_1:1;
    th<= th2 by A17,XXREAL_1:1;
then     th <=1 by A7,XXREAL_0:2;
        hence thesis by A4,A18,XXREAL_1:1;
      end;
  ].th1,th2.[c=].0,1 .[ by A11,SUBSET_1:7;
then A22:  tan is_differentiable_on ].th1,th2.[ by Lm16,FDIFF_1:34;
        [.th1,th2.]c=[. 0,1 .] & tan|[.th1,th2.] is continuous by A16,Th76,
      FCONT_1:17,SUBSET_1:7;
      then consider r such that
A24:  r in ].th1,th2.[ & diff(tan,r)=0
      by A1,A2,A3,A10,A22,Th75,ROLLE:1,XBOOLE_1:1;
      take th=r;
      thus th in ].0,1 .[ & diff(tan,(th))=0 by A11,A24;
    end;
    suppose
A25:  th2<th1;
A26:  for th be Real st th in ].th2,th1 .[ holds th in ].0,1 .[
      proof
        let th be Real;
        assume
A27:    th in ].th2,th1 .[;
then A28:    th2<th by XXREAL_1:4;
    th< th1 by A27,XXREAL_1:4;
then     th <1 by A5,XXREAL_0:2;
        hence thesis by A6,A28,XXREAL_1:4;
      end;
A31:  for th be Real st th in [.th2,th1 .] holds th in [.0,1 .]
      proof
        let th be Real;
        assume
A32:    th in [.th2,th1 .];
then A33:    th2<=th by XXREAL_1:1;
    th<= th1 by A32,XXREAL_1:1;
then     th <=1 by A5,XXREAL_0:2;
        hence thesis by A6,A33,XXREAL_1:1;
      end;
  ].th2,th1 .[c=].0,1 .[ by A26,SUBSET_1:7;
then A37:  tan is_differentiable_on ].th2,th1 .[ by Lm16,FDIFF_1:34;
        [.
th2,th1 .] c=[.0,1 .] & tan|[.th2,th1.] is continuous by A31,Th76,
      FCONT_1:17,SUBSET_1:7;
      then consider r such that
A39:  r in ].th2,th1 .[ & diff(tan,r)=0
      by A1,A2,A3,A25,A37,Th75,ROLLE:1,XBOOLE_1:1;
      take th=r;
      thus th in ].0,1 .[ & diff(tan,(th))=0 by A26,A39;
    end;
  end;
  hence thesis by Lm16;
end;

Lm17: tan.0 = 0 & tan.1 >1
proof
 0 in [.0,1 .] by XXREAL_1:1;
then A2: tan.0=(sin.0)* (cos.0)" by Th75,RFUNCT_1:def 4
    .=0 by Th33;
 1 in [.0,1 .] by XXREAL_1:1;
then  tan.1 =(sin.1)/(cos.1) by Th75,RFUNCT_1:def 4;
  hence thesis by A2,Th47,XREAL_1:189;
end;

begin :: Existence of Circle Ratio

definition
  func PI -> real number means
  :Def32:
  tan.(it/4) = 1 & it in ]. 0, 4 .[;
  existence
  proof
A1: [.tan.0,tan.1 .] = {r : tan.0<=r & r<= tan.1 } by RCOMP_1:def 1;
 [.tan.1,tan.0 .]= {} by Lm17,XXREAL_1:29;
then  1 in [.tan.0,tan.1 .] \/ [.tan.1,tan.0 .] by A1,Lm17;
    then consider th be Real such that
A4: th in [.0, 1 .] and
A5: tan.(th)=1 by Th75,Th76,FCONT_2:16;
A6: 0<=th by A4,XXREAL_1:1;
A7: th <= 1 by A4,XXREAL_1:1;
A8: 0 <th by A5,A6,Lm17;
A9: th <1 by A5,A7,Lm17,XXREAL_0:1;
    take th1 =th*4;
    thus tan.(th1/4)=1 by A5;
 th*4<1*4 by A9,XREAL_1:70;
    hence thesis by A8,XXREAL_1:4;
  end;
  uniqueness
  proof
    let th1,th2 be real number such that
A11: tan.(th1/4) = 1 and
A12: th1 in ]. 0, 4 .[ and
A13: tan.(th2/4) = 1 and
A14: th2 in ]. 0, 4 .[;
A15: 0<th1 by A12,XXREAL_1:4;
 th1 <4 by A12,XXREAL_1:4;
then  th1/4 <4/4 by XREAL_1:76;
then A18: th1/4 in ]. 0, 1 .[ by A15,XXREAL_1:4;
A19: 0<th2 by A14,XXREAL_1:4;
 th2 < 4 by A14,XXREAL_1:4;
then  th2/4 <4/4 by XREAL_1:76;
then  th2/4 in ]. 0, 1 .[ by A19,XXREAL_1:4;
then  th1/4=th2/4 by A11,A13,A18,Th77;
    hence thesis;
  end;
end;

definition
  redefine func PI -> Real;
  coherence by XREAL_0:def 1;
end;

theorem Th78:
  sin.(PI/4) = cos.(PI/4)
proof
A1: PI in ].0, 4.[ by Def32;
then A2: 0< PI by XXREAL_1:4;
 PI < 4 by A1,XXREAL_1:4;
then  PI/4 < 4/4 by XREAL_1:76;
then A5: PI/4 in ].0,1 .[ by A2,XXREAL_1:4;
 tan.(PI/4) =1 by Def32;
then  sin.(PI/4) * (cos.(PI/4))"=1 by A5,Th75,RFUNCT_1:def 4;
  hence thesis by XCMPLX_1:210;
end;

begin :: Formulas of sin and cos

theorem Th79:
  sin.(th1+th2)=(sin.(th1)) *(cos.(th2))+(cos.(th1)) * (sin.(th2)) &
  cos.(th1+th2)=(cos.(th1)) *(cos.(th2))-(sin.(th1)) * (sin.(th2))
proof
  reconsider th1,th2 as Real by XREAL_0:def 1;
A1: (th1+th2)*<i> = 0+0+(th1+th2)*<i>;
A2: Sum(th1*<i> ExpSeq ) * Sum(th2*<i> ExpSeq )
  =Sum((th1*<i>+th2*<i>) ExpSeq ) by Lm2
    .=cos.(th1+th2)+(sin.(th1+th2))*<i> by A1,Lm3;
 Sum((th1*<i> ExpSeq) ) * Sum((th2*<i> ExpSeq) )
  =(cos.(th1)+(sin.th1)*<i>)* Sum((th2*<i> ExpSeq) ) by Lm3
    .=(cos.(th1)+sin.(th1)*<i>)*(cos.(th2)+(sin.th2)*<i>) by Lm3
    .=((cos.(th1)) *( cos.(th2))-(sin.(th1)) * (sin.(th2))+
  ((sin.(th1)) * (cos.(th2))+(cos.(th1)) * (sin.(th2)))*<i>);
  hence thesis by A2,COMPLEX1:163;
end;

theorem
  sin(th1+th2)=(sin(th1)) *(cos(th2))+(cos(th1)) * (sin(th2)) &
  cos(th1+th2)=(cos(th1)) *(cos(th2))-(sin(th1)) * (sin(th2)) by Th79;

theorem Th81:
  cos.(PI/2) = 0 & sin.(PI/2) = 1 & cos.(PI) = -1 & sin.(PI) = 0 &
  cos.(PI+PI/2) = 0 & sin.(PI+PI/2) = -1 & cos.(2 * PI) = 1 & sin.(2 * PI) =0
proof
  thus
A1: cos.(PI/2)=cos.(PI/4+PI/4) .=(cos.(PI/4)) * (cos.(PI/4))
  -(cos.(PI/4)) * (cos.(PI/4)) by Th78,Th79
    .=0;
  thus
A2: sin.(PI/2)=sin.(PI/4+PI/4) .= (cos.(PI/4)) *( cos.(PI/4))
  +(sin.(PI/4)) * (sin.(PI/4)) by Th78,Th79
    .=1 by Th31;
  thus
A3: cos.(PI)=cos.(PI/2+PI/2) .=0 * 0-sin.(PI/2) * sin.(PI/2) by A1,Th79
    .=-1 by A2;
  thus
A4: sin.(PI)=sin.(PI/2+PI/2) .=(sin.(PI/2)) *( cos.(PI/2))
  +(cos.(PI/2) )*( sin.(PI/2)) by Th79
    .=0 by A1;
  thus cos.(PI+PI/2) =(cos.(PI)) *( cos.(PI/2))
  -(sin.(PI)) * (sin.(PI/2)) by Th79
    .=0 by A1,A4;
  thus sin.(PI+PI/2) =(sin.(PI) )*( cos.(PI/2))
  +(cos.(PI)) *( sin.(PI/2) ) by Th79
    .=-1 by A1,A2,A3;
  thus cos.(2 * PI)=cos.(PI+PI) .=(-1 ) * (-1) -sin.(PI) * sin.(PI) by A3,Th79
    .=1 by A4;
  thus sin.(2 * PI)=sin.(PI+PI)
    .=(sin.(PI)) * (cos.(PI))+(cos.(PI)) * (sin.(PI)) by Th79
    .=0 by A4;
end;

theorem
  cos(PI/2) = 0 & sin(PI/2) = 1 & cos(PI) = -1 & sin(PI) = 0 &
  cos(PI+PI/2) = 0 & sin(PI+PI/2) = -1 &
  cos(2 * PI) = 1 & sin(2 * PI) =0 by Th81;

theorem Th83:
  sin.(th+2 * PI) = sin.th & cos.(th+2 * PI) = cos.th &
  sin.(PI/2-th) = cos.th & cos.(PI/2-th) = sin.th & sin.(PI/2+th) = cos.th &
  cos.(PI/2+th) = -sin.th & sin.(PI+th) = -sin.th & cos.(PI+th) = -cos.th
proof
  thus sin.(th+2 * PI)=sin.(th) * 1 +cos.(th) * 0 by Th79,Th81
    .=sin.th;
  thus cos.(th+2 * PI)=cos.(th) * 1 - sin.(th) * 0 by Th79,Th81
    .=cos.th;
  thus sin.(PI/2-th) =(sin.(PI/2) )* (cos.(-th))
  + (cos.(PI/2)) *( sin.(-th)) by Th79
    .=cos.(th) by Th33,Th81;
  thus cos.(PI/2-th) =(cos.(PI/2)) * (cos.(-th))
  - ( sin.(PI/2)) *( sin.(-th)) by Th79
    .=0 - 1 * (-sin.(th)) by Th33,Th81
    .=sin.(th);
  thus sin.(PI/2+th) =1 * cos.(th) + 0 * (sin.(th)) by Th79,Th81
    .=cos.(th);
  thus cos.(PI/2+th) =(cos.(PI/2)) *( cos.(th))
  -( sin.(PI/2) )*( sin.(th)) by Th79
    .=-sin.(th) by Th81;
  thus
  sin.(PI+th) =(sin.(PI)) * (cos.(th)) +( cos.(PI) )*( sin.(th)) by Th79
    .=-sin.(th) by Th81;
  thus cos.(PI+th) =(-1) * cos.(th) - 0 * sin.(th) by Th79,Th81
    .=-cos.(th);
end;

theorem
  sin(th+2 * PI) = sin(th) & cos(th+2 * PI) = cos(th) &
  sin(PI/2-th) = cos(th) & cos(PI/2-th) = sin(th) & sin(PI/2+th) = cos(th) &
  cos(PI/2+th) = -sin(th) & sin(PI+th) = -sin(th) &
  cos(PI+th) = -cos(th) by Th83;

Lm18: th in [.0,1 .] implies sin.th>=0
proof
  assume
A1: th in [.0,1 .];
then A2: th<=1 by XXREAL_1:1;
 sin.th>= sin.0
  proof
 now per cases by A1,XXREAL_1:1;
      suppose
     th=0;
        hence thesis;
      end;
      suppose
A6:     0<th;
     sin|REAL is continuous by Th73,FDIFF_1:33;
then      sin|[.0,th.] is continuous by FCONT_1:17;
        then consider r be Real such that
A9:     r in ].0,th.[ and
A10:    diff(sin,r)=(sin.th-sin.0)/(th-0)
        by A6,Th27,Th73,FDIFF_1:34,ROLLE:3;
A11:    r < th by A9,XXREAL_1:4;
A12:    0<r by A9,XXREAL_1:4;
    r <1 by A2,A11,XXREAL_0:2;
then     r in [.0,1 .] by A12,XXREAL_1:1;
then     cos.r >0 by Th74;
then     sin.th-sin.0 >0 by A6,A10,Th73;
        hence thesis by XREAL_1:49;
      end;
    end;
    hence thesis;
  end;
  hence thesis by Th33;
end;

theorem Th85:
  th in ].0,PI/2.[ implies cos.th > 0
proof
  assume that
A1: th in ].0,PI/2.[ and
A2: cos.th <= 0;
 cos|REAL is continuous by Th72,FDIFF_1:33;
then A4: cos|[.0,th.] is continuous by FCONT_1:17;
A5: 0 < th by A1,XXREAL_1:4;
A6: th is Real by XREAL_0:def 1;
   [.
cos.0,cos.th .] \/ [.cos.th,cos.0 .] = [.cos.th,cos.0 .] & 0 in [.cos.th,
  cos.0 .] by A2,Th33,XXREAL_1:1,222;
then  ex th2 be Real st th2 in [.0,th .] & cos.th2 = 0
  by A4,A5,A6,Th27,FCONT_2:16;
  then consider th2 be Real such that
A9: th2 in [.0,th .] and 0 < th and
A10: cos.th2 = 0 by A5;
A11: 0 <= th2 by A9,XXREAL_1:1;
A12: th2 <= th by A9,XXREAL_1:1;
A13: th < PI/2 by A1,XXREAL_1:4;
A14: 0<th2 by A10,A11,Th33;
