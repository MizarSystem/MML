Lm8: for th,th1,th2,th3 be real number holds
th |^ (2*n)=(th |^ n)|^ 2 & (-1)|^(2*n)=1 & (-1)|^ (2*n+1)=-1
proof
  let th,th1,th2,th3 be real number;
  thus th |^ (2*n)=(th |^ n)|^ 2 by NEWTON:14;
  thus
A1: (-1)|^(2*n)=(1)|^(2*n) by POWER:1
    .=1 by NEWTON:15;
  thus (-1)|^ (2*n+1)=(-1)|^ (2*n)*(-1) by NEWTON:11
    .=-1 by A1;
end;

theorem Th42:
  n <= k implies n! <= k!
proof
  assume
A1: n <= k;
A2: for m holds 1<= m+1
  proof
    let m;
 0+1<= m+1 by XREAL_1:8;
    hence thesis;
  end;
  deffunc U(Element of NAT) = $1!;
  consider rseq such that
A4: for l holds rseq.l=U(l) from SEQ_1:sch 1;
 for l holds rseq.l<=rseq.(l+1)& rseq.l>0
  proof
    let l;
    defpred X[Element of NAT] means rseq.$1<=rseq.($1+1)& rseq.$1>0;
 rseq.(0+1)=(0+1)! by A4
      .=0!*1 by NEWTON:21
      .=1 by NEWTON:18;
then A7: X[0] by A4,NEWTON:18;
A8: for l st X[l] holds X[l+1]
    proof
      let l;
      assume
A9:   rseq.l<=rseq.(l+1) & rseq.l>0;
  rseq.(l+1+1)= (l+1+1)! by A4
        .=(l+1)!*(l+1+1) by NEWTON:21
        .=rseq.(l+1) *(l+1+1) by A4;
      hence thesis by A2,A9,XREAL_1:153;
    end;
 for l holds X[l] from NAT_1:sch 1(A7,A8);
    hence thesis;
  end;
then A12: rseq is non-decreasing by SEQM_3:def 13;
 n!=rseq.n & k! = rseq.k by A4;
  hence thesis by A1,A12,SEQM_3:12;
end;

theorem Th43:
  0<=th & th <=1 & n<=k implies th |^ k <= th |^ n
proof
  assume that
A1: 0<=th and
A2: th <=1 and
A3: n<=k;
 for m holds th GeoSeq.(m+1)<=th GeoSeq.m& th GeoSeq.m >=0
  proof
    let m;
    defpred X[Element of NAT] means
    th GeoSeq.($1+1)<=th GeoSeq.$1 &th GeoSeq.$1 >=0;
 th GeoSeq.(0+1)=th GeoSeq.0 * th by PREPOWER:4
      .= 1*th by PREPOWER:4
      .=th;
then A6: X[0] by A2,PREPOWER:4;
A7: for m st X[m] holds X[m+1]
    proof
      let m;
      assume that
      th GeoSeq.(m+1)<=th GeoSeq.m and
A8:   th GeoSeq.m >=0;
         th
 GeoSeq.(m+1+1)=th GeoSeq.(m+1)*th & th GeoSeq.(m+1)=th GeoSeq.(m)*th by
      PREPOWER:4;
      hence thesis by A1,A2,A8,XREAL_1:155;
    end;
 for m holds X[m] from NAT_1:sch 1(A6,A7);
    hence thesis;
  end;
then A11: th GeoSeq is non-increasing by SEQM_3:def 14;
   th |^ k=th GeoSeq. k & th |^ n=th GeoSeq. n by PREPOWER:def 1;
  hence thesis by A3,A11,SEQM_3:14;
end;

