             +0c by A13,COMSEQ_1:def 7
        .= Partial_Sums(Sift((z (#) Expan(n,z,w)))).(n+1) by COMPLEX1:22;
         end;
    A14:(w (#) Expan(n,z,w))+Sift((z (#) Expan(n,z,w)))
