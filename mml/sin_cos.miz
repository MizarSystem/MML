:: Some definitions and properties of complex sequence

definition
  let m,k be Element of NAT;
  canceled;
  func CHK(m,k) -> Element of COMPLEX equals
  :Def2:
  1 if m <= k otherwise 0;
  correctness
  proof
 0c = 0;
    hence thesis by COMPLEX1:def 7;
  end;
end;

registration
  let m,k be Element of NAT;
  cluster CHK(m,k) -> real;
  coherence by Def2;
end;

scheme ExComplexCASE{F(Element of NAT,Element of NAT)->Element of COMPLEX}:
  for k holds ex seq st for n holds (n <= k implies seq.n=F(k,n)) &
  (n > k implies seq.n=0)
proof
  let k;
  defpred P[set,set] means ex n st (n=$1 & (n <= k implies $2=F(k,n)) &
  (n > k implies $2=0c) );
A1: now
    let x be set;
    assume x in NAT;
    then consider n such that
A3: n=x;
A4: now
      assume n <= k;
      hence CHK(n,k) * F (k,n) =1 * F(k,n) by Def2
        .=F(k,n);
    end;
A6: now
      assume n > k;
      hence CHK(n,k) * F (k,n) =0 * F(k,n) by Def2
        .=0;
    end;
    reconsider y=CHK(n,k) * F (k,n) as set;
    take y;
    thus P[x,y] by A3,A4,A6;
  end;
  consider f be Function such that
A8: dom f = NAT and
A9: for x be set st x in NAT holds P[x,f.x] from CLASSES1:sch 1(A1);
 now
    let x be set;
    assume x in NAT;
then  ex n st n=x & (n <= k implies f.x=F(k,n)) &
    (n > k implies f.x=0c) by A9;
    hence f.x is Element of COMPLEX;
  end;
  then reconsider f as Complex_Sequence by A8,COMSEQ_1:1;
  take seq=f;
  let n;
 ex l be Element of NAT st l=n &
  (l <= k implies seq.n=F(k,l)) & (l > k implies seq.n=0c) by A9;
  hence thesis;
end;

scheme ExRealCASE{F(Element of NAT,Element of NAT)->real number}: for k holds
  ex rseq st for n holds (n <= k implies rseq.n=F(k,n)) &
  (n > k implies rseq.n=0)
proof
  let k;
  defpred P[set,set] means ex n st (n=$1 & (n <= k implies $2=F(k,n )) &
  (n > k implies $2=0) );
A1: now
    let x be set;
    assume x in NAT;
    then consider n such that
A3: n=x;
A4: now
      assume n <= k;
      hence CHK(n,k) * F (k,n) =1 * F(k,n) by Def2
        .=F(k,n);
    end;
A6: now
      assume n > k;
      hence CHK(n,k) * F (k,n) =0 * F(k,n) by Def2
        .=0;
    end;
    reconsider y=CHK(n,k) * F (k,n) as set;
    take y;
    thus P[x,y] by A3,A4,A6;
  end;
  consider f be Function such that
A8: dom f = NAT and
A9: for x be set st x in NAT holds P[x,f.x] from CLASSES1:sch 1(A1);
 now
    let x be set;
    assume x in NAT;
then  ex n st n=x & (n <= k implies f.x=F(k,n)) &
    (n > k implies f.x=0) by A9;
    hence f.x is real;
  end;
  then reconsider f as Real_Sequence by A8,SEQ_1:3;
  take rseq=f;
  let n;
 ex l be Element of NAT st l=n &
  (l <= k implies rseq.n=F(k,l)) & (l > k implies rseq.n=0) by A9;
  hence thesis;
end;

definition
  let x be Element of COMPLEX, c be complex number;
  redefine func x*c -> Element of COMPLEX;
  coherence by XCMPLX_0:def 2;
end;

definition
  canceled;
  func Prod_complex_n -> Complex_Sequence means
  :Def4:
  it.0 = 1 & for n holds it.(n+1) = it.n * (n+1);
  existence
  proof
    deffunc U(Nat,Element of COMPLEX) = $2 * ($1+1);
    consider f being Function of NAT,COMPLEX such that
A1: f.0 = 1r & for n being Nat holds f.(n+1) = U(n,f.n) from NAT_1:sch 12;
    take f;
    thus f.0 = 1 by A1;
    let n be Element of NAT;
    thus thesis by A1;
  end;
  uniqueness
  proof
    let seq1,seq2;
    assume that
A2: seq1.0=1 and
A3: for n holds seq1.(n+1)=seq1.n * (n+1) and
A4: seq2.0=1 and
A5: for n holds seq2.(n+1)=seq2.n * (n+1);
    defpred X[Element of NAT] means seq1.$1 = seq2.$1;
A6: X[0] by A2,A4;
A7: for k be Element of NAT st X[k] holds X[k+1]
    proof
      let k be Element of NAT;
      assume seq1.k =seq2.k;
      hence seq1.(k+1) = seq2.k * (k+1) by A3
        .= seq2.(k+1) by A5;
    end;
 for n holds X[n] from NAT_1:sch 1(A6,A7);
    hence seq1 = seq2 by FUNCT_2:113;
  end;
end;

definition
  func Prod_real_n -> Real_Sequence means
  :Def5:
