                1 <= k by A16,A18,AXIOMS:22;
            hence thesis by A16,A18,A19,A20,AXIOMS:22,SCMFSA_7:3,XCMPLX_1:27;
       end;
          ((w (#) Expan(n,z,w))+Sift((z (#) Expan(n,z,w)))).k
