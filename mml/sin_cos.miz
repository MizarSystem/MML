       proof
          let q be Real such that
              A15: q>0;
              ex k st for m st k<=m holds |.cq1.m-0c.|<q
            proof
              consider r such that
              A16: r>0 & for z st |.z.|<r holds |.(Sum
(z P_dt)).|<q by A15,Th63;
