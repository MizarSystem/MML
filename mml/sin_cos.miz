definition
  let n be Element of NAT;
  func n !c -> Element of COMPLEX equals
  Prod_complex_n.n;
  coherence;
end;

definition
  let n be Element of NAT;
  redefine func n! equals
  Prod_real_n.n;
  compatibility
  proof
    defpred X[Element of NAT] means Prod_real_n.$1 = $1!;
A1: X[0] by Def5,NEWTON:18;
A2: now
      let l be Element of NAT;
      assume
A3:   X[l];
   Prod_real_n.(l+1) = Prod_real_n.l * (l+1) by Def5
        .= (l+1)! by A3,NEWTON:21;
      hence X[l+1];
    end;
 for k be Element of NAT holds X[k] from NAT_1:sch 1(A1,A2);
    hence thesis;
  end;
end;

definition
  let z be complex number;
  deffunc U(Element of NAT) = z #N $1 /($1!c);
  func z ExpSeq -> Complex_Sequence means
  :Def8:
  for n holds it.n = z #N n / (n!c);
  existence
  proof
    thus ex s being Complex_Sequence st
    for n holds s.n = U(n) from COMSEQ_1:sch 1;
  end;
  uniqueness
  proof
    thus for s1,s2 being Complex_Sequence st
    (for x being Element of NAT holds s1.x = U(x)) &
    (for x being Element of NAT holds s2.x = U(x)) holds s1 = s2
    from BINOP_2:sch 1;
  end;
end;

definition
  let a be real number;
  deffunc U(Element of NAT) = a |^ $1 /($1!);
  func a rExpSeq -> Real_Sequence means
  :Def9:
  for n holds it.n = a |^ n /(n!);
  existence
  proof
    thus
    ex s being Real_Sequence st for n holds s.n = U(n) from SEQ_1:sch 1;
  end;
  uniqueness
  proof
    thus for s1,s2 being Real_Sequence st
    (for x being Element of NAT holds s1.x = U(x)) &
    (for x being Element of NAT holds s2.x = U(x)) holds s1 = s2
    from BINOP_2:sch 1;
  end;
end;

theorem Th1:
  0!c = 1 & n!c <> 0 & (n+1)!c = n!c * (n+1)
proof
  defpred X[Element of NAT] means $1!c <> 0;
  thus 0!c = 1 by Def4;
A1: X[0] by Def4;
A2: now
    let n;
    assume
A3: X[n];
 (n+1)!c = n!c * (n+1+0*<i>) by Def4;
    hence X[n+1] by A3;
  end;
 for n holds X[n] from NAT_1:sch 1(A1,A2);
  hence n!c <> 0;
  thus thesis by Def4;
end;

canceled;

theorem Th3:
  (0 < k implies ((k-'1)!c ) * k = k!c) &
  (k <= m implies ((m-'k)!c) * (m+1-k) = (m+1-'k)!c)
proof
A1: now
    let k;
    assume 0 < k;
then  0+1 <= k by INT_1:20;
then  k-'1+1=k-1+1 by XREAL_1:235
      .=k;
    hence k!c =(k-'1)!c * k by Th1;
  end;
 now
    let m,k such that
A6: k <= m;
 m <= m+1 by XREAL_1:31;
then  m+1-'k=m+1-k by A6,XREAL_1:235,XXREAL_0:2
      .=m-k+1
      .=m-'k+1 by A6,XREAL_1:235;
    hence (m+1-'k)!c=((m-'k)!c ) * (m-'k+1+0*<i>) by Th1
      .=((m-'k)!c ) * (m-k+1+0*<i>) by A6,XREAL_1:235
      .=((m-'k)!c ) * (m+1-k);
  end;
  hence thesis by A1;
end;

definition
  let n be Element of NAT;
  func Coef(n) -> Complex_Sequence means
  :Def10:
  for k be Element of NAT holds
  (k <= n implies it.k = n!c /( (k!c ) * ((n-'k)!c ))) &
  (k > n implies it.k=0);
  existence
  proof
    deffunc U(Element of NAT,Element of NAT) = $1!c /( ($2!c ) * (($1-'$2)!c));
 for n holds ex seq st for k holds (k <= n implies seq.k = U(n,k)) &
    (k > n implies seq.k=0 ) from ExComplexCASE;
    hence thesis;
  end;
  uniqueness
  proof
    let seq1,seq2;
    assume that
A2: for k holds (k <= n implies seq1.k = n!c /( (k!c ) * ((n-'k)!c))) &
    (k > n implies seq1.k=0) and
A3: for k holds (k <= n implies seq2.k = n!c /( (k!c ) * ((n-'k)!c))) &
    (k > n implies seq2.k=0);
 now
      let k;
      per cases;
      suppose
A5:     k <= n;
        hence seq1.k = n!c /( (k!c ) * ((n-'k)!c)) by A2
          .= seq2.k by A3,A5;
      end;
      suppose
A6:     k > n;
        hence seq1.k = 0c by A2
          .= seq2.k by A3,A6;
      end;
    end;
    hence seq1=seq2 by FUNCT_2:113;
  end;
end;

definition
  let n be Element of NAT;
  func Coef_e(n) -> Complex_Sequence means
  :Def11:
  for k be Element of NAT holds
  (k <= n implies it.k = 1r/((k!c ) * ((n-'k)!c ))) & (k > n implies it.k=0);
  existence
  proof
    deffunc U(Element of NAT,Element of NAT) = 1r/(($2!c ) * (($1-'$2)!c ));
 for n holds ex seq st for k holds (k <= n implies seq.k = U(n,k)) &
    (k > n implies seq.k=0) from ExComplexCASE;
    hence thesis;
  end;
  uniqueness
  proof
    let seq1,seq2;
    assume that
A2: for k holds (k <= n implies seq1.k = 1r/((k!c ) * ((n-'k)!c ))) &
    (k > n implies seq1.k=0) and
A3: for k holds (k <= n implies seq2.k = 1r/((k!c ) * ((n-'k)!c ))) &
    (k > n implies seq2.k=0);
 now
      let k;
      per cases;
      suppose
A5:     k <= n;
        hence seq1.k =1r/((k!c ) * ((n-'k)!c )) by A2
          .= seq2.k by A3,A5;
      end;
      suppose
A6:     k > n;
        hence seq1.k = 0c by A2
          .= seq2.k by A3,A6;
      end;
    end;
    hence seq1=seq2 by FUNCT_2:113;
  end;
end;

definition
  let seq;
  func Shift seq -> Complex_Sequence means
  :Def12:
  it.0=0 & for k be Element of NAT holds it.(k+1) = seq.k;
  existence
  proof
    deffunc U(Nat,Element of COMPLEX) = seq.$1;
    consider f being Function of NAT,COMPLEX such that
A1: f.0 = 0c & for n being Nat holds f.(n+1)=U(n,f.n) from NAT_1:sch 12;
    take f;
    thus thesis by A1;
  end;
  uniqueness
  proof
    let seq1,seq2;
    assume that
A2: seq1.0=0 and
A3: for n holds seq1.(n+1)=seq.n and
A4: seq2.0=0 and
A5: for n holds seq2.(n+1)=seq.n;
    defpred X[Element of NAT] means seq1.$1 = seq2.$1;
A6: X[0] by A2,A4;
A7: for k be Element of NAT st X[k] holds X[k+1]
    proof
      let k be Element of NAT;
      assume seq1.k =seq2.k;
      thus seq1.(k+1) = seq.k by A3
        .= seq2.(k+1) by A5;
    end;
 for n holds X[n] from NAT_1:sch 1(A6,A7);
    hence seq1 = seq2 by FUNCT_2:113;
  end;
end;

definition
  let n;
  let z,w be Element of COMPLEX;
  func Expan(n,z,w) -> Complex_Sequence means
  :Def13:
  for k be Element of NAT holds ( k <= n implies
  it.k=((Coef(n)).k) * (z |^ k) * (w |^ (n-'k)) ) & (n < k implies it.k=0);
  existence
  proof
    deffunc U(Element of NAT,Element of NAT)
    = ((Coef($1)).$2) * (z #N $2) * (w #N ($1-'$2));
 for n holds ex seq st for k holds (k <= n implies seq.k = U(n,k)) &
    (k > n implies seq.k=0) from ExComplexCASE;
    hence thesis;
  end;
  uniqueness
  proof
    let seq1,seq2;
    assume that
A2: for k holds
    (k <= n implies seq1.k =((Coef(n)).k) * (z |^ k) * (w |^ (n-'k)))&
    (k > n implies seq1.k=0) and
A3: for k holds
    (k <= n implies seq2.k = ((Coef(n)).k) * (z |^ k) * (w |^ (n-'k)) )&
    (k > n implies seq2.k=0);
 now
      let k;
      per cases;
      suppose
A5:     k <= n;
        hence seq1.k =((Coef(n)).k) * (z |^ k) * (w |^ (n-'k)) by A2
          .= seq2.k by A3,A5;
      end;
      suppose
A6:     k > n;
        hence seq1.k = 0c by A2
          .= seq2.k by A3,A6;
      end;
    end;
    hence seq1=seq2 by FUNCT_2:113;
  end;
end;

