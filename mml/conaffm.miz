        reconsider e1=e1' as Element of X by ANALMETR:47;
        LIN o,p,e1 by A158,ANALMETR:55;
        then o,p // o,e1 by ANALMETR:def 11;
        then o,e1 // o,a1 by A153,A154,ANALMETR:82;
        then
A160:   LIN o,e1,a1 by ANALMETR:def 11;
        LIN e,p1,e1 by A159,ANALMETR:55;
        then e,p1 // e,e1 by ANALMETR:def 11;
        then e,e1 // a,a1 by A155,A156,ANALMETR:82;
        hence thesis by A160;
      end;
      then consider e1 be Element of X such that
A161: LIN o,e1,a1 and
A162: e,e1 // a,a1;
      reconsider e1'=e1 as Element of Af(X) by ANALMETR:47;
      o,e // o,a by A149,ANALMETR:def 11;
      then o',e' // o',a' by ANALMETR:48;
      then o',a' // o',e' by AFF_1:13;
      then o,a // o,e by ANALMETR:48;
      then
A163: o,a1 _|_ o,e by A93,A134,ANALMETR:84;
      o,e1 // o,a1 by A161,ANALMETR:def 11;
      then o',e1' // o',a1' by ANALMETR:48;
      then o',a1' // o',e1' by AFF_1:13;
      then
A164: o,a1 // o,e1 by ANALMETR:48;
A165: o<>e
      proof
        assume o=e;
        then LIN b',c',o' by A148,ANALMETR:55;
        then LIN o',b',c' by AFF_1:15;
        hence contradiction by A126,ANALMETR:55;
      end;
A166: o<>e1
      proof
        assume o=e1;
        then e',o' // a',a1' by A162,ANALMETR:48;
        then o',e' // a',a1' by AFF_1:13;
        then
A167:   o,e // a,a1 by ANALMETR:48;
        o,e // o,a by A149,ANALMETR:def 11;
        then
A168:   o,a // a,a1 by A165,A167,ANALMETR:82;
        then
A169:   o,a1 _|_ a,a1 by A93,A134,ANALMETR:84;
        o',a' // a',a1' by A168,ANALMETR:48;
        then a',o' // a',a1' by AFF_1:13;
        then LIN a',o',a1' by AFF_1:def 1;
        then LIN a1',o',a' by AFF_1:15;
        then a1',o' // a1',a' by AFF_1:def 1;
        then o',a1' // a',a1' by AFF_1:13;
        then o,a1 // a,a1 by ANALMETR:48;
        then a,a1 _|_ a,a1 by A127,A169,ANALMETR:84;
        hence contradiction by A136,ANALMETR:51;
      end;
A170: o,e _|_ o,e1 by A127,A163,A164,ANALMETR:84;
A171: not LIN o,b,a
      proof
        assume LIN o,b,a;
        then o,b // o,a by ANALMETR:def 11;
        then o',b' // o',a' by ANALMETR:48;
        then o',a' // o',b' by AFF_1:13;
        hence contradiction by A99,ANALMETR:48;
      end;
      o,e // o,a by A149,ANALMETR:def 11;
      then o',e' // o',a' by ANALMETR:48;
      then o',a' // o',e' by AFF_1:13;
      then o,a // o,e by ANALMETR:48;
      then
A172: LIN o,a,e by ANALMETR:def 11;
      o,e1 // o,a1 by A161,ANALMETR:def 11;
      then o',e1' // o',a1' by ANALMETR:48;
      then o',a1' // o',e1' by AFF_1:13;
      then o,a1 // o,e1 by ANALMETR:48;
      then
A173: LIN o,a1,e1 by ANALMETR:def 11;
      e',e1' // a',a1' by A162,ANALMETR:48;
      then a',a1' // e',e1' by AFF_1:13;
      then
A174: a,a1 // e,e1 by ANALMETR:48;
      then
A175: e,b _|_ e1,b1 by A9,A93,A94,A96,A127,A130,A134,A135,A152,A165
      ,A166,A170,A171,A172,A173,Def6;
      not LIN o,c,a by A98,ANALMETR:def 11;
      then
A176: e,c _|_ e1,c1 by A9,A93,A95,A97,A127,A129,A132,A134,A152,A165
      ,A166,A170,A172,A173,A174,Def6;
A177: e1<>b1
      proof
        assume e1=b1;
        then o,b1 // o,a1 by A161,ANALMETR:def 11;
        then o,a1 _|_ o,b by A94,A130,ANALMETR:84;
        hence contradiction by A93,A99,A127,ANALMETR:85;
      end;
A178: c,e _|_ c1,e1 by A176,ANALMETR:83;
A179: LIN b',c',e' by A148,ANALMETR:55;
      then LIN c',e',b' by AFF_1:15;
      then LIN c,e,b by ANALMETR:55;
      then c,e // c,b by ANALMETR:def 11;
      then
A180: c,b _|_ c1,e1 by A151,A178,ANALMETR:84;
A181: c <>b
      proof
        assume c = b;
        then LIN o',b',c' by AFF_1:16;
        hence contradiction by A126,ANALMETR:55;
      end;
      b',c' // b',e' by A179,AFF_1:def 1;
      then c',b' // e',b' by AFF_1:13;
      then c,b // e,b by ANALMETR:48;
      then e,b _|_ c1,e1 by A180,A181,ANALMETR:84;
      then e1,b1 // c1,e1 by A150,A175,ANALMETR:85;
      then e1',b1' // c1',e1' by ANALMETR:48;
      then e1',b1' // e1',c1' by AFF_1:13;
      then LIN e1',b1',c1' by AFF_1:def 1;
      then LIN b1',e1',c1' by AFF_1:15;
      then b1',e1' // b1',c1' by AFF_1:def 1;
      then e1',b1' // b1',c1' by AFF_1:13;
      then
A182: e1,b1 // b1,c1 by ANALMETR:48;
      LIN b',e',c' by A179,AFF_1:15;
      then b',e' // b',c' by AFF_1:def 1;
      then e',b' // b',c' by AFF_1:13;
      then e,b // b,c by ANALMETR:48;
      then e1,b1 _|_ b,c by A150,A175,ANALMETR:84;
      hence b,c _|_ b1,c1 by A177,A182,ANALMETR:84;
    end;
    hence b,c _|_ b1,c1 by A101,A108,A117;
  end;
  then
A183: not o,a // c,b implies b,c _|_ b1,c1 by A2,A3,A4,A5,A6,A7,A8;
  now
    let o,a,a1,b,b1,c,c1;
    assume X is satisfying_LIN;
    assume that
A184: o,a _|_ o,a1 and
A185: o,b _|_ o,b1 and
A186: o,c _|_ o,c1 and
A187: a,b _|_ a1,b1 and
A188: a,c _|_ a1,c1 and
A189: not o,c // o,a and
A190: not o,a // o,b;
    assume that
A191: o,a // c,b
    and o,b // a,c;
    reconsider a'=a,b'=b,c'=c,o'=o as Element of Af(X) by ANALMETR:47;
A192: now
      assume
A193: o=a1;
      then
A194: a1,b1 _|_ b,a1 by A185,ANALMETR:83;
A195: a1,b1 _|_ b,a by A187,ANALMETR:83;
      not b,a1 // b,a
      proof
        assume b,a1 // b,a;
        then LIN b,o,a by A193,ANALMETR:def 11;
        then LIN b',o',a' by ANALMETR:55;
        then LIN o',a',b' by AFF_1:15;
        then LIN o,a,b by ANALMETR:55;
        hence contradiction by A190,ANALMETR:def 11;
      end;
      then
A196: a1=b1 by A194,A195,ANALMETR:85;
A197: a1,c1 _|_ c,a1 by A186,A193,ANALMETR:83;
      a1,c1 _|_ c,a by A188,ANALMETR:83;
      then
A198: c,a1 // c,a or a1=c1 by A197,ANALMETR:85;
      not c,a1 // c,a
      proof
        assume c,a1 // c,a;
        then LIN c,o,a by A193,ANALMETR:def 11;
        then LIN c',o',a' by ANALMETR:55;
        then LIN o',c',a' by AFF_1:15;
        then LIN o,c,a by ANALMETR:55;
        hence contradiction by A189,ANALMETR:def 11;
      end;
      hence b,c _|_ b1,c1 by A196,A198,ANALMETR:51;
    end;
A199: now
      assume that
A200: o,a1 // c1,b1 and
A201: o<>a1;
      o<>a
      proof
        assume o=a;
        then LIN o',c',a' by AFF_1:16;
        then LIN o,c,a by ANALMETR:55;
        hence contradiction by A189,ANALMETR:def 11;
      end;
      then c,b _|_ o,a1 by A184,A191,ANALMETR:84;
      then c,b _|_ c1,b1 by A200,A201,ANALMETR:84;
      hence b,c _|_ b1,c1 by ANALMETR:83;
    end;
    now
      assume that
A202: not o,a1 // c1,b1 and
A203: o<>a1;
A204: o,a1 _|_ o,a by A184,ANALMETR:83;
A205: o,b1 _|_ o,b by A185,ANALMETR:83;
A206: o,c1 _|_ o,c by A186,ANALMETR:83;
A207: a1,b1 _|_ a,b by A187,ANALMETR:83;
A208: a1,c1 _|_ a,c by A188,ANALMETR:83;
A209: not o,c1 // o,a1
      proof
        assume
A210:   o,c1 // o,a1;
        o<>c1
        proof
          assume o=c1;
          then a,c _|_ o,a1 by A188,ANALMETR:83;
          then a,c // o,a by A184,A203,ANALMETR:85;
          then a,c // a,o by ANALMETR:81;
          then LIN a,c,o by ANALMETR:def 11;
          then LIN a',c',o' by ANALMETR:55;
          then LIN o',c',a' by AFF_1:15;
          then LIN o,c,a by ANALMETR:55;
          hence contradiction by A189,ANALMETR:def 11;
        end;
        then o,c _|_ o,a1 by A186,A210,ANALMETR:84;
        hence contradiction by A184,A189,A203,ANALMETR:85;
      end;
      not o,a1 // o,b1
      proof
        assume
A211:   o,a1 // o,b1;
A212:   o<>b1
        proof
          assume o=b1;
          then a,b _|_ o,a1 by A187,ANALMETR:83;
          then a,b // o,a by A184,A203,ANALMETR:85;
          then a,b // a,o by ANALMETR:81;
          then LIN a,b,o by ANALMETR:def 11;
          then LIN a',b',o' by ANALMETR:55;
          then LIN o',a',b' by AFF_1:15;
          then LIN o,a,b by ANALMETR:55;
          hence contradiction by A190,ANALMETR:def 11;
        end;
        o,a _|_ o,b1 by A184,A203,A211,ANALMETR:84;
        hence contradiction by A185,A190,A212,ANALMETR:85;
      end;
      then b1,c1 _|_ b,c by A92,A202,A204,A205,A206,A207,A208,A209;
      hence b,c _|_ b1,c1 by ANALMETR:83;
    end;
    hence b,c _|_ b1,c1 by A192,A199;
  end;
  hence thesis by A1,A2,A3,A4,A5,A6,A7,A8,A91,A183;
end;

theorem
  X is satisfying_LIN implies X is satisfying_3H
proof
  assume
A1: X is satisfying_LIN;
  let a,b,c;
  assume
A2: not LIN a,b,c;
  consider e1 such that
A3: b,c _|_ a,e1 and
A4: a<>e1 by ANALMETR:def 10;
  consider e2 such that
A5: a,c _|_ b,e2 and
A6: b<>e2 by ANALMETR:def 10;
  reconsider a'=a,b'=b,c' = c,e1'=e1,e2'=e2
  as Element of Af(X) by ANALMETR:47;
  not a',e1' // b',e2'
  proof
    assume a',e1' // b',e2';
    then a,e1 // b,e2 by ANALMETR:48;
    then b,e2 _|_ b,c by A3,A4,ANALMETR:84;
    then a,c // b,c by A5,A6,ANALMETR:85;
    then a',c' // b',c' by ANALMETR:48;
    then c',a' // c',b' by AFF_1:13;
    then LIN c',a',b' by AFF_1:def 1;
    then LIN a',b',c' by AFF_1:15;
    hence contradiction by A2,ANALMETR:55;
  end;
  then consider d' be Element of Af(X) such that
A7: LIN a',e1',d' and
A8: LIN b',e2',d' by AFF_1:74;
  reconsider d=d' as Element of X by ANALMETR:47;
  take d;
  LIN b,e2,d by A8,ANALMETR:55;
  then
A9: b,e2 // b,d by ANALMETR:def 11;
  then
A10: a,c _|_ b,d by A5,A6,ANALMETR:84;
  then
A11: d,b _|_ a,c by ANALMETR:83;
  LIN a,e1,d by A7,ANALMETR:55;
  then
A12: a,e1 // a,d by ANALMETR:def 11;
  then
A13: b,c _|_ a,d by A3,A4,ANALMETR:84;
  then
A14: d,a _|_ b,c by ANALMETR:83;
A15: X is satisfying_LIN1 by A1,Th3;
A16: now
    assume
A17: d<>c;
    now
      assume
A18:  b<>d;
      not b',d' // a',c'
      proof
        assume b',d' // a',c';
        then a',c' // b',d' by AFF_1:13;
        then
A19:    a,c // b,d by ANALMETR:48;
        a<>c
        proof
          assume a= c;
          then LIN a',b',c' by AFF_1:16;
          hence contradiction by A2,ANALMETR:55;
        end;
        then b,d _|_ b,d by A10,A19,ANALMETR:84;
        hence contradiction by A18,ANALMETR:51;
      end;
      then consider o' be Element of Af(X) such that
A20:  LIN b',d',o' and
A21:  LIN a',c',o' by AFF_1:74;
      reconsider o=o' as Element of X by ANALMETR:47;
      now
        assume
A22:    d<>a;
A23:    o<>a
        proof
          assume
A24:      o = a;
          then LIN b,d,a by A20,ANALMETR:55;
          then b,d // b,a by ANALMETR:def 11;
          then b,a _|_ a,c by A10,A18,ANALMETR:84;
          then
A25:      a,b _|_ a,c by ANALMETR:83;
          LIN a',b',d' by A20,A24,AFF_1:15;
          then LIN a,b,d by ANALMETR:55;
          then
A26:      a,b // a,d by ANALMETR:def 11;
          a<>b
          proof
            assume a=b;
            then LIN a',b',c' by AFF_1:16;
            hence contradiction by A2,ANALMETR:55;
          end;
          then a,d _|_ a,c by A25,A26,ANALMETR:84;
          then d,a _|_ a,c by ANALMETR:83;
          then a,c // b,c by A14,A22,ANALMETR:85;
          then c,a // c,b by ANALMETR:81;
          then LIN c,a,b by ANALMETR:def 11;
          then LIN c',a',b' by ANALMETR:55;
          then LIN a',b',c' by AFF_1:15;
          hence contradiction by A2,ANALMETR:55;
        end;
A27:    c <>o
        proof
          assume
A28:      c = o;
          then LIN b,d,c by A20,ANALMETR:55;
          then b,d // b,c by ANALMETR:def 11;
          then
A29:      b,c _|_ a,c by A10,A18,ANALMETR:84;
          b<>c
          proof
            assume b = c;
            then LIN a',b',c' by AFF_1:16;
            hence contradiction by A2,ANALMETR:55;
          end;
          then a,d // a,c by A13,A29,ANALMETR:85;
          then LIN a,d,c by ANALMETR:def 11;
          then LIN a',d',c' by ANALMETR:55;
          then LIN c',d',a' by AFF_1:15;
          then LIN c,d,a by ANALMETR:55;
          then
A30:      c,d // c,a by ANALMETR:def 11;
          LIN c',d',b' by A20,A28,AFF_1:15;
          then LIN c,d,b by ANALMETR:55;
          then c,d // c,b by ANALMETR:def 11;
          then c,a // c,b by A17,A30,ANALMETR:82;
          then LIN c,a,b by ANALMETR:def 11;
          then LIN c',a',b' by ANALMETR:55;
          then LIN a',b',c' by AFF_1:15;
          hence contradiction by A2,ANALMETR:55;
        end;
        consider e1 such that
A31:    a,c // a,e1 and
A32:    a<>e1 by ANALMETR:51;
        consider e2 such that
A33:    b,c // d,e2 and
A34:    d<>e2 by ANALMETR:51;
        reconsider e1'=e1,e2'=e2 as Element of Af X by ANALMETR:47;
        not a',e1' // d',e2'
        proof
          assume a',e1' // d',e2';
          then a,e1 // d,e2 by ANALMETR:48;
          then d,e2 // a,c by A31,A32,ANALMETR:82;
          then a,c // b,c by A33,A34,ANALMETR:82;
          then c,a // c,b by ANALMETR:81;
          then LIN c,a,b by ANALMETR:def 11;
          then LIN c',a',b' by ANALMETR:55;
          then LIN a',b',c' by AFF_1:15;
          hence contradiction by A2,ANALMETR:55;
        end;
        then consider d1' such that
A35:    LIN a',e1',d1' and
A36:    LIN d',e2',d1' by AFF_1:74;
        reconsider d1=d1' as Element of X by ANALMETR:47;
        LIN a,e1,d1 by A35,ANALMETR:55;
        then a,e1 // a,d1 by ANALMETR:def 11;
        then
A37:    a,c // a,d1 by A31,A32,ANALMETR:82;
        then
A38:    LIN a,c,d1 by ANALMETR:def 11;
        LIN d,e2,d1 by A36,ANALMETR:55;
        then d,e2 // d,d1 by ANALMETR:def 11;
        then
A39:    b,c // d,d1 by A33,A34,ANALMETR:82;
A40:    o<>d
        proof
          assume
A41:      o = d;
          then
A42:      a,o _|_ b,c by A3,A4,A12,ANALMETR:84;
          LIN a,c,o by A21,ANALMETR:55;
          then a,c // a,o by ANALMETR:def 11;
          then
A43:      a,c _|_ b,c by A23,A42,ANALMETR:84;
          a<>c
          proof
            assume a= c;
            then LIN a',b',c' by AFF_1:16;
            hence contradiction by A2,ANALMETR:55;
          end;
          then b,o // b,c by A10,A41,A43,ANALMETR:85;
          then LIN b,o,c by ANALMETR:def 11;
          then LIN b',o',c' by ANALMETR:55;
          then LIN c',o',b' by AFF_1:15;
          then LIN c,o,b by ANALMETR:55;
          then
A44:      c,o // c,b by ANALMETR:def 11;
          LIN c',o',a' by A21,AFF_1:15;
          then LIN c,o,a by ANALMETR:55;
          then c,o // c,a by ANALMETR:def 11;
          then c,b // c,a by A27,A44,ANALMETR:82;
          then LIN c,b,a by ANALMETR:def 11;
          then LIN c',b',a' by ANALMETR:55;
          then LIN a',b',c' by AFF_1:15;
          hence contradiction by A2,ANALMETR:55;
        end;
A45:    o<>d1
        proof
          assume
A46:      o = d1;
          LIN o',d',b' by A20,AFF_1:15;
          then LIN o,d,b by ANALMETR:55;
          then o,d // o,b by ANALMETR:def 11;
          then d,o // b,o by ANALMETR:81;
          then b,c //b,o by A39,A40,A46,ANALMETR:82;
          then LIN b,c,o by ANALMETR:def 11;
          then LIN b',c',o' by ANALMETR:55;
          then LIN c',o',b' by AFF_1:15;
          then LIN c,o,b by ANALMETR:55;
          then
A47:      c,o // c,b by ANALMETR:def 11;
          LIN c',o',a' by A21,AFF_1:15;
          then LIN c,o,a by ANALMETR:55;
          then c,o // c,a by ANALMETR:def 11;
          then c,a // c,b by A27,A47,ANALMETR:82;
          then LIN c,a,b by ANALMETR:def 11;
          then LIN c',a',b' by ANALMETR:55;
          then LIN a',b',c' by AFF_1:15;
          hence contradiction by A2,ANALMETR:55;
        end;
A48:    o<>b
        proof
          assume o=b;
          then LIN a',b',c' by A21,AFF_1:15;
          hence contradiction by A2,ANALMETR:55;
        end;
A49:    d1<>c
        proof
          assume
A50:      d1 = c;
A51:      b<>c
          proof
            assume b = c;
            then LIN a',b',c' by AFF_1:16;
            hence contradiction by A2,ANALMETR:55;
          end;
          c,b // c,d by A39,A50,ANALMETR:81;
          then LIN c,b,d by ANALMETR:def 11;
          then
A52:      LIN c',b',d' by ANALMETR:55;
          b,c // c,d by A39,A50,ANALMETR:81;
          then d,a _|_ c,d by A14,A51,ANALMETR:84;
          then
A53:      d,c _|_ d,a by ANALMETR:83;
          LIN d',c',b' by A52,AFF_1:15;
          then LIN d,c,b by ANALMETR:55;
          then d,c // d,b by ANALMETR:def 11;
          then d,b _|_ d,a by A17,A53,ANALMETR:84;
          then a,c // d,a by A11,A18,ANALMETR:85;
          then a,c // a,d by ANALMETR:81;
          then LIN a,c,d by ANALMETR:def 11;
          then LIN a',c',d' by ANALMETR:55;
          then LIN c',a',d' by AFF_1:15;
          then LIN c,a,d by ANALMETR:55;
          then
A54:      c,a // c,d by ANALMETR:def 11;
          c,d // b,c by A39,A50,ANALMETR:81;
          then c,a // b,c by A17,A54,ANALMETR:82;
          then c,a // c,b by ANALMETR:81;
          then LIN c,a,b by ANALMETR:def 11;
          then LIN c',a',b' by ANALMETR:55;
          then LIN a',b',c' by AFF_1:15;
          hence contradiction by A2,ANALMETR:55;
        end;
        LIN d',o',b' by A20,AFF_1:15;
        then d',o' // d',b' by AFF_1:def 1;
        then b',d' // o',d' by AFF_1:13;
        then b,d // o,d by ANALMETR:48;
        then
A55:    a,c _|_ o,d by A10,A18,ANALMETR:84;
        LIN a,c,o by A21,ANALMETR:55;
        then a,c // a,o by ANALMETR:def 11;
        then
A56:    a,c // o,a by ANALMETR:81;
        a<>c
        proof
          assume a= c;
          then LIN a',b',c' by AFF_1:16;
          hence contradiction by A2,ANALMETR:55;
        end;
        then
A57:    o,d _|_ o,a by A55,A56,ANALMETR:84;
        LIN a,c,o by A21,ANALMETR:55;
        then
A58:    a,c // a,o by ANALMETR:def 11;
A59:    a<>c
        proof
          assume a= c;
          then LIN a',b',c' by AFF_1:16;
          hence contradiction by A2,ANALMETR:55;
        end;
        then a,o // a,d1 by A37,A58,ANALMETR:82;
        then LIN a,o,d1 by ANALMETR:def 11;
        then LIN a',o',d1' by ANALMETR:55;
        then LIN o',a',d1' by AFF_1:15;
        then LIN o,a,d1 by ANALMETR:55;
        then
A60:    o,a // o,d1 by ANALMETR:def 11;
        LIN a,c,o by A21,ANALMETR:55;
        then a,c // a,o by ANALMETR:def 11;
        then o,a // a,c by ANALMETR:81;
        then a,c // o,d1 by A23,A60,ANALMETR:82;
        then
A61:    b,d _|_ o,d1 by A10,A59,ANALMETR:84;
        LIN d',o',b' by A20,AFF_1:15;
        then LIN d,o,b by ANALMETR:55;
        then d,o // d,b by ANALMETR:def 11;
        then b,d // o,d by ANALMETR:81;
        then
A62:    o,d1 _|_ o,d by A18,A61,ANALMETR:84;
        LIN c',o',a' by A21,AFF_1:15;
        then c',o' // c',a' by AFF_1:def 1;
        then a',c' // o',c' by AFF_1:13;
        then
A63:    a,c // o,c by ANALMETR:48;
        a<>c
        proof
          assume a= c;
          then LIN a',b',c' by AFF_1:16;
          hence contradiction by A2,ANALMETR:55;
        end;
        then
A64:    b,d _|_ o,c by A10,A63,ANALMETR:84;
        b',d' // b',o' by A20,AFF_1:def 1;
        then b',d' // o',b' by AFF_1:13;
        then b,d // o,b by ANALMETR:48;
        then
A65:    o,c _|_ o,b by A18,A64,ANALMETR:84;
A66:    not LIN o,d,d1
        proof
          assume LIN o,d,d1;
          then o,d // o,d1 by ANALMETR:def 11;
          then o,d _|_ o,d by A45,A62,ANALMETR:84;
          hence contradiction by A40,ANALMETR:51;
        end;
        LIN a',c',d1' by A38,ANALMETR:55;
        then LIN c',d1',a' by AFF_1:15;
        then c',d1' // c',a' by AFF_1:def 1;
        then a',c' // c',d1' by AFF_1:13;
        then
A67:    a,c // c,d1 by ANALMETR:48;
A68:    a<>c
        proof
          assume a= c;
          then LIN a',b',c' by AFF_1:16;
          hence contradiction by A2,ANALMETR:55;
        end;
        LIN c',a',o' by A21,AFF_1:15;
        then c',a' // c',o' by AFF_1:def 1;
        then a',c' // c',o' by AFF_1:13;
        then a,c // c,o by ANALMETR:48;
        then c,d1 // c,o by A67,A68,ANALMETR:82;
        then LIN c,d1,o by ANALMETR:def 11;
        then LIN c',d1',o' by ANALMETR:55;
        then LIN o',d1',c' by AFF_1:15;
        then
A69:    LIN o,d1,c by ANALMETR:55;
        LIN o',d',b' by A20,AFF_1:15;
        then
A70:    LIN o,d,b by ANALMETR:55;
A71:    d1,d // c,b by A39,ANALMETR:81;
        b<>c
        proof
          assume b = c;
          then LIN a',b',c' by AFF_1:16;
          hence contradiction by A2,ANALMETR:55;
        end;
        then d,d1 _|_ a,d by A13,A39,ANALMETR:84;
        then d1,d _|_ d,a by ANALMETR:83;
        then c,d _|_
        b,a by A15,A23,A27,A40,A45,A48,A49,A57,A62,A65,A66,A69,A70
        ,A71,Def6;
        hence thesis by A10,A13,ANALMETR:83;
      end;
      hence thesis by A3,A4,A10,A12,ANALMETR:83,84;
    end;
    hence thesis by A5,A6,A9,A13,ANALMETR:83,84;
  end;
  now
    assume d = c;
    then a,b _|_ d,c by ANALMETR:51;
    hence thesis by A10,A13,ANALMETR:83;
  end;
  hence thesis by A16;
end;

