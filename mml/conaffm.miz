:: Metric-Affine Configurations in Metric Affine Planes -  Part I
::  by Jolanta \'Swierzy\'nska and Bogdan \'Swierzy\'nski
::
:: Received October 31, 1990
:: Copyright (c) 1990 Association of Mizar Users

environ

 vocabularies ANALMETR, SUBSET_1, DIRAF, ANALOAF, SYMSP_1, CONAFFM;
 notations STRUCT_0, ANALOAF, AFF_1, ANALMETR;
 constructors AFF_1, ANALMETR;
 registrations ANALMETR;
 theorems AFF_1, ANALMETR, DIRAF;

begin

reserve X for OrtAfPl;
reserve o,a,a1,a2,b,b1,b2,c,c1,c2,d,e1,e2 for Element of X;
reserve b2',c2',d1' for Element of Af(X);

definition
  let X;
  attr X is satisfying_DES means
  for o,a,a1,b,b1,c,c1 st o<>a & o<>a1 & o<>b & o<>b1 & o<>c & o<>c1 &
  not LIN b,b1,a & not LIN a,a1,c & LIN o,a,a1 & LIN o,b,b1 &
  LIN o,c,c1 & a,b // a1,b1 & a,c // a1,c1 holds b,c // b1,c1;
  attr X is satisfying_AH means
  for o,a,a1,b,b1,c,c1 st o,a _|_ o,a1 & o,b _|_ o,b1
  & o,c _|_ o,c1 & a,b _|_ a1,b1 & o,a // b,c &
  a,c _|_ a1,c1 & not o,c // o,a & not o,a // o,b holds b,c _|_ b1,c1;
  attr X is satisfying_3H means
  for a,b,c st not LIN a,b,c holds ex d st d,a _|_ b,c & d,b _|_ a,c &
  d,c _|_ a,b;
  attr X is satisfying_ODES means
  :Def4:
  for o,a,a1,b,b1,c,c1 st o,a _|_ o,a1 & o,b _|_ o,b1 & o,c _|_ o,c1 &
  a,b _|_ a1,b1 & a,c _|_ a1,c1 & not o,c // o,a & not o,a // o,b
  holds b,c _|_ b1,c1;
  attr X is satisfying_LIN means
  :Def5:
  for o,a,a1,b,b1,c,c1 st o<>a & o<>a1 & o<>b & o<>b1 & o<>c &
  o<>c1 & a<>b & o,c _|_ o,c1 & o,a _|_ o,a1 & o,b _|_ o,b1 &
  not LIN o,c,a & LIN o,a,b & LIN o,a1,b1 & a,c _|_ a1,c1 & b,c _|_ b1,c1
  holds a,a1 // b,b1;
  attr X is satisfying_LIN1 means
  :Def6:
  for o,a,a1,b,b1,c,c1 st o<>a & o<>a1 & o<>b & o<>b1 & o<>c &
  o<>c1 & a<>b & o,c _|_ o,c1 & o,a _|_ o,a1 & o,b _|_ o,b1 &
  not LIN o,c,a & LIN o,a,b & LIN o,a1,b1 & a,c _|_ a1,c1 & a,a1 // b,b1
  holds b,c _|_ b1,c1;
  attr X is satisfying_LIN2 means
  for o,a,a1,b,b1,c,c1 st o<>a & o<>a1 & o<>b & o<>b1 & o<>c &
  o<>c1 & a<>b & a,a1 // b,b1 & o,a _|_ o,a1 & o,b _|_ o,b1 &
  not LIN o,c,a & LIN o,a,b & LIN o,a1,b1 & a,c _|_ a1,c1 & b,c _|_ b1,c1
  holds o,c _|_ o,c1;
end;

theorem
  X is satisfying_ODES implies X is satisfying_DES
proof
  assume
A1: X is satisfying_ODES;
  let o,a,a1,b,b1,c,c1;
  assume that
A2: o<>a and
A3: o<>a1 and
A4: o<>b and
A5: o<>b1 and
A6: o<>c
  and o<>c1 and
A7: not LIN b,b1,a and
A8: not LIN a,a1,c and
A9: LIN o,a,a1 and
A10: LIN o,b,b1 and
A11: LIN o,c,c1 and
A12: a,b // a1,b1 and
A13: a,c // a1,c1;
  consider a2 such that
A14: o,a _|_ o,a2 and
A15: o<>a2 by ANALMETR:def 10;
  consider e1 such that
A16: o,b _|_ o,e1 and
A17: o<>e1 by ANALMETR:def 10;
  consider e2 such that
A18: a,b _|_ a2,e2 and
A19: a2<>e2 by ANALMETR:def 10;
  reconsider o'=o,a'=a,a1'=a1,b'=b,b1'=b1,c'=c,c1'=c1,a2'=a2,e1'=e1,e2'=e2
  as Element of Af(X) by ANALMETR:47;
  not o',e1' // a2',e2'
  proof
    assume o',e1' // a2',e2';
    then o,e1 // a2,e2 by ANALMETR:48;
    then a2,e2 _|_ o,b by A16,A17,ANALMETR:84;
    then a,b // o,b by A18,A19,ANALMETR:85;
    then
A20: a',b' // o',b' by ANALMETR:48;
    then b',a' // b',o' by AFF_1:13;
    then
A21: LIN b',a',o' by AFF_1:def 1;
    o',b' // b',a' by A20,AFF_1:13;
    then
A22: o,b // b,a by ANALMETR:48;
A23: b'<>a'
    proof
      assume b'=a';
      then LIN b',b1',a' by AFF_1:16;
      hence contradiction by A7,ANALMETR:55;
    end;
    o,b // o,b1 by A10,ANALMETR:def 11;
    then b,a // o,b1 by A4,A22,ANALMETR:82;
    then b',a' // o',b1' by ANALMETR:48;
    then LIN b',a',b1' by A21,A23,AFF_1:18;
    then LIN b',b1',a' by AFF_1:15;
    hence contradiction by A7,ANALMETR:55;
  end;
  then consider b2' such that
A24: LIN o',e1',b2' and
A25: LIN a2',e2',b2' by AFF_1:74;
  reconsider b2=b2' as Element of X by ANALMETR:47;
  LIN o,e1,b2 by A24,ANALMETR:55;
  then o,e1 // o,b2 by ANALMETR:def 11;
  then
A26: o,b _|_ o,b2 by A16,A17,ANALMETR:84;
  LIN a2,e2,b2 by A25,ANALMETR:55;
  then a2,e2 // a2,b2 by ANALMETR:def 11;
  then
A27: a,b _|_ a2,b2 by A18,A19,ANALMETR:84;
  consider e1 such that
A28: o,c _|_ o,e1 and
A29: o<>e1 by ANALMETR:def 10;
  consider e2 such that
A30: a,c _|_ a2,e2 and
A31: a2<>e2 by ANALMETR:def 10;
  reconsider e1'=e1,e2'=e2 as Element of Af(X) by ANALMETR:47;
  not o',e1' // a2',e2'
  proof
    assume o',e1' // a2',e2';
    then o,e1 // a2,e2 by ANALMETR:48;
    then o,c _|_ a2,e2 by A28,A29,ANALMETR:84;
    then o,c // a,c by A30,A31,ANALMETR:85;
    then c,o // c,a by ANALMETR:81;
    then LIN c,o,a by ANALMETR:def 11;
    then LIN c',o',a' by ANALMETR:55;
    then LIN a',c',o' by AFF_1:15;
    then LIN a,c,o by ANALMETR:55;
    then a,c // a,o by ANALMETR:def 11;
    then
A32: o,a // a,c by ANALMETR:81;
    LIN o',a',a1' by A9,ANALMETR:55;
    then LIN a',o',a1' by AFF_1:15;
    then LIN a,o,a1 by ANALMETR:55;
    then a,o // a,a1 by ANALMETR:def 11;
    then o,a // a,a1 by ANALMETR:81;
    then a,a1 // a,c by A2,A32,ANALMETR:82;
    hence contradiction by A8,ANALMETR:def 11;
  end;
  then consider c2' such that
A33: LIN o',e1',c2' and
A34: LIN a2',e2',c2' by AFF_1:74;
  reconsider c2=c2' as Element of X by ANALMETR:47;
  LIN o,e1,c2 by A33,ANALMETR:55;
  then o,e1 // o,c2 by ANALMETR:def 11;
  then
A35: o,c _|_ o,c2 by A28,A29,ANALMETR:84;
  LIN a2,e2,c2 by A34,ANALMETR:55;
  then a2,e2 // a2,c2 by ANALMETR:def 11;
  then
A36: a,c _|_ a2,c2 by A30,A31,ANALMETR:84;
A37: not o,c // o,a
  proof
    assume o,c // o,a;
    then LIN o,c,a by ANALMETR:def 11;
    then LIN o',c',a' by ANALMETR:55;
    then LIN a',o',c' by AFF_1:15;
    then LIN a,o,c by ANALMETR:55;
    then
A38: a,o // a,c by ANALMETR:def 11;
    LIN o',a',a1' by A9,ANALMETR:55;
    then LIN a',o',a1' by AFF_1:15;
    then LIN a,o,a1 by ANALMETR:55;
    then a,o // a,a1 by ANALMETR:def 11;
    then a,a1 // a,c by A2,A38,ANALMETR:82;
    hence contradiction by A8,ANALMETR:def 11;
  end;
  not o,a // o,b
  proof
    assume o,a // o,b;
    then LIN o,a,b by ANALMETR:def 11;
    then LIN o',a',b' by ANALMETR:55;
    then LIN b',o',a' by AFF_1:15;
    then LIN b,o,a by ANALMETR:55;
    then
A39: b,o // b,a by ANALMETR:def 11;
    LIN o',b',b1' by A10,ANALMETR:55;
    then LIN b',o',b1' by AFF_1:15;
    then LIN b,o,b1 by ANALMETR:55;
    then b,o // b,b1 by ANALMETR:def 11;
    then b,b1 // b,a by A4,A39,ANALMETR:82;
    hence contradiction by A7,ANALMETR:def 11;
  end;
  then
A40: b,c _|_ b2,c2 by A1,A14,A26,A27,A35,A36,A37,Def4;
  o,a // o,a1 by A9,ANALMETR:def 11;
  then
A41: o,a1 _|_ o,a2 by A2,A14,ANALMETR:84;
  o,b // o,b1 by A10,ANALMETR:def 11;
  then
A42: o,b1 _|_ o,b2 by A4,A26,ANALMETR:84;
  o,c // o,c1 by A11,ANALMETR:def 11;
  then
A43: o,c1 _|_ o,c2 by A6,A35,ANALMETR:84;
  a<>b
  proof
    assume a=b;
    then LIN b',b1',a' by AFF_1:16;
    hence contradiction by A7,ANALMETR:55;
  end;
  then
A44: a1,b1 _|_ a2,b2 by A12,A27,ANALMETR:84;
  a<>c
  proof
    assume a= c;
    then LIN a',a1',c' by AFF_1:16;
    hence contradiction by A8,ANALMETR:55;
  end;
  then
A45: a1,c1 _|_ a2,c2 by A13,A36,ANALMETR:84;
A46: not o,c1 // o,a1
  proof
    assume o,c1 // o,a1;
    then LIN o,c1,a1 by ANALMETR:def 11;
    then LIN o',c1',a1' by ANALMETR:55;
    then LIN a1',o',c1' by AFF_1:15;
    then LIN a1,o,c1 by ANALMETR:55;
    then
A47: a1,o // a1,c1 by ANALMETR:def 11;
A48: a1<> c1
    proof
      assume a1 = c1;
      then LIN o',c',a1' by A11,ANALMETR:55;
      then LIN a1',c',o' by AFF_1:15;
      then LIN a1,c,o by ANALMETR:55;
      then
A49:  a1,c // a1,o by ANALMETR:def 11;
      LIN o',a',a1' by A9,ANALMETR:55;
      then LIN a1',o',a' by AFF_1:15;
      then LIN a1,o,a by ANALMETR:55;
      then a1,o // a1,a by ANALMETR:def 11;
      then a1,c // a1,a by A3,A49,ANALMETR:82;
      then LIN a1,c,a by ANALMETR:def 11;
      then LIN a1',c',a' by ANALMETR:55;
      then LIN a',a1',c' by AFF_1:15;
      hence contradiction by A8,ANALMETR:55;
    end;
    LIN o',a',a1' by A9,ANALMETR:55;
    then LIN a1',o',a' by AFF_1:15;
    then LIN a1,o,a by ANALMETR:55;
    then a1,o // a1,a by ANALMETR:def 11;
    then a1,c1 // a1,a by A3,A47,ANALMETR:82;
    then a1,a // a,c by A13,A48,ANALMETR:82;
    then a,a1 // a,c by ANALMETR:81;
    hence contradiction by A8,ANALMETR:def 11;
  end;
  not o,a1 // o,b1
  proof
    assume o,a1 // o,b1;
    then LIN o,a1,b1 by ANALMETR:def 11;
    then LIN o',a1',b1' by ANALMETR:55;
    then LIN b1',a1',o' by AFF_1:15;
    then LIN b1,a1,o by ANALMETR:55;
    then
A50: b1,a1 // b1,o by ANALMETR:def 11;
A51: a1<>b1
    proof
      assume a1=b1;
      then LIN o',a',b1' by A9,ANALMETR:55;
      then LIN b1',o',a' by AFF_1:15;
      then LIN b1,o,a by ANALMETR:55;
      then
A52:  b1,o // b1,a by ANALMETR:def 11;
      LIN o',b',b1' by A10,ANALMETR:55;
      then LIN b1',b',o' by AFF_1:15;
      then LIN b1,b,o by ANALMETR:55;
      then b1,b // b1,o by ANALMETR:def 11;
      then b1,a // b1,b by A5,A52,ANALMETR:82;
      then LIN b1,a,b by ANALMETR:def 11;
      then LIN b1',a',b' by ANALMETR:55;
      then LIN b',b1',a' by AFF_1:15;
      hence contradiction by A7,ANALMETR:55;
    end;
A53: b1,a1 // a,b by A12,ANALMETR:81;
    LIN o',b',b1' by A10,ANALMETR:55;
    then LIN b1',b',o' by AFF_1:15;
    then LIN b1,b,o by ANALMETR:55;
    then b1,b // b1,o by ANALMETR:def 11;
    then b1,a1 // b1,b by A5,A50,ANALMETR:82;
    then b1,b // a,b by A51,A53,ANALMETR:82;
    then b,b1 // b,a by ANALMETR:81;
    hence contradiction by A7,ANALMETR:def 11;
  end;
  then
A54: b1,c1 _|_ b2,c2 by A1,A41,A42,A43,A44,A45,A46,Def4;
A55: now
    assume
A56: not LIN o,b,c;
    b2<>c2
    proof
      assume
A57:  b2 = c2;
      o<>b2
      proof
        assume
A58:    o=b2;
        a2,o _|_ a,o by A14,ANALMETR:83;
        then a,o // a,b by A15,A27,A58,ANALMETR:85;
        then LIN a,o,b by ANALMETR:def 11;
        then LIN a',o',b' by ANALMETR:55;
        then LIN b',o',a' by AFF_1:15;
        then LIN b,o,a by ANALMETR:55;
        then
A59:    b,o // b,a by ANALMETR:def 11;
        LIN o',b',b1' by A10,ANALMETR:55;
        then LIN b',o',b1' by AFF_1:15;
        then LIN b,o,b1 by ANALMETR:55;
        then b,o // b,b1 by ANALMETR:def 11;
        then b,b1 // b,a by A4,A59,ANALMETR:82;
        hence contradiction by A7,ANALMETR:def 11;
      end;
      then o,b // o,c by A26,A35,A57,ANALMETR:85;
      hence contradiction by A56,ANALMETR:def 11;
    end;
    hence thesis by A40,A54,ANALMETR:85;
  end;
  now
    assume
A60: LIN o,b,c;
    then LIN o',b',c' by ANALMETR:55;
    then LIN b',o',c' by AFF_1:15;
    then
A61: b',o' // b',c' by AFF_1:def 1;
    LIN o',b',b1' by A10,ANALMETR:55;
    then LIN b',o',b1' by AFF_1:15;
    then b',o' // b',b1' by AFF_1:def 1;
    then b',c' // b',b1' by A4,A61,AFF_1:14;
    then
A62: LIN b',c',b1' by AFF_1:def 1;
    LIN o',b',c' by A60,ANALMETR:55;
    then LIN c',o',b' by AFF_1:15;
    then
A63: c',o' // c',b' by AFF_1:def 1;
    LIN o',c',c1' by A11,ANALMETR:55;
    then LIN c',o',c1' by AFF_1:15;
    then c',o' // c',c1' by AFF_1:def 1;
    then c',b' // c',c1' by A6,A63,AFF_1:14;
    then LIN c',b',c1' by AFF_1:def 1;
    then LIN b',c',c1' by AFF_1:15;
    then b',c' // b1',c1' by A62,AFF_1:19;
    hence thesis by ANALMETR:48;
  end;
  hence thesis by A55;
end;

theorem
  X is satisfying_ODES implies X is satisfying_AH
proof
  assume
A1: X is satisfying_ODES;
  let o,a,a1,b,b1,c,c1;
  assume that
A2: o,a _|_ o,a1 and
A3: o,b _|_ o,b1 and
A4: o,c _|_ o,c1 and
A5: a,b _|_ a1,b1
  and o,a // b,c and
A6: a,c _|_ a1,c1 and
A7: not o,c // o,a and
A8: not o,a // o,b;
  thus thesis by A1,A2,A3,A4,A5,A6,A7,A8,Def4;
end;

theorem Th3:
  X is satisfying_LIN implies X is satisfying_LIN1
proof
  assume
A1: X is satisfying_LIN;
  let o,a,a1,b,b1,c,c1;
  assume that
A2: o<>a and
A3: o<>a1 and
A4: o<>b
  and o<>b1 and
A5: o<>c and
A6: o<>c1 and
A7: a<>b and
A8: o,c _|_ o,c1 and
A9: o,a _|_ o,a1
  and o,b _|_ o,b1 and
A10: not LIN o,c,a and
A11: LIN o,a,b and
A12: LIN o,a1,b1 and
A13: a,c _|_ a1,c1 and
A14: a,a1 // b,b1;
  reconsider a'=a,a1'=a1,b'=b,b1'=b1,c'=c,c1'=c1,o'=o
  as Element of Af(X) by ANALMETR:47;
  now ex b2 st LIN o,a1,b2 & c1,b2 _|_ b,c & c1<>b2
    proof
      consider y be Element of X such that
A15:  o,a1 // o,y and
A16:  o<>y by ANALMETR:51;
      consider x be Element of X such that
A17:  b,c _|_ c1,x and
A18:  c1<>x by ANALMETR:51;
A19:  not o,y // c1,x
      proof
        assume
A20:    o,y // c1,x;
        reconsider y'=y,x'=x as Element of Af(X) by ANALMETR:47;
A21:    o',y' // c1',x' by A20,ANALMETR:48;
        o',a1' // o',y' by A15,ANALMETR:48;
        then o',y' // o',a1' by AFF_1:13;
        then c1',x' // o',a1' by A16,A21,DIRAF:47;
        then c1,x // o,a1 by ANALMETR:48;
        then o,a1 _|_ b,c by A17,A18,ANALMETR:84;
        then
A22:    o,a // b,c by A3,A9,ANALMETR:85;
        o,a // o,b by A11,ANALMETR:def 11;
        then b,c // o,b by A2,A22,ANALMETR:82;
        then b',c' // o',b' by ANALMETR:48;
        then b',c' // b',o' by AFF_1:13;
        then LIN b',c',o' by AFF_1:def 1;
        then LIN o',b',c' by AFF_1:15;
        then
A23:    o',b' // o',c' by AFF_1:def 1;
        LIN o',a',b' by A11,ANALMETR:55;
        then LIN o',b',a' by AFF_1:15;
        then o',b' // o',a' by AFF_1:def 1;
        then o',c' // o',a' by A4,A23,DIRAF:47;
        then LIN o',c',a' by AFF_1:def 1;
        hence contradiction by A10,ANALMETR:55;
      end;
      reconsider y'=y,x'=x as Element of Af(X) by ANALMETR:47;
      not o',y' // c1',x' by A19,ANALMETR:48;
      then consider b2' be Element of Af(X) such that
A24:  LIN o',y',b2' and
A25:  LIN c1',x',b2' by AFF_1:74;
      reconsider b2=b2' as Element of X by ANALMETR:47;
      LIN c1,x,b2 by A25,ANALMETR:55;
      then c1,x // c1,b2 by ANALMETR:def 11;
      then
A26:  c1,b2 _|_ b,c by A17,A18,ANALMETR:84;
      o',a1' // o',y' by A15,ANALMETR:48;
      then
A27:  o',y' // o',a1' by AFF_1:13;
      o',y' // o',b2' by A24,AFF_1:def 1;
      then o',a1' // o',b2' by A16,A27,DIRAF:47;
      then LIN o',a1',b2' by AFF_1:def 1;
      then
A28:  LIN o,a1,b2 by ANALMETR:55;
      c1<>b2
      proof
        assume c1=b2;
        then o,a1 // o,c1 by A28,ANALMETR:def 11;
        then o,c1 _|_ o,a by A3,A9,ANALMETR:84;
        then o,c // o,a by A6,A8,ANALMETR:85;
        hence contradiction by A10,ANALMETR:def 11;
      end;
      hence thesis by A26,A28;
    end;
    then consider b2 such that
A29: LIN o,a1,b2 and
A30: c1,b2 _|_ b,c
    and c1<>b2;
    reconsider b2'=b2 as Element of Af(X) by ANALMETR:47;
    o,a1 // o,b2 by A29,ANALMETR:def 11;
    then
A31: o,a _|_ o,b2 by A3,A9,ANALMETR:84;
A32: o,a // o,b by A11,ANALMETR:def 11;
A33: o<>b2
    proof
      assume o=b2;
      then o,c1 _|_ b,c by A30,ANALMETR:83;
      then o,c // b,c by A6,A8,ANALMETR:85;
      then o',c' // b',c' by ANALMETR:48;
      then c',o' // c',b' by AFF_1:13;
      then LIN c',o',b' by AFF_1:def 1;
      then LIN o',b',c' by AFF_1:15;
      then
A34:  o',b' // o',c' by AFF_1:def 1;
      LIN o',a',b' by A11,ANALMETR:55;
      then LIN o',b',a' by AFF_1:15;
      then o',b' // o',a' by AFF_1:def 1;
      then o',c' // o',a' by A4,A34,DIRAF:47;
      then LIN o',c',a' by AFF_1:def 1;
      hence contradiction by A10,ANALMETR:55;
    end;
A35: o,b _|_ o,b2 by A2,A31,A32,ANALMETR:84;
    b,c _|_ b2,c1 by A30,ANALMETR:83;
    then
A36: a,a1 // b,b2 by A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A13,A29,A33,A35
    ,Def5;
    not LIN o,a,a1
    proof
      assume LIN o,a,a1;
      then o,a // o,a1 by ANALMETR:def 11;
      then o,a1 _|_ o,a1 by A2,A9,ANALMETR:84;
      hence contradiction by A3,ANALMETR:51;
    end;
    then
A37: not LIN o',a',a1' by ANALMETR:55;
A38: LIN o',a',b' by A11,ANALMETR:55;
A39: LIN o',a1',b1' by A12,ANALMETR:55;
A40: LIN o',a1',b2' by A29,ANALMETR:55;
A41: a',a1' // b',b1' by A14,ANALMETR:48;
    a',a1' // b',b2' by A36,ANALMETR:48;
    then b1=b2 by A37,A38,A39,A40,A41,AFF_1:70;
    hence thesis by A30,ANALMETR:83;
  end;
  hence thesis;
end;

theorem
  X is satisfying_LIN1 implies X is satisfying_LIN2
proof
  assume
A1: X is satisfying_LIN1;
  let o,a,a1,b,b1,c,c1;
  assume that
A2: o<>a and
A3: o<>a1 and
A4: o<>b and
A5: o<>b1 and
A6: o<>c
  and o<>c1 and
A7: a<>b and
A8: a,a1 // b,b1 and
A9: o,a _|_ o,a1 and
A10: o,b _|_ o,b1 and
A11: not LIN o,c,a and
A12: LIN o,a,b and
A13: LIN o,a1,b1 and
A14: a,c _|_ a1,c1 and
A15: b,c _|_ b1,c1;
  reconsider a'=a,a1'=a1,b'=b,b1'=b1,c'=c,c1'=c1,o'=o
  as Element of Af(X) by ANALMETR:47;
  ex c2 st LIN a1,c1,c2 & o,c _|_ o,c2 & o<>c2
  proof
    consider e1 such that
A16: a1,c1 // a1,e1 and
A17: a1<>e1 by ANALMETR:51;
    consider e2 such that
A18: o,c _|_ o,e2 and
A19: o<>e2 by ANALMETR:51;
    reconsider e1'=e1,e2'=e2 as Element of Af(X) by ANALMETR:47;
    not a1',e1' // o',e2'
    proof
      assume a1',e1' // o',e2';
      then a1,e1 // o,e2 by ANALMETR:48;
      then a1,c1 // o,e2 by A16,A17,ANALMETR:82;
      then
A20:  a1,c1 _|_ o,c by A18,A19,ANALMETR:84;
      a1<>c1
      proof
        assume
A21:    a1=c1;
A22:    b1<>a1
        proof
          assume b1=a1;
          then a1,a // a1,b by A8,ANALMETR:81;
          then LIN a1,a,b by ANALMETR:def 11;
          then LIN a1',a',b' by ANALMETR:55;
          then LIN a',b',a1' by AFF_1:15;
          then LIN a,b,a1 by ANALMETR:55;
          then
A23:      a,b // a,a1 by ANALMETR:def 11;
          LIN o',a',b' by A12,ANALMETR:55;
          then LIN a',b',o' by AFF_1:15;
          then LIN a,b,o by ANALMETR:55;
          then a,b // a,o by ANALMETR:def 11;
          then o,a // a,b by ANALMETR:81;
          then a,a1 // o,a by A7,A23,ANALMETR:82;
          then a,a1 // a,o by ANALMETR:81;
          then LIN a,a1,o by ANALMETR:def 11;
          then LIN a',a1',o' by ANALMETR:55;
          then LIN o',a',a1' by AFF_1:15;
          then LIN o,a,a1 by ANALMETR:55;
          then o,a // o,a1 by ANALMETR:def 11;
          then o,a _|_ o,a by A3,A9,ANALMETR:84;
          hence contradiction by A2,ANALMETR:51;
        end;
A24:    LIN o',a',b' by A12,ANALMETR:55;
A25:    LIN o',a1',b1' by A13,ANALMETR:55;
A26:    LIN b',o',a' by A24,AFF_1:15;
A27:    LIN b1',o',a1' by A25,AFF_1:15;
A28:    LIN b,o,a by A26,ANALMETR:55;
A29:    LIN b1,o,a1 by A27,ANALMETR:55;
A30:    b,o // b,a by A28,ANALMETR:def 11;
A31:    b1,o // b1,a1 by A29,ANALMETR:def 11;
        b,o _|_ b1,o by A10,ANALMETR:83;
        then b,a _|_ b1,o by A4,A30,ANALMETR:84;
        then b1,a1 _|_ b,a by A5,A31,ANALMETR:84;
        then b,c // b,a by A15,A21,A22,ANALMETR:85;
        then LIN b,c,a by ANALMETR:def 11;
        then LIN b',c',a' by ANALMETR:55;
        then LIN a',b',c' by AFF_1:15;
        then LIN a,b,c by ANALMETR:55;
        then
A32:    a,b // a,c by ANALMETR:def 11;
        LIN o',a',b' by A12,ANALMETR:55;
        then LIN a',b',o' by AFF_1:15;
        then LIN a,b,o by ANALMETR:55;
        then a,b // a,o by ANALMETR:def 11;
        then a,c // a,o by A7,A32,ANALMETR:82;
        then LIN a,c,o by ANALMETR:def 11;
        then LIN a',c',o' by ANALMETR:55;
        then LIN o',c',a' by AFF_1:15;
        hence contradiction by A11,ANALMETR:55;
      end;
      then a,c // o,c by A14,A20,ANALMETR:85;
      then c,a // c,o by ANALMETR:81;
      then c',a' // c',o' by ANALMETR:48;
      then LIN c',a',o' by AFF_1:def 1;
      then LIN o',c',a' by AFF_1:15;
      hence contradiction by A11,ANALMETR:55;
    end;
    then consider c2' such that
A33: LIN a1',e1',c2' and
A34: LIN o',e2',c2' by AFF_1:74;
    reconsider c2=c2' as Element of X by ANALMETR:47;
    take c2;
    a1',e1' // a1',c2' by A33,AFF_1:def 1;
    then a1,e1 // a1,c2 by ANALMETR:48;
    then
A35: a1,c1 // a1,c2 by A16,A17,ANALMETR:82;
    LIN o,e2,c2 by A34,ANALMETR:55;
    then
A36: o,e2 // o,c2 by ANALMETR:def 11;
    o<>c2
    proof
      assume o=c2;
      then a1,c1 // o,a1 by A35,ANALMETR:81;
      then
A37:  o,a _|_ a1,c1 by A3,A9,ANALMETR:84;
      a1<>c1
      proof
        assume
A38:    a1=c1;
A39:    a1<>b1
        proof
          assume a1=b1;
          then a1,a // a1,b by A8,ANALMETR:81;
          then LIN a1,a,b by ANALMETR:def 11;
          then LIN a1',a',b' by ANALMETR:55;
          then LIN a',b',a1' by AFF_1:15;
          then LIN a,b,a1 by ANALMETR:55;
          then
A40:      a,b // a,a1 by ANALMETR:def 11;
          LIN o',a',b' by A12,ANALMETR:55;
          then LIN a',b',o' by AFF_1:15;
          then LIN a,b,o by ANALMETR:55;
          then a,b // a,o by ANALMETR:def 11;
          then o,a // a,b by ANALMETR:81;
          then a,a1 // o,a by A7,A40,ANALMETR:82;
          then a,a1 // a,o by ANALMETR:81;
          then LIN a,a1,o by ANALMETR:def 11;
          then LIN a',a1',o' by ANALMETR:55;
          then LIN o',a',a1' by AFF_1:15;
          then LIN o,a,a1 by ANALMETR:55;
          then o,a // o,a1 by ANALMETR:def 11;
          then o,a _|_ o,a by A3,A9,ANALMETR:84;
          hence contradiction by A2,ANALMETR:51;
        end;
        LIN o',a1',b1' by A13,ANALMETR:55;
        then LIN b1',a1',o' by AFF_1:15;
        then LIN b1,a1,o by ANALMETR:55;
        then b1,a1 // b1,o by ANALMETR:def 11;
        then b1,a1 // o,b1 by ANALMETR:81;
        then b,c _|_ o,b1 by A15,A38,A39,ANALMETR:84;
        then
A41:    b,c // o,b by A5,A10,ANALMETR:85;
A42:    LIN o',a',b' by A12,ANALMETR:55;
        then
A43:    LIN b',a',o' by AFF_1:15;
A44:    LIN a',b',o' by A42,AFF_1:15;
A45:    LIN b,a,o by A43,ANALMETR:55;
A46:    LIN a,b,o by A44,ANALMETR:55;
A47:    b,a // b,o by A45,ANALMETR:def 11;
A48:    a,b // a,o by A46,ANALMETR:def 11;
        o,b // b,a by A47,ANALMETR:81;
        then b,a // b,c by A4,A41,ANALMETR:82;
        then LIN b,a,c by ANALMETR:def 11;
        then LIN b',a',c' by ANALMETR:55;
        then LIN a',b',c' by AFF_1:15;
        then LIN a,b,c by ANALMETR:55;
        then a,b // a,c by ANALMETR:def 11;
        then a,o // a,c by A7,A48,ANALMETR:82;
        then LIN a,o,c by ANALMETR:def 11;
        then LIN a',o',c' by ANALMETR:55;
        then LIN o',c',a' by AFF_1:15;
        hence contradiction by A11,ANALMETR:55;
      end;
      then o,a // a,c by A14,A37,ANALMETR:85;
      then a,c // a,o by ANALMETR:81;
      then LIN a,c,o by ANALMETR:def 11;
      then LIN a',c',o' by ANALMETR:55;
      then LIN o',c',a' by AFF_1:15;
      hence contradiction by A11,ANALMETR:55;
    end;
    hence thesis by A18,A19,A35,A36,ANALMETR:84,def 11;
  end;
  then consider c2 such that
A49: LIN a1,c1,c2 and
A50: o,c _|_ o,c2 and
A51: o<>c2;
  reconsider c2'=c2 as Element of Af(X) by ANALMETR:47;
A52: b<>c & a1<>b1 & a1<>c1
  proof
    assume
A53: b=c or a1=b1 or a1=c1;
A54: now
      assume b=c;
      then LIN o',a',c' by A12,ANALMETR:55;
      then LIN o',c',a' by AFF_1:15;
      hence contradiction by A11,ANALMETR:55;
    end;
A55: now
      assume a1=b1;
      then a1,a // a1,b by A8,ANALMETR:81;
      then LIN a1,a,b by ANALMETR:def 11;
      then LIN a1',a',b' by ANALMETR:55;
      then LIN a',b',a1' by AFF_1:15;
      then LIN a,b,a1 by ANALMETR:55;
      then
A56:  a,b // a,a1 by ANALMETR:def 11;
      LIN o',a',b' by A12,ANALMETR:55;
      then LIN a',b',o' by AFF_1:15;
      then LIN a,b,o by ANALMETR:55;
      then a,b // a,o by ANALMETR:def 11;
      then o,a // a,b by ANALMETR:81;
      then a,a1 // o,a by A7,A56,ANALMETR:82;
      then a,a1 // a,o by ANALMETR:81;
      then LIN a,a1,o by ANALMETR:def 11;
      then LIN a',a1',o' by ANALMETR:55;
      then LIN o',a',a1' by AFF_1:15;
      then LIN o,a,a1 by ANALMETR:55;
      then o,a // o,a1 by ANALMETR:def 11;
      then o,a _|_ o,a by A3,A9,ANALMETR:84;
      hence contradiction by A2,ANALMETR:51;
    end;
    now
      assume
A57:  a1=c1;
      LIN o',a1',b1' by A13,ANALMETR:55;
      then LIN b1',a1',o' by AFF_1:15;
      then LIN b1,a1,o by ANALMETR:55;
      then b1,a1 // b1,o by ANALMETR:def 11;
      then b1,a1 // o,b1 by ANALMETR:81;
      then b,c _|_ o,b1 by A15,A55,A57,ANALMETR:84;
      then
A58:  b,c // o,b by A5,A10,ANALMETR:85;
A59:  LIN o',a',b' by A12,ANALMETR:55;
      then
A60:  LIN b',a',o' by AFF_1:15;
A61:  LIN a',b',o' by A59,AFF_1:15;
A62:  LIN b,a,o by A60,ANALMETR:55;
A63:  LIN a,b,o by A61,ANALMETR:55;
A64:  b,a // b,o by A62,ANALMETR:def 11;
A65:  a,b // a,o by A63,ANALMETR:def 11;
      o,b // b,a by A64,ANALMETR:81;
      then b,a // b,c by A4,A58,ANALMETR:82;
      then LIN b,a,c by ANALMETR:def 11;
      then LIN b',a',c' by ANALMETR:55;
      then LIN a',b',c' by AFF_1:15;
      then LIN a,b,c by ANALMETR:55;
      then a,b // a,c by ANALMETR:def 11;
      then a,o // a,c by A7,A65,ANALMETR:82;
      then LIN a,o,c by ANALMETR:def 11;
      then LIN a',o',c' by ANALMETR:55;
      then LIN o',c',a' by AFF_1:15;
      hence contradiction by A11,ANALMETR:55;
    end;
    hence contradiction by A53,A54,A55;
  end;
  a1,c1 // a1,c2 by A49,ANALMETR:def 11;
  then a,c _|_ a1,c2 by A14,A52,ANALMETR:84;
  then b,c _|_ b1,c2 by A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,A13,A50,A51
  ,Def6;
  then b1,c1 // b1,c2 by A15,A52,ANALMETR:85;
  then
A66: LIN b1,c1,c2 by ANALMETR:def 11;
  not LIN b1,a1,c1
  proof
    assume LIN b1,a1,c1;
    then LIN b1',a1',c1' by ANALMETR:55;
    then LIN a1',b1',c1' by AFF_1:15;
    then a1',b1' // a1',c1' by AFF_1:def 1;
    then
A67: a1,b1 // a1,c1 by ANALMETR:48;
    LIN o',a1',b1' by A13,ANALMETR:55;
    then LIN a1',b1',o' by AFF_1:15;
    then LIN a1,b1,o by ANALMETR:55;
    then a1,b1 // a1,o by ANALMETR:def 11;
    then a1,c1 // a1,o by A52,A67,ANALMETR:82;
    then a,c _|_ a1,o by A14,A52,ANALMETR:84;
    then
A68: o,a1 _|_ a,c by ANALMETR:83;
    o,a1 _|_ a,o by A9,ANALMETR:83;
    then a,o // a,c by A3,A68,ANALMETR:85;
    then LIN a,o,c by ANALMETR:def 11;
    then LIN a',o',c' by ANALMETR:55;
    then LIN o',c',a' by AFF_1:15;
    hence contradiction by A11,ANALMETR:55;
  end;
  then
A69: not LIN b1',a1',c1' by ANALMETR:55;
A70: LIN b1',c1',c2' by A66,ANALMETR:55;
  a1,c1 // a1,c2 by A49,ANALMETR:def 11;
  then a1',c1' // a1',c2' by ANALMETR:48;
  hence thesis by A50,A69,A70,AFF_1:23;
end;

theorem
  X is satisfying_LIN implies X is satisfying_ODES
proof
  assume
A1: X is satisfying_LIN;
  let o,a,a1,b,b1,c,c1;
  assume that
A2: o,a _|_ o,a1 and
A3: o,b _|_ o,b1 and
A4: o,c _|_ o,c1 and
A5: a,b _|_ a1,b1 and
A6: a,c _|_ a1,c1 and
A7: not o,c // o,a and
A8: not o,a // o,b;
A9: X is satisfying_LIN1 by A1,Th3;
  now
    let o,a,a1,b,b1,c,c1;
    assume
A10: X is satisfying_LIN;
    assume that
A11: o,a _|_ o,a1 and
A12: o,b _|_ o,b1 and
A13: o,c _|_ o,c1 and
A14: a,b _|_ a1,b1 and
A15: a,c _|_ a1,c1 and
A16: not o,c // o,a and
A17: not o,a // o,b;
    assume
A18: not o,b // a,c;
    reconsider a'=a,a1'=a1,b'=b,b1'=b1,c'=c,c1'=c1,o'=o
    as Element of Af(X) by ANALMETR:47;
A19: now
      assume
A20:  o=a1;
      then
A21:  a1,b1 _|_ b,a1 by A12,ANALMETR:83;
A22:  a1,b1 _|_ b,a by A14,ANALMETR:83;
      not b,a1 // b,a
      proof
        assume b,a1 // b,a;
        then LIN b,o,a by A20,ANALMETR:def 11;
        then LIN b',o',a' by ANALMETR:55;
        then LIN o',a',b' by AFF_1:15;
        then LIN o,a,b by ANALMETR:55;
        hence contradiction by A17,ANALMETR:def 11;
      end;
      then
A23:  a1=b1 by A21,A22,ANALMETR:85;
A24:  a1,c1 _|_ c,a1 by A13,A20,ANALMETR:83;
      a1,c1 _|_ c,a by A15,ANALMETR:83;
      then
A25:  c,a1 // c,a or a1=c1 by A24,ANALMETR:85;
      not c,a1 // c,a
      proof
        assume c,a1 // c,a;
        then LIN c,o,a by A20,ANALMETR:def 11;
        then LIN c',o',a' by ANALMETR:55;
        then LIN o',c',a' by AFF_1:15;
        then LIN o,c,a by ANALMETR:55;
        hence contradiction by A16,ANALMETR:def 11;
      end;
      hence b,c _|_ b1,c1 by A23,A25,ANALMETR:51;
    end;
A26: now
      assume that
A27:  LIN o,b,c and
A28:  o<>a1;
A29:  o<>b by A17,ANALMETR:51;
A30:  o<>c
      proof
        assume o=c;
        then o,a // o,c by ANALMETR:51;
        then o',a' // o',c' by ANALMETR:48;
        then o',c' // o',a' by AFF_1:13;
        hence contradiction by A16,ANALMETR:48;
      end;
A31:  o<>b1
      proof
        assume
A32:    o=b1;
        a1,o _|_ a,o by A11,ANALMETR:83;
        then a,o // a,b by A14,A28,A32,ANALMETR:85;
        then LIN a,o,b by ANALMETR:def 11;
        then LIN a',o',b' by ANALMETR:55;
        then LIN o',a',b' by AFF_1:15;
        then LIN o,a,b by ANALMETR:55;
        hence contradiction by A17,ANALMETR:def 11;
      end;
      o,b // o,c by A27,ANALMETR:def 11;
      then o,c _|_ o,b1 by A12,A29,ANALMETR:84;
      then o,b1 // o,c1 by A13,A30,ANALMETR:85;
      then LIN o,b1,c1 by ANALMETR:def 11;
      then LIN o',b1',c1' by ANALMETR:55;
      then LIN b1',o',c1' by AFF_1:15;
      then LIN b1,o,c1 by ANALMETR:55;
      then
A33:  b1,o // b1,c1 by ANALMETR:def 11;
      b1,o _|_ b,o by A12,ANALMETR:83;
      then
A34:  b,o _|_ b1,c1 by A31,A33,ANALMETR:84;
      LIN o',b',c' by A27,ANALMETR:55;
      then LIN b',o',c' by AFF_1:15;
      then LIN b,o,c by ANALMETR:55;
      then b,o // b,c by ANALMETR:def 11;
      hence b,c _|_ b1,c1 by A29,A34,ANALMETR:84;
    end;
A35: now
      assume that
A36:  LIN a,b,c
      and not LIN o,b,c and
A37:  o<>a1;
A38:  a<>c
      proof
        assume a=c;
        then a,o // a,c by ANALMETR:51;
        then LIN a,o,c by ANALMETR:def 11;
        then LIN a',o',c' by ANALMETR:55;
        then LIN o',c',a' by AFF_1:15;
        then LIN o,c,a by ANALMETR:55;
        hence contradiction by A16,ANALMETR:def 11;
      end;
A39:  a<>b
      proof
        assume a=b;
        then a,o // a,b by ANALMETR:51;
        then LIN a,o,b by ANALMETR:def 11;
        then LIN a',o',b' by ANALMETR:55;
        then LIN o',a',b' by AFF_1:15;
        then LIN o,a,b by ANALMETR:55;
        hence contradiction by A17,ANALMETR:def 11;
      end;
A40:  a1<>b1 by A11,A12,A17,A37,ANALMETR:85;
      a,b // a,c by A36,ANALMETR:def 11;
      then a,c _|_ a1,b1 by A14,A39,ANALMETR:84;
      then a1,b1 // a1,c1 by A15,A38,ANALMETR:85;
      then LIN a1,b1,c1 by ANALMETR:def 11;
      then LIN a1',b1',c1' by ANALMETR:55;
      then LIN b1',a1',c1' by AFF_1:15;
      then LIN b1,a1,c1 by ANALMETR:55;
      then
A41:  b1,a1 // b1,c1 by ANALMETR:def 11;
      b1,a1 _|_ b,a by A14,ANALMETR:83;
      then
A42:  b,a _|_ b1,c1 by A40,A41,ANALMETR:84;
      LIN a',b',c' by A36,ANALMETR:55;
      then LIN b',a',c' by AFF_1:15;
      then LIN b,a,c by ANALMETR:55;
      then b,a // b,c by ANALMETR:def 11;
      hence b,c _|_ b1,c1 by A39,A42,ANALMETR:84;
    end;
    now
      assume that
