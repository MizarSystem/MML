     x in dom ((1/t1(#)(abs f1)) to_power m) &
     x in dom ((1/t2(#)(abs g1)) to_power n) by A52,A55,XBOOLE_0:def 4; then
 ((1/t1(#)(abs f1)).x) to_power m = ((1/t1(#)(abs f1)) to_power m).x &
