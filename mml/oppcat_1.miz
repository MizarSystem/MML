    .= (id((Obj S).c) qua Morphism of B) opp by Th31
    .= (id((Obj S).c)) opp by Def6,A1
    .= id(((Obj S).c) opp) by Th20;
end;

theorem Th45:
  for S being Contravariant_Functor of C,B, c being Object of C
