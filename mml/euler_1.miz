    end;
    then A119:x2 = y2 by XCMPLX_1:15;
    consider p being Integer such that
      A120:p = n;
    A121:m,p are_relative_prime by A1,A120,Lm2;
       m*(x1 - y1) = n*(m*w) - n*(x2 - y2) by A105,XCMPLX_1:26
