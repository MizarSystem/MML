   then A190:i = (m*y + n*x) mod (m*n) by A6,A130,A137,Lm3;
   A191:dom f = [:A,B:] by FUNCT_2:def 1;
   consider w being set such that
     A192:w = [y,x];
   A193:w in dom f by A191,A192,ZFMISC_1:106;
      i = f.w by A190,A73,A192;
   hence z in rng f by A128,A193,FUNCT_1:def 5;
 end;
 then Card[:A,B:] = Card C by A74,Th12;
hence thesis by A40,CARD_2:65;
