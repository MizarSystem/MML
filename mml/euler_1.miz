        end;
        suppose A112:(x2 - y2)<=0;
          consider k being Integer such that
            A113:k = -(x2 - y2);
          A114:m divides k by A106,A113,INT_2:14;
          A115:-0 <= k by A112,A113,REAL_1:50;
          then reconsider k as Nat by INT_1:16;
          A116:m divides k by A2,A114,Lm4;
             k = 0
          proof
            assume k <> 0;
