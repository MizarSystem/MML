:: from CARD_4, 2007.08.16, A.T.

theorem Th102:
  A,n are_equipotent implies A = n
proof
  defpred P[natural number] means for A st A,$1 are_equipotent holds A = $1;
A1: P[{}] by Th46;
A2: for n st P[n] holds P[succ n]
  proof
    let n such that
A3: P[n];
    let A;
    assume
A4: A,succ n are_equipotent;
    then A <> {} by Th46;
    then
A5: {} in A by ORDINAL3:10;
    now
      assume A is_limit_ordinal;
      then
A6:   omega c= A by A5,ORDINAL1:def 12;
      Card succ n = succ n by Def5;
      then Card omega <=` Card A & Card A = Card (succ n) &
      Card (succ n) <` Card omega by A4,A6,Lm1,Th21,Th27,Th83;
      hence contradiction by ORDINAL1:7;
    end;
    then consider B such that
A7: A = succ B by ORDINAL1:42;
    B in A & n in (succ n) by A7,ORDINAL1:10;
    then A \ {B},(succ n) \ { n} are_equipotent & A\{B} = B &
    (succ n) \ { n} = n by A4,A7,Th61,ORDINAL1:52;
    hence thesis by A3,A7;
  end;
  P[n] from ORDINAL2:sch 17(A1,A2);
  hence thesis;
end;

theorem Th103:
  A is finite iff A in omega
proof
  thus A is finite implies A in omega
  proof
    assume A is finite;
    then consider n such that
A1: A,n are_equipotent by Th74;
    A = n by A1,Th102;
    hence thesis by ORDINAL1:def 13;
  end;
  assume A in omega;
  then A is Element of omega;
  hence thesis;
end;

registration
  cluster natural -> finite set;
  coherence
  proof
    let a be set;
    assume a in omega;
    then a is Element of omega;
    hence thesis;
  end;
end;

registration
  cluster natural -> cardinal set;
  coherence
  proof
    let X be set;
    assume X is natural;
    then X in omega by ORDINAL1:def 13;
    hence X is cardinal by Th65;
  end;
end;

