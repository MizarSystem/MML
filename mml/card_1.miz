    hence thesis;
  end;
  thus dom f = X by FUNCOP_1:19;
  thus rng f c= { x } by FUNCOP_1:19;
  let a;
  assume a in { x };
  then
A4: a = x & y in { y } by TARSKI:def 1;
  then f.y = x by A2,FUNCOP_1:13;
  hence a in rng f by A2,A3,A4,FUNCT_1:def 5;
end;

