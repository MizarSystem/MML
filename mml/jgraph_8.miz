         A229: q1`2=p1`2 by XXREAL_0:1,A218;
          q1`2<=p2`2 & p2`2<= q2`2 by A221,A5;then
         A230: q1`2=p2`2 by XXREAL_0:1,A218;
         A231: p1`1<=pz`1 & pz`1<=p2`1 by A227,A3;then
         p2`1>=p1`1 by XXREAL_0:2;then
         p2`1>p1`1 by A223,REAL_1:def 5;then
         A232: p2`1-p1`1>0 by XREAL_1:52;
         A233: pz`1-p1`1>=0 by XREAL_1:50,A231;
         set ln=(pz`1-p1`1)/(p2`1-p1`1);
