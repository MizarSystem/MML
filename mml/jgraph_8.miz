      A240: q1 in LSeg(p1,p2)
       proof
         0 in the carrier of I[01] by BORSUK_1:86;then
         0 in dom f by FUNCT_2:def 1;then
         f.0 in rng f by FUNCT_1:12;then
         q1`2<=p1`2 & p1`2<= q2`2 by A7,A1,A5;then
         A241: q1`2=p1`2 by XXREAL_0:1,A218;
         1 in the carrier of I[01] by BORSUK_1:86;then
         1 in dom f by FUNCT_2:def 1;then
         f.1 in rng f by FUNCT_1:12;then
         q1`2<=p2`2 & p2`2<= q2`2 by A7,A1,A5;then
         A242: q1`2=p2`2 by XXREAL_0:1,A218;
         0 in the carrier of I[01] by BORSUK_1:86;then
         0 in dom g by FUNCT_2:def 1;then
         g.0 in rng g by FUNCT_1:12;then
         A243: p1`1<=q1`1 & q1`1<=p2`1 by A2,A8,A4;then
         p2`1>=p1`1 by XXREAL_0:2;then
         p2`1>p1`1 by A223,REAL_1:def 5;then
         A244: p2`1-p1`1>0 by XREAL_1:52;
         A245: q1`1-p1`1>=0 by XREAL_1:50,A243;
         set ln=(q1`1-p1`1)/(p2`1-p1`1);
         A246: ln>=0 by A244,A245,REAL_2:125;
         q1`1-p1`1<=p2`1-p1`1 by A243,REAL_1:92;then
         ln<= (p2`1-p1`1)/(p2`1-p1`1) by A244,REAL_1:73;then
         A247: ln <= 1 by A244,XCMPLX_1:60;
         A248: p2`1-p1`1 <>0 by A223;
         1-ln=(p2`1-p1`1)/(p2`1-p1`1)-(q1`1-p1`1)/(p2`1-p1`1)
                               by XCMPLX_1:60,A248
            .=((p2`1-p1`1)-(q1`1-p1`1))/(p2`1-p1`1) by XCMPLX_1:121
            .=(p2`1-q1`1)/(p2`1-p1`1);then
         A249: (1-ln)*p1`1=(p2`1-q1`1)*p1`1/(p2`1-p1`1) by XCMPLX_1:75
                   .=(p2`1*p1`1-q1`1*p1`1)/(p2`1-p1`1);
         A250: ln*p2`1=(q1`1-p1`1)*p2`1/(p2`1-p1`1) by XCMPLX_1:75
               .=(q1`1*p2`1-p1`1*p2`1)/(p2`1-p1`1);
         A251: ((1-ln)*p1+ln*p2)`2= ((1-ln)*p1)`2+(ln*p2)`2 by TOPREAL3:7
                           .=(1-ln)*p1`2+(ln*p2)`2 by TOPREAL3:9
                           .=(1-ln)*q1`2+(ln)*q1`2 by A241,A242,TOPREAL3:9;
         ((1-ln)*p1+ln*p2)`1=((1-ln)*p1)`1+(ln*p2)`1 by TOPREAL3:7
                           .=(1-ln)*p1`1+(ln*p2)`1 by TOPREAL3:9
                           .=(1-ln)*p1`1+ln*p2`1 by TOPREAL3:9
                           .=((p2`1*p1`1-q1`1*p1`1)+(q1`1*p2`1-p1`1*p2`1))/
                              (p2`1-p1`1) by XCMPLX_1:63,A249,A250
                           .=q1`1*(p2`1-p1`1)/(p2`1-p1`1)
                           .=q1`1*((p2`1-p1`1)/(p2`1-p1`1)) by XCMPLX_1:75
                           .=q1`1*1 by XCMPLX_1:60,A248;then
         (1-ln)*p1+ln*p2=q1 by A251,TOPREAL3:11;then
         q1 in { (1-lambda)*p1 + lambda*p2 where lambda is Real
                         : 0 <= lambda & lambda <= 1 } by A246,A247;
        hence q1 in LSeg(p1,p2) by TOPREAL1:def 3;
       end;
      q1 in P by A1,A240,A226,Th3;
     hence contradiction by A16,A14,XBOOLE_0:def 3;
    end;
A252: f2/.1<>f2/.len f2 by A7,A216,A19,A20,A25,A154,A161,A163,A164
,A180,A181;
     A253:1<=len hb by A66,XXREAL_0:2;
     then 1 in dom hb by FINSEQ_3:27;
     then A254:h2/.1=g.(hb.1) by A76;
     len hb in dom hb by A253,FINSEQ_3:27;
     then g2.1<>g2.len g2
     by A8,A217,A66,A70,A76,A156,A168,A169,A254;
     then L~f2 meets L~g2 by A154,A156,A163,A164,A165,A166,A170,A172,A173,A174
,A175,A176,A177,A178,A252,JGRAPH_1:30;
     then consider s being set such that
     A255:s in L~f2 & s in L~g2 by XBOOLE_0:3;
     reconsider ps=s as Point of TOP-REAL 2 by A255;
     ps in union{ LSeg(f2,i) : 1 <= i & i+1 <= len f2 }
          by A255,TOPREAL1:def 6;
     then consider x such that
     A256: ps in x & x in
     { LSeg(f2,i) : 1 <= i & i+1 <= len f2 } by TARSKI:def 4;
     consider i such that
     A257: x=LSeg(f2,i) &(1 <= i & i+1 <= len f2) by A256;
     A258:LSeg(f2,i)=LSeg(f2/.i,f2/.(i+1)) by A257,TOPREAL1:def 5;
     i<len f2 by A257,NAT_1:13;
     then i in dom f2 by A257,FINSEQ_3:27;
     then consider k being Element of NAT such that
     A259: k in dom h1 & |.f2/.i - h1/.k.|<e by A154;
     A260:|.ps-f2/.i.|<=|.f2/.i-f2/.(i+1).| by A256,A257,A258,JGRAPH_1:53;
     |.f2/.i-f2/.(i+1).|<e by A154,A257;
     then A261:|.ps-f2/.i.|<e by A260,XXREAL_0:2;
     A262:|.ps-h1/.k.|<=|.ps-f2/.i.|+|.f2/.i-h1/.k.| by TOPRNS_1:35;
     |.ps-f2/.i.|+|.f2/.i-h1/.k.|<e+e by A259,A261,XREAL_1:10;
     then A263:|.ps-h1/.k.|<e+e by A262,XXREAL_0:2;
     A264:k in Seg len h1 by A259,FINSEQ_1:def 3;
     then A265:k in dom h by A20,FINSEQ_1:def 3;
     1<=k & k<=len h1 by A264,FINSEQ_1:3;
     then h1.k=h1/.k by FINSEQ_4:24;
     then A266:h1/.k=f.(h.k) by A20,A264;
     A267:h.k in rng h by A265,FUNCT_1:def 5;
     dom f=the carrier of I[01] by FUNCT_2:def 1;
     then A268: h1/.k in P by A1,A19,A266,A267,FUNCT_1:def 5;
     reconsider p11=h1/.k as Point of TOP-REAL 2;
     A269: |.(p11-ps).|<e+e by A263,TOPRNS_1:28;
     ps in union{ LSeg(g2,j) : 1 <= j & j+1 <= len g2 }
       by A255,TOPREAL1:def 6;
     then consider y such that
     A270: ps in y &
     y in { LSeg(g2,j) : 1 <= j & j+1 <= len g2 } by TARSKI:def 4;
     consider j such that
     A271:y=LSeg(g2,j) &(1 <= j & j+1 <= len g2) by A270;
     A272: LSeg(g2,j)=LSeg((g2/.j),g2/.(j+1)) &
      ps in LSeg(g2,j) & y=LSeg(g2,j) &(1 <= j & j+1 <= len g2)
                             by A270,A271,TOPREAL1:def 5;
      j<len g2 & LSeg(g2,j)=LSeg((g2/.j),g2/.(j+1)) &
      ps in LSeg(g2,j) & y=LSeg(g2,j) & (1 <= j & j+1 <= len g2)
        by A270,A271,NAT_1:13,TOPREAL1:def 5;
     then j in dom g2 by FINSEQ_3:27;
     then consider k' being Element of NAT such that
     A273: k' in dom h2 & |.((g2/.j) - h2/.k').|<e by A156;
     A274:|.ps-(g2/.j).|<=|.(g2/.j)-g2/.(j+1).| by A272,JGRAPH_1:53;
     |.(g2/.j)-g2/.(j+1).|<e by A156,A271;
     then A275:|.ps-(g2/.j).|<e by A274,XXREAL_0:2;
     A276:|.ps-h2/.k'.|<=|.ps-(g2/.j).|+|.(g2/.j)-h2/.k'.| by TOPRNS_1:35;
     |.ps-(g2/.j).|+|.(g2/.j)-(h2/.k').|<e+e by A273,A275,XREAL_1:10;
     then A277:|.ps-(h2/.k').|<e+e by A276,XXREAL_0:2;
     k' in Seg len h2 by A273,FINSEQ_1:def 3;
     then k' in dom hb by A70,FINSEQ_1:def 3;
     then A278:hb.k' in rng hb &
     h2/.k'=g.(hb.k') & k' in dom hb by A76,FUNCT_1:def 5;
     dom g =the carrier of I[01] by FUNCT_2:def 1;
     then A279:Q<>{}(TOP-REAL 2) &
     h2/.k' in rng g & hb.k' in dom g by A66,A278,FUNCT_1:def 5;
     reconsider q11=h2/.k' as Point of TOP-REAL 2;
     reconsider x1=p11,x2=q11 as Point of Euclid 2 by EUCLID:25;
     min_dist_min(P',Q')<=dist(x1,x2) by A2,A15,A268,A279,WEIERSTR:40;
     then A280:|.(p11-q11).|<= |.(p11-ps).|+|.(ps-q11).|
       & min_dist_min(P',Q')<=|.(p11-q11).| by JGRAPH_1:45,TOPRNS_1:35;
     |.(p11-ps).|+|.(ps-q11).| <e+e+(e+e) by A269,A277,XREAL_1:10;
     then |.(p11-q11).|<e+e+e+e by A280,XXREAL_0:2;
     then min_dist_min(P',Q')<4*e by A280,XXREAL_0:2;
     then 4*e-5*e>0 by XREAL_1:52; then (4-5)*e/e>0 by A18,REAL_2:127;
