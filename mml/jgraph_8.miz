                           .=(1-ln)*q1`2+(ln)*q1`2 by A229,A230,TOPREAL3:9
                           .=pz`2 by A228,XXREAL_0:1,A218;
          ((1-ln)*p1+ln*p2)`1=((1-ln)*p1)`1+(ln*p2)`1 by TOPREAL3:7
                           .=(1-ln)*p1`1+(ln*p2)`1 by TOPREAL3:9
                           .=(1-ln)*p1`1+ln*p2`1 by TOPREAL3:9
                           .=((p2`1*p1`1-pz`1*p1`1)+(pz`1*p2`1-p1`1*p2`1))/
