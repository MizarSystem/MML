theorem Th19:
cosh.(-p) = cosh.p &
sinh.(-p) = -sinh.p &
tanh.(-p) = -tanh.p
proof
 A1: cosh.(-p) = cosh.p
 proof
   cosh.(-p) = (exp.(-p) + exp.(-(-p)))/2 by Def3
          .= cosh.p by Def3;
 hence thesis;
 end;
 A2: sinh.(-p) = -sinh.p
 proof
   sinh.(-p) = (exp.(-p) - exp.(-(-p)))/2 by Def1
          .= (-(exp.(p) - exp.(-p)))/2 by XCMPLX_1:143
          .= -(exp.(p) - exp.(-p))/2 by XCMPLX_1:188
          .= -sinh.p by Def1;
 hence thesis;
 end;
   tanh.(-p) = -tanh.p
 proof
   tanh.(-p) = (-sinh.p)/(cosh.(-p)) by A2,Th17
          .= -(sinh.p)/(cosh.p) by A1,XCMPLX_1:188
          .= -tanh.p by Th17;
 hence thesis;
 end;
hence thesis by A1,A2;
end;

