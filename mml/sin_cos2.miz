definition
  func tanh -> PartFunc of REAL, REAL means :Def5:
  dom it= REAL &
  for d being real number holds
    it.d=(exp_R.d - exp_R.(-d))/(exp_R.d + exp_R.(-d));
  existence
   proof
    defpred X[set] means $1 in REAL;
    deffunc U(Real) = (exp_R.$1 - exp_R.(-$1))/(exp_R.$1 + exp_R.(-$1));
    consider f being PartFunc of REAL, REAL such that
    A1: for d be Element of REAL holds d in dom f iff X[d] and
    A2: for d be Element of REAL st d in dom f holds f/.d = U(d)
                      from PARTFUN2:sch 2;
    take f;
    thus dom(f)=REAL
     proof
       for x be set st x in REAL holds x in dom f by A1;
      then REAL c=dom(f) by TARSKI:def 3;
      hence dom(f)=REAL by XBOOLE_0:def 10;
     end;
     let d be real number;
   d is Real by XREAL_0:def 1;
       then A3: d in dom f by A1;
       then f/.d = (exp_R.d - exp_R.(-d))/(exp_R.d + exp_R.(-d)) by A2;
        hence f.d = (exp_R.d - exp_R.(-d))/(exp_R.d + exp_R.(-d))
        by A3,FINSEQ_4:def 4;
   end;
  uniqueness
   proof
    let f1,f2 be PartFunc of REAL, REAL;
    assume
    A4: dom f1= REAL & for d being real number holds
        f1.d=(exp_R.(d) - exp_R.(-d))/(exp_R.d + exp_R.(-d));
    assume
    A5: dom f2= REAL & for d being real number holds
        f2.d=(exp_R.(d) - exp_R.(-d))/(exp_R.(d) + exp_R.(-d));
        for d being Element of REAL st d in REAL holds f1.d = f2.d
     proof
      let d be Element of REAL such that
       d in REAL;
      thus f1.d=(exp_R.(d) - exp_R.(-d))/(exp_R.(d) + exp_R.(-d)) by A4
              .=f2.d by A5;
     end;
    hence f1=f2 by A4,A5,PARTFUN1:34;
   end;
 end;

definition let d be number;
  func tanh(d) equals
   tanh.d;
  coherence;
end;

registration let d be number;
