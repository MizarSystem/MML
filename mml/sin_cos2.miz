begin :: Monotone increasing and monotone decreasing of sine and cosine.

 reserve p,q,r,th for real number;
 reserve n for Element of NAT;

Lm1:
0 < PI/2 & PI/2 < PI & PI < 3/2*PI & 3/2*PI < 2*PI
proof
 thus A1: 0 < PI/2
 proof
  PI in ].0, 4.[ by SIN_COS:def 32;
 then 0< PI & PI < 4 by RCOMP_1:47;
 then 0/2 <PI/2 by REAL_1:73;
 hence 0< PI/2;
 end;
 thus A2: PI/2 < PI
 proof
     0 + PI/2 < PI/2 + PI/2 by A1,XREAL_1:8;
 hence PI/2 < PI;
 end;
 thus A3: PI < 3/2*PI
 proof
   PI/2 + PI/2 < PI + PI/2 by A2,XREAL_1:8;
 hence thesis;
 end;
A4: PI + PI/2 = 3/2*PI;
   PI/2 + 3/2*PI = 2*PI;
  hence 3/2*PI < 2*PI by A3,A4,XREAL_1:8;
end;

theorem Th1:
  p>=0 & r>=0 implies p+r>=2*sqrt(p*r)
 proof
 assume A1:p>=0 & r>=0;
 A2:(sqrt p - sqrt r)^2>=0 by XREAL_1:65;
       (sqrt p - sqrt r)^2
  =(sqrt p)^2 - 2*sqrt p*sqrt r + (sqrt r)^2
 .=p-2*sqrt p*sqrt r + (sqrt r)^2 by A1,SQUARE_1:def 4
 .=p -2*sqrt p*sqrt r +r by A1,SQUARE_1:def 4
 .=p+r-2*sqrt p*sqrt r;
 then 0+2*sqrt p*sqrt r <= p+r by A2,XREAL_1:21;
 then 2*(sqrt p*sqrt r) <= p+r;
 hence 2*sqrt(p*r) <= p+r by A1,SQUARE_1:97;
 end;

theorem
   sin is_increasing_on ].0,PI/2.[
proof
A1: sin is_differentiable_on ].0,PI/2.[ by FDIFF_1:34,SIN_COS:73;
    for th be Real st th in ].0,PI/2.[ holds 0 < diff(sin,th)
 proof
 let th be Real; assume
     th in ].0,PI/2.[;
 then cos.th > 0 by SIN_COS:85;
 hence diff(sin,th) > 0 by SIN_COS:73;
 end;
hence thesis by A1,Lm1,ROLLE:9;
end;

Lm2:
for th st th in ].0,PI/2.[ holds 0 < sin.th
proof
let th; assume th in ].0,PI/2.[;
then 0 < th & th < PI/2 by RCOMP_1:47;
then 0-th<0 & -th>-PI/2 by XREAL_1:26,REAL_2:105;
then -th+PI/2 < 0+PI/2 & -th+PI/2 > -PI/2+ PI/2 by XREAL_1:8;
then PI/2-th in ].0,PI/2.[ by RCOMP_1:47;
then cos.(PI/2-th) > 0 by SIN_COS:85;
hence sin.th > 0 by SIN_COS:83;
end;

theorem
   sin is_decreasing_on ].PI/2,PI.[
proof
A1: sin is_differentiable_on ].PI/2,PI.[ by FDIFF_1:34,SIN_COS:73;
    for th1 be Real st th1 in ].PI/2,PI.[ holds 0 > diff(sin,th1)
 proof
 let th1 be Real; assume th1 in ].PI/2,PI.[;
 then PI/2 < th1 & th1 < PI by RCOMP_1:47;
 then PI/2 - PI/2 < th1 - PI/2
 & th1 - PI/2 < PI - PI/2 by XREAL_1:11;
 then A2: th1-PI/2 in ].0,PI/2.[ by RCOMP_1:47;
A3: diff(sin,(th1))=cos.(PI/2+(th1-PI/2)) by SIN_COS:73
               .=-sin.(th1-PI/2) by SIN_COS:83;
     sin.(th1-PI/2) > 0 by A2,Lm2;
 then 0-sin.(th1-PI/2) < 0 by REAL_2:105;
 hence diff(sin,(th1)) < 0 by A3;
 end;
hence thesis by A1,Lm1,ROLLE:10;
end;

theorem
   cos is_decreasing_on ].0,PI/2.[
proof
 A1: cos is_differentiable_on ].0,PI/2.[ by FDIFF_1:34,SIN_COS:72;
     for th be Real st th in ].0,PI/2.[ holds diff(cos,th) < 0
  proof
  let th be Real such that
  A2: th in ].0,PI/2.[;
  A3: diff(cos,th) = -sin.(th) by SIN_COS:72;
    0 < sin.(th) by A2,Lm2;
  then 0-sin.(th) < 0 by REAL_2:105;
  hence diff(cos,th) < 0 by A3;
  end;
hence thesis by A1,Lm1,ROLLE:10;
end;

theorem
   cos is_decreasing_on ].PI/2,PI.[
proof
A1: cos is_differentiable_on ].PI/2,PI.[ by FDIFF_1:34,SIN_COS:72;
     for th be Real st th in ].PI/2,PI.[ holds diff(cos,th) < 0
  proof
  let th be Real; assume th in ].PI/2,PI.[;
   then PI/2 < th & th < PI by RCOMP_1:47;
   then PI/2 - PI/2 < th - PI/2
   & th - PI/2 < PI - PI/2 by XREAL_1:11;
   then A2: th-PI/2 in ].0,PI/2.[ by RCOMP_1:47;
   A3:diff(cos,(th))=-sin.(PI/2+(th-PI/2)) by SIN_COS:72
                 .=-cos.(th-PI/2) by SIN_COS:83;
       cos.(th-PI/2) > 0 by A2,SIN_COS:85;
   then 0-cos.(th-PI/2) < 0 by REAL_2:105;
   hence diff(cos,(th)) < 0 by A3;
  end;
hence thesis by A1,Lm1,ROLLE:10;
end;

theorem
   sin is_decreasing_on ].PI,3/2*PI.[
proof
A1: sin is_differentiable_on ].PI,3/2*PI.[ by FDIFF_1:34,SIN_COS:73;
    for th be Real st th in ].PI,3/2*PI.[ holds diff(sin,(th))<0
 proof
 let th be Real such that
 A2:th in ].PI,3/2*PI.[;
 A3:diff(sin,(th))= cos.(PI+(th-PI)) by SIN_COS:73
             .= -cos.(th-PI) by SIN_COS:83;
     PI < th & th < 3/2*PI by A2,RCOMP_1:47;
 then PI-PI < th-PI & th-PI < 3/2*PI-PI by XREAL_1:11;
 then th-PI in ].0,PI/2.[ by RCOMP_1:47;
 then cos.(th-PI) > 0 by SIN_COS:85;
 then 0-cos.(th-PI) < 0 by REAL_2:105;
 hence diff(sin,(th)) < 0 by A3;
 end;
hence thesis by A1,Lm1,ROLLE:10;
end;

theorem
   sin is_increasing_on ].3/2*PI,2*PI.[
proof
A1: sin is_differentiable_on ].3/2*PI,2*PI.[ by FDIFF_1:34,SIN_COS:73;
    for th be Real st th in ].3/2*PI,2*PI.[ holds diff(sin,(th))>0
 proof
 let th be Real such that
 A2:th in ].3/2*PI,2*PI.[;
 A3:diff(sin,(th)) = cos.(PI + (PI/2+(th-3/2*PI))) by SIN_COS:73
             .= -cos.(PI/2+(th-3/2*PI)) by SIN_COS:83
             .= -(-sin.(th-3/2*PI)) by SIN_COS:83
             .= sin.(th-3/2*PI);
     3/2*PI < th & th < 2*PI by A2,RCOMP_1:47;
 then 3/2*PI-3/2*PI < th-3/2*PI
   & th-3/2*PI < 2*PI-3/2*PI by XREAL_1:11;
 then th-3/2*PI in ].0,PI/2.[ by RCOMP_1:47;
 hence diff(sin,(th)) > 0 by A3,Lm2;
 end;
hence thesis by A1,Lm1,ROLLE:9;
end;

theorem
   cos is_increasing_on ].PI,3/2*PI.[
proof
A1: cos is_differentiable_on ].PI,3/2*PI.[ by FDIFF_1:34,SIN_COS:72;
    for th be Real st th in ].PI,3/2*PI.[ holds diff(cos,(th))>0
 proof
 let th be Real such that
 A2:th in ].PI,3/2*PI.[;
 A3:diff(cos,(th)) = -sin.(PI+(th-PI)) by SIN_COS:72
             .= -(-sin.(th-PI)) by SIN_COS:83
             .= sin.(th-PI);
     PI < th & th < 3/2*PI by A2,RCOMP_1:47;
 then PI-PI < th-PI & th-PI < 3/2*PI-PI by XREAL_1:11;
 then th-PI in ].0,PI/2.[ by RCOMP_1:47;
 hence diff(cos,(th)) > 0 by A3,Lm2;
end;
hence thesis by A1,Lm1,ROLLE:9;
end;

theorem
    cos is_increasing_on ].3/2*PI,2*PI.[
proof
A1: cos is_differentiable_on ].3/2*PI,2*PI.[ by FDIFF_1:34,SIN_COS:72;
    for th be Real st th in ].3/2*PI,2*PI.[ holds diff(cos,(th))>0
 proof
 let th be Real such that
 A2:th in ].3/2*PI,2*PI.[;
 A3: diff(cos,(th)) = -sin.(PI + (PI/2+(th-3/2*PI))) by SIN_COS:72
               .= -(-sin.(PI/2+(th-3/2*PI))) by SIN_COS:83
               .= cos.(th-3/2*PI) by SIN_COS:83;
     3/2*PI < th & th < 2*PI by A2,RCOMP_1:47;
 then 3/2*PI-3/2*PI < th-3/2*PI
   & th-3/2*PI < 2*PI-3/2*PI by XREAL_1:11;
 then th-3/2*PI in ].0,PI/2.[ by RCOMP_1:47;
 hence diff(cos,(th)) > 0 by A3,SIN_COS:85;
 end;
hence thesis by A1,Lm1,ROLLE:9;
end;

theorem
  for n being Nat holds sin.th = sin.(2*PI*n + th)
proof let n be Nat;
  defpred X[Nat] means for th holds sin.th = sin.(2*PI*$1 + th);
A1: X[0];
A2:for n being Nat st X[n] holds X[n+1]
 proof
 let n be Nat such that
 A3:for th holds sin.th = sin.(2*PI*n + th);
    for th holds sin.th = sin.(2*PI*(n+1) + th)
  proof
  let th;
      sin.(2*PI*(n+1) + th) = sin.((2*PI*n+th) + 2*PI)
                        .= sin.(2*PI*n + th) by SIN_COS:83
                        .= sin.th by A3;
  hence thesis;
  end;
  hence thesis;
 end;
   for n being Nat holds X[n] from NAT_1:sch 2(A1,A2);
 hence thesis;
end;

theorem
   for n being Nat holds cos.th = cos.(2*PI*n + th)
proof let n be Nat;
 defpred X[Nat] means for th holds cos.th = cos.(2*PI*$1 + th);
A1: X[0];
A2: for n being Nat st X[n] holds X[n+1]
proof
 let n be Nat such that
 A3:for th holds cos.th = cos.(2*PI*n + th);
    for th holds cos.th = cos.(2*PI*(n+1) + th)
  proof
  let th;
      cos.(2*PI*(n+1) + th) = cos.((2*PI*n+th) + 2*PI)
                        .= cos.(2*PI*n + th) by SIN_COS:83
                        .= cos.th by A3;
  hence thesis;
  end;
  hence thesis;
 end;
   for n being Nat holds X[n] from NAT_1:sch 2(A1,A2);
 hence thesis;
end;

begin ::Hyperbolic sine, hyperbolic cosine and hyperbolic tangent.

definition
  func sinh -> PartFunc of REAL, REAL means :Def1:
  dom it= REAL & for d being real number holds it.d=(exp_R.d - exp_R.(-d))/2;
  existence
  proof
    defpred X[set] means $1 in REAL;
    deffunc U(Real) = (exp_R.($1) - exp_R.(-$1))/2;
   consider f being PartFunc of REAL, REAL such that
   A1: for d be Element of REAL holds d in dom f iff X[d] and
   A2: for d be Element of REAL st d in dom f holds f/.d = U(d) from
PARTFUN2:sch 2;
   take f;
   thus dom(f)=REAL
    proof
      for x be set st x in REAL holds x in dom f by A1;
     then REAL c=dom(f) by TARSKI:def 3;
     hence dom(f)=REAL by XBOOLE_0:def 10;
    end;
     let d be real number;
    d is Real by XREAL_0:def 1;
      then A3: d in dom f by A1;
      then f/.d = (exp_R.d - exp_R.(-d))/2 by A2;
      hence f.d = (exp_R.d - exp_R.(-d))/2 by A3,FINSEQ_4:def 4;
    end;
 uniqueness
  proof
   let f1,f2 be PartFunc of REAL, REAL;
   assume
   A4: dom f1= REAL & for d being real number holds
   f1.d=(exp_R.d - exp_R.(-d))/2;
   assume
   A5: dom f2= REAL & for d being real number holds
   f2.d=(exp_R.d - exp_R.(-d))/2;
       for d being Element of REAL st d in REAL holds f1.d = f2.d
    proof
     let d be Element of REAL such that
         d in REAL;
     thus f1.d=(exp_R.d - exp_R.(-d))/2 by A4
             .=f2.d by A5;
    end;
   hence f1=f2 by A4,A5,PARTFUN1:34;
  end;
 end;

definition let d be number;
  func sinh(d) equals
   sinh.d;
  coherence;
end;

registration let d be number;
  cluster sinh(d) -> real;
  coherence;
end;

definition let d be number;
  redefine func sinh(d) -> Real;
  coherence;
end;

definition
  func cosh -> PartFunc of REAL, REAL means :Def3:
  dom it = REAL & for d being real number holds it.d=(exp_R.d + exp_R.(-d))/2;
  existence
  proof
    defpred X[set] means $1 in REAL;
    deffunc U(Real) = (exp_R.$1 + exp_R.(-$1))/2;
   consider f being PartFunc of REAL, REAL such that
   A1:   for d be Element of REAL holds d in dom f iff X[d] and
   A2:   for d be Element of REAL st d in dom f holds f/.d = U(d)
    from PARTFUN2:sch 2;
   take f;
   thus dom(f)=REAL
    proof
      for x be set st x in REAL holds x in dom f by A1;
     then REAL c=dom(f) by TARSKI:def 3;
     hence dom(f)=REAL by XBOOLE_0:def 10;
    end;
    let d be real number;
   d is Real by XREAL_0:def 1;
      then A3: d in dom f by A1;
      then f/.d = (exp_R.d + exp_R.(-d))/2 by A2;
     hence f.d = (exp_R.d + exp_R.(-d))/2 by A3,FINSEQ_4:def 4;
  end;
 uniqueness
  proof
   let f1,f2 be PartFunc of REAL, REAL;
   assume
   A4: dom f1= REAL & for d being real number holds
   f1.d=(exp_R.d + exp_R.(-d))/2;
   assume
   A5: dom f2= REAL & for d being real number holds
   f2.d=(exp_R.d + exp_R.(-d))/2;
     for d being Element of REAL st d in REAL holds f1.d = f2.d
    proof
     let d be Element of REAL such that
      d in REAL;
     thus f1.d=(exp_R.d + exp_R.(-d))/2 by A4
             .=f2.d by A5;
    end;
   hence f1=f2 by A4,A5,PARTFUN1:34;
  end;
 end;

definition let d be number;
  func cosh(d) equals
