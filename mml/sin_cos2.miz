theorem Th30:
dom sinh=REAL & dom cosh=REAL & dom tanh=REAL by Def1,Def3,Def5;

 Lm11:for d being real number holds compreal.d=(-1)*d
 proof
   let d be real number;
       d is Real by XREAL_0:def 1;
   hence compreal.d= -d by BINOP_2:def 7
   .= (-1)*d;
 end;

 Lm12:
 dom(compreal)=REAL & rng(compreal)c=REAL by FUNCT_2:def 1;

  Lm13:
 for f being PartFunc of REAL,REAL st f = compreal holds
 for p holds f is_differentiable_in p & diff(f,p) = -1
 proof
     let f be PartFunc of REAL,REAL such that
    A1:f = compreal;
     let p;
    defpred X[set] means $1 in REAL;
    deffunc U(Real) = -$1;
    consider f1 being PartFunc of REAL,REAL such that
    A2:for p be Element of REAL holds p in dom f1 iff X[p] and
    A3:for p be Element of REAL st p in dom f1 holds f1/.p=U(p) from
PARTFUN2:sch 2;
    A4:f1 is LINEAR
     proof
         dom f1 = REAL
      proof
            for x be set st x in REAL holds x in dom f1 by A2;
        then REAL c=dom f1 by TARSKI:def 3;
       hence dom f1=REAL by XBOOLE_0:def 10;
      end;
    then A5:f1 is total by PARTFUN1:def 4;
    A6: for p be Element of REAL holds
          f1.p = - p
         proof
         let p be Element of REAL;
         A7:  p in dom f1 by A2;
         then f1/.p = -p by A3;
         hence f1.p = -p by A7,FINSEQ_4:def 4;
         end;
        for p be Real holds f1.p=(-1)*p
     proof
      let p be Real;
         f1.p = -p by A6;
      hence f1.p = (-1)*p;
      end;
    hence f1 is LINEAR by A5,FDIFF_1:def 4;
    end;
    reconsider f2 = REAL --> 0 as PartFunc of REAL,REAL by FUNCOP_1:57;
       A8: dom f2 = REAL by FUNCOP_1:19;
   A9:f2 is REST
    proof
  A10:f2 is total by PARTFUN1:def 4,A8;
      f2 is REST-like PartFunc of REAL,REAL
   proof
       for hy1 be convergent_to_0 Real_Sequence holds
          (hy1")(#)(f2*hy1) is convergent & lim ((hy1")(#)(f2*hy1)) = 0
    proof
    let hy1 be convergent_to_0 Real_Sequence;
    A11: for n holds ((hy1")(#)(f2*hy1)).n=0
      proof
      let n;
      A12: ((hy1")(#)(f2*hy1)).n
         = (hy1".n)*((f2*hy1).n) by SEQ_1:12
        .= (hy1.n)"*((f2*hy1).n) by SEQ_1:def 8;
           rng hy1 c= dom f2 by A8;
      then ((hy1")(#)(f2*hy1)).n
         =(hy1.n)"*(f2.(hy1.n)) by A12,RFUNCT_2:21
        .=(hy1.n)"*(0) by FUNCOP_1:13
        .= 0;
      hence thesis;
      end;
   then A13:(hy1")(#)(f2*hy1) is constant by SEQM_3:def 6;
     for n holds lim ((hy1")(#)(f2*hy1)) = 0
    proof
    let n;
          lim ((hy1")(#)(f2*hy1))
     =((hy1")(#)(f2*hy1)).n by A13,SEQ_4:41
    .=0 by A11;
    hence lim ((hy1")(#)(f2*hy1)) = 0;
    end;
   hence thesis by A13,SEQ_4:39;
   end;
   hence thesis by A10,FDIFF_1:def 3;
   end;
   hence thesis;
   end;
  A14: ex N being Neighbourhood of p st N c= dom compreal &
   for r be Real st r in N holds compreal.r - compreal.p = f1.(r-p) + f2.(r-p)
     proof
       A15: for r st r in ].p-1,p+1 .[ holds compreal.r - compreal.p =
          f1.(r-p) + f2.(r-p)
      proof
       let r;
         A16: for p be real number holds f1.p = - p
          proof
          let p be real number;
        p is Real by XREAL_0:def 1;
          then A17:  p in dom f1 by A2;
          then f1/.p = -p by A3;
          hence f1.p = -p by A17,FINSEQ_4:def 4;
          end;
         A18: for d be real number holds f2.d = 0
          proof
          let d be real number;
           d is Real by XREAL_0:def 1;
          hence f2.d = 0 by FUNCOP_1:13;
          end;
          compreal.r-compreal.p
        =(-1)*r - compreal.p by Lm11
       .=(-1)*r -(-1)*p + 0 by Lm11
       .=-(r-p) + f2.(r-p) by A18
       .=f1.(r-p) + f2.(r-p) by A16;
       hence thesis;
       end;
     take ].p-1,p+1 .[;
     thus thesis by A15,Lm12,RCOMP_1:def 7;
    end;
   then A19:f is_differentiable_in p by A1,A4,A9,FDIFF_1:def 5;
   A20: for p be Element of REAL holds f1.p = - p
           proof
            let p be Element of REAL;
            A21:  p in dom f1 by A2;
            then f1/.p = -p by A3;
            hence f1.p = -p by A21,FINSEQ_4:def 4;
          end;
       diff(f,p) = f1.1 by A1,A4,A9,A14,A19,FDIFF_1:def 6
             .=-1 by A20;
   hence thesis by A1,A4,A9,A14,FDIFF_1:def 5;
end;

Lm14:
 for f being PartFunc of REAL,REAL st f = compreal holds
 exp_R*f is_differentiable_in p &
 diff(exp_R*f,p) = (-1)*exp_R.(f.p)
 proof
  let f be PartFunc of REAL,REAL such that
A1: f = compreal;
A2: p is Real by XREAL_0:def 1;
 A3: exp_R is_differentiable_in f.p by SIN_COS:70;
 A4: f is_differentiable_in p by A1,Lm13;
   then diff(exp_R*f,p)
  =diff(exp_R,f.p)*diff(f,p) by A2,A3,FDIFF_2:13
 .=diff(exp_R,f.p)*(-1) by A1,Lm13
 .=exp_R.(f.p)*(-1) by SIN_COS:70;
 hence thesis by A2,A3,A4,FDIFF_2:13;
 end;

 Lm15:
for f being PartFunc of REAL,REAL st f = compreal holds
  exp_R.((-1)*p)= (exp_R*f).p
 proof
  let f be PartFunc of REAL,REAL;
   assume A1:f = compreal;
A2: p is Real by XREAL_0:def 1;
  exp_R.((-1)*p) =exp_R.(f.p) by A1,Lm11
 .=(exp_R*f).p by A1,A2,FUNCT_2:70;
 hence thesis;
 end;

 Lm16:
 for f being PartFunc of REAL,REAL st f = compreal holds
 (exp_R-(exp_R*f)) is_differentiable_in p &
 (exp_R+(exp_R*f)) is_differentiable_in p &
 diff(exp_R-(exp_R*f),p)=exp_R.p+exp_R.(-p) &
 diff(exp_R+(exp_R*f),p)=exp_R.p-exp_R.(-p)
 proof
A1: p is Real by XREAL_0:def 1;
  let f be PartFunc of REAL,REAL;
  assume
A2:  f = compreal;
  then A3: exp_R is_differentiable_in p & exp_R*f is_differentiable_in p
    by Lm14,SIN_COS:70;
  then A4:
    diff(exp_R-(exp_R*f),p)
   =diff(exp_R,p)-diff((exp_R*f),p) by A1,FDIFF_1:22
  .=exp_R.p-diff((exp_R*f),p) by SIN_COS:70
  .=exp_R.p- (-1)*exp_R.(f.p) by A2,Lm14
  .=exp_R.p- (-1)*exp_R.((-1)*p) by A2,Lm11
  .=exp_R.p+exp_R.(-p);
      diff(exp_R+(exp_R*f),p)
   =diff(exp_R,p)+diff((exp_R*f),p) by A1,A3,FDIFF_1:21
  .=exp_R.p+diff((exp_R*f),p) by SIN_COS:70
  .=exp_R.p+ (-1)*exp_R.(f.p) by A2,Lm14
  .=exp_R.p+ (-1)*exp_R.((-1)*p) by A2,Lm11
  .=exp_R.p-exp_R.(-p);
  hence thesis by A1,A3,A4,FDIFF_1:21,22;
 end;

Lm17:
 for f being PartFunc of REAL,REAL st f = compreal holds
 (1/2)(#)(exp_R-exp_R*f) is_differentiable_in p &
 diff(((1/2)(#)(exp_R-exp_R*f)),p) = (1/2)*diff((exp_R-(exp_R*f)),p)
 proof
A1: p is Real by XREAL_0:def 1;
  let f be PartFunc of REAL,REAL;
  assume f = compreal;
  then (exp_R-exp_R*f) is_differentiable_in p by Lm16;
  hence thesis by A1,FDIFF_1:23;
 end;

Lm18:
 for ff being PartFunc of REAL,REAL st ff = compreal holds
  sinh.p=((1/2)(#)(exp_R-exp_R*ff)).p
 proof
A1: p is Real by XREAL_0:def 1;
  let ff be PartFunc of REAL,REAL such that
A2:  ff = compreal;
 A3: dom (exp_R - exp_R*ff) = dom exp_R /\ dom (exp_R*ff) &
  for p be Real st p in dom (exp_R - exp_R*ff) holds
  exp_R.p - (exp_R*ff).p = (exp_R - exp_R*ff).p by SEQ_1:def 4;
 A4:dom (exp_R - exp_R*ff) = REAL
  proof
      dom (exp_R*ff) = REAL by A2,Lm12,RELAT_1:46,SIN_COS:51;
     hence thesis by A3,SIN_COS:51;
  end;
 then A5:dom((1/2)(#)(exp_R-exp_R*ff))=REAL by SEQ_1:def 6;
    sinh.p = (exp_R.p - exp_R.(-p))/2 by Def1
 .=(1/2)*(exp_R.p - exp_R.((-1)*p))
 .=(1/2)*(exp_R.p - (exp_R*ff).p) by A2,Lm15
 .=(1/2)*((exp_R - exp_R*ff).p) by A1,A4,SEQ_1:def 4
 .=((1/2)(#)(exp_R-exp_R*ff)).p by A1,A5,SEQ_1:def 6;
 hence thesis;
 end;

 Lm19:
 for ff being PartFunc of REAL,REAL st ff = compreal holds
 sinh = (1/2)(#)(exp_R-exp_R*ff)
 proof
   let ff be PartFunc of REAL,REAL such that
A1:   ff = compreal;
   A2: dom (exp_R - exp_R*ff) = dom exp_R /\ dom (exp_R*ff) &
    for p be Real st p in dom (exp_R - exp_R*ff) holds
    exp_R.p - (exp_R*ff).p = (exp_R - exp_R*ff).p by SEQ_1:def 4;
      dom (exp_R - exp_R*ff) = REAL
    proof
     dom (exp_R*ff) = REAL by A1,Lm12,RELAT_1:46,SIN_COS:51;
    hence thesis by A2,SIN_COS:51;
    end;
   then A3:REAL=dom(sinh) &
     REAL=dom((1/2)(#)(exp_R-exp_R*ff)) by Def1,SEQ_1:def 6;
      for p being Element of REAL st p in REAL holds
     sinh.p=((1/2)(#)(exp_R-exp_R*ff)).p by A1,Lm18;
      hence sinh=(1/2)(#)(exp_R-exp_R*ff) by A3,PARTFUN1:34;
 thus thesis;
 end;

 theorem Th31:
 sinh is_differentiable_in p & diff(sinh,p)=cosh.p
 proof
   reconsider ff = compreal as PartFunc of REAL,REAL;
A1:sinh = (1/2)(#)(exp_R-exp_R*ff) by Lm19;
    diff(sinh,p) =diff( ((1/2)(#)(exp_R-exp_R*ff)),p ) by Lm19
 .=(1/2)*diff((exp_R-(exp_R*ff)),p) by Lm17
 .=(1/2)*( exp_R.p+exp_R.(-p) ) by Lm16
 .=( exp_R.p+exp_R.(-p) )/2
 .=cosh.p by Def3;
 hence thesis by A1,Lm17;
 end;

 Lm20:
 for ff being PartFunc of REAL,REAL st ff = compreal holds
  (1/2)(#)(exp_R+exp_R*ff) is_differentiable_in p &
  diff(((1/2)(#)(exp_R+exp_R*ff)),p) = (1/2)*diff((exp_R+(exp_R*ff)),p)
  proof
A1: p is Real by XREAL_0:def 1;
   let ff be PartFunc of REAL,REAL; assume ff = compreal;
  then (exp_R+exp_R*ff) is_differentiable_in p by Lm16;
  hence thesis by A1,FDIFF_1:23;
  end;

  Lm21:
   for ff being PartFunc of REAL,REAL st ff = compreal holds
   cosh.p=((1/2)(#)(exp_R+exp_R*ff)).p
  proof
A1: p is Real by XREAL_0:def 1;
   let ff be PartFunc of REAL,REAL such that
A2:   ff = compreal;
A3: dom (exp_R + exp_R*ff) = dom exp_R /\ dom (exp_R*ff) &
   for p be Real st p in dom (exp_R + exp_R*ff) holds
   exp_R.p + (exp_R*ff).p = (exp_R + exp_R*ff).p by SEQ_1:def 3;
   A4:dom (exp_R + exp_R*ff) = REAL
   proof
       dom (exp_R*ff) = REAL by A2,Lm12,RELAT_1:46,SIN_COS:51;
   hence thesis by A3,SIN_COS:51;
   end;
  then A5:dom((1/2)(#)(exp_R+exp_R*ff))=REAL by SEQ_1:def 6;
    cosh.p =(exp_R.p + exp_R.(-p))/2 by Def3
  .=(1/2)*(exp_R.p + exp_R.((-1)*p))
  .=(1/2)*(exp_R.p + (exp_R*ff).p) by A2,Lm15
  .=(1/2)*((exp_R + exp_R*ff).p) by A1,A4,SEQ_1:def 3
  .=((1/2)(#)(exp_R+exp_R*ff)).p by A1,A5,SEQ_1:def 6;
  hence thesis;
  end;

  Lm22:
  for ff being PartFunc of REAL,REAL st ff = compreal holds
  cosh = (1/2)(#)(exp_R+exp_R*ff)
  proof
   let ff be PartFunc of REAL,REAL such that
   A1: ff = compreal;
   A2: dom (exp_R + exp_R*ff) = dom exp_R /\ dom (exp_R*ff) &
     for p be Real st p in dom (exp_R + exp_R*ff) holds
    exp_R.p + (exp_R*ff).p = (exp_R + exp_R*ff).p by SEQ_1:def 3;
       dom (exp_R + exp_R*ff) = REAL
     proof
      dom (exp_R*ff) = REAL by A1,Lm12,RELAT_1:46,SIN_COS:51;
     hence thesis by A2,SIN_COS:51;
     end;
   then A3:REAL=dom(cosh) & REAL=dom((1/2)(#)(exp_R+exp_R*ff))
     by Def3,SEQ_1:def 6;
       for p being Element of REAL st p in REAL holds
      cosh.p=((1/2)(#)(exp_R+exp_R*ff)).p by A1,Lm21;
    hence cosh=(1/2)(#)(exp_R+exp_R*ff) by A3,PARTFUN1:34;
    end;

theorem Th32:
  cosh is_differentiable_in p & diff(cosh,p)=sinh.p
  proof
   reconsider ff = compreal as PartFunc of REAL,REAL;
A1: cosh = (1/2)(#)(exp_R+exp_R*ff) by Lm22;
    diff(cosh,p) =diff( ((1/2)(#)(exp_R+exp_R*ff)),p ) by Lm22
  .=(1/2)*diff((exp_R+(exp_R*ff)),p) by Lm20
  .=(1/2)*( exp_R.p-exp_R.(-p) ) by Lm16
  .=( exp_R.p-exp_R.(-p) )/2
  .=sinh.p by Def1;
  hence thesis by A1,Lm20;
  end;

 Lm23:
  sinh/cosh is_differentiable_in p &
  diff(sinh/cosh,p) = 1/(cosh.p)^2
  proof
A1: p is Real by XREAL_0:def 1;
  A2:cosh.p<>0 by Th15;
  A3:sinh is_differentiable_in p by Th31;
  A4:cosh is_differentiable_in p by Th32;
    then diff(sinh/cosh,p)
   =(diff(sinh,p) * cosh.p - diff(cosh,p)*sinh.p)/(cosh.p)^2
        by A1,A2,A3,FDIFF_2:14
  .=((cosh.p)*(cosh.p) - diff(cosh,p) * sinh.p)/(cosh.p)^2 by Th31
  .=( (cosh.p)^2 - (sinh.p)*(sinh.p) )/(cosh.p)^2 by Th32
  .=1/(cosh.p)^2 by Th14;
   hence thesis by A1,A2,A3,A4,FDIFF_2:14;
  end;

Lm24: tanh=sinh/cosh
  proof
  A1: dom (sinh/cosh) = dom sinh /\ (dom cosh \ cosh"{0}) &
       for p be Real st p in dom (sinh/cosh) holds
       (sinh/cosh).p = sinh.p*(cosh.p)" by RFUNCT_1:def 4;
      not(0 in rng(cosh))
    proof
    assume 0 in rng(cosh);
    then ex p be Real st p in dom(cosh) & 0=cosh.p by PARTFUN1:26;
    hence contradiction by Th15;
    end;
 then A2: cosh"{0}={} by FUNCT_1:142;
     for p being Element of REAL st p in REAL holds
     tanh.p=(sinh/cosh).p
   proof
   let p;
    p is Real by XREAL_0:def 1;
   then (sinh/cosh).p =sinh.p*(cosh.p)" by A1,A2,Th30
   .=(sinh.p)/(cosh.p) by XCMPLX_0:def 9
   .=tanh.p by Th17;
   hence thesis;
   end;
  hence tanh=sinh/cosh by A1,A2,Th30,PARTFUN1:34;
 end;

theorem
   tanh is_differentiable_in p & diff(tanh,p)=1/(cosh.p)^2 by Lm23,Lm24;

theorem Th34:
  sinh is_differentiable_on REAL &
  diff(sinh,p)=cosh.p
  proof
      REAL is open Subset of REAL &
  REAL c= dom(sinh) &
  for p be Real st p in REAL holds sinh is_differentiable_in p
     by Def1,Th31,FCONT_3:4;
  hence thesis by Th31,FDIFF_1:16;
  end;

theorem Th35:
  cosh is_differentiable_on REAL &
  diff(cosh,p)=sinh.p
  proof
    REAL is open Subset of REAL &
  REAL c=dom cosh &
  for r be Real st r in REAL holds cosh is_differentiable_in r
  by Def3,Th32,FCONT_3:4;
  hence thesis by Th32,FDIFF_1:16;
  end;

theorem Th36:
  tanh is_differentiable_on REAL &
  diff(tanh,p)=1/(cosh.p)^2
  proof
      REAL is open Subset of REAL &
  for p be Real st p in REAL holds tanh is_differentiable_in p
     by Lm23,Lm24,FCONT_3:4;
   hence thesis by Lm23,Lm24,Th30,FDIFF_1:16;
  end;

 Lm25:
 exp_R.p + exp_R.-p >= 2
 proof
 A1:exp_R.p >= 0 by SIN_COS:59;
 A2: exp_R.-p>= 0 by SIN_COS:59;
       2*sqrt((exp_R.p) * (exp_R.-p))
  =2*sqrt(exp_R.(p+-p)) by Th12
 .=2*1 by SIN_COS:56,def 27,SQUARE_1:83;
 hence thesis by A1,A2,Th1;
 end;

theorem
   cosh.p >= 1
 proof
  (exp_R.p + exp_R.-p)>=2 by Lm25;
 then (exp_R.p + exp_R.-p)/2 >= 2/2 by REAL_1:73;
 hence thesis by Def3;
 end;

theorem
   sinh is_continuous_in p
 proof
      sinh is_differentiable_in p by Th31;
 hence thesis by FDIFF_1:32;
 end;

theorem
   cosh is_continuous_in p
 proof
     cosh is_differentiable_in p by Th32;
 hence thesis by FDIFF_1:32;
 end;

theorem
   tanh is_continuous_in p
 proof
     tanh is_differentiable_in p by Lm23,Lm24;
 hence thesis by FDIFF_1:32;
 end;

theorem
   sinh is_continuous_on REAL by Th34,FDIFF_1:33;

theorem
   cosh is_continuous_on REAL by Th35,FDIFF_1:33;

theorem
   tanh is_continuous_on REAL by Th36,FDIFF_1:33;

theorem
   tanh.p<1 & tanh.p>-1
 proof
  thus tanh.p<1
  proof
  assume tanh.p>=1;
  then A1: (exp_R.p - exp_R.(-p))/(exp_R.p + exp_R.(-p))>=1 by Def5;
      (exp_R.p + exp_R.-p) >= 2 by Lm25;
  then A2:(exp_R.p - exp_R.(-p))/(exp_R.p + exp_R.(-p))*(exp_R.p + exp_R.(-p))
                    >=1*(exp_R.p + exp_R.(-p)) by A1,XREAL_1:66;
      exp_R.p > 0 & exp_R.(-p) > 0 by SIN_COS:59;
  then exp_R.p + exp_R.(-p) > 0+0 by XREAL_1:10;
  then (exp_R.p - exp_R.(-p))/(exp_R.p + exp_R.(-p))*(exp_R.p + exp_R.(-p))
       =exp_R.p - exp_R.(-p) by XCMPLX_1:88;
  then (exp_R.p - exp_R.(-p))-exp_R.p
        >= (exp_R.p + exp_R.(-p))-exp_R.p by A2,XREAL_1:11;
  then A3: -exp_R.(-p)+exp_R.(-p) >= exp_R.(-p)+exp_R.(-p) by XREAL_1:8;
      exp_R.(-p) > 0 & exp_R.(-p) > 0 by SIN_COS:59;
  then exp_R.(-p) + exp_R.(-p) > 0+0 by XREAL_1:10;
  hence contradiction by A3;
  end;
  assume tanh.p<=-1;
  then A4: (exp_R.p - exp_R.(-p))/(exp_R.p + exp_R.(-p))<=-1 by Def5;
      (exp_R.p + exp_R.-p) >= 2 by Lm25;
  then A5:(exp_R.p - exp_R.(-p))/(exp_R.p + exp_R.(-p))*(exp_R.p + exp_R.(-p))
                    <=(-1)*(exp_R.p + exp_R.(-p)) by A4,XREAL_1:66;
      exp_R.p > 0 & exp_R.(-p) > 0 by SIN_COS:59;
  then exp_R.p + exp_R.(-p) > 0+0 by XREAL_1:10;
  then exp_R.p - exp_R.(-p)<=-(exp_R.p + exp_R.(-p)) by A5,XCMPLX_1:88;
  then exp_R.p - exp_R.(-p)+exp_R.(-p)
           <=-exp_R.p + -exp_R.(-p)+exp_R.(-p) by XREAL_1:8;
  then A6: exp_R.p+exp_R.p<=-exp_R.p+exp_R.p by XREAL_1:8;
      exp_R.p > 0 & exp_R.p > 0 by SIN_COS:59;
  then exp_R.p + exp_R.p > 0+0 by XREAL_1:10;
  hence contradiction by A6;
  end;
