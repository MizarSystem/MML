   end;
   case max_p f>=len f;
     then A9: max_p f=2 by A2,A3,AXIOMS:21;
       1 in dom f by A2,FINSEQ_3:27;
     then A10: r1<>r2 by A2,A9,Def1;
       max f <=max(r1,r2) by A2,A7,A9,SQUARE_1:46;
     then max f=max(r1,r2) by A6,AXIOMS:21;
    hence max f=max(r1,r2) & max_p f=IFEQ(r1,max(r1,r2),1,2)
                      by A2,A7,A9,A10,CQC_LANG:def 1;
   end;
   end;
   hence max f=max(r1,r2) & max_p f=IFEQ(r1,max(r1,r2),1,2);
  end;
  case r1<r2;
   then A11: max(r1,r2)<=max f by A2,A5,SQUARE_1:def 2;
   A12: max f=f.(max_p f) by Def3;
     now per cases;
   case max_p f<len f; then max_p f <1+1 by A1,FINSEQ_1:61;
     then max_p f <=1 by NAT_1:38;
     then A13: max_p f=1 by A3,AXIOMS:21;
      then max f <=max(r1,r2) by A2,A12,SQUARE_1:46;
     then max f=max(r1,r2) by A11,AXIOMS:21;
    hence max f=max(r1,r2) & max_p f=IFEQ(r1,max(r1,r2),1,2)
                       by A2,A12,A13,CQC_LANG:def 1;
   end;
   case max_p f>=len f;
     then A14: max_p f=2 by A2,A3,AXIOMS:21;
       1 in dom f by A2,FINSEQ_3:27;
     then A15: r1<>r2 by A2,A14,Def1;
       max f <=max(r1,r2) by A2,A12,A14,SQUARE_1:46;
     then max f=max(r1,r2) by A11,AXIOMS:21;
    hence max f=max(r1,r2) & max_p f=IFEQ(r1,max(r1,r2),1,2)
                        by A2,A12,A14,A15,CQC_LANG:def 1;
   end;
   end;
   hence max f=max(r1,r2) & max_p f=IFEQ(r1,max(r1,r2),1,2);
  end;
  end;
 hence thesis;
end;

theorem   for f being FinSequence of REAL,r1,r2 being Real
