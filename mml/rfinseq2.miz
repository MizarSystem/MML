environ

 vocabularies RELAT_1, CARD_1, FUNCT_1, BOOLE, FUNCT_2, FINSEQ_1, ARYTM_1,
      SQUARE_1, PROB_1, RFINSEQ, RFINSEQ2, CQC_LANG;
 notations TARSKI, XBOOLE_0, NUMBERS, XREAL_0, RELAT_1, FUNCT_1, FUNCT_2,
      FINSEQ_1, CARD_1, REAL_1, NAT_1, SQUARE_1, SEQ_1, RVSUM_1, CQC_LANG,
      RFINSEQ, INTEGRA2;
 constructors REAL_1, NAT_1, SQUARE_1, SEQ_1, CQC_LANG, RFINSEQ, INTEGRA2,
      BINOP_2;
 registrations NUMBERS, FUNCT_1, RELSET_1, FINSEQ_1, XREAL_0, FUNCT_2, REAL_1,
      NAT_1, RFINSEQ, INTEGRA2, MEMBERED;
 requirements REAL, NUMERALS, BOOLE, SUBSET, ARITHM;
 definitions INTEGRA2;
 theorems AXIOMS, TARSKI, FINSEQ_1, FINSEQ_2, FINSEQ_3, REAL_1, NAT_1, FUNCT_1,
      FUNCT_2, SQUARE_1, RVSUM_1, RELAT_1, XCMPLX_1, CQC_LANG, EUCLID_2,
      RFINSEQ, XBOOLE_0, INTEGRA2;
 schemes NAT_1;

begin

reserve n,m for Nat;

definition let f be FinSequence of REAL;
func max_p f -> Nat means
 :Def1: (len f=0 implies it=0) &
 (len f>0 implies
 it in dom f &
 (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.it
 holds r1<=r2) & (for j being Nat st j in dom f & f.j=f.it holds it<=j));
 existence
 proof
  A1: dom f=Seg len f by FINSEQ_1:def 3;
  per cases;
  suppose len f=0;
  hence ex n being Nat st
 (len f=0 implies n=0) & (len f>0 implies n in dom f &
 (for i be Nat,r1,r2 be Real st i in dom f & r1=f.i & r2=f.n
 holds r1<=r2) & (for j being Nat st j in dom f & f.j=f.n holds n<=j));
  end;
  suppose A2: len f<>0;
  defpred P[Nat] means (ex n being Nat st ($1<>0 implies n<=$1 &
   n in dom f) &
   (for i being Nat,r1,r2 being Real st i<=$1 & i in dom f
   & r1=f.i & r2=f.n holds r1<=r2) &
   (for j being Nat st j<=$1 & j in dom f & f.j=f.n holds n<=j));
    A3: for i being Nat,r1,r2 being Real st i<=0 & i in dom f
     & r1=f.i & r2=f.1 holds r1<=r2 by A1,FINSEQ_1:3;
      for j being Nat st j<=0 & j in dom f & f.j=f.1 holds 1<=j by FINSEQ_3:27;
    then A5: P[0] by A3;
   A6: for k being Nat st P[k] holds P[k+1]
   proof let k be Nat;
    assume P[k];
     then consider n1 being Nat such that
     A7: (k<>0 implies n1<=k & n1 in dom f) &
      (for i being Nat,r1,r2 being Real st i<=k & i in dom f
      & r1=f.i & r2=f.n1
      holds r1<=r2) & (for j being Nat st j<=k & j in dom f & f.j=f.n1
      holds n1<=j);
      per cases;
      suppose A8: k=0;
        A9: dom f=Seg len f by FINSEQ_1:def 3;
          len f>0 by A2,NAT_1:19;
        then len f>=0+1 by NAT_1:38;
        then A10: 1<=1 & 1 in dom f by A9,FINSEQ_1:3;
        A11: for i being Nat,r1,r2 being Real st i<=1 & i in dom f
        & r1=f.i & r2=f.1 holds r1<=r2
        proof let i be Nat,r1,r2 be Real;
         assume A12: i<=1 & i in dom f & r1=f.i & r2=f.1;
           then 1<=i & i<=len f by FINSEQ_3:27;
         hence r1<=r2 by A12,AXIOMS:21;
        end;
          for j being Nat st j<=1 & j in dom f & f.j=f.1
          holds 1<=j by A9,FINSEQ_1:3;
       hence P[k+1] by A8,A10,A11;
      end;
      suppose A13: k<>0;
         now per cases;
       case A14: f.n1>=f.(k+1);
      A15: n1<=k+1 by A7,A13,NAT_1:38;
      A16: k+1<>0 implies n1<=k+1 & n1 in dom f by A7,A13,NAT_1:38;
      A17: for i being Nat,r1,r2 being Real st i<=k+1 & i in dom f
       & r1=f.i & r2=f.n1
      holds r1<=r2
      proof let i be Nat,r1,r2 be Real;
       assume A18: i<=k+1 & i in dom f
       & r1=f.i & r2=f.n1;
        per cases;
        suppose i<k+1; then i<=k by NAT_1:38;
         hence r1<=r2 by A7,A18;
        end;
        suppose i>=k+1;
         hence r1<=r2 by A14,A18,AXIOMS:21;
      end;
      end;
        for j being Nat st j<=k+1 & j in dom f & f.j=f.n1
       holds n1<=j
       proof let j be Nat;
        assume A19: j<=k+1 & j in dom f & f.j=f.n1;
          now per cases;
        case j<k+1; then j<=k by NAT_1:38;
         hence n1<=j by A7,A19;
        end;
        case j>=k+1;
         hence n1<=j by A15,A19,AXIOMS:21;
        end;
        end;
        hence n1<=j;
       end;
      hence P[k+1] by A16,A17;
      end;
      case A20: f.n1<f.(k+1);
         now per cases;
       case A21: k+1>len f;
        then A22: k>=len f by NAT_1:38;
          1<=n1 & n1<=len f by A1,A7,A13,FINSEQ_1:3;
        then A23: k+1<>0 implies n1<=k+1 & n1 in dom f by A7,A21,AXIOMS:22;
        A24: for i being Nat,r1,r2 being Real st i<=k+1 & i in dom f
           & r1=f.i & r2=f.n1 holds r1<=r2
         proof let i be Nat,r1,r2 be Real;
          assume A25: i<=k+1 & i in dom f
           & r1=f.i & r2=f.n1;
           then 1<=i & i<=len f by A1,FINSEQ_1:3;
           then i<=k by A22,AXIOMS:22;
          hence r1<=r2 by A7,A25;
         end;
           for j being Nat st j<=k+1 & j in dom f & f.j=f.n1
           holds n1<=j
          proof let j be Nat;
           assume A26: j<=k+1 & j in dom f & f.j=f.n1;
            per cases;
            suppose j<k+1;
              then j<=k by NAT_1:38;
             hence n1<=j by A7,A26;
            end;
            suppose j>=k+1;
             then k<j by NAT_1:38;
             hence n1<=j by A7,A13,AXIOMS:22;
          end;
          end;
       hence P[k+1] by A23,A24;
       end;
       case A27: k+1<=len f;
A28:     1<=1+k by NAT_1:37;
       set n2=k+1;
      A29: (k+1<>0 implies n2<=k+1 &
      n2 in dom f) by A1,A27,A28,FINSEQ_1:3;
     A30: for i being Nat,r1,r2 being Real st i<=k+1 & i in dom f
       & r1=f.i & r2=f.n2 holds r1<=r2
      proof let i be Nat,r1,r2 be Real;
       assume A31: i<=k+1 & i in dom f & r1=f.i & r2=f.n2;
        per cases;
        suppose i<k+1;
          then A32: i<=k by NAT_1:38;
          reconsider r3=f.n1 as Real;
            r1<=r3 by A7,A31,A32;
         hence r1<=r2 by A20,A31,AXIOMS:22;
        end;
        suppose i>=k+1;
         hence r1<=r2 by A31,AXIOMS:21;
      end;
      end;
        for j being Nat st j<=k+1 & j in dom f & f.j=f.n2 holds n2<=j
       proof let j be Nat;
        assume A33: j<=k+1 & j in dom f & f.j=f.n2;
        per cases;
        suppose j<k+1;
          then j<=k by NAT_1:38;
         hence n2<=j by A7,A20,A33;
        end;
        suppose j>=k+1;
         hence n2<=j;
       end;
       end;
       hence P[k+1] by A29,A30;
      end;
      end;
    hence P[k+1];
    end;
    end;
    hence P[k+1];
   end;
   end;
     for k being Nat holds P[k] from NAT_1:sch 1(A5,A6);
   then consider n1 being Nat such that
   A34: (len f<>0 implies n1<=len f &
   n1 in dom f) &
   (for i being Nat,r1,r2 being Real st i<=len f & i in dom f
   & r1=f.i & r2=f.n1
 holds r1<=r2) & (for j being Nat st j<=len f & j in dom f & f.j=f.n1
      holds n1<=j);
   A35: for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.n1
    holds r1<=r2
   proof let i be Nat,r1,r2 be Real;
    assume A36: i in dom f & r1=f.i & r2=f.n1;
     then 1<=i & i<=len f by FINSEQ_3:27;
    hence r1<=r2 by A34,A36;
   end;
     for j being Nat st j in dom f & f.j=f.n1 holds n1<=j
   proof let j be Nat;
    assume A37: j in dom f & f.j=f.n1;
     then 1<=j & j<=len f by FINSEQ_3:27;
    hence n1<=j by A34,A37;
   end;
  hence ex n being Nat st
 (len f=0 implies n=0) &
 (len f>0 implies n in dom f &
 (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.n
 holds r1<=r2) & (for j being Nat st j in dom f & f.j=f.n holds n<=j))
              by A2,A34,A35;
 end;
 end;
 uniqueness
 proof
 thus (for m1,m2 being Nat st ((len f=0 implies m1=0) &
 (len f>0 implies
 m1 in dom f &
 (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.m1
 holds r1<=r2) & (for j being Nat st j in dom f & f.j=f.m1 holds m1<=j)))
 &
  ((len f=0 implies m2=0) &
 (len f>0 implies m2 in dom f &
 (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.m2
 holds r1<=r2) & (for j being Nat st j in dom f & f.j=f.m2 holds m2<=j)))
 holds m1=m2)
 proof let m1,m2 be Nat;
  assume A38: ((len f=0 implies m1=0) &
   (len f>0 implies
   m1 in dom f &
   (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.m1
   holds r1<=r2) & (for j being Nat st j in dom f & f.j=f.m1 holds m1<=j)))
   &
    ((len f=0 implies m2=0) &
   (len f>0 implies m2 in dom f &
   (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.m2
   holds r1<=r2) & (for j being Nat st j in dom f & f.j=f.m2 holds m2<=j)));
     then A39: f.m2<=f.m1 by NAT_1:19;
       f.m1<=f.m2 by A38,NAT_1:19;
     then A40: f.m1=f.m2 by A39,AXIOMS:21;
     then A41: m1<=m2 by A38,NAT_1:19;
       m2<=m1 by A38,A40,NAT_1:19;
    hence m1=m2 by A41,AXIOMS:21;
 end;
 end;
end;

definition let f be FinSequence of REAL;
func min_p f -> Nat means
 :Def2: (len f=0 implies it=0) &
 (len f>0 implies
 it in dom f &
 (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.it
 holds r1>=r2) & (for j being Nat st j in dom f & f.j=f.it holds it<=j));
 existence
 proof
  A1: dom f=Seg len f by FINSEQ_1:def 3;
    now per cases;
  case len f=0;
  hence ex n being Nat st
 (len f=0 implies n=0) & (len f>0 implies n in dom f &
 (for i be Nat,r1,r2 be Real st i in dom f & r1=f.i & r2=f.n
 holds r1>=r2) & (for j being Nat st j in dom f & f.j=f.n holds n<=j));
  end;
  case A2: len f<>0;
  defpred P[Nat] means (ex n being Nat st ($1<>0 implies n<=$1 &
   n in dom f) &
   (for i being Nat,r1,r2 being Real st i<=$1 & i in dom f
   & r1=f.i & r2=f.n
 holds r1>=r2) & (for j being Nat st j<=$1 & j in dom f & f.j=f.n
      holds n<=j));
    A3: for i being Nat,r1,r2 being Real st i<=0 & i in dom f
     & r1=f.i & r2=f.1
     holds r1>=r2 by A1,FINSEQ_1:3;
      for j being Nat st j<=0 & j in dom f & f.j=f.1
      holds 1<=j by FINSEQ_3:27;
   then A5: P[0] by A3;
   A6: for k being Nat st P[k] holds P[k+1]
   proof let k be Nat;
    assume P[k];
     then consider n1 being Nat such that
     A7: (k<>0 implies n1<=k &
      n1 in dom f) &
      (for i being Nat,r1,r2 being Real st i<=k & i in dom f
      & r1=f.i & r2=f.n1
      holds r1>=r2) & (for j being Nat st j<=k & j in dom f & f.j=f.n1
      holds n1<=j);
        now per cases;
      case A8: k=0;
        A9: dom f=Seg len f by FINSEQ_1:def 3;
          len f>0 by A2,NAT_1:19;
        then len f>=0+1 by NAT_1:38;
        then A10: 1<=1 & 1 in dom f by A9,FINSEQ_1:3;
        A11: for i being Nat,r1,r2 being Real st i<=1 & i in dom f
        & r1=f.i & r2=f.1 holds r1>=r2
        proof let i be Nat,r1,r2 be Real;
         assume A12: i<=1 & i in dom f
           & r1=f.i & r2=f.1;
           then 1<=i & i<=len f by FINSEQ_3:27;
         hence r1>=r2 by A12,AXIOMS:21;
        end;
          for j being Nat st j<=1 & j in dom f & f.j=f.1
          holds 1<=j by A9,FINSEQ_1:3;
       hence P[k+1] by A8,A10,A11;
      end;
      case A13: k<>0;
         now per cases;
       case A14: f.n1<=f.(k+1);
      A15: n1<=k+1 by A7,A13,NAT_1:38;
      A16: k+1<>0 implies n1<=k+1 & n1 in dom f by A7,A13,NAT_1:38;
      A17: for i being Nat,r1,r2 being Real st i<=k+1 & i in dom f
       & r1=f.i & r2=f.n1 holds r1>=r2
      proof let i be Nat,r1,r2 be Real;
       assume A18: i<=k+1 & i in dom f & r1=f.i & r2=f.n1;
        per cases;
        suppose i<k+1; then i<=k by NAT_1:38;
         hence r1>=r2 by A7,A18;
        end;
        suppose i>=k+1;
         hence r1>=r2 by A14,A18,AXIOMS:21;
      end;
      end;
        for j being Nat st j<=k+1 & j in dom f & f.j=f.n1 holds n1<=j
       proof let j be Nat;
        assume A19: j<=k+1 & j in dom f & f.j=f.n1;
        per cases;
        suppose j<k+1; then j<=k by NAT_1:38;
         hence n1<=j by A7,A19;
        end;
        suppose j>=k+1;
         hence n1<=j by A15,A19,AXIOMS:21;
       end;
       end;
      hence P[k+1] by A16,A17;
      end;
      case A20: f.n1>f.(k+1);
         now per cases;
       case A21: k+1>len f;
        then A22: k>=len f by NAT_1:38;
          1<=n1 & n1<=len f by A1,A7,A13,FINSEQ_1:3;
        then A23: k+1<>0 implies n1<=k+1 & n1 in dom f by A7,A21,AXIOMS:22;
        A24: for i being Nat,r1,r2 being Real st i<=k+1 & i in dom f
           & r1=f.i & r2=f.n1 holds r1>=r2
         proof let i be Nat,r1,r2 be Real;
          assume A25: i<=k+1 & i in dom f
           & r1=f.i & r2=f.n1;
           then 1<=i & i<=len f by A1,FINSEQ_1:3;
           then i<=k by A22,AXIOMS:22;
          hence r1>=r2 by A7,A25;
         end;
           for j being Nat st j<=k+1 & j in dom f & f.j=f.n1
           holds n1<=j
          proof let j be Nat;
           assume A26: j<=k+1 & j in dom f & f.j=f.n1;
            per cases;
            suppose j<k+1;
             then j<=k by NAT_1:38;
             hence n1<=j by A7,A26;
            end;
            suppose j>=k+1;
             then k<j by NAT_1:38;
             hence n1<=j by A7,A13,AXIOMS:22;
          end;
          end;
       hence P[k+1] by A23,A24;
       end;
       case A27: k+1<=len f;
A28:     1<=1+k by NAT_1:37;
       set n2=k+1;
      A29: (k+1<>0 implies n2<=k+1 &
      n2 in dom f) by A1,A27,A28,FINSEQ_1:3;
     A30: for i being Nat,r1,r2 being Real st i<=k+1 & i in dom f
       & r1=f.i & r2=f.n2 holds r1>=r2
      proof let i be Nat,r1,r2 be Real;
       assume A31: i<=k+1 & i in dom f & r1=f.i & r2=f.n2;
        per cases;
        suppose i<k+1;
          then A32: i<=k by NAT_1:38;
          reconsider r3=f.n1 as Real;
            r1>=r3 by A7,A31,A32;
         hence r1>=r2 by A20,A31,AXIOMS:22;
        end;
        suppose i>=k+1;
         hence r1>=r2 by A31,AXIOMS:21;
      end;
      end;
        for j being Nat st j<=k+1 & j in dom f & f.j=f.n2
       holds n2<=j
       proof let j be Nat;
        assume A33: j<=k+1 & j in dom f & f.j=f.n2;
        per cases;
        suppose j<k+1;
          then j<=k by NAT_1:38;
         hence n2<=j by A7,A20,A33;
        end;
        suppose j>=k+1;
         hence n2<=j;
       end;
       end;
       hence P[k+1] by A29,A30;
       end;
       end;
       hence P[k+1];
      end;
      end;
    hence P[k+1];
    end;
    end;
    hence P[k+1];
   end;
     for k being Nat holds P[k] from NAT_1:sch 1(A5,A6);
   then consider n1 being Nat such that
   A34: (len f<>0 implies n1<=len f & n1 in dom f) &
   (for i being Nat,r1,r2 being Real st i<=len f & i in dom f
   & r1=f.i & r2=f.n1
 holds r1>=r2) & (for j being Nat st j<=len f & j in dom f & f.j=f.n1
      holds n1<=j);
   A35: for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.n1
    holds r1>=r2
   proof let i be Nat,r1,r2 be Real;
    assume A36: i in dom f & r1=f.i & r2=f.n1;
     then 1<=i & i<=len f by FINSEQ_3:27;
    hence r1>=r2 by A34,A36;
   end;
     for j being Nat st j in dom f & f.j=f.n1 holds n1<=j
   proof let j be Nat;
    assume A37: j in dom f & f.j=f.n1;
     then 1<=j & j<=len f by FINSEQ_3:27;
    hence n1<=j by A34,A37;
   end;
  hence ex n being Nat st (len f=0 implies n=0) &
 (len f>0 implies n in dom f &
 (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.n
 holds r1>=r2) & (for j being Nat st j in dom f & f.j=f.n holds n<=j))
              by A2,A34,A35;
  end;
  end;
  hence ex n being Nat st
 (len f=0 implies n=0) & (len f>0 implies n in dom f &
 (for i be Nat,r1,r2 be Real st i in dom f & r1=f.i & r2=f.n
 holds r1>=r2) & (for j being Nat st j in dom f & f.j=f.n holds n<=j));
 end;
 uniqueness
 proof
 thus (for m1,m2 being Nat st ((len f=0 implies m1=0) &
 (len f>0 implies
 m1 in dom f &
 (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.m1
 holds r1>=r2) & (for j being Nat st j in dom f & f.j=f.m1 holds m1<=j)))
 &
  ((len f=0 implies m2=0) &
 (len f>0 implies
 m2 in dom f &
 (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.m2
 holds r1>=r2) & (for j being Nat st j in dom f & f.j=f.m2 holds m2<=j)))
 holds m1=m2)
 proof let m1,m2 be Nat;
  assume A38: ((len f=0 implies m1=0) &
   (len f>0 implies
   m1 in dom f &
   (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.m1
   holds r1>=r2) & (for j being Nat st j in dom f & f.j=f.m1 holds m1<=j)))
   &
    ((len f=0 implies m2=0) &
   (len f>0 implies m2 in dom f &
   (for i being Nat,r1,r2 being Real st i in dom f & r1=f.i & r2=f.m2
   holds r1>=r2) & (for j being Nat st j in dom f & f.j=f.m2 holds m2<=j)));
     then A39: f.m2>=f.m1 by NAT_1:19;
       f.m1>=f.m2 by A38,NAT_1:19;
     then A40: f.m1=f.m2 by A39,AXIOMS:21;
     then A41: m1>=m2 by A38,NAT_1:19;
       m2>=m1 by A38,A40,NAT_1:19;
    hence m1=m2 by A41,AXIOMS:21;
 end;
 end;
end;

definition let f be FinSequence of REAL;
func max f -> Real equals
:Def3: f.(max_p f);
correctness;
func min f -> Real equals
:Def4: f.(min_p f);
correctness;
end;

theorem Th1: for f being FinSequence of REAL,i being Nat
 st 1<=i & i<=len f holds f.i<=f.(max_p f) & f.i<=max f
proof let f be FinSequence of REAL,i be Nat;
 assume A1: 1<=i & i<=len f;
  then A2: i in dom f by FINSEQ_3:27;
    0<len f by A1;
 hence f.i<=f.(max_p f) by A2,Def1;
 hence f.i<=max f by Def3;
end;

theorem Th2: for f being FinSequence of REAL,i being Nat
 st 1<=i & i<=len f holds f.i>=f.(min_p f) & f.i>=min f
proof let f be FinSequence of REAL,i be Nat;
 assume A1: 1<=i & i<=len f;
  then A2: i in dom f by FINSEQ_3:27;
    0<len f by A1;
 hence f.i>=f.(min_p f) by A2,Def2;
 hence f.i>=min f by Def4;
end;

theorem   for f being FinSequence of REAL,r being Real
 st f=<*r*> holds max_p f=1 & max f=r
proof let f be FinSequence of REAL,r be Real;
 assume f=<*r*>;
  then A1: len f=1 & f.1=r by FINSEQ_1:57;
  then (max_p f) in dom f by Def1;
  then 1<= (max_p f) & (max_p f)<=len f by FINSEQ_3:27;
