A11:      p.i=c2.i by A5,A6,Def5;
          (XFS2FS*(b)).i= b.i by A1,A2,A10,Def4;
          then (a*(XFS2FS*(b))).i = a*(b.i) by RVSUM_1:66;
          hence p2.i=a*(b.i) by A9,A10,A11,AFINSQ_1:48;
        end;
      end;
      hence thesis;
    end;
    p2.0=b.0 by A1,A6,AFINSQ_1:48;
    hence thesis by A8;
  end;
  then m scalar_prd_prg p2,a,b by A1,A6,A7,Def8;
