end;

:: Vector Calculation Program: Minus Vector
::#define V_SIZE 1024
::void vector_minus_prg(float c[V_SIZE], float b[V_SIZE])
::{       int n,i;
::        c[0]=b[0];
::        n=(int)(b[0]);
::        if (n != 0)
::         { for(i=1;i<=n;i++)c[i]= -b[i];
::         }
::        return;
::}
::The following definition is Logical-Model of the above program.

definition
  let b,c be XFinSequence of REAL,m be Integer;
  pred m vector_minus_prg c,b means
  :Def9:
  len c =m & len b =m &
  ex n being Integer st c.0=b.0 & n= b.0 & (n<>0 implies
  for i being Nat st 1<=i & i<=n holds c.i= -(b.i));
end;

theorem
  for b being non empty XFinSequence of REAL,m being Nat
  st b.0 is Nat & len b=m & 0<=b.0 & b.0 < m holds
  (ex c being XFinSequence of REAL st m vector_minus_prg c,b)&
  for c being non empty XFinSequence of REAL st m vector_minus_prg c,b
  holds (XFS2FS*(c))= -(XFS2FS*(b))
proof
  let b be non empty XFinSequence of REAL,m be Nat;
  assume
A1: b.0 is Nat & len b=m & 0<=b.0 & b.0 < m;
  then
A2: b.0 in m by NAT_1:45;
A3: 0 < len b by Th2;
  reconsider k= b.0 as Nat by A1;
  reconsider c2= -(XFS2FS*(b)) as FinSequence of REAL;
A4: len (XFS2FS*(b)) = b.0 by A1,A2,Def4;
  dom( -(XFS2FS*(b))) = dom (XFS2FS*(b)) by VALUED_1:8;
  then Seg len ( -(XFS2FS*(b)))= dom (XFS2FS*(b)) by FINSEQ_1:def 3;
  then
A5: len c2 =k by A4,FINSEQ_1:def 3;
  then consider p being XFinSequence of REAL such that
A6: len p=m & p is_an_xrep_of c2 by A1,Th7;
  set p2=Replace(p,0,b.0);
A7: len p=len p2 by AFINSQ_1:48;
  ex n being Integer st p2.0=b.0 & n=b.0 &
  (n<>0 implies for i being Nat st 1<=i & i<=n holds p2.i= -(b.i))
  proof
A8: (k<>0 implies for i being Nat st 1<=i & i<=k holds p2.i= -(b.i))
    proof
      assume k<>0;
      let i be Nat;
A9:   i in NAT by ORDINAL1:def 13;
      assume
A10:  1<=i & i<=k;
      then
A11:  p.i=c2.i by A5,A6,Def5;
      (XFS2FS*(b)).i= b.i by A1,A2,A10,Def4;
      then ( -(XFS2FS*(b))).i = -(b.i) by RVSUM_1:35;
      hence p2.i= -(b.i) by A9,A10,A11,AFINSQ_1:48;
    end;
    p2.0=b.0 by A1,A6,AFINSQ_1:48;
    hence thesis by A8;
  end;
  then m vector_minus_prg p2,b by A1,A6,A7,Def9;
  hence (ex c being XFinSequence of REAL st m vector_minus_prg c,b);
  thus for c being non empty XFinSequence of REAL st m vector_minus_prg c,b
  holds (XFS2FS*(c))= -(XFS2FS*(b))
  proof
    let c be non empty XFinSequence of REAL;
    assume
A12: m vector_minus_prg c,b;
    then
A13: len c =m & len b =m & ex n being Integer st c.0=b.0 & n=b.0 &
    (n<>0 implies for i being Nat st 1<=i & i<=n holds c.i= -(b.i)) by Def9;
    consider n being Integer such that
A14: c.0=b.0 & n=b.0 & (n<>0 implies for i being Nat st 1<=i & i<=n
    holds c.i= -(b.i)) by A12,Def9;
A15: len (XFS2FS*(c)) = (c.0) by A1,A2,A13,Def4;
    now per cases;
      case
A16:    n=0;
        then (XFS2FS*(b))=<*>REAL by A3,A14,Th5;
        hence (XFS2FS*(c))= -(XFS2FS*(b)) by A3,A13,A14,A16,Th5,RVSUM_1:37;
      end;
      case n<>0;
        set p3=(XFS2FS*(c));
        for k3 being Nat st 1 <=k3 & k3 <= len p3 holds p3.k3=c2.k3
        proof
          let k3 be Nat;
          assume
A17:      1 <=k3 & k3 <= len p3;
          (c.0) in len c by A1,A13,NAT_1:45;
          then
A18:      len p3=n & for i st 1<=i & i<= n holds p3.i=c.i by A1, A14,Def4;
          then
A19:      p3.k3=c.k3 by A17
            .= -(b.k3) by A14,A17,A18;
          b.k3 = (XFS2FS*b).k3 by A1,A2,A14,A17,A18,Def4;
          hence p3.k3=c2.k3 by A19,RVSUM_1:35;
        end;
        hence XFS2FS*(c)= -(XFS2FS*(b)) by A5,A14,A15,FINSEQ_1:18;
      end;
    end;
    hence (XFS2FS*(c))= -(XFS2FS*(b));
  end;
end;

:: Vector Calculation Program: Sum of Vectors
::
:: The following program is the same type of scalar_prd_prg, but gives a result
:: a+b in a variable c.
::
::#define V_SIZE 1024
::void vector_add_prg(float c[V_SIZE], float a[V_SIZE], float b[V_SIZE])
::{       int n,i;
::        c[0]=b[0];
::        n=(int)(b[0]);
::        if (n != 0)
::         { for(i=1;i<=n;i++)c[i]=a[i]+b[i];
::         }
::        return;
::}

definition
  let a,b,c be XFinSequence of REAL,m be Integer;
  pred m vector_add_prg c,a,b means
  :Def10:
  len c =m & len a=m & len b =m &
  ex n being Integer st c.0=b.0 & n=b.0 & (n<>0 implies
  for i being Nat st 1<=i & i<=n holds c.i=(a.i)+(b.i) );
end;

theorem
  for a,b being non empty XFinSequence of REAL, m being Nat
  st b.0 is Nat & len a=m & len b=m & a.0=b.0 & 0<=b.0 & b.0 < m holds
  (ex c being XFinSequence of REAL st m vector_add_prg c,a,b)&
  for c being non empty XFinSequence of REAL st m vector_add_prg c,a,b
  holds (XFS2FS*(c))=(XFS2FS*(a))+(XFS2FS*(b))
proof
  let a,b be non empty XFinSequence of REAL,m be Nat;
  assume
A1: b.0 is Nat & len a=m & len b=m & (a.0)=b.0 & 0<=b.0 & b.0 < m;
  then
A2: b.0 in m by NAT_1:45;
A3: 0 < len b by Th2;
  reconsider k= b.0 as Nat by A1;
  reconsider Fb=(XFS2FS*(b)) as FinSequence of REAL;
  reconsider Fa=(XFS2FS*(a)) as FinSequence of REAL;
  reconsider c2= Fa + Fb as FinSequence of REAL;
A4: len (XFS2FS*(b)) = b.0 by A1,A2,Def4;
A5: len (XFS2FS*(a)) = b.0 by A1,A2,Def4;
  then
A6: len c2=len (XFS2FS*(a)) by A4,EUCLID_2:6;
  then dom c2= Seg len (XFS2FS*(b)) by A4,A5,FINSEQ_1:def 3;
  then dom((XFS2FS*(a))+(XFS2FS*(b))) = dom (XFS2FS*(b)) by FINSEQ_1:def 3;
  then Seg len (c2) = dom (XFS2FS*(b)) by FINSEQ_1:def 3;
  then
A7: len c2 =k by A4,FINSEQ_1:def 3;
  then consider p being XFinSequence of REAL such that
A8: len p=m & p is_an_xrep_of c2 by A1,Th7;
  set p2=Replace(p,0,b.0);
A9: len p=len p2 by AFINSQ_1:48;
  ex n being Integer st p2.0=b.0 & n=b.0 & (n<>0 implies
  for i being Nat st 1<=i & i<=n holds p2.i=(a.i)+(b.i) )
  proof
A10: (k<>0 implies for i being Nat st 1<=i & i<=k holds p2.i=(a.i)+(b.i) )
    proof
      now
        assume k<>0;
        thus for i being Nat st 1<=i & i<=k holds p2.i=(a.i)+(b.i)
        proof
          let i be Nat;
A11:      i in NAT by ORDINAL1:def 13;
          assume
A12:      1<=i & i<=k;
          then
A13:      p.i=c2.i by A7,A8,Def5;
          i in Seg len c2 by A5,A6,A12,FINSEQ_1:3;
          then
A14:      i in dom c2 by FINSEQ_1:def 3;
A15:      (XFS2FS*(a)).i= a.i by A1,A2,A12,Def4;
          (XFS2FS*(b)).i= b.i by A1,A2,A12,Def4;
          then ((XFS2FS*(a))+(XFS2FS*(b))).i
          = (a.i)+(b.i) by A14,A15,VALUED_1:def 1;
          hence p2.i=(a.i)+(b.i) by A11,A12,A13,AFINSQ_1:48;
        end;
      end;
      hence thesis;
    end;
    p2.0=b.0 by A1,A8,AFINSQ_1:48;
    hence thesis by A10;
  end;
  then m vector_add_prg p2,a,b by A1,A8,A9,Def10;
  hence (ex c being XFinSequence of REAL st m vector_add_prg c,a,b);
  thus for c being non empty XFinSequence of REAL st m vector_add_prg c,a,b
  holds (XFS2FS*(c))=(XFS2FS*(a))+(XFS2FS*(b))
  proof
    let c be non empty XFinSequence of REAL;
    assume
A16: m vector_add_prg c,a,b;
    then
A17: len c =m & len b =m & ex n being Integer st c.0=b.0 & n=b.0 &
    (n<>0 implies for i being Nat st 1<=i & i<=n
    holds c.i=(a.i)+(b.i)) by Def10;
    consider n being Integer such that
A18: c.0=b.0 & n=b.0 & (n<>0 implies for i being Nat st 1<=i & i<=n
    holds c.i=(a.i)+(b.i)) by A16,Def10;
A19: len (XFS2FS*(c)) = (c.0) by A1,A2,A17,Def4;
    now per cases;
      case
A20:    n=0;
        then
A21:    (XFS2FS*(b))=<*>REAL by A3,A18,Th5;
        (XFS2FS*(c))=<*>REAL by A3,A17,A18,A20,Th5;
        hence (XFS2FS*(c))=(XFS2FS*(a))+(XFS2FS*(b)) by A21,RVSUM_1:28;
      end;
      case n<>0;
        set p3=(XFS2FS*(c));
        for k3 being Nat st 1 <=k3 & k3 <= len p3 holds p3.k3=c2.k3
        proof
          let k3 be Nat;
          assume
A22:      1 <=k3 & k3 <= len p3;
A23:      (c.0) in len c by A1,A17,NAT_1:45;
          then
A24:      len p3=n & for i st 1<=i & i<= n holds p3.i=c.i by A1, A18,Def4;
A25:      1 <=k3 & k3 <= n by A1,A18,A22,A23,Def4;
A26:      p3.k3=c.k3 by A22,A24
            .=(a.k3)+(b.k3) by A18,A25;
          k3 in Seg len c2 by A7,A18,A25,FINSEQ_1:3;
          then
A27:      k3 in dom c2 by FINSEQ_1:def 3;
A28:      a.k3 = (XFS2FS*(a)).k3 by A1,A2,A18,A25,Def4;
          b.k3 = (XFS2FS*(b)).k3 by A1,A2,A18,A25,Def4;
          hence p3.k3=c2.k3 by A26,A27,A28,VALUED_1:def 1;
        end;
        hence (XFS2FS*(c))=(XFS2FS*(a))+(XFS2FS*(b)) by A7,A18,A19,FINSEQ_1:18;
      end;
    end;
    hence (XFS2FS*(c))=(XFS2FS*(a))+(XFS2FS*(b));
  end;
end;

:: Vector Calculation Program: Subtraction of Vectors
::
:: The following program is the same type of scalar_prd_prg, but gives a result
:: a-b in a variable c.
::
::#define V_SIZE 1024
::void vector_sub_prg(float c[V_SIZE], float a[V_SIZE], float b[V_SIZE])
::{       int n,i;
::        c[0]=b[0];
::        n=(int)(b[0]);
::        if (n != 0)
::         { for(i=1;i<=n;i++)c[i]=a[i]-b[i];
::         }
::        return;
::}

definition
  let a,b,c be XFinSequence of REAL,m be Integer;
  pred m vector_sub_prg c,a,b means
  :Def11:
  len c =m & len a=m & len b =m &
  ex n being Integer st c.0=b.0 & n=b.0 & (n<>0 implies
  for i being Nat st 1<=i & i<=n holds c.i=(a.i)-(b.i) );
end;

theorem
  for a,b being non empty XFinSequence of REAL, m being Nat
  st b.0 is Nat & len a=m & len b=m & a.0=b.0 & 0<=b.0 & b.0 < m holds
  (ex c being XFinSequence of REAL st m vector_sub_prg c,a,b) &
  for c being non empty XFinSequence of REAL st m vector_sub_prg c,a,b
  holds XFS2FS*(c)=XFS2FS*(a)-XFS2FS*(b)
proof
  let a,b be non empty XFinSequence of REAL,m be Nat;
  assume
A1: b.0 is Nat & len a=m & len b=m & a.0=b.0 & 0<=b.0 & b.0 < m;
  then
A2: b.0 in m by NAT_1:45;
A3: 0 < len b by Th2;
  reconsider k= b.0 as Nat by A1;
  reconsider Fb=(XFS2FS*(b)) as FinSequence of REAL;
  reconsider Fa=(XFS2FS*(a)) as FinSequence of REAL;
  reconsider c2= Fa - Fb as FinSequence of REAL;
A4: len (XFS2FS*(b)) = b.0 by A1,A2,Def4;
A5: len (XFS2FS*(a)) = b.0 by A1,A2,Def4;
  then
A6: len c2=len (XFS2FS*(a)) by A4,EUCLID_2:7;
  then dom c2= Seg len (XFS2FS*(b)) by A4,A5,FINSEQ_1:def 3;
  then dom((XFS2FS*(a))-(XFS2FS*(b))) = dom (XFS2FS*(b)) by FINSEQ_1:def 3;
  then Seg len (c2) = dom (XFS2FS*(b)) by FINSEQ_1:def 3;
  then
A7: len c2 =k by A4,FINSEQ_1:def 3;
  then consider p being XFinSequence of REAL such that
A8: len p=m & p is_an_xrep_of c2 by A1,Th7;
  set p2=Replace(p,0,b.0);
A9: len p=len p2 by AFINSQ_1:48;
  ex n being Integer st p2.0=b.0 & n=b.0 & (n<>0 implies
  for i being Nat st 1<=i & i<=n holds p2.i=(a.i)-(b.i) )
  proof
A10: (k<>0 implies for i being Nat st 1<=i & i<=k holds p2.i=(a.i)-(b.i))
    proof
      now
        assume k<>0;
        thus for i being Nat st 1<=i & i<=k holds p2.i=(a.i)-(b.i)
        proof
          let i be Nat;
A11:      i in NAT by ORDINAL1:def 13;
          assume
A12:      1<=i & i<=k;
          then
A13:      p.i=c2.i by A7,A8,Def5;
          i in Seg len c2 by A5,A6,A12,FINSEQ_1:3;
          then
A14:      i in dom c2 by FINSEQ_1:def 3;
A15:      (XFS2FS*(a)).i= a.i by A1,A2,A12,Def4;
          (XFS2FS*(b)).i= b.i by A1,A2,A12,Def4;
          then ((XFS2FS*(a))-(XFS2FS*(b))).i
          = (a.i)-(b.i) by A14,A15,VALUED_1:13;
          hence p2.i=(a.i)-(b.i) by A11,A12,A13,AFINSQ_1:48;
        end;
      end;
      hence thesis;
    end;
    p2.0=b.0 by A1,A8,AFINSQ_1:48;
    hence thesis by A10;
  end;
  then m vector_sub_prg p2,a,b by A1,A8,A9,Def11;
  hence (ex c being XFinSequence of REAL st m vector_sub_prg c,a,b);
  thus for c being non empty XFinSequence of REAL st m vector_sub_prg c,a,b
  holds (XFS2FS*(c))=(XFS2FS*(a))-(XFS2FS*(b))
  proof
    let c be non empty XFinSequence of REAL;
    assume
A16: m vector_sub_prg c,a,b;
    then
A17: len c =m & len b =m & ex n being Integer st c.0=b.0 & n=b.0 &
    (n<>0 implies for i being Nat st 1<=i & i<=n
    holds c.i=(a.i)-(b.i)) by Def11;
    consider n being Integer such that
A18: c.0=b.0 & n=b.0 & (n<>0 implies for i being Nat st 1<=i & i<=n
    holds c.i=(a.i)-(b.i)) by A16,Def11;
A19: len (XFS2FS*(c)) = (c.0) by A1,A2,A17,Def4;
    now per cases;
      case
A20:    n=0;
        then
A21:    (XFS2FS*(b))=<*>REAL by A3,A18,Th5;
        (XFS2FS*(c))=<*>REAL by A3,A17,A18,A20,Th5;
        hence (XFS2FS*(c))=(XFS2FS*(a))-(XFS2FS*(b)) by A21,RVSUM_1:49;
      end;
      case n<>0;
        set p3=(XFS2FS*(c));
        for k3 being Nat st 1 <=k3 & k3 <= len p3 holds p3.k3=c2.k3
        proof
          let k3 be Nat;
          assume
A22:      1 <=k3 & k3 <= len p3;
A23:      (c.0) in len c by A1,A17,NAT_1:45;
          then
A24:      len p3=n & for i st 1<=i & i<= n holds p3.i=c.i by A1, A18,Def4;
A25:      1 <=k3 & k3 <= n by A1,A18,A22,A23,Def4;
A26:      p3.k3=c.k3 by A22,A24
            .=(a.k3)-(b.k3) by A18,A22,A24;
          k3 in Seg len c2 by A7,A18,A25,FINSEQ_1:3;
          then
A27:      k3 in dom c2 by FINSEQ_1:def 3;
A28:      a.k3 = (XFS2FS*(a)).k3 by A1,A2,A18,A25,Def4;
          b.k3 = (XFS2FS*(b)).k3 by A1,A2,A18,A25,Def4;
          hence p3.k3=c2.k3 by A26,A27,A28,VALUED_1:13;
        end;
        hence (XFS2FS*(c))=(XFS2FS*(a))-(XFS2FS*(b)) by A7,A18,A19,FINSEQ_1:18;
      end;
    end;
    hence (XFS2FS*(c))=(XFS2FS*(a))-(XFS2FS*(b));
  end;
end;

