  *(cos(th2/2)*sin(th1/2)+(-cos(th1/2)*sin(th2/2)))
  +(cos(th1/2)*sin(th2/2)*(sin(th1/2)*cos(th2/2))
  +cos(th1/2)*sin(th2/2)*(-cos(th1/2)*sin(th2/2)))) by XCMPLX_1:8
  .= -2*((sin(th1/2)*cos(th2/2)+(-cos(th1/2)*sin(th2/2)))
  *(sin(th1/2)*cos(th2/2))
  +((sin(th1/2)*cos(th2/2)+(-cos(th1/2)*sin(th2/2)))
  *(cos(th1/2)*sin(th2/2)))) by XCMPLX_1:8
  .= -2*((sin(th1/2)*cos(th2/2)+(-cos(th1/2)*sin(th2/2)))
  *(sin(th1/2)*cos(th2/2)+cos(th1/2)*sin(th2/2))) by XCMPLX_1:8
  .= -2*((sin(th1/2)*cos(th2/2)--(-cos(th1/2)*sin(th2/2)))
  *(sin(th1/2)*cos(th2/2)+cos(th1/2)*sin(th2/2))) by XCMPLX_1:151
  .= -2*(sin(th1/2+th2/2)*(sin(th1/2)*cos(th2/2)-cos(th1/2)*sin(th2/2)))
  by SIN_COS:80
  .= -2*(sin(th1/2+th2/2)*sin(th1/2-th2/2)) by TH0101
  .= -2*(sin((th1+th2)/2)*sin(th1/2-th2/2)) by XCMPLX_1:63
  .= -2*(sin((th1+th2)/2)*sin((th1-th2)/2)) by XCMPLX_1:121;
  hence thesis;
end;

theorem
  cos(th1)<>0 & cos(th2)<>0 implies
  tan(th1)+tan(th2)= sin(th1+th2)/(cos(th1)*cos(th2))
proof
  assume
  A: cos(th1)<>0 & cos(th2)<>0;
  tan(th1)+tan(th2)= sin(th1)/cos(th1)+tan(th2) by DF000
  .= sin(th1)/cos(th1)+sin(th2)/cos(th2) by DF000
  .= (sin(th1)*cos(th2)+sin(th2)*cos(th1))/(cos(th1)*cos(th2))
  by XCMPLX_1:117, A
  .= sin(th1+th2)/(cos(th1)*cos(th2)) by SIN_COS:80;
  hence thesis;
end;

theorem
  cos(th1)<>0 & cos(th2)<>0 implies
  tan(th1)-tan(th2)= sin(th1-th2)/(cos(th1)*cos(th2))
proof
  assume
  A: cos(th1)<>0 & cos(th2)<>0;
  tan(th1)-tan(th2)= sin(th1)/cos(th1)-tan(th2) by DF000
  .= sin(th1)/cos(th1)-sin(th2)/cos(th2) by DF000
  .= (sin(th1)*cos(th2)-sin(th2)*cos(th1))/(cos(th1)*cos(th2))
  by XCMPLX_1:131, A
  .= sin(th1-th2)/(cos(th1)*cos(th2)) by TH0101;
  hence thesis;
end;

theorem
  cos(th1)<>0 & sin(th2)<>0 implies
  tan(th1)+cot(th2)= cos(th1-th2)/(cos(th1)*sin(th2))
proof
  assume A1: cos(th1)<>0 & sin(th2)<>0;
  tan(th1)+cot(th2)= sin(th1)/cos(th1)+cot(th2) by DF000
  .= sin(th1)/cos(th1)+(cos(th2)/sin(th2)) by DF001
  .= (cos(th1)*cos(th2)+sin(th1)*sin(th2))/(cos(th1)*sin(th2))
  by XCMPLX_1:117, A1
  .= cos(th1-th2)/(cos(th1)*sin(th2)) by TH0102;
  hence thesis;
end;

theorem
  cos(th1)<>0 & sin(th2)<>0 implies
  tan(th1)-cot(th2)= -cos(th1+th2)/(cos(th1)*sin(th2))
proof
  assume A: cos(th1)<>0 & sin(th2)<>0;
  tan(th1)-cot(th2)= sin(th1)/cos(th1)-cot(th2) by DF000
  .= sin(th1)/cos(th1)-cos(th2)/sin(th2) by DF001
  .= (sin(th1)*sin(th2)---cos(th1)*cos(th2))/(cos(th1)*sin(th2))
  by XCMPLX_1:131, A
  .= (-cos(th1)*cos(th2)+sin(th1)*sin(th2))/(cos(th1)*sin(th2))
  by XCMPLX_1:151
  .= (-(cos(th1)*cos(th2)-sin(th1)*sin(th2)))/(cos(th1)*sin(th2))
  by XCMPLX_1:162
  .= -(cos(th1)*cos(th2)-sin(th1)*sin(th2))/(cos(th1)*sin(th2))
  by XCMPLX_1:188
  .= -cos(th1+th2)/(cos(th1)*sin(th2)) by SIN_COS:80;
  hence thesis;
end;

theorem
  sin(th1)<>0 & sin(th2)<>0 implies
  cot(th1)+cot(th2) = sin(th1+th2)/(sin(th1)*sin(th2))
proof
  assume A: sin(th1)<>0 & sin(th2)<>0;
  cot(th1)+cot(th2)= cos(th1)/sin(th1)+cot(th2) by DF001
  .= cos(th1)/sin(th1)+cos(th2)/sin(th2) by DF001
  .= (cos(th1)*sin(th2)+cos(th2)*sin(th1))/(sin(th1)*sin(th2))
  by XCMPLX_1:117, A
  .= sin(th1+th2)/(sin(th1)*sin(th2)) by SIN_COS:80;
  hence thesis;
end;

theorem
  sin(th1)<>0 & sin(th2)<>0 implies
  cot(th1)-cot(th2)= -sin(th1-th2)/(sin(th1)*sin(th2))
proof
  assume A: sin(th1)<>0 & sin(th2)<>0;
  cot(th1)-cot(th2)= cos(th1)/sin(th1)-cot(th2) by DF001
  .= cos(th1)/sin(th1)-cos(th2)/sin(th2) by DF001
  .= (cos(th1)*sin(th2)---cos(th2)*sin(th1))/(sin(th1)*sin(th2))
  by XCMPLX_1:131, A
  .= (-cos(th2)*sin(th1)+cos(th1)*sin(th2))/(sin(th1)*sin(th2))
  by XCMPLX_1:151
  .= (-(sin(th1)*cos(th2)-cos(th1)*sin(th2)))/(sin(th1)*sin(th2))
  by XCMPLX_1:162
  .= (-(sin(th1-th2)))/(sin(th1)*sin(th2)) by TH0101
  .= -sin(th1-th2)/(sin(th1)*sin(th2)) by XCMPLX_1:188;
  hence thesis;
end;

theorem
  sin(th1+th2)+sin(th1-th2) = 2*(sin(th1)*cos(th2))
proof
  sin(th1+th2)+sin(th1-th2)
  = (sin(th1)*cos(th2)+cos(th1)*sin(th2))+sin(th1-th2) by SIN_COS:80
  .= (sin(th1)*cos(th2)+cos(th1)*sin(th2))
  +(sin(th1)*cos(th2)---cos(th1)*sin(th2)) by TH0101
  .= (sin(th1)*cos(th2)+cos(th1)*sin(th2))
  +(-cos(th1)*sin(th2)+sin(th1)*cos(th2)) by XCMPLX_1:151
  .= ((sin(th1)*cos(th2)+cos(th1)*sin(th2))
  +-cos(th1)*sin(th2))+sin(th1)*cos(th2) by XCMPLX_1:1
  .= (sin(th1)*cos(th2)+(cos(th1)*sin(th2)
  +-cos(th1)*sin(th2)))+sin(th1)*cos(th2) by XCMPLX_1:1
  .= (sin(th1)*cos(th2)+(cos(th1)*sin(th2)
  ---cos(th1)*sin(th2)))+sin(th1)*cos(th2) by XCMPLX_1:151
  .= sin(th1)*cos(th2)+sin(th1)*cos(th2) by XCMPLX_1:25
  .= 2*(sin(th1)*cos(th2)) by XCMPLX_1:11;
  hence thesis;
end;

theorem
  sin(th1+th2)-sin(th1-th2) = 2*(cos(th1)*sin(th2))
proof
  sin(th1+th2)-sin(th1-th2)
  = (sin(th1)*cos(th2)+cos(th1)*sin(th2))-sin(th1-th2) by SIN_COS:80
  .= (sin(th1)*cos(th2)+cos(th1)*sin(th2))
  ---(sin(th1)*cos(th2)-cos(th1)*sin(th2)) by TH0101
  .= (sin(th1)*cos(th2)+cos(th1)*sin(th2))
  +-(sin(th1)*cos(th2)-cos(th1)*sin(th2)) by XCMPLX_1:151
  .= (sin(th1)*cos(th2)+cos(th1)*sin(th2))
  +(-sin(th1)*cos(th2)+cos(th1)*sin(th2)) by XCMPLX_1:162
  .= ((cos(th1)*sin(th2)+sin(th1)*cos(th2))
  +-sin(th1)*cos(th2))+cos(th1)*sin(th2) by XCMPLX_1:1
  .= (cos(th1)*sin(th2)+(sin(th1)*cos(th2)
  +-sin(th1)*cos(th2)))+cos(th1)*sin(th2) by XCMPLX_1:1
  .= (cos(th1)*sin(th2)+(sin(th1)*cos(th2)
  ---sin(th1)*cos(th2)))+cos(th1)*sin(th2) by XCMPLX_1:151
  .= cos(th1)*sin(th2)+cos(th1)*sin(th2) by XCMPLX_1:25
  .= 2*(cos(th1)*sin(th2)) by XCMPLX_1:11;
  hence thesis;
end;

theorem
  cos(th1+th2)+cos(th1-th2) = 2*(cos(th1)*cos(th2))
proof
  cos(th1+th2)+cos(th1-th2)
  = (cos(th1)*cos(th2)-sin(th1)*sin(th2))+cos(th1-th2) by SIN_COS:80
  .= (cos(th1)*cos(th2)---sin(th1)*sin(th2))
  +(cos(th1)*cos(th2)+sin(th1)*sin(th2)) by TH0102
  .= (cos(th1)*cos(th2)+-sin(th1)*sin(th2))
  +(sin(th1)*sin(th2)+cos(th1)*cos(th2)) by XCMPLX_1:151
  .= ((cos(th1)*cos(th2)+-sin(th1)*sin(th2))
  +sin(th1)*sin(th2))+cos(th1)*cos(th2) by XCMPLX_1:1
  .= (cos(th1)*cos(th2)+(sin(th1)*sin(th2)
  +-sin(th1)*sin(th2)))+cos(th1)*cos(th2) by XCMPLX_1:1
  .= (cos(th1)*cos(th2)+(sin(th1)*sin(th2)
  ---sin(th1)*sin(th2)))+cos(th1)*cos(th2) by XCMPLX_1:151
  .= cos(th1)*cos(th2)+cos(th1)*cos(th2) by XCMPLX_1:25
  .= 2*(cos(th1)*cos(th2)) by XCMPLX_1:11;
  hence thesis;
end;

theorem
  cos(th1+th2)-cos(th1-th2) = -2*(sin(th1)*sin(th2))
proof
  cos(th1+th2)-cos(th1-th2)
  = (cos(th1)*cos(th2)-sin(th1)*sin(th2))-cos(th1-th2) by SIN_COS:80
  .= (cos(th1)*cos(th2)---sin(th1)*sin(th2))
  -(cos(th1)*cos(th2)+sin(th1)*sin(th2)) by TH0102
  .= (cos(th1)*cos(th2)+-sin(th1)*sin(th2))
  -(cos(th1)*cos(th2)+sin(th1)*sin(th2)) by XCMPLX_1:151
  .= (cos(th1)*cos(th2)+-sin(th1)*sin(th2))
  ---(cos(th1)*cos(th2)--sin(th1)*sin(th2)) by XCMPLX_1:151
  .= (cos(th1)*cos(th2)+-sin(th1)*sin(th2))
  +-(cos(th1)*cos(th2)--sin(th1)*sin(th2)) by XCMPLX_1:151
  .= (-sin(th1)*sin(th2)+cos(th1)*cos(th2))
  +(-cos(th1)*cos(th2)+-sin(th1)*sin(th2))
  by XCMPLX_1:162
  .= ((-sin(th1)*sin(th2)+cos(th1)*cos(th2))
  +-cos(th1)*cos(th2))+-sin(th1)*sin(th2) by XCMPLX_1:1
  .= (-sin(th1)*sin(th2)+(cos(th1)*cos(th2)
  +-cos(th1)*cos(th2)))+-sin(th1)*sin(th2) by XCMPLX_1:1
  .= (-sin(th1)*sin(th2)+(cos(th1)*cos(th2)
  ---cos(th1)*cos(th2)))+-sin(th1)*sin(th2) by XCMPLX_1:151
  .= -sin(th1)*sin(th2)+-sin(th1)*sin(th2) by XCMPLX_1:25
  .= --2*(-sin(th1)*sin(th2)) by XCMPLX_1:11
  .= -2*(sin(th1)*sin(th2)) by XCMPLX_1:178;
  hence thesis;
end;

theorem TH0301:
  sin(th1)*sin(th2) = -(1/2)*(cos(th1+th2)-cos(th1-th2))
proof
  sin(th1)*sin(th2)
  = (sin(th1)*sin(th2)+sin(th1)*sin(th2))/2 by XCMPLX_1:65
  .= (cos(th1)*cos(th2)-cos(th1)*cos(th2)
  +sin(th1)*sin(th2)+sin(th1)*sin(th2))/2 by XCMPLX_1:25
  .= (cos(th1)*cos(th2)+sin(th1)*sin(th2)
  -cos(th1)*cos(th2)+sin(th1)*sin(th2))/2 by XCMPLX_1:29
  .= (cos(th1)*cos(-th2)+sin(th1)*sin(th2)
  -cos(th1)*cos(th2)+sin(th1)*sin(th2))/2 by SIN_COS:34
  .= (cos(th1)*cos(-th2)--sin(th1)*sin(th2)
  -cos(th1)*cos(th2)+(sin(th1)*sin(th2)))/2 by XCMPLX_1:151
  .= (cos(th1)*cos(-th2)-sin(th1)*(-sin(th2))
  -cos(th1)*cos(th2)+sin(th1)*sin(th2))/2 by XCMPLX_1:175
  .= (cos(th1)*cos(-th2)-sin(th1)*sin(-th2)
  -cos(th1)*cos(th2)+sin(th1)*sin(th2))/2 by SIN_COS:34
  .= (cos(th1+(-th2)) - cos(th1)*cos(th2)+sin(th1)*sin(th2))/2 by SIN_COS:80
  .= (cos(th1-th2)-cos(th1)*cos(th2)+sin(th1)*sin(th2))/2 by XCMPLX_0:def 8
  .= (cos(th1-th2)-(cos(th1)*cos(th2)-sin(th1)*sin(th2)))/2 by XCMPLX_1:37
  .= (cos(th1-th2)-cos(th1+th2))/(2/1)
  by SIN_COS:80
  .= 1/2*(cos(th1-th2)-cos(th1+th2)) by XCMPLX_1:81
  .= 1/2*-(cos(th1+th2)-cos(th1-th2)) by XCMPLX_1:143
  .= -(1/2)*(cos(th1+th2)-cos(th1-th2)) by XCMPLX_1:175;
  hence thesis;
end;

theorem TH0302:
  sin(th1)*cos(th2)= (1/2)*(sin(th1+th2)+sin(th1-th2))
proof
  sin(th1)*cos(th2)
  = (sin(th1)*cos(th2)+sin(th1)*cos(th2))/(2/1)
  by XCMPLX_1:65
  .= (1/2)*(sin(th1)*cos(th2)+sin(th1)*cos(th2)) by XCMPLX_1:81
  .= (1/2)*((sin(th1)*cos(th2)+cos(th1)*sin(th2))
  +(sin(th1)*cos(th2)-cos(th1)*sin(th2))) by XCMPLX_1:28
  .= (1/2)*(sin(th1+th2)+(sin(th1)*cos(th2)-cos(th1)*sin(th2)))
  by SIN_COS:80
  .= (1/2)*(sin(th1+th2)+(sin(th1)*cos(th2)+-(cos(th1)*sin(th2))))
  by XCMPLX_0:def 8
  .= (1/2)*(sin(th1+th2)+(sin(th1)*cos(th2)+cos(th1)*(-sin(th2))))
  by XCMPLX_1:175
  .= (1/2)*(sin(th1+th2)+(sin(th1)*cos(th2)+cos(th1)*sin(-th2)))
  by SIN_COS:34
  .= (1/2)*(sin(th1+th2)+(sin(th1)*cos(-th2)+cos(th1)*sin(-th2)))
  by SIN_COS:34
  .= (1/2)*(sin(th1+th2)+sin(th1+(-th2))) by SIN_COS:80
  .= (1/2)*(sin(th1+th2)+sin(th1-th2)) by XCMPLX_0:def 8;
  hence thesis;
end;

theorem TH0303:
  cos(th1)*sin(th2)= (1/2)*(sin(th1+th2)-sin(th1-th2))
proof
  cos(th1)*sin(th2)
  = (cos(th1)*sin(th2)+cos(th1)*sin(th2))/2 by XCMPLX_1:65
  .= (1/2)*(cos(th1)*sin(th2)+cos(th1)*sin(th2)) by XCMPLX_1:100
  .= (1/2)*(cos(th1)*sin(th2)--cos(th1)*sin(th2)) by XCMPLX_1:151
  .= (1/2)*((sin(th1)*cos(th2)--cos(th1)*sin(th2))
  -(sin(th1)*cos(th2)-cos(th1)*sin(th2))) by XCMPLX_1:23
  .= (1/2)*(sin(th1)*cos(th2)+cos(th1)*sin(th2)
  -(sin(th1)*cos(th2)-cos(th1)*sin(th2))) by XCMPLX_1:151
  .= (1/2)*(sin(th1+th2)-(sin(th1)*cos(th2)-cos(th1)*sin(th2)))
  by SIN_COS:80
  .= (1/2)*(sin(th1+th2)-(sin(th1)*cos(th2)+-(cos(th1)*sin(th2))))
  by XCMPLX_0:def 8
  .= (1/2)*(sin(th1+th2)-(sin(th1)*cos(th2)+cos(th1)*(-sin(th2))))
  by XCMPLX_1:175
  .= (1/2)*(sin(th1+th2)-(sin(th1)*cos(th2)+cos(th1)*sin(-th2)))
  by SIN_COS:34
  .= (1/2)*(sin(th1+th2)-(sin(th1)*cos(-th2)+cos(th1)*sin(-th2)))
  by SIN_COS:34
  .= (1/2)*(sin(th1+th2)-sin(th1+(-th2))) by SIN_COS:80
  .= (1/2)*(sin(th1+th2)-sin(th1-th2)) by XCMPLX_0:def 8;
  hence thesis;
end;

theorem TH0304:
  cos(th1)*cos(th2)= (1/2)*(cos(th1+th2)+cos(th1-th2))
proof
  cos(th1)*cos(th2)
  = (cos(th1)*cos(th2)+cos(th1)*cos(th2))/2 by XCMPLX_1:65
  .= (1/2)*(cos(th1)*cos(th2)+cos(th1)*cos(th2)) by XCMPLX_1:100
  .= (1/2)*((cos(th1)*cos(th2)+(-sin(th1)*sin(th2)))
  +(cos(th1)*cos(th2)-(-sin(th1)*sin(th2)))) by XCMPLX_1:28
  .= (1/2)*((cos(th1)*cos(th2)-sin(th1)*sin(th2))
  +(cos(th1)*cos(th2)-(-sin(th1)*sin(th2)))) by XCMPLX_0:def 8
  .= (1/2)*((cos(th1)*cos(th2)-sin(th1)*sin(th2))
  +(cos(th1)*cos(th2)+sin(th1)*sin(th2))) by XCMPLX_1:151
  .= (1/2)*(cos(th1+th2)+(cos(th1)*cos(th2)+sin(th1)*sin(th2)))
  by SIN_COS:80
  .= (1/2)*(cos(th1+th2)+(cos(th1)*cos(th2)--(sin(th1)*sin(th2))))
  by XCMPLX_1:151
  .= (1/2)*(cos(th1+th2)+(cos(th1)*cos(th2)-sin(th1)*(-sin(th2))))
  by XCMPLX_1:175
  .= (1/2)*(cos(th1+th2)+(cos(th1)*cos(th2)-sin(th1)*sin(-th2)))
  by SIN_COS:34
  .= (1/2)*(cos(th1+th2)+(cos(th1)*cos(-th2)-sin(th1)*sin(-th2)))
  by SIN_COS:34
  .= (1/2)*(cos(th1+th2)+(cos(th1+(-th2)))) by SIN_COS:80
  .= (1/2)*(cos(th1+th2)+cos(th1-th2)) by XCMPLX_0:def 8;
  hence thesis;
end;

theorem
  sin(th1)*sin(th2)*sin(th3)
  = (1/4)
  *(sin(th1+th2-th3)+sin(th2+th3-th1)+sin(th3+th1-th2)-sin(th1+th2+th3))
proof
  sin(th1)*sin(th2)*sin(th3)
  =(-((1/2)*(cos(th1+th2)-cos(th1-th2))))*sin(th3) by TH0301
  .=((1/2)*(-(cos(th1+th2)-cos(th1-th2))))*sin(th3) by XCMPLX_1:175
  .=(1/2)*(cos(th1-th2)-cos(th1+th2))*sin(th3) by XCMPLX_1:143
  .=(1/2)*((cos(th1-th2)-cos(th1+th2))*sin(th3)) by XCMPLX_1:4
  .=(1/2)*(cos(th1-th2)*sin(th3)-cos(th1+th2)*sin(th3)) by XCMPLX_1:40
  .=(1/2)*((1/2)*(sin((th1-th2)+th3)-sin((th1-th2)-th3))
  -cos(th1+th2)*sin(th3)) by TH0303
  .=(1/2)*((1/2)*(sin((th1-th2)+th3)-sin((th1-th2)-th3))
  -(1/2)*(sin((th1+th2)+th3)-sin((th1+th2)-th3))) by  TH0303
  .=(1/2)*((1/2)*((sin((th1-th2)+th3)-sin((th1-th2)-th3))
  -(sin((th1+th2)+th3)-sin((th1+th2)-th3)))) by XCMPLX_1:40
  .=((1/2)*(1/2))*((sin((th1-th2)+th3)-sin((th1-th2)-th3))
  -(sin((th1+th2)+th3)-sin((th1+th2)-th3))) by XCMPLX_1:4
  .=(1/(2*2))*((sin((th1-th2)+th3)---sin((th1-th2)-th3))
  ---(sin((th1+th2)+th3)-sin((th1+th2)-th3)))
  .=(1/(2*2))*((sin((th1-th2)+th3)---sin((th1-th2)-th3))
  +-(sin((th1+th2)+th3)-sin((th1+th2)-th3))) by XCMPLX_1:151
  .=(1/(2*2))*((sin((th1-th2)+th3)+-sin((th1-th2)-th3))
  +-(sin((th1+th2)+th3)-sin((th1+th2)-th3))) by XCMPLX_1:151
  .=(1/(2*2))*((sin((th1-th2)+th3)+-sin((th1-th2)-th3))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by XCMPLX_1:143
  .=(1/(2*2))*((sin((th1-th2)+th3)+sin(-((th1-th2)-th3)))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by SIN_COS:34
  .=(1/(2*2))*((sin((th1-th2)+th3)+sin((th3---(th1-th2))))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by XCMPLX_1:143
  .=(1/(2*2))*((sin((th1-th2)+th3)+sin((th3+-(th1-th2))))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by XCMPLX_1:151
  .=(1/(2*2))*((sin(th3+(th1-th2))+sin(th3+(th2---th1)))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3)))
  by XCMPLX_1:143
  .=(1/(2*2))*((sin(th3+(th1-th2))+sin(th3+(th2+-th1)))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by XCMPLX_1:151
  .=(1/(2*2))*((sin(th3+(th1-th2))+sin((th3+th2)+-th1))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by XCMPLX_1:1
  .=(1/(2*2))*((sin(th3+(th1---th2))+sin((th3+th2)---th1))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by XCMPLX_1:151
  .=(1/(2*2))*((sin(th3+(th1+-th2))+sin((th3+th2)-th1))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by XCMPLX_1:151
  .=(1/(2*2))*((sin((th3+th1)+-th2)+sin((th3+th2)-th1))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by XCMPLX_1:1
  .=(1/(2*2))*((sin((th3+th1)---th2)+sin((th3+th2)-th1))
  +(sin((th1+th2)-th3)-sin((th1+th2)+th3))) by XCMPLX_1:151
  .=(1/(2*2))*((sin((th3+th1)-th2)+sin((th3+th2)-th1))
  +(sin((th1+th2)-th3)---sin((th1+th2)+th3)))
  .=(1/(2*2))*((sin((th3+th1)-th2)+sin((th3+th2)-th1))
  +(sin((th1+th2)-th3)+-sin((th1+th2)+th3))) by XCMPLX_1:151
  .=(1/(2*2))*(((sin(th3+th1-th2)+sin(th2+th3-th1))
  +sin(th1+th2-th3))+-sin(th1+th2+th3)) by XCMPLX_1:1
  .=(1/(2*2))*(((sin(th1+th2-th3)+sin(th2+th3-th1))
  +sin(th3+th1-th2))+-sin(th1+th2+th3)) by XCMPLX_1:1
  .=(1/(2*2))*(((sin(th1+th2-th3)+sin(th2+th3-th1))
  +sin(th3+th1-th2))---sin(th1+th2+th3)) by XCMPLX_1:151
  .=(1/(2*2))*(sin(th1+th2-th3)+sin(th2+th3-th1)
  +sin(th3+th1-th2)-sin(th1+th2+th3));
  hence thesis;
end;

theorem
  sin(th1)*sin(th2)*cos(th3)
  = (1/4)
  *(-cos(th1+th2-th3)+cos(th2+th3-th1)+cos(th3+th1-th2)-cos(th1+th2+th3))
proof
  sin(th1)*sin(th2)*cos(th3)
  =(-((1/2)*(cos(th1+th2)-cos(th1-th2))))*cos(th3) by TH0301
  .=((1/2)*(-(cos(th1+th2)-cos(th1-th2))))*cos(th3) by XCMPLX_1:175
  .=(1/2)*(cos(th1-th2)-cos(th1+th2))*cos(th3) by XCMPLX_1:143
  .=(1/2)*((cos(th1-th2)-cos(th1+th2))*cos(th3)) by XCMPLX_1:4
  .=(1/2)*(cos(th1-th2)*cos(th3)-cos(th1+th2)*cos(th3)) by XCMPLX_1:40
  .=(1/2)*((1/2)*(cos((th1-th2)+th3)+cos((th1-th2)-th3))
  -cos(th1+th2)*cos(th3)) by TH0304
  .=(1/2)*((1/2)*(cos((th1-th2)+th3)+cos((th1-th2)-th3))
  -(1/2)*(cos((th1+th2)+th3)+cos((th1+th2)-th3))) by TH0304
  .=(1/2)*((1/2)*((cos((th1-th2)+th3)+cos((th1-th2)-th3))
  -(cos((th1+th2)+th3)+cos((th1+th2)-th3)))) by XCMPLX_1:40
  .=((1/2)*(1/2))*((cos((th1-th2)+th3)+cos((th1-th2)-th3))
  -(cos((th1+th2)+th3)+cos((th1+th2)-th3))) by XCMPLX_1:4
  .=(1/(2*2))*((cos(th3+(th1---th2))+cos((th1-th2)-th3))
  -(cos((th1+th2)+th3)+cos((th1+th2)-th3)))
  .=(1/(2*2))*((cos(th3+(th1+-th2))+cos((th1-th2)-th3))
  ---(cos((th1+th2)+th3)+cos((th1+th2)-th3))) by XCMPLX_1:151
  .=(1/(2*2))*((cos(th3+(th1+-th2))+cos((th1-th2)-th3))
  +-(cos((th1+th2)+th3)+cos((th1+th2)-th3))) by XCMPLX_1:151
  .=(1/(2*2))*((cos((th3+th1)+-th2)+cos((th1-th2)-th3))
  +-(cos((th1+th2)+th3)+cos((th1+th2)-th3))) by XCMPLX_1:1
  .=(1/(2*2))*((cos((th3+th1)+-th2)+cos((th1-th2)-th3))
  +(-cos((th1+th2)+th3)+-cos((th1+th2)-th3))) by XCMPLX_1:140
  .=(1/(2*2))*((-cos((th1+th2)-th3)+(cos((th1-th2)-th3)
  +cos((th3+th1)+-th2)))+-cos((th1+th2)+th3)) by XCMPLX_1:1
  .=(1/(2*2))*(((-cos((th1+th2)-th3)+cos((th1-th2)-th3))
  +cos((th3+th1)+-th2))+-cos((th1+th2)+th3)) by XCMPLX_1:1
  .=(1/(2*2))*((-cos((th1+th2)-th3)+cos((th1-th2)-th3))
  +(cos((th3+th1)+-th2)+-cos((th1+th2)+th3))) by XCMPLX_1:1
  .=(1/(2*2))*((-cos((th1+th2)-th3)+cos(-(th2-th1)---th3))
  +(cos((th3+th1)+-th2)+-cos((th1+th2)+th3))) by XCMPLX_1:143
  .=(1/(2*2))*((-cos((th1+th2)-th3)+cos(-(th2-th1)+-th3))
  +(cos((th3+th1)+-th2)+-cos((th1+th2)+th3))) by XCMPLX_1:151
  .=(1/(2*2))*((-cos((th1+th2)-th3)+cos(-((th2-th1)+th3)))
  +(cos((th3+th1)+-th2)+-cos((th1+th2)+th3))) by XCMPLX_1:140
  .=(1/(2*2))*((-cos((th1+th2)-th3)+cos((th2-th1)+th3))
  +(cos((th3+th1)+-th2)+-cos((th1+th2)+th3))) by SIN_COS:34
  .=(1/(2*2))*(((-cos((th1+th2)-th3)+cos((th2-th1)+th3))
  +cos((th3+th1)+-th2))+-cos((th1+th2)+th3)) by XCMPLX_1:1
  .=(1/(2*2))*(((-cos((th1+th2)-th3)+cos((th2-th1)+th3))
  +cos((th3+th1)+-th2))---cos((th1+th2)+th3)) by XCMPLX_1:151
  .=(1/(2*2))*(((-cos((th1+th2)-th3)+cos((th2-th1)+th3))
  +cos((th3+th1)---th2))-cos((th1+th2)+th3)) by XCMPLX_1:151
  .=(1/(2*2))*(-cos(th1+th2-th3)+cos((th2---th1)+th3)
  +cos(th3+th1-th2)-cos(th1+th2+th3))
  .=(1/(2*2))*(-cos(th1+th2-th3)+cos((th2+-th1)+th3)
  +cos(th3+th1-th2)-cos(th1+th2+th3)) by XCMPLX_1:151
  .=(1/(2*2))*(-cos(th1+th2-th3)+cos((th2+th3)+-th1)
  +cos(th3+th1-th2)-cos(th1+th2+th3)) by XCMPLX_1:1
  .=(1/(2*2))*(-cos(th1+th2-th3)+cos((th2+th3)---th1)
  +cos(th3+th1-th2)-cos(th1+th2+th3)) by XCMPLX_1:151
  .=(1/(2*2))*(-cos(th1+th2-th3)+cos(th2+th3-th1)
  +cos(th3+th1-th2)-cos(th1+th2+th3));
  hence thesis;
end;

theorem
  sin(th1)*cos(th2)*cos(th3)
  = (1/4)
  *(sin(th1+th2-th3)-sin(th2+th3-th1)+sin(th3+th1-th2)+sin(th1+th2+th3))
proof
  sin(th1)*cos(th2)*cos(th3)
  =((1/2)*(sin(th1+th2)+sin(th1-th2)))*cos(th3) by TH0302
  .=(1/2)*((sin(th1+th2)+sin(th1-th2))*cos(th3)) by XCMPLX_1:4
  .=(1/2)*(sin(th1+th2)*cos(th3)+sin(th1-th2)*cos(th3)) by XCMPLX_1:8
  .=(1/2)*(((1/2)*(sin((th1+th2)+th3)+sin((th1+th2)-th3)))
  +sin(th1-th2)*cos(th3)) by TH0302
  .=(1/2)*(((1/2)*(sin((th1+th2)+th3)+sin((th1+th2)-th3)))
  +((1/2)*(sin((th1-th2)+th3)+sin((th1-th2)-th3)))) by TH0302
  .=(1/2)*((1/2)*((sin((th1+th2)+th3)+sin((th1+th2)-th3))
  +(sin((th1-th2)+th3)+sin((th1-th2)-th3)))) by XCMPLX_1:8
  .=((1/2)*(1/2))*((sin((th1+th2)+th3)+sin((th1+th2)-th3))
  +(sin((th1-th2)+th3)+sin((th1-th2)-th3))) by XCMPLX_1:4
  .=(1/(2*2))*((sin((th1+th2)+th3)+sin((th1+th2)-th3))
  +(sin((th1---th2)+th3)+sin(-(th2-th1)---th3)))
  by XCMPLX_1:143
  .=(1/(2*2))*((sin((th1+th2)+th3)+sin((th1+th2)-th3))
  +(sin((th1+-th2)+th3)+sin(-(th2-th1)---th3))) by XCMPLX_1:151
  .=(1/(2*2))*((sin((th1+th2)+th3)+sin((th1+th2)-th3))
  +(sin((th1+-th2)+th3)+sin(-(th2-th1)+-th3))) by XCMPLX_1:151
  .=(1/(2*2))*((sin((th1+th2)+th3)+sin((th1+th2)-th3))
  +(sin((th1+-th2)+th3)+sin(-((th2-th1)+th3)))) by XCMPLX_1:140
  .=(1/(2*2))*((sin((th1+th2)+th3)+sin((th1+th2)-th3))
  +(sin(th3+(th1+-th2))+-sin((th2-th1)+th3))) by SIN_COS:34
  .=(1/(2*2))*((sin((th1+th2)+th3)+sin((th1+th2)-th3))
  +(-sin((th2-th1)+th3)+sin((th3+th1)+-th2))) by XCMPLX_1:1
  .=(1/(2*2))*((sin((th1+th2)-th3)+(-sin((th2-th1)+th3)
  +sin((th3+th1)+-th2)))+sin((th1+th2)+th3)) by XCMPLX_1:1
  .=(1/(2*2))*(((sin((th1+th2)-th3)+-sin((th2-th1)+th3)))
  +sin((th3+th1)+-th2)+sin((th1+th2)+th3)) by XCMPLX_1:1
  .=(1/(2*2))*(((sin((th1+th2)-th3)---sin((th2-th1)+th3)))
  +sin((th3+th1)+-th2)+sin((th1+th2)+th3)) by XCMPLX_1:151
  .=(1/(2*2))*(((sin((th1+th2)-th3)-sin((th2---th1)+th3)))
  +sin((th3+th1)+-th2)+sin((th1+th2)+th3))
  .=(1/(2*2))*(((sin((th1+th2)-th3)-sin((th2+-th1)+th3)))
  +sin((th3+th1)+-th2)+sin((th1+th2)+th3)) by XCMPLX_1:151
  .=(1/(2*2))*(((sin((th1+th2)-th3)-sin((th2+-th1)+th3)))
  +sin((th3+th1)---th2)+sin((th1+th2)+th3)) by XCMPLX_1:151
  .=(1/(2*2))*(((sin((th1+th2)-th3)-sin((th2+th3)+-th1)))
  +sin((th3+th1)-th2)+sin((th1+th2)+th3))
  by XCMPLX_1:1
  .=(1/(2*2))*(((sin((th1+th2)-th3)-sin((th2+th3)---th1)))
  +sin((th3+th1)-th2)+sin((th1+th2)+th3)) by XCMPLX_1:151
  .=(1/(2*2))*(sin(th1+th2-th3)-sin(th2+th3-th1)
  +sin(th3+th1-th2)+sin(th1+th2+th3));
  hence thesis;
end;

theorem
  cos(th1)*cos(th2)*cos(th3)
  = (1/4)
  *(cos(th1+th2-th3)+cos(th2+th3-th1)+cos(th3+th1-th2)+cos(th1+th2+th3))
proof
  cos(th1)*cos(th2)*cos(th3)
  =((1/2)*(cos(th1+th2)+cos(th1-th2)))*cos(th3) by TH0304
  .=(1/2)*((cos(th1+th2)+cos(th1-th2))*cos(th3)) by XCMPLX_1:4
  .=(1/2)*(cos(th1+th2)*cos(th3)+cos(th1-th2)*cos(th3)) by XCMPLX_1:8
  .=(1/2)*(((1/2)*(cos((th1+th2)+th3)+cos((th1+th2)-th3)))
  +cos(th1-th2)*cos(th3)) by TH0304
  .=(1/2)*(((1/2)*(cos((th1+th2)+th3)+cos((th1+th2)-th3)))
  +((1/2)*(cos((th1-th2)+th3)+cos((th1-th2)-th3)))) by TH0304
  .=(1/2)*((1/2)*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th1-th2)+th3)+cos((th1-th2)-th3)))) by XCMPLX_1:8
  .=((1/2)*(1/2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th1-th2)+th3)+cos((th1-th2)-th3))) by XCMPLX_1:4
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos(th3+(th1---th2))+cos((th1-th2)-th3)))
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos(th3+(th1+-th2))+cos((th1-th2)-th3))) by XCMPLX_1:151
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th3+th1)+-th2)+cos((th1-th2)-th3))) by XCMPLX_1:1
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th3+th1)+-th2)+cos(-(th2-th1)---th3))) by XCMPLX_1:143
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th3+th1)+-th2)+cos(-(th2-th1)+-th3))) by XCMPLX_1:151
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th3+th1)+-th2)+cos(-((th2-th1)+th3)))) by XCMPLX_1:140
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th3+th1)+-th2)+cos((th2---th1)+th3))) by SIN_COS:34
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th3+th1)+-th2)+cos((th2+-th1)+th3))) by XCMPLX_1:151
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th3+th1)---th2)+cos((th2+-th1)+th3))) by XCMPLX_1:151
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th3+th1)-th2)+cos((th2+th3)+-th1)))
  by XCMPLX_1:1
  .=(1/(2*2))*((cos((th1+th2)+th3)+cos((th1+th2)-th3))
  +(cos((th3+th1)-th2)+cos((th2+th3)---th1))) by XCMPLX_1:151
  .=(1/(2*2))*((cos((th1+th2)-th3)+(cos((th2+th3)-th1)
  +cos((th3+th1)-th2)))+cos((th1+th2)+th3))
  by XCMPLX_1:1
  .=(1/(2*2))*(cos(th1+th2-th3)+cos(th2+th3-th1)
  +cos(th3+th1-th2)+cos(th1+th2+th3)) by XCMPLX_1:1;
  hence thesis;
end;

theorem TH0309:
  sin(th1+th2)*sin(th1-th2) = sin(th1)*sin(th1)-sin(th2)*sin(th2)
proof
  sin(th1+th2)*sin(th1-th2)
  = (sin(th1)*cos(th2)+cos(th1)*sin(th2))*sin(th1-th2) by SIN_COS:80
  .= (sin(th1)*cos(th2)+cos(th1)*sin(th2))
  *(sin(th1)*cos(th2)---cos(th1)*sin(th2)) by TH0101
  .= (sin(th1)*cos(th2)+cos(th1)*sin(th2))
  *(sin(th1)*cos(th2)+-cos(th1)*sin(th2)) by XCMPLX_1:151
  .= (sin(th1)*cos(th2)*(sin(th1)*cos(th2))
  +sin(th1)*cos(th2)*(-cos(th1)*sin(th2))
  +sin(th1)*cos(th2)*(cos(th1)*sin(th2))
  +cos(th1)*sin(th2)*(-cos(th1)*sin(th2))) by XCMPLX_1:10
  .= ((sin(th1)*cos(th2)*(sin(th1)*cos(th2))
  +(sin(th1)*cos(th2)*(cos(th1)*sin(th2))
  +(sin(th1)*cos(th2))*(-cos(th1)*sin(th2))))
  +cos(th1)*sin(th2)*(-cos(th1)*sin(th2))) by XCMPLX_1:1
  .= ((sin(th1)*cos(th2)*(sin(th1)*cos(th2))
  +(sin(th1)*cos(th2)*(cos(th1)*sin(th2))
  +-sin(th1)*cos(th2)*(cos(th1)*sin(th2))))
  +cos(th1)*sin(th2)*(-cos(th1)*sin(th2))) by XCMPLX_1:175
  .= ((sin(th1)*cos(th2)*(sin(th1)*cos(th2))
  +(sin(th1)*cos(th2)*(cos(th1)*sin(th2))
  ---sin(th1)*cos(th2)*(cos(th1)*sin(th2))))
  +cos(th1)*sin(th2)*(-cos(th1)*sin(th2))) by XCMPLX_1:151
  .= ((sin(th1)*cos(th2)*(sin(th1)*cos(th2)))
  +cos(th1)*sin(th2)*(-cos(th1)*sin(th2))) by XCMPLX_1:25
  .= ((sin(th1)*cos(th2)*(sin(th1)*cos(th2)))
  +-cos(th1)*sin(th2)*(cos(th1)*sin(th2))) by XCMPLX_1:175
  .= ((sin(th1)*cos(th2)*(sin(th1)*cos(th2)))
  ---cos(th1)*sin(th2)*(cos(th1)*sin(th2))) by XCMPLX_1:151
  .= (((sin(th1)*cos(th2)*sin(th1))*cos(th2))
  -cos(th1)*sin(th2)*(cos(th1)*sin(th2))) by XCMPLX_1:4
  .= (((sin(th1)*sin(th1)*cos(th2))*cos(th2))
  -cos(th1)*sin(th2)*(cos(th1)*sin(th2))) by XCMPLX_1:4
  .= ((sin(th1)*sin(th1)*(cos(th2)*cos(th2)))
  -cos(th1)*sin(th2)*(cos(th1)*sin(th2))) by XCMPLX_1:4
  .= ((sin(th1)*sin(th1)*(cos(th2)*cos(th2)))
  -(cos(th1)*sin(th2)*cos(th1))*sin(th2)) by XCMPLX_1:4
  .= (sin(th1)*sin(th1)*(cos(th2)*cos(th2))
  -((cos(th1)*cos(th1))*sin(th2))*sin(th2)) by XCMPLX_1:4
  .= (sin(th1)*sin(th1)*(1-sin(th2)*sin(th2))
  -((cos(th1)*cos(th1))*sin(th2))*sin(th2)) by TH0019
  .= (sin(th1)*sin(th1)*(1---sin(th2)*sin(th2))
  -((1-sin(th1)*sin(th1))*sin(th2))*sin(th2)) by TH0019
  .= (sin(th1)*sin(th1)*(1+-sin(th2)*sin(th2))
  -((1---sin(th1)*sin(th1))*sin(th2))*sin(th2)) by XCMPLX_1:151
  .= (sin(th1)*sin(th1)*(1+-sin(th2)*sin(th2))
  -((1+-sin(th1)*sin(th1))*sin(th2))*sin(th2)) by XCMPLX_1:151
  .= ((sin(th1)*sin(th1)*(1)+sin(th1)*sin(th1)*(-sin(th2)*sin(th2)))
  -((1+-sin(th1)*sin(th1))*sin(th2))*sin(th2)) by XCMPLX_1:8
  .= ((sin(th1)*sin(th1)*(1)+sin(th1)*sin(th1)*(-sin(th2)*sin(th2)))
  -sin(th2)*sin(th2)*(1+-sin(th1)*sin(th1))) by XCMPLX_1:4
  .= ((sin(th1)*sin(th1))+sin(th1)*sin(th1)*(-sin(th2)*sin(th2))
  -((sin(th2)*sin(th2)*(1/1))+sin(th2)*sin(th2)*(-sin(th1)*sin(th1))))
  by XCMPLX_1:8
  .= (sin(th1)*sin(th1)+sin(th1)*sin(th1)*(-sin(th2)*sin(th2))
  ---(sin(th2)*sin(th2)+sin(th2)*sin(th2)*(-sin(th1)*sin(th1))))
  .= (sin(th1)*sin(th1)+sin(th1)*sin(th1)*(-sin(th2)*sin(th2))
  +-(sin(th2)*sin(th2)+sin(th2)*sin(th2)*(-sin(th1)*sin(th1))))
  by XCMPLX_1:151
  .= (sin(th1)*sin(th1)+sin(th1)*sin(th1)*(-sin(th2)*sin(th2))
  +(-sin(th2)*sin(th2)*(-sin(th1)*sin(th1))+-sin(th2)*sin(th2)))
  by XCMPLX_1:140
  .= ((sin(th1)*sin(th1)+sin(th1)*sin(th1)*(-sin(th2)*sin(th2))
  +-sin(th2)*sin(th2)*(-sin(th1)*sin(th1)))+-sin(th2)*sin(th2))
  by XCMPLX_1:1
  .= ((sin(th1)*sin(th1)+(sin(th1)*sin(th1)*(-sin(th2)*sin(th2))
  +-sin(th2)*sin(th2)*(-sin(th1)*sin(th1))))+-sin(th2)*sin(th2))
  by XCMPLX_1:1
  .= ((sin(th1)*sin(th1)+(sin(th1)*sin(th1)*(-sin(th2)*sin(th2))
  ---sin(th2)*sin(th2)*(-sin(th1)*sin(th1))))+-sin(th2)*sin(th2))
  by XCMPLX_1:151
  .= ((sin(th1)*sin(th1)+(sin(th1)*sin(th1)*(-sin(th2)*sin(th2))
  --(sin(th2)*sin(th2)*(sin(th1)*sin(th1)))))+-sin(th2)*sin(th2))
  by XCMPLX_1:175
  .= ((sin(th1)*sin(th1)+(-(sin(th1)*sin(th1)*(sin(th2)*sin(th2)))
  --(sin(th2)*sin(th2)*(sin(th1)*sin(th1)))))+-sin(th2)*sin(th2))
  by XCMPLX_1:175
  .= (sin(th1)*sin(th1)+-sin(th2)*sin(th2)) by XCMPLX_1:25
  .= (sin(th1)*sin(th1)---sin(th2)*sin(th2)) by XCMPLX_1:151
  .= (sin(th1)*sin(th1)-sin(th2)*sin(th2));
  hence thesis;
end;

theorem
  sin(th1+th2)*sin(th1-th2)= cos(th2)*cos(th2)-cos(th1)*cos(th1)
proof
  sin(th1+th2)*sin(th1-th2)
  = (sin(th1)*sin(th1)-sin(th2)*sin(th2)) by TH0309
  .= ((1-cos(th1)*cos(th1))-sin(th2)*sin(th2)) by TH0018
  .= ((1---cos(th1)*cos(th1))---(1-cos(th2)*cos(th2)))
  by TH0018
  .= (1---cos(th1)*cos(th1))+-(1-cos(th2)*cos(th2)) by XCMPLX_1:151
  .= (1+-cos(th1)*cos(th1))+-(1-cos(th2)*cos(th2)) by XCMPLX_1:151
  .= (1+-cos(th1)*cos(th1))+(-1+cos(th2)*cos(th2)) by XCMPLX_1:162
  .= ((-cos(th1)*cos(th1)+1)+-1)+cos(th2)*cos(th2)
  by XCMPLX_1:1
  .= (-cos(th1)*cos(th1)+(1+-1))+cos(th2)*cos(th2) by XCMPLX_1:1
  .= cos(th2)*cos(th2)---cos(th1)*cos(th1)
  by XCMPLX_1:151
  .= cos(th2)*cos(th2)-cos(th1)*cos(th1);
  hence thesis;
end;

theorem TH0311:
  sin(th1+th2)*cos(th1-th2) = sin(th1)*cos(th1)+sin(th2)*cos(th2)
proof
  sin(th1+th2)*cos(th1-th2)
  = (sin(th1)*cos(th2)+cos(th1)*sin(th2))*cos(th1-th2) by SIN_COS:80
  .= (sin(th1)*cos(th2)+cos(th1)*sin(th2))
  *(cos(th1)*cos(th2)+sin(th1)*sin(th2)) by TH0102
  .= (sin(th1)*cos(th2)*(cos(th2)*cos(th1))
  +sin(th1)*cos(th2)*(sin(th1)*sin(th2))
  +cos(th1)*cos(th2)*(cos(th1)*sin(th2))
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:10
  .= ((sin(th1)*cos(th2)*cos(th2))*cos(th1)
  +sin(th1)*cos(th2)*(sin(th1)*sin(th2))
  +cos(th1)*cos(th2)*(cos(th1)*sin(th2))
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:4
  .= ((sin(th1)*(cos(th2)*cos(th2)))*cos(th1)
  +sin(th1)*cos(th2)*(sin(th1)*sin(th2))
  +cos(th1)*cos(th2)*(cos(th1)*sin(th2))
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +sin(th1)*cos(th2)*(sin(th1)*sin(th2))
  +cos(th1)*cos(th2)*(cos(th1)*sin(th2))
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +(sin(th1)*(sin(th1)*cos(th2)))*sin(th2)
  +cos(th1)*cos(th2)*(cos(th1)*sin(th2))
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +((sin(th1)*sin(th1))*cos(th2))*sin(th2)
  +cos(th1)*cos(th2)*(cos(th1)*sin(th2))
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +sin(th1)*sin(th1)*(sin(th2)*cos(th2))
  +cos(th1)*cos(th2)*(cos(th1)*sin(th2))
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +sin(th1)*sin(th1)*(sin(th2)*cos(th2))
  +(cos(th1)*(cos(th1)*cos(th2)))*sin(th2)
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +sin(th1)*sin(th1)*(sin(th2)*cos(th2))
  +((cos(th1)*cos(th1))*cos(th2))*sin(th2)
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +sin(th1)*sin(th1)*(sin(th2)*cos(th2))
  +sin(th2)*cos(th2)*(cos(th1)*cos(th1))
  +cos(th1)*sin(th2)*(sin(th1)*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +sin(th1)*sin(th1)*(sin(th2)*cos(th2))
  +sin(th2)*cos(th2)*(cos(th1)*cos(th1))
  +cos(th1)*((sin(th1)*sin(th2))*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +sin(th1)*sin(th1)*(sin(th2)*cos(th2))
  +sin(th2)*cos(th2)*(cos(th1)*cos(th1))
  +cos(th1)*(sin(th1)*(sin(th2)*sin(th2)))) by XCMPLX_1:4
  .= ((sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +sin(th2)*cos(th2)*(sin(th1)*sin(th1))
  +sin(th2)*cos(th2)*(cos(th1)*cos(th1)))
  +sin(th1)*cos(th1)*(sin(th2)*sin(th2))) by XCMPLX_1:4
  .= (sin(th1)*cos(th1)*(sin(th2)*sin(th2))
  +(sin(th1)*cos(th1)*(cos(th2)*cos(th2))
  +(sin(th2)*cos(th2)*(sin(th1)*sin(th1))
  +sin(th2)*cos(th2)*(cos(th1)*cos(th1))))) by XCMPLX_1:1
  .= ((sin(th1)*cos(th1)*(sin(th2)*sin(th2))
  +sin(th1)*cos(th1)*(cos(th2)*cos(th2)))
  +(sin(th2)*cos(th2)*(sin(th1)*sin(th1))
  +sin(th2)*cos(th2)*(cos(th1)*cos(th1)))) by XCMPLX_1:1
  .= ((sin(th1)*cos(th1)*(sin(th2)*sin(th2)+cos(th2)*cos(th2)))
  +(sin(th2)*cos(th2)*(sin(th1)*sin(th1))
  +sin(th2)*cos(th2)*(cos(th1)*cos(th1)))) by XCMPLX_1:8
  .= ((sin(th1)*cos(th1)*(sin(th2)*sin(th2)+cos(th2)*cos(th2)))
  +(sin(th2)*cos(th2)*(sin(th1)*sin(th1)+cos(th1)*cos(th1))))
  by XCMPLX_1:8
  .= ((sin(th1)*cos(th1)*(1))
  +(sin(th2)*cos(th2)*(sin(th1)*sin(th1)+cos(th1)*cos(th1))))
  by SIN_COS:32
  .= ((sin(th1)*cos(th1))+(sin(th2)*cos(th2)*(1/1)))
  by SIN_COS:32
  .= sin(th1)*cos(th1)+sin(th2)*cos(th2);
  hence thesis;
end;

theorem
  cos(th1+th2)*sin(th1-th2) = sin(th1)*cos(th1)-sin(th2)*cos(th2)
proof
  cos(th1+th2)*sin(th1-th2)
  = cos(th1--th2)*sin(th1---th2) by XCMPLX_1:151
  .= sin(th1+(-th2))*cos(th1-(-th2)) by XCMPLX_1:151
  .= sin(th1)*cos(th1)+sin(-th2)*cos(-th2) by TH0311
  .= sin(th1)*cos(th1)+sin(-th2)*cos(th2) by SIN_COS:34
  .= sin(th1)*cos(th1)+(-sin(th2))*cos(th2) by SIN_COS:34
  .= sin(th1)*cos(th1)+-(sin(th2)*cos(th2)) by XCMPLX_1:175
  .= sin(th1)*cos(th1)---(sin(th2)*cos(th2)) by XCMPLX_1:151
  .= sin(th1)*cos(th1)-(sin(th2)*cos(th2));
  hence thesis;
end;

theorem TH0313:
  cos(th1+th2)*cos(th1-th2)= cos(th1)*cos(th1)-sin(th2)*sin(th2)
proof
  cos(th1+th2)*cos(th1-th2)
  = (cos(th1)*cos(th2)-sin(th1)*sin(th2))*cos(th1-th2) by SIN_COS:80
  .= (cos(th1)*cos(th2)---sin(th1)*sin(th2))
  *(cos(th1)*cos(th2)+sin(th1)*sin(th2)) by TH0102
  .= (cos(th1)*cos(th2)+-sin(th1)*sin(th2))
  *(cos(th1)*cos(th2)+sin(th1)*sin(th2))
  by XCMPLX_1:151
  .= (cos(th1)*cos(th2)*(cos(th1)*cos(th2))
  +cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +(-sin(th1)*sin(th2))*(cos(th1)*cos(th2))
  +(-sin(th1)*sin(th2))*(sin(th1)*sin(th2)))
  by XCMPLX_1:10
  .= ((cos(th1)*(cos(th1)*cos(th2)))*cos(th2)
  +cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +(-sin(th1)*sin(th2))*(cos(th1)*cos(th2))
  +(-sin(th1)*sin(th2))*(sin(th1)*sin(th2)))
  by XCMPLX_1:4
  .= (((cos(th1)*cos(th1))*cos(th2))*cos(th2)
  +cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +(-sin(th1)*sin(th2))*(cos(th1)*cos(th2))
  +(-sin(th1)*sin(th2))*(sin(th1)*sin(th2)))
  by XCMPLX_1:4
  .= (cos(th1)*cos(th1)*(cos(th2)*cos(th2))
  +cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +(-sin(th1)*sin(th2))*(cos(th1)*cos(th2))
  +(-sin(th1)*sin(th2))*(sin(th1)*sin(th2)))
  by XCMPLX_1:4
  .= (cos(th1)*cos(th1)*(cos(th2)*cos(th2))
  +cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +(-sin(th1)*sin(th2))*(cos(th1)*cos(th2))
  +-((sin(th1)*sin(th2))*(sin(th1)*sin(th2))))
  by XCMPLX_1:175
  .= (cos(th1)*cos(th1)*(cos(th2)*cos(th2))
  +cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +-((sin(th1)*sin(th2))*(cos(th1)*cos(th2)))
  +-((sin(th1)*sin(th2))*(sin(th1)*sin(th2))))
  by XCMPLX_1:175
  .= (cos(th1)*cos(th1)*(cos(th2)*cos(th2))
  +cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +-((sin(th1)*sin(th2))*(cos(th1)*cos(th2)))
  +-((sin(th1)*(sin(th1)*sin(th2)))*sin(th2)))
  by XCMPLX_1:4
  .= (cos(th1)*cos(th1)*(cos(th2)*cos(th2))
  +cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +-((sin(th1)*sin(th2))*(cos(th1)*cos(th2)))
  +-(((sin(th1)*sin(th1))*sin(th2))*sin(th2)))
  by XCMPLX_1:4
  .= ((cos(th1)*cos(th1)*(cos(th2)*cos(th2))
  +cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +-((cos(th1)*cos(th2))*(sin(th1)*sin(th2))))
  +-((sin(th1)*sin(th1))*(sin(th2)*sin(th2))))
  by XCMPLX_1:4
  .= ((cos(th1)*cos(th1)*(cos(th2)*cos(th2))
  +(cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  +-((cos(th1)*cos(th2))*(sin(th1)*sin(th2)))))
  +-((sin(th1)*sin(th1))*(sin(th2)*sin(th2))))
  by XCMPLX_1:1
  .= ((cos(th1)*cos(th1)*(cos(th2)*cos(th2))
  +(cos(th1)*cos(th2)*(sin(th1)*sin(th2))
  ---((cos(th1)*cos(th2))*(sin(th1)*sin(th2)))))
  +-((sin(th1)*sin(th1))*(sin(th2)*sin(th2))))
  by XCMPLX_1:151
  .= ((cos(th1)*cos(th1)*(cos(th2)*cos(th2)))
  +-((sin(th1)*sin(th1))*(sin(th2)*sin(th2))))
  by XCMPLX_1:25
  .= (cos(th1)*cos(th1)*(1---sin(th2)*sin(th2))
  +-((sin(th1)*sin(th1))*(sin(th2)*sin(th2))))
  by TH0019
  .= (cos(th1)*cos(th1)*(1+-sin(th2)*sin(th2))
  +-((sin(th1)*sin(th1))*(sin(th2)*sin(th2))))
  by XCMPLX_1:151
  .= ((cos(th1)*cos(th1)*(1)+cos(th1)*cos(th1)*(-sin(th2)*sin(th2)))
  +-((sin(th1)*sin(th1))*(sin(th2)*sin(th2))))
  by XCMPLX_1:8
  .= (cos(th1)*cos(th1)*(1)+((-sin(th2)*sin(th2))*(cos(th1)*cos(th1))
  +-((sin(th2)*sin(th2))*(sin(th1)*sin(th1)))))
  by XCMPLX_1:1
  .= (cos(th1)*cos(th1)*(1)+((sin(th2)*sin(th2))*(-cos(th1)*cos(th1))
  +-((sin(th2)*sin(th2))*(sin(th1)*sin(th1)))))
  by XCMPLX_1:176
  .= (cos(th1)*cos(th1)*(1)+((sin(th2)*sin(th2))*(-cos(th1)*cos(th1))
  +(sin(th2)*sin(th2))*(-sin(th1)*sin(th1))))
  by XCMPLX_1:175
  .= (cos(th1)*cos(th1)*(1)
  +((sin(th2)*sin(th2))*(-cos(th1)*cos(th1)+-sin(th1)*sin(th1))))
  by XCMPLX_1:8
  .= (cos(th1)*cos(th1)*(1)
  +((sin(th2)*sin(th2))*(-cos(th1)*cos(th1)---sin(th1)*sin(th1))))
  by XCMPLX_1:151
  .= (cos(th1)*cos(th1)*(1)
  +((sin(th2)*sin(th2))*(-((cos(th1)*cos(th1)+sin(th1)*sin(th1))))))
  by XCMPLX_1:161
  .= (cos(th1)*cos(th1)*(1)+(sin(th2)*sin(th2)*(-1)))
  by SIN_COS:32
  .= cos(th1)*cos(th1)+-(sin(th2)*sin(th2)*(1/1))
  by XCMPLX_1:175
  .= cos(th1)*cos(th1)---(sin(th2)*sin(th2))
  by XCMPLX_1:151
  .= cos(th1)*cos(th1)-sin(th2)*sin(th2);
  hence thesis;
end;

theorem
  cos(th1+th2)*cos(th1-th2) = cos(th2)*cos(th2)-sin(th1)*sin(th1)
proof
  cos(th1+th2)*cos(th1-th2)
  = cos(th1)*cos(th1)-sin(th2)*sin(th2)
  by TH0313
  .= (1-sin(th1)*sin(th1))-sin(th2)*sin(th2)
  by TH0019
  .= (1-sin(th1)*sin(th1))-(1-cos(th2)*cos(th2))
  by TH0018
  .= cos(th2)*cos(th2)-sin(th1)*sin(th1)
  by XCMPLX_1:23;
  hence thesis;
end;

theorem
  cos(th1)<>0 & cos(th2)<>0 implies
  sin(th1+th2)/sin(th1-th2)= (tan(th1)+tan(th2))/(tan(th1)-tan(th2))
proof
  assume A1: cos(th1)<>0 & cos(th2)<>0;
  A2: cos(th1)*cos(th2)<>0 by A1, XCMPLX_1:6;
  sin(th1+th2)/sin(th1-th2)
  = (sin(th1)*cos(th2)+cos(th1)*sin(th2))/sin(th1-th2)
  by SIN_COS:80
  .= (sin(th1)*cos(th2)+cos(th1)*sin(th2))
  /(sin(th1)*cos(th2)-cos(th1)*sin(th2))
  by TH0101
  .= ((sin(th1)*cos(th2)+cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /((sin(th1)*cos(th2)-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  by XCMPLX_1:55, A2
  .= ((sin(th1)*cos(th2)/(cos(th1)*cos(th2)))
  +cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /((sin(th1)*cos(th2)---cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  by XCMPLX_1:63
  .= ((sin(th1)*cos(th2)/(cos(th1)*cos(th2)))
  +cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /((sin(th1)*cos(th2)+(-cos(th1)*sin(th2)))/(cos(th1)*cos(th2)))
  by XCMPLX_1:151
  .= ((sin(th1)*cos(th2)/(cos(th1)*cos(th2)))
  +cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /((sin(th1)*cos(th2)/(cos(th1)*cos(th2)))
  +(-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  by XCMPLX_1:63
  .= (((sin(th1)/cos(th1))*(cos(th2)/cos(th2)))
  +cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /((sin(th1)*cos(th2)/(cos(th1)*cos(th2)))
  +(-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  by XCMPLX_1:77
  .= (((sin(th1)/cos(th1))*(cos(th2)/cos(th2)))
  +(cos(th1)/cos(th1))*(sin(th2)/cos(th2)))
  /((sin(th1)*cos(th2)/(cos(th1)*cos(th2)))
  +(-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  by XCMPLX_1:77
  .= (((sin(th1)/cos(th1))*(cos(th2)/cos(th2)))
  +(cos(th1)/cos(th1))*(sin(th2)/cos(th2)))
  /((sin(th1)/cos(th1)*(cos(th2)/cos(th2)))
  +(-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  by XCMPLX_1:77
  .= (((sin(th1)/cos(th1))*(cos(th2)/cos(th2)))
  +(cos(th1)/cos(th1))*(sin(th2)/cos(th2)))
  /((sin(th1)/cos(th1)*(cos(th2)/cos(th2)))
  +(cos(th1))*(-sin(th2))/(cos(th1)*cos(th2)))
  by XCMPLX_1:175
  .= (((sin(th1)/cos(th1))*(cos(th2)/cos(th2)))
  +(cos(th1)/cos(th1))*(sin(th2)/cos(th2)))
  /((sin(th1)/cos(th1)*(cos(th2)/cos(th2)))
  +(cos(th1)/cos(th1)*((-sin(th2))/cos(th2))))
  by XCMPLX_1:77
  .= (sin(th1)/cos(th1)+(cos(th1)/cos(th1))*(sin(th2)/cos(th2)))
  /((sin(th1)/cos(th1)*(cos(th2)/cos(th2)))
  +(cos(th1)/cos(th1)*((-sin(th2))/cos(th2))))
  by XCMPLX_1:89,A1
  .= (sin(th1)/cos(th1)+(sin(th2)/cos(th2)))
  /((sin(th1)/cos(th1)*(cos(th2)/cos(th2)))
  +(cos(th1)/cos(th1)*((-sin(th2))/cos(th2))))
  by XCMPLX_1:89,A1
  .= (sin(th1)/cos(th1)+(sin(th2)/cos(th2)))
  /(sin(th1)/cos(th1)+(cos(th1)/cos(th1)*((-sin(th2))/cos(th2))))
  by XCMPLX_1:89,A1
  .= (sin(th1)/cos(th1)+(sin(th2)/cos(th2)))
  /(sin(th1)/cos(th1)+(-sin(th2))/cos(th2))
  by XCMPLX_1:89,A1
  .= (sin(th1)/cos(th1)+(sin(th2)/cos(th2)))
  /(sin(th1)/cos(th1)+-(sin(th2))/cos(th2))
  by XCMPLX_1:188
  .= (tan(th1)+(sin(th2)/cos(th2)))
  /(sin(th1)/cos(th1)+-sin(th2)/cos(th2))
  by DF000
  .= (tan(th1)+tan(th2))/(sin(th1)/cos(th1)+-sin(th2)/cos(th2))
  by DF000
  .= (tan(th1)+tan(th2))/(tan(th1)+-sin(th2)/cos(th2))
  by DF000
  .= (tan(th1)+tan(th2))/(tan(th1)+-tan(th2))
  by DF000
  .= (tan(th1)+tan(th2))/(tan(th1)---tan(th2))
  by XCMPLX_1:151
  .= (tan(th1)+tan(th2))/(tan(th1)-tan(th2));
  hence thesis;
 end;

theorem
  cos(th1) <> 0 & cos(th2) <> 0 implies
  cos(th1+th2)/cos(th1-th2) = (1-tan(th1)*tan(th2))/(1+tan(th1)*tan(th2))
proof
  assume cos(th1) <>0 & cos(th2) <> 0; then
  A: cos(th1)*cos(th2) <> 0 by XCMPLX_1:6;
  cos(th1+th2)/cos(th1-th2)
  =(cos(th1)*cos(th2)-sin(th1)*sin(th2))/cos(th1-th2) by SIN_COS:80
  .=((cos(th1)*cos(th2)-sin(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /(cos(th1-th2)/(cos(th1)*cos(th2))) by A, XCMPLX_1:55
  .=(cos(th1)*cos(th2)/(cos(th1)*cos(th2))
  -(sin(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /(cos(th1-th2)/(cos(th1)*cos(th2))) by XCMPLX_1:121
  .=(1-(sin(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /(cos(th1-th2)/(cos(th1)*cos(th2))) by A, XCMPLX_1:60
  .=(1-(sin(th1)/cos(th1))*(sin(th2)/cos(th2)))
  /(cos(th1-th2)/(cos(th1)*cos(th2))) by XCMPLX_1:77
  .=(1-tan(th1)*(sin(th2)/cos(th2)))
  /(cos(th1-th2)/(cos(th1)*cos(th2))) by DF000
  .=(1-tan(th1)*tan(th2))
  /(cos(th1-th2)/(cos(th1)*cos(th2))) by DF000
  .=(1-tan(th1)*tan(th2))
  /((cos(th1)*cos(th2)+sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by TH0102
  .=(1-tan(th1)*tan(th2))
  /(cos(th1)*cos(th2)/(cos(th1)*cos(th2))
  +sin(th1)*sin(th2)/(cos(th1)*cos(th2))) by XCMPLX_1:63
  .=(1-tan(th1)*tan(th2))
  /(1+sin(th1)*sin(th2)/(cos(th1)*cos(th2))) by A, XCMPLX_1:60
  .=(1-tan(th1)*tan(th2))
  /(1+sin(th1)/cos(th1)*(sin(th2)/cos(th2))) by XCMPLX_1:77
  .=(1-tan(th1)*tan(th2))
  /(1+tan(th1)*(sin(th2)/cos(th2))) by DF000
  .=(1-tan(th1)*tan(th2))/(1+tan(th1)*tan(th2)) by DF000;
  hence thesis;
end;

theorem
  (sin(th1)+sin(th2))/(sin(th1)-sin(th2)) = tan((th1+th2)/2)*cot((th1-th2)/2)
proof
  (sin(th1)+sin(th2))/(sin(th1)-sin(th2))
  = 2*(cos((th1-th2)/2)*sin((th1+th2)/2))/(sin(th1)-sin(th2))
  by TH0201
  .= 2*(cos((th1-th2)/2)*sin((th1+th2)/2))
  /(2*(cos((th1+th2)/2)*sin((th1-th2)/2)))
  by TH0202
  .= (2/2)*((cos((th1-th2)/2)*sin((th1+th2)/2))
  /(cos((th1+th2)/2)*sin((th1-th2)/2)))
  by XCMPLX_1:77
  .= (sin((th1+th2)/2)/cos((th1+th2)/2))
  *(cos((th1-th2)/2)/sin((th1-th2)/2))
  by XCMPLX_1:77
  .= tan((th1+th2)/2)*(cos((th1-th2)/2)/sin((th1-th2)/2))
  by DF000
  .= tan((th1+th2)/2)*cot((th1-th2)/2)
  by DF001;
  hence thesis;
end;

theorem
  cos((th1-th2)/2)<>0 implies
  (sin(th1)+sin(th2))/(cos(th1)+cos(th2)) = tan((th1+th2)/2)
proof
  assume A1:cos((th1-th2)/2)<>0;
  (sin(th1)+sin(th2))/(cos(th1)+cos(th2))
  = 2*(cos((th1-th2)/2)*sin((th1+th2)/2))/(cos(th1)+cos(th2))
  by TH0201
  .= 2*(cos((th1-th2)/2)*sin((th1+th2)/2))
  /(2*(cos((th1+th2)/2)*cos((th1-th2)/2)))
  by TH0203
  .= (2/2)*((cos((th1-th2)/2)*sin((th1+th2)/2))
  /(cos((th1-th2)/2)*cos((th1+th2)/2)))
  by XCMPLX_1:77
  .= (cos((th1-th2)/2)/cos((th1-th2)/2))
  *(sin((th1+th2)/2)/cos((th1+th2)/2))
  by XCMPLX_1:77
  .= sin((th1+th2)/2)/cos((th1+th2)/2)
  by XCMPLX_1:89,A1
  .= tan((th1+th2)/2)
  by DF000;
  hence thesis;
end;

theorem
  cos((th1+th2)/2)<>0 implies
  (sin(th1)-sin(th2))/(cos(th1)+cos(th2)) = tan((th1-th2)/2)
proof
  assume A1:cos((th1+th2)/2)<>0;
  (sin(th1)-sin(th2))/(cos(th1)+cos(th2))
  = 2*(cos((th1+th2)/2)*sin((th1-th2)/2))/(cos(th1)+cos(th2))
  by TH0202
  .= 2*(cos((th1+th2)/2)*sin((th1-th2)/2))
  /(2*(cos((th1+th2)/2)*cos((th1-th2)/2)))
  by TH0203
  .= (2/2)*((cos((th1+th2)/2)*sin((th1-th2)/2))
  /(cos((th1+th2)/2)*cos((th1-th2)/2)))
  by XCMPLX_1:77
  .= (cos((th1+th2)/2)/cos((th1+th2)/2))*(sin((th1-th2)/2)/cos((th1-th2)/2))
  by XCMPLX_1:77
  .= sin((th1-th2)/2)/cos((th1-th2)/2)
  by XCMPLX_1:89,A1
  .= tan((th1-th2)/2)
  by DF000;
  hence thesis;
end;

theorem
  sin((th1+th2)/2)<>0 implies
  (sin(th1)+sin(th2))/(cos(th2)-cos(th1)) = cot((th1-th2)/2)
proof
  assume A1:sin((th1+th2)/2)<>0;
  (sin(th1)+sin(th2))/(cos(th2)-cos(th1))
  = 2*(cos((th1-th2)/2)*sin((th1+th2)/2))/(cos(th2)-cos(th1))
  by TH0201
  .= 2*(cos((th1-th2)/2)*sin((th1+th2)/2))
  /(-2*(sin((th2+th1)/2)*sin((th2-th1)/2)))
  by TH0204
  .= 2*(cos((th1-th2)/2)*sin((th1+th2)/2))
  /(2*(-(sin((th2+th1)/2)*sin((th2-th1)/2))))
  by XCMPLX_1:175
  .= 2*(cos((th1-th2)/2)*sin((th1+th2)/2))
  /(2*((sin((th2+th1)/2)*(-sin((th2-th1)/2)))))
  by XCMPLX_1:175
  .= 2*(cos((th1-th2)/2)*sin((th1+th2)/2))
  /(2*((sin((th2+th1)/2)*sin(-((th2-th1)/2)))))
  by SIN_COS:34
  .= 2*(cos((th1-th2)/2)*sin((th1+th2)/2))
  /(2*((sin((th2+th1)/2)*sin(((-(th2-th1))/2)))))
  by XCMPLX_1:188
  .= 2*(cos((th1-th2)/2)*sin((th1+th2)/2))
  /(2*((sin((th2+th1)/2)*sin(((th1-th2))/2))))
  by XCMPLX_1:143
  .= (2/2)*(cos((th1-th2)/2)*sin((th1+th2)/2)
  /(sin((th2+th1)/2)*sin(((th1-th2))/2)))
  by XCMPLX_1:77
  .= (cos((th1-th2)/2)/(sin((th1-th2)/2))
  *(sin((th1+th2)/2)/sin((th2+th1)/2)))
  by XCMPLX_1:77
  .= cos((th1-th2)/2)/(sin((th1-th2)/2))
  by XCMPLX_1:89,A1
  .= cot((th1-th2)/2)
  by DF001;
  hence thesis;
end;

theorem
  sin((th1-th2)/2)<>0 implies
