environ

 vocabulary ARYTM_3, ARYTM_1, SIN_COS, SIN_COS4;
 notation REAL_1, SIN_COS;
 constructors REAL_1, SQUARE_1, PREPOWER, SIN_COS;
 clusters INT_1, XCMPLX_0, SIN_COS, NUMBERS;
 requirements NUMERALS, SUBSET, ARITHM, REAL;
 theorems XCMPLX_0, XCMPLX_1, SIN_COS;

begin

reserve th, th1, th2, th3 for Real;

definition let th;
  func tan(th) -> Real equals :DF000: sin(th)/cos(th);
  correctness;
end;

definition let th;
  func cot(th) -> Real equals :DF001: cos(th)/sin(th);
  correctness;
end;

definition let th;
  func cosec(th) -> Real equals :DF002: 1/sin(th);
  correctness;
end;

definition let th;
  func sec(th) -> Real equals :DF003: 1/cos(th);
  correctness;
end;

theorem
  tan(th) = 1/cot(th)
proof
  tan(th) = sin(th)/cos(th) by DF000
  .=1/(cos(th)/sin(th)) by XCMPLX_1:57
  .=1/cot(th) by DF001;
  hence thesis;
end;

theorem
  tan(-th) = -tan(th)
proof
  tan(-th) = sin(-th)/cos(-th) by DF000
  .= sin(-th)/cos(th) by SIN_COS:34
  .= (-sin(th))/cos(th) by SIN_COS:34
  .= -sin(th)/cos(th) by XCMPLX_1:188
  .= -tan(th) by DF000;
  hence thesis;
end;

theorem
  cosec(-th) = -1/sin(th)
proof
  cosec(-th) = 1/sin(-th) by DF002
  .= 1/-sin(th) by SIN_COS:34
  .= -1/sin(th) by XCMPLX_1:189;
  hence thesis;
end;

theorem
  cot(-th) = -cot(th)
proof
  cot(-th) = cos(-th)/sin(-th) by DF001
  .=cos(th)/sin(-th) by SIN_COS:34
  .=cos(th)/(-sin(th)) by SIN_COS:34
  .=-cos(th)/sin(th) by XCMPLX_1:189
  .=-cot(th) by DF001;
  hence thesis;
end;

theorem
  cos(th1) <> 0 implies cos(th1)*sec(th1) = 1
proof
  assume
  A: cos(th1) <> 0;
  cos(th1)*sec(th1) = cos(th1)*(1/cos(th1)) by DF003
  .= 1 by A, XCMPLX_1:107;
  hence thesis;
end;

theorem TH0018:
  sin(th)*sin(th) = 1-cos(th)*cos(th)
proof
  sin(th)*sin(th) = sin(th)*sin(th)+(1-1)
  .= sin(th)*sin(th)+(1---(sin(th)*sin(th)+cos(th)*cos(th)))
  by SIN_COS:32
  .= sin(th)*sin(th)+(-(sin(th)*sin(th)+cos(th)*cos(th))+1)
  by XCMPLX_1:151
  .= sin(th)*sin(th)+(-sin(th)*sin(th)+-cos(th)*cos(th)+1) by XCMPLX_1:140
  .= sin(th)*sin(th)+(-sin(th)*sin(th)+(1+-cos(th)*cos(th)))
  by XCMPLX_1:1
  .= (1+-cos(th)*cos(th))+(sin(th)*sin(th)+-sin(th)*sin(th))
  by XCMPLX_1:1
  .= (1+-cos(th)*cos(th))+(sin(th)*sin(th)---sin(th)*sin(th)) by XCMPLX_1:151
  .= (1+-cos(th)*cos(th)) by XCMPLX_1:25
  .= (1---cos(th)*cos(th)) by XCMPLX_1:151
  .= 1-cos(th)*cos(th);
  hence thesis;
end;

theorem TH0019:
  cos(th)*cos(th) = 1-sin(th)*sin(th)
proof
  cos(th)*cos(th) = cos(th)*cos(th)+(1-1)
  .= cos(th)*cos(th)+(1---(sin(th)*sin(th)+cos(th)*cos(th)))
  by SIN_COS:32
  .= cos(th)*cos(th)+(1+-(sin(th)*sin(th)+cos(th)*cos(th))) by XCMPLX_1:151
  .= cos(th)*cos(th)+(1+(-sin(th)*sin(th)+-cos(th)*cos(th))) by XCMPLX_1:140
  .= cos(th)*cos(th)+(-cos(th)*cos(th)+(1+-sin(th)*sin(th)))
  by XCMPLX_1:1
  .= (cos(th)*cos(th)+-cos(th)*cos(th))+(1+-sin(th)*sin(th)) by XCMPLX_1:1
  .= (cos(th)*cos(th)---cos(th)*cos(th))+(1+-sin(th)*sin(th)) by XCMPLX_1:151
  .= (cos(th)*cos(th)-cos(th)*cos(th))+(1---sin(th)*sin(th)) by XCMPLX_1:151
  .= 1-sin(th)*sin(th) by XCMPLX_1:25;
  hence thesis;
end;

theorem TH0024:
  cos(th)<>0 implies sin(th) = cos(th)*tan(th)
proof
  assume A1: cos(th)<>0;
  sin(th)= (cos(th)/cos(th))*sin(th) by XCMPLX_1:89,A1
  .= cos(th)*(sin(th)/cos(th)) by XCMPLX_1:76
  .= cos(th)*tan(th) by DF000;
  hence thesis;
end;

theorem TH0101:
  sin(th1-th2) = sin(th1)*cos(th2)-cos(th1)*sin(th2)
proof
  sin(th1-th2)
  = sin(th1+(-th2)) by XCMPLX_0:def 8
  .= sin(th1)*cos(-th2)+cos(th1)*sin(-th2) by SIN_COS:80
  .= sin(th1)*cos(th2)+cos(th1)*sin(-th2) by SIN_COS:34
  .= sin(th1)*cos(th2)+cos(th1)*(-sin(th2)) by SIN_COS:34
  .= sin(th1)*cos(th2)+-(cos(th1)*sin(th2)) by XCMPLX_1:175
  .= sin(th1)*cos(th2)-cos(th1)*sin(th2) by XCMPLX_0:def 8;
  hence thesis;
end;

theorem TH0102:
  cos(th1-th2) = cos(th1)*cos(th2)+sin(th1)*sin(th2)
proof
  cos(th1-th2)= cos(th1+(-th2)) by XCMPLX_0:def 8
  .= cos(th1)*cos(-th2)-sin(th1)*sin(-th2)  by SIN_COS:80
  .= cos(th1)*cos(th2)-sin(th1)*sin(-th2)   by SIN_COS:34
  .= cos(th1)*cos(th2)-sin(th1)*(-sin(th2)) by SIN_COS:34
  .= cos(th1)*cos(th2)--(sin(th1)*sin(th2)) by XCMPLX_1:175
  .= cos(th1)*cos(th2)+sin(th1)*sin(th2)     by XCMPLX_1:151;
  hence thesis;
end;

theorem
  cos(th1)<>0 & cos(th2)<>0 implies
  tan(th1+th2)=(tan(th1)+tan(th2))/(1-tan(th1)*tan(th2))
proof
  assume A1: cos(th1)<>0 & cos(th2)<>0; then
  A2: cos(th1)*cos(th2)<>0 by XCMPLX_1:6;
  tan(th1+th2)=(sin(th1+th2))/(cos(th1+th2)) by DF000
  .=(sin(th1+th2)/(cos(th1)*cos(th2)))
  /(cos(th1+th2)/(cos(th1)*cos(th2))) by A2, XCMPLX_1:55
  .= ((sin(th1)*cos(th2)+cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /((cos(th1+th2))/(cos(th1)*cos(th2))) by SIN_COS:80
  .= ((sin(th1)*cos(th2)+cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /((cos(th1)*cos(th2)-sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by SIN_COS:80
  .= (sin(th1)*cos(th2)/(cos(th1)*cos(th2))
  +cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /((cos(th1)*cos(th2)-sin(th1)*sin(th2))/(cos(th1)*cos(th2)))
  by XCMPLX_1:63
  .= (sin(th1)*cos(th2)/(cos(th1)*cos(th2))
  +cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /(cos(th1)*cos(th2)/(cos(th1)*cos(th2))
  -(sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by XCMPLX_1:121
  .= (sin(th1)/cos(th1)*(cos(th2)/cos(th2))
  +cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /(cos(th1)*cos(th2)/(cos(th1)*cos(th2))
  -(sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by XCMPLX_1:77
  .= (sin(th1)/cos(th1)*(cos(th2)/cos(th2))
  +sin(th2)/cos(th2)*(cos(th1)/cos(th1)))
  /(cos(th1)*cos(th2)/(cos(th1)*cos(th2))
  -(sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by XCMPLX_1:77
  .= (sin(th1)/cos(th1)*(cos(th2)/cos(th2))
  +sin(th2)/cos(th2)*(cos(th1)/cos(th1)))
  /((cos(th1)/cos(th1))*(cos(th2)/cos(th2))
  -(sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by XCMPLX_1:77
  .= (sin(th1)/cos(th1)*(cos(th2)/cos(th2))
  +sin(th2)/cos(th2)*(cos(th1)/cos(th1)))
  /((cos(th1)/cos(th1))*(cos(th2)/cos(th2))
  -(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by XCMPLX_1:77
  .= (sin(th1)/cos(th1)+sin(th2)/cos(th2)*(cos(th1)/cos(th1)))
  /((cos(th1)/cos(th1))*(cos(th2)/cos(th2))
  -(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by XCMPLX_1:89, A1
  .= (sin(th1)/cos(th1)+sin(th2)/cos(th2))
  /((cos(th1)/cos(th1))*(cos(th2)/cos(th2))
  -(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by XCMPLX_1:89, A1
  .= (sin(th1)/cos(th1)+sin(th2)/cos(th2))
  /((cos(th1)/cos(th1))-(sin(th1)/cos(th1))*(sin(th2)/cos(th2)))
  by XCMPLX_1:89,A1
  .= (sin(th1)/cos(th1)+sin(th2)/cos(th2))
  /(1-(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by XCMPLX_1:60, A1
  .= (tan(th1)+sin(th2)/cos(th2))
  /(1-(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by DF000
  .= (tan(th1)+tan(th2))/(1-(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by DF000
  .= (tan(th1)+tan(th2))/(1-tan(th1)*(sin(th2)/cos(th2))) by DF000
  .= (tan(th1)+tan(th2))/(1-tan(th1)*tan(th2)) by DF000;
  hence thesis;
end;

theorem
  cos(th1)<>0 & cos(th2)<>0 implies
  tan(th1-th2)=(tan(th1)-tan(th2))/(1+tan(th1)*tan(th2))
proof
  assume
  A1: cos(th1)<>0 & cos(th2)<>0; then
  A2: cos(th1)*cos(th2)<>0 by XCMPLX_1:6;
  tan(th1-th2)=sin(th1-th2)/cos(th1-th2) by DF000
  .= sin(th1+(-th2))/cos(th1-th2) by XCMPLX_0:def 8
  .= sin(th1+(-th2))/cos(th1+(-th2)) by XCMPLX_0:def 8
  .= (sin(th1+(-th2))/(cos(th1)*cos(th2)))
  /(cos(th1+(-th2))/(cos(th1)*cos(th2))) by XCMPLX_1:55, A2
  .= ((sin(th1)*cos(-th2)+cos(th1)*sin(-th2))/(cos(th1)*cos(th2)))
  /((cos(th1+(-th2)))/(cos(th1)*cos(th2))) by SIN_COS:80
  .= ((sin(th1)*cos(th2)+cos(th1)*sin(-th2))/(cos(th1)*cos(th2)))
  /((cos(th1+(-th2)))/(cos(th1)*cos(th2))) by SIN_COS:34
  .= ((sin(th1)*cos(th2)+cos(th1)*(-sin(th2)))/(cos(th1)*cos(th2)))
  /((cos(th1+(-th2)))/(cos(th1)*cos(th2))) by SIN_COS:34
  .= ((sin(th1)*cos(th2)+-(cos(th1)*sin(th2)))/(cos(th1)*cos(th2)))
  /((cos(th1+(-th2)))/(cos(th1)*cos(th2))) by XCMPLX_1:175
  .= ((sin(th1)*cos(th2)-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /((cos(th1+(-th2)))/(cos(th1)*cos(th2))) by XCMPLX_0:def 8
  .= ((sin(th1)*cos(th2)-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /((cos(th1)*cos(-th2)-sin(th1)*sin(-th2))/(cos(th1)*cos(th2))) by SIN_COS:80
  .= ((sin(th1)*cos(th2)-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /((cos(th1)*cos(th2)-sin(th1)*sin(-th2))/(cos(th1)*cos(th2))) by SIN_COS:34
  .= ((sin(th1)*cos(th2)-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /((cos(th1)*cos(th2)-sin(th1)*(-sin(th2)))/(cos(th1)*cos(th2))) by SIN_COS:34
  .= ((sin(th1)*cos(th2)-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /((cos(th1)*cos(th2)--(sin(th1)*sin(th2)))/(cos(th1)*cos(th2)))
  by XCMPLX_1:175
  .= ((sin(th1)*cos(th2)-cos(th1)*sin(th2))/(cos(th1)*cos(th2)))
  /((cos(th1)*cos(th2)+sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by XCMPLX_1:151
  .= (sin(th1)*cos(th2)/(cos(th1)*cos(th2))
  - cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /((cos(th1)*cos(th2)+sin(th1)*sin(th2))/(cos(th1)*cos(th2)))
  by XCMPLX_1:121
  .= (sin(th1)*cos(th2)/(cos(th1)*cos(th2))
  -cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /(cos(th1)*cos(th2)/(cos(th1)*cos(th2))
  +sin(th1)*sin(th2)/(cos(th1)*cos(th2))) by XCMPLX_1:63
  .= (sin(th1)/cos(th1)*(cos(th2)/cos(th2))
  -cos(th1)*sin(th2)/(cos(th1)*cos(th2)))
  /(cos(th1)*cos(th2)/(cos(th1)*cos(th2))
  +(sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by XCMPLX_1:77
  .= (sin(th1)/cos(th1)*(cos(th2)/cos(th2))
  -sin(th2)/cos(th2)*(cos(th1)/cos(th1)))
  /(cos(th1)*cos(th2)/(cos(th1)*cos(th2))
  +(sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by XCMPLX_1:77
  .= (sin(th1)/cos(th1)*(cos(th2)/cos(th2))
  -sin(th2)/cos(th2)*(cos(th1)/cos(th1)))
  /((cos(th1)/cos(th1))*(cos(th2)/cos(th2))
  +(sin(th1)*sin(th2))/(cos(th1)*cos(th2))) by XCMPLX_1:77
  .= (sin(th1)/cos(th1)*(cos(th2)/cos(th2))
  -sin(th2)/cos(th2)*(cos(th1)/cos(th1)))
  /((cos(th1)/cos(th1))*(cos(th2)/cos(th2))
  +(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by XCMPLX_1:77
  .= (sin(th1)/cos(th1)-sin(th2)/cos(th2)*(cos(th1)/cos(th1)))
  /((cos(th1)/cos(th1))*(cos(th2)/cos(th2))
  +(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by XCMPLX_1:89,A1
  .= (sin(th1)/cos(th1)-sin(th2)/cos(th2))
  /((cos(th1)/cos(th1))*(cos(th2)/cos(th2))
  +(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by XCMPLX_1:89,A1
  .= (sin(th1)/cos(th1)-sin(th2)/cos(th2))
  /((cos(th1)/cos(th1))+(sin(th1)/cos(th1))*(sin(th2)/cos(th2)))
  by XCMPLX_1:89,A1
  .= (sin(th1)/cos(th1)-sin(th2)/cos(th2))
  /(1+(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by XCMPLX_1:60,A1
  .= (tan(th1)-sin(th2)/cos(th2))
  /(1+(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by DF000
  .= (tan(th1)-tan(th2))
  /(1+(sin(th1)/cos(th1))*(sin(th2)/cos(th2))) by DF000
  .= (tan(th1)-tan(th2)) / (1+tan(th1)*(sin(th2)/cos(th2))) by DF000
  .= (tan(th1)-tan(th2))/(1+tan(th1)*tan(th2)) by DF000;
  hence thesis;
end;

theorem
  sin(th1)<>0 & sin(th2)<>0 implies
  cot(th1+th2) = (cot(th1)*cot(th2)-1)/(cot(th2)+cot(th1))
proof
  assume A1: sin(th1)<>0 & sin(th2)<>0; then
  A2: sin(th1)*sin(th2)<>0 by XCMPLX_1:6;
  cot(th1+th2)=(cos(th1+th2))/(sin(th1+th2)) by DF001
  .=(cos(th1+th2)/(sin(th1)*sin(th2)))
  /(sin(th1+th2)/(sin(th1)*sin(th2))) by XCMPLX_1:55, A2
  .=((cos(th1)*cos(th2)-sin(th1)*sin(th2))/(sin(th1)*sin(th2)))
  /(sin(th1+th2)/(sin(th1)*sin(th2))) by SIN_COS:80
  .=((cos(th1)*cos(th2)-sin(th1)*sin(th2))/(sin(th1)*sin(th2)))
  /((sin(th1)*cos(th2)+cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by SIN_COS:80
  .=(cos(th1)*cos(th2)/(sin(th1)*sin(th2))
  -sin(th1)*sin(th2)/(sin(th1)*sin(th2)))
  /((sin(th1)*cos(th2)+cos(th1)*sin(th2))/(sin(th1)*sin(th2)))
  by XCMPLX_1:121
  .=(cos(th1)*cos(th2)/(sin(th1)*sin(th2))
  -sin(th1)*sin(th2)/(sin(th1)*sin(th2)))
  /(sin(th1)*cos(th2)/(sin(th1)*sin(th2))
  +(cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by XCMPLX_1:63
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))
  -sin(th1)*sin(th2)/(sin(th1)*sin(th2)))
  /(sin(th1)*cos(th2)/(sin(th1)*sin(th2))
  +(cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by XCMPLX_1:77
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))
  -sin(th1)/sin(th1)*(sin(th2)/sin(th2)))
  /(sin(th1)*cos(th2)/(sin(th1)*sin(th2))
  +(cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by XCMPLX_1:77
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))
  -sin(th1)/sin(th1)*(sin(th2)/sin(th2)))
  /((sin(th1)/sin(th1))*(cos(th2)/sin(th2))
  +(cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by XCMPLX_1:77
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))
  -sin(th1)/sin(th1)*(sin(th2)/sin(th2)))
  /((sin(th1)/sin(th1))*(cos(th2)/sin(th2))
  +(cos(th1)/sin(th1))*(sin(th2)/sin(th2))) by XCMPLX_1:77
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))-sin(th1)/sin(th1))
  /((sin(th1)/sin(th1))*(cos(th2)/sin(th2))
  +(cos(th1)/sin(th1))*(sin(th2)/sin(th2))) by XCMPLX_1:89, A1
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))-1)
  /((sin(th1)/sin(th1))*(cos(th2)/sin(th2))
  +(cos(th1)/sin(th1))*(sin(th2)/sin(th2))) by XCMPLX_1:60, A1
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))-1)
  /((cos(th2)/sin(th2))+(cos(th1)/sin(th1))*(sin(th2)/sin(th2)))
  by XCMPLX_1:89,A1
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))-1)
  /((cos(th2)/sin(th2))+(cos(th1)/sin(th1))) by XCMPLX_1:89,A1
  .=(cot(th1)*(cos(th2)/sin(th2))-1)
  /((cos(th2)/sin(th2))+(cos(th1)/sin(th1))) by DF001
  .=(cot(th1)*cot(th2)-1)/((cos(th2)/sin(th2))+(cos(th1)/sin(th1))) by DF001
  .=(cot(th1)*cot(th2)-1)/(cot(th2)+(cos(th1)/sin(th1))) by DF001
  .=(cot(th1)*cot(th2)-1)/(cot(th2)+cot(th1)) by DF001;
  hence thesis;
end;

theorem
  sin(th1)<>0 & sin(th2)<>0 implies
  cot(th1-th2)=(cot(th1)*cot(th2)+1)/(cot(th2)-cot(th1))
proof
  assume A1: sin(th1)<>0 & sin(th2)<>0; then
  A2: sin(th1)*sin(th2)<>0 by XCMPLX_1:6;
  cot(th1-th2)=(cos(th1-th2))/(sin(th1-th2)) by DF001
  .=(cos(th1-th2)/(sin(th1)*sin(th2)))
  /(sin(th1-th2)/(sin(th1)*sin(th2))) by XCMPLX_1:55, A2
  .=((cos(th1)*cos(th2)+sin(th1)*sin(th2))/(sin(th1)*sin(th2)))
  /(sin(th1-th2)/(sin(th1)*sin(th2))) by TH0102
  .=((cos(th1)*cos(th2)+sin(th1)*sin(th2))/(sin(th1)*sin(th2)))
  /((sin(th1)*cos(th2)-cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by TH0101
  .=(cos(th1)*cos(th2)/(sin(th1)*sin(th2))
  +sin(th1)*sin(th2)/(sin(th1)*sin(th2)))
  /((sin(th1)*cos(th2)-cos(th1)*sin(th2))/(sin(th1)*sin(th2)))
  by XCMPLX_1:63
  .=(cos(th1)*cos(th2)/(sin(th1)*sin(th2))
  +sin(th1)*sin(th2)/(sin(th1)*sin(th2)))
  /(sin(th1)*cos(th2)/(sin(th1)*sin(th2))
  -(cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by XCMPLX_1:121
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))
  +sin(th1)*sin(th2)/(sin(th1)*sin(th2)))
  /(sin(th1)*cos(th2)/(sin(th1)*sin(th2))
  -(cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by XCMPLX_1:77
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))
  +sin(th1)/sin(th1)*(sin(th2)/sin(th2)))
  /(sin(th1)*cos(th2)/(sin(th1)*sin(th2))
  -(cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by XCMPLX_1:77
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))
  +sin(th1)/sin(th1)*(sin(th2)/sin(th2)))
  /((sin(th1)/sin(th1))*(cos(th2)/sin(th2))
  -(cos(th1)*sin(th2))/(sin(th1)*sin(th2))) by XCMPLX_1:77
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))
  +sin(th1)/sin(th1)*(sin(th2)/sin(th2)))
  /((sin(th1)/sin(th1))*(cos(th2)/sin(th2))
  -(cos(th1)/sin(th1))*(sin(th2)/sin(th2))) by XCMPLX_1:77
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))+sin(th1)/sin(th1))
  /((sin(th1)/sin(th1))*(cos(th2)/sin(th2))
  -(cos(th1)/sin(th1))*(sin(th2)/sin(th2))) by XCMPLX_1:89,A1
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))+1)
  /((sin(th1)/sin(th1))*(cos(th2)/sin(th2))
  -(cos(th1)/sin(th1))*(sin(th2)/sin(th2))) by XCMPLX_1:60,A1
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))+1)
  /((cos(th2)/sin(th2))-(cos(th1)/sin(th1))*(sin(th2)/sin(th2)))
  by XCMPLX_1:89,A1
  .=(cos(th1)/sin(th1)*(cos(th2)/sin(th2))+1)
  /((cos(th2)/sin(th2))-(cos(th1)/sin(th1))) by XCMPLX_1:89,A1
  .=(cot(th1)*(cos(th2)/sin(th2))+1)
  /((cos(th2)/sin(th2))-(cos(th1)/sin(th1))) by DF001
  .=(cot(th1)*cot(th2)+1)/((cos(th2)/sin(th2))-(cos(th1)/sin(th1))) by DF001
  .=(cot(th1)*cot(th2)+1)/(cot(th2)-(cos(th1)/sin(th1))) by DF001
  .=(cot(th1)*cot(th2)+1)/(cot(th2)-cot(th1)) by DF001;
  hence thesis;
end;

theorem  TH0107:
  cos(th1)<>0 & cos(th2)<>0 & cos(th3)<>0 implies
  sin(th1+th2+th3)
  = cos(th1)*cos(th2)*cos(th3)
  *(tan(th1)+tan(th2)+tan(th3)-tan(th1)*tan(th2)*tan(th3))
proof
  assume A1: cos(th1)<>0 & cos(th2)<>0 & cos(th3)<>0;
  sin(th1+th2+th3) = sin(th1+(th2+th3)) by XCMPLX_1:1
  .=sin(th1)*cos(th2+th3)+cos(th1)*sin(th2+th3) by SIN_COS:80
  .=sin(th1)*(cos(th2)*cos(th3)-sin(th2)*sin(th3)) + cos(th1)*sin(th2+th3)
  by SIN_COS:80
  .=sin(th1)*(cos(th2)*cos(th3)-sin(th2)*sin(th3))
  +cos(th1)*(sin(th2)*cos(th3)+cos(th2)*sin(th3)) by SIN_COS:80
  .=sin(th1)*(cos(th2)*cos(th3))-sin(th1)*(sin(th2)*sin(th3))
  +cos(th1)*(sin(th2)*cos(th3)+cos(th2)*sin(th3)) by XCMPLX_1:40
  .=(sin(th1))*(cos(th2)*cos(th3))-sin(th1)*(sin(th2)*sin(th3))
  +(cos(th1)*(sin(th2)*cos(th3))+cos(th1)*(cos(th2)*sin(th3))) by XCMPLX_1:8
  .=(cos(th1)*tan(th1))*(cos(th2)*cos(th3))-sin(th1)*(sin(th2)*sin(th3))
  +(cos(th1)*(sin(th2)*cos(th3))+cos(th1)*(cos(th2)*sin(th3))) by TH0024,A1
  .=(cos(th1)*tan(th1))*(cos(th2)*cos(th3))
  -(cos(th1)*tan(th1))*(sin(th2)*sin(th3))
  +(cos(th1)*(sin(th2)*cos(th3))+cos(th1)*(cos(th2)*sin(th3))) by TH0024,A1
  .=(cos(th1)*tan(th1))*(cos(th2)*cos(th3))
   -(cos(th1)*tan(th1))*((cos(th2)*tan(th2))*sin(th3))
  +(cos(th1)*(sin(th2)*cos(th3))+cos(th1)*(cos(th2)*sin(th3))) by TH0024,A1
  .=(cos(th1)*tan(th1))*(cos(th2)*cos(th3))
  -(cos(th1)*tan(th1))*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  +(cos(th1)*(sin(th2)*cos(th3))+cos(th1)*(cos(th2)*sin(th3))) by TH0024,A1
  .=(cos(th1)*tan(th1))*(cos(th2)*cos(th3))
  -(cos(th1)*tan(th1))*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))+cos(th1)*(cos(th2)*sin(th3)))
  by TH0024,A1
  .=(cos(th1)*tan(th1))*(cos(th2)*cos(th3))
  -(cos(th1)*tan(th1))*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by TH0024,A1
  .=((cos(th1)*tan(th1))*cos(th2))*cos(th3)
  -(cos(th1)*tan(th1))*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=((cos(th1)*cos(th2))*tan(th1))*cos(th3)
  -(cos(th1)*tan(th1))*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*tan(th1))*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by  XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -((cos(th1)*tan(th1))*(cos(th2)*tan(th2)))*(cos(th3)*tan(th3))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*(tan(th1)*(cos(th2)*tan(th2))))*(cos(th3)*tan(th3))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*(cos(th2)*(tan(th1)*tan(th2))))*(cos(th3)*tan(th3))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*cos(th2)*(tan(th1)*tan(th2)))*(cos(th3)*tan(th3))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*cos(th2)*(cos(th3)*tan(th3)))*(tan(th1)*tan(th2))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -((cos(th1)*cos(th2)*cos(th3))*tan(th3))*(tan(th1)*tan(th2))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*cos(th2)*cos(th3))*((tan(th1)*tan(th2))*tan(th3))
  +(cos(th1)*((cos(th2)*tan(th2))*cos(th3))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*cos(th2)*cos(th3))*((tan(th1)*tan(th2))*tan(th3))
  +(cos(th1)*(cos(th2)*(tan(th2)*cos(th3)))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*cos(th2)*cos(th3))*((tan(th1)*tan(th2))*tan(th3))
  +((cos(th1)*cos(th2))*(cos(th3)*tan(th2))
  +cos(th1)*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*cos(th2)*cos(th3))*((tan(th1)*tan(th2))*tan(th3))
  +((cos(th1)*cos(th2))*(cos(th3)*tan(th2))
  +cos(th1)*((cos(th2)*cos(th3))*tan(th3))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*(cos(th3)*tan(th1))
  -(cos(th1)*cos(th2)*cos(th3))*((tan(th1)*tan(th2))*tan(th3))
  +((cos(th1)*cos(th2))*(cos(th3)*tan(th2))
  +(cos(th1)*(cos(th2)*cos(th3)))*tan(th3)) by XCMPLX_1:4
  .=(cos(th1)*cos(th2)*cos(th3))*tan(th1)
  -(cos(th1)*cos(th2)*cos(th3))*((tan(th1)*tan(th2))*tan(th3))
  +((cos(th1)*cos(th2))*(cos(th3)*tan(th2))
  +(cos(th1)*(cos(th2)*cos(th3)))*tan(th3)) by XCMPLX_1:4
  .=(cos(th1)*cos(th2)*cos(th3))*tan(th1)
  -(cos(th1)*cos(th2)*cos(th3))*((tan(th1)*tan(th2))*tan(th3))
  +((cos(th1)*cos(th2)*cos(th3))*tan(th2)
  +(cos(th1)*(cos(th2)*cos(th3)))*tan(th3)) by XCMPLX_1:4
  .=(cos(th1)*cos(th2)*cos(th3))*tan(th1)
  -(cos(th1)*cos(th2)*cos(th3))*((tan(th1)*tan(th2))*tan(th3))
  +((cos(th1)*cos(th2)*cos(th3))*tan(th2)
  +(cos(th1)*cos(th2)*cos(th3))*tan(th3)) by XCMPLX_1:4
  .=(cos(th1)*cos(th2)*cos(th3))
  *(tan(th1)-(tan(th1)*tan(th2))*tan(th3))
  +((cos(th1)*cos(th2)*cos(th3))*tan(th2)
  +(cos(th1)*cos(th2)*cos(th3))*tan(th3)) by XCMPLX_1:40
  .=(cos(th1)*cos(th2)*cos(th3))
  *(tan(th1)-(tan(th1)*tan(th2))*tan(th3))
  +((cos(th1)*cos(th2)*cos(th3))*(tan(th2)+tan(th3))) by XCMPLX_1:8
  .=cos(th1)*cos(th2)*cos(th3)
  *(tan(th1)---(tan(th1)*tan(th2))*tan(th3)+(tan(th2)+tan(th3)))
  by XCMPLX_1:8
  .=cos(th1)*cos(th2)*cos(th3)
  *(tan(th1)+-(tan(th1)*tan(th2))*tan(th3)+(tan(th2)+tan(th3))) by XCMPLX_1:151
  .=cos(th1)*cos(th2)*cos(th3)
  *((tan(th1)+(tan(th2)+tan(th3)))+-(tan(th1)*tan(th2))*tan(th3))
  by XCMPLX_1:1
  .=cos(th1)*cos(th2)*cos(th3)
  *(tan(th1)+tan(th2)+tan(th3)+-(tan(th1)*tan(th2))*tan(th3)) by XCMPLX_1:1
  .=cos(th1)*cos(th2)*cos(th3)
  *(tan(th1)+tan(th2)+tan(th3)---(tan(th1)*tan(th2))*tan(th3)) by XCMPLX_1:151
  .=cos(th1)*cos(th2)*cos(th3)
  *(tan(th1)+tan(th2)+tan(th3)-tan(th1)*tan(th2)*tan(th3));
  hence thesis;
end;

theorem TH0108:
  cos(th1)<>0 & cos(th2)<>0 & cos(th3)<>0 implies
  cos(th1+th2+th3)= cos(th1)*cos(th2)*cos(th3)
  *(1-tan(th2)*tan(th3)-tan(th3)*tan(th1)-tan(th1)*tan(th2))
proof
  assume
  A1: cos(th1)<>0 & cos(th2)<>0 & cos(th3)<>0;
  cos(th1+th2+th3)=cos(th1+(th2+th3)) by XCMPLX_1:1
  .=cos(th1)*cos(th2+th3)-sin(th1)*sin(th2+th3) by SIN_COS:80
  .=cos(th1)*(cos(th2)*cos(th3)-sin(th2)*sin(th3))
  -sin(th1)*sin(th2+th3) by SIN_COS:80
  .=cos(th1)*(cos(th2)*cos(th3)-sin(th2)*sin(th3))
  -sin(th1)*(sin(th2)*cos(th3)+cos(th2)*sin(th3)) by SIN_COS:80
  .=cos(th1)*(cos(th2)*cos(th3))-cos(th1)*(sin(th2)*sin(th3))
  -(sin(th1))*(sin(th2)*cos(th3)+cos(th2)*sin(th3)) by XCMPLX_1:40
  .=cos(th1)*(cos(th2)*cos(th3))-cos(th1)*((cos(th2)*tan(th2))*sin(th3))
  -(sin(th1)*(sin(th2)*cos(th3)+cos(th2)*sin(th3))) by TH0024,A1
  .=cos(th1)*(cos(th2)*cos(th3))
  -cos(th1)*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  -(sin(th1)*(sin(th2)*cos(th3)+cos(th2)*sin(th3))) by TH0024,A1
  .=cos(th1)*(cos(th2)*cos(th3))
  -cos(th1)*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  -((cos(th1)*tan(th1))*(sin(th2)*cos(th3) + cos(th2)*sin(th3))) by TH0024,A1
  .=cos(th1)*(cos(th2)*cos(th3))
  -cos(th1)*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  -((cos(th1)*tan(th1))*((cos(th2)*tan(th2))*cos(th3) + cos(th2)*sin(th3)))
  by TH0024,A1
  .=cos(th1)*(cos(th2)*cos(th3))
  -cos(th1)*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  -((cos(th1)*tan(th1))*((cos(th2)*tan(th2))*cos(th3)
  +cos(th2)*(cos(th3)*tan(th3)))) by TH0024,A1
  .=(cos(th1)*cos(th2))*cos(th3)
  -cos(th1)*((cos(th2)*tan(th2))*(cos(th3)*tan(th3)))
  -((cos(th1)*tan(th1))*((cos(th2)*tan(th2))*cos(th3)
  +cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -cos(th1)*(cos(th2)*(tan(th2)*(cos(th3)*tan(th3))))
  -((cos(th1)*tan(th1))*((cos(th2)*tan(th2))*cos(th3)
  +cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -cos(th1)*(cos(th2)*(cos(th3)*(tan(th2)*tan(th3))))
  -((cos(th1)*tan(th1))*((cos(th2)*tan(th2))*cos(th3)
  +cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -cos(th1)*((cos(th2)*cos(th3))*(tan(th2)*tan(th3)))
  -((cos(th1)*tan(th1))*((cos(th2)*tan(th2))*cos(th3)
  +cos(th2)*(cos(th3)*tan(th3))))by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -(cos(th1)*(cos(th2)*cos(th3)))*(tan(th2)*tan(th3))
  -((cos(th1)*tan(th1))*((cos(th2)*tan(th2))*cos(th3)
  +cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -((cos(th1)*tan(th1))*(((cos(th2)*tan(th2))*cos(th3))
  +cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -((cos(th1)*tan(th1))*(((cos(th2)*cos(th3))*tan(th2))
  +cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -((cos(th1)*tan(th1))*(((cos(th2)*cos(th3))*tan(th2)))
  +(cos(th1)*tan(th1))*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:8
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -(((cos(th1)*tan(th1))*((cos(th2)*cos(th3)))*tan(th2))
  +(cos(th1)*tan(th1))*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -(((cos(th1)*(cos(th2)*cos(th3)))*tan(th1)*tan(th2))
  +(cos(th1)*tan(th1))*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -((((cos(th1)*cos(th2))*cos(th3))*tan(th1)*tan(th2))
  +(cos(th1)*tan(th1))*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -(((cos(th1)*cos(th2))*cos(th3))*(tan(th1)*tan(th2))
  +(cos(th1)*tan(th1))*(cos(th2)*(cos(th3)*tan(th3)))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -(((cos(th1)*cos(th2))*cos(th3))*(tan(th1)*tan(th2))
  +(cos(th1)*tan(th1))*((cos(th2)*cos(th3))*tan(th3))) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -(((cos(th1)*cos(th2))*cos(th3))*(tan(th1)*tan(th2))
  +((cos(th1)*tan(th1))*(cos(th2)*cos(th3)))*tan(th3)) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -(((cos(th1)*cos(th2))*cos(th3))*(tan(th1)*tan(th2))
  +((cos(th1)*(cos(th2)*cos(th3)))*tan(th1))*tan(th3)) by XCMPLX_1:4
  .=(cos(th1)*cos(th2))*cos(th3)
  -((cos(th1)*cos(th2))*cos(th3))*(tan(th2)*tan(th3))
  -(((cos(th1)*cos(th2))*cos(th3))*(tan(th1)*tan(th2))
  +(((cos(th1)*cos(th2))*cos(th3))*tan(th1))*tan(th3)) by XCMPLX_1:4
  .=cos(th1)*cos(th2)*cos(th3)*1
  -cos(th1)*cos(th2)*cos(th3)*(tan(th2)*tan(th3))
  -(((cos(th1)*cos(th2))*cos(th3))*(tan(th1)*tan(th2))
  +((cos(th1)*cos(th2))*cos(th3))*(tan(th1)*tan(th3)))
  by XCMPLX_1:4
  .=cos(th1)*cos(th2)*cos(th3)*(1-tan(th2)*tan(th3))
  -(cos(th1)*cos(th2)*cos(th3)*(tan(th1)*tan(th2))
  +cos(th1)*cos(th2)*cos(th3)*(tan(th1)*tan(th3))) by XCMPLX_1:40
  .=cos(th1)*cos(th2)*cos(th3)*(1-tan(th2)*tan(th3))
  -(cos(th1)*cos(th2)*cos(th3)*(tan(th1)*tan(th2)+tan(th1)*tan(th3)))
  by XCMPLX_1:8
  .=cos(th1)*cos(th2)*cos(th3)
  *(1-tan(th2)*tan(th3)-(tan(th3)*tan(th1)+tan(th1)*tan(th2)))
  by XCMPLX_1:40
  .=cos(th1)*cos(th2)*cos(th3)
  *(1-tan(th2)*tan(th3)-tan(th3)*tan(th1)-tan(th1)*tan(th2)) by XCMPLX_1:36;
  hence thesis;
end;

theorem
  cos(th1)<>0 & cos(th2)<>0 & cos(th3)<>0 implies
  tan(th1+th2+th3)
  = (tan(th1)+tan(th2)+tan(th3)-tan(th1)*tan(th2)*tan(th3))
  /(1-tan(th2)*tan(th3)-tan(th3)*tan(th1)-tan(th1)*tan(th2))
proof
  assume
  A1: cos(th1)<>0 & cos(th2)<>0 & cos(th3)<>0; then
  cos(th1)*cos(th2) <> 0 by XCMPLX_1:6; then
  A2: cos(th1)*cos(th2)*cos(th3) <> 0 by A1, XCMPLX_1:6;
  tan(th1+th2+th3) = sin(th1+th2+th3)/cos(th1+th2+th3) by DF000
  .=cos(th1)*cos(th2)*cos(th3)
  *(tan(th1)+tan(th2)+tan(th3)-tan(th1)*tan(th2)*tan(th3))/cos(th1+th2+th3)
  by TH0107, A1
  .=cos(th1)*cos(th2)*cos(th3)
  *(tan(th1)+tan(th2)+tan(th3)-tan(th1)*tan(th2)*tan(th3))
  /(cos(th1)*cos(th2)*cos(th3)
  *(1-tan(th2)*tan(th3)-tan(th3)*tan(th1)-tan(th1)*tan(th2))) by TH0108, A1
  .=(cos(th1)*cos(th2)*cos(th3)/(cos(th1)*cos(th2)*cos(th3)))
  /((1-tan(th2)*tan(th3)-tan(th3)*tan(th1)-tan(th1)*tan(th2))
  /(tan(th1)+tan(th2)+tan(th3)-tan(th1)*tan(th2)*tan(th3))) by XCMPLX_1:85
  .=1/((1-tan(th2)*tan(th3)-tan(th3)*tan(th1)-tan(th1)*tan(th2))
  /(tan(th1)+tan(th2)+tan(th3)-tan(th1)*tan(th2)*tan(th3))) by XCMPLX_1:60, A2
  .=(tan(th1)+tan(th2)+tan(th3)-(tan(th1)*tan(th2)*tan(th3)))
  /(1-tan(th2)*tan(th3)-tan(th3)*tan(th1)-tan(th1)*tan(th2)) by XCMPLX_1:57;
  hence thesis;
end;

theorem
  sin(th1) <> 0 & sin(th2) <> 0 & sin(th3) <> 0 implies
  cot(th1+th2+th3)
  = (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th2)*cot(th3)+cot(th3)*cot(th1)+cot(th1)*cot(th2)-1)
proof
  assume
  A1: sin(th1) <> 0 & sin(th2) <> 0 & sin(th3) <> 0; then
  A2: sin(th1)*sin(th2) <> 0 by XCMPLX_1:6; then
  A3: sin(th1)*sin(th2)*sin(th3) <> 0 by A1, XCMPLX_1:6;
  cot(th1+th2+th3)
  = cos(th1+th2+th3)/sin(th1+th2+th3) by DF001
  .= (cos(th1+th2)*cos(th3)-sin(th1+th2)*sin(th3))
  /sin(th1+th2+th3) by SIN_COS:80
  .= ((cos(th1)*cos(th2)-sin(th1)*sin(th2))*cos(th3)-sin(th1+th2)*sin(th3))
  /sin(th1+th2+th3) by SIN_COS:80
  .= (cos(th3)*(cos(th1)*cos(th2)-sin(th1)*sin(th2))
  -(sin(th3)*(sin(th1)*cos(th2)+cos(th1)*sin(th2))))
  /sin(th1+th2+th3) by SIN_COS:80
  .= ((cos(th3)*(cos(th1)*cos(th2)-sin(th1)*sin(th2))
  -(sin(th3)*(sin(th1)*cos(th2)+cos(th1)*sin(th2))))
  /(sin(th1)*sin(th2)*sin(th3)))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by A3, XCMPLX_1:55
  .= ((cos(th3)*(cos(th1)*cos(th2)-sin(th1)*sin(th2))
  /(sin(th3)*(sin(th1)*sin(th2))))
  -((sin(th3)*(sin(th1)*cos(th2)+cos(th1)*sin(th2)))
  /(sin(th3)*(sin(th1)*sin(th2)))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by XCMPLX_1:121
  .= (((cos(th3)/sin(th3))*((cos(th1)*cos(th2)-sin(th1)*sin(th2))
  /(sin(th1)*sin(th2)))) -((sin(th3)*(sin(th1)*cos(th2)+cos(th1)*sin(th2)))
  /(sin(th3)*(sin(th1)*sin(th2)))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by XCMPLX_1:77
  .= (cot(th3)*((cos(th1)*cos(th2)-sin(th1)*sin(th2))/(sin(th1)*sin(th2)))
  -((sin(th3)*(sin(th1)*cos(th2)+cos(th1)*sin(th2)))
  /(sin(th3)*(sin(th1)*sin(th2)))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by DF001
  .= (cot(th3)*((cos(th1)*cos(th2)/(sin(th1)*sin(th2)))
  -(sin(th1)*sin(th2))/(sin(th1)*sin(th2)))
  -((sin(th3)*(sin(th1)*cos(th2)+cos(th1)*sin(th2)))
  /(sin(th3)*(sin(th1)*sin(th2)))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by XCMPLX_1:121
  .= (cot(th3)*((cos(th1)/sin(th1))*(cos(th2)/(sin(th2)))
  -(sin(th1)*sin(th2))/(sin(th1)*sin(th2)))
  -((sin(th3)*(sin(th1)*cos(th2)+cos(th1)*sin(th2)))
  /(sin(th3)*(sin(th1)*sin(th2)))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by XCMPLX_1:77
  .= (cot(th3)*(cot(th1)*(cos(th2)/(sin(th2)))
  -(sin(th1)*sin(th2))/(sin(th1)*sin(th2)))
  -((sin(th3)*(sin(th1)*cos(th2)+cos(th1)*sin(th2)))
  /(sin(th3)*(sin(th1)*sin(th2)))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by DF001
  .= (cot(th3)*(cot(th1)*cot(th2)
  -(sin(th1)*sin(th2))/(sin(th1)*sin(th2)))
  -((sin(th3)*(sin(th1)*cos(th2)+cos(th1)*sin(th2)))
  /(sin(th3)*(sin(th1)*sin(th2)))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by DF001
  .= (cot(th3)*(cot(th1)*cot(th2)-1)
  -(((sin(th1)*cos(th2)+cos(th1)*sin(th2))*sin(th3))
  /(sin(th1)*sin(th2)*sin(th3))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by A2, XCMPLX_1:60
  .= (cot(th3)*(cot(th1)*cot(th2)-1)
  -((sin(th1)*cos(th2)+cos(th1)*sin(th2))/(sin(th1)*sin(th2))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by A1, XCMPLX_1:92
  .= (cot(th3)*(cot(th1)*cot(th2)-1)
  -((sin(th1)*cos(th2)/(sin(th1)*sin(th2)))
  +(cos(th1)*sin(th2))/(sin(th1)*sin(th2))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by XCMPLX_1:63
  .= (cot(th3)*(cot(th1)*cot(th2)-1)
  -(cos(th2)/sin(th2)+(cos(th1)*sin(th2))/(sin(th1)*sin(th2))))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by A1, XCMPLX_1:92
  .= (cot(th3)*(cot(th1)*cot(th2)-1)
  -(cos(th2)/sin(th2)+cos(th1)/sin(th1)))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by A1, XCMPLX_1:92
  .= (cot(th3)*(cot(th1)*cot(th2)-1)-(cot(th2)+cos(th1)/sin(th1)))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by DF001
  .= (cot(th3)*(cot(th1)*cot(th2)-1)-(cot(th2)+cot(th1)))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by DF001
  .= ((cot(th1)*cot(th2)*cot(th3)-cot(th3)*1)-(cot(th2)+cot(th1)))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by XCMPLX_1:40
  .= (cot(th1)*cot(th2)*cot(th3)-(cot(th2)+cot(th1))-cot(th3))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3))) by XCMPLX_1:21
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(sin(th1+th2+th3)/(sin(th1)*sin(th2)*sin(th3)))
  by XCMPLX_1:36
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /((sin(th1+th2)*cos(th3)+cos(th1+th2)*sin(th3))
  /(sin(th1)*sin(th2)*sin(th3))) by SIN_COS:80
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(sin(th1+th2)*cos(th3)/(sin(th1)*sin(th2)*sin(th3))
  +cos(th1+th2)*sin(th3)/(sin(th1)*sin(th2)*sin(th3))) by XCMPLX_1:63
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(sin(th1+th2)*cos(th3)/(sin(th1)*sin(th2)*sin(th3))
  +cos(th1+th2)/(sin(th1)*sin(th2))) by A1, XCMPLX_1:92
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /((cos(th3)/sin(th3))*(sin(th1+th2)/(sin(th1)*sin(th2)))
  +cos(th1+th2)/(sin(th1)*sin(th2))) by XCMPLX_1:77
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(sin(th1+th2)/(sin(th1)*sin(th2)))
  +cos(th1+th2)/(sin(th1)*sin(th2))) by DF001
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*((sin(th1)*cos(th2)+cos(th1)*sin(th2))/(sin(th1)*sin(th2)))
  +cos(th1+th2)/(sin(th1)*sin(th2))) by SIN_COS:80
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*((sin(th1)*cos(th2)/(sin(th1)*sin(th2))
  +cos(th1)*sin(th2)/(sin(th1)*sin(th2))))
  +cos(th1+th2)/(sin(th1)*sin(th2))) by XCMPLX_1:63
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cos(th2)/sin(th2)+cos(th1)*sin(th2)/(sin(th1)*sin(th2)))
  +cos(th1+th2)/(sin(th1)*sin(th2))) by A1, XCMPLX_1:92
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cos(th2)/sin(th2)+cos(th1)/sin(th1))
  +cos(th1+th2)/(sin(th1)*sin(th2))) by A1, XCMPLX_1:92
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cot(th2)+cos(th1)/sin(th1))
  +cos(th1+th2)/(sin(th1)*sin(th2))) by DF001
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cot(th2)+cot(th1))+cos(th1+th2)/(sin(th1)*sin(th2))) by DF001
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cot(th2)+cot(th1))
  +(cos(th1)*cos(th2)-sin(th1)*sin(th2))/(sin(th1)*sin(th2))) by SIN_COS:80
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cot(th2)+cot(th1))
  +(cos(th1)*cos(th2)/(sin(th1)*sin(th2))
  -sin(th1)*sin(th2)/(sin(th1)*sin(th2)))) by XCMPLX_1:121
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cot(th2)+cot(th1))
  +(cos(th1)*cos(th2)/(sin(th1)*sin(th2))-1)) by A2, XCMPLX_1:60
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cot(th2)+cot(th1))
  +((cos(th1)/sin(th1))*(cos(th2)/sin(th2))-1)) by XCMPLX_1:77
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cot(th2)+cot(th1))
  +(cot(th1)*(cos(th2)/sin(th2))-1)) by DF001
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th3)*(cot(th2)+cot(th1))+(cot(th1)*cot(th2)-1)) by DF001
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th2)*cot(th3)+cot(th3)*cot(th1)+(cot(th1)*cot(th2)-1)) by XCMPLX_1:8
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th2)*cot(th3)+cot(th3)*cot(th1)-(1-cot(th1)*cot(th2))) by XCMPLX_1:38
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th2)*cot(th3)+cot(th3)*cot(th1)-1+cot(th1)*cot(th2)) by XCMPLX_1:37
  .= (cot(th1)*cot(th2)*cot(th3)-cot(th1)-cot(th2)-cot(th3))
  /(cot(th2)*cot(th3)+cot(th3)*cot(th1)+cot(th1)*cot(th2)-1) by XCMPLX_1:29;
  hence thesis;
end;

theorem TH0201:
  sin(th1)+sin(th2)= 2*(cos((th1-th2)/2)*sin((th1+th2)/2))
proof
  sin(th1)+sin(th2)= sin(th1/2+th1/2)+sin(th2) by XCMPLX_1:66
  .= sin(th1/2+th1/2)+sin(th2/2+th2/2) by XCMPLX_1:66
  .= sin(th1/2)*cos(th1/2)+cos(th1/2)*sin(th1/2)+sin(th2/2+th2/2) by SIN_COS:80
  .= sin(th1/2)*cos(th1/2)+sin(th1/2)*cos(th1/2)
  +(sin(th2/2)*cos(th2/2)+sin(th2/2)*cos(th2/2)) by SIN_COS:80
  .= 2*(sin(th1/2)*cos(th1/2))
  +(sin(th2/2)*cos(th2/2)+sin(th2/2)*cos(th2/2)) by XCMPLX_1:11
  .= 2*(sin(th1/2)*cos(th1/2))+2*(sin(th2/2)*cos(th2/2)) by XCMPLX_1:11
  .= 2*(sin(th1/2)*cos(th1/2)*1+sin(th2/2)*cos(th2/2))
  by XCMPLX_1:8
  .= 2*(sin(th1/2)*cos(th1/2) *(cos(th2/2)*cos(th2/2)+sin(th2/2)*sin(th2/2))
  +sin(th2/2)*cos(th2/2)*1) by SIN_COS:32
  .= 2*(sin(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2)+sin(th2/2)*sin(th2/2))
  +sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2)+sin(th1/2)*sin(th1/2)))
  by SIN_COS:32
  .= 2*(sin(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  +sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2)+sin(th1/2)*sin(th1/2)))
  by XCMPLX_1:8
  .= 2*(sin(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  +(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:8
  .= 2*(sin(th1/2)*(cos(th1/2)*(cos(th2/2)*cos(th2/2)))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  +(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*(cos(th2/2)*cos(th1/2)*cos(th2/2))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  +(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  +(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +(sin(th1/2)*cos(th1/2)*(sin(th2/2))*sin(th2/2))
  +(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +((cos(th1/2)*sin(th2/2))*sin(th1/2))*sin(th2/2)
  +(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +((cos(th1/2)*sin(th2/2))*(sin(th1/2))*sin(th2/2))
  +(cos(th1/2)*(sin(th2/2)*cos(th2/2))*cos(th1/2)
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +((cos(th1/2)*sin(th2/2))*sin(th1/2))*sin(th2/2)
  +(cos(th1/2)*sin(th2/2)*cos(th2/2)*cos(th1/2)
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +(cos(th1/2)*sin(th2/2)*sin(th1/2))*sin(th2/2)
  +(cos(th1/2)*sin(th2/2)*(cos(th1/2)*cos(th2/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +cos(th1/2)*sin(th2/2)*(sin(th1/2)*sin(th2/2))
  +(cos(th1/2)*sin(th2/2)*(cos(th1/2)*cos(th2/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +cos(th1/2)*sin(th2/2)*(sin(th1/2)*sin(th2/2))
  +(cos(th1/2)*sin(th2/2)*(cos(th1/2)*cos(th2/2))
  +(sin(th2/2)*cos(th2/2)*sin(th1/2))*sin(th1/2))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +cos(th1/2)*sin(th2/2)*(sin(th1/2)*sin(th2/2))
  +(cos(th1/2)*sin(th2/2)*(cos(th1/2)*cos(th2/2))
  +sin(th1/2)*cos(th2/2)*sin(th2/2)*sin(th1/2))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +cos(th1/2)*sin(th2/2)*(sin(th1/2)*sin(th2/2))
  +(cos(th1/2)*sin(th2/2)*(cos(th1/2)*cos(th2/2))
  +sin(th1/2)*cos(th2/2)*(sin(th1/2)*sin(th2/2)))) by XCMPLX_1:4
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +((sin(th1/2)*cos(th2/2)*(sin(th1/2)*sin(th2/2))
  +cos(th1/2)*sin(th2/2)*(cos(th1/2)*cos(th2/2)))
  +cos(th1/2)*sin(th2/2)*(sin(th1/2)*sin(th2/2))))
  by XCMPLX_1:1
  .= 2*(sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +(sin(th1/2)*cos(th2/2)*(sin(th1/2)*sin(th2/2))
  +(cos(th1/2)*sin(th2/2)*(cos(th1/2)*cos(th2/2))
  +cos(th1/2)*sin(th2/2)*(sin(th1/2)*sin(th2/2))))) by XCMPLX_1:1
  .= 2*((sin(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  +sin(th1/2)*cos(th2/2)*(sin(th1/2)*sin(th2/2)))
  +(cos(th1/2)*sin(th2/2)*(cos(th1/2)*cos(th2/2))
  +cos(th1/2)*sin(th2/2)*(sin(th1/2)*sin(th2/2)))) by XCMPLX_1:1
  .= 2*((sin(th1/2)*cos(th2/2)
  *(cos(th1/2)*cos(th2/2)+sin(th1/2)*sin(th2/2)))
  +(cos(th1/2)*sin(th2/2)*(cos(th1/2)*cos(th2/2))
  +cos(th1/2)*sin(th2/2)*(sin(th1/2)*sin(th2/2)))) by XCMPLX_1:8
  .= 2*((sin(th1/2)*cos(th2/2)
  *(cos(th1/2)*cos(th2/2)+sin(th1/2)*sin(th2/2)))
  +(cos(th1/2)*sin(th2/2)
  *(cos(th1/2)*cos(th2/2)+sin(th1/2)*sin(th2/2)))) by XCMPLX_1:8
  .= 2*((sin(th1/2)*cos(th2/2)+cos(th1/2)*sin(th2/2))
  *(cos(th1/2)*cos(th2/2)+sin(th1/2)*sin(th2/2))) by XCMPLX_1:8
  .= 2*((sin(th1/2+th2/2))
  *(cos(th1/2)*cos(th2/2)+sin(th1/2)*sin(th2/2))) by SIN_COS:80
  .= 2*(sin(th1/2+th2/2)*cos(th1/2-th2/2)) by TH0102
  .= 2*(cos(th1/2-th2/2)*sin((th1+th2)/2)) by XCMPLX_1:63
  .= 2*(cos((th1-th2)/2)*sin((th1+th2)/2)) by XCMPLX_1:121;
  hence thesis;
end;

theorem TH0202:
  sin(th1)-sin(th2)= 2*(cos((th1+th2)/2)*sin((th1-th2)/2))
proof
  sin(th1)-sin(th2)= sin(th1/2+th1/2)-sin(th2) by XCMPLX_1:66
  .= sin(th1/2+th1/2)-sin(th2/2+th2/2) by XCMPLX_1:66
  .= sin(th1/2)*cos(th1/2)+cos(th1/2)*sin(th1/2)-sin(th2/2+th2/2) by SIN_COS:80
  .= sin(th1/2)*cos(th1/2)+sin(th1/2)*cos(th1/2)
  -(sin(th2/2)*cos(th2/2)+sin(th2/2)*cos(th2/2)) by SIN_COS:80
  .= 2*(sin(th1/2)*cos(th1/2))
  -(sin(th2/2)*cos(th2/2)+sin(th2/2)*cos(th2/2)) by XCMPLX_1:11
  .= 2*(sin(th1/2)*cos(th1/2))-2*(sin(th2/2)*cos(th2/2)) by XCMPLX_1:11
  .= 2*(sin(th1/2)*cos(th1/2)*1-sin(th2/2)*cos(th2/2))
  by XCMPLX_1:40
  .= 2*(sin(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2)+sin(th2/2)*sin(th2/2))
  -sin(th2/2)*cos(th2/2)*1)
  by SIN_COS:32
  .= 2*(sin(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2)+sin(th2/2)*sin(th2/2))
  -sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2)+sin(th1/2)*sin(th1/2)))
  by SIN_COS:32
  .= 2*(sin(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  -(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2)+sin(th1/2)*sin(th1/2))))
  by XCMPLX_1:8
  .= 2*(sin(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  -(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:8
  .= 2*(cos(th1/2)*(cos(th2/2)*(cos(th2/2))*sin(th1/2))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  -(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*(cos(th2/2)*(sin(th1/2)*cos(th2/2)))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  -(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  -(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*((sin(th2/2)*sin(th2/2))*cos(th1/2))
  -(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*(sin(th2/2)*(sin(th2/2)*cos(th1/2)))
  -(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*sin(th2/2)*(sin(th2/2)*cos(th1/2))
  -(sin(th2/2)*cos(th2/2)*(cos(th1/2)*cos(th1/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*sin(th2/2)*(sin(th2/2)*cos(th1/2))
  -((cos(th2/2)*(cos(th1/2)*cos(th1/2)))*sin(th2/2)
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*sin(th2/2)*(sin(th2/2)*cos(th1/2))
  -(((cos(th2/2)*cos(th1/2))*cos(th1/2))*sin(th2/2)
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*sin(th2/2)*(sin(th2/2)*cos(th1/2))
  -(cos(th2/2)*cos(th1/2)*(cos(th1/2)*sin(th2/2))
  +sin(th2/2)*cos(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*sin(th2/2)*(sin(th2/2)*cos(th1/2))
  -(cos(th2/2)*cos(th1/2)*(cos(th1/2)*sin(th2/2))
  +sin(th2/2)*((sin(th1/2)*sin(th1/2)*cos(th2/2))))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*sin(th2/2)*(sin(th2/2)*cos(th1/2))
  -(cos(th2/2)*cos(th1/2)*(cos(th1/2)*sin(th2/2))
  +sin(th2/2)*(sin(th1/2)*(sin(th1/2)*cos(th2/2))))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*sin(th2/2)*(cos(th1/2)*sin(th2/2))
  -(cos(th1/2)*cos(th2/2)*(cos(th1/2)*sin(th2/2))
  +sin(th1/2)*sin(th2/2)*(sin(th1/2)*cos(th2/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*sin(th2/2)*(cos(th1/2)*sin(th2/2))
  --(-cos(th1/2)*cos(th2/2)*(cos(th1/2)*sin(th2/2))
  +-sin(th1/2)*sin(th2/2)*(sin(th1/2)*cos(th2/2)))) by XCMPLX_1:142
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +sin(th1/2)*sin(th2/2)*(cos(th1/2)*sin(th2/2))
  +(-cos(th1/2)*cos(th2/2)*(cos(th1/2)*sin(th2/2))
  +-sin(th1/2)*sin(th2/2)*(sin(th1/2)*cos(th2/2)))) by XCMPLX_1:151
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +(sin(th1/2)*sin(th2/2)*(cos(th1/2)*sin(th2/2))
  +(-cos(th1/2)*cos(th2/2)*(cos(th1/2)*sin(th2/2))
  +-sin(th1/2)*sin(th2/2)*(sin(th1/2)*cos(th2/2))))) by XCMPLX_1:1
  .= 2*(cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +(-cos(th1/2)*cos(th2/2)*(cos(th1/2)*sin(th2/2))
  +(sin(th1/2)*sin(th2/2)*(cos(th1/2)*sin(th2/2))
  +-sin(th1/2)*sin(th2/2)*(sin(th1/2)*cos(th2/2)))))
  by XCMPLX_1:1
  .= 2*((cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +-cos(th1/2)*cos(th2/2)*(--cos(th1/2)*sin(th2/2)))
  +(sin(th1/2)*sin(th2/2)*(cos(th1/2)*sin(th2/2))
  +-sin(th1/2)*sin(th2/2)*(sin(th1/2)*cos(th2/2))))
  by XCMPLX_1:1
  .= 2*((cos(th1/2)*cos(th2/2)*(sin(th1/2)*cos(th2/2))
  +cos(th1/2)*cos(th2/2)*(-cos(th1/2)*sin(th2/2)))
  +(sin(th1/2)*sin(th2/2)*(cos(th1/2)*sin(th2/2))
  +-sin(th1/2)*sin(th2/2)*(sin(th1/2)*cos(th2/2)))) by XCMPLX_1:178
  .= 2*((cos(th1/2)*cos(th2/2)
  *(sin(th1/2)*cos(th2/2)+(-cos(th1/2)*sin(th2/2))))
  +(sin(th1/2)*sin(th2/2)*(cos(th1/2)*sin(th2/2))
  +-sin(th1/2)*sin(th2/2)*(--sin(th1/2)*cos(th2/2))))
  by XCMPLX_1:8
  .= 2*((cos(th1/2)*cos(th2/2)
  *(sin(th1/2)*cos(th2/2)+(-cos(th1/2)*sin(th2/2))))
  +(sin(th1/2)*sin(th2/2)*(cos(th1/2)*sin(th2/2))
  +sin(th1/2)*sin(th2/2)*(-sin(th1/2)*cos(th2/2)))) by XCMPLX_1:178
  .= 2*((cos(th1/2)*cos(th2/2)
  *(sin(th1/2)*cos(th2/2)+(-cos(th1/2)*sin(th2/2))))
  +(sin(th1/2)*sin(th2/2)*((-sin(th1/2)*cos(th2/2))+cos(th1/2)*sin(th2/2))))
  by XCMPLX_1:8
  .= 2*((cos(th1/2)*cos(th2/2)
  *(sin(th1/2)*cos(th2/2)+(-cos(th1/2)*sin(th2/2))))
  +((-sin(th1/2)*sin(th2/2))
  *(-((-sin(th1/2)*cos(th2/2))+cos(th1/2)*sin(th2/2))))) by XCMPLX_1:177
  .= 2*((((sin(th1/2)*cos(th2/2)+-(cos(th1/2)*sin(th2/2)))
  *(cos(th1/2)*cos(th2/2))))
  +((sin(th1/2)*cos(th2/2)-cos(th1/2)*sin(th2/2))*(-sin(th1/2)*sin(th2/2))))
  by XCMPLX_1:163
  .= 2*((((sin(th1/2)*cos(th2/2)---(cos(th1/2)*sin(th2/2)))
  *(cos(th1/2)*cos(th2/2))))
  +((sin(th1/2)*cos(th2/2)-cos(th1/2)*sin(th2/2))*(-sin(th1/2)*sin(th2/2))))
  by XCMPLX_1:151
  .= 2*((sin(th1/2)*cos(th2/2)-cos(th1/2)*sin(th2/2))
  *(cos(th1/2)*cos(th2/2)+-sin(th1/2)*sin(th2/2)))
  by XCMPLX_1:8
  .= 2*((sin(th1/2)*cos(th2/2)-cos(th1/2)*sin(th2/2))
  *(cos(th1/2)*cos(th2/2)---sin(th1/2)*sin(th2/2))) by XCMPLX_1:151
  .= 2*(sin(th1/2-th2/2)*(cos(th1/2)*cos(th2/2)-sin(th1/2)*sin(th2/2)))
  by TH0101
  .= 2*(sin(th1/2-th2/2)*(cos(th1/2+th2/2))) by SIN_COS:80
  .= 2*(sin((th1-th2)/2)*(cos(th1/2+th2/2))) by XCMPLX_1:121
  .= 2*(cos((th1+th2)/2)*sin((th1-th2)/2)) by XCMPLX_1:63;
  hence thesis;
end;

theorem TH0203:
  cos(th1)+cos(th2)= 2*(cos((th1+th2)/2)*cos((th1-th2)/2))
proof
  cos(th1)+cos(th2)= cos(th1/2+th1/2)+cos(th2) by XCMPLX_1:66
  .= cos(th1/2+th1/2)+cos(th2/2+th2/2) by XCMPLX_1:66
  .= cos(th1/2)*cos(th1/2)-sin(th1/2)*sin(th1/2)+cos(th2/2+th2/2)
  by SIN_COS:80
  .= ((cos(th1/2)*cos(th1/2)---sin(th1/2)*sin(th1/2))+(1---1))
  +(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2))
  by SIN_COS:80
  .= ((cos(th1/2)*cos(th1/2)+(-sin(th1/2)*sin(th1/2)))+(1+(-1)))
  +(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2)) by XCMPLX_1:151
  .= (((cos(th1/2)*cos(th1/2)+(-sin(th1/2)*sin(th1/2)))+1)+(-1))
  +(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2)) by XCMPLX_1:1
  .= ((cos(th1/2)*cos(th1/2)+(-sin(th1/2)*sin(th1/2)))+1)
  +((-1)+(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2))) by XCMPLX_1:1
  .= ((cos(th1/2)*cos(th1/2)+(-sin(th1/2)*sin(th1/2)))
  +(sin(th1/2)*sin(th1/2)+cos(th1/2)*cos(th1/2)))
  +((-1)+(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2))) by SIN_COS:32
  .= ((cos(th1/2)*cos(th1/2)+(-sin(th1/2)*sin(th1/2)))
  +(sin(th1/2)*sin(th1/2)+cos(th1/2)*cos(th1/2)))
  +((-(sin(th2/2)*sin(th2/2)+cos(th2/2)*cos(th2/2)))
  +(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2))) by SIN_COS:32
  .= (cos(th1/2)*cos(th1/2)+((-sin(th1/2)*sin(th1/2))
  +(sin(th1/2)*sin(th1/2)+cos(th1/2)*cos(th1/2))))
  +((-(sin(th2/2)*sin(th2/2)+cos(th2/2)*cos(th2/2)))
  +(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2))) by XCMPLX_1:1
  .= (cos(th1/2)*cos(th1/2)+((sin(th1/2)*sin(th1/2)
  +(-sin(th1/2)*sin(th1/2)))+cos(th1/2)*cos(th1/2)))
  +((-(sin(th2/2)*sin(th2/2)+cos(th2/2)*cos(th2/2)))
  +(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2))) by XCMPLX_1:1
  .= (cos(th1/2)*cos(th1/2)+((sin(th1/2)*sin(th1/2)
  --(-sin(th1/2)*sin(th1/2)))+cos(th1/2)*cos(th1/2)))
  +((-(sin(th2/2)*sin(th2/2)+cos(th2/2)*cos(th2/2)))
  +(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2))) by XCMPLX_1:151
  .= (cos(th1/2)*cos(th1/2)+(cos(th1/2)*cos(th1/2)))
  +((-(sin(th2/2)*sin(th2/2)+cos(th2/2)*cos(th2/2)))
  +(cos(th2/2)*cos(th2/2)-sin(th2/2)*sin(th2/2)))
  by XCMPLX_1:25
  .= (cos(th1/2)*cos(th1/2)+(cos(th1/2)*cos(th1/2)))
  +((-sin(th2/2)*sin(th2/2)+-cos(th2/2)*cos(th2/2))
  +(cos(th2/2)*cos(th2/2)---sin(th2/2)*sin(th2/2)))
  by XCMPLX_1:140
  .= (cos(th1/2)*cos(th1/2)+(cos(th1/2)*cos(th1/2)))
  +((-sin(th2/2)*sin(th2/2)+-cos(th2/2)*cos(th2/2))
  +(cos(th2/2)*cos(th2/2)+-sin(th2/2)*sin(th2/2))) by XCMPLX_1:151
  .= (cos(th1/2)*cos(th1/2)+(cos(th1/2)*cos(th1/2)))
  +(((-sin(th2/2)*sin(th2/2)+-cos(th2/2)*cos(th2/2))
  +cos(th2/2)*cos(th2/2))+-sin(th2/2)*sin(th2/2)) by XCMPLX_1:1
  .= (cos(th1/2)*cos(th1/2)+(cos(th1/2)*cos(th1/2)))
  +((-sin(th2/2)*sin(th2/2)+(cos(th2/2)*cos(th2/2)
  +-cos(th2/2)*cos(th2/2)))+-sin(th2/2)*sin(th2/2)) by XCMPLX_1:1
  .= (cos(th1/2)*cos(th1/2)+(cos(th1/2)*cos(th1/2)))
  +((-sin(th2/2)*sin(th2/2)+(cos(th2/2)*cos(th2/2)
  ---cos(th2/2)*cos(th2/2)))+-sin(th2/2)*sin(th2/2)) by XCMPLX_1:151
  .= cos(th1/2)*cos(th1/2)+cos(th1/2)*cos(th1/2)
  +(-sin(th2/2)*sin(th2/2)+-sin(th2/2)*sin(th2/2))
  by XCMPLX_1:25
  .= 2*(cos(th1/2)*cos(th1/2))
  +(-sin(th2/2)*sin(th2/2)+-sin(th2/2)*sin(th2/2)) by XCMPLX_1:11
  .= 2*(cos(th1/2)*cos(th1/2))+2*(-sin(th2/2)*sin(th2/2)) by XCMPLX_1:11
  .= 2*(cos(th1/2)*cos(th1/2)+-sin(th2/2)*sin(th2/2)) by XCMPLX_1:8
  .= 2*(cos(th1/2)*cos(th1/2)---sin(th2/2)*sin(th2/2)) by XCMPLX_1:151
  .= 2*(cos(th1/2)*cos(th1/2)-(sin(th2/2)*sin(th2/2)*(1)))
  .= 2*(cos(th1/2)*cos(th1/2)-(sin(th2/2)*sin(th2/2)
  *(cos(th1/2)*cos(th1/2)+sin(th1/2)*sin(th1/2))))
  by SIN_COS:32
  .= 2*(cos(th1/2)*cos(th1/2)
  ---(cos(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  +sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2))))
  by XCMPLX_1:8
  .= 2*(cos(th1/2)*cos(th1/2)
  +-(cos(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  +sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:151
  .= 2*(cos(th1/2)*cos(th1/2)
  +(-cos(th1/2)*cos(th1/2)*(sin(th2/2)*sin(th2/2))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:140
  .= 2*((cos(th1/2)*cos(th1/2)
  +-cos(th1/2)*cos(th1/2)*(--sin(th2/2)*sin(th2/2)))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2)))
  by XCMPLX_1:1
  .= 2*((cos(th1/2)*cos(th1/2)*1
  +cos(th1/2)*cos(th1/2)*(-sin(th2/2)*sin(th2/2)))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2)))
  by XCMPLX_1:178
  .= 2*(cos(th1/2)*cos(th1/2)*(1+-sin(th2/2)*sin(th2/2))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2))) by XCMPLX_1:8
  .= 2*(cos(th1/2)*cos(th1/2)*(1---sin(th2/2)*sin(th2/2))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2))) by XCMPLX_1:151
  .= 2*(cos(th1/2)*cos(th1/2)*((cos(th2/2)*cos(th2/2)+sin(th2/2)*sin(th2/2))
  ---sin(th2/2)*sin(th2/2))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2)))
  by SIN_COS:32
  .= 2*(cos(th1/2)*cos(th1/2)*((cos(th2/2)*cos(th2/2)+sin(th2/2)*sin(th2/2))
  +-sin(th2/2)*sin(th2/2))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2))) by XCMPLX_1:151
  .= 2*(cos(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2)+(sin(th2/2)*sin(th2/2)
  +-sin(th2/2)*sin(th2/2)))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2))) by XCMPLX_1:1
  .= 2*(cos(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2)+(sin(th2/2)*sin(th2/2)
  ---sin(th2/2)*sin(th2/2)))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2))) by XCMPLX_1:151
  .= 2*(cos(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2))
  +-sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2)))
  by XCMPLX_1:25
  .= 2*(cos(th1/2)*cos(th1/2)*(cos(th2/2)*cos(th2/2))
  ---sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2))) by XCMPLX_1:151
  .= 2*(cos(th1/2)*(cos(th1/2)*(cos(th2/2)*cos(th2/2)))
  -sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2)))
  by XCMPLX_1:4
  .= 2*(cos(th1/2)*((cos(th2/2)*cos(th1/2))*cos(th2/2))
  -sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  -sin(th2/2)*sin(th2/2)*(sin(th1/2)*sin(th1/2))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  -sin(th2/2)*(sin(th2/2)*(sin(th1/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
  -sin(th2/2)*(sin(th1/2)*(sin(th2/2)*sin(th1/2)))) by XCMPLX_1:4
  .= 2*(cos(th1/2)*cos(th2/2)*(cos(th1/2)*cos(th2/2))
