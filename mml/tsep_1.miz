              then
A35:          C /\ (A1 \/ A2) c= A1 /\ A2 by A31,A34,Th4;
A36:          A1 /\ A2 c= A1 by XBOOLE_1:17;
              then
A37:          C /\ (A1 \/ A2) c= A1 by A35,XBOOLE_1:1;
              now
                per cases;
