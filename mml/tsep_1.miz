              .= (C1 \/ (C2 \/ C)) /\ (A1 \/ A2) by XBOOLE_1:4
              .= (C1 /\ (A1 \/ A2)) \/ ((C2 \/ C) /\ (A1 \/ A2)) by XBOOLE_1
            :23
              .= {} \/ ((C2 \/ C) /\ (A1 \/ A2)) by A12,XBOOLE_0:def 7
