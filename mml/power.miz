Z:  k is odd;
  then consider l such that
A2: k=2*l + 1 by ABIAN:1;
  per cases;
    suppose
S:    a = 0;
   k <> 0 by Z;
      then a to_power k = 0 by S,Th49;
     hence thesis by S;
    end;
    suppose
A4:   a>0;
then A5:   -a<>-0;
   a to_power k = a to_power (2*l) * a to_power 1 by A2,A4,Th32
        .= a to_power (2*l) * a by Th30
        .= (-a) to_power (2*l) * a by Th54
        .= - (-a) to_power (2*l) * (-a)
        .= - (-a) #Z (2*l) * (-a) by Def2
        .= - (-a) #Z (2*l) * (-a) #Z 1 by PREPOWER:45
        .= - (-a) #Z k by A2,A5,PREPOWER:54
        .= - (-a) to_power k by Def2;
      hence thesis;
    end;
    suppose
A7:   a<0;
then    -a>0 by XREAL_1:60;
