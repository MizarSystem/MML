(-a) to_power k = (-a) to_power (2*l) * (-a) to_power 1 by A2,Th32
        .= (-a) to_power (2*l) * (-a) by Th30
        .= a to_power (2*l) * (-a) by Th54
        .= - a to_power (2*l) * a
        .= - a #Z (2*l) * a by Def2
        .= - a #Z (2*l) * a #Z 1 by PREPOWER:45
        .= - a #Z k by A2,A7,PREPOWER:54
        .= - a to_power k by Def2;
    end;
end;

