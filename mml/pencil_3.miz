          assume o in {bbx qua set};
          then A23: o = bbx by TARSKI:def 1;
          A24: dom bbx = I by PBOOLE:def 3 .= dom (b1+*(i,{x})) by PBOOLE:def 3
;
           now
            let z be set;
            assume z in dom (b1+*(i,{x}));
