          A33: u.z in (b1+*(i,{x})).z by A29,A31;
          per cases;
          suppose A34: zz=i;
            then u.z in {x} by A16,A33,FUNCT_7:33;
            then u.z = x by TARSKI:def 1;
            hence u.z = bbx.z by A15,A16,A34,FUNCT_7:33;
          end;
          suppose A35: zz<>i;
            then b1.zz is non empty trivial by PENCIL_1:def 21;
            then consider o being set such that
