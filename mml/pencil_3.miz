      then reconsider bbx1=bbx as Point of Segre_Product A by A11,A45,CARD_3:18
;
       dom f = the carrier of Segre_Product A by FUNCT_2:def 1;
      then A49: bbx1 in dom f;
A50:    {bbx qua set} = product(b1+*(i,{x}))
      proof
        thus {bbx qua set} c= product(b1+*(i,{x}))
        proof
          let o be set;
          assume o in {bbx qua set};
          then A51: o = bbx by TARSKI:def 1;
          A52: dom bbx = I by PBOOLE:def 3 .= dom (b1+*(i,{x})) by PBOOLE:def 3
;
           now
            let z be set;
            assume z in dom (b1+*(i,{x}));
            then A53: z in I by PBOOLE:def 3;
            then A54: z in dom bb by PBOOLE:def 3;
            per cases;
            suppose A55: z=i;
              then bbx.z = x by A54,FUNCT_7:33;
              then bbx.z in {x} by TARSKI:def 1;
              hence bbx.z in (b1+*(i,{x})).z by A44,A55,FUNCT_7:33;
            end;
            suppose A56: z<>i;
              then bbx.z = bb.z by FUNCT_7:34;
              then bbx.z in b1.z by A42,A44,A53,CARD_3:18;
              hence bbx.z in (b1+*(i,{x})).z by A56,FUNCT_7:34;
            end;
          end;
          hence o in product(b1+*(i,{x})) by A51,A52,CARD_3:18;
