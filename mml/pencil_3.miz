        then u=bbx by A23,FUNCT_1:9;
        hence thesis by A20,TARSKI:def 1;
      end;
      assume
A31:  x in B;
A32:  now
        let o be set;
        assume
A33:    o in dom Carrier A;
        then reconsider u=o as Element of I by PARTFUN1:def 4;
        per cases;
        suppose
A34:      u=i;
          then bbx.u in [#](A.i) by A31,A10,A11,FUNCT_7:33;
