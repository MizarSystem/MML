begin :: Some theorems on real and natural numbers.

theorem Th1:
  for r,s being real number holds 0<=r & s*s<r*r implies s<r
proof
  let r,s be real number;
  assume that
A1: 0<=r and
A2: s*s<r*r;
  assume
  s>=r;
  then r*s<=s*s & r*r<=s*r by A1,XREAL_1:66;
  hence contradiction by A2,XXREAL_0:2;
end;

theorem Th2:
  for r,s being real number holds 1<r & r*r <= s implies r<s
proof
  let r,s be real number;
  assume that
A1: 1<r and
A2: r*r <= s;
  r<r*r by A1,XREAL_1:157;
  hence thesis by A2,XXREAL_0:2;
end;

theorem Th3:
  for a,n being Nat st a>1 holds a|^n > n
proof
  defpred P[Nat] means for a being Nat st a>1 holds a|^$1 > $1;
A1: for n being Nat holds P[n] implies P[n+1]
  proof
    let n be Nat;
    assume
A2: P[n];
    now
      let a be Nat;
      assume
A3:   a>1;
      then a>=1+1 by NAT_1:13;
      then
A4:   a*n>=2*n by XREAL_1:66;
      per cases;
      suppose
        n=0;
        hence a|^(n+1)>n+1 by A3,NEWTON:10;
      end;
      suppose
        n>0;
        then n>=0+1 by NAT_1:13;
        then n+n>=n+1 by XREAL_1:8;
        then
A5:     a*n>=n+1 by A4,XXREAL_0:2;
        (a|^n)*a>n*a by A2,A3,XREAL_1:70;
        then a|^(n+1)>n*a by NEWTON:11;
        hence a|^(n+1)>n+1 by A5,XXREAL_0:2;
      end;
    end;
    hence thesis;
  end;
A6: P[0];
  for n being Nat holds P[n] from NAT_1:sch 2(A6,A1);
  hence thesis;
end;

theorem Th4:
  for n,k,m being Nat st k <= n & m = [\ n/2 /] holds n choose m >=
  n choose k
proof
  defpred P[Nat] means for k,m being Nat st k <= m & m = [\ $1/2 /] holds $1
  choose m >= $1 choose k;
A1: 1/2 - 1 < 0;
A2: for n being Nat st P[n] holds P[n+1]
  proof
    let n be Nat;
    assume
A3: P[n];
A4: n in NAT by ORDINAL1:def 13;
    for k,m1 being Nat st k <= m1 & m1 = [\ (n+1)/2 /] holds (n+1) choose
    m1 >= (n+1) choose k
    proof
      [\ n/2 /]<=n/2 by INT_1:def 4;
      then -[\ n/2 /] >= -n/2 by XREAL_1:26;
      then
A5:   -[\ n/2 /]+(n+1) >= -n/2+(n+1) by XREAL_1:8;
      set m = [\ n/2 /];
      let k,m1 be Nat;
      assume
A6:   k <= m1;
      set nk1 = n + 1 - k;
      set l = n - m;
      reconsider m as Element of NAT by INT_1:80;
      2*n >= 1*n by XREAL_1:66;
      then
A8:   n/2 >= [\ n/2 /] & 2*n/2 >= n/2 by INT_1:def 4,XREAL_1:74;
      then
A9:   n >= m by XXREAL_0:2;
      n - m = n -' m by A8,XREAL_1:235,XXREAL_0:2;
      then reconsider l as Element of NAT;
      ((n+1)!)/(n!) = (n+1)*(n!)/(n!) by NEWTON:21
        .= (n+1)*((n!)/(n!)) by XCMPLX_1:75
        .= (n+1)*1 by XCMPLX_1:60;
      then
A11:  (n!)/((n+1)!) = 1/(n+1) by XCMPLX_1:57;
A12:  -n/2+(n+1)=n/2+1;
      set nk = n-k;
      set l1 = (n+1) -' m1;
A13:  1*m1 <= 2*m1 by XREAL_1:66;
      assume
A14:  m1 = [\ (n+1)/2 /];
      then m1 <= (n+1)/2 by INT_1:def 4;
      then
A15:  m1*2 <= (n+1)/2*2 by XREAL_1:66;
      then
A16:  n+1 >= m1 & l1=(n+1) - m1 by A13,XREAL_1:235,XXREAL_0:2;
A17:  (n+1)/2 >= [\ (n+1)/2 /] by INT_1:def 4;
A18:  n >= k
      proof
        per cases;
        suppose
          n=0;
          hence thesis by A1,A6,A14,INT_1:def 4;
        end;
        suppose
          n<>0;
          then n+1>0+1 by XREAL_1:8;
          then n>=1 by NAT_1:13;
          then n+n>=1+n by XREAL_1:8;
          then 2*n/2>=(n+1)/2 by XREAL_1:74;
          then n>=[\ (n+1)/2 /] by A17,XXREAL_0:2;
          hence thesis by A6,A14,XXREAL_0:2;
        end;
      end;
      then n - k = n -' k by XREAL_1:235;
      then reconsider nk as Element of NAT;
      n/2 - 1 < [\ n/2 /] by INT_1:def 4;
      then
A20:  n/2 - 1 +1 < [\ n/2 /]+1 by XREAL_1:8;
      n + 1 - k = nk + 1;
      then reconsider nk1 as Element of NAT;
      0+n<=1+n by XREAL_1:8;
      then
A21:  k<=n+1 by A18,XXREAL_0:2;
      then
A22:  (n+1) choose k = ((n+1)!)/((nk1!)*(k!)) by NEWTON:def 3;
A23:  (nk1!)/(nk!) = (nk+1)*(nk!)/(nk!) by NEWTON:21
        .= (nk+1)*((nk!)/(nk!)) by XCMPLX_1:75
        .= (nk+1)*1 by XCMPLX_1:60;
      n choose k = (n!)/((nk!)*(k!)) by A18,NEWTON:def 3;
      then
      (n choose k)/((n+1) choose k) = ((n!)/((nk!)*(k!)))/(((n+1)!)/((nk1
      !)*(k!))) by A21,NEWTON:def 3
        .= ((n!)/((nk!)*(k!)))/((n+1)!)*((nk1!)*(k!)) by XCMPLX_1:83
        .= ((n!)/(((n+1)!)*((nk!)*(k!))))*((nk1!)*(k!)) by XCMPLX_1:79
        .= ((1/(n+1))/((nk!)*(k!)))*((nk1!)*(k!)) by A11,XCMPLX_1:79
        .= (1/(n+1))/(((nk!)*(k!))/((nk1!)*(k!))) by XCMPLX_1:83
        .= (1/(n+1))/(((nk!)/(nk1!))/((k!)/(k!))) by XCMPLX_1:85
        .= (1/(n+1))/(((nk!)/(nk1!))/1) by XCMPLX_1:60
        .= (1/(n+1))/(1/(nk+1)) by A23,XCMPLX_1:57
        .= (1*(nk+1))/(1*(n+1)) by XCMPLX_1:85
        .= (nk+1)/(n+1);
      then
A24:  (n choose k)/(((n+1) choose k)/((n+1) choose k)) = (nk+1)/(n+1)*((n
      +1) choose k) by XCMPLX_1:83;
A26:  n choose k = (n choose k)/1
        .= (nk+1)/(n+1)*((n+1) choose k) by A22,A24,XCMPLX_1:60;
      then
A27:  (n choose k)*((n+1)/(l+1)) = ((nk+1)/(n+1)*((n+1) choose k)*(n+1))/
      (l+1) by XCMPLX_1:75
        .= ((nk+1)/(n+1)*(n+1)*((n+1) choose k))/(l+1)
        .= (((n+1)/(n+1))*(nk+1)*((n+1) choose k))/(l+1) by XCMPLX_1:76
        .= (1*(nk+1)*((n+1) choose k))/(l+1) by XCMPLX_1:60
        .= ((n+1) choose k)*((nk+1)/(l+1)) by XCMPLX_1:75;
A28:  (n choose k)*((n+1)/(m+1)) = ((nk+1)/(n+1)*((n+1) choose k)*(n+1))/
      (m+1) by A26,XCMPLX_1:75
        .= ((nk+1)/(n+1)*(n+1)*((n+1) choose k))/(m+1)
        .= (((n+1)/(n+1))*(nk+1)*((n+1) choose k))/(m+1) by XCMPLX_1:76
        .= (1*(nk+1)*((n+1) choose k))/(m+1) by XCMPLX_1:60
        .= ((n+1) choose k)*((nk+1)/(m+1)) by XCMPLX_1:75;
      per cases;
      suppose
        n is even;
        then consider k1 be Element of NAT such that
A29:    n = 2*k1 by A4,ABIAN:def 2;
A30:    [\ (n+1)/2 /] = [\ 2*k1/2 + 1/2 /] by A29
          .= k1 + [\ 1/2 /] by INT_1:51
          .= k1 + 0 by A1,INT_1:def 4
          .= [\ n/2 /] by A29,INT_1:47;
        then
A31:    n choose m >= n choose k by A3,A6,A14;
        -k>=-[\ n/2 /] by A6,A14,A30,XREAL_1:26;
        then -k+(n+1)>=-[\ n/2 /]+(n+1) by XREAL_1:8;
        then
        (n-k+1)/(n-[\ n/2 /]+1)>= (n-[\ n/2 /]+1)/(n-[\ n/2 /]+1) by A5,
        A12,XREAL_1:74;
        then (n-k+1)/(n-[\ n/2 /]+1)>=1 by A5,A12,XCMPLX_1:60;
        then
A32:    ((n-k+1)/(n-[\ n/2 /]+1))*((n+1) choose k)>= 1*((n+1) choose k)
        by XREAL_1:66;
A33:    l1 = (n + 1 - m1) by A15,A13,XREAL_1:235,XXREAL_0:2
          .= l + 1 by A14,A30;
        (n+1) choose m1 = ((n+1)!)/((m1!)*(l1!)) by A16,NEWTON:def 3
          .= ((n+1)*(n!))/((m!)*((l + 1)!)) by A14,A30,A33,NEWTON:21
          .= ((n+1)*(n!))/((m!)*((l + 1)*(l!))) by NEWTON:21
          .= ((n+1)*(n!))/((l + 1)*((m!)*(l!)))
          .= ((n+1)/(l+1))/(((m!)*(l!))/(n!)) by XCMPLX_1:85
          .= ((n!)/((m!)*(l!)))*((n+1)/(l+1)) by XCMPLX_1:81
          .= (n choose m)*((n+1)/(l+1)) by A9,NEWTON:def 3;
        then
        (n+1) choose m >= ((n+1) choose k)*((nk+1)/(l+1)) by A14,A27,A30,
        A31,XREAL_1:66;
        hence thesis by A14,A30,A32,XXREAL_0:2;
      end;
      suppose
        n is not even;
        then consider j be Integer such that
A34:    n = 2*j+1 by ABIAN:1;
A35:    [\ (n+1)/2 /] = j + 0 + 1 by A34,INT_1:47
          .= j + [\ 1/2 /] + 1 by A1,INT_1:def 4
          .= [\ j + 1/2 /] + 1 by INT_1:51
          .= [\ n/2 /] + 1 by A34;
A36:    l1 = (n + 1 - m1) by A15,A13,XREAL_1:235,XXREAL_0:2
          .= l by A14,A35;
        per cases by A6,A14,A35,XXREAL_0:1;
        suppose
          k=m+1;
          hence thesis by A14,A35;
        end;
        suppose
          k<m+1;
          then
A37:      k<=m by NAT_1:13;
          then
A38:      n choose m >= n choose k by A3;
          n/2>=[\ n/2 /] by INT_1:def 4;
          then n/2*2>=[\ n/2 /]*2 by XREAL_1:66;
          then n - [\ n/2 /] >= [\ n/2 /]*2 - [\ n/2 /] by XREAL_1:11;
          then n - [\ n/2 /] >= k by A37,XXREAL_0:2;
          then n - [\ n/2 /] - (k - [\ n/2 /]) >= k - (k - [\ n/2 /]) by
          XREAL_1:11;
          then n-k+1>=[\ n/2 /]+1 by XREAL_1:8;
          then (n-k+1)/([\ n/2 /]+1)>= ([\ n/2 /]+1)/([\ n/2 /]+1) by A20,
          XREAL_1:74;
          then (n-k+1)/([\ n/2 /]+1)>=1 by A20,XCMPLX_1:60;
          then
A39:      ((n-k+1)/([\ n/2 /]+1))*((n+1) choose k)>= 1*((n+1) choose k)
          by XREAL_1:66;
          (n+1) choose m1 = ((n+1)!)/((m1!)*(l1!)) by A16,NEWTON:def 3
            .= ((n+1)*(n!))/(((m+1)!)*(l!)) by A14,A35,A36,NEWTON:21
            .= ((n+1)*(n!))/(((m!)*(m+1))*(l!)) by NEWTON:21
            .= ((n+1)*(n!))/((m + 1)*((m!)*(l!)))
            .= ((n+1)/(m+1))/(((m!)*(l!))/(n!)) by XCMPLX_1:85
            .= ((n!)/((m!)*(l!)))*((n+1)/(m+1)) by XCMPLX_1:81
            .= (n choose m)*((n+1)/(m+1)) by A9,NEWTON:def 3;
          then (n+1) choose m1 >= ((n+1) choose k)*((nk+1)/(m+1)) by A28,A38,
          XREAL_1:66;
          hence thesis by A39,XXREAL_0:2;
        end;
      end;
    end;
    hence thesis;
  end;
A40: P[0]
  proof
    let k,m be Nat;
    assume
A41: k <= m;
    assume
    m = [\ 0/2 /];
    then m = 0 by INT_1:47;
    hence thesis by A41;
  end;
A42: for n being Nat holds P[n] from NAT_1:sch 2(A40,A2);
  for n,k,m being Nat st k <= n & m = [\ n/2 /] holds n choose m >= n
  choose k
  proof
    let n,k,m be Nat;
A43: n in NAT by ORDINAL1:def 13;
    assume
A44: k <= n;
    assume
A45: m = [\ n/2 /];
    per cases;
    suppose
      k<=m;
      hence thesis by A42,A45;
    end;
    suppose
A46:  k>m;
      set r=n-'k;
A47:  n-'k=n-k by A44,XREAL_1:235;
      then
A48:  n choose k = n choose r by A44,NEWTON:30;
      per cases;
      suppose
A49:    n is even;
        -k < -m by A46,XREAL_1:26;
        then
A50:    -k+n < -m+n by XREAL_1:8;
        ex j be Element of NAT st n = 2*j by A43,A49,ABIAN:def 2;
        then n/2 = [\ n/2 /] by INT_1:47;
        hence thesis by A42,A45,A47,A48,A50;
      end;
      suppose
        n is not even;
        then consider j be Integer such that
A51:    n = 2*j+1 by ABIAN:1;
A52:    k>=m+1 by A46,NAT_1:13;
A53:    [\ n/2 /] = [\ j + 1/2 /] by A51
          .= j + [\ 1/2 /] by INT_1:51
          .= j + 0 by A1,INT_1:def 4
          .=j;
        per cases by A52,XXREAL_0:1;
        suppose
A54:      k=m+1;
          set nm1 = n-(m+1);
          set nm = n-m;
          2*n >= 1*n by XREAL_1:66;
          then
A55:      n/2 >= [\ n/2 /] & 2*n/2 >= n/2 by INT_1:def 4,XREAL_1:74;
          then
A56:      n>=m by A45,XXREAL_0:2;
          n - m = n -' m by A45,A55,XREAL_1:235,XXREAL_0:2;
          then reconsider nm as Element of NAT;
          2*m >= 1*m by XREAL_1:66;
          then
A57:      n>= m+1 by A45,A51,A53,XREAL_1:8;
          then n - (m+1) = n -' (m+1) by XREAL_1:235;
          then reconsider nm1 as Element of NAT;
A58:      n choose (m+1) = (n!)/((nm1!)*((m+1)!)) by A57,NEWTON:def 3;
          nm = m+1 by A45,A51,A53;
          hence thesis by A54,A56,A58,NEWTON:def 3;
        end;
        suppose
          k>m+1;
          then -k < -(m+1) by XREAL_1:26;
          then -k+n < -(m+1)+n by XREAL_1:8;
          hence thesis by A42,A45,A47,A48,A51,A53;
        end;
      end;
    end;
  end;
  hence thesis;
end;

theorem Th5:
  for n,m being Nat st m=[\ n/2 /] & n>=2 holds n choose m >= 2|^n
  / n
proof
  set f2a=<* 1 *>;
  let n,m be Nat;
  assume
A1: m=[\ n/2 /];
  set f1 = Newton_Coeff n;
  set f2b=(n -' 1) |-> (n choose m);
  set f2 = f2a ^ f2b ^ f2a;
A2: Sum f2b = (n-'1)*(n choose m) by RVSUM_1:110;
  assume
A3: n>=2;
  then
A4: n -' 1= n-1 by XREAL_1:235,XXREAL_0:2;
A5: len f2 = len (f2a ^ f2b) + len f2a by FINSEQ_1:35
    .= len (f2a ^ f2b) + 1 by FINSEQ_1:56
    .= len f2a + len f2b + 1 by FINSEQ_1:35
    .= len f2a + (n - 1) + 1 by A4,FINSEQ_1:def 18
    .= 1 + (n - 1) + 1 by FINSEQ_1:56
    .= n+1;
  len f1 = n+1 by NEWTON:def 5;
  then dom f1 = Seg (n+1) by FINSEQ_1:def 3;
  then
A6: dom f1 = dom f2 by A5,FINSEQ_1:def 3;
A7: for i being Element of NAT st i in dom f1 holds f1.i <= f2.i
  proof
    let i be Element of NAT;
    assume
A8: i in dom f1;
    per cases by A6,A8,FINSEQ_1:38;
    suppose
A9:   i in dom (f2a ^ f2b);
      then
A10:  f2.i=(f2a^f2b).i by FINSEQ_1:def 7;
      per cases by A9,FINSEQ_1:38;
      suppose
A11:    i in dom f2a;
        set k=1-1;
        k=0;
        then reconsider k as Element of NAT;
A12:    f2.i=f2a.i by A10,A11,FINSEQ_1:def 7;
        i in Seg 1 by A11,FINSEQ_1:55;
        then
A13:    i = 1 by FINSEQ_1:4,TARSKI:def 1;
        then f1.i = n choose k by A8,NEWTON:def 5
          .= 1 by NEWTON:29;
        hence thesis by A12,A13,FINSEQ_1:57;
      end;
      suppose
A14:    ex j be Nat st j in dom f2b & i=len f2a + j;
        set k = i-'1;
        consider j be Nat such that
A15:    j in dom f2b and
A16:    i=len f2a + j by A14;
A17:    j in Seg (n-'1) by A15,FUNCOP_1:19;
        j+1>=0+1 by XREAL_1:8;
        then i>=1 by A16,FINSEQ_1:56;
        then
A18:    k=i-1 by XREAL_1:235;
        f2.i = f2b.j by A10,A15,A16,FINSEQ_1:def 7;
        then
A19:    f2.i = n choose m by A17,FUNCOP_1:13;
        i=1+j by A16,FINSEQ_1:56;
        then -1+n<0+n & i-1<=n-1 by A4,A17,FINSEQ_1:3,XREAL_1:8;
        then
A20:    k <= n by A18,XXREAL_0:2;
        f1.i = n choose k by A8,A18,NEWTON:def 5;
        hence thesis by A1,A19,A20,Th4;
      end;
    end;
    suppose
A21:  ex j be Nat st j in dom f2a & i=len (f2a ^ f2b) + j;
      reconsider k=(n+1)-1 as Element of NAT by ORDINAL1:def 13;
      consider j be Nat such that
A22:  j in dom f2a and
A23:  i=len(f2a^f2b)+j by A21;
A24:  j in Seg 1 by A22,FINSEQ_1:55;
      then
A25:  j = 1 by FINSEQ_1:4,TARSKI:def 1;
      i = len(f2a^f2b)+1 by A23,A24,FINSEQ_1:4,TARSKI:def 1
        .= len(f2a)+len(f2b)+1 by FINSEQ_1:35
        .= len f2a + (n - 1) + 1 by A4,FINSEQ_1:def 18
        .= 1 + (n - 1) + 1 by FINSEQ_1:56
        .= n+1;
      then
A26:  f1.i = n choose k by A8,NEWTON:def 5
        .= 1 by NEWTON:31;
      f2.i = f2a.j by A22,A23,FINSEQ_1:def 7;
      hence thesis by A25,A26,FINSEQ_1:57;
    end;
  end;
  1<=n by A3,XXREAL_0:2;
  then
A27: n choose 1 <= n choose m by A1,Th4;
  2 <= n choose 1 by A3,NEWTON:33,XXREAL_0:2;
  then 2 <= (n choose m) by A27,XXREAL_0:2;
  then
A28: 2+(n-1)*(n choose m)<=(n choose m)+(n-1)*(n choose m) by
  XREAL_1:8;
A29: Sum f2 = Sum(f2a^f2b)+1 by RVSUM_1:104
    .= (1 + Sum(f2b)) +1 by RVSUM_1:106
    .= 2 + (n-'1)*(n choose m) by A2
    .= 2 + (n-1)*(n choose m) by A3,XREAL_1:235,XXREAL_0:2;
  len f1 = len f2 by A5,NEWTON:def 5;
  then Sum f1 <= Sum f2 by A7,INTEGRA5:3;
  then 2|^n <= 2 + (n-1)*(n choose m) by A29,NEWTON:44;
  then 2|^n <= n*(n choose m) by A28,XXREAL_0:2;
  then 2|^n / n <= n*(n choose m)/n by XREAL_1:74;
  hence thesis by A3,XCMPLX_1:90;
end;

theorem Th6:
  for n being Nat holds 2*n choose n >= 4|^n / (2*n)
proof
  let n be Nat;
  per cases;
  suppose
    n = 0;
    hence thesis;
  end;
  suppose
A1: n <> 0;
    set m=[\ 2*n/2 /];
A2: 2|^2 = Product <* 2,2 *> by FINSEQ_2:75
      .= 2*2 by RVSUM_1:129
      .= 4;
A3: m = n by INT_1:47;
    then reconsider m as Element of NAT by ORDINAL1:def 13;
    2*n >= 2*1 by A1,NAT_1:14,XREAL_1:66;
    then (2*n) choose m >= 2|^(2*n) / (2*n) by Th5;
    hence thesis by A2,A3,NEWTON:14;
  end;
end;

theorem Th7:
  for n,p being Nat holds p>0 & n divides p & n <> 1 & n <> p
  implies 1 < n & n < p
proof
  let n,p be Nat;
  assume
A1: p>0 & n divides p;
  assume
A2: n <> 1;
  assume
A3: n <> p;
  n <> 0 by A1,INT_2:3;
  hence 1 < n by A2,NAT_1:26;
  n <= p by A1,NAT_D:7;
  hence thesis by A3,XXREAL_0:1;
end;

theorem Th8:
  for p being Nat holds (ex n being Element of NAT st n divides p &
  1<n & n<p) implies ex n being Element of NAT st n divides p & 1<n & n*n <= p
proof
  let p be Nat;
  given n be Element of NAT such that
A1: n divides p and
A2: 1<n and
A3: n<p;
  per cases;
  suppose
A4: n*n<=p;
    take n;
    thus thesis by A1,A2,A4;
  end;
  suppose
A5: n*n>p;
    consider k be Nat such that
A6: p=n*k by A1,NAT_D:def 3;
A7: k*k<=p
    proof
      assume
      k*k>p;
      then (k*k)*(n*n)>p*p by A5,XREAL_1:98;
      hence contradiction by A6;
    end;
    take k;
    n/n<n*k/n by A2,A3,A6,XREAL_1:76;
    then 1<k*n/n by A2,XCMPLX_1:60;
    hence thesis by A2,A6,A7,NAT_D:def 3,ORDINAL1:def 13,XCMPLX_1:90;
  end;
end;

theorem Th9:
  for i,j,k,l being Nat st i=j*k+l & l<j & 0<l holds not j divides
  i
proof
  let i,j,k,l be Nat;
  assume
A1: i=j*k+l;
  assume that
A2: l<j and
A3: 0<l;
  assume
  j divides i;
  then i = j * (i div j) by NAT_D:3;
  then j * (i div j) = j * (i div j) + l by A1,A2,NAT_D:def 1;
  hence contradiction by A3;
end;

theorem
  for n,q,b being Nat holds q gcd b = 1 & q<>0 & b<>0 implies q|^n gcd b
  = 1
proof
  let n,q,b be Nat;
  assume that
A1: q gcd b = 1 and
A2: q<>0 & b<>0;
  q,b are_relative_prime by A1,INT_2:def 4;
  then (q|^n),b are_relative_prime by A2,EULER_2:32;
  hence thesis by INT_2:def 4;
end;

theorem Th11:
  for a,b,c being Nat holds a|^(2*b) mod c = (a|^b mod c)*(a|^b
  mod c) mod c
proof
  let a,b,c be Nat;
  reconsider a,b,c as Element of NAT by ORDINAL1:def 13;
  a|^(2*b) mod c = a|^(b+b) mod c
    .=(a|^b*a|^b) mod c by NEWTON:13
    .=(a|^b mod c) * (a|^b mod c) mod c by EULER_2:11;
  hence thesis;
end;

theorem Th12:
  for p being Nat holds not p is prime iff p<=1 or ex n being
  Element of NAT st n divides p & 1<n & n<p
proof
  now
    let p be Nat;
    assume that
A1: not p is prime and
A2: not p<=1;
    consider n being Nat such that
A3: n divides p and
A4: n<>1 & n<>p by A1,A2,INT_2:def 5;
    reconsider n as Element of NAT by ORDINAL1:def 13;
    take n;
    thus n divides p by A3;
    thus 1<n & n<p by A2,A3,A4,Th7;
  end;
  hence thesis by INT_2:def 5;
end;

theorem Th13:
  for n,k being Nat st n divides k & 1<n holds ex p being Element
  of NAT st p divides k & p<=n & p is prime
proof
  let n,k be Nat;
  assume
A1: n divides k;
  assume
A2: 1<n;
  then 1+1<n+1 by XREAL_1:8;
  then 2<=n by NAT_1:13;
  then consider p be Element of NAT such that
A3: p is prime and
A4: p divides n by INT_2:48;
  take p;
  thus p divides k by A1,A4,NAT_D:4;
  thus p<=n by A2,A4,NAT_D:7;
  thus thesis by A3;
end;

Lm1: for p being Nat holds not p is prime iff p<=1 or ex n being Element of
NAT st n divides p & 1<n & n*n<=p & n is prime

proof
  let p be Nat;
A1: now
    assume
A2: not p is prime;
    assume
    not p<=1;
    then ex k be Element of NAT st k divides p & 1<k & k<p by A2,Th12;
    then consider k be Element of NAT such that
A3: k divides p & 1<k and
A4: k*k<=p by Th8;
    consider n be Element of NAT such that
A5: n divides p and
A6: n<=k and
A7: n is prime by A3,Th13;
    take n;
    thus n divides p by A5;
    thus 1<n by A7,Th12;
    n*n<=k*k by A6,XREAL_1:68;
    hence n*n<=p by A4,XXREAL_0:2;
    thus n is prime by A7;
  end;
  now
    assume
A8: p<=1 or ex n being Element of NAT st n divides p & 1<n & n*n<=p;
    per cases by A8;
    suppose
      p<=1;
      hence not p is prime by Th12;
    end;
    suppose
      ex n being Element of NAT st n divides p & 1<n & n*n<=p;
      then consider n being Element of NAT such that
A9:   n divides p and
A10:  1<n and
A11:  n*n<=p;
      n<p by A10,A11,Th2;
      hence not p is prime by A9,A10,Th12;
    end;
  end;
  hence thesis by A1;
end;

theorem Th14:
  for p being Nat holds p is prime iff p>1 & for n being Element
  of NAT holds 1<n & n*n<=p & n is prime implies not n divides p
proof
  now
    let p be Nat;
    assume
A1: p>1;
    assume
    for n being Element of NAT holds not (1<n & n*n<=p & n is prime)
    or not n divides p;
    then
    for n being Element of NAT holds not n divides p or not (1<n & n*n<=p
    & n is prime);
    hence p is prime by A1,Lm1;
  end;
  hence thesis by Lm1;
end;

theorem Th15:
  for a,p,k being Nat holds a|^k mod p = 1 & k>=1 & p is prime
  implies a,p are_relative_prime
proof
  let a,p,k be Nat;
  assume that
A1: a|^k mod p = 1 and
A2: k>=1;
  assume
A3: p is prime;
  assume
A4: not a,p are_relative_prime;
  reconsider a,p,k as Element of NAT by ORDINAL1:def 13;
  a gcd p = p by A3,A4,PEPIN:2;
  then p divides a by NAT_D:def 5;
  then consider i being Nat such that
A5: a=p*i by NAT_D:def 3;
  ((p*i mod p)|^k) mod p = 1 by A1,A5,PEPIN:12;
  then 0 qua Nat|^k mod p = 1 by NAT_D:13;
  then p*0 mod p = 1 by A2,NEWTON:16;
  hence contradiction by NAT_D:13;
end;

theorem Th16:
  for p being Prime, a being Element of NAT, x being set st a<>0 &
x=p |^ ( p |-count a) holds ex b being Element of NAT st b=x & 1 <= b & b <= a
proof
  let p be Prime, a be Element of NAT, x be set;
  assume
A1: a<>0;
  assume
A2: x=p |^ (p |-count a);
  then reconsider b = x as Element of NAT by ORDINAL1:def 13;
  take b;
  thus b=x;
  p >= 1 by INT_2:def 5;
  hence 1 <= b by A2,PREPOWER:19;
  p <> 1 by INT_2:def 5;
  then b divides a by A1,A2,NAT_3:def 7;
  hence thesis by A1,NAT_D:7;
end;

theorem Th17:
  for k,q,n,d being Element of NAT holds q is prime & d divides k*
  q|^(n+1) & not d divides k*q|^n implies q|^(n+1) divides d
proof
  defpred P[natural number] means for k,q,d being Element of NAT holds q is
prime & d divides k*q|^($1+1) & not d divides k*q|^$1 implies q|^($1+1) divides
  d;
A1: for n being Element of NAT holds P[n] implies P[n+1]
  proof
    let n be Element of NAT;
    assume
A2: for k,q,d being Element of NAT holds q is prime & d divides k*q|^
    (n+1) & not d divides k*q|^n implies q|^(n+1) divides d;
    for k,q,d being Element of NAT holds q is prime & d divides k*q|^(n+1
    +1) & not d divides k*q|^(n+1) implies q|^(n+1+1) divides d
    proof
      let k,q,d be Element of NAT;
      assume
A3:   q is prime;
      assume
A4:   d divides k*q|^(n+1+1);
      then consider i be Nat such that
A5:   k*q|^(n+1+1)=d*i by NAT_D:def 3;
      assume
A6:   not d divides k*q|^(n+1);
      then not d divides k*(q*q|^n) by NEWTON:11;
      then
A7:   not d divides (k*q)*q|^n;
      d divides k*(q*q|^(n+1)) by A4,NEWTON:11;
      then d divides (k*q)*q|^(n+1);
      then q|^(n+1) divides d by A2,A3,A7;
      then consider j be Nat such that
A8:   d=q|^(n+1)*j by NAT_D:def 3;
A9:   not q divides i
      proof
        assume
        q divides i;
        then consider i1 be Nat such that
A10:    i=q*i1 by NAT_D:def 3;
        k*(q|^(n+1)*q)=d*(q*i1) by A5,A10,NEWTON:11;
        then k*q|^(n+1)*q=d*i1*q;
        then k*q|^(n+1)=d*i1 by A3,XCMPLX_1:5;
        hence contradiction by A6,NAT_D:def 3;
      end;
      k*q|^(n+1+1)=k*(q|^(n+1)*q) by NEWTON:11
        .=k*q*q|^(n+1);
      then k*q*q|^(n+1) = j*i*q|^(n+1) by A5,A8;
      then k*q=j*i by A3,XCMPLX_1:5;
      then q divides j*i by NAT_D:def 3;
      then q divides j by A3,A9,NEWTON:98;
      then consider j1 be Nat such that
A11:  j=q*j1 by NAT_D:def 3;
      d=(q|^(n+1)*q)*j1 by A8,A11;
      then d=q|^(n+1+1)*j1 by NEWTON:11;
      hence thesis by NAT_D:def 3;
    end;
    hence thesis;
  end;
A12: P[0]
  proof
    let k,q,d be Element of NAT;
    assume that
A13: q is prime and
A14: d divides k*q|^(0+1) and
A15: not d divides k*q|^0;
    d divides k*q by A14,NEWTON:10;
    then consider i be Nat such that
A16: k*q=d*i by NAT_D:def 3;
    assume
    not q|^(0+1) divides d;
    then
A17: not q divides d by NEWTON:10;
    q divides d*i by A16,NAT_D:def 3;
    then q divides i by A13,A17,NEWTON:98;
    then consider j be Nat such that
A18: i=q*j by NAT_D:def 3;
    d*j*q=k*q by A16,A18;
    then
A19: d*j=k by A13,XCMPLX_1:5;
    not d divides k*1 by A15,NEWTON:9;
    hence contradiction by A19,NAT_D:def 3;
  end;
  for n being Element of NAT holds P[n] from NAT_1:sch 1(A12,A1);
  hence thesis;
end;

theorem Th18:
  for q1,q,n1 being Element of NAT st q1 divides q|^n1 & q is
  prime & q1 is prime holds q=q1
proof
  let q1,q,n1 be Element of NAT;
  assume that
A1: q1 divides q|^n1 and
A2: q is prime;
  assume
A3: q1 is prime;
  then q1>1 by INT_2:def 5;
  then consider k be Element of NAT such that
A4: q1=q*k by A1,A2,PEPIN:33;
A5: k<>q1
  proof
    assume
    k=q1;
    then q=1 by A3,A4,XCMPLX_1:7;
    hence contradiction by A2,INT_2:def 5;
  end;
  k divides q1 by A4,NAT_D:def 3;
  then k=1 or k=q1 by A3,INT_2:def 5;
  hence thesis by A4,A5;
end;

theorem Th19:
  for p being Prime, n being Element of NAT st n<p holds not p
  divides n!
proof
  defpred P[natural number] means for p being Prime st $1<p holds not p
  divides ($1!);
  let p be Prime;
  let n be Element of NAT;
  assume
A1: n<p;
A2: for n being Element of NAT st P[n] holds P[n+1]
  proof
    let n be Element of NAT;
    assume
A3: P[n];
    for p being Prime st n+1<p holds not p divides ((n+1)!)
    proof
      let p be Prime;
      assume
A4:   n+1<p;
      assume
      p divides (n+1)!;
      then
A5:   p divides (n!)*(n+1) by NEWTON:21;
      n+0<n+1 by XREAL_1:8;
      then n<p by A4,XXREAL_0:2;
      then not p divides n! by A3;
      then p divides (n+1) by A5,NEWTON:98;
      hence contradiction by A4,NAT_D:7;
    end;
    hence thesis;
  end;
A6: P[0]
  proof
    let p be Prime;
    assume
    0<p;
    assume
    p divides 0!;
    then p<=1 by NAT_D:7,NEWTON:18;
    hence contradiction by INT_2:def 5;
  end;
  for n being Element of NAT holds P[n] from NAT_1:sch 1(A6,A2);
  hence thesis by A1;
end;

theorem Th20:
  for a,b being non empty Nat holds (for p being Element of NAT st
p is prime holds p |-count a <= p |-count b) implies ex c being Element of NAT
  st b=a*c
proof
A1: for a,a1,b,b1 being non empty Nat, x being Prime st (for p being
Element of NAT st p is prime holds p |-count a <= p |-count b) & a1=a div (x|^(
x|-count a)) & b1=b div (x|^(x|-count b)) holds for p being Element of NAT st p
  is prime holds p |-count a1 <= p |-count b1
  proof
    let a,a1,b,b1 be non empty Nat;
    let x be Prime;
    assume
A2: for p being Element of NAT st p is prime holds p |-count a <= p
    |-count b;
    set xcb=x|-count b;
    set xca=x|-count a;
    assume
A3: a1=a div (x|^(x|-count a));
    assume
A4: b1=b div (x|^(x|-count b));
    let p be Element of NAT;
    assume
A5: p is prime;
    then
A6: p>1 by INT_2:def 5;
A7: x>1 by INT_2:def 5;
    then x|^xca divides a by NAT_3:def 7;
    then
A8: p|-count a1 =(p|-count a)-'(p|-count x|^xca) by A3,A5,
    NAT_3:31;
    x|^xcb divides b by A7,NAT_3:def 7;
    then
A9: p|-count b1 = (p|-count b)-'(p|-count x|^xcb) by A4,A5,
    NAT_3:31;
    per cases;
    suppose
      x=p;
      then p|-count x|^xca = p|-count a by A6,NAT_3:25;
      hence thesis by A8,NAT_2:10;
    end;
    suppose
A10:  x<>p;
      p|-count x|^xcb=xcb * (p|-count x) by A5,NAT_3:32
        .= xcb * 0 by A6,A10,NAT_3:24
        .= 0;
      then
A11:  p|-count b1=(p|-count b)-0 by A9,XREAL_1:235
        .=p|-count b;
      p|-count x|^xca=xca * (p|-count x) by A5,NAT_3:32
        .= xca * 0 by A6,A10,NAT_3:24
        .= 0;
      then p|-count a1=(p|-count a)-0 by A8,XREAL_1:235
        .=p|-count a;
      hence thesis by A2,A5,A11;
    end;
  end;
A12: for a being non empty Nat, x being Prime holds a div (x|^(x|-count a))
  is non empty Nat
  proof
    let a be non empty Nat;
    let x be Prime;
    assume
A13: not a div (x|^(x|-count a)) is non empty Nat;
      set xca=x|^(x|-count a);
      x>1 by INT_2:def 5;
      then
A14:  xca divides a by NAT_3:def 7;
      ex t being Nat st a = xca * (0 qua Nat) + t & t < xca by A13,NAT_D:
      def 1;
      hence contradiction by A14,NAT_D:7;
  end;
  defpred P[natural number] means for a,b being non empty Nat st card(support
  pfexp b)=$1 holds (for p being Element of NAT st p is prime holds p |-count a
  <= p |-count b) implies (ex c being Element of NAT st b=a*c);
  let a,b be non empty Nat;
  assume
A15: for p being Element of NAT st p is prime holds p |-count a <= p
  |-count b;
A16: ex n1 being Element of NAT st n1=card(support pfexp b);
A17: for n1,n2 being non empty Nat st n1 divides n2 holds (n2 div n1)*n1 =
  n2
  proof
    let n1,n2 be non empty Nat;
    assume
    n1 divides n2;
    then ex a being Nat st n2=n1*a by NAT_D:def 3;
    hence thesis by NAT_D:18;
  end;
A18: for b,b1 being non empty Nat, x being Prime, n being Element of NAT st
  card(support pfexp b)=n+1 & b1=b div (x|^(x|-count b)) & x in support pfexp b
  holds card(support pfexp b1)=n
  proof
    let b,b1 be non empty Nat;
    let x be Prime;
    let n be Element of NAT;
    assume
A19: card(support pfexp b)=n+1;
    set xcb=x|-count b;
    assume
A20: b1=b div (x|^(x|-count b));
A21: x>1 by INT_2:def 5;
    then x|^xcb divides b by NAT_3:def 7;
    then
A22: b=b1*x|^xcb by A17,A20;
    reconsider b1 as Element of NAT by ORDINAL1:def 13;
A23: b1,x are_relative_prime
    proof
      assume
      not b1,x are_relative_prime;
      then b1 gcd x = x by PEPIN:2;
      then x divides b1 by NAT_D:def 5;
      then consider c be Nat such that
A24:  b1=x*c by NAT_D:def 3;
A25:  not x|^(xcb+1) divides b by A21,NAT_3:def 7;
      x|^xcb divides b by A21,NAT_3:def 7;
      then b = x*c*x|^xcb by A17,A20,A24
        .= c*(x|^xcb*x)
        .= c*x|^(xcb+1) by NEWTON:11;
      hence contradiction by A25,NAT_D:def 3;
    end;
    assume
    x in support pfexp b;
    then (pfexp b).x<>0 by PRE_POLY:def 7;
    then
A26: xcb<>0 by NAT_3:def 8;
    reconsider b1 as non empty Nat;
    card support pfexp (x|^xcb*b1) = card support pfexp x|^xcb + card
    support pfexp b1 by A23,EULER_2:32 ,NAT_3:47;
    then n+1=card {x} + card support pfexp b1 by A19,A22,A26,NAT_3:42;
    then n+1=1+card (support pfexp b1) by CARD_2:60;
    hence thesis;
  end;
A27: for n being Element of NAT holds P[n] implies P[n+1]
  proof
    let n be Element of NAT;
    assume
A28: P[n];
    for a,b being non empty Nat st card(support pfexp b)=n+1 holds (for p
being Element of NAT st p is prime holds p |-count a <= p |-count b) implies ex
    c being Element of NAT st b=a*c
    proof
      let a,b be non empty Nat;
      assume
A29:  card(support pfexp b)=n+1;
      then support pfexp b<>{};
      then consider x being set such that
A30:  x in support pfexp b by XBOOLE_0:def 1;
      reconsider x as Prime by A30,NAT_3:34;
      set a1=a div (x|^(x|-count a));
      set b1=b div (x|^(x|-count b));
      reconsider a1,b1 as non empty Nat by A12;
      assume
A31:  for p being Element of NAT st p is prime holds p |-count a <= p
      |-count b;
      then
A32:  for p being Element of NAT st p is prime holds p |-count a1 <= p
      |-count b1 by A1;
      set xca=x|-count a;
      set xcb=x|-count b;
      x in NAT by ORDINAL1:def 13;
      then
A33:  xcb-xca=xcb-'xca by A31,XREAL_1:235;
      card(support pfexp b1)=n by A18,A29,A30;
      then consider d be Element of NAT such that
A34:  b1=a1*d by A28,A32;
      reconsider e=d*x|^(xcb-'xca) as Element of NAT;
      take e;
A35:  x<>1 by INT_2:def 5;
      then
A36:  x|^(xca) divides a by NAT_3:def 7;
      x|^(xcb) divides b by A35,NAT_3:def 7;
      then b = d*(a div (x|^xca))*(x|^(xca+(xcb-'xca))) by A17,A34,A33
        .= d*(a div (x|^xca))*(x|^xca*x|^(xcb-'xca)) by NEWTON:13
        .= d*((a div (x|^xca))*x|^xca)*x|^(xcb-'xca)
        .= d*a*x|^(xcb-'xca) by A17,A36
        .= a*(d*x|^(xcb-'xca));
      hence thesis;
    end;
    hence thesis;
  end;
A37: P[0]
  proof
    let a,b be non empty Nat;
    assume
A38: card support pfexp b=0;
    (for p being Element of NAT st p is prime holds p |-count a <= p
    |-count b) implies ex c being Element of NAT st b=a*c
    proof
      set d=1;
      assume
A39:  for p being Element of NAT st p is prime holds p |-count a <= p
      |-count b;
      take d;
A40:  (support pfexp b)={} by A38;
      (support pfexp a)={}
      proof
        assume
        (support pfexp a)<>{};
        then consider x be set such that
A41:    x in support pfexp a by XBOOLE_0:def 1;
        reconsider x as Prime by A41,NAT_3:34;
        reconsider x as prime Element of NAT by ORDINAL1:def 13;
        (pfexp a).x<>0 by A41,PRE_POLY:def 7;
        then x|-count a<>0 by NAT_3:def 8;
        then x|-count b>0 by A39;
        then (pfexp b).x<>0 by NAT_3:def 8;
        hence contradiction by A40,PRE_POLY:def 7;
      end;
      then a=1 by NAT_3:52;
      hence thesis by A40,NAT_3:52;
    end;
    hence thesis;
  end;
  for n being Element of NAT holds P[n] from NAT_1:sch 1(A37,A27);
  hence thesis by A15,A16;
end;

theorem Th21:
  for a,b being non empty Nat holds (for p being Element of NAT st
  p is prime holds p |-count a = p |-count b) implies a=b
proof
  let a,b be non empty Nat;
  assume
A1: for p being Element of NAT st p is prime holds p |-count a = p
  |-count b;
  then
  for p being Element of NAT st p is prime holds p |-count a <= p |-count
  b;
  then consider c being Element of NAT such that
A2: b=a*c by Th20;
  for p being Element of NAT st p is prime holds p |-count b <= p |-count
  a by A1;
  then consider d being Element of NAT such that
A3: a=b*d by Th20;
  a=a*(c*d) by A2,A3;
  then c = 1 by NAT_1:15,XCMPLX_1:7;
  hence thesis by A2;
end;

theorem Th22:
  for p1,p2 being Prime, m being non empty Element of NAT st p1|^(
  p1 |-count m) = p2|^(p2 |-count m) & p1 |-count m > 0 holds p1=p2
proof
  let p1,p2 be Prime;
  let m be non empty Element of NAT;
  assume
A1: p1|^(p1 |-count m) = p2|^(p2 |-count m);
A2: p1 > 1 by INT_2:def 5;
  assume
  p1 |-count m > 0;
  then p1 to_power (p1 |-count m) > 1 by A2,POWER:40;
  then
A3: p1|^(p1|-count m) > 1 by POWER:46;
  assume
  p1 <> p2;
  then p1 <> 1 & not p1 divides p2|^(p2 |-count m) by INT_2:def 5,NAT_3:6;
  then p1 |-count (p1|^(p1 |-count m)) = 0 by A1,NAT_3:27;
  then p1 |-count m = 0 by A2,NAT_3:25;
  hence contradiction by A3,NEWTON:9;
end;

begin :: Pocklington's theorem.

theorem Th23:
  for n,k,q,p,n1,p,a being Element of NAT st n-1=k*q|^n1 & k>0 &
  n1>0 & q is prime & a|^(n-'1) mod n = 1 & p is prime & p divides n holds p
  divides (a|^((n-'1) div q) -' 1) or p mod q|^n1 = 1
proof
  let n,k,q,p,n1,p,a be Element of NAT;
  assume that
A1: n-1=k*q|^n1 and
A2: k>0 and
A3: n1>0 and
A4: q is prime;
A5: n-1+1 > 0+1 by A1,A2,A4,XREAL_1:8;
  assume
A6: a|^(n-'1) mod n = 1;
  a|^((n-'1) div q)>=1
  proof
    assume
    a|^((n-'1) div q)<1;
    then
A7: a=0 by NAT_1:14;
    n-'1 + 1 > 1 by A5,XREAL_1:235;
    then n-'1 >= 1 by NAT_1:13;
    then a|^(n-'1) mod n = 0*n mod n by A7,NEWTON:16
      .= 0 by NAT_D:13;
    hence contradiction by A6;
  end;
  then
A8: a|^((n-'1) div q)-'1=a|^((n-'1) div q)-1 by XREAL_1:235;
  n1+1 > 0+1 by A3,XREAL_1:8;
  then n1>=1 by NAT_1:13;
  then
A9: n1-'1=n1-1 by XREAL_1:235;
  then
A10: (n-'1) div q = (q|^(n1-'1+1))*k div q by A1,A5,XREAL_1:235
    .=q*q|^(n1-'1)*k div q by NEWTON:11
    .=q*(q|^(n1-'1)*k) div q
    .=k*q|^(n1-'1) by A4,NAT_D:18;
  assume that
A11: p is prime and
A12: p divides n;
  consider i being Nat such that
A13: n=p*i by A12,NAT_D:def 3;
  assume
  not p divides (a|^((n-'1) div q)-'1);
  then
A14: not a|^((n-'1) div q) mod p = 1 by A11,A8,PEPIN:8;
  set nn=n-'1;
  n1+1 > 0+1 by A3,XREAL_1:8;
  then n1>=1 by NAT_1:13;
  then
A15: n1-'1=n1-1 by XREAL_1:235;
A16: p > 1 by A11,INT_2:def 5;
  then p-'1=p-1 by XREAL_1:235;
  then
A17: p-'1+1=p;
  reconsider i as Element of NAT by ORDINAL1:def 13;
  i*p<>0 by A1,A13;
  then
A18: a|^(n-'1) mod p = 1 by A6,A16,A13,PEPIN:9;
  0+1<nn+1 by A5,XREAL_1:235;
  then
A19: 1<=nn by NAT_1:13;
  then
A20: a,p are_relative_prime by A11,A18,Th15;
A21: order(a,p) divides p-'1 by A11,A18,A19,Th15,PEPIN:54;
  n-'1=n-1 by A5,XREAL_1:235;
  then order(a,p) divides k*q|^(n1-'1+1) by A1,A16,A15,A18,A20,PEPIN:52;
  then q|^n1 divides order(a,p) by A4,A16,A20,A14,A9,A10,Th17,PEPIN:53;
  then q|^n1 divides p-'1 by A21,NAT_D:4;
  then
A22: p-'1 mod q|^n1 = 0 by A4,PEPIN:6;
  q > 1 by A4,INT_2:def 5;
  hence thesis by A3,A17,A22,NAT_D:16,PEPIN:26;
end;

theorem Th24:
  for n,f,c being Element of NAT st n-1=f*c & f>c & c>0 & (for q
  being Element of NAT st q divides f & q is prime holds ex a being Element of
NAT st a|^(n-'1) mod n = 1 & (a|^((n-'1) div q)-'1) gcd n = 1) holds n is prime
proof
  let n,f,c be Element of NAT;
  assume that
A1: n-1=f*c and
A2: f>c and
A3: c>0;
A4: n-1+1>0+1 by A1,A2,A3,XREAL_1:8;
  assume
A5: for q being Element of NAT st q divides f & q is prime holds ex a
being Element of NAT st a|^(n-'1) mod n = 1 & (a|^((n-'1) div q)-'1) gcd n = 1;
  assume
  not n is prime;
  then consider p be Element of NAT such that
A6: p divides n and
A7: 1<p and
A8: p*p<=n and
A9: p is prime by A4,Lm1;
A10: p-1=p-'1 by A7,XREAL_1:235;
A11: p-1>1-1 by A7,XREAL_1:11;
  then
A12: p-'1>0 by A7,XREAL_1:235;
A13: for q1 being Element of NAT st q1 in support (pfexp f) & q1 |-count f >
  0 holds p mod q1|^(q1 |-count f) = 1
  proof
    let q1 be Element of NAT;
    set n1=q1 |-count f;
    assume
A14: q1 in support (pfexp f);
    then
A15: q1 is prime by NAT_3:34;
A16: q1 is prime by A14,NAT_3:34;
    then consider a be Element of NAT such that
A17: a|^(n-'1) mod n = 1 and
A18: (a|^((n-'1) div q1)-'1) gcd n = 1 by A5,A14,NAT_3:36;
    assume
A19: q1 |-count f > 0;
A20: not p divides (a|^((n-'1) div q1)-'1)
    proof
      assume
      p divides (a|^((n-'1) div q1)-'1);
      then p divides (n gcd (a|^((n-'1) div q1)-'1)) by A6,NEWTON:63;
      hence contradiction by A7,A18,NAT_D:7;
    end;
    q1<>1 by A16,INT_2:def 5;
    then q1|^(q1 |-count f) divides f by A2,NAT_3:def 7;
    then consider d be Nat such that
A21: f=q1|^n1*d by NAT_D:def 3;
    reconsider d as Element of NAT by ORDINAL1:def 13;
    set k=d*c;
A22: k>0
    proof
      assume
      k<=0;
      then d=0 by A3;
      hence contradiction by A2,A21;
    end;
    n-1=k*q1|^n1 by A1,A21;
    hence thesis by A6,A9,A19,A17,A22,A15,A20,Th23;
  end;
  for q1 being Element of NAT st q1 is prime holds q1 |-count f <= q1
  |-count (p-'1)
  proof
    let q1 be Element of NAT;
    assume
A23: q1 is prime;
    per cases;
    suppose
A24:  q1 in support (pfexp f);
      then (pfexp f).q1<>0 by PRE_POLY:def 7;
      then q1 |-count f>0 by A23,NAT_3:def 8;
      then p mod q1|^(q1 |-count f) = 1 by A13,A24;
      then
A25:  q1 |-count (q1|^(q1 |-count f)) <= q1 |-count (p-'1) by A11,A10,A23
      ,NAT_3:30,PEPIN:8;
      q1>1 by A23,INT_2:def 5;
      hence thesis by A25,NAT_3:25;
    end;
    suppose
      not q1 in support (pfexp f);
      then (pfexp f).q1=0 by PRE_POLY:def 7;
      hence thesis by A23,NAT_3:def 8;
    end;
  end;
  then ex n2 being Element of NAT st (p-'1)=f*n2 by A2,A12,Th20;
  then f divides (p-'1) by NAT_D:def 3;
  then f<=(p-'1) by A12,NAT_D:7;
  then p-1+1>=f+1 by A10,XREAL_1:8;
  then p>f by NAT_1:13;
  then
A26: p^2>f^2 by SQUARE_1:78;
  c>=0+1 by A3,NAT_1:13;
  then
A27: c+1>=1+1 by XREAL_1:8;
  f>=c+1 by A2,NAT_1:13;
  then f>=2 by A27,XXREAL_0:2;
  then f-1>=2-1 by XREAL_1:11;
  then
A28: n+(f-1)>=n+1 by XREAL_1:8;
  f>=c+1 by A2,NAT_1:13;
  then f*f>=f*(c+1) by XREAL_1:66;
  then p^2>f+(n-1) by A1,A26,XXREAL_0:2;
  then p^2>=n+1 by A28,XXREAL_0:2;
  hence contradiction by A8,NAT_1:13;
end;

theorem Th25:
  for n,f,d,n1,a,q being Element of NAT st n-1=q|^n1*d & q|^n1>d &
d>0 & q is prime & a|^(n-'1) mod n = 1 & (a|^((n-'1) div q)-'1) gcd n = 1 holds
  n is prime
proof
  let n,f,d,n1,a,q be Element of NAT;
  assume that
A1: n-1=q|^n1*d & q|^n1>d & d>0 and
A2: q is prime;
  set f=q|^n1;
  assume
A3: a|^(n-'1) mod n = 1;
  assume
A4: (a|^((n-'1) div q)-'1) gcd n = 1;
  for q1 being Element of NAT st q1 divides f & q1 is prime holds ex a
being Element of NAT st a|^(n-'1) mod n = 1 & (a|^((n-'1) div q1)-'1) gcd n = 1
  proof
    let q1 be Element of NAT;
    assume
A5: q1 divides f;
    assume
A6: q1 is prime;
    consider a be Element of NAT such that
A7: a|^(n-'1) mod n = 1 & (a|^((n-'1) div q)-'1) gcd n = 1 by A3,A4;
    take a;
    thus thesis by A2,A5,A6,A7,Th18;
  end;
  hence thesis by A1,Th24;
end;

Lm2: for n being Element of NAT st 1<n & n<5 & n is prime holds n=2 or n=3
proof
  let n be Element of NAT;
  assume that
A1: 1<n & n<5 and
A2: n is prime;
  1+1<n+1 & n<4+1 by A1,XREAL_1:8;
  then 2<=n & n<=2+1 or 3<=n & n<=3+1 by NAT_1:13;
  hence thesis by A2,INT_2:46,NAT_1:9;
end;

Lm3: for k being Element of NAT st k<25 holds for n being Element of NAT st 1<
n & n*n<=k & n is prime holds n=2 or n=3

proof
  let k be Element of NAT;
  assume
A1: k<25;
  let n be Element of NAT;
  assume that
A2: 1<n and
A3: n*n<=k and
A4: n is prime;
  n*n<5*5 by A1,A3,XXREAL_0:2;
  then n<5 by Th1;
  hence thesis by A2,A4,Lm2;
end;

begin :: Some prime numbers.

theorem Th26:
  7 is prime
proof
  now
    let n be Element of NAT;
    7 = 2*3 + 1;
    then
A1: not 2 divides 7 by Th9;
    7 = 3*2 + 1;
    then
A2: not 3 divides 7 by Th9;
    assume
    1<n & n*n<=7 & n is prime;
    hence not n divides 7 by A1,A2,Lm3;
  end;
  hence thesis by Th14;
end;

