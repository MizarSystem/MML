theorem Th28:
  G is finite implies ord a divides ord G
proof
 assume A1: G is finite;
 then ord a = ord gr {a} by Th27;
 hence ord a divides ord G by A1,GROUP_2:178;
end;

theorem Th29:
  G is finite implies a|^ord G = 1_G
proof
 assume G is finite;
 then ord a divides ord G by Th28;
 then consider t being Nat such that
 A1: ord G = ord a * t by NAT_D:def 3;
 a|^ord G = a |^ ord a |^ t by A1,GROUP_1:50
         .= (1_G) |^ t by GROUP_1:82 .= 1_G by GROUP_1:42;
 hence thesis;
end;

theorem Th30:
  G is finite implies (a|^n)" = a|^(ord G - (n mod ord G))
  proof
   assume A1: G is finite;
   set q = ord G;
   set p' = n mod q;
   A2: q >=1 by A1,GROUP_1:90;
   then n mod q <=q by NAT_D:1;
   then reconsider q'=q -( n mod q) as Element of NAT by INT_1:18;
   a|^n*(a|^(q-(n mod q)))=a|^(n+q') by GROUP_1:48
   .= a|^((q*(n div q)+(n mod q))+q') by A2,NAT_D:2
   .= a|^(q*(n div q)+q +(-p'+p'))
   .= a|^(q*(n div q)+q)*(a|^(-p'+p')) by GROUP_1:64
   .= a|^(q*(n div q)+q)*(a|^(-p')*(a|^p')) by GROUP_1:65
   .= a|^(q*(n div q)+q)*((a|^p')"*(a|^p')) by GROUP_1:71
   .= a|^(q*(n div q)+q)*(1_G) by GROUP_1:def 6
   .= a|^(q*(n div q+1)) by GROUP_1:def 5
   .= a|^q|^(n div q+1) by GROUP_1:50
   .= (1_G)|^(n div q+1) by A1,Th29
   .= 1_G by GROUP_1:42;
   hence thesis by GROUP_1:20;
  end;

registration let G be associative (non empty multMagma);
  cluster the multMagma of G -> associative;
  coherence
  proof
    set H = the multMagma of G;
A1: the multF of G is associative by GROUP_1:31;
    let x,y,z be Element of H;
    reconsider a = x, b = y, c = z as Element of G;
    (x*y)*z = (the multF of G).((x*y),z)
           .= (the multF of G).((the multF of G).(x,y),z)
           .= (the multF of G).(x,(the multF of G).(y,z)) by A1,BINOP_1:def 3
           .= x*(y*z);
    hence thesis;
  end;
end;

registration let G be Group;
  cluster the multMagma of G -> Group-like;
  coherence
  proof
    set H = the multMagma of G;
    reconsider e = 1_G as Element of H;
    take e;
    let h be Element of H;
    reconsider h' = h as Element of G;
    thus h * e = (the multF of G).(h,e) .= h' * 1_G .= h by GROUP_1:def 5;
    thus e * h = (the multF of G).(e,h) .= 1_G * h' .= h by GROUP_1:def 5;
    reconsider g = h'" as Element of H;
    take g;
    thus h * g = (the multF of G).(h,g) .= h' * h'" .= e by GROUP_1:def 6;
    thus g * h = (the multF of G).(g,h) .= h'" * h' .= e by GROUP_1:def 6;
  end;
end;

theorem Th31:
  for G being strict Group st
  ord G > 1 holds ex a being Element of G st a <> 1_G
  proof let G be strict Group;
   assume that A1: ord G > 1 and
 A2: for a being Element of G holds a = 1_G;
     for a being Element of G holds a in (1).G
     proof
      let a be Element of G;
      a = 1_G by A2;
      then a in {1_G} by TARSKI:def 1;
      then a in the carrier of (1).G by GROUP_2:def 7;
      hence thesis by STRUCT_0:def 5;
     end;
   then the multMagma of G = (1).G by GROUP_2:71;
   hence contradiction by A1,GROUP_2:81;
 end;

theorem
  for G being strict Group st
  G is finite & ord G = p & p is prime holds
   for H being strict Subgroup of G holds H = (1).G or H = G
 proof let G be strict Group;
  assume that A1: G is finite and A2: ord G = p and A3: p is prime;
  let H be strict Subgroup of G;
  A4:H is finite by A1,GROUP_2:48;
  ord H divides p by A1,A2,GROUP_2:178;
  then ord H = 1 or ord H = p by A3,INT_2:def 5;
  hence thesis by A1,A2,A4,GROUP_2:82,85;
 end;

theorem Th33:
  multMagma(#INT,addint#) is associative Group-like
proof
  set G = multMagma (# INT, addint #);
  thus for h,g,f being Element of G holds (h * g) * f = h * (g * f)
   proof let h,g,f be Element of G;
     reconsider A=h,B = g, C = f as Integer by INT_1:def 2;
     A1: h * g = A + B by Th14;
     A2: g * f = B + C by Th14;
 thus (h * g) * f = A + B + C by A1,Th14
                 .= A + (B + C)
                 .= h * (g * f) by A2,Th14;
   end;
  reconsider e=0 as Element of G by INT_1:def 2;
  take e;
  let h be Element of G;
  reconsider A = h as Integer by INT_1:def 2;
  thus h * e = A + 0 by Th14 .= h;
  thus e * h = 0 + A by Th14 .= h;
  reconsider g = - A as Element of G by INT_1:def 2;
  take g;
  thus h * g = A + (- A) by Th14  .= e;
  thus g * h = (- A) + A by Th14  .= e;
end;

