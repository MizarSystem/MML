theorem Th25:
  b in gr {a} iff ex j1 st b=a|^j1
proof
 A1:b in gr {a} implies ex j1 st b=a|^j1
  proof
   assume b in gr {a};
   then consider F,I such that A2: len F = len I and
   A3: rng F c= {a} and A4: Product(F |^ I ) = b by GROUP_4:37;
   take Sum I;
A5:dom ((len F) |-> a) = Seg len F by FUNCOP_1:19
                      .= dom F by FINSEQ_1:def 3;
        for p be Nat st p in dom F holds F.p = ((len F)|-> a).p
    proof
     let p be Nat;
A6:  dom F = Seg len F by FINSEQ_1:def 3;
     assume A7: p in dom F;
     then F.p in rng F by FUNCT_1:def 5;
     then F.p = a by A3,TARSKI:def 1 .= ((len F)|-> a).p by A6,A7,FUNCOP_1:13;
     hence thesis;
    end;
   then b= Product(((len I) |-> a)|^I) by A2,A4,A5,FINSEQ_1:17
           .= a|^Sum I by Th24;
   hence thesis;
end;
  (ex j1 st b=a|^j1) implies b in gr {a}
  proof
   given j1 such that A8: b =a|^j1;
   consider n such that A9: j1 = n or j1 = -n by INT_1:8;
     per cases by A9;
 suppose A10:j1=n;
         ex F,I st len F = len I & rng F c= {a} & Product(F |^ I) = b
    proof
     take F =(n |-> a);
     A11: len F = n by FINSEQ_2:69 .= len(n |->@ 1) by FINSEQ_2:69;
     A12: rng F c= {a} by FUNCOP_1:19;
           Product(F|^(n |->@ 1)) = Product(F|^(len F |->@ 1)) by FINSEQ_2:69
               .=Product(n |-> a) by GROUP_4:31
               .=b by A8,A10,GROUP_4:15;
     hence thesis by A11,A12;
     end;
   hence thesis by GROUP_4:37;
end;
suppose j1 = -n;
     then A13: b"=a|^(-(-n)) by A8,GROUP_1:70 .= a|^n;
          ex F,I st len F = len I & rng F c= {a} & Product(F |^ I) = b"
      proof
       take F =(n |-> a);
       A14: len F = n by FINSEQ_2:69 .= len(n |->@ 1) by FINSEQ_2:69;
       A15: rng F c= {a} by FUNCOP_1:19;
             Product(F|^(n |->@ 1)) = Product(F|^(len F |->@ 1)) by FINSEQ_2:69
                 .=Product(n |-> a) by GROUP_4:31 .= b" by A13,GROUP_4:15;
       hence thesis by A14,A15;
     end;
  then b" in gr {a} by GROUP_4:37;
  then b"" in gr {a} by GROUP_2:60;
  hence thesis by GROUP_1:19;
 end;
end;
hence thesis by A1;
end;

theorem Th26:
  G is finite implies a is_not_of_order_0
  proof
    assume A1:G is finite;
   ex n st n<> 0 & a|^n = 1_G
   proof
    deffunc F(Nat) = a|^$1;
    consider F being FinSequence such that A2: len F = ord G +1 and
    A3: for p be Nat st p in Seg (ord G +1) holds F.p = F(p)
    from FINSEQ_1:sch 2;
    A4: dom F = Seg (ord G + 1) by A2,FINSEQ_1:def 3;
    A5: for y st y in rng F holds ex n st y=a|^n
      proof
       let y;
       assume y in rng F;
       then consider x such that A6: x in dom F and A7: F.x=y by FUNCT_1:def 5;
       reconsider n=x as Element of NAT by A6;
       take n;
       thus thesis by A3,A4,A6,A7;
      end;
        rng F c= the carrier of G
    proof
     for x holds x in rng F implies x in the carrier of G
       proof
        let y;
        assume y in rng F;
        then ex n st y= a|^n by A5;
        hence thesis;
       end;
      hence thesis by TARSKI:def 3;
    end;
then reconsider F'=F as Function of Seg (ord G +1),the carrier of G by A4,
FUNCT_2:def 1,RELSET_1:11;
  consider c being finite set such that
A8: c = the carrier of G & ord G = card c by A1,GROUP_1:def 15;
A9:Card ord G = Card the carrier of G by A8;
A10:Card Seg(ord G+1) = Card (ord G +1) by FINSEQ_1:76;
     ord G < ord G +1 by XREAL_1:31;
then Card the carrier of G <` Card Seg (ord G +1) by A9,A10,NAT_1:42;
then consider x,y such that A11:x in Seg (ord G +1) and
A12: y in Seg (ord G +1) and
A13:x <> y and A14: F'.x = F'.y by FINSEQ_4:80;
reconsider p=x,n=y as Element of NAT by A11,A12;
      per cases by A13,REAL_1:def 5;
  suppose A15: n>p;
      then reconsider t = n-p as Element of NAT by INT_1:18;
      reconsider e=0 as Integer;
      take t;
      F'.p =a|^p by A3,A11;
      then a|^n=a|^p by A3,A12,A14;
      then a|^(n+-p)=a|^p *a|^(-p) by GROUP_1:64;
      then a|^t =a|^(p+-p) by GROUP_1:64;
      hence thesis by A15,GROUP_1:59;
   end;
   suppose A16: p>n;
      then reconsider t = p-n as Element of NAT by INT_1:18;
      take t;
      reconsider e=0 as Integer;
      F'.p =a|^p by A3,A11;
      then a|^n=a|^p by A3,A12,A14;
      then a|^(p+-n)=a|^n *a|^(-n) by GROUP_1:64;
      then a|^t =a|^(n+-n) by GROUP_1:64;
      hence thesis by A16,GROUP_1:59;
    end;
  end;
 hence thesis by GROUP_1:def 11;
end;

theorem Th27:
  G is finite implies ord a = ord gr {a}
 proof
  assume A1:G is finite;
  then A2:gr {a} is finite by GROUP_2:48;
  deffunc F(Nat) = a|^$1;
  consider F being FinSequence such that A3: len F = ord a and
  A4:  for p be Nat st p in Seg ord a holds F.p = F(p) from FINSEQ_1:sch 2;
  A5: dom F = Seg ord a by A3,FINSEQ_1:def 3;
  a is_not_of_order_0 by A1,Th26;
  then A6:ord a <> 0 by GROUP_1:def 12;
  then A7:ord a >0;
   A8: a in rng F
    proof
      ex x st x in dom F & F.x=a
        proof
         set x'=1;
         A9:x' in dom F
           proof
            (ord a)+x' > 0+x' by A7,XREAL_1:8;
            then (ord a) >= x' by NAT_1:13;
            hence thesis by A5;
           end;
         then F.x'=a|^x' by A4,A5 .=a by GROUP_1:44;
         hence thesis by A9;
        end;
     hence thesis by FUNCT_1:def 5;
   end;
A10: rng F = the carrier of gr {a}
 proof
   A11:for y st y in rng F holds ex n st y=a|^n
     proof
      let y;
      assume y in rng F;
      then consider x such that
A12:  x in dom F and A13: F.x=y by FUNCT_1:def 5;
      reconsider n=x as Element of NAT by A12;
      take n;
      thus thesis by A4,A5,A12,A13;
     end;
A14: rng F c= the carrier of gr {a}
    proof
      for x st x in rng F holds x in the carrier of gr {a}
      proof
       let y;
       assume y in rng F;
       then consider n such that A15:y= a|^n by A11;
       y in gr {a} by A15,Th25;
       hence thesis by STRUCT_0:def 5;
      end;
     hence thesis by TARSKI:def 3;
   end;
     the carrier of gr {a} c= rng F
 proof
   ex H being strict Subgroup of G st the carrier of H = rng F
   proof
     reconsider car=rng F as non empty set by A8;
     A16:dom(the multF of G)=
     [:the carrier of G,the carrier of G:] by FUNCT_2:def 1;
     the carrier of gr {a} c= the carrier of G by GROUP_2:def 5;
     then A17: car c= the carrier of G by A14,XBOOLE_1:1;
     then [:car,car:] c= [:the carrier of G,the carrier of G:] by ZFMISC_1:119
;
 then A18:dom((the multF of G)||car)=[:car,car:] by A16,RELAT_1:91;
          rng((the multF of G)||car) c= car
       proof
           for y st y in rng((the multF of G)||car) holds y in car
            proof
              set f=(the multF of G)||car;
              let y;
              assume y in rng(f);
              then consider x such that A19:x in dom(f) and
              A20: f.x=y by FUNCT_1:def 5;
              consider xp,yp such that A21:[xp,yp]=x
               by A18,A19,RELAT_1:def 1;
              xp in car by A18,A19,A21,ZFMISC_1:106;
              then consider p such that A22:  xp = a|^p by A11;
              yp in car by A18,A19,A21,ZFMISC_1:106;
              then consider s such that A23:  yp = a|^s by A11;
              A24:  y = a|^p*(a|^s) by A19,A20,A21,A22,A23,FUNCT_1:70
                      .= a|^(p+s) by GROUP_1:48;
              a|^(p+s) in car
               proof
                  ex x st x in dom F & F.x=a|^(p+s)
                 proof
                  set t=(p+s) mod (ord a);
                  A25:t < ord a by A7,NAT_D:1;
                  per cases;
                    suppose A26:t=0;
                A27:ord a in dom F by A5,A6,FINSEQ_1:5;
  A28:a|^(p+s)=a|^((ord a)*((p+s) div (ord a)) + ((p+s) mod (ord a))) by A7,
NAT_D:2
    .=a|^((ord a)*((p+s) div (ord a)))*(a|^ ((p+s) mod (ord a))) by GROUP_1:48
    .=a|^(ord a)|^((p+s) div (ord a))*(a|^ ((p+s) mod (ord a))) by GROUP_1:50
    .=(1_G)|^((p+s) div (ord a))*(a|^ ((p+s) mod (ord a))) by GROUP_1:82
    .=(1_G) *(a|^ ((p+s) mod (ord a))) by GROUP_1:42
    .=a|^0 by A26,GROUP_1:def 5 .= 1_G by GROUP_1:43
    .= a|^(ord a) by GROUP_1:82 .= F.ord a by A4,A5,A27;
     take x = ord a;
     thus x in dom F & F.x=a|^(p+s) by A5,A6,A28,FINSEQ_1:5;
     end;
     suppose t>0;
       then t+1 > 0+1 by XREAL_1:8;
       then A29:t >= 1 by NAT_1:13;
       take x = t;
       A30:t in dom F by A5,A25,A29;
       a|^(p+s)=a|^((ord a)*((p+s) div (ord a)) + ((p+s) mod (ord a))) by A7,
NAT_D:2
    .=a|^((ord a)*((p+s) div (ord a))) *(a|^ ((p+s) mod (ord a))) by GROUP_1:48
    .=a|^(ord a)|^((p+s) div (ord a)) *(a|^ ((p+s) mod (ord a))) by GROUP_1:50
    .=(1_G)|^((p+s) div (ord a)) *(a|^ ((p+s) mod (ord a))) by GROUP_1:82
    .=(1_G) *(a|^ ((p+s) mod (ord a))) by GROUP_1:42
    .=a|^ ((p+s) mod (ord a)) by GROUP_1:def 5;
         hence x in dom F & F.x=a|^(p+s) by A4,A5,A30;
       end;
       end;
       hence thesis by FUNCT_1:def 5;
     end;
   hence thesis by A24;
   end;
hence thesis by TARSKI:def 3;
  end;
 then reconsider op=(the multF of G)||car as BinOp of car by A18,FUNCT_2:def 1
,RELSET_1:11;
 set H=multMagma(#car,op#);
 H is Group
 proof
A31: for f,g being Element of H, f',g' being Element of G
    st f = f' & g = g' holds f'*g' = f*g
 proof
  let f,g be Element of H;
  let f',g' be Element of G;
  assume A32: f = f' & g =g';
  set A = [:car,car:];
  f*g = ((the multF of G)||car).[f,g];
  hence thesis by A32,FUNCT_1:72;
  end;
 A33:for f,g,h being Element of H holds (f * g) * h = f * (g * h)
 proof
  let f,g,h be Element of H;
  reconsider f'=f,g'=g,h'=h as Element of G by A17,TARSKI:def 3;
  A34:  f'*g' = f*g by A31;
  A35:  g * h = g' * h' by A31;
  (f*g)*h =(f'*g')*h' by A31,A34 .= f'*(g'*h') by GROUP_1:def 4
 .=f*(g*h) by A31,A35;
  hence thesis;
 end;
   ex e being Element of H st
    for h being Element of H holds
     h * e = h & e * h = h &
     ex g being Element of H st h * g = e & g * h = e
     proof
       a|^(ord a) in car
       proof
          ex x st x in dom F & F.x=a|^(ord a)
          proof
           set x'=(ord a);
           A36: x' in Seg ord a by A6,FINSEQ_1:5;
           then F.x'=a|^x' by A4;
           hence thesis by A5,A36;
          end;
        hence thesis by FUNCT_1:def 5;
     end;
 then reconsider e=1_G as Element of H by GROUP_1:82;
 take e;
 for h being Element of H holds
 h * e = h & e * h = h &
 ex g being Element of H st h * g = e & g * h = e
  proof
   let h be Element of H;
   reconsider h'=h as Element of G by A17,TARSKI:def 3;
   thus h * e = h
    proof
     h*e = h' *(1_G) by A31 .= h' by GROUP_1:def 5;
     hence thesis;
    end;
   thus e * h = h
    proof
     e*h = (1_G)*h' by A31 .= h' by GROUP_1:def 5;
     hence thesis;
    end;
