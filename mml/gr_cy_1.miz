    reconsider A = h as Element of Segm(n) by A2;
    reconsider A as Element of NAT by ORDINAL1:def 13;
    A3:A < n by A1,NAT_1:45;
    A4: h * e = (A + 0) mod n by A1,A2,Def5
             .= h by A3,NAT_D:24;
          e * h = (0 + A) mod n by A1,A2,Def5
             .= h by A3,NAT_D:24;
