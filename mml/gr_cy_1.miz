   then A2: INT.Group(n) = multMagma(#Segm(n),addint(n)#) by Def6;
   reconsider e = 0 as Element of Segm(n) by A1,NAT_1:45;
   reconsider e as Element of INT.Group(n) by A2;
    for h being Element of INT.Group(n)
   holds h * e = h & e * h = h
   proof
