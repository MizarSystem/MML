A1202:          n<=n+1 by NAT_1:11;
A1203:          len g=len g1+len g2 by FINSEQ_1:22;
                then len g1+1<=len g by A1017,A1191,XREAL_1:7;
                then
A1204:          LSeg(g,len g1)=LSeg(ppi,qq) by A1183,A1184,A1194,TOPREAL1:def 3
