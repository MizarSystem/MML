                        then 1<k by A24,XXREAL_0:1;
                        then
A1219:                  L~f1 /\ LSeg(f,k) ={f/.k} by A3,A6,A7,GOBOARD2:4;
A1220:                  n+1 in dom g1 by A1185,A1198,A1213,FINSEQ_3:25;
