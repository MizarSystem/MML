                let n,m;
                assume that
A1185:          m>n+1 and
A1186:          n in dom g and
A1187:          n+1 in dom g and
A1188:          m in dom g and
A1189:          m+1 in dom g;
A1190:          1<=n by A1186,FINSEQ_3:25;
                i1+1<=j1 by A1011,NAT_1:13;
                then
A1191:          1<=l by XREAL_1:19;
                then
A1192:          1 in dom g2 by A1017,FINSEQ_3:25;
