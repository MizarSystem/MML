                hence thesis by A1126,A1118,A1121,A1128,A1123,A1130,TOPREAL1:8
;
              end;
              thus g is unfolded
              proof
                let n be Nat;
                assume that
A1131:          1<=n and
A1132:          n+2 <= len g;
A1133:          n+1+1<=len g by A1132;
