A1136:          n-len g1 + 2 <= len g2 by A1132,A1135,XREAL_1:20;
A1137:          1<= n+1 by NAT_1:11;
A1138:          n<=n+1 by NAT_1:11;
A1139:          n+(1+1)=n+1+1;
                per cases;
                suppose
A1140:            n+2 <= len g1;
A1141:            n+(1+1)=n+1+1;
A1142:            n+1 in dom g1 by A1131,A1140,SEQ_4:135;
                  then
A1143:            g/.(n+1)=g1/.(n+1) by FINSEQ_4:68;
