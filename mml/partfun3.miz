registration
  let r be real number;
  cluster r/r -> non negative;
coherence
proof
   r <= 0 or 0 <= r;
  hence thesis by REAL_2:125;
end;
end;

registration
  let r be real number;
  cluster r*r -> non negative;
coherence by XREAL_1:65;
  cluster r*(r") -> non negative;
coherence
proof
   r*(r") = r/r by XCMPLX_0:def 9;
  hence thesis;
end;
end;

