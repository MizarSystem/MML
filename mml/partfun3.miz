definition
  let C be set, f be PartFunc of C,REAL;
  func sqrt f -> PartFunc of C,REAL means :dsqrt:
  dom it = dom f &
  for x being set st x in dom it holds it.x = sqrt(f.x);
existence
proof
  per cases;
  suppose
A1: C is empty;
  reconsider F = {} as PartFunc of C,REAL by PARTFUN1:56;
  take F;
  thus dom F = dom f by A1,RELAT_1:60,XBOOLE_1:3;
  thus thesis by RELAT_1:60;
  end;
  suppose C is non empty;
  then reconsider C' = C as non empty set;
  defpred X[set] means $1 in dom f;
  deffunc U(Element of C') = sqrt(f.$1);
  consider F be PartFunc of C',REAL such that
A2: for c being Element of C' holds c in dom F iff X[c] and
A3: for c being Element of C' st c in dom F holds F.c = U(c) from SEQ_1:sch 3;
  reconsider F as PartFunc of C,REAL;
  take F;
  thus dom F = dom f by A2,SUBSET_1:8;
  thus thesis by A3;
end;
end;
uniqueness
proof
  deffunc U(Element of C) = sqrt(f.$1);
  let f1, f2 be PartFunc of C,REAL such that
a1: dom f1 = dom f and
a2: for x being set st x in dom f1 holds f1.x = sqrt(f.x) and
a3: dom f2 = dom f and
a4: for x being set st x in dom f2 holds f2.x = sqrt(f.x);
a5: for f1, f2 be PartFunc of C,REAL st
   (dom f1 = dom f & for c being Element of C st c in dom f1 holds
      f1.c = U(c)) &
   (dom f2 = dom f & for c being Element of C st c in dom f2 holds
      f2.c = U(c)) holds f1 = f2 from SEQ_1:sch 4;
a6: for c being Element of C st c in dom f1 holds f1.c = U(c) by a2;
  for c being Element of C st c in dom f2 holds f2.c = U(c) by a4;
  hence thesis by a1,a3,a5,a6;
 end;
end;

registration
  let X be set;
