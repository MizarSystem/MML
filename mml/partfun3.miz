registration
  let X be set,
      f, g be Function of X, REAL;
  cluster f+g -> quasi_total;
coherence
proof
  dom (f+g) = dom f /\ dom g by SEQ_1:def 3
  .= X /\ dom g by FUNCT_2:def 1
  .= X /\ X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
  cluster f-g -> quasi_total;
coherence
proof
  dom (f-g) = dom f /\ dom g by SEQ_1:def 4
  .= X /\ dom g by FUNCT_2:def 1
  .= X /\ X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
  cluster f(#)g -> quasi_total;
coherence
proof
