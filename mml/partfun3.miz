  hence thesis by A1,A2,RFUNCT_1:def 8;
end;
end;

registration
  let X be non empty set;
  let f be non-empty Function of X,REAL;
  cluster f^ -> non-empty;
coherence
proof
  set R = f^;
  assume not thesis;
  then 0 in rng R by RELAT_1:def 9;
  then consider x being set such that
A1: x in dom R and
A2: R.x = 0 by FUNCT_1:def 5;
   dom R = dom f by Th2;
  then f.x in rng f by A1,FUNCT_1:def 5;
  then reconsider a = f.x as non zero (real number) by RELAT_1:def 9;
   a" is non zero;
  hence thesis by A1,A2,RFUNCT_1:def 8;
end;
end;

definition
  let f be real-yielding Function;
  func sqrt f -> Function means :Def5:
  dom it = dom f &
  for x being set st x in dom it holds it.x = sqrt(f.x);
existence
  proof
    deffunc F(set) = sqrt (f.$1);
     ex h being Function st dom h = dom f &
    for x being set st x in dom f holds h.x = F(x) from FUNCT_1:sch 3;
    hence thesis;
  end;
  uniqueness
  proof
  let h, g be Function such that
A1: dom h = dom f and
A2: for c being set st c in dom h holds h.c = sqrt (f.c) and
A3: dom g = dom f and
A4: for c being set st c in dom g holds g.c = sqrt (f.c);
     now let x be set;
      assume
A5:   x in dom h;
      hence h.x = sqrt (f.x) by A2 .= g.x by A1,A3,A4,A5;
    end;
    hence h = g by A1,A3,FUNCT_1:9;
  end;
end;

registration
  let f be real-yielding Function;
  cluster sqrt f -> real-yielding;
  coherence
  proof
    set F = sqrt f;
    let y be set;
    assume y in rng F;
    then consider x being set such that
A1: x in dom F and
A2: F.x = y by FUNCT_1:def 5;
     F.x = sqrt(f.x) by A1,Def5;
    hence thesis by A2;
  end;
end;

definition
  let C be set, D be real-membered set, f be PartFunc of C,D;
  redefine func sqrt f -> PartFunc of C,REAL;
  coherence
  proof
    set F = sqrt f;
A1: dom F = dom f by Def5;
     rng F c= REAL by SEQ_1:def 1;
    then F is PartFunc of dom F, REAL by PARTFUN1:25;
    hence thesis by A1,PARTFUN1:28;
  end;
end;

registration
  let X be set;
  let f be nonnegative-yielding Function of X,REAL;
  cluster sqrt f -> nonnegative-yielding;
coherence
proof
  set R = sqrt f;
  let r be real number;
  assume r in rng R;
  then consider x being set such that
A1: x in dom R and
A2: R.x = r by FUNCT_1:def 5;
   dom R = dom f by Def5;
  then f.x in rng f by A1,FUNCT_1:def 5;
  then reconsider a = f.x as non negative (real number) by Def4;
   sqrt a is non negative;
  hence thesis by A1,A2,Def5;
end;
end;

registration
  let X be set;
  let f be positive-yielding Function of X,REAL;
