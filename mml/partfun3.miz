reserve
  T for non empty TopSpace,
  f, g for continuous RealMap of T,
  r for real number;

registration
  let T, f, g;
  set c = the carrier of T;
  reconsider F = f, G = g as continuous map of T,R^1 by TOPMETR:24,TOPREAL6:83;
  cluster f+g -> continuous;
coherence
proof
  consider H being map of T,R^1 such that
a1: for p being Point of T, r1,r2 being real number st
     F.p = r1 & G.p = r2 holds H.p = r1+r2 and
a2: H is continuous by JGRAPH_2:29;
  reconsider h = H as RealMap of T by TOPMETR:24;
a3: dom h = c /\ c by FUNCT_2:def 1
   .= c /\ dom g by FUNCT_2:def 1
   .= dom f /\ dom g by FUNCT_2:def 1;
  for c being Element of the carrier of T st c in dom h holds h.c = f.c + g.c
    by a1;
  then h = f+g by a3,SEQ_1:def 3;
  hence thesis by a2,TOPREAL6:83;
end;
  cluster f-g -> continuous;
coherence
proof
  consider H being map of T,R^1 such that
a1: for p being Point of T, r1,r2 being real number st
     F.p = r1 & G.p = r2 holds H.p = r1-r2 and
a2: H is continuous by JGRAPH_2:31;
  reconsider h = H as RealMap of T by TOPMETR:24;
a3: dom h = c /\ c by FUNCT_2:def 1
   .= c /\ dom g by FUNCT_2:def 1
   .= dom f /\ dom g by FUNCT_2:def 1;
  for c being Element of the carrier of T st c in dom h holds h.c = f.c - g.c
    by a1;
  then h = f-g by a3,SEQ_1:def 4;
  hence thesis by a2,TOPREAL6:83;
end;
  cluster f(#)g -> continuous;
coherence
proof
  consider H being map of T,R^1 such that
a1: for p being Point of T, r1,r2 being real number st
     F.p = r1 & G.p = r2 holds H.p = r1*r2 and
a2: H is continuous by JGRAPH_2:35;
  reconsider h = H as RealMap of T by TOPMETR:24;
a3: dom h = c /\ c by FUNCT_2:def 1
   .= c /\ dom g by FUNCT_2:def 1
   .= dom f /\ dom g by FUNCT_2:def 1;
  for c being Element of the carrier of T st c in dom h holds h.c = f.c * g.c
    by a1;
  then h = f(#)g by a3,SEQ_1:def 5;
  hence thesis by a2,TOPREAL6:83;
end;
end;

registration
  let T, f;
  cluster -f -> continuous;
coherence
proof
  set c = the carrier of T;
  reconsider F = f as continuous map of T,R^1 by TOPMETR:24,TOPREAL6:83;
  consider H being map of T,R^1 such that
a1: for p being Point of T, r1 being real number st
     F.p = r1 holds H.p = -r1 and
a2: H is continuous by JGRAPH_4:13;
  reconsider h = H as RealMap of T by TOPMETR:24;
a3: dom h = c by FUNCT_2:def 1
   .= dom f by FUNCT_2:def 1;
  for c being Element of the carrier of T st c in dom h holds h.c = -f.c
    by a1;
  then h = -f by a3,SEQ_1:def 7;
  hence thesis by a2,TOPREAL6:83;
end;
end;

registration
