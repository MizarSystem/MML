a3: R.x = r by FUNCT_1:def 5;
  dom R = dom f by Tdd;
  then f.x in rng f by a2,FUNCT_1:def 5;
  then reconsider a = f.x as positive (real number) by dposy;
  a" is positive;
  hence thesis by a3,a2,RFUNCT_1:def 8;
end;
end;

