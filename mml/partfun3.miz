registration
  cluster positive-yielding -> nonnegative-yielding non-empty Relation;
coherence
proof
  let R be Relation such that
A1: for r being real number st r in rng R holds 0 < r;
  thus R is nonnegative-yielding
  proof
    let r be real number;
    thus thesis by A1;
  end;
  assume not thesis;
  then 0 in rng R by RELAT_1:def 9;
  hence thesis by A1;
end;
  cluster negative-yielding -> nonpositive-yielding non-empty Relation;
coherence
proof
  let R be Relation such that
A2: for r being real number st r in rng R holds 0 > r;
  thus R is nonpositive-yielding
  proof
    let r be real number;
    thus thesis by A2;
  end;
  assume not thesis;
  then 0 in rng R by RELAT_1:def 9;
  hence thesis by A2;
end;
end;

registration
  let X be set;
  cluster negative-yielding Function of X,REAL;
existence
proof
  take X --> -1;
  thus thesis;
end;
  cluster positive-yielding Function of X,REAL;
existence
proof
  take X --> (1 qua Element of REAL);
  thus thesis;
end;
end;

registration
  cluster non-empty real-yielding Function;
  existence
  proof
    consider f being non-empty Function of 0,REAL;
    take f;
    thus thesis;
  end;
end;

theorem Th2:
  for f being non-empty real-yielding Function holds
  dom(f^) = dom f
proof
  let f be non-empty real-yielding Function;
  thus dom (f^) = dom f \ f"{0} by RFUNCT_1:def 8
  .= dom f;
end;

theorem Th3:
  for X being non empty set,
      f being PartFunc of X,REAL, g being non-empty PartFunc of X,REAL holds
  dom(f/g) = dom f /\ dom g
proof
  let X be non empty set,
      f be PartFunc of X,REAL,
      g be non-empty PartFunc of X,REAL;
  thus dom (f/g) = dom f /\ (dom g \ g"{0}) by RFUNCT_1:def 4
  .= dom f /\ dom g;
end;

registration
  let X be set;
  let f, g be nonpositive-yielding PartFunc of X,REAL;
  cluster f+g -> nonpositive-yielding;
coherence
proof
  set R = f+g;
  let r be real number;
  assume r in rng R;
  then consider x being set such that
A1: x in dom R and
A2: R.x = r by FUNCT_1:def 5;
   dom R = dom f /\ dom g by SEQ_1:def 3;
  then x in dom f & x in dom g by A1,XBOOLE_0:def 3;
  then f.x in rng f & g.x in rng g by FUNCT_1:def 5;
  then reconsider a = f.x, b = g.x as non positive (real number) by Def3;
   a+b is non positive;
  hence thesis by A1,A2,SEQ_1:def 3;
end;
end;

