registration
  cluster positive-yielding -> nonnegative-yielding non-empty Relation;
coherence
proof
  let R be Relation such that
a1: for r being real number st r in rng R holds 0 < r;
  thus R is nonnegative-yielding
  proof
    let r be real number;
    thus thesis by a1;
  end;
  assume not thesis;
  then 0 in rng R by RELAT_1:def 9;
  hence thesis by a1;
end;
  cluster negative-yielding -> nonpositive-yielding non-empty Relation;
coherence
proof
  let R be Relation such that
a1: for r being real number st r in rng R holds 0 > r;
  thus R is nonpositive-yielding
  proof
    let r be real number;
    thus thesis by a1;
  end;
  assume not thesis;
  then 0 in rng R by RELAT_1:def 9;
  hence thesis by a1;
end;
end;

registration
  let X be set;
  cluster negative-yielding Function of X,REAL;
existence
proof
  take X --> -1;
  thus thesis;
end;
  cluster positive-yielding Function of X,REAL;
existence
proof
  take X --> (1 qua Element of REAL);
  thus thesis;
end;
end;

theorem Tdd:
  for X being non empty set, f being non-empty PartFunc of X,REAL holds
  dom(f^) = dom f
proof
  let X be non empty set,
      f be non-empty PartFunc of X,REAL;
  thus dom (f^) = dom f \ f"{0} by RFUNCT_1:def 8
  .= dom f;
end;

theorem Tdd2:
  for X being non empty set,
      f being PartFunc of X,REAL, g being non-empty PartFunc of X,REAL holds
  dom(f/g) = dom f /\ dom g
proof
  let X be non empty set,
      f be PartFunc of X,REAL,
      g be non-empty PartFunc of X,REAL;
  thus dom (f/g) = dom f /\ (dom g \ g"{0}) by RFUNCT_1:def 4
  .= dom f /\ dom g;
end;

registration
  let X be set;
  let f, g be nonpositive-yielding PartFunc of X,REAL;
  cluster f+g -> nonpositive-yielding;
coherence
proof
  set R = f+g;
  let r be real number;
  assume r in rng R;
  then consider x being set such that
a2: x in dom R and
a3: R.x = r by FUNCT_1:def 5;
  dom R = dom f /\ dom g by SEQ_1:def 3;
  then x in dom f & x in dom g by a2,XBOOLE_0:def 3;
  then f.x in rng f & g.x in rng g by FUNCT_1:def 5;
  then reconsider a = f.x, b = g.x as non positive (real number) by dnonpy;
  a+b is non positive;
  hence thesis by a3,a2,SEQ_1:def 3;
end;
end;

