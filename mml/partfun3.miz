begin

registration
  let r be real number;
  cluster r/r -> non negative;
coherence
proof
  r <= 0 or 0 <= r;
  hence thesis by REAL_2:125;
end;
end;

registration
  let r be real number;
  cluster r*r -> non negative;
coherence by REAL_1:93;
  cluster r*(r") -> non negative;
coherence
proof
  r*(r") = r/r by XCMPLX_0:def 9;
  hence thesis;
end;
end;

registration
  let r be non negative (real number);
  cluster sqrt r -> non negative;
coherence by SQUARE_1:def 4;
end;

registration
  let r be positive (real number);
  cluster sqrt r -> positive;
coherence by SQUARE_1:93;
end;

theorem
  for f being Function, A being set st f is one-to-one & A c= dom (f")
   holds f.:(f".:A) = A
proof
  let f be Function, A be set;
  assume that
A1: f is one-to-one and
A2: A c= dom (f");
  f" is one-to-one by A1,FUNCT_1:62;
  then f"".:(f".:A)=A by A2,JGRAPH_6:22;
  hence thesis by A1,FUNCT_1:65;
end;

registration
  let f be non-empty Function;
  cluster f"{0} -> empty;
coherence
proof
  assume not thesis;
  then consider x being set such that
a1:  x in f"{0} by XBOOLE_0:def 1;
  f.x in {0} by a1,FUNCT_1:def 13;
  then
a2: f.x = 0 by TARSKI:def 1;
  x in dom f by a1,FUNCT_1:def 13;
  then f.x in rng f by FUNCT_1:def 5;
  hence thesis by a2,RELAT_1:def 9;
end;
end;

definition
  let R be Relation;
  attr R is positive-yielding means :dposy:
  for r being real number st r in rng R holds 0 < r;
  attr R is negative-yielding means :dnegy:
  for r being real number st r in rng R holds 0 > r;
  attr R is nonpositive-yielding means :dnonpy:
  for r being real number st r in rng R holds 0 >= r;
  attr R is nonnegative-yielding means :dnonny:
  for r being real number st r in rng R holds 0 <= r;
end;

registration
  let X be set, r be positive (real number);
  cluster X --> r -> positive-yielding;
coherence
proof
  let x be real number;
  assume
a1: x in rng (X --> r);
  then X is non empty by FUNCOP_1:16;
  then rng (X --> r) = {r} by FUNCOP_1:14;
  hence thesis by a1,TARSKI:def 1;
end;
end;

