registration
  let X be set;
  let r be non positive (real number);
  let f be nonpositive-yielding PartFunc of X,REAL;
  cluster r(#)f -> nonnegative-yielding;
coherence
proof
  set R = r(#)f;
  let z be real number;
  assume z in rng R;
  then consider x being set such that
a2: x in dom R and
a3: R.x = z by FUNCT_1:def 5;
  dom R = dom f by SEQ_1:def 6;
  then f.x in rng f by a2,FUNCT_1:def 5;
  then reconsider a = f.x as non positive (real number) by dnonpy;
  r*a is non negative;
  hence thesis by a3,a2,SEQ_1:def 6;
end;
end;

