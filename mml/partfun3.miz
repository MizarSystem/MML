definition
  let X be set,
  f, g be Function of X, REAL;
  redefine func f+g -> Function of X,REAL;
coherence
proof
   dom (f+g) = dom f /\ dom g by SEQ_1:def 3
  .= X /\ dom g by FUNCT_2:def 1
  .= X /\ X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
  redefine func f-g -> Function of X,REAL;
coherence
proof
   dom (f-g) = dom f /\ dom g by SEQ_1:def 4
  .= X /\ dom g by FUNCT_2:def 1
  .= X /\ X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
  redefine func f(#)g -> Function of X,REAL;
coherence
proof
   dom (f(#)g) = dom f /\ dom g by SEQ_1:def 5
  .= X /\ dom g by FUNCT_2:def 1
  .= X /\ X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
end;

definition
  let X be set,
      f be Function of X, REAL;
  redefine func -f -> Function of X,REAL;
coherence
proof
   dom -f = dom f by SEQ_1:def 7
  .= X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
  redefine func abs f -> Function of X,REAL;
coherence
proof
   dom abs f = dom f by SEQ_1:def 10
  .= X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
  redefine func sqrt f -> Function of X,REAL;
coherence
proof
   dom sqrt f = dom f by Def5
  .= X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
end;

definition
  let X be set,
      f be Function of X, REAL,
      r be real number;
  redefine func  r(#)f -> Function of X,REAL;
coherence
proof
   dom (r(#)f) = dom f by SEQ_1:def 6
  .= X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
end;

definition
  let X be set, f be non-empty Function of X, REAL;
  redefine func f^ -> Function of X,REAL;
coherence
proof
   dom (f^) = dom f by Th2
  .= X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
end;

definition
  let X be non empty set,
      f be Function of X, REAL,
      g be non-empty Function of X, REAL;
  redefine func f/g -> Function of X,REAL;
coherence
proof
   dom (f/g) = dom f /\ dom g by Th3
  .= X /\ dom g by FUNCT_2:def 1
  .= X /\ X by FUNCT_2:def 1;
  hence thesis by FUNCT_2:def 1;
end;
end;

reserve
  T for non empty TopSpace,
  f, g for continuous RealMap of T,
  r for real number;

definition
  let T, f, g;
  set c = the carrier of T;
  redefine func f+g -> continuous RealMap of T;
coherence
proof
  reconsider F = f, G = g as continuous Function
 of T,R^1 by TOPMETR:24,TOPREAL6:83;
  consider H being Function of T,R^1 such that
A1: for p being Point of T, r1,r2 being real number st
     F.p = r1 & G.p = r2 holds H.p = r1+r2 and
A2: H is continuous by JGRAPH_2:29;
  reconsider h = H as RealMap of T by TOPMETR:24;
A3: dom h = c /\ c by FUNCT_2:def 1
   .= c /\ dom g by FUNCT_2:def 1
   .= dom f /\ dom g by FUNCT_2:def 1;
   for c being set st c in dom h holds h.c = f.c + g.c by A1;
  then h = f+g by A3,SEQ_1:def 3;
  hence thesis by A2,TOPREAL6:83;
end;
  redefine func f-g -> continuous RealMap of T;
coherence
proof
  reconsider F = f, G = g as continuous Function
 of T,R^1 by TOPMETR:24,TOPREAL6:83;
  consider H being Function of T,R^1 such that
A4: for p being Point of T, r1,r2 being real number st
     F.p = r1 & G.p = r2 holds H.p = r1-r2 and
A5: H is continuous by JGRAPH_2:31;
  reconsider h = H as RealMap of T by TOPMETR:24;
A6: dom h = c /\ c by FUNCT_2:def 1
   .= c /\ dom g by FUNCT_2:def 1
   .= dom f /\ dom g by FUNCT_2:def 1;
   for c being set st c in dom h holds h.c = f.c - g.c by A4;
  then h = f-g by A6,SEQ_1:def 4;
  hence thesis by A5,TOPREAL6:83;
end;
  redefine func f(#)g -> continuous RealMap of T;
coherence
proof
  reconsider F = f, G = g as continuous Function
 of T,R^1 by TOPMETR:24,TOPREAL6:83;
  consider H being Function of T,R^1 such that
A7: for p being Point of T, r1,r2 being real number st
     F.p = r1 & G.p = r2 holds H.p = r1*r2 and
A8: H is continuous by JGRAPH_2:35;
  reconsider h = H as RealMap of T by TOPMETR:24;
A9: dom h = c /\ c by FUNCT_2:def 1
   .= c /\ dom g by FUNCT_2:def 1
   .= dom f /\ dom g by FUNCT_2:def 1;
   for c being set st c in dom h holds h.c = f.c * g.c by A7;
  then h = f(#)g by A9,SEQ_1:def 5;
  hence thesis by A8,TOPREAL6:83;
end;
end;

definition
  let T, f;
  redefine func -f -> continuous RealMap of T;
coherence
proof
  set c = the carrier of T;
  reconsider F = f as continuous Function of T,R^1 by TOPMETR:24,TOPREAL6:83;
  consider H being Function of T,R^1 such that
A1: for p being Point of T, r1 being real number st
     F.p = r1 holds H.p = -r1 and
A2: H is continuous by JGRAPH_4:13;
  reconsider h = H as RealMap of T by TOPMETR:24;
A3: dom h = c by FUNCT_2:def 1
   .= dom f by FUNCT_2:def 1;
   for c being set st c in dom h holds h.c = -f.c by A1;
  then h = -f by A3,SEQ_1:def 7;
  hence thesis by A2,TOPREAL6:83;
end;
end;

definition
  let T, f;
  redefine func abs f -> continuous RealMap of T;
coherence
proof
  set c = the carrier of T;
  reconsider F = f as continuous Function of T,R^1 by TOPMETR:24,TOPREAL6:83;
  consider H being Function of T,R^1 such that
A1: for p being Point of T, r1 being real number st
     F.p = r1 holds H.p = abs r1 and
A2: H is continuous by JGRAPH_4:12;
  reconsider h = H as RealMap of T by TOPMETR:24;
A3: dom h = c by FUNCT_2:def 1
   .= dom f by FUNCT_2:def 1;
   for c being set st c in dom h holds h.c = abs(f.c) by A1;
  then h = abs f by A3,SEQ_1:def 10;
  hence thesis by A2,TOPREAL6:83;
end;
end;

Lm1:
now
  let T;
  let a be Real;
  set c = the carrier of T;
  set f = c --> a;
  thus f is continuous
  proof
A1: dom f = c by FUNCT_2:def 1;
A2: rng f = {a} by FUNCOP_1:14;
  let Y be Subset of REAL; assume Y is closed;
  per cases;
  suppose a in Y;
  then A3: rng f c= Y by A2,ZFMISC_1:37;
   f"Y = f"(rng f /\ Y) by RELAT_1:168
     .= f"rng f by A3,XBOOLE_1:28
     .= c by A1,RELAT_1:169
     .= [#]T by PRE_TOPC:12;
  hence f"Y is closed;
  end;
  suppose not a in Y;
  then A4: rng f misses Y by A2,ZFMISC_1:56;
   f"Y = f"(rng f /\ Y) by RELAT_1:168
     .= f"{} by A4,XBOOLE_0:def 7
     .= {}T by RELAT_1:171;
  hence f"Y is closed;
  end;
end;
end;

registration
  let T;
  cluster positive-yielding continuous RealMap of T;
existence
proof
  take f = (the carrier of T) --> (1 qua Real);
  thus f is positive-yielding;
  thus thesis by Lm1;
end;
  cluster negative-yielding continuous RealMap of T;
existence
proof
  take f = (the carrier of T) --> -1;
  thus f is negative-yielding;
  thus thesis by Lm1;
end;
end;

definition
  let T; let f be nonnegative-yielding continuous RealMap of T;
  redefine func sqrt f -> continuous RealMap of T;
coherence
proof
  set c = the carrier of T;
  reconsider F = f as continuous Function of T,R^1 by TOPMETR:24,TOPREAL6:83;
   for q being Point of T ex r being real number st f.q = r & r >= 0
  proof
    let q be Point of T;
    take f.q;
    thus f.q = f.q;
     dom f = c by FUNCT_2:def 1;
    then f.q in rng f by FUNCT_1:def 5;
    hence f.q >= 0 by Def4;
  end;
  then consider H being Function of T,R^1 such that
A1: for p being Point of T, r1 being real number st
     F.p = r1 holds H.p = sqrt r1 and
A2: H is continuous by JGRAPH_3:15;
  reconsider h = H as RealMap of T by TOPMETR:24;
A3: dom h = c by FUNCT_2:def 1
   .= dom f by FUNCT_2:def 1;
   for c being set st c in dom h holds h.c = sqrt(f.c) by A1;
  then h = sqrt f by A3,Def5;
  hence thesis by A2,TOPREAL6:83;
end;
end;

definition
  let T, f, r;
  redefine func r(#)f -> continuous RealMap of T;
coherence
proof
  set c = the carrier of T;
  reconsider F = f as continuous Function of T,R^1 by TOPMETR:24,TOPREAL6:83;
  consider H being Function of T,R^1 such that
A1: for p being Point of T, r1 being real number st
     F.p = r1 holds H.p = r*r1 and
A2: H is continuous by JGRAPH_2:33;
  reconsider h = H as RealMap of T by TOPMETR:24;
A3: dom h = c by FUNCT_2:def 1
   .= dom f by FUNCT_2:def 1;
   for c being set st c in dom h holds h.c = r * f.c by A1;
  then h = r(#)f by A3,SEQ_1:def 6;
  hence thesis by A2,TOPREAL6:83;
end;
end;

definition
  let T; let f be non-empty continuous RealMap of T;
  redefine func f^ -> continuous RealMap of T;
coherence
proof
  set c = the carrier of T;
  reconsider F = f as continuous Function of T,R^1 by TOPMETR:24,TOPREAL6:83;
   for q being Point of T holds f.q <> 0
  proof
    let q be Point of T;
     dom f = c by FUNCT_2:def 1;
    then f.q in rng f by FUNCT_1:def 5;
    hence f.q <> 0 by RELAT_1:def 9;
  end;
  then consider H being Function of T,R^1 such that
A1: for p being Point of T, r1 being real number st
     F.p = r1 holds H.p = 1/r1 and
A2: H is continuous by JGRAPH_2:36;
  reconsider h = H as RealMap of T by TOPMETR:24;
A3: dom h = c by FUNCT_2:def 1
   .= dom f \ f"{0} by FUNCT_2:def 1;
   now
    let a be set;
    assume a in dom h;
    hence h.a = 1/f.a by A1
    .= 1 * (f.a)" by XCMPLX_0:def 9
    .= (f.a)";
  end;
  then h = f^ by A3,RFUNCT_1:def 8;
  hence thesis by A2,TOPREAL6:83;
end;
end;

definition
  let T, f; let g be non-empty continuous RealMap of T;
  redefine func f/g -> continuous RealMap of T;
coherence
proof
  set c = the carrier of T;
  reconsider F = f, G = g as continuous Function
 of T,R^1 by TOPMETR:24,TOPREAL6:83;
   for q being Point of T holds g.q <> 0
  proof
    let q be Point of T;
     dom g = c by FUNCT_2:def 1;
    then g.q in rng g by FUNCT_1:def 5;
    hence g.q <> 0 by RELAT_1:def 9;
  end;
  then consider H being Function of T,R^1 such that
A1: for p being Point of T, r1,r2 being real number st
     F.p = r1 & G.p = r2 holds H.p = r1/r2 and
A2: H is continuous by JGRAPH_2:37;
  reconsider h = H as RealMap of T by TOPMETR:24;
A3: dom h = c /\ c by FUNCT_2:def 1
   .= c /\ dom g by FUNCT_2:def 1
   .= dom f /\ (dom g \ g"{0}) by FUNCT_2:def 1;
   now
    let c be set;
    assume c in dom h;
    hence h.c = f.c / g.c by A1
    .= f.c * (g.c)" by XCMPLX_0:def 9;
  end;
  then h = f/g by A3,RFUNCT_1:def 4;
  hence thesis by A2,TOPREAL6:83;
end;
end;
