  assume r in rng R;
  then consider x being set such that
a2: x in dom R and
a3: R.x = r by FUNCT_1:def 5;
  dom R = dom f /\ (dom g \ g"{0}) by RFUNCT_1:def 4;
  then x in dom f & x in dom g \ g"{0} by a2,XBOOLE_0:def 3;
  then x in dom f & x in dom g by XBOOLE_0:def 4;
  then f.x in rng f & g.x in rng g by FUNCT_1:def 5;
  then reconsider a = f.x, b = g.x as non positive (real number) by dnonpy;
  a*b" is non negative;
  hence thesis by a3,a2,RFUNCT_1:def 4;
end;
end;

