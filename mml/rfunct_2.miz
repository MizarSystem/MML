  let r1,r2;
  assume that
A9: r1 in dom(h|Y) & r2 in dom(h|Y) and
A10: r1<r2;
A11: (h|Y).r1 = h.r1 & (h|Y).r2 = h.r2 by A9,FUNCT_1:70;
  r1 in Y /\ dom h & r2 in Y /\ dom h by A9,RELAT_1:90;
  hence thesis by A8,A10,A11;
end;

