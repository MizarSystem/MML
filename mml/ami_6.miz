    thus InsCode SCM-goto i1 is jump-only;
    JUMP SCM-goto i1 <> {};
    hence SCM-goto i1 is non sequential by AMISTD_1:13;
    dom JumpPart SCM-goto i1 = dom <*i1*> by RECDEF_2:def 2
      .= {1} by FINSEQ_1:2,def 8;
   hence JumpPart SCM-goto i1 is not empty;
  end;
