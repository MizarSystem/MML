theorem
 T = 0 or T = 1 or T = 2 or T = 3 or T = 4 or T = 5 or T = 6 or T = 7 or T
= 8
proof
  consider y being set such that
A1: [T,y] in proj1 the InstructionsF of SCM by XTUPLE_0:def 12;
  consider x being set such that
A2: [[T,y],x] in the InstructionsF of SCM by A1,XTUPLE_0:def 12;
   reconsider I = [T,y,x] as Instruction of SCM by A2;
   T = InsCode I by RECDEF_2:def 1;
  hence thesis by AMI_5:5,NAT_1:32;
end;

theorem Th2:
  JumpPart halt SCM = {};

theorem
  T = 0 implies JumpParts T = {0}
proof
  assume
A1: T = 0;
  hereby
    let a be set;
    assume a in JumpParts T;
    then consider I such that
A2: a = JumpPart I and
A3: InsCode I = T;
    I = halt SCM by A1,A3,AMI_5:7;
    hence a in {0} by A2,Th2,TARSKI:def 1;
  end;
  let a be set;
  assume a in {0};
  then
A: a = 0 by TARSKI:def 1;
   InsCode halt SCM = 0 by COMPOS_1:70;
  hence thesis by A1,Th2,A;
end;

theorem
  T = 1 implies JumpParts T = {{}}
proof
  assume
A1: T = 1;
  hereby
    let x be set;
    assume x in JumpParts T;
     then consider I being Instruction of SCM such that
A2:   x = JumpPart I and
A3:   InsCode I = T;
     consider a,b such that
A4:   I = a:=b by A1,A3,AMI_5:8;
     x = {} by A2,A4,RECDEF_2:def 2;
    hence x in {{}} by TARSKI:def 1;
  end;
  set a = the Data-Location;
  let x be set;
  assume x in {{}};
   then x = {} by TARSKI:def 1;
   then
A5:  x = JumpPart(a:= a) by RECDEF_2:def 2;
    InsCode(a:= a) = 1 by RECDEF_2:def 1;
  hence thesis by A5,A1;
end;

