    hence a >0_goto i1 is non sequential by AMISTD_1:13;
    dom JumpPart (a >0_goto i1) = dom <*i1*> by RECDEF_2:def 2
      .= {1} by FINSEQ_1:2,38;
   hence JumpPart(a >0_goto i1) is not empty;
