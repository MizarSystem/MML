          then Index(p,do)+1 <= len LS-'Index(Gij,LS) by NAT_1:13;
          then Index(p,do) <= len LS-'Index(Gij,LS)-1 by XREAL_1:21;
          then Index(p,do) <= len LS-Index(Gij,LS)-1 by A209,BINARITH:def 3;
          then Index(p,do) <= len LS-Gij..LS by A203;
          then Index(p,do) <= len LS-'Gij..LS by BINARITH:def 3;
          then Index(p,do) < len LS-'(Gij..LS)+1 by NAT_1:13;
          then
A210:     LSeg(mid(LS,Gij..LS,len LS),Index(p,do)) =
          LSeg(LS,Index(p,do)+(Gij..LS)-'1) by A200,A201,A202,JORDAN4:31;
A211:     1+1 <= Gij..LS by A203,A208,XREAL_1:9;
          then Index(p,do)+Gij..LS >= 1+1+1 by A202,XREAL_1:9;
          then Index(p,do)+Gij..LS-1 >= 1+1+1-1 by XREAL_1:11;
          then
A212:     tt >= 1+1 by BINARITH:def 3;
A213:     2 in dom LS by A170,FINSEQ_3:27;
          now per cases by A212,REAL_1:def 5;
            suppose tt > 1+1;
              then LSeg(LS,1) misses LSeg(LS,tt) by TOPREAL1:def 9;
              hence contradiction by A197,A198,A199,A210,XBOOLE_0:3;
            end;
            suppose
A214:         tt = 1+1;
              then LSeg(LS,1) /\ LSeg(LS,tt) = {LS/.2} by A23,TOPREAL1:def 8;
              then p in {LS/.2} by A197,A198,A199,A210,XBOOLE_0:def 3;
              then
A215:         p = LS/.2 by TARSKI:def 1;
              then
A216:         p..LS = 2 by A213,FINSEQ_5:44;
              1+1 = Index(p,do)+(Gij..LS)-1 by A214,BINARITH:def 3;
              then 1+1+1 = Index(p,do)+(Gij..LS);
              then
A217:         Gij..LS = 2 by A202,A211,JORDAN1E:10;
              p in rng LS by A213,A215,PARTFUN2:4;
              then p = Gij by A36,A216,A217,FINSEQ_5:10;
              then Gij`1 = Ebo by A215,JORDAN1G:40;
              then Gij`1 = G*(len G,j)`1 by A10,A15,JORDAN1A:92;
              hence contradiction by A1,A12,A63,JORDAN1G:7;
            end;
          end;
          hence contradiction;
        end;
      end;
      then east_halfline E-max C c= (L~godo)` by SUBSET_1:43;
      then consider W be Subset of TOP-REAL 2 such that
A218: W is_a_component_of (L~godo)` and
A219: east_halfline E-max C c= W by GOBOARD9:5;
      W is not Bounded by A219,JORDAN2C:16,129;
      then W is_outside_component_of L~godo by A218,JORDAN2C:def 4;
      then W c= UBD L~godo by JORDAN2C:27;
      then
A220: east_halfline E-max C c= UBD L~godo by A219,XBOOLE_1:1;
      E-max C in east_halfline E-max C by TOPREAL1:45;
      then E-max C in UBD L~godo by A220;
      then E-max C in LeftComp godo by GOBRD14:46;
      then UA meets L~godo by A125,A126,A134,A141,JORDAN1J:36;
      then
A221: UA meets (L~go \/ L~pion1) or UA meets L~do by A135,XBOOLE_1:70;
A222: UA c= C by JORDAN6:76;
      now per cases by A221,XBOOLE_1:70;
        suppose UA meets L~go;
          then UA meets L~Cage(C,n) by A45,A136,XBOOLE_1:1,63;
          hence contradiction by A222,JORDAN10:5,XBOOLE_1:63;
        end;
        suppose UA meets L~pion1;
          hence contradiction by A5,A7,A79;
        end;
        suppose UA meets L~do;
          then UA meets L~Cage(C,n) by A52,A136,XBOOLE_1:1,63;
          hence contradiction by A222,JORDAN10:5,XBOOLE_1:63;
        end;
      end;
      hence contradiction;
    end;
    suppose Gij`1 = Gik`1;
      then
A223: i1 = i2 by A12,A13,JORDAN1G:7;
      then poz = {Gi1k} by TOPREAL1:7;
      then poz c= pio by A75,ZFMISC_1:37;
      then pio \/ poz = pio by XBOOLE_1:12;
      hence contradiction by A1,A2,A3,A4,A5,A223,JORDAN1J:59;
    end;
    suppose Gij`2 = Gik`2;
      then
A224: j = k by A12,A13,JORDAN1G:6;
      then pio = {Gi1k} by TOPREAL1:7;
      then pio c= poz by A76,ZFMISC_1:37;
      then pio \/ poz = poz by XBOOLE_1:12;
      hence contradiction by A1,A2,A3,A4,A5,A224,Th31;
    end;
  end;
  hence contradiction;
end;

theorem Th47:
  for C be Simple_closed_curve for i1,i2,j,k be Element of NAT st
  1 < i1 & i1 <= i2 & i2 < len Gauge(C,n) &
  1 <= j & j <= k & k <= width Gauge(C,n) &
  (LSeg(Gauge(C,n)*(i1,j),Gauge(C,n)*(i1,k)) \/
  LSeg(Gauge(C,n)*(i1,k),Gauge(C,n)*(i2,k))) /\ L~Upper_Seq(C,n) =
  {Gauge(C,n)*(i2,k)} & (LSeg(Gauge(C,n)*(i1,j),Gauge(C,n)*(i1,k)) \/
  LSeg(Gauge(C,n)*(i1,k),Gauge(C,n)*(i2,k))) /\ L~Lower_Seq(C,n) =
  {Gauge(C,n)*(i1,j)} holds (LSeg(Gauge(C,n)*(i1,j),Gauge(C,n)*(i1,k)) \/
  LSeg(Gauge(C,n)*(i1,k),Gauge(C,n)*(i2,k))) meets Lower_Arc C
proof
  let C be Simple_closed_curve;
  let i1,i2,j,k be Element of NAT;
  set G = Gauge(C,n);
  set pio = LSeg(G*(i1,j),G*(i1,k));
  set poz = LSeg(G*(i1,k),G*(i2,k));
  set US = Upper_Seq(C,n);
  set LS = Lower_Seq(C,n);
  assume that
A1: 1 < i1 & i1 <= i2 & i2 < len G and
A2: 1 <= j & j <= k & k <= width G and
A3: (pio \/ poz) /\ L~US = {G*(i2,k)} and
A4: (pio \/ poz) /\ L~LS = {G*(i1,j)} and
A5: (pio \/ poz) misses Lower_Arc C;
  set LA = Lower_Arc C;
  set Wmin = W-min L~Cage(C,n);
  set Emax = E-max L~Cage(C,n);
  set Wbo = W-bound L~Cage(C,n);
  set Ebo = E-bound L~Cage(C,n);
  set Gij = G*(i1,j);
  set Gik = G*(i2,k);
  set Gi1k = G*(i1,k);
A6: 1 < i1 & i1 < len G & 1 < i2 & i2 < len G by A1,XXREAL_0:2;
A7: L~<*Gik,Gi1k,Gij*> = poz \/ pio by TOPREAL3:23;
  Gij in {Gij} by TARSKI:def 1;
  then
A8: Gij in L~LS by A4,XBOOLE_0:def 3;
  Gik in {Gik} by TARSKI:def 1;
  then
A9: Gik in L~US by A3,XBOOLE_0:def 3;
A10: 1 <= j & j <= width G by A2,XXREAL_0:2;
A11: 1 <= k & k <= width G by A2,XXREAL_0:2;
A12: [i1,j] in Indices G by A6,A10,MATRIX_1:37;
A13: [i2,k] in Indices G by A6,A11,MATRIX_1:37;
A14: [i1,k] in Indices G by A6,A11,MATRIX_1:37;
  set go = R_Cut(US,Gik);
  set do = L_Cut(LS,Gij);
A15: len G = width G by JORDAN8:def 1;
A16: len US >= 3 by JORDAN1E:19;
  then len US >= 1 by XXREAL_0:2;
  then 1 in dom US by FINSEQ_3:27;
  then
A17: US.1 = US/.1 by PARTFUN1:def 8
    .= Wmin by JORDAN1F:5;
A18: Wmin`1 = Wbo by EUCLID:56
    .= G*(1,k)`1 by A11,A15,JORDAN1A:94;
  len G >= 4 by JORDAN8:13;
  then
A19: len G >= 1 by XXREAL_0:2;
  then
A20: [1,k] in Indices G by A11,MATRIX_1:37;
  then
A21: Gik <> US.1 by A1,A13,A17,A18,JORDAN1G:7;
  then reconsider go as being_S-Seq FinSequence of TOP-REAL 2
  by A9,JORDAN3:70;
A22: [1,j] in Indices G by A10,A19,MATRIX_1:37;
A23: len LS >= 1+2 by JORDAN1E:19;
  then len LS >= 1 by XXREAL_0:2;
  then
A24: 1 in dom LS & len LS in dom LS by FINSEQ_3:27;
  then
A25: LS.len LS = LS/.len LS by PARTFUN1:def 8
    .= Wmin by JORDAN1F:8;
A26: Wmin`1 = Wbo by EUCLID:56
    .= G*(1,k)`1 by A11,A15,JORDAN1A:94;
A27: [i1,j] in Indices G by A6,A10,MATRIX_1:37;
  then
A28: Gij <> LS.len LS by A1,A20,A25,A26,JORDAN1G:7;
  then reconsider do as being_S-Seq FinSequence of TOP-REAL 2
  by A8,JORDAN3:69;
A29: [len G,k] in Indices G by A11,A19,MATRIX_1:37;
A30: LS.1 = LS/.1 by A24,PARTFUN1:def 8
    .= Emax by JORDAN1F:6;
  Emax`1 = Ebo by EUCLID:56
    .= G*(len G,k)`1 by A11,A15,JORDAN1A:92;
  then
A31: Gij <> LS.1 by A1,A27,A29,A30,JORDAN1G:7;
A32: len go >= 1+1 by TOPREAL1:def 10;
A33: Gik in rng US by A6,A9,A11,JORDAN1G:4,JORDAN1J:40;
  then
A34: go is_sequence_on G by JORDAN1G:4,JORDAN1J:38;
A35: len do >= 1+1 by TOPREAL1:def 10;
A36: Gij in rng LS by A6,A8,A10,JORDAN1G:5,JORDAN1J:40;
  then
A37: do is_sequence_on G by JORDAN1G:5,JORDAN1J:39;
  reconsider go as non constant s.c.c.
  (being_S-Seq FinSequence of TOP-REAL 2) by A32,A34,JGRAPH_1:16,JORDAN8:8;
  reconsider do as non constant s.c.c.
  (being_S-Seq FinSequence of TOP-REAL 2) by A35,A37,JGRAPH_1:16,JORDAN8:8;
A38: len go > 1 by A32,NAT_1:13;
  then
A39: len go in dom go by FINSEQ_3:27;
  then
A40: go/.len go = go.len go by PARTFUN1:def 8
    .= Gik by A9,JORDAN3:59;
  len do >= 1 by A35,XXREAL_0:2;
  then 1 in dom do by FINSEQ_3:27;
  then
A41: do/.1 = do.1 by PARTFUN1:def 8
    .= Gij by A8,JORDAN3:58;
  reconsider m = len go - 1 as Element of NAT by A39,FINSEQ_3:28;
A42: m+1 = len go;
  then
A43: len go-'1 = m by BINARITH:39;
A44: LSeg(go,m) c= L~go by TOPREAL3:26;
A45: L~go c= L~US by A9,JORDAN3:76;
  then LSeg(go,m) c= L~US by A44,XBOOLE_1:1;
  then
A46: LSeg(go,m) /\ L~<*Gik,Gi1k,Gij*> c= {Gik} by A3,A7,XBOOLE_1:26;
  m >= 1 by A32,XREAL_1:21;
  then
A47: LSeg(go,m) = LSeg(go/.m,Gik) by A40,A42,TOPREAL1:def 5;
  {Gik} c= LSeg(go,m) /\ L~<*Gik,Gi1k,Gij*>
  proof
    let x be set;
    assume x in {Gik};
    then
A48: x = Gik by TARSKI:def 1;
A49: Gik in LSeg(go,m) by A47,TOPREAL1:6;
    Gik in LSeg(Gik,Gi1k) by TOPREAL1:6;
    then Gik in LSeg(Gik,Gi1k) \/ LSeg(Gi1k,Gij) by XBOOLE_0:def 2;
    then Gik in L~<*Gik,Gi1k,Gij*> by SPRECT_1:10;
    hence x in LSeg(go,m) /\ L~<*Gik,Gi1k,Gij*> by A48,A49,XBOOLE_0:def 3;
  end;
  then
A50: LSeg(go,m) /\ L~<*Gik,Gi1k,Gij*> = {Gik} by A46,XBOOLE_0:def 10;
A51: LSeg(do,1) c= L~do by TOPREAL3:26;
A52: L~do c= L~LS by A8,JORDAN3:77;
  then LSeg(do,1) c= L~LS by A51,XBOOLE_1:1;
  then
A53: LSeg(do,1) /\ L~<*Gik,Gi1k,Gij*> c= {Gij} by A4,A7,XBOOLE_1:26;
A54: LSeg(do,1) = LSeg(Gij,do/.(1+1)) by A35,A41,TOPREAL1:def 5;
  {Gij} c= LSeg(do,1) /\ L~<*Gik,Gi1k,Gij*>
  proof
    let x be set;
    assume x in {Gij};
    then
A55: x = Gij by TARSKI:def 1;
A56: Gij in LSeg(do,1) by A54,TOPREAL1:6;
    Gij in LSeg(Gi1k,Gij) by TOPREAL1:6;
    then Gij in LSeg(Gik,Gi1k) \/ LSeg(Gi1k,Gij) by XBOOLE_0:def 2;
    then Gij in L~<*Gik,Gi1k,Gij*> by SPRECT_1:10;
    hence x in LSeg(do,1) /\ L~<*Gik,Gi1k,Gij*> by A55,A56,XBOOLE_0:def 3;
  end;
  then
A57: L~<*Gik,Gi1k,Gij*> /\ LSeg(do,1) = {Gij} by A53,XBOOLE_0:def 10;
A58: go/.1 = US/.1 by A9,SPRECT_3:39
    .= Wmin by JORDAN1F:5;
  then
A59: go/.1 = LS/.len LS by JORDAN1F:8
    .= do/.len do by A8,JORDAN1J:35;
A60: rng go c= L~go & rng do c= L~do by A32,A35,SPPOL_2:18;
A61: {go/.1} c= L~go /\ L~do
  proof
    let x be set;
    assume x in {go/.1};
    then x = go/.1 by TARSKI:def 1;
    then x in rng go & x in rng do by A59,FINSEQ_6:46,REVROT_1:3;
    hence x in L~go /\ L~do by A60,XBOOLE_0:def 3;
  end;
A62: LS.1 = LS/.1 by A24,PARTFUN1:def 8
    .= Emax by JORDAN1F:6;
A63: [len G,j] in Indices G by A10,A19,MATRIX_1:37;
  L~go /\ L~do c= {go/.1}
  proof
    let x be set;
    assume x in L~go /\ L~do;
    then
A64: x in L~go & x in L~do by XBOOLE_0:def 3;
    then x in L~US /\ L~LS by A45,A52,XBOOLE_0:def 3;
    then x in {Wmin,Emax} by JORDAN1E:20;
    then
A65: x = Wmin or x = Emax by TARSKI:def 2;
    now
      assume x = Emax;
      then
A66:  Emax = Gij by A8,A62,A64,JORDAN1E:11;
      G*(len G,j)`1 = Ebo by A10,A15,JORDAN1A:92;
      then Emax`1 <> Ebo by A1,A12,A63,A66,JORDAN1G:7;
      hence contradiction by EUCLID:56;
    end;
    hence x in {go/.1} by A58,A65,TARSKI:def 1;
  end;
  then
A67: L~go /\ L~do = {go/.1} by A61,XBOOLE_0:def 10;
  set W2 = go/.2;
A68: 2 in dom go by A32,FINSEQ_3:27;
A69: Gik..US >= 1 by A33,FINSEQ_4:31;
A70: now
    assume Gij`1 = Wbo;
    then G*(1,j)`1 = G*(i1,j)`1 by A10,A15,JORDAN1A:94;
    hence contradiction by A1,A12,A22,JORDAN1G:7;
  end;
  go = mid(US,1,Gik..US) by A33,JORDAN1G:57
    .= US|(Gik..US) by A69,JORDAN3:25;
  then
A71: W2 = US/.2 by A68,FINSEQ_4:85;
A72: Wmin in rng go by A58,FINSEQ_6:46;
  set pion = <*Gik,Gi1k,Gij*>;
A73: now
    let n be Element of NAT;
    assume n in dom pion;
    then n in {1,2,3} by FINSEQ_3:1,30;
    then n = 1 or n = 2 or n = 3 by ENUMSET1:def 1;
    then pion/.n = Gik or pion/.n = Gi1k or pion/.n = Gij by FINSEQ_4:27;
    hence ex i,j be Element of NAT st [i,j] in Indices G & pion/.n = G*(i,j)
    by A12,A13,A14;
  end;
A74: Gi1k`1 = G*(i1,1)`1 by A6,A11,GOBOARD5:3
    .= Gij`1 by A6,A10,GOBOARD5:3;
  Gi1k`2 = G*(1,k)`2 by A6,A11,GOBOARD5:2
    .= Gik`2 by A6,A11,GOBOARD5:2;
  then
A75: Gi1k = |[Gij`1,Gik`2]| by A74,EUCLID:57;
A76: Gi1k in pio by TOPREAL1:6;
A77: Gi1k in poz by TOPREAL1:6;
  now per cases;
    suppose Gij`1 <> Gik`1 & Gij`2 <> Gik`2;
      then pion is_S-Seq by A75,TOPREAL3:42;
      then consider pion1 be FinSequence of TOP-REAL 2 such that
A78:  pion1 is_sequence_on G and
A79:  pion1 is_S-Seq and
A80:  L~pion = L~pion1 and
A81:  pion/.1 = pion1/.1 and
A82:  pion/.len pion = pion1/.len pion1 and
A83:  len pion <= len pion1 by A73,GOBOARD3:2;
      reconsider pion1 as being_S-Seq FinSequence of TOP-REAL 2 by A79;
      set godo = go^'pion1^'do;
A84:  Gi1k`1 = G*(i1,1)`1 by A6,A11,GOBOARD5:3
        .= Gij`1 by A6,A10,GOBOARD5:3;
A85:  Gi1k`1 <= Gik`1 by A1,A11,JORDAN1A:39;
      then
A86:  W-bound poz = Gi1k`1 by SPRECT_1:62;
A87:  W-bound pio = Gij`1 by A84,SPRECT_1:62;
      W-bound (poz \/ pio) = min(W-bound poz, W-bound pio) by SPRECT_1:54
        .= Gij`1 by A84,A86,A87;
      then
A88:  W-bound L~pion1 = Gij`1 by A80,TOPREAL3:23;
A89:  1+1 <= len Cage(C,n) by GOBOARD7:36,XXREAL_0:2;
      then
A90:  1+1 <= len Rotate(Cage(C,n),Wmin) by REVROT_1:14;
      len (go^'pion1) >= len go by TOPREAL8:7;
      then
A91:  len (go^'pion1) >= 1+1 by A32,XXREAL_0:2;
      then
A92:  len (go^'pion1) > 1+0 by NAT_1:13;
A93:  len godo >= len (go^'pion1) by TOPREAL8:7;
      then
A94:  1+1 <= len godo by A91,XXREAL_0:2;
A95:  US is_sequence_on G by JORDAN1G:4;
A96:  go/.len go = pion1/.1 by A40,A81,FINSEQ_4:27;
      then
A97:  go^'pion1 is_sequence_on G by A34,A78,TOPREAL8:12;
A98:  (go^'pion1)/.len (go^'pion1) = pion/.len pion by A82,GRAPH_2:58
        .= pion/.3 by FINSEQ_1:62
        .= do/.1 by A41,FINSEQ_4:27;
      then
A99:  godo is_sequence_on G by A37,A97,TOPREAL8:12;
      LSeg(pion1,1) c= L~pion by A80,TOPREAL3:26;
      then
A100: LSeg(go,len go-'1) /\ LSeg(pion1,1) c={Gik} by A43,A50,XBOOLE_1:27;
      len pion1 >= 2+1 by A83,FINSEQ_1:62;
      then
A101: len pion1 > 1+1 by NAT_1:13;
      {Gik} c= LSeg(go,m) /\ LSeg(pion1,1)
      proof
        let x be set;
        assume x in {Gik};
        then
A102:   x = Gik by TARSKI:def 1;
A103:   Gik in LSeg(go,m) by A47,TOPREAL1:6;
        Gik in LSeg(pion1,1) by A40,A96,A101,TOPREAL1:27;
        hence x in LSeg(go,m) /\ LSeg(pion1,1) by A102,A103,XBOOLE_0:def 3;
      end;
      then LSeg(go,len go-'1) /\ LSeg(pion1,1) = { go/.len go }
      by A40,A43,A100,XBOOLE_0:def 10;
      then
A104: go^'pion1 is unfolded by A96,TOPREAL8:34;
      len pion1 >= 2+1 by A83,FINSEQ_1:62;
      then
A105: len pion1-2 >= 0 by XREAL_1:21;
      len (go^'pion1)+1-1 = len go+len pion1-1 by GRAPH_2:13;
      then len (go^'pion1)-1 = len go + (len pion1-2)
        .= len go + (len pion1-'2) by A105,BINARITH:def 3;
      then
A106: len (go^'pion1)-'1 = len go + (len pion1-'2) by BINARITH:def 3;
A107: len pion1-1 >= 1 by A101,XREAL_1:21;
      then
A108: len pion1-'1 = len pion1-1 by BINARITH:def 3;
A109: len pion1-'2+1 = len pion1-2+1 by A105,BINARITH:def 3
        .= len pion1-'1 by A107,BINARITH:def 3;
      len pion1-1+1 <= len pion1;
      then
A110: len pion1-'1 < len pion1 by A108,NAT_1:13;
      LSeg(pion1,len pion1-'1) c= L~pion by A80,TOPREAL3:26;
      then
A111: LSeg(pion1,len pion1-'1) /\ LSeg(do,1) c= {Gij} by A57,XBOOLE_1:27;
      {Gij} c= LSeg(pion1,len pion1-'1) /\ LSeg(do,1)
      proof
        let x be set;
        assume x in {Gij};
        then
A112:   x = Gij by TARSKI:def 1;
A113:   Gij in LSeg(do,1) by A54,TOPREAL1:6;
        pion1/.(len pion1-'1+1) = pion/.3 by A82,A108,FINSEQ_1:62
          .= Gij by FINSEQ_4:27;
        then Gij in LSeg(pion1,len pion1-'1) by A107,A108,TOPREAL1:27;
        hence x in LSeg(pion1,len pion1-'1) /\ LSeg(do,1)
        by A112,A113,XBOOLE_0:def 3;
      end;
      then LSeg(pion1,len pion1-'1) /\ LSeg(do,1) = {Gij}
      by A111,XBOOLE_0:def 10;
      then
A114: LSeg(go^'pion1,len go+(len pion1-'2)) /\ LSeg(do,1) =
      {(go^'pion1)/.len (go^'pion1)} by A41,A96,A98,A109,A110,TOPREAL8:31;
A115: (go^'pion1) is non trivial by A91,REALSET1:13;
A116: rng pion1 c= L~pion1 by A101,SPPOL_2:18;
A117: {pion1/.1} c= L~go /\ L~pion1
      proof
        let x be set;
        assume x in {pion1/.1};
        then x = pion1/.1 by TARSKI:def 1;
        then x in rng go & x in rng pion1 by A96,FINSEQ_6:46,REVROT_1:3;
        hence x in L~go /\ L~pion1 by A60,A116,XBOOLE_0:def 3;
      end;
      L~go /\ L~pion1 c= {pion1/.1}
      proof
        let x be set;
        assume x in L~go /\ L~pion1;
        then x in L~go & x in L~pion1 by XBOOLE_0:def 3;
        hence x in {pion1/.1} by A3,A7,A40,A45,A80,A96,XBOOLE_0:def 3;
      end;
      then
A118: L~go /\ L~pion1 = {pion1/.1} by A117,XBOOLE_0:def 10;
      then
A119: (go^'pion1) is s.n.c. by A96,JORDAN1J:54;
      rng go /\ rng pion1 c= {pion1/.1} by A60,A116,A118,XBOOLE_1:27;
      then
A120: go^'pion1 is one-to-one by JORDAN1J:55;
A121: pion/.len pion = pion/.3 by FINSEQ_1:62
        .= do/.1 by A41,FINSEQ_4:27;
A122: {pion1/.len pion1} c= L~do /\ L~pion1
      proof
        let x be set;
        assume x in {pion1/.len pion1};
        then x = pion1/.len pion1 by TARSKI:def 1;
        then x in rng do & x in rng pion1 by A82,A121,FINSEQ_6:46,REVROT_1:3;
        hence x in L~do /\ L~pion1 by A60,A116,XBOOLE_0:def 3;
      end;
      L~do /\ L~pion1 c= {pion1/.len pion1}
      proof
        let x be set;
        assume x in L~do /\ L~pion1;
        then x in L~do & x in L~pion1 by XBOOLE_0:def 3;
        hence x in {pion1/.len pion1}
        by A4,A7,A41,A52,A80,A82,A121,XBOOLE_0:def 3;
      end;
      then
A123: L~do /\ L~pion1 = {pion1/.len pion1} by A122,XBOOLE_0:def 10;
A124: L~(go^'pion1) /\ L~do = (L~go \/ L~pion1) /\ L~do by A96,TOPREAL8:35
        .= {go/.1} \/ {do/.1} by A67,A82,A121,A123,XBOOLE_1:23
        .= {(go^'pion1)/.1} \/ {do/.1} by GRAPH_2:57
        .= {(go^'pion1)/.1,do/.1} by ENUMSET1:41;
      do/.len do = (go^'pion1)/.1 by A59,GRAPH_2:57;
      then reconsider godo as non constant standard special_circular_sequence
      by A94,A98,A99,A104,A106,A114,A115,A119,A120,A124,JORDAN8:7,8,TOPREAL8:11
      ,33,34;
A125: LA is_an_arc_of E-max C,W-min C by JORDAN6:def 9;
      then
A126: LA is connected by JORDAN6:11;
A127: W-min C in LA & E-max C in LA by A125,TOPREAL1:4;
      set ff = Rotate(Cage(C,n),Wmin);
      Wmin in rng Cage(C,n) by SPRECT_2:47;
      then
A128: ff/.1 = Wmin by FINSEQ_6:98;
A129: L~ff = L~Cage(C,n) by REVROT_1:33;
      then (W-max L~ff)..ff > 1 by A128,SPRECT_5:23;
      then (N-min L~ff)..ff > 1 by A128,A129,SPRECT_5:24,XXREAL_0:2;
      then (N-max L~ff)..ff > 1 by A128,A129,SPRECT_5:25,XXREAL_0:2;
      then
A130: Emax..ff > 1 by A128,A129,SPRECT_5:26,XXREAL_0:2;
A131: now
        assume
A132:   Gik..US <= 1;
        Gik..US >= 1 by A33,FINSEQ_4:31;
        then Gik..US = 1 by A132,XXREAL_0:1;
        then Gik = US/.1 by A33,FINSEQ_5:41;
        hence contradiction by A17,A21,JORDAN1F:5;
      end;
A133: Cage(C,n) is_sequence_on G by JORDAN9:def 1;
      then
A134: ff is_sequence_on G by REVROT_1:34;
A135: right_cell(godo,1,G)\L~godo c= RightComp godo by A94,A99,JORDAN9:29;
A136: L~godo = L~(go^'pion1) \/ L~do by A98,TOPREAL8:35
        .= L~go \/ L~pion1 \/ L~do by A96,TOPREAL8:35;
      L~Cage(C,n) = L~US \/ L~LS by JORDAN1E:17;
      then
A137: L~US c= L~Cage(C,n) & L~LS c= L~Cage(C,n) by XBOOLE_1:7;
      then
A138: L~go c=L~Cage(C,n) & L~do c=L~Cage(C,n) by A45,A52,XBOOLE_1:1;
A139: W-min C in C by SPRECT_1:15;
A140: now
        assume W-min C in L~godo;
        then
A141:   W-min C in L~go \/ L~pion1 or W-min C in L~do by A136,XBOOLE_0:def 2;
        per cases by A141,XBOOLE_0:def 2;
        suppose W-min C in L~go;
          then C meets L~Cage(C,n) by A138,A139,XBOOLE_0:3;
          hence contradiction by JORDAN10:5;
        end;
        suppose W-min C in L~pion1;
          hence contradiction by A5,A7,A80,A127,XBOOLE_0:3;
        end;
        suppose W-min C in L~do;
          then C meets L~Cage(C,n) by A138,A139,XBOOLE_0:3;
          hence contradiction by JORDAN10:5;
        end;
      end;
      right_cell(Rotate(Cage(C,n),Wmin),1) =
      right_cell(ff,1,GoB ff) by A90,JORDAN1H:29
        .= right_cell(ff,1,GoB Cage(C,n)) by REVROT_1:28
        .= right_cell(ff,1,G) by JORDAN1H:52
        .= right_cell(ff-:Emax,1,G) by A130,A134,JORDAN1J:53
        .= right_cell(US,1,G) by JORDAN1E:def 1
        .= right_cell(R_Cut(US,Gik),1,G) by A33,A95,A131,JORDAN1J:52
        .= right_cell(go^'pion1,1,G) by A38,A97,JORDAN1J:51
        .= right_cell(godo,1,G) by A92,A99,JORDAN1J:51;
      then W-min C in right_cell(godo,1,G) by JORDAN1I:8;
      then
A142: W-min C in right_cell(godo,1,G)\L~godo by A140,XBOOLE_0:def 4;
A143: godo/.1 = (go^'pion1)/.1 by GRAPH_2:57
        .= Wmin by A58,GRAPH_2:57;
A144: len US >= 2 by A16,XXREAL_0:2;
A145: godo/.2 = (go^'pion1)/.2 by A91,GRAPH_2:61
        .= US/.2 by A32,A71,GRAPH_2:61
        .= (US^'LS)/.2 by A144,GRAPH_2:61
        .= Rotate(Cage(C,n),Wmin)/.2 by JORDAN1E:15;
A146: L~go \/ L~do is compact by COMPTS_1:19;
      Wmin in L~go \/ L~do by A60,A72,XBOOLE_0:def 2;
      then
A147: W-min (L~go \/ L~do) = Wmin by A138,A146,JORDAN1J:21,XBOOLE_1:8;
A148: (W-min (L~go \/ L~do))`1 = W-bound (L~go \/ L~do) & Wmin`1 = Wbo
      by EUCLID:56;
      Gij`1 >= Wbo by A8,A137,PSCOMP_1:71;
      then Gij`1 > Wbo by A70,REAL_1:def 5;
      then W-min (L~go\/L~do\/L~pion1) = W-min (L~go \/ L~do)
      by A88,A146,A147,A148,JORDAN1J:33;
      then
A149: W-min L~godo = Wmin by A136,A147,XBOOLE_1:4;
A150: rng godo c= L~godo by A91,A93,SPPOL_2:18,XXREAL_0:2;
      2 in dom godo by A94,FINSEQ_3:27;
      then
A151: godo/.2 in rng godo by PARTFUN2:4;
      godo/.2 in W-most L~Cage(C,n) by A145,JORDAN1I:27;
      then (godo/.2)`1 = (W-min L~godo)`1 by A149,PSCOMP_1:88
        .= W-bound L~godo by EUCLID:56;
      then godo/.2 in W-most L~godo by A150,A151,SPRECT_2:16;
      then Rotate(godo,W-min L~godo)/.2 in W-most L~godo
      by A143,A149,FINSEQ_6:95;
      then reconsider godo as clockwise_oriented non constant standard
      special_circular_sequence by JORDAN1I:27;
      len US in dom US by FINSEQ_5:6;
      then
A152: US.len US = US/.len US by PARTFUN1:def 8
        .= Emax by JORDAN1F:7;
A153: E-max C in E-most C by PSCOMP_1:111;
A154: east_halfline E-max C misses L~go
      proof
        assume east_halfline E-max C meets L~go;
        then consider p be set such that
A155:   p in east_halfline E-max C and
A156:   p in L~go by XBOOLE_0:3;
        reconsider p as Point of TOP-REAL 2 by A155;
        p in L~US by A45,A156;
        then p in east_halfline E-max C /\ L~Cage(C,n)
        by A137,A155,XBOOLE_0:def 3;
        then
A157:   p`1 = Ebo by A153,JORDAN1A:104;
        then
A158:   p = Emax by A45,A156,JORDAN1J:46;
        then Emax = Gik by A9,A152,A156,JORDAN1J:43;
        then Gik`1 = G*(len G,k)`1 by A11,A15,A157,A158,JORDAN1A:92;
        hence contradiction by A1,A13,A29,JORDAN1G:7;
      end;
      now
        assume east_halfline E-max C meets L~godo;
        then
A159:   east_halfline E-max C meets (L~go \/ L~pion1) or
        east_halfline E-max C meets L~do by A136,XBOOLE_1:70;
        per cases by A159,XBOOLE_1:70;
        suppose east_halfline E-max C meets L~go;
          hence contradiction by A154;
        end;
        suppose east_halfline E-max C meets L~pion1;
          then consider p be set such that
A160:     p in east_halfline E-max C and
A161:     p in L~pion1 by XBOOLE_0:3;
          reconsider p as Point of TOP-REAL 2 by A160;
A162:     now per cases by A7,A80,A161,XBOOLE_0:def 2;
            suppose p in poz;
              hence p`1 <= Gik`1 by A85,TOPREAL1:9;
            end;
            suppose p in pio;
              hence p`1 <= Gik`1 by A84,A85,GOBOARD7:5;
            end;
          end;
          i2+1 <= len G by A1,NAT_1:13;
          then i2+1-1 <= len G-1 by XREAL_1:11;
          then
A163:     i2 <= len G-'1 by BINARITH:def 3;
          len G-'1 <= len G by BINARITH:52;
          then Gik`1 <= G*(len G-'1,1)`1 by A6,A11,A15,A19,A163,JORDAN1A:39;
          then p`1 <= G*(len G-'1,1)`1 by A162,XXREAL_0:2;
          then p`1 <= E-bound C by A19,JORDAN8:15;
          then
A164:     p`1 <= (E-max C)`1 by EUCLID:56;
          p`1 >= (E-max C)`1 by A160,TOPREAL1:def 13;
          then
A165:     p`1 = (E-max C)`1 by A164,XXREAL_0:1;
          p`2 = (E-max C)`2 by A160,TOPREAL1:def 13;
          then p = E-max C by A165,TOPREAL3:11;
          hence contradiction by A5,A7,A80,A127,A161,XBOOLE_0:3;
        end;
        suppose east_halfline E-max C meets L~do;
          then consider p be set such that
A166:     p in east_halfline E-max C and
A167:     p in L~do by XBOOLE_0:3;
          reconsider p as Point of TOP-REAL 2 by A166;
          p in L~LS by A52,A167;
          then p in east_halfline E-max C /\ L~Cage(C,n)
          by A137,A166,XBOOLE_0:def 3;
          then
A168:     p`1 = Ebo by A153,JORDAN1A:104;
A169:     (E-max C)`2 = p`2 by A166,TOPREAL1:def 13;
          set RC = Rotate(Cage(C,n),Emax);
A170:     E-max C in right_cell(RC,1) by JORDAN1I:9;
A171:     1+1 <= len LS by A23,XXREAL_0:2;
          LS = RC-:Wmin by JORDAN1G:26;
          then
A172:     LSeg(LS,1) = LSeg(RC,1) by A171,SPPOL_2:9;
A173:     L~RC = L~Cage(C,n) by REVROT_1:33;
A174:     len RC = len Cage(C,n) by REVROT_1:14;
A175:     GoB RC = GoB Cage(C,n) by REVROT_1:28
            .= G by JORDAN1H:52;
A176:     Emax in rng Cage(C,n) by SPRECT_2:50;
A177:     RC is_sequence_on G by A133,REVROT_1:34;
A178:     RC/.1 = E-max L~RC by A173,A176,FINSEQ_6:98;
          then consider ii,jj be Element of NAT such that
A179:     [ii,jj+1] in Indices G and
A180:     [ii,jj] in Indices G and
A181:     RC/.1 = G*(ii,jj+1) and
A182:     RC/.(1+1) = G*(ii,jj) by A89,A174,A177,JORDAN1I:25;
          consider jj2 be Element of NAT such that
A183:     1 <= jj2 & jj2 <= width G and
A184:     Emax = G*(len G,jj2) by JORDAN1D:29;
A185:     len G >= 4 by JORDAN8:13;
          then len G >= 1 by XXREAL_0:2;
          then [len G,jj2] in Indices G by A183,MATRIX_1:37;
          then
A186:     ii = len G by A173,A178,A179,A181,A184,GOBOARD1:21;
A187:     1 <= ii & ii <= len G & 1 <= jj+1 & jj+1 <= width G
          by A179,GOBOARD5:1;
A188:     1 <= ii & ii <= len G & 1 <= jj & jj <= width G by A180,GOBOARD5:1;
A189:     ii+1 <> ii;
          jj+1+1 <> jj;
          then
A190:     right_cell(RC,1) = cell(G,ii-'1,jj)
          by A89,A174,A175,A179,A180,A181,A182,A189,GOBOARD5:def 6;
A191:     ii-'1+1 = ii by A187,BINARITH:53;
          ii-1 >= 4-1 by A185,A186,XREAL_1:11;
          then
A192:     ii-1 >= 1 by XXREAL_0:2;
          then
A193:     1 <= ii-'1 by BINARITH:def 3;
A194:     G*(ii-'1,jj)`2 <= p`2 & p`2 <= G*(ii-'1,jj+1)`2
          by A169,A170,A187,A188,A190,A191,A192,JORDAN9:19;
A195:     ii-'1 < len G by A187,A191,NAT_1:13;
          then
A196:     G*(ii-'1,jj)`2 = G*(1,jj)`2 by A188,A193,GOBOARD5:2
            .= G*(ii,jj)`2 by A188,GOBOARD5:2;
A197:     G*(ii-'1,jj+1)`2 = G*(1,jj+1)`2 by A187,A193,A195,GOBOARD5:2
            .= G*(ii,jj+1)`2 by A187,GOBOARD5:2;
          G*(len G,jj)`1 = Ebo & Ebo = G*(len G,jj+1)`1
          by A15,A187,A188,JORDAN1A:92;
          then p in LSeg(RC/.1,RC/.(1+1))
          by A168,A181,A182,A186,A194,A196,A197,GOBOARD7:8;
          then
A198:     p in LSeg(LS,1) by A89,A172,A174,TOPREAL1:def 5;
A199:     p in LSeg(do,Index(p,do)) by A167,JORDAN3:42;
A200:     do = mid(LS,Gij..LS,len LS) by A36,JORDAN1J:37;
A201:     1<=Gij..LS & Gij..LS<=len LS by A36,FINSEQ_4:31;
          Gij..LS <> len LS by A28,A36,FINSEQ_4:29;
          then
A202:     Gij..LS < len LS by A201,REAL_1:def 5;
A203:     1<=Index(p,do) & Index(p,do) < len do by A167,JORDAN3:41;
A204:     Index(Gij,LS)+1 = Gij..LS by A31,A36,JORDAN1J:56;
          consider t be Nat such that
A205:     t in dom LS and
A206:     LS.t = Gij by A36,FINSEQ_2:11;
A207:     1 <= t & t <= len LS by A205,FINSEQ_3:27;
          then 1 < t by A31,A206,REAL_1:def 5;
          then Index(Gij,LS)+1 = t by A206,A207,JORDAN3:45;
          then
A208:     len L_Cut(LS,Gij) = len LS-Index(Gij,LS) by A8,A206,JORDAN3:61;
          set tt = Index(p,do)+(Gij..LS)-'1;
A209:     1<=Index(Gij,LS) & 0+Index(Gij,LS) < len LS by A8,JORDAN3:41;
          then
A210:     len LS-Index(Gij,LS) > 0 by XREAL_1:22;
          Index(p,do) < len LS-'Index(Gij,LS) by A203,A208,BINARITH:def 3;
          then Index(p,do)+1 <= len LS-'Index(Gij,LS) by NAT_1:13;
          then Index(p,do) <= len LS-'Index(Gij,LS)-1 by XREAL_1:21;
          then Index(p,do) <= len LS-Index(Gij,LS)-1 by A210,BINARITH:def 3;
          then Index(p,do) <= len LS-Gij..LS by A204;
          then Index(p,do) <= len LS-'Gij..LS by BINARITH:def 3;
          then Index(p,do) < len LS-'(Gij..LS)+1 by NAT_1:13;
          then
A211:     LSeg(mid(LS,Gij..LS,len LS),Index(p,do)) =
          LSeg(LS,Index(p,do)+(Gij..LS)-'1) by A201,A202,A203,JORDAN4:31;
A212:     1+1 <= Gij..LS by A204,A209,XREAL_1:9;
          then Index(p,do)+Gij..LS >= 1+1+1 by A203,XREAL_1:9;
          then Index(p,do)+Gij..LS-1 >= 1+1+1-1 by XREAL_1:11;
          then
A213:     tt >= 1+1 by BINARITH:def 3;
A214:     2 in dom LS by A171,FINSEQ_3:27;
          now per cases by A213,REAL_1:def 5;
            suppose tt > 1+1;
              then LSeg(LS,1) misses LSeg(LS,tt) by TOPREAL1:def 9;
              hence contradiction by A198,A199,A200,A211,XBOOLE_0:3;
            end;
            suppose
A215:         tt = 1+1;
              then LSeg(LS,1) /\ LSeg(LS,tt) = {LS/.2} by A23,TOPREAL1:def 8;
              then p in {LS/.2} by A198,A199,A200,A211,XBOOLE_0:def 3;
              then
A216:         p = LS/.2 by TARSKI:def 1;
              then
A217:         p..LS = 2 by A214,FINSEQ_5:44;
              1+1 = Index(p,do)+(Gij..LS)-1 by A215,BINARITH:def 3;
              then 1+1+1 = Index(p,do)+(Gij..LS);
              then
A218:         Gij..LS = 2 by A203,A212,JORDAN1E:10;
              p in rng LS by A214,A216,PARTFUN2:4;
              then p = Gij by A36,A217,A218,FINSEQ_5:10;
              then Gij`1 = Ebo by A216,JORDAN1G:40;
              then Gij`1 = G*(len G,j)`1 by A10,A15,JORDAN1A:92;
              hence contradiction by A1,A12,A63,JORDAN1G:7;
            end;
          end;
          hence contradiction;
        end;
      end;
      then east_halfline E-max C c= (L~godo)` by SUBSET_1:43;
      then consider W be Subset of TOP-REAL 2 such that
A219: W is_a_component_of (L~godo)` and
A220: east_halfline E-max C c= W by GOBOARD9:5;
      W is not Bounded by A220,JORDAN2C:16,129;
      then W is_outside_component_of L~godo by A219,JORDAN2C:def 4;
      then W c= UBD L~godo by JORDAN2C:27;
      then
A221: east_halfline E-max C c= UBD L~godo by A220,XBOOLE_1:1;
      E-max C in east_halfline E-max C by TOPREAL1:45;
      then E-max C in UBD L~godo by A221;
      then E-max C in LeftComp godo by GOBRD14:46;
      then LA meets L~godo by A126,A127,A135,A142,JORDAN1J:36;
      then
A222: LA meets (L~go \/ L~pion1) or LA meets L~do by A136,XBOOLE_1:70;
A223: LA c= C by JORDAN6:76;
      now per cases by A222,XBOOLE_1:70;
        suppose LA meets L~go;
          then LA meets L~Cage(C,n) by A45,A137,XBOOLE_1:1,63;
          hence contradiction by A223,JORDAN10:5,XBOOLE_1:63;
        end;
        suppose LA meets L~pion1;
          hence contradiction by A5,A7,A80;
        end;
        suppose LA meets L~do;
          then LA meets L~Cage(C,n) by A52,A137,XBOOLE_1:1,63;
          hence contradiction by A223,JORDAN10:5,XBOOLE_1:63;
        end;
      end;
      hence contradiction;
    end;
    suppose Gij`1 = Gik`1;
      then
A224: i1 = i2 by A12,A13,JORDAN1G:7;
      then poz = {Gi1k} by TOPREAL1:7;
      then poz c= pio by A76,ZFMISC_1:37;
      then pio \/ poz = pio by XBOOLE_1:12;
      hence contradiction by A1,A2,A3,A4,A5,A224,JORDAN1J:58;
    end;
    suppose Gij`2 = Gik`2;
      then
A225: j = k by A12,A13,JORDAN1G:6;
      then pio = {Gi1k} by TOPREAL1:7;
      then pio c= poz by A77,ZFMISC_1:37;
      then pio \/ poz = poz by XBOOLE_1:12;
      hence contradiction by A1,A2,A3,A4,A5,A225,Th30;
    end;
  end;
  hence contradiction;
end;

theorem Th48:
  for C be Simple_closed_curve for i1,i2,j,k be Element of NAT st
  1 < i2 & i2 <= i1 & i1 < len Gauge(C,n) &
  1 <= j & j <= k & k <= width Gauge(C,n) &
  (LSeg(Gauge(C,n)*(i1,j),Gauge(C,n)*(i1,k)) \/
  LSeg(Gauge(C,n)*(i1,k),Gauge(C,n)*(i2,k))) /\ L~Upper_Seq(C,n) =
  {Gauge(C,n)*(i2,k)} & (LSeg(Gauge(C,n)*(i1,j),Gauge(C,n)*(i1,k)) \/
  LSeg(Gauge(C,n)*(i1,k),Gauge(C,n)*(i2,k))) /\ L~Lower_Seq(C,n) =
  {Gauge(C,n)*(i1,j)} holds (LSeg(Gauge(C,n)*(i1,j),Gauge(C,n)*(i1,k)) \/
  LSeg(Gauge(C,n)*(i1,k),Gauge(C,n)*(i2,k))) meets Upper_Arc C
proof
  let C be Simple_closed_curve;
  let i1,i2,j,k be Element of NAT;
  set G = Gauge(C,n);
  set pio = LSeg(G*(i1,j),G*(i1,k));
  set poz = LSeg(G*(i1,k),G*(i2,k));
  set US = Upper_Seq(C,n);
  set LS = Lower_Seq(C,n);
  assume that
A1: 1 < i2 & i2 <= i1 & i1 < len G and
A2: 1 <= j & j <= k & k <= width G and
A3: (pio \/ poz) /\ L~US = {G*(i2,k)} and
A4: (pio \/ poz) /\ L~LS = {G*(i1,j)} and
A5: (pio \/ poz) misses Upper_Arc C;
  set UA = Upper_Arc C;
  set Wmin = W-min L~Cage(C,n);
  set Emax = E-max L~Cage(C,n);
  set Wbo = W-bound L~Cage(C,n);
  set Ebo = E-bound L~Cage(C,n);
  set Gij = G*(i1,j);
  set Gik = G*(i2,k);
  set Gi1k = G*(i1,k);
A6: 1 < i1 & i1 < len G & 1 < i2 & i2 < len G by A1,XXREAL_0:2;
A7: L~<*Gik,Gi1k,Gij*> = poz \/ pio by TOPREAL3:23;
  Gij in {Gij} by TARSKI:def 1;
  then
A8: Gij in L~LS by A4,XBOOLE_0:def 3;
  Gik in {Gik} by TARSKI:def 1;
  then
A9: Gik in L~US by A3,XBOOLE_0:def 3;
A10: 1 <= j & j <= width G by A2,XXREAL_0:2;
A11: 1 <= k & k <= width G by A2,XXREAL_0:2;
A12: [i1,j] in Indices G by A6,A10,MATRIX_1:37;
A13: [i2,k] in Indices G by A6,A11,MATRIX_1:37;
A14: [i1,k] in Indices G by A6,A11,MATRIX_1:37;
  set go = R_Cut(US,Gik);
  set do = L_Cut(LS,Gij);
A15: len G = width G by JORDAN8:def 1;
A16: len US >= 3 by JORDAN1E:19;
  then len US >= 1 by XXREAL_0:2;
  then 1 in dom US by FINSEQ_3:27;
  then
A17: US.1 = US/.1 by PARTFUN1:def 8
    .= Wmin by JORDAN1F:5;
A18: Wmin`1 = Wbo by EUCLID:56
    .= G*(1,k)`1 by A11,A15,JORDAN1A:94;
  len G >= 4 by JORDAN8:13;
  then
A19: len G >= 1 by XXREAL_0:2;
  then
A20: [1,k] in Indices G by A11,MATRIX_1:37;
  then
A21: Gik <> US.1 by A1,A13,A17,A18,JORDAN1G:7;
  then reconsider go as being_S-Seq FinSequence of TOP-REAL 2
  by A9,JORDAN3:70;
A22: len LS >= 1+2 by JORDAN1E:19;
  then len LS >= 1 by XXREAL_0:2;
  then
A23: 1 in dom LS & len LS in dom LS by FINSEQ_3:27;
  then
A24: LS.len LS = LS/.len LS by PARTFUN1:def 8
    .= Wmin by JORDAN1F:8;
A25: Wmin`1 = Wbo by EUCLID:56
    .= G*(1,k)`1 by A11,A15,JORDAN1A:94;
A26: [i1,j] in Indices G by A6,A10,MATRIX_1:37;
  then
A27: Gij <> LS.len LS by A1,A20,A24,A25,JORDAN1G:7;
  then reconsider do as being_S-Seq FinSequence of TOP-REAL 2
  by A8,JORDAN3:69;
A28: [len G,k] in Indices G by A11,A19,MATRIX_1:37;
A29: LS.1 = LS/.1 by A23,PARTFUN1:def 8
    .= Emax by JORDAN1F:6;
  Emax`1 = Ebo by EUCLID:56
    .= G*(len G,k)`1 by A11,A15,JORDAN1A:92;
  then
A30: Gij <> LS.1 by A1,A26,A28,A29,JORDAN1G:7;
A31: len go >= 1+1 by TOPREAL1:def 10;
A32: Gik in rng US by A6,A9,A11,JORDAN1G:4,JORDAN1J:40;
  then
A33: go is_sequence_on G by JORDAN1G:4,JORDAN1J:38;
A34: len do >= 1+1 by TOPREAL1:def 10;
A35: Gij in rng LS by A6,A8,A10,JORDAN1G:5,JORDAN1J:40;
  then
A36: do is_sequence_on G by JORDAN1G:5,JORDAN1J:39;
  reconsider go as non constant s.c.c.
  (being_S-Seq FinSequence of TOP-REAL 2) by A31,A33,JGRAPH_1:16,JORDAN8:8;
  reconsider do as non constant s.c.c.
  (being_S-Seq FinSequence of TOP-REAL 2) by A34,A36,JGRAPH_1:16,JORDAN8:8;
A37: len go > 1 by A31,NAT_1:13;
  then
A38: len go in dom go by FINSEQ_3:27;
  then
A39: go/.len go = go.len go by PARTFUN1:def 8
    .= Gik by A9,JORDAN3:59;
  len do >= 1 by A34,XXREAL_0:2;
  then 1 in dom do by FINSEQ_3:27;
  then
A40: do/.1 = do.1 by PARTFUN1:def 8
    .= Gij by A8,JORDAN3:58;
  reconsider m = len go - 1 as Element of NAT by A38,FINSEQ_3:28;
A41: m+1 = len go;
  then
A42: len go-'1 = m by BINARITH:39;
A43: LSeg(go,m) c= L~go by TOPREAL3:26;
A44: L~go c= L~US by A9,JORDAN3:76;
  then LSeg(go,m) c= L~US by A43,XBOOLE_1:1;
  then
A45: LSeg(go,m) /\ L~<*Gik,Gi1k,Gij*> c= {Gik} by A3,A7,XBOOLE_1:26;
  m >= 1 by A31,XREAL_1:21;
  then
A46: LSeg(go,m) = LSeg(go/.m,Gik) by A39,A41,TOPREAL1:def 5;
  {Gik} c= LSeg(go,m) /\ L~<*Gik,Gi1k,Gij*>
  proof
    let x be set;
    assume x in {Gik};
    then
A47: x = Gik by TARSKI:def 1;
A48: Gik in LSeg(go,m) by A46,TOPREAL1:6;
    Gik in LSeg(Gik,Gi1k) by TOPREAL1:6;
    then Gik in LSeg(Gik,Gi1k) \/ LSeg(Gi1k,Gij) by XBOOLE_0:def 2;
    then Gik in L~<*Gik,Gi1k,Gij*> by SPRECT_1:10;
    hence x in LSeg(go,m) /\ L~<*Gik,Gi1k,Gij*> by A47,A48,XBOOLE_0:def 3;
  end;
  then
A49: LSeg(go,m) /\ L~<*Gik,Gi1k,Gij*> = {Gik} by A45,XBOOLE_0:def 10;
A50: LSeg(do,1) c= L~do by TOPREAL3:26;
A51: L~do c= L~LS by A8,JORDAN3:77;
  then LSeg(do,1) c= L~LS by A50,XBOOLE_1:1;
  then
A52: LSeg(do,1) /\ L~<*Gik,Gi1k,Gij*> c= {Gij} by A4,A7,XBOOLE_1:26;
A53: LSeg(do,1) = LSeg(Gij,do/.(1+1)) by A34,A40,TOPREAL1:def 5;
  {Gij} c= LSeg(do,1) /\ L~<*Gik,Gi1k,Gij*>
  proof
    let x be set;
    assume x in {Gij};
    then
A54: x = Gij by TARSKI:def 1;
A55: Gij in LSeg(do,1) by A53,TOPREAL1:6;
    Gij in LSeg(Gi1k,Gij) by TOPREAL1:6;
    then Gij in LSeg(Gik,Gi1k) \/ LSeg(Gi1k,Gij) by XBOOLE_0:def 2;
    then Gij in L~<*Gik,Gi1k,Gij*> by SPRECT_1:10;
    hence x in LSeg(do,1) /\ L~<*Gik,Gi1k,Gij*> by A54,A55,XBOOLE_0:def 3;
  end;
  then
A56: L~<*Gik,Gi1k,Gij*> /\ LSeg(do,1) = {Gij} by A52,XBOOLE_0:def 10;
A57: go/.1 = US/.1 by A9,SPRECT_3:39
    .= Wmin by JORDAN1F:5;
  then
A58: go/.1 = LS/.len LS by JORDAN1F:8
    .= do/.len do by A8,JORDAN1J:35;
A59: rng go c= L~go & rng do c= L~do by A31,A34,SPPOL_2:18;
A60: {go/.1} c= L~go /\ L~do
  proof
    let x be set;
    assume x in {go/.1};
    then x = go/.1 by TARSKI:def 1;
    then x in rng go & x in rng do by A58,FINSEQ_6:46,REVROT_1:3;
    hence x in L~go /\ L~do by A59,XBOOLE_0:def 3;
  end;
A61: LS.1 = LS/.1 by A23,PARTFUN1:def 8
    .= Emax by JORDAN1F:6;
A62: [len G,j] in Indices G by A10,A19,MATRIX_1:37;
  L~go /\ L~do c= {go/.1}
  proof
    let x be set;
    assume x in L~go /\ L~do;
    then
A63: x in L~go & x in L~do by XBOOLE_0:def 3;
    then x in L~US /\ L~LS by A44,A51,XBOOLE_0:def 3;
    then x in {Wmin,Emax} by JORDAN1E:20;
    then
A64: x = Wmin or x = Emax by TARSKI:def 2;
    now
      assume x = Emax;
      then
A65:  Emax = Gij by A8,A61,A63,JORDAN1E:11;
      G*(len G,j)`1 = Ebo by A10,A15,JORDAN1A:92;
      then Emax`1 <> Ebo by A1,A12,A62,A65,JORDAN1G:7;
      hence contradiction by EUCLID:56;
    end;
    hence x in {go/.1} by A57,A64,TARSKI:def 1;
  end;
  then
A66: L~go /\ L~do = {go/.1} by A60,XBOOLE_0:def 10;
  set W2 = go/.2;
A67: 2 in dom go by A31,FINSEQ_3:27;
A68: Gik..US >= 1 by A32,FINSEQ_4:31;
A69: now
    assume Gik`1 = Wbo;
    then G*(1,k)`1 = G*(i2,k)`1 by A11,A15,JORDAN1A:94;
    hence contradiction by A1,A13,A20,JORDAN1G:7;
  end;
  go = mid(US,1,Gik..US) by A32,JORDAN1G:57
    .= US|(Gik..US) by A68,JORDAN3:25;
  then
A70: W2 = US/.2 by A67,FINSEQ_4:85;
A71: Wmin in rng go by A57,FINSEQ_6:46;
  set pion = <*Gik,Gi1k,Gij*>;
A72: now
    let n be Element of NAT;
    assume n in dom pion;
    then n in {1,2,3} by FINSEQ_3:1,30;
    then n = 1 or n = 2 or n = 3 by ENUMSET1:def 1;
    then pion/.n = Gik or pion/.n = Gi1k or pion/.n = Gij by FINSEQ_4:27;
    hence ex i,j be Element of NAT st [i,j] in Indices G & pion/.n = G*(i,j)
    by A12,A13,A14;
  end;
A73: Gi1k`1 = G*(i1,1)`1 by A6,A11,GOBOARD5:3
    .= Gij`1 by A6,A10,GOBOARD5:3;
  Gi1k`2 = G*(1,k)`2 by A6,A11,GOBOARD5:2
    .= Gik`2 by A6,A11,GOBOARD5:2;
  then
A74: Gi1k = |[Gij`1,Gik`2]| by A73,EUCLID:57;
A75: Gi1k in pio by TOPREAL1:6;
A76: Gi1k in poz by TOPREAL1:6;
  now per cases;
    suppose Gij`1 <> Gik`1 & Gij`2 <> Gik`2;
      then pion is_S-Seq by A74,TOPREAL3:42;
      then consider pion1 be FinSequence of TOP-REAL 2 such that
A77:  pion1 is_sequence_on G and
A78:  pion1 is_S-Seq and
A79:  L~pion = L~pion1 and
A80:  pion/.1 = pion1/.1 and
A81:  pion/.len pion = pion1/.len pion1 and
A82:  len pion <= len pion1 by A72,GOBOARD3:2;
      reconsider pion1 as being_S-Seq FinSequence of TOP-REAL 2 by A78;
      set godo = go^'pion1^'do;
A83:  Gi1k`1 = G*(i1,1)`1 by A6,A11,GOBOARD5:3
        .= Gij`1 by A6,A10,GOBOARD5:3;
A84:  Gik`1 <= Gi1k`1 by A1,A11,JORDAN1A:39;
      then
A85:  W-bound poz = Gik`1 by SPRECT_1:62;
A86:  W-bound pio = Gij`1 by A83,SPRECT_1:62;
      W-bound (poz \/ pio) = min(W-bound poz, W-bound pio) by SPRECT_1:54
        .= Gik`1 by A83,A84,A85,A86,XXREAL_0:def 8;
      then
A87:  W-bound L~pion1 = Gik`1 by A79,TOPREAL3:23;
A88:  1+1 <= len Cage(C,n) by GOBOARD7:36,XXREAL_0:2;
      then
A89:  1+1 <= len Rotate(Cage(C,n),Wmin) by REVROT_1:14;
      len (go^'pion1) >= len go by TOPREAL8:7;
      then
A90:  len (go^'pion1) >= 1+1 by A31,XXREAL_0:2;
      then
A91:  len (go^'pion1) > 1+0 by NAT_1:13;
A92:  len godo >= len (go^'pion1) by TOPREAL8:7;
      then
A93:  1+1 <= len godo by A90,XXREAL_0:2;
A94:  US is_sequence_on G by JORDAN1G:4;
A95:  go/.len go = pion1/.1 by A39,A80,FINSEQ_4:27;
      then
A96:  go^'pion1 is_sequence_on G by A33,A77,TOPREAL8:12;
A97:  (go^'pion1)/.len (go^'pion1) = pion/.len pion by A81,GRAPH_2:58
        .= pion/.3 by FINSEQ_1:62
        .= do/.1 by A40,FINSEQ_4:27;
      then
A98:  godo is_sequence_on G by A36,A96,TOPREAL8:12;
      LSeg(pion1,1) c= L~pion by A79,TOPREAL3:26;
      then
A99:  LSeg(go,len go-'1) /\ LSeg(pion1,1) c={Gik} by A42,A49,XBOOLE_1:27;
      len pion1 >= 2+1 by A82,FINSEQ_1:62;
      then
A100: len pion1 > 1+1 by NAT_1:13;
      {Gik} c= LSeg(go,m) /\ LSeg(pion1,1)
      proof
        let x be set;
        assume x in {Gik};
        then
A101:   x = Gik by TARSKI:def 1;
A102:   Gik in LSeg(go,m) by A46,TOPREAL1:6;
        Gik in LSeg(pion1,1) by A39,A95,A100,TOPREAL1:27;
        hence x in LSeg(go,m) /\ LSeg(pion1,1) by A101,A102,XBOOLE_0:def 3;
      end;
      then LSeg(go,len go-'1) /\ LSeg(pion1,1) = { go/.len go }
      by A39,A42,A99,XBOOLE_0:def 10;
      then
A103: go^'pion1 is unfolded by A95,TOPREAL8:34;
      len pion1 >= 2+1 by A82,FINSEQ_1:62;
      then
A104: len pion1-2 >= 0 by XREAL_1:21;
      len (go^'pion1)+1-1 = len go+len pion1-1 by GRAPH_2:13;
      then len (go^'pion1)-1 = len go + (len pion1-2)
        .= len go + (len pion1-'2) by A104,BINARITH:def 3;
      then
A105: len (go^'pion1)-'1 = len go + (len pion1-'2) by BINARITH:def 3;
A106: len pion1-1 >= 1 by A100,XREAL_1:21;
      then
A107: len pion1-'1 = len pion1-1 by BINARITH:def 3;
A108: len pion1-'2+1 = len pion1-2+1 by A104,BINARITH:def 3
        .= len pion1-'1 by A106,BINARITH:def 3;
      len pion1-1+1 <= len pion1;
      then
A109: len pion1-'1 < len pion1 by A107,NAT_1:13;
      LSeg(pion1,len pion1-'1) c= L~pion by A79,TOPREAL3:26;
      then
A110: LSeg(pion1,len pion1-'1) /\ LSeg(do,1) c= {Gij} by A56,XBOOLE_1:27;
      {Gij} c= LSeg(pion1,len pion1-'1) /\ LSeg(do,1)
      proof
        let x be set;
        assume x in {Gij};
        then
A111:   x = Gij by TARSKI:def 1;
A112:   Gij in LSeg(do,1) by A53,TOPREAL1:6;
        pion1/.(len pion1-'1+1) = pion/.3 by A81,A107,FINSEQ_1:62
          .= Gij by FINSEQ_4:27;
        then Gij in LSeg(pion1,len pion1-'1) by A106,A107,TOPREAL1:27;
        hence x in LSeg(pion1,len pion1-'1) /\ LSeg(do,1)
        by A111,A112,XBOOLE_0:def 3;
      end;
      then LSeg(pion1,len pion1-'1) /\ LSeg(do,1) = {Gij}
      by A110,XBOOLE_0:def 10;
      then
A113: LSeg(go^'pion1,len go+(len pion1-'2)) /\ LSeg(do,1) =
      {(go^'pion1)/.len (go^'pion1)} by A40,A95,A97,A108,A109,TOPREAL8:31;
A114: (go^'pion1) is non trivial by A90,REALSET1:13;
A115: rng pion1 c= L~pion1 by A100,SPPOL_2:18;
A116: {pion1/.1} c= L~go /\ L~pion1
      proof
        let x be set;
        assume x in {pion1/.1};
        then x = pion1/.1 by TARSKI:def 1;
        then x in rng go & x in rng pion1 by A95,FINSEQ_6:46,REVROT_1:3;
        hence x in L~go /\ L~pion1 by A59,A115,XBOOLE_0:def 3;
      end;
      L~go /\ L~pion1 c= {pion1/.1}
      proof
        let x be set;
        assume x in L~go /\ L~pion1;
        then x in L~go & x in L~pion1 by XBOOLE_0:def 3;
        hence x in {pion1/.1} by A3,A7,A39,A44,A79,A95,XBOOLE_0:def 3;
      end;
      then
A117: L~go /\ L~pion1 = {pion1/.1} by A116,XBOOLE_0:def 10;
      then
A118: (go^'pion1) is s.n.c. by A95,JORDAN1J:54;
      rng go /\ rng pion1 c= {pion1/.1} by A59,A115,A117,XBOOLE_1:27;
      then
A119: go^'pion1 is one-to-one by JORDAN1J:55;
A120: pion/.len pion = pion/.3 by FINSEQ_1:62
        .= do/.1 by A40,FINSEQ_4:27;
A121: {pion1/.len pion1} c= L~do /\ L~pion1
      proof
        let x be set;
        assume x in {pion1/.len pion1};
        then x = pion1/.len pion1 by TARSKI:def 1;
        then x in rng do & x in rng pion1 by A81,A120,FINSEQ_6:46,REVROT_1:3;
        hence x in L~do /\ L~pion1 by A59,A115,XBOOLE_0:def 3;
      end;
      L~do /\ L~pion1 c= {pion1/.len pion1}
      proof
        let x be set;
        assume x in L~do /\ L~pion1;
        then x in L~do & x in L~pion1 by XBOOLE_0:def 3;
        hence x in {pion1/.len pion1}
        by A4,A7,A40,A51,A79,A81,A120,XBOOLE_0:def 3;
      end;
      then
A122: L~do /\ L~pion1 = {pion1/.len pion1} by A121,XBOOLE_0:def 10;
A123: L~(go^'pion1) /\ L~do = (L~go \/ L~pion1) /\ L~do by A95,TOPREAL8:35
        .= {go/.1} \/ {do/.1} by A66,A81,A120,A122,XBOOLE_1:23
        .= {(go^'pion1)/.1} \/ {do/.1} by GRAPH_2:57
        .= {(go^'pion1)/.1,do/.1} by ENUMSET1:41;
      do/.len do = (go^'pion1)/.1 by A58,GRAPH_2:57;
      then reconsider godo as non constant standard special_circular_sequence
      by A93,A97,A98,A103,A105,A113,A114,A118,A119,A123,JORDAN8:7,8,TOPREAL8:11
      ,33,34;
A124: UA is_an_arc_of W-min C,E-max C by JORDAN6:def 8;
      then
A125: UA is connected by JORDAN6:11;
A126: W-min C in UA & E-max C in UA by A124,TOPREAL1:4;
      set ff = Rotate(Cage(C,n),Wmin);
      Wmin in rng Cage(C,n) by SPRECT_2:47;
      then
A127: ff/.1 = Wmin by FINSEQ_6:98;
A128: L~ff = L~Cage(C,n) by REVROT_1:33;
      then (W-max L~ff)..ff > 1 by A127,SPRECT_5:23;
      then (N-min L~ff)..ff > 1 by A127,A128,SPRECT_5:24,XXREAL_0:2;
      then (N-max L~ff)..ff > 1 by A127,A128,SPRECT_5:25,XXREAL_0:2;
      then
A129: Emax..ff > 1 by A127,A128,SPRECT_5:26,XXREAL_0:2;
A130: now
        assume
A131:   Gik..US <= 1;
        Gik..US >= 1 by A32,FINSEQ_4:31;
        then Gik..US = 1 by A131,XXREAL_0:1;
        then Gik = US/.1 by A32,FINSEQ_5:41;
        hence contradiction by A17,A21,JORDAN1F:5;
      end;
A132: Cage(C,n) is_sequence_on G by JORDAN9:def 1;
      then
A133: ff is_sequence_on G by REVROT_1:34;
A134: right_cell(godo,1,G)\L~godo c= RightComp godo by A93,A98,JORDAN9:29;
A135: L~godo = L~(go^'pion1) \/ L~do by A97,TOPREAL8:35
        .= L~go \/ L~pion1 \/ L~do by A95,TOPREAL8:35;
      L~Cage(C,n) = L~US \/ L~LS by JORDAN1E:17;
      then
A136: L~US c= L~Cage(C,n) & L~LS c= L~Cage(C,n) by XBOOLE_1:7;
      then
A137: L~go c=L~Cage(C,n) & L~do c=L~Cage(C,n) by A44,A51,XBOOLE_1:1;
A138: W-min C in C by SPRECT_1:15;
A139: now
        assume W-min C in L~godo;
        then
A140:   W-min C in L~go \/ L~pion1 or W-min C in L~do by A135,XBOOLE_0:def 2;
        per cases by A140,XBOOLE_0:def 2;
        suppose W-min C in L~go;
          then C meets L~Cage(C,n) by A137,A138,XBOOLE_0:3;
          hence contradiction by JORDAN10:5;
        end;
        suppose W-min C in L~pion1;
          hence contradiction by A5,A7,A79,A126,XBOOLE_0:3;
        end;
        suppose W-min C in L~do;
          then C meets L~Cage(C,n) by A137,A138,XBOOLE_0:3;
          hence contradiction by JORDAN10:5;
        end;
      end;
      right_cell(Rotate(Cage(C,n),Wmin),1) =
      right_cell(ff,1,GoB ff) by A89,JORDAN1H:29
        .= right_cell(ff,1,GoB Cage(C,n)) by REVROT_1:28
        .= right_cell(ff,1,G) by JORDAN1H:52
        .= right_cell(ff-:Emax,1,G) by A129,A133,JORDAN1J:53
        .= right_cell(US,1,G) by JORDAN1E:def 1
        .= right_cell(R_Cut(US,Gik),1,G) by A32,A94,A130,JORDAN1J:52
        .= right_cell(go^'pion1,1,G) by A37,A96,JORDAN1J:51
        .= right_cell(godo,1,G) by A91,A98,JORDAN1J:51;
      then W-min C in right_cell(godo,1,G) by JORDAN1I:8;
      then
A141: W-min C in right_cell(godo,1,G)\L~godo by A139,XBOOLE_0:def 4;
A142: godo/.1 = (go^'pion1)/.1 by GRAPH_2:57
        .= Wmin by A57,GRAPH_2:57;
A143: len US >= 2 by A16,XXREAL_0:2;
A144: godo/.2 = (go^'pion1)/.2 by A90,GRAPH_2:61
        .= US/.2 by A31,A70,GRAPH_2:61
        .= (US^'LS)/.2 by A143,GRAPH_2:61
        .= Rotate(Cage(C,n),Wmin)/.2 by JORDAN1E:15;
A145: L~go \/ L~do is compact by COMPTS_1:19;
      Wmin in L~go \/ L~do by A59,A71,XBOOLE_0:def 2;
      then
A146: W-min (L~go \/ L~do) = Wmin by A137,A145,JORDAN1J:21,XBOOLE_1:8;
A147: (W-min (L~go \/ L~do))`1 = W-bound (L~go \/ L~do) & Wmin`1 = Wbo
      by EUCLID:56;
      Gik`1 >= Wbo by A9,A136,PSCOMP_1:71;
      then Gik`1 > Wbo by A69,REAL_1:def 5;
      then W-min (L~go\/L~do\/L~pion1) = W-min (L~go \/ L~do)
      by A87,A145,A146,A147,JORDAN1J:33;
      then
A148: W-min L~godo = Wmin by A135,A146,XBOOLE_1:4;
A149: rng godo c= L~godo by A90,A92,SPPOL_2:18,XXREAL_0:2;
      2 in dom godo by A93,FINSEQ_3:27;
      then
A150: godo/.2 in rng godo by PARTFUN2:4;
      godo/.2 in W-most L~Cage(C,n) by A144,JORDAN1I:27;
      then (godo/.2)`1 = (W-min L~godo)`1 by A148,PSCOMP_1:88
        .= W-bound L~godo by EUCLID:56;
      then godo/.2 in W-most L~godo by A149,A150,SPRECT_2:16;
      then Rotate(godo,W-min L~godo)/.2 in W-most L~godo
      by A142,A148,FINSEQ_6:95;
      then reconsider godo as clockwise_oriented non constant standard
      special_circular_sequence by JORDAN1I:27;
      len US in dom US by FINSEQ_5:6;
      then
A151: US.len US = US/.len US by PARTFUN1:def 8
        .= Emax by JORDAN1F:7;
A152: E-max C in E-most C by PSCOMP_1:111;
A153: east_halfline E-max C misses L~go
      proof
        assume east_halfline E-max C meets L~go;
        then consider p be set such that
A154:   p in east_halfline E-max C and
A155:   p in L~go by XBOOLE_0:3;
        reconsider p as Point of TOP-REAL 2 by A154;
        p in L~US by A44,A155;
        then p in east_halfline E-max C /\ L~Cage(C,n)
        by A136,A154,XBOOLE_0:def 3;
        then
A156:   p`1 = Ebo by A152,JORDAN1A:104;
        then
A157:   p = Emax by A44,A155,JORDAN1J:46;
        then Emax = Gik by A9,A151,A155,JORDAN1J:43;
        then Gik`1 = G*(len G,k)`1 by A11,A15,A156,A157,JORDAN1A:92;
        hence contradiction by A1,A13,A28,JORDAN1G:7;
      end;
      now
        assume east_halfline E-max C meets L~godo;
        then
A158:   east_halfline E-max C meets (L~go \/ L~pion1) or
        east_halfline E-max C meets L~do by A135,XBOOLE_1:70;
        per cases by A158,XBOOLE_1:70;
        suppose east_halfline E-max C meets L~go;
          hence contradiction by A153;
        end;
        suppose east_halfline E-max C meets L~pion1;
          then consider p be set such that
A159:     p in east_halfline E-max C and
A160:     p in L~pion1 by XBOOLE_0:3;
          reconsider p as Point of TOP-REAL 2 by A159;
A161:     now per cases by A7,A79,A160,XBOOLE_0:def 2;
            suppose p in poz;
              hence p`1 <= Gi1k`1 by A84,TOPREAL1:9;
            end;
            suppose p in pio;
              hence p`1 <= Gi1k`1 by A83,GOBOARD7:5;
            end;
          end;
          i1+1 <= len G by A1,NAT_1:13;
          then i1+1-1 <= len G-1 by XREAL_1:11;
          then
A162:     i1 <= len G-'1 by BINARITH:def 3;
          len G-'1 <= len G by BINARITH:52;
          then Gi1k`1 <= G*(len G-'1,1)`1 by A6,A11,A15,A19,A162,JORDAN1A:39;
          then p`1 <= G*(len G-'1,1)`1 by A161,XXREAL_0:2;
          then p`1 <= E-bound C by A19,JORDAN8:15;
          then
A163:     p`1 <= (E-max C)`1 by EUCLID:56;
          p`1 >= (E-max C)`1 by A159,TOPREAL1:def 13;
          then
A164:     p`1 = (E-max C)`1 by A163,XXREAL_0:1;
          p`2 = (E-max C)`2 by A159,TOPREAL1:def 13;
          then p = E-max C by A164,TOPREAL3:11;
          hence contradiction by A5,A7,A79,A126,A160,XBOOLE_0:3;
        end;
        suppose east_halfline E-max C meets L~do;
          then consider p be set such that
A165:     p in east_halfline E-max C and
A166:     p in L~do by XBOOLE_0:3;
          reconsider p as Point of TOP-REAL 2 by A165;
          p in L~LS by A51,A166;
          then p in east_halfline E-max C /\ L~Cage(C,n)
          by A136,A165,XBOOLE_0:def 3;
          then
A167:     p`1 = Ebo by A152,JORDAN1A:104;
A168:     (E-max C)`2 = p`2 by A165,TOPREAL1:def 13;
          set RC = Rotate(Cage(C,n),Emax);
A169:     E-max C in right_cell(RC,1) by JORDAN1I:9;
A170:     1+1 <= len LS by A22,XXREAL_0:2;
          LS = RC-:Wmin by JORDAN1G:26;
          then
A171:     LSeg(LS,1) = LSeg(RC,1) by A170,SPPOL_2:9;
A172:     L~RC = L~Cage(C,n) by REVROT_1:33;
A173:     len RC = len Cage(C,n) by REVROT_1:14;
A174:     GoB RC = GoB Cage(C,n) by REVROT_1:28
            .= G by JORDAN1H:52;
A175:     Emax in rng Cage(C,n) by SPRECT_2:50;
A176:     RC is_sequence_on G by A132,REVROT_1:34;
A177:     RC/.1 = E-max L~RC by A172,A175,FINSEQ_6:98;
          then consider ii,jj be Element of NAT such that
A178:     [ii,jj+1] in Indices G and
A179:     [ii,jj] in Indices G and
A180:     RC/.1 = G*(ii,jj+1) and
A181:     RC/.(1+1) = G*(ii,jj) by A88,A173,A176,JORDAN1I:25;
          consider jj2 be Element of NAT such that
A182:     1 <= jj2 & jj2 <= width G and
A183:     Emax = G*(len G,jj2) by JORDAN1D:29;
A184:     len G >= 4 by JORDAN8:13;
          then len G >= 1 by XXREAL_0:2;
          then [len G,jj2] in Indices G by A182,MATRIX_1:37;
          then
A185:     ii = len G by A172,A177,A178,A180,A183,GOBOARD1:21;
A186:     1 <= ii & ii <= len G & 1 <= jj+1 & jj+1 <= width G
          by A178,GOBOARD5:1;
A187:     1 <= ii & ii <= len G & 1 <= jj & jj <= width G by A179,GOBOARD5:1;
A188:     ii+1 <> ii;
          jj+1+1 <> jj;
          then
A189:     right_cell(RC,1) = cell(G,ii-'1,jj)
          by A88,A173,A174,A178,A179,A180,A181,A188,GOBOARD5:def 6;
A190:     ii-'1+1 = ii by A186,BINARITH:53;
          ii-1 >= 4-1 by A184,A185,XREAL_1:11;
          then
A191:     ii-1 >= 1 by XXREAL_0:2;
          then
A192:     1 <= ii-'1 by BINARITH:def 3;
A193:     G*(ii-'1,jj)`2 <= p`2 & p`2 <= G*(ii-'1,jj+1)`2
          by A168,A169,A186,A187,A189,A190,A191,JORDAN9:19;
A194:     ii-'1 < len G by A186,A190,NAT_1:13;
          then
A195:     G*(ii-'1,jj)`2 = G*(1,jj)`2 by A187,A192,GOBOARD5:2
            .= G*(ii,jj)`2 by A187,GOBOARD5:2;
A196:     G*(ii-'1,jj+1)`2 = G*(1,jj+1)`2 by A186,A192,A194,GOBOARD5:2
            .= G*(ii,jj+1)`2 by A186,GOBOARD5:2;
          G*(len G,jj)`1 = Ebo & Ebo = G*(len G,jj+1)`1
          by A15,A186,A187,JORDAN1A:92;
          then p in LSeg(RC/.1,RC/.(1+1))
          by A167,A180,A181,A185,A193,A195,A196,GOBOARD7:8;
          then
A197:     p in LSeg(LS,1) by A88,A171,A173,TOPREAL1:def 5;
A198:     p in LSeg(do,Index(p,do)) by A166,JORDAN3:42;
A199:     do = mid(LS,Gij..LS,len LS) by A35,JORDAN1J:37;
A200:     1<=Gij..LS & Gij..LS<=len LS by A35,FINSEQ_4:31;
          Gij..LS <> len LS by A27,A35,FINSEQ_4:29;
          then
A201:     Gij..LS < len LS by A200,REAL_1:def 5;
A202:     1<=Index(p,do) & Index(p,do) < len do by A166,JORDAN3:41;
A203:     Index(Gij,LS)+1 = Gij..LS by A30,A35,JORDAN1J:56;
          consider t be Nat such that
A204:     t in dom LS and
A205:     LS.t = Gij by A35,FINSEQ_2:11;
A206:     1 <= t & t <= len LS by A204,FINSEQ_3:27;
          then 1 < t by A30,A205,REAL_1:def 5;
          then Index(Gij,LS)+1 = t by A205,A206,JORDAN3:45;
          then
A207:     len L_Cut(LS,Gij) = len LS-Index(Gij,LS) by A8,A205,JORDAN3:61;
          set tt = Index(p,do)+(Gij..LS)-'1;
A208:     1<=Index(Gij,LS) & 0+Index(Gij,LS) < len LS by A8,JORDAN3:41;
          then
A209:     len LS-Index(Gij,LS) > 0 by XREAL_1:22;
          Index(p,do) < len LS-'Index(Gij,LS) by A202,A207,BINARITH:def 3;
          then Index(p,do)+1 <= len LS-'Index(Gij,LS) by NAT_1:13;
          then Index(p,do) <= len LS-'Index(Gij,LS)-1 by XREAL_1:21;
          then Index(p,do) <= len LS-Index(Gij,LS)-1 by A209,BINARITH:def 3;
          then Index(p,do) <= len LS-Gij..LS by A203;
          then Index(p,do) <= len LS-'Gij..LS by BINARITH:def 3;
          then Index(p,do) < len LS-'(Gij..LS)+1 by NAT_1:13;
          then
A210:     LSeg(mid(LS,Gij..LS,len LS),Index(p,do)) =
          LSeg(LS,Index(p,do)+(Gij..LS)-'1) by A200,A201,A202,JORDAN4:31;
A211:     1+1 <= Gij..LS by A203,A208,XREAL_1:9;
          then Index(p,do)+Gij..LS >= 1+1+1 by A202,XREAL_1:9;
          then Index(p,do)+Gij..LS-1 >= 1+1+1-1 by XREAL_1:11;
          then
A212:     tt >= 1+1 by BINARITH:def 3;
A213:     2 in dom LS by A170,FINSEQ_3:27;
          now per cases by A212,REAL_1:def 5;
            suppose tt > 1+1;
              then LSeg(LS,1) misses LSeg(LS,tt) by TOPREAL1:def 9;
              hence contradiction by A197,A198,A199,A210,XBOOLE_0:3;
            end;
            suppose
A214:         tt = 1+1;
              then LSeg(LS,1) /\ LSeg(LS,tt) = {LS/.2} by A22,TOPREAL1:def 8;
              then p in {LS/.2} by A197,A198,A199,A210,XBOOLE_0:def 3;
              then
A215:         p = LS/.2 by TARSKI:def 1;
              then
A216:         p..LS = 2 by A213,FINSEQ_5:44;
              1+1 = Index(p,do)+(Gij..LS)-1 by A214,BINARITH:def 3;
              then 1+1+1 = Index(p,do)+(Gij..LS);
              then
A217:         Gij..LS = 2 by A202,A211,JORDAN1E:10;
              p in rng LS by A213,A215,PARTFUN2:4;
              then p = Gij by A35,A216,A217,FINSEQ_5:10;
              then Gij`1 = Ebo by A215,JORDAN1G:40;
              then Gij`1 = G*(len G,j)`1 by A10,A15,JORDAN1A:92;
              hence contradiction by A1,A12,A62,JORDAN1G:7;
            end;
          end;
          hence contradiction;
        end;
      end;
      then east_halfline E-max C c= (L~godo)` by SUBSET_1:43;
      then consider W be Subset of TOP-REAL 2 such that
A218: W is_a_component_of (L~godo)` and
A219: east_halfline E-max C c= W by GOBOARD9:5;
      W is not Bounded by A219,JORDAN2C:16,129;
      then W is_outside_component_of L~godo by A218,JORDAN2C:def 4;
      then W c= UBD L~godo by JORDAN2C:27;
      then
A220: east_halfline E-max C c= UBD L~godo by A219,XBOOLE_1:1;
      E-max C in east_halfline E-max C by TOPREAL1:45;
      then E-max C in UBD L~godo by A220;
      then E-max C in LeftComp godo by GOBRD14:46;
      then UA meets L~godo by A125,A126,A134,A141,JORDAN1J:36;
      then
A221: UA meets (L~go \/ L~pion1) or UA meets L~do by A135,XBOOLE_1:70;
A222: UA c= C by JORDAN6:76;
      now per cases by A221,XBOOLE_1:70;
        suppose UA meets L~go;
          then UA meets L~Cage(C,n) by A44,A136,XBOOLE_1:1,63;
          hence contradiction by A222,JORDAN10:5,XBOOLE_1:63;
        end;
        suppose UA meets L~pion1;
          hence contradiction by A5,A7,A79;
        end;
        suppose UA meets L~do;
          then UA meets L~Cage(C,n) by A51,A136,XBOOLE_1:1,63;
          hence contradiction by A222,JORDAN10:5,XBOOLE_1:63;
        end;
      end;
      hence contradiction;
    end;
    suppose Gij`1 = Gik`1;
      then
A223: i1 = i2 by A12,A13,JORDAN1G:7;
      then poz = {Gi1k} by TOPREAL1:7;
      then poz c= pio by A75,ZFMISC_1:37;
      then pio \/ poz = pio by XBOOLE_1:12;
      hence contradiction by A1,A2,A3,A4,A5,A223,JORDAN1J:59;
    end;
    suppose Gij`2 = Gik`2;
      then
A224: j = k by A12,A13,JORDAN1G:6;
      then pio = {Gi1k} by TOPREAL1:7;
      then pio c= poz by A76,ZFMISC_1:37;
      then pio \/ poz = poz by XBOOLE_1:12;
      hence contradiction by A1,A2,A3,A4,A5,A224,Th39;
    end;
  end;
  hence contradiction;
end;

theorem Th49:
  for C be Simple_closed_curve for i1,i2,j,k be Element of NAT st
  1 < i2 & i2 <= i1 & i1 < len Gauge(C,n) &
  1 <= j & j <= k & k <= width Gauge(C,n) &
  (LSeg(Gauge(C,n)*(i1,j),Gauge(C,n)*(i1,k)) \/
  LSeg(Gauge(C,n)*(i1,k),Gauge(C,n)*(i2,k))) /\ L~Upper_Seq(C,n) =
  {Gauge(C,n)*(i2,k)} & (LSeg(Gauge(C,n)*(i1,j),Gauge(C,n)*(i1,k)) \/
  LSeg(Gauge(C,n)*(i1,k),Gauge(C,n)*(i2,k))) /\ L~Lower_Seq(C,n) =
  {Gauge(C,n)*(i1,j)} holds (LSeg(Gauge(C,n)*(i1,j),Gauge(C,n)*(i1,k)) \/
  LSeg(Gauge(C,n)*(i1,k),Gauge(C,n)*(i2,k))) meets Lower_Arc C
proof
  let C be Simple_closed_curve;
  let i1,i2,j,k be Element of NAT;
  set G = Gauge(C,n);
  set pio = LSeg(G*(i1,j),G*(i1,k));
  set poz = LSeg(G*(i1,k),G*(i2,k));
  set US = Upper_Seq(C,n);
  set LS = Lower_Seq(C,n);
  assume that
A1: 1 < i2 & i2 <= i1 & i1 < len G and
A2: 1 <= j & j <= k & k <= width G and
A3: (pio \/ poz) /\ L~US = {G*(i2,k)} and
A4: (pio \/ poz) /\ L~LS = {G*(i1,j)} and
A5: (pio \/ poz) misses Lower_Arc C;
  set LA = Lower_Arc C;
  set Wmin = W-min L~Cage(C,n);
  set Emax = E-max L~Cage(C,n);
  set Wbo = W-bound L~Cage(C,n);
  set Ebo = E-bound L~Cage(C,n);
  set Gij = G*(i1,j);
  set Gik = G*(i2,k);
  set Gi1k = G*(i1,k);
A6: 1 < i1 & i1 < len G & 1 < i2 & i2 < len G by A1,XXREAL_0:2;
A7: L~<*Gik,Gi1k,Gij*> = poz \/ pio by TOPREAL3:23;
  Gij in {Gij} by TARSKI:def 1;
  then
A8: Gij in L~LS by A4,XBOOLE_0:def 3;
  Gik in {Gik} by TARSKI:def 1;
  then
A9: Gik in L~US by A3,XBOOLE_0:def 3;
A10: 1 <= j & j <= width G by A2,XXREAL_0:2;
A11: 1 <= k & k <= width G by A2,XXREAL_0:2;
A12: [i1,j] in Indices G by A6,A10,MATRIX_1:37;
A13: [i2,k] in Indices G by A6,A11,MATRIX_1:37;
A14: [i1,k] in Indices G by A6,A11,MATRIX_1:37;
  set go = R_Cut(US,Gik);
  set do = L_Cut(LS,Gij);
A15: len G = width G by JORDAN8:def 1;
A16: len US >= 3 by JORDAN1E:19;
  then len US >= 1 by XXREAL_0:2;
  then 1 in dom US by FINSEQ_3:27;
  then
A17: US.1 = US/.1 by PARTFUN1:def 8
    .= Wmin by JORDAN1F:5;
A18: Wmin`1 = Wbo by EUCLID:56
    .= G*(1,k)`1 by A11,A15,JORDAN1A:94;
  len G >= 4 by JORDAN8:13;
  then
A19: len G >= 1 by XXREAL_0:2;
  then
A20: [1,k] in Indices G by A11,MATRIX_1:37;
  then
A21: Gik <> US.1 by A1,A13,A17,A18,JORDAN1G:7;
  then reconsider go as being_S-Seq FinSequence of TOP-REAL 2
  by A9,JORDAN3:70;
A22: len LS >= 1+2 by JORDAN1E:19;
  then len LS >= 1 by XXREAL_0:2;
  then
A23: 1 in dom LS & len LS in dom LS by FINSEQ_3:27;
  then
A24: LS.len LS = LS/.len LS by PARTFUN1:def 8
    .= Wmin by JORDAN1F:8;
A25: Wmin`1 = Wbo by EUCLID:56
    .= G*(1,k)`1 by A11,A15,JORDAN1A:94;
A26: [i1,j] in Indices G by A6,A10,MATRIX_1:37;
  then
A27: Gij <> LS.len LS by A1,A20,A24,A25,JORDAN1G:7;
  then reconsider do as being_S-Seq FinSequence of TOP-REAL 2
  by A8,JORDAN3:69;
A28: [len G,k] in Indices G by A11,A19,MATRIX_1:37;
A29: LS.1 = LS/.1 by A23,PARTFUN1:def 8
    .= Emax by JORDAN1F:6;
  Emax`1 = Ebo by EUCLID:56
    .= G*(len G,k)`1 by A11,A15,JORDAN1A:92;
  then
A30: Gij <> LS.1 by A1,A26,A28,A29,JORDAN1G:7;
A31: len go >= 1+1 by TOPREAL1:def 10;
A32: Gik in rng US by A6,A9,A11,JORDAN1G:4,JORDAN1J:40;
  then
A33: go is_sequence_on G by JORDAN1G:4,JORDAN1J:38;
A34: len do >= 1+1 by TOPREAL1:def 10;
A35: Gij in rng LS by A6,A8,A10,JORDAN1G:5,JORDAN1J:40;
  then
A36: do is_sequence_on G by JORDAN1G:5,JORDAN1J:39;
  reconsider go as non constant s.c.c.
  (being_S-Seq FinSequence of TOP-REAL 2) by A31,A33,JGRAPH_1:16,JORDAN8:8;
  reconsider do as non constant s.c.c.
  (being_S-Seq FinSequence of TOP-REAL 2) by A34,A36,JGRAPH_1:16,JORDAN8:8;
A37: len go > 1 by A31,NAT_1:13;
  then
A38: len go in dom go by FINSEQ_3:27;
  then
A39: go/.len go = go.len go by PARTFUN1:def 8
    .= Gik by A9,JORDAN3:59;
  len do >= 1 by A34,XXREAL_0:2;
  then 1 in dom do by FINSEQ_3:27;
  then
