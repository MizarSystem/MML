            end;
            suppose p in pio;
              hence p`1 <= Gi1k`1 by A83,GOBOARD7:5;
            end;
          end;
          i1+1 <= len G by A1,NAT_1:13;
          then i1+1-1 <= len G-1 by XREAL_1:11;
          then
A162:     i1 <= len G-'1 by BINARITH:def 3;
          len G-'1 <= len G by BINARITH:52;
          then Gi1k`1 <= G*(len G-'1,1)`1 by A6,A11,A15,A19,A162,JORDAN1A:39;
          then p`1 <= G*(len G-'1,1)`1 by A161,XXREAL_0:2;
          then p`1 <= E-bound C by A19,JORDAN8:15;
          then
A163:     p`1 <= (E-max C)`1 by EUCLID:56;
          p`1 >= (E-max C)`1 by A159,TOPREAL1:def 13;
          then
A164:     p`1 = (E-max C)`1 by A163,XXREAL_0:1;
          p`2 = (E-max C)`2 by A159,TOPREAL1:def 13;
          then p = E-max C by A164,TOPREAL3:11;
          hence contradiction by A5,A7,A79,A126,A160,XBOOLE_0:3;
        end;
        suppose east_halfline E-max C meets L~do;
          then consider p be set such that
A165:     p in east_halfline E-max C and
A166:     p in L~do by XBOOLE_0:3;
          reconsider p as Point of TOP-REAL 2 by A165;
          p in L~LS by A51,A166;
          then p in east_halfline E-max C /\ L~Cage(C,n)
          by A136,A165,XBOOLE_0:def 3;
          then
A167:     p`1 = Ebo by A152,JORDAN1A:104;
A168:     (E-max C)`2 = p`2 by A165,TOPREAL1:def 13;
          set RC = Rotate(Cage(C,n),Emax);
A169:     E-max C in right_cell(RC,1) by JORDAN1I:9;
A170:     1+1 <= len LS by A22,XXREAL_0:2;
          LS = RC-:Wmin by JORDAN1G:26;
          then
A171:     LSeg(LS,1) = LSeg(RC,1) by A170,SPPOL_2:9;
A172:     L~RC = L~Cage(C,n) by REVROT_1:33;
A173:     len RC = len Cage(C,n) by REVROT_1:14;
A174:     GoB RC = GoB Cage(C,n) by REVROT_1:28
            .= G by JORDAN1H:52;
A175:     Emax in rng Cage(C,n) by SPRECT_2:50;
A176:     RC is_sequence_on G by A132,REVROT_1:34;
A177:     RC/.1 = E-max L~RC by A172,A175,FINSEQ_6:98;
          then consider ii,jj be Element of NAT such that
A178:     [ii,jj+1] in Indices G and
A179:     [ii,jj] in Indices G and
A180:     RC/.1 = G*(ii,jj+1) and
A181:     RC/.(1+1) = G*(ii,jj) by A88,A173,A176,JORDAN1I:25;
          consider jj2 be Element of NAT such that
A182:     1 <= jj2 & jj2 <= width G and
A183:     Emax = G*(len G,jj2) by JORDAN1D:29;
A184:     len G >= 4 by JORDAN8:13;
          then len G >= 1 by XXREAL_0:2;
          then [len G,jj2] in Indices G by A182,MATRIX_1:37;
          then
A185:     ii = len G by A172,A177,A178,A180,A183,GOBOARD1:21;
A186:     1 <= ii & ii <= len G & 1 <= jj+1 & jj+1 <= width G
          by A178,GOBOARD5:1;
A187:     1 <= ii & ii <= len G & 1 <= jj & jj <= width G by A179,GOBOARD5:1;
A188:     ii+1 <> ii;
          jj+1+1 <> jj;
          then
A189:     right_cell(RC,1) = cell(G,ii-'1,jj)
          by A88,A173,A174,A178,A179,A180,A181,A188,GOBOARD5:def 6;
A190:     ii-'1+1 = ii by A186,BINARITH:53;
          ii-1 >= 4-1 by A184,A185,XREAL_1:11;
          then
A191:     ii-1 >= 1 by XXREAL_0:2;
          then
A192:     1 <= ii-'1 by BINARITH:def 3;
A193:     G*(ii-'1,jj)`2 <= p`2 & p`2 <= G*(ii-'1,jj+1)`2
          by A168,A169,A186,A187,A189,A190,A191,JORDAN9:19;
A194:     ii-'1 < len G by A186,A190,NAT_1:13;
          then
A195:     G*(ii-'1,jj)`2 = G*(1,jj)`2 by A187,A192,GOBOARD5:2
            .= G*(ii,jj)`2 by A187,GOBOARD5:2;
A196:     G*(ii-'1,jj+1)`2 = G*(1,jj+1)`2 by A186,A192,A194,GOBOARD5:2
            .= G*(ii,jj+1)`2 by A186,GOBOARD5:2;
          G*(len G,jj)`1 = Ebo & Ebo = G*(len G,jj+1)`1
          by A15,A186,A187,JORDAN1A:92;
          then p in LSeg(RC/.1,RC/.(1+1))
          by A167,A180,A181,A185,A193,A195,A196,GOBOARD7:8;
          then
A197:     p in LSeg(LS,1) by A88,A171,A173,TOPREAL1:def 5;
A198:     p in LSeg(do,Index(p,do)) by A166,JORDAN3:42;
A199:     do = mid(LS,Gij..LS,len LS) by A35,JORDAN1J:37;
A200:     1<=Gij..LS & Gij..LS<=len LS by A35,FINSEQ_4:31;
          Gij..LS <> len LS by A27,A35,FINSEQ_4:29;
          then
A201:     Gij..LS < len LS by A200,REAL_1:def 5;
A202:     1<=Index(p,do) & Index(p,do) < len do by A166,JORDAN3:41;
A203:     Index(Gij,LS)+1 = Gij..LS by A30,A35,JORDAN1J:56;
          consider t be Nat such that
A204:     t in dom LS and
A205:     LS.t = Gij by A35,FINSEQ_2:11;
A206:     1 <= t & t <= len LS by A204,FINSEQ_3:27;
          then 1 < t by A30,A205,REAL_1:def 5;
          then Index(Gij,LS)+1 = t by A205,A206,JORDAN3:45;
          then
A207:     len L_Cut(LS,Gij) = len LS-Index(Gij,LS) by A8,A205,JORDAN3:61;
          set tt = Index(p,do)+(Gij..LS)-'1;
A208:     1<=Index(Gij,LS) & 0+Index(Gij,LS) < len LS by A8,JORDAN3:41;
          then
A209:     len LS-Index(Gij,LS) > 0 by XREAL_1:22;
          Index(p,do) < len LS-'Index(Gij,LS) by A202,A207,BINARITH:def 3;
          then Index(p,do)+1 <= len LS-'Index(Gij,LS) by NAT_1:13;
          then Index(p,do) <= len LS-'Index(Gij,LS)-1 by XREAL_1:21;
          then Index(p,do) <= len LS-Index(Gij,LS)-1 by A209,BINARITH:def 3;
          then Index(p,do) <= len LS-Gij..LS by A203;
          then Index(p,do) <= len LS-'Gij..LS by BINARITH:def 3;
          then Index(p,do) < len LS-'(Gij..LS)+1 by NAT_1:13;
          then
A210:     LSeg(mid(LS,Gij..LS,len LS),Index(p,do)) =
          LSeg(LS,Index(p,do)+(Gij..LS)-'1) by A200,A201,A202,JORDAN4:31;
A211:     1+1 <= Gij..LS by A203,A208,XREAL_1:9;
          then Index(p,do)+Gij..LS >= 1+1+1 by A202,XREAL_1:9;
          then Index(p,do)+Gij..LS-1 >= 1+1+1-1 by XREAL_1:11;
          then
A212:     tt >= 1+1 by BINARITH:def 3;
A213:     2 in dom LS by A170,FINSEQ_3:27;
          now per cases by A212,REAL_1:def 5;
            suppose tt > 1+1;
              then LSeg(LS,1) misses LSeg(LS,tt) by TOPREAL1:def 9;
              hence contradiction by A197,A198,A199,A210,XBOOLE_0:3;
            end;
            suppose
A214:         tt = 1+1;
              then LSeg(LS,1) /\ LSeg(LS,tt) = {LS/.2} by A22,TOPREAL1:def 8;
              then p in {LS/.2} by A197,A198,A199,A210,XBOOLE_0:def 3;
              then
A215:         p = LS/.2 by TARSKI:def 1;
              then
A216:         p..LS = 2 by A213,FINSEQ_5:44;
              1+1 = Index(p,do)+(Gij..LS)-1 by A214,BINARITH:def 3;
              then 1+1+1 = Index(p,do)+(Gij..LS);
              then
A217:         Gij..LS = 2 by A202,A211,JORDAN1E:10;
              p in rng LS by A213,A215,PARTFUN2:4;
              then p = Gij by A35,A216,A217,FINSEQ_5:10;
              then Gij`1 = Ebo by A215,JORDAN1G:40;
              then Gij`1 = G*(len G,j)`1 by A10,A15,JORDAN1A:92;
              hence contradiction by A1,A12,A62,JORDAN1G:7;
            end;
          end;
          hence contradiction;
        end;
      end;
      then east_halfline E-max C c= (L~godo)` by SUBSET_1:43;
      then consider W be Subset of TOP-REAL 2 such that
A218: W is_a_component_of (L~godo)` and
A219: east_halfline E-max C c= W by GOBOARD9:5;
      W is not Bounded by A219,JORDAN2C:16,129;
      then W is_outside_component_of L~godo by A218,JORDAN2C:def 4;
      then W c= UBD L~godo by JORDAN2C:27;
      then
A220: east_halfline E-max C c= UBD L~godo by A219,XBOOLE_1:1;
      E-max C in east_halfline E-max C by TOPREAL1:45;
      then E-max C in UBD L~godo by A220;
      then E-max C in LeftComp godo by GOBRD14:46;
      then LA meets L~godo by A125,A126,A134,A141,JORDAN1J:36;
      then
A221: LA meets (L~go \/ L~pion1) or LA meets L~do by A135,XBOOLE_1:70;
A222: LA c= C by JORDAN6:76;
      now per cases by A221,XBOOLE_1:70;
        suppose LA meets L~go;
          then LA meets L~Cage(C,n) by A44,A136,XBOOLE_1:1,63;
          hence contradiction by A222,JORDAN10:5,XBOOLE_1:63;
        end;
        suppose LA meets L~pion1;
          hence contradiction by A5,A7,A79;
        end;
        suppose LA meets L~do;
          then LA meets L~Cage(C,n) by A51,A136,XBOOLE_1:1,63;
          hence contradiction by A222,JORDAN10:5,XBOOLE_1:63;
        end;
      end;
      hence contradiction;
    end;
    suppose Gij`1 = Gik`1;
      then
A223: i1 = i2 by A12,A13,JORDAN1G:7;
      then poz = {Gi1k} by TOPREAL1:7;
      then poz c= pio by A75,ZFMISC_1:37;
      then pio \/ poz = pio by XBOOLE_1:12;
      hence contradiction by A1,A2,A3,A4,A5,A223,JORDAN1J:58;
    end;
    suppose Gij`2 = Gik`2;
      then
A224: j = k by A12,A13,JORDAN1G:6;
      then pio = {Gi1k} by TOPREAL1:7;
      then pio c= poz by A76,ZFMISC_1:37;
      then pio \/ poz = poz by XBOOLE_1:12;
      hence contradiction by A1,A2,A3,A4,A5,A224,Th38;
    end;
  end;
  hence contradiction;
end;

theorem Th50:
  for C be Simple_closed_curve for i1,i2,j,k be Element of NAT holds
  1 < i1 & i1 < len Gauge(C,n+1) & 1 < i2 & i2 < len Gauge(C,n+1) &
  1 <= j & j <= k & k <= width Gauge(C,n+1) &
  Gauge(C,n+1)*(i1,k) in Upper_Arc L~Cage(C,n+1) &
  Gauge(C,n+1)*(i2,j) in Lower_Arc L~Cage(C,n+1) implies
  LSeg(Gauge(C,n+1)*(i2,j),Gauge(C,n+1)*(i2,k)) \/
  LSeg(Gauge(C,n+1)*(i2,k),Gauge(C,n+1)*(i1,k)) meets Upper_Arc C
proof
  let C be Simple_closed_curve;
  let i1,i2,j,k be Element of NAT;
  set G=Gauge(C,n+1);
  assume that
A1: 1 < i1 & i1 < len G and
A2: 1 < i2 & i2 < len G and
A3: 1 <= j & j <= k & k <= width G and
A4: G*(i1,k) in Upper_Arc L~Cage(C,n+1) and
A5: G*(i2,j) in Lower_Arc L~Cage(C,n+1);
A6: Upper_Arc L~Cage(C,n+1) = L~Upper_Seq(C,n+1) by JORDAN1G:63;
A7: Lower_Arc L~Cage(C,n+1) = L~Lower_Seq(C,n+1) by JORDAN1G:64;
A8: Upper_Seq(C,n+1) is_sequence_on G by JORDAN1G:4;
A9: Lower_Seq(C,n+1) is_sequence_on G by JORDAN1G:5;
A10: 1 <= j & j <= width G by A3,XXREAL_0:2;
  then
A11: [i2,j] in Indices G by A2,MATRIX_1:37;
A12: 1 <= k & k <= width G by A3,XXREAL_0:2;
  then
A13: [i2,k] in Indices G by A2,MATRIX_1:37;
  G*(i2,j)`1 = G*(i2,1)`1 by A2,A10,GOBOARD5:3
    .= G*(i2,k)`1 by A2,A12,GOBOARD5:3;
  then
A14: LSeg(G*(i2,j),G*(i2,k)) is vertical by SPPOL_1:37;
  G*(i2,k)`2 = G*(1,k)`2 by A2,A12,GOBOARD5:2
    .= G*(i1,k)`2 by A1,A12,GOBOARD5:2;
  then
A15: LSeg(G*(i2,k),G*(i1,k)) is horizontal by SPPOL_1:36;
A16: [i2,k] in Indices G by A2,A12,MATRIX_1:37;
A17: [i1,k] in Indices G by A1,A12,MATRIX_1:37;
  now per cases;
    suppose
A18:  LSeg(G*(i2,j),G*(i2,k)) meets Upper_Arc L~Cage(C,n+1);
      then consider m be Element of NAT such that
A19:  j <= m & m <= k and
A20:  G*(i2,m)`2 = inf(proj2.:(LSeg(G*(i2,j),G*(i2,k))
      /\ L~Upper_Seq(C,n+1))) by A3,A6,A8,A11,A13,JORDAN1F:1;
A21:  1 <= m & m <= width G by A3,A19,XXREAL_0:2;
      set X = LSeg(G*(i2,j),G*(i2,k)) /\ L~Upper_Seq(C,n+1);
A22:  G*(i2,m)`1 = G*(i2,1)`1 by A2,A21,GOBOARD5:3;
      then
A23:  |[G*(i2,1)`1,inf(proj2.:X)]| = G*(i2,m) by A20,EUCLID:57;
      then
A24:  G*(i2,j)`1 = |[G*(i2,1)`1,inf(proj2.:X)]|`1 by A2,A10,A22,GOBOARD5:3;
      ex x be set st x in LSeg(G*(i2,j),G*(i2,k)) &
      x in L~Upper_Seq(C,n+1) by A6,A18,XBOOLE_0:3;
      then reconsider X1=X as non empty compact Subset of TOP-REAL 2
      by PSCOMP_1:64,XBOOLE_0:def 3;
      consider pp be set such that
A25:  pp in S-most X1 by XBOOLE_0:def 1;
      reconsider pp as Point of TOP-REAL 2 by A25;
A26:  pp in X by A25,XBOOLE_0:def 3;
      then
A27:  pp in L~Upper_Seq(C,n+1) by XBOOLE_0:def 3;
      pp in LSeg(G*(i2,j),G*(i2,k)) by A26,XBOOLE_0:def 3;
      then
A28:  pp`1 = |[G*(i2,1)`1,inf(proj2.:X)]|`1 by A14,A24,SPPOL_1:64;
      |[G*(i2,1)`1,inf(proj2.:X)]|`2 = S-bound X by A20,A23,SPRECT_1:49
        .= (S-min X)`2 by EUCLID:56
        .= pp`2 by A25,PSCOMP_1:118;
      then G*(i2,m) in Upper_Arc L~Cage(C,n+1) by A6,A23,A27,A28,TOPREAL3:11;
      then LSeg(G*(i2,j),G*(i2,m)) meets Upper_Arc C by A2,A3,A5,A19,A21,Th26;
      then LSeg(G*(i2,j),G*(i2,k)) meets Upper_Arc C by A2,A3,A19,Th7,
      XBOOLE_1:63;
      hence LSeg(G*(i2,j),G*(i2,k)) \/
      LSeg(G*(i2,k),G*(i1,k)) meets Upper_Arc C by XBOOLE_1:70;
    end;
    suppose
A29:  LSeg(G*(i2,k),G*(i1,k)) meets Lower_Arc L~Cage(C,n+1) & i2 <= i1;
      then consider m be Element of NAT such that
A30:  i2 <= m & m <= i1 and
A31:  G*(m,k)`1 = sup(proj1.:(LSeg(G*(i2,k),G*(i1,k))
      /\ L~Lower_Seq(C,n+1))) by A7,A9,A16,A17,JORDAN1F:4;
A32:  1 < m & m < len G by A1,A2,A30,XXREAL_0:2;
      set X = LSeg(G*(i2,k),G*(i1,k)) /\ L~Lower_Seq(C,n+1);
A33:  G*(m,k)`2 = G*(1,k)`2 by A12,A32,GOBOARD5:2;
      then
A34:  |[sup(proj1.:X),G*(1,k)`2]| = G*(m,k) by A31,EUCLID:57;
      then
A35:  G*(i2,k)`2 = |[sup(proj1.:X),G*(1,k)`2]|`2 by A2,A12,A33,GOBOARD5:2;
      ex x be set st x in LSeg(G*(i2,k),G*(i1,k)) &
      x in L~Lower_Seq(C,n+1) by A7,A29,XBOOLE_0:3;
      then reconsider X1=X as non empty compact Subset of TOP-REAL 2
      by PSCOMP_1:64,XBOOLE_0:def 3;
      consider pp be set such that
A36:  pp in E-most X1 by XBOOLE_0:def 1;
      reconsider pp as Point of TOP-REAL 2 by A36;
A37:  pp in X by A36,XBOOLE_0:def 3;
      then
A38:  pp in L~Lower_Seq(C,n+1) by XBOOLE_0:def 3;
      pp in LSeg(G*(i2,k),G*(i1,k)) by A37,XBOOLE_0:def 3;
      then
A39:  pp`2 = |[sup(proj1.:X),G*(1,k)`2]|`2 by A15,A35,SPPOL_1:63;
      |[sup(proj1.:X),G*(1,k)`2]|`1 = E-bound X by A31,A34,SPRECT_1:51
        .= (E-min X)`1 by EUCLID:56
        .= pp`1 by A36,PSCOMP_1:108;
      then G*(m,k) in Lower_Arc L~Cage(C,n+1) by A7,A34,A38,A39,TOPREAL3:11;
      then LSeg(G*(m,k),G*(i1,k)) meets Upper_Arc C by A1,A4,A12,A30,A32,Th35;
      then LSeg(G*(i2,k),G*(i1,k)) meets Upper_Arc C by A1,A2,A12,A30,Th8,
      XBOOLE_1:63;
      hence LSeg(G*(i2,j),G*(i2,k)) \/
      LSeg(G*(i2,k),G*(i1,k)) meets Upper_Arc C by XBOOLE_1:70;
    end;
    suppose
A40:  LSeg(G*(i2,k),G*(i1,k)) meets Lower_Arc L~Cage(C,n+1) & i1 < i2;
      then consider m be Element of NAT such that
A41:  i1 <= m & m <= i2 and
A42:  G*(m,k)`1 = inf(proj1.:(LSeg(G*(i1,k),G*(i2,k))
      /\ L~Lower_Seq(C,n+1))) by A7,A9,A16,A17,JORDAN1F:3;
A43:  1 < m & m < len G by A1,A2,A41,XXREAL_0:2;
      set X = LSeg(G*(i1,k),G*(i2,k)) /\ L~Lower_Seq(C,n+1);
A44:  G*(m,k)`2 = G*(1,k)`2 by A12,A43,GOBOARD5:2;
      then
A45:  |[inf(proj1.:X),G*(1,k)`2]| = G*(m,k) by A42,EUCLID:57;
      then
A46:  G*(i1,k)`2 = |[inf(proj1.:X),G*(1,k)`2]|`2 by A1,A12,A44,GOBOARD5:2;
      ex x be set st x in LSeg(G*(i1,k),G*(i2,k)) &
      x in L~Lower_Seq(C,n+1) by A7,A40,XBOOLE_0:3;
      then reconsider X1=X as non empty compact Subset of TOP-REAL 2
      by PSCOMP_1:64,XBOOLE_0:def 3;
      consider pp be set such that
A47:  pp in W-most X1 by XBOOLE_0:def 1;
      reconsider pp as Point of TOP-REAL 2 by A47;
A48:  pp in X by A47,XBOOLE_0:def 3;
      then
A49:  pp in L~Lower_Seq(C,n+1) by XBOOLE_0:def 3;
      pp in LSeg(G*(i1,k),G*(i2,k)) by A48,XBOOLE_0:def 3;
      then
A50:  pp`2 = |[inf(proj1.:X),G*(1,k)`2]|`2 by A15,A46,SPPOL_1:63;
      |[inf(proj1.:X),G*(1,k)`2]|`1 = W-bound X by A42,A45,SPRECT_1:48
        .= (W-min X)`1 by EUCLID:56
        .= pp`1 by A47,PSCOMP_1:88;
      then G*(m,k) in Lower_Arc L~Cage(C,n+1) by A7,A45,A49,A50,TOPREAL3:11;
      then LSeg(G*(i1,k),G*(m,k)) meets Upper_Arc C by A1,A4,A12,A41,A43,Th43;
      then LSeg(G*(i1,k),G*(i2,k)) meets Upper_Arc C by A1,A2,A12,A41,Th8,
      XBOOLE_1:63;
      hence LSeg(G*(i2,j),G*(i2,k)) \/
      LSeg(G*(i2,k),G*(i1,k)) meets Upper_Arc C by XBOOLE_1:70;
    end;
    suppose
A51:  LSeg(G*(i2,j),G*(i2,k)) misses Upper_Arc L~Cage(C,n+1) &
      LSeg(Gauge(C,n+1)*(i2,k),Gauge(C,n+1)*(i1,k))
      misses Lower_Arc L~Cage(C,n+1);
      consider j1 be Element of NAT such that
A52:  j <= j1 & j1 <= k and
A53:  LSeg(G*(i2,j1),G*(i2,k)) /\ L~Lower_Seq(C,n+1) =
      {G*(i2,j1)} by A2,A3,A5,A7,Th11;
      G*(i2,j1) in LSeg(G*(i2,j1),G*(i2,k)) /\
      L~Lower_Seq(C,n+1) by A53,TARSKI:def 1;
      then
A54:  G*(i2,j1) in L~Lower_Seq(C,n+1) by XBOOLE_0:def 3;
A55:  1 <= j1 & j1 <= width G by A3,A52,XXREAL_0:2;
      now per cases;
        suppose i2 <= i1;
          then consider i3 be Element of NAT such that
A56:      i2 <= i3 & i3 <= i1 and
A57:      LSeg(G*(i2,k),G*(i3,k)) /\ L~Upper_Seq(C,n+1) = {G*(i3,k)}
          by A1,A2,A4,A6,A12,Th15;
A58:      1 < i3 & i3 < len G by A1,A2,A56,XXREAL_0:2;
          G*(i3,k) in LSeg(G*(i2,k),G*(i3,k)) /\ L~Upper_Seq(C,n+1)
          by A57,TARSKI:def 1;
          then
A59:      G*(i3,k) in L~Upper_Seq(C,n+1) by XBOOLE_0:def 3;
A60:      LSeg(G*(i2,j1),G*(i2,k)) c=
          LSeg(G*(i2,j),G*(i2,k)) by A2,A3,A52,Th7;
A61:      LSeg(G*(i2,k),G*(i3,k)) c= LSeg(G*(i2,k),G*(i1,k))
          by A1,A2,A12,A56,Th8;
          then
A62:      LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k)) c=

LSeg(G*(i2,j),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i1,k)) by A60,XBOOLE_1:13;
A63:      (LSeg(G*(i2,j1),G*(i2,k)) \/
          LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1) = {G*(i3,k)}
          proof
            thus (LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1) c= {G*(i3,k)}
            proof
              let x be set;
              assume x in (LSeg(G*(i2,j1),G*(i2,k)) \/
              LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1);
              then x in LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k)) &
              x in L~Upper_Seq(C,n+1) by XBOOLE_0:def 3;
              then (x in LSeg(G*(i2,j1),G*(i2,k)) or
              x in LSeg(G*(i2,k),G*(i3,k))) &
              x in L~Upper_Seq(C,n+1) by XBOOLE_0:def 2;
              hence x in {G*(i3,k)} by A6,A51,A57,A60,XBOOLE_0:3,def 3;
            end;
            let x be set;
            assume x in {G*(i3,k)};
            then
A64:        x = G*(i3,k) by TARSKI:def 1;
            G*(i3,k) in LSeg(G*(i2,k),G*(i3,k)) by TOPREAL1:6;
            then G*(i3,k) in LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k)) by XBOOLE_0:def 2;
            hence x in (LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1)
            by A59,A64,XBOOLE_0:def 3;
          end;
          (LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k))) /\
          L~Lower_Seq(C,n+1) = {G*(i2,j1)}
          proof
            thus (LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k))) /\
            L~Lower_Seq(C,n+1) c= {G*(i2,j1)}
            proof
              let x be set;
              assume x in (LSeg(G*(i2,j1),G*(i2,k)) \/
              LSeg(G*(i2,k),G*(i3,k))) /\ L~Lower_Seq(C,n+1);
              then x in LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k)) &
              x in L~Lower_Seq(C,n+1) by XBOOLE_0:def 3;
              then (x in LSeg(G*(i2,j1),G*(i2,k)) or
              x in LSeg(G*(i2,k),G*(i3,k))) &
              x in L~Lower_Seq(C,n+1) by XBOOLE_0:def 2;
              hence x in {G*(i2,j1)} by A7,A51,A53,A61,XBOOLE_0:3,def 3;
            end;
            let x be set;
            assume x in {G*(i2,j1)};
            then
A65:        x = G*(i2,j1) by TARSKI:def 1;
            G*(i2,j1) in LSeg(G*(i2,j1),G*(i2,k)) by TOPREAL1:6;
            then G*(i2,j1) in LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k)) by XBOOLE_0:def 2;
            hence x in (LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k))) /\ L~Lower_Seq(C,n+1)
            by A54,A65,XBOOLE_0:def 3;
          end;
          hence LSeg(G*(i2,j),G*(i2,k)) \/

LSeg(G*(i2,k),G*(i1,k)) meets Upper_Arc C by A2,A3,A52,A55,A56,A58,A62,A63,Th46
          ,XBOOLE_1:63;
        end;
        suppose i1 < i2;
          then consider i3 be Element of NAT such that
A66:      i1 <= i3 & i3 <= i2 and
A67:      LSeg(G*(i3,k),G*(i2,k)) /\ L~Upper_Seq(C,n+1) = {G*(i3,k)}
          by A1,A2,A4,A6,A12,Th20;
A68:      1 < i3 & i3 < len G by A1,A2,A66,XXREAL_0:2;
          G*(i3,k) in LSeg(G*(i2,k),G*(i3,k)) /\ L~Upper_Seq(C,n+1)
          by A67,TARSKI:def 1;
          then
A69:      G*(i3,k) in L~Upper_Seq(C,n+1) by XBOOLE_0:def 3;
A70:      LSeg(G*(i2,j1),G*(i2,k)) c=
          LSeg(G*(i2,j),G*(i2,k)) by A2,A3,A52,Th7;
A71:      LSeg(G*(i2,k),G*(i3,k)) c= LSeg(G*(i2,k),G*(i1,k))
          by A1,A2,A12,A66,Th8;
          then
A72:      LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k)) c=

LSeg(G*(i2,j),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i1,k)) by A70,XBOOLE_1:13;
A73:      (LSeg(G*(i2,j1),G*(i2,k)) \/
          LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1) = {G*(i3,k)}
          proof
            thus (LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1) c= {G*(i3,k)}
            proof
              let x be set;
              assume x in (LSeg(G*(i2,j1),G*(i2,k)) \/
              LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1);
              then x in LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k)) &
              x in L~Upper_Seq(C,n+1) by XBOOLE_0:def 3;

then (x in LSeg(G*(i2,j1),G*(i2,k)) or x in LSeg(G*(i2,k),G*(i3,k)))
              & x in L~Upper_Seq(C,n+1) by XBOOLE_0:def 2;
              hence x in {G*(i3,k)} by A6,A51,A67,A70,XBOOLE_0:3,def 3;
            end;
            let x be set;
            assume x in {G*(i3,k)};
            then
A74:        x = G*(i3,k) by TARSKI:def 1;
            G*(i3,k) in LSeg(G*(i2,k),G*(i3,k)) by TOPREAL1:6;
            then G*(i3,k) in LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k)) by XBOOLE_0:def 2;
            hence x in (LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1)
            by A69,A74,XBOOLE_0:def 3;
          end;
          (LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k))) /\
          L~Lower_Seq(C,n+1) = {G*(i2,j1)}
          proof
            thus (LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k))) /\
            L~Lower_Seq(C,n+1) c= {G*(i2,j1)}
            proof
              let x be set;

assume x in (LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k))) /\
              L~Lower_Seq(C,n+1);
              then x in LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k)) &
              x in L~Lower_Seq(C,n+1) by XBOOLE_0:def 3;

then (x in LSeg(G*(i2,j1),G*(i2,k)) or x in LSeg(G*(i2,k),G*(i3,k)))
              & x in L~Lower_Seq(C,n+1) by XBOOLE_0:def 2;
              hence x in {G*(i2,j1)} by A7,A51,A53,A71,XBOOLE_0:3,def 3;
            end;
            let x be set;
            assume x in {G*(i2,j1)};
            then
A75:        x = G*(i2,j1) by TARSKI:def 1;
            G*(i2,j1) in LSeg(G*(i2,j1),G*(i2,k)) by TOPREAL1:6;
            then G*(i2,j1) in LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k)) by XBOOLE_0:def 2;
            hence x in (LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k))) /\ L~Lower_Seq(C,n+1)
            by A54,A75,XBOOLE_0:def 3;
          end;
          hence LSeg(G*(i2,j),G*(i2,k)) \/

LSeg(G*(i2,k),G*(i1,k)) meets Upper_Arc C by A2,A3,A52,A55,A66,A68,A72,A73,Th48
          ,XBOOLE_1:63;
        end;
      end;
      hence LSeg(G*(i2,j),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i1,k))
      meets Upper_Arc C;
    end;
  end;
  hence LSeg(G*(i2,j),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i1,k)) meets Upper_Arc C;
end;

theorem Th51:
  for C be Simple_closed_curve for i1,i2,j,k be Element of NAT holds
  1 < i1 & i1 < len Gauge(C,n+1) & 1 < i2 & i2 < len Gauge(C,n+1) &
  1 <= j & j <= k & k <= width Gauge(C,n+1) &
  Gauge(C,n+1)*(i1,k) in Upper_Arc L~Cage(C,n+1) &
  Gauge(C,n+1)*(i2,j) in Lower_Arc L~Cage(C,n+1) implies
  LSeg(Gauge(C,n+1)*(i2,j),Gauge(C,n+1)*(i2,k)) \/
  LSeg(Gauge(C,n+1)*(i2,k),Gauge(C,n+1)*(i1,k)) meets Lower_Arc C
proof
  let C be Simple_closed_curve;
  let i1,i2,j,k be Element of NAT;
  set G=Gauge(C,n+1);
  assume that
A1: 1 < i1 & i1 < len G and
A2: 1 < i2 & i2 < len G and
A3: 1 <= j & j <= k & k <= width G and
A4: G*(i1,k) in Upper_Arc L~Cage(C,n+1) and
A5: G*(i2,j) in Lower_Arc L~Cage(C,n+1);
A6: Upper_Arc L~Cage(C,n+1) = L~Upper_Seq(C,n+1) by JORDAN1G:63;
A7: Lower_Arc L~Cage(C,n+1) = L~Lower_Seq(C,n+1) by JORDAN1G:64;
A8: Upper_Seq(C,n+1) is_sequence_on G by JORDAN1G:4;
A9: Lower_Seq(C,n+1) is_sequence_on G by JORDAN1G:5;
A10: 1 <= j & j <= width G by A3,XXREAL_0:2;
  then
A11: [i2,j] in Indices G by A2,MATRIX_1:37;
A12: 1 <= k & k <= width G by A3,XXREAL_0:2;
  then
A13: [i2,k] in Indices G by A2,MATRIX_1:37;
  G*(i2,j)`1 = G*(i2,1)`1 by A2,A10,GOBOARD5:3
    .= G*(i2,k)`1 by A2,A12,GOBOARD5:3;
  then
A14: LSeg(G*(i2,j),G*(i2,k)) is vertical by SPPOL_1:37;
  G*(i2,k)`2 = G*(1,k)`2 by A2,A12,GOBOARD5:2
    .= G*(i1,k)`2 by A1,A12,GOBOARD5:2;
  then
A15: LSeg(G*(i2,k),G*(i1,k)) is horizontal by SPPOL_1:36;
A16: [i2,k] in Indices G by A2,A12,MATRIX_1:37;
A17: [i1,k] in Indices G by A1,A12,MATRIX_1:37;
  now per cases;
    suppose
A18:  LSeg(G*(i2,j),G*(i2,k)) meets Upper_Arc L~Cage(C,n+1);
      then consider m be Element of NAT such that
A19:  j <= m & m <= k and
A20:  G*(i2,m)`2 = inf(proj2.:(LSeg(G*(i2,j),G*(i2,k))
      /\ L~Upper_Seq(C,n+1))) by A3,A6,A8,A11,A13,JORDAN1F:1;
A21:  1 <= m & m <= width G by A3,A19,XXREAL_0:2;
      set X = LSeg(G*(i2,j),G*(i2,k)) /\ L~Upper_Seq(C,n+1);
A22:  G*(i2,m)`1 = G*(i2,1)`1 by A2,A21,GOBOARD5:3;
      then
A23:  |[G*(i2,1)`1,inf(proj2.:X)]| = G*(i2,m) by A20,EUCLID:57;
      then
A24:  G*(i2,j)`1 = |[G*(i2,1)`1,inf(proj2.:X)]|`1 by A2,A10,A22,GOBOARD5:3;
      ex x be set st x in LSeg(G*(i2,j),G*(i2,k)) &
      x in L~Upper_Seq(C,n+1) by A6,A18,XBOOLE_0:3;
      then reconsider X1=X as non empty compact Subset of TOP-REAL 2
      by PSCOMP_1:64,XBOOLE_0:def 3;
      consider pp be set such that
A25:  pp in S-most X1 by XBOOLE_0:def 1;
      reconsider pp as Point of TOP-REAL 2 by A25;
A26:  pp in X by A25,XBOOLE_0:def 3;
      then
A27:  pp in L~Upper_Seq(C,n+1) by XBOOLE_0:def 3;
      pp in LSeg(G*(i2,j),G*(i2,k)) by A26,XBOOLE_0:def 3;
      then
A28:  pp`1 = |[G*(i2,1)`1,inf(proj2.:X)]|`1 by A14,A24,SPPOL_1:64;
      |[G*(i2,1)`1,inf(proj2.:X)]|`2 = S-bound X by A20,A23,SPRECT_1:49
        .= (S-min X)`2 by EUCLID:56
        .= pp`2 by A25,PSCOMP_1:118;
      then G*(i2,m) in Upper_Arc L~Cage(C,n+1) by A6,A23,A27,A28,TOPREAL3:11;
      then LSeg(G*(i2,j),G*(i2,m)) meets Lower_Arc C by A2,A3,A5,A19,A21,Th25;
      then LSeg(G*(i2,j),G*(i2,k)) meets Lower_Arc C by A2,A3,A19,Th7,
      XBOOLE_1:63;
      hence LSeg(G*(i2,j),G*(i2,k)) \/
      LSeg(G*(i2,k),G*(i1,k)) meets Lower_Arc C by XBOOLE_1:70;
    end;
    suppose
A29:  LSeg(G*(i2,k),G*(i1,k)) meets Lower_Arc L~Cage(C,n+1) & i2 <= i1;
      then consider m be Element of NAT such that
A30:  i2 <= m & m <= i1 and
A31:  G*(m,k)`1 = sup(proj1.:(LSeg(G*(i2,k),G*(i1,k))
      /\ L~Lower_Seq(C,n+1))) by A7,A9,A16,A17,JORDAN1F:4;
A32:  1 < m & m < len G by A1,A2,A30,XXREAL_0:2;
      set X = LSeg(G*(i2,k),G*(i1,k)) /\ L~Lower_Seq(C,n+1);
A33:  G*(m,k)`2 = G*(1,k)`2 by A12,A32,GOBOARD5:2;
      then
A34:  |[sup(proj1.:X),G*(1,k)`2]| = G*(m,k) by A31,EUCLID:57;
      then
A35:  G*(i2,k)`2 = |[sup(proj1.:X),G*(1,k)`2]|`2 by A2,A12,A33,GOBOARD5:2;
      ex x be set st x in LSeg(G*(i2,k),G*(i1,k)) &
      x in L~Lower_Seq(C,n+1) by A7,A29,XBOOLE_0:3;
      then reconsider X1=X as non empty compact Subset of TOP-REAL 2
      by PSCOMP_1:64,XBOOLE_0:def 3;
      consider pp be set such that
A36:  pp in E-most X1 by XBOOLE_0:def 1;
      reconsider pp as Point of TOP-REAL 2 by A36;
A37:  pp in X by A36,XBOOLE_0:def 3;
      then
A38:  pp in L~Lower_Seq(C,n+1) by XBOOLE_0:def 3;
      pp in LSeg(G*(i2,k),G*(i1,k)) by A37,XBOOLE_0:def 3;
      then
A39:  pp`2 = |[sup(proj1.:X),G*(1,k)`2]|`2 by A15,A35,SPPOL_1:63;
      |[sup(proj1.:X),G*(1,k)`2]|`1 = E-bound X by A31,A34,SPRECT_1:51
        .= (E-min X)`1 by EUCLID:56
        .= pp`1 by A36,PSCOMP_1:108;
      then G*(m,k) in Lower_Arc L~Cage(C,n+1) by A7,A34,A38,A39,TOPREAL3:11;
      then LSeg(G*(m,k),G*(i1,k)) meets Lower_Arc C by A1,A4,A12,A30,A32,Th34;
      then LSeg(G*(i2,k),G*(i1,k)) meets Lower_Arc C by A1,A2,A12,A30,Th8,
      XBOOLE_1:63;
      hence LSeg(G*(i2,j),G*(i2,k)) \/
      LSeg(G*(i2,k),G*(i1,k)) meets Lower_Arc C by XBOOLE_1:70;
    end;
    suppose
A40:  LSeg(G*(i2,k),G*(i1,k)) meets Lower_Arc L~Cage(C,n+1) & i1 < i2;
      then consider m be Element of NAT such that
A41:  i1 <= m & m <= i2 and
A42:  G*(m,k)`1 = inf(proj1.:(LSeg(G*(i1,k),G*(i2,k))
      /\ L~Lower_Seq(C,n+1))) by A7,A9,A16,A17,JORDAN1F:3;
A43:  1 < m & m < len G by A1,A2,A41,XXREAL_0:2;
      set X = LSeg(G*(i1,k),G*(i2,k)) /\ L~Lower_Seq(C,n+1);
A44:  G*(m,k)`2 = G*(1,k)`2 by A12,A43,GOBOARD5:2;
      then
A45:  |[inf(proj1.:X),G*(1,k)`2]| = G*(m,k) by A42,EUCLID:57;
      then
A46:  G*(i1,k)`2 = |[inf(proj1.:X),G*(1,k)`2]|`2 by A1,A12,A44,GOBOARD5:2;
      ex x be set st x in LSeg(G*(i1,k),G*(i2,k)) &
      x in L~Lower_Seq(C,n+1) by A7,A40,XBOOLE_0:3;
      then reconsider X1=X as non empty compact Subset of TOP-REAL 2
      by PSCOMP_1:64,XBOOLE_0:def 3;
      consider pp be set such that
A47:  pp in W-most X1 by XBOOLE_0:def 1;
      reconsider pp as Point of TOP-REAL 2 by A47;
A48:  pp in X by A47,XBOOLE_0:def 3;
      then
A49:  pp in L~Lower_Seq(C,n+1) by XBOOLE_0:def 3;
      pp in LSeg(G*(i1,k),G*(i2,k)) by A48,XBOOLE_0:def 3;
      then
A50:  pp`2 = |[inf(proj1.:X),G*(1,k)`2]|`2 by A15,A46,SPPOL_1:63;
      |[inf(proj1.:X),G*(1,k)`2]|`1 = W-bound X by A42,A45,SPRECT_1:48
        .= (W-min X)`1 by EUCLID:56
        .= pp`1 by A47,PSCOMP_1:88;
      then G*(m,k) in Lower_Arc L~Cage(C,n+1) by A7,A45,A49,A50,TOPREAL3:11;
      then LSeg(G*(i1,k),G*(m,k)) meets Lower_Arc C by A1,A4,A12,A41,A43,Th42;
      then LSeg(G*(i1,k),G*(i2,k)) meets Lower_Arc C by A1,A2,A12,A41,Th8,
      XBOOLE_1:63;
      hence LSeg(G*(i2,j),G*(i2,k)) \/
      LSeg(G*(i2,k),G*(i1,k)) meets Lower_Arc C by XBOOLE_1:70;
    end;
    suppose
A51:  LSeg(G*(i2,j),G*(i2,k)) misses Upper_Arc L~Cage(C,n+1) &
      LSeg(Gauge(C,n+1)*(i2,k),Gauge(C,n+1)*(i1,k))
      misses Lower_Arc L~Cage(C,n+1);
      consider j1 be Element of NAT such that
A52:  j <= j1 & j1 <= k and
A53:  LSeg(G*(i2,j1),G*(i2,k)) /\ L~Lower_Seq(C,n+1) =
      {G*(i2,j1)} by A2,A3,A5,A7,Th11;
      G*(i2,j1) in LSeg(G*(i2,j1),G*(i2,k)) /\
      L~Lower_Seq(C,n+1) by A53,TARSKI:def 1;
      then
A54:  G*(i2,j1) in L~Lower_Seq(C,n+1) by XBOOLE_0:def 3;
A55:  1 <= j1 & j1 <= width G by A3,A52,XXREAL_0:2;
      now per cases;
        suppose i2 <= i1;
          then consider i3 be Element of NAT such that
A56:      i2 <= i3 & i3 <= i1 and
A57:      LSeg(G*(i2,k),G*(i3,k)) /\ L~Upper_Seq(C,n+1) = {G*(i3,k)}
          by A1,A2,A4,A6,A12,Th15;
A58:      1 < i3 & i3 < len G by A1,A2,A56,XXREAL_0:2;
          G*(i3,k) in LSeg(G*(i2,k),G*(i3,k)) /\ L~Upper_Seq(C,n+1)
          by A57,TARSKI:def 1;
          then
A59:      G*(i3,k) in L~Upper_Seq(C,n+1) by XBOOLE_0:def 3;
A60:      LSeg(G*(i2,j1),G*(i2,k)) c=
          LSeg(G*(i2,j),G*(i2,k)) by A2,A3,A52,Th7;
A61:      LSeg(G*(i2,k),G*(i3,k)) c= LSeg(G*(i2,k),G*(i1,k))
          by A1,A2,A12,A56,Th8;
          then
A62:      LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k)) c=

LSeg(G*(i2,j),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i1,k)) by A60,XBOOLE_1:13;
A63:      (LSeg(G*(i2,j1),G*(i2,k)) \/
          LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1) = {G*(i3,k)}
          proof
            thus (LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1) c= {G*(i3,k)}
            proof
              let x be set;
              assume x in (LSeg(G*(i2,j1),G*(i2,k)) \/
              LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1);
              then x in LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k)) &
              x in L~Upper_Seq(C,n+1) by XBOOLE_0:def 3;
              then (x in LSeg(G*(i2,j1),G*(i2,k)) or
              x in LSeg(G*(i2,k),G*(i3,k))) &
              x in L~Upper_Seq(C,n+1) by XBOOLE_0:def 2;
              hence x in {G*(i3,k)} by A6,A51,A57,A60,XBOOLE_0:3,def 3;
            end;
            let x be set;
            assume x in {G*(i3,k)};
            then
A64:        x = G*(i3,k) by TARSKI:def 1;
            G*(i3,k) in LSeg(G*(i2,k),G*(i3,k)) by TOPREAL1:6;
            then G*(i3,k) in LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k)) by XBOOLE_0:def 2;
            hence x in (LSeg(G*(i2,j1),G*(i2,k)) \/
            LSeg(G*(i2,k),G*(i3,k))) /\ L~Upper_Seq(C,n+1)
            by A59,A64,XBOOLE_0:def 3;
          end;
          (LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k))) /\
          L~Lower_Seq(C,n+1) = {G*(i2,j1)}
          proof
            thus (LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k))) /\
            L~Lower_Seq(C,n+1) c= {G*(i2,j1)}
            proof
              let x be set;
              assume x in (LSeg(G*(i2,j1),G*(i2,k)) \/
              LSeg(G*(i2,k),G*(i3,k))) /\ L~Lower_Seq(C,n+1);
              then x in LSeg(G*(i2,j1),G*(i2,k)) \/ LSeg(G*(i2,k),G*(i3,k)) &
              x in L~Lower_Seq(C,n+1) by XBOOLE_0:def 3;
              then (x in LSeg(G*(i2,j1),G*(i2,k)) or
              x in LSeg(G*(i2,k),G*(i3,k))) &
              x in L~Lower_Seq(C,n+1) by XBOOLE_0:def 2;
