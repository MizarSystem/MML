      p`1 >= (E-max C)`1 by A151,TOPREAL1:def 13;
      then
A156: p`1 = (E-max C)`1 by A155,XXREAL_0:1;
      p`2 = (E-max C)`2 by A151,TOPREAL1:def 13;
      then p = E-max C by A156,TOPREAL3:11;
      hence contradiction by A5,A73,A115,A128,A152,XBOOLE_0:3;
    end;
    suppose east_halfline E-max C meets L~do;
      then consider p be set such that
A157: p in east_halfline E-max C and
A158: p in L~do by XBOOLE_0:3;
      reconsider p as Point of TOP-REAL 2 by A157;
      p in L~LS by A48,A158;
      then p in east_halfline E-max C /\ L~Cage(C,n)
      by A125,A157,XBOOLE_0:def 3;
      then
A159: p`1 = Ebo by A144,JORDAN1A:104;
A160: (E-max C)`2 = p`2 by A157,TOPREAL1:def 13;
      set RC = Rotate(Cage(C,n),Emax);
A161: E-max C in right_cell(RC,1) by JORDAN1I:9;
A162: 1+1 <= len LS by A21,XXREAL_0:2;
      LS = RC-:Wmin by JORDAN1G:26;
      then
A163: LSeg(LS,1) = LSeg(RC,1) by A162,SPPOL_2:9;
A164: L~RC = L~Cage(C,n) by REVROT_1:33;
A165: len RC = len Cage(C,n) by REVROT_1:14;
A166: GoB RC = GoB Cage(C,n) by REVROT_1:28
        .= Ga by JORDAN1H:52;
A167: Emax in rng Cage(C,n) by SPRECT_2:50;
A168: RC is_sequence_on Ga by A121,REVROT_1:34;
A169: RC/.1 = E-max L~RC by A164,A167,FINSEQ_6:98;
      then consider ii,g be Element of NAT such that
A170: [ii,g+1] in Indices Ga and
A171: [ii,g] in Indices Ga and
A172: RC/.1 = Ga*(ii,g+1) and
A173: RC/.(1+1) = Ga*(ii,g) by A77,A165,A168,JORDAN1I:25;
      consider g2 be Element of NAT such that
A174: 1 <= g2 & g2 <= width Ga and
A175: Emax = Ga*(len Ga,g2) by JORDAN1D:29;
A176: len Ga >= 4 by JORDAN8:13;
      then len Ga >= 1 by XXREAL_0:2;
      then [len Ga,g2] in Indices Ga by A174,MATRIX_1:37;
      then
A177: ii = len Ga by A164,A169,A170,A172,A175,GOBOARD1:21;
A178: 1 <= ii & ii <= len Ga & 1 <= g+1 & g+1 <= width Ga by A170,GOBOARD5:1;
A179: 1 <= ii & ii <= len Ga & 1 <= g & g <= width Ga by A171,GOBOARD5:1;
A180: ii+1 <> ii;
      g+1+1 <> g;
      then
A181: right_cell(RC,1) = cell(Ga,ii-'1,g)
      by A77,A165,A166,A170,A171,A172,A173,A180,GOBOARD5:def 6;
A182: ii-'1+1 = ii by A178,BINARITH:53;
      ii-1 >= 4-1 by A176,A177,XREAL_1:11;
      then
A183: ii-1 >= 1 by XXREAL_0:2;
      then
A184: 1 <= ii-'1 by BINARITH:def 3;
A185: Ga*(ii-'1,g)`2 <= p`2 & p`2 <= Ga*(ii-'1,g+1)`2
      by A160,A161,A178,A179,A181,A182,A183,JORDAN9:19;
A186: ii-'1 < len Ga by A178,A182,NAT_1:13;
      then
A187: Ga*(ii-'1,g)`2 = Ga*(1,g)`2 by A179,A184,GOBOARD5:2
        .= Ga*(ii,g)`2 by A179,GOBOARD5:2;
A188: Ga*(ii-'1,g+1)`2 = Ga*(1,g+1)`2 by A178,A184,A186,GOBOARD5:2
        .= Ga*(ii,g+1)`2 by A178,GOBOARD5:2;
      Ga*(len Ga,g)`1 = Ebo & Ebo = Ga*(len Ga,g+1)`1
      by A8,A178,A179,JORDAN1A:92;
      then p in LSeg(RC/.1,RC/.(1+1))
      by A159,A172,A173,A177,A185,A187,A188,GOBOARD7:8;
      then
A189: p in LSeg(LS,1) by A77,A163,A165,TOPREAL1:def 5;
A190: p in LSeg(do,Index(p,do)) by A158,JORDAN3:42;
A191: do = mid(LS,Gik..LS,len LS) by A32,JORDAN1J:37;
A192: 1<=Gik..LS & Gik..LS<=len LS by A32,FINSEQ_4:31;
      Gik..LS <> len LS by A24,A32,FINSEQ_4:29;
      then
A193: Gik..LS < len LS by A192,REAL_1:def 5;
A194: 1<=Index(p,do) & Index(p,do) < len do by A158,JORDAN3:41;
A195: Index(Gik,LS)+1 = Gik..LS by A27,A32,JORDAN1J:56;
      consider t be Nat such that
A196: t in dom LS and
A197: LS.t = Gik by A32,FINSEQ_2:11;
A198: 1 <= t & t <= len LS by A196,FINSEQ_3:27;
      then 1 < t by A27,A197,REAL_1:def 5;
      then Index(Gik,LS)+1 = t by A197,A198,JORDAN3:45;
      then
A199: len L_Cut(LS,Gik) = len LS-Index(Gik,LS) by A6,A197,JORDAN3:61;
      set tt = Index(p,do)+(Gik..LS)-'1;
A200: 1<=Index(Gik,LS) & 0+Index(Gik,LS) < len LS by A6,JORDAN3:41;
      then
A201: len LS-Index(Gik,LS) > 0 by XREAL_1:22;
      Index(p,do) < len LS-'Index(Gik,LS) by A194,A199,BINARITH:def 3;
      then Index(p,do)+1 <= len LS-'Index(Gik,LS) by NAT_1:13;
      then Index(p,do) <= len LS-'Index(Gik,LS)-1 by XREAL_1:21;
      then Index(p,do) <= len LS-Index(Gik,LS)-1 by A201,BINARITH:def 3;
      then Index(p,do) <= len LS-Gik..LS by A195;
      then Index(p,do) <= len LS-'Gik..LS by BINARITH:def 3;
      then Index(p,do) < len LS-'(Gik..LS)+1 by NAT_1:13;
      then
A202: LSeg(mid(LS,Gik..LS,len LS),Index(p,do)) =
      LSeg(LS,Index(p,do)+(Gik..LS)-'1) by A192,A193,A194,JORDAN4:31;
A203: 1+1 <= Gik..LS by A195,A200,XREAL_1:9;
      then Index(p,do)+Gik..LS >= 1+1+1 by A194,XREAL_1:9;
      then Index(p,do)+Gik..LS-1 >= 1+1+1-1 by XREAL_1:11;
      then
A204: tt >= 1+1 by BINARITH:def 3;
A205: 2 in dom LS by A162,FINSEQ_3:27;
      now per cases by A204,REAL_1:def 5;
        suppose tt > 1+1;
          then LSeg(LS,1) misses LSeg(LS,tt) by TOPREAL1:def 9;
          hence contradiction by A189,A190,A191,A202,XBOOLE_0:3;
        end;
        suppose
A206:     tt = 1+1;
          then LSeg(LS,1) /\ LSeg(LS,tt) = {LS/.2} by A21,TOPREAL1:def 8;
          then p in {LS/.2} by A189,A190,A191,A202,XBOOLE_0:def 3;
          then
A207:     p = LS/.2 by TARSKI:def 1;
          then
A208:     p..LS = 2 by A205,FINSEQ_5:44;
          1+1 = Index(p,do)+(Gik..LS)-1 by A206,BINARITH:def 3;
          then 1+1+1 = Index(p,do)+(Gik..LS);
          then
A209:     Gik..LS = 2 by A194,A203,JORDAN1E:10;
          p in rng LS by A205,A207,PARTFUN2:4;
          then p = Gik by A32,A208,A209,FINSEQ_5:10;
          then Gik`1 = Ebo by A207,JORDAN1G:40;
          then Gik`1 = Ga*(len Ga,j)`1 by A8,A10,JORDAN1A:92;
          hence contradiction by A1,A13,A59,JORDAN1G:7;
        end;
      end;
      hence contradiction;
    end;
  end;
  then east_halfline E-max C c= (L~godo)` by SUBSET_1:43;
  then consider W be Subset of TOP-REAL 2 such that
A210: W is_a_component_of (L~godo)` and
A211: east_halfline E-max C c= W by GOBOARD9:5;
  W is not Bounded by A211,JORDAN2C:16,129;
  then W is_outside_component_of L~godo by A210,JORDAN2C:def 4;
  then W c= UBD L~godo by JORDAN2C:27;
  then
A212: east_halfline E-max C c= UBD L~godo by A211,XBOOLE_1:1;
  E-max C in east_halfline E-max C by TOPREAL1:45;
  then E-max C in UBD L~godo by A212;
  then E-max C in LeftComp godo by GOBRD14:46;
  then LA meets L~godo by A114,A115,A123,A131,JORDAN1J:36;
  then
A213: LA meets (L~go \/ L~pion1) or LA meets L~do by A124,XBOOLE_1:70;
A214: LA c= C by JORDAN6:76;
  per cases by A213,XBOOLE_1:70;
  suppose LA meets L~go;
    then LA meets L~Cage(C,n) by A41,A125,XBOOLE_1:1,63;
    hence contradiction by A214,JORDAN10:5,XBOOLE_1:63;
  end;
  suppose LA meets L~pion1;
    hence contradiction by A5,A73,A128;
  end;
  suppose LA meets L~do;
    then LA meets L~Cage(C,n) by A48,A125,XBOOLE_1:1,63;
    hence contradiction by A214,JORDAN10:5,XBOOLE_1:63;
  end;
end;

theorem Th39:
  for C be Simple_closed_curve for i,j,k be Element of NAT st
  1 < j & j <= k & k < len Gauge(C,n) & 1 <= i & i <= width Gauge(C,n) &
  LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) /\ L~Upper_Seq(C,n) =
  {Gauge(C,n)*(j,i)} &
  LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) /\ L~Lower_Seq(C,n) =
  {Gauge(C,n)*(k,i)} holds
  LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) meets Upper_Arc C
proof
  let C be Simple_closed_curve;
  let i,j,k be Element of NAT;
  set Ga = Gauge(C,n);
  set US = Upper_Seq(C,n);
  set LS = Lower_Seq(C,n);
  set UA = Upper_Arc C;
  set Wmin = W-min L~Cage(C,n);
  set Emax = E-max L~Cage(C,n);
  set Wbo = W-bound L~Cage(C,n);
  set Ebo = E-bound L~Cage(C,n);
  set Gij = Ga*(j,i);
  set Gik = Ga*(k,i);
  assume that
A1: 1 < j & j <= k & k < len Ga and
A2: 1 <= i & i <= width Ga and
A3: LSeg(Gij,Gik) /\ L~US = {Gij} and
A4: LSeg(Gij,Gik) /\ L~LS = {Gik} and
A5: LSeg(Gij,Gik) misses UA;
  Gik in {Gik} by TARSKI:def 1;
  then
A6: Gik in L~LS by A4,XBOOLE_0:def 3;
  Gij in {Gij} by TARSKI:def 1;
  then
A7: Gij in L~US by A3,XBOOLE_0:def 3;
A8: len Ga = width Ga by JORDAN8:def 1;
A9: j <> k by A1,A2,A6,A7,Th29;
A10: 1 < j & j < width Ga by A1,A8,XXREAL_0:2;
A11: 1 < k & k < width Ga by A1,JORDAN8:def 1,XXREAL_0:2;
A12: [j,i] in Indices Ga by A2,A8,A10,MATRIX_1:37;
A13: [k,i] in Indices Ga by A2,A8,A11,MATRIX_1:37;
  set go = R_Cut(US,Gij);
  set do = L_Cut(LS,Gik);
A14: len US >= 3 by JORDAN1E:19;
  then len US >= 1 by XXREAL_0:2;
  then 1 in dom US by FINSEQ_3:27;
  then
A15: US.1 = US/.1 by PARTFUN1:def 8
    .= Wmin by JORDAN1F:5;
A16: Wmin`1 = Wbo by EUCLID:56
    .= Ga*(1,i)`1 by A2,A8,JORDAN1A:94;
  len Ga >= 4 by JORDAN8:13;
  then
A17: len Ga >= 1 by XXREAL_0:2;
  then
A18: [1,k] in Indices Ga by A11,MATRIX_1:37;
A19: [1,i] in Indices Ga by A2,A17,MATRIX_1:37;
  then
A20: Gij <> US.1 by A1,A12,A15,A16,JORDAN1G:7;
  then reconsider go as being_S-Seq FinSequence of TOP-REAL 2
  by A7,JORDAN3:70;
A21: len LS >= 1+2 by JORDAN1E:19;
  then len LS >= 1 by XXREAL_0:2;
  then
A22: 1 in dom LS & len LS in dom LS by FINSEQ_3:27;
  then
A23: LS.len LS = LS/.len LS by PARTFUN1:def 8
    .= Wmin by JORDAN1F:8;
  Wmin`1 = Wbo by EUCLID:56
    .= Ga*(1,i)`1 by A2,A8,JORDAN1A:94;
  then
A24: Gik <> LS.len LS by A1,A13,A19,A23,JORDAN1G:7;
  then reconsider do as being_S-Seq FinSequence of TOP-REAL 2
  by A6,JORDAN3:69;
A25: [len Ga,k] in Indices Ga by A11,A17,MATRIX_1:37;
A26: LS.1 = LS/.1 by A22,PARTFUN1:def 8
    .= Emax by JORDAN1F:6;
  Emax`1 = Ebo by EUCLID:56
    .= Ga*(len Ga,k)`1 by A8,A11,JORDAN1A:92;
  then
A27: Gik <> LS.1 by A1,A13,A25,A26,JORDAN1G:7;
A28: len go >= 1+1 by TOPREAL1:def 10;
A29: Gij in rng US by A2,A7,A8,A10,JORDAN1G:4,JORDAN1J:40;
  then
A30: go is_sequence_on Ga by JORDAN1G:4,JORDAN1J:38;
A31: len do >= 1+1 by TOPREAL1:def 10;
A32: Gik in rng LS by A2,A6,A8,A11,JORDAN1G:5,JORDAN1J:40;
  then
A33: do is_sequence_on Ga by JORDAN1G:5,JORDAN1J:39;
  reconsider go as non constant s.c.c.
  (being_S-Seq FinSequence of TOP-REAL 2) by A28,A30,JGRAPH_1:16,JORDAN8:8;
  reconsider do as non constant s.c.c.
  (being_S-Seq FinSequence of TOP-REAL 2) by A31,A33,JGRAPH_1:16,JORDAN8:8;
A34: len go > 1 by A28,NAT_1:13;
  then
A35: len go in dom go by FINSEQ_3:27;
  then
A36: go/.len go = go.len go by PARTFUN1:def 8
    .= Gij by A7,JORDAN3:59;
  len do >= 1 by A31,XXREAL_0:2;
  then 1 in dom do by FINSEQ_3:27;
  then
A37: do/.1 = do.1 by PARTFUN1:def 8
    .= Gik by A6,JORDAN3:58;
  reconsider m = len go - 1 as Element of NAT by A35,FINSEQ_3:28;
A38: m+1 = len go;
  then
A39: len go-'1 = m by BINARITH:39;
A40: LSeg(go,m) c= L~go by TOPREAL3:26;
A41: L~go c= L~US by A7,JORDAN3:76;
  then LSeg(go,m) c= L~US by A40,XBOOLE_1:1;
  then
A42: LSeg(go,m) /\ LSeg(Gik,Gij) c= {Gij} by A3,XBOOLE_1:26;
  m >= 1 by A28,XREAL_1:21;
  then
A43: LSeg(go,m) = LSeg(go/.m,Gij) by A36,A38,TOPREAL1:def 5;
  {Gij} c= LSeg(go,m) /\ LSeg(Gik,Gij)
  proof
    let x be set;
    assume x in {Gij};
    then
A44: x = Gij by TARSKI:def 1;
A45: Gij in LSeg(go,m) by A43,TOPREAL1:6;
    Gij in LSeg(Gik,Gij) by TOPREAL1:6;
    hence x in LSeg(go,m) /\ LSeg(Gik,Gij) by A44,A45,XBOOLE_0:def 3;
  end;
  then
A46: LSeg(go,m) /\ LSeg(Gik,Gij) = {Gij} by A42,XBOOLE_0:def 10;
A47: LSeg(do,1) c= L~do by TOPREAL3:26;
A48: L~do c= L~LS by A6,JORDAN3:77;
  then LSeg(do,1) c= L~LS by A47,XBOOLE_1:1;
  then
A49: LSeg(do,1) /\ LSeg(Gik,Gij) c= {Gik} by A4,XBOOLE_1:26;
A50: LSeg(do,1) = LSeg(Gik,do/.(1+1)) by A31,A37,TOPREAL1:def 5;
  {Gik} c= LSeg(do,1) /\ LSeg(Gik,Gij)
  proof
    let x be set;
    assume x in {Gik};
    then
A51: x = Gik by TARSKI:def 1;
A52: Gik in LSeg(do,1) by A50,TOPREAL1:6;
    Gik in LSeg(Gik,Gij) by TOPREAL1:6;
    hence x in LSeg(do,1) /\ LSeg(Gik,Gij) by A51,A52,XBOOLE_0:def 3;
  end;
  then
A53: LSeg(Gik,Gij) /\ LSeg(do,1) = {Gik} by A49,XBOOLE_0:def 10;
A54: go/.1 = US/.1 by A7,SPRECT_3:39
    .= Wmin by JORDAN1F:5;
  then
A55: go/.1 = LS/.len LS by JORDAN1F:8
    .= do/.len do by A6,JORDAN1J:35;
A56: rng go c= L~go & rng do c= L~do by A28,A31,SPPOL_2:18;
A57: {go/.1} c= L~go /\ L~do
  proof
    let x be set;
    assume x in {go/.1};
    then x = go/.1 by TARSKI:def 1;
    then x in rng go & x in rng do by A55,FINSEQ_6:46,REVROT_1:3;
    hence x in L~go /\ L~do by A56,XBOOLE_0:def 3;
  end;
A58: LS.1 = LS/.1 by A22,PARTFUN1:def 8
    .= Emax by JORDAN1F:6;
A59: [len Ga,j] in Indices Ga by A10,A17,MATRIX_1:37;
  L~go /\ L~do c= {go/.1}
  proof
    let x be set;
    assume x in L~go /\ L~do;
    then
A60: x in L~go & x in L~do by XBOOLE_0:def 3;
    then x in L~US /\ L~LS by A41,A48,XBOOLE_0:def 3;
    then x in {Wmin,Emax} by JORDAN1E:20;
    then
A61: x = Wmin or x = Emax by TARSKI:def 2;
    now
      assume x = Emax;
      then
A62:  Emax = Gik by A6,A58,A60,JORDAN1E:11;
      Ga*(len Ga,j)`1 = Ebo by A8,A10,JORDAN1A:92;
      then Emax`1 <> Ebo by A1,A13,A59,A62,JORDAN1G:7;
      hence contradiction by EUCLID:56;
    end;
    hence x in {go/.1} by A54,A61,TARSKI:def 1;
  end;
  then
A63: L~go /\ L~do = {go/.1} by A57,XBOOLE_0:def 10;
  set W2 = go/.2;
A64: 2 in dom go by A28,FINSEQ_3:27;
A65: Gij..US >= 1 by A29,FINSEQ_4:31;
A66: now
    assume Gij`1 = Wbo;
    then Ga*(1,k)`1 = Ga*(j,i)`1 by A8,A11,JORDAN1A:94;
    hence contradiction by A1,A12,A18,JORDAN1G:7;
  end;
  go = mid(US,1,Gij..US) by A29,JORDAN1G:57
    .= US|(Gij..US) by A65,JORDAN3:25;
  then
A67: W2 = US/.2 by A64,FINSEQ_4:85;
A68: Wmin in rng go by A54,FINSEQ_6:46;
  set pion = <*Gij,Gik*>;
A69: now
    let n be Element of NAT;
    assume n in dom pion;
    then n in {1,2} by FINSEQ_1:4,FINSEQ_3:29;
    then n = 1 or n = 2 by TARSKI:def 2;
    then pion/.n = Gik or pion/.n = Gij by FINSEQ_4:26;
    hence ex j,i be Element of NAT st [j,i] in Indices Ga & pion/.n = Ga*(j,i)
    by A12,A13;
  end;
A70: Gik <> Gij by A9,A12,A13,GOBOARD1:21;
  Gik`2 = Ga*(1,i)`2 by A2,A8,A11,GOBOARD5:2
    .= Gij`2 by A2,A8,A10,GOBOARD5:2;
  then LSeg(Gik,Gij) is horizontal by SPPOL_1:36;
  then pion is_S-Seq by A70,JORDAN1B:9;
  then consider pion1 be FinSequence of TOP-REAL 2 such that
A71: pion1 is_sequence_on Ga and
A72: pion1 is_S-Seq and
A73: L~pion = L~pion1 and
A74: pion/.1 = pion1/.1 and
A75: pion/.len pion = pion1/.len pion1 and
A76: len pion <= len pion1 by A69,GOBOARD3:2;
  reconsider pion1 as being_S-Seq FinSequence of TOP-REAL 2 by A72;
  set godo = go^'pion1^'do;
A77: 1+1 <= len Cage(C,n) by GOBOARD7:36,XXREAL_0:2;
  then
A78: 1+1 <= len Rotate(Cage(C,n),Wmin) by REVROT_1:14;
  len (go^'pion1) >= len go by TOPREAL8:7;
  then
A79: len (go^'pion1) >= 1+1 by A28,XXREAL_0:2;
  then
A80: len (go^'pion1) > 1+0 by NAT_1:13;
A81: len godo >= len (go^'pion1) by TOPREAL8:7;
  then
A82: 1+1 <= len godo by A79,XXREAL_0:2;
A83: US is_sequence_on Ga by JORDAN1G:4;
A84: go/.len go = pion1/.1 by A36,A74,FINSEQ_4:26;
  then
A85: go^'pion1 is_sequence_on Ga by A30,A71,TOPREAL8:12;
A86: (go^'pion1)/.len (go^'pion1) = pion/.len pion by A75,GRAPH_2:58
    .= pion/.2 by FINSEQ_1:61
    .= do/.1 by A37,FINSEQ_4:26;
  then
A87: godo is_sequence_on Ga by A33,A85,TOPREAL8:12;
  LSeg(pion1,1) c= L~<*Gij,Gik*> by A73,TOPREAL3:26;
  then LSeg(pion1,1) c= LSeg(Gij,Gik) by SPPOL_2:21;
  then
A88: LSeg(go,len go-'1) /\ LSeg(pion1,1) c= {Gij} by A39,A46,XBOOLE_1:27;
A89: len pion1 >= 1+1 by A76,FINSEQ_1:61;
  {Gij} c= LSeg(go,m) /\ LSeg(pion1,1)
  proof
    let x be set;
    assume x in {Gij};
    then
A90: x = Gij by TARSKI:def 1;
A91: Gij in LSeg(go,m) by A43,TOPREAL1:6;
    Gij in LSeg(pion1,1) by A36,A84,A89,TOPREAL1:27;
    hence x in LSeg(go,m) /\ LSeg(pion1,1) by A90,A91,XBOOLE_0:def 3;
  end;
