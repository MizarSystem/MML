      by A127,A159,XBOOLE_0:def 3;
      then
A161: p`1 = Ebo by A146,JORDAN1A:104;
A162: (E-max C)`2 = p`2 by A159,TOPREAL1:def 13;
      set RC = Rotate(Cage(C,n),Emax);
A163: E-max C in right_cell(RC,1) by JORDAN1I:9;
A164: 1+1 <= len LS by A21,XXREAL_0:2;
      LS = RC-:Wmin by JORDAN1G:26;
      then
A165: LSeg(LS,1) = LSeg(RC,1) by A164,SPPOL_2:9;
A166: L~RC = L~Cage(C,n) by REVROT_1:33;
A167: len RC = len Cage(C,n) by REVROT_1:14;
A168: GoB RC = GoB Cage(C,n) by REVROT_1:28
        .= Ga by JORDAN1H:52;
A169: Emax in rng Cage(C,n) by SPRECT_2:50;
A170: RC is_sequence_on Ga by A123,REVROT_1:34;
A171: RC/.1 = E-max L~RC by A166,A169,FINSEQ_6:98;
      then consider ii,jj be Element of NAT such that
A172: [ii,jj+1] in Indices Ga and
A173: [ii,jj] in Indices Ga and
A174: RC/.1 = Ga*(ii,jj+1) and
A175: RC/.(1+1) = Ga*(ii,jj) by A79,A167,A170,JORDAN1I:25;
      consider jj2 be Element of NAT such that
A176: 1 <= jj2 & jj2 <= width Ga and
A177: Emax = Ga*(len Ga,jj2) by JORDAN1D:29;
A178: len Ga >= 4 by JORDAN8:13;
      then len Ga >= 1 by XXREAL_0:2;
      then [len Ga,jj2] in Indices Ga by A176,MATRIX_1:37;
      then
A179: ii = len Ga by A166,A171,A172,A174,A177,GOBOARD1:21;
A180: 1 <= ii & ii <= len Ga & 1 <= jj+1 & jj+1 <= width Ga
      by A172,GOBOARD5:1;
A181: 1 <= ii & ii <= len Ga & 1 <= jj & jj <= width Ga by A173,GOBOARD5:1;
A182: ii+1 <> ii;
      jj+1+1 <> jj;
      then
A183: right_cell(RC,1) = cell(Ga,ii-'1,jj)
      by A79,A167,A168,A172,A173,A174,A175,A182,GOBOARD5:def 6;
A184: ii-'1+1 = ii by A180,BINARITH:53;
      ii-1 >= 4-1 by A178,A179,XREAL_1:11;
      then
A185: ii-1 >= 1 by XXREAL_0:2;
      then
A186: 1 <= ii-'1 by BINARITH:def 3;
A187: Ga*(ii-'1,jj)`2 <= p`2 & p`2 <= Ga*(ii-'1,jj+1)`2
      by A162,A163,A180,A181,A183,A184,A185,JORDAN9:19;
A188: ii-'1 < len Ga by A180,A184,NAT_1:13;
      then
A189: Ga*(ii-'1,jj)`2 = Ga*(1,jj)`2 by A181,A186,GOBOARD5:2
        .= Ga*(ii,jj)`2 by A181,GOBOARD5:2;
A190: Ga*(ii-'1,jj+1)`2 = Ga*(1,jj+1)`2 by A180,A186,A188,GOBOARD5:2
        .= Ga*(ii,jj+1)`2 by A180,GOBOARD5:2;
      Ga*(len Ga,jj)`1 = Ebo & Ebo = Ga*(len Ga,jj+1)`1
      by A8,A180,A181,JORDAN1A:92;
      then p in LSeg(RC/.1,RC/.(1+1))
      by A161,A174,A175,A179,A187,A189,A190,GOBOARD7:8;
      then
A191: p in LSeg(LS,1) by A79,A165,A167,TOPREAL1:def 5;
A192: p in LSeg(do,Index(p,do)) by A160,JORDAN3:42;
A193: do = mid(LS,Gij..LS,len LS) by A34,JORDAN1J:37;
A194: 1<=Gij..LS & Gij..LS<=len LS by A34,FINSEQ_4:31;
      Gij..LS <> len LS by A26,A34,FINSEQ_4:29;
      then
A195: Gij..LS < len LS by A194,REAL_1:def 5;
A196: 1<=Index(p,do) & Index(p,do) < len do by A160,JORDAN3:41;
A197: Index(Gij,LS)+1 = Gij..LS by A29,A34,JORDAN1J:56;
      consider t be Nat such that
A198: t in dom LS and
A199: LS.t = Gij by A34,FINSEQ_2:11;
A200: 1 <= t & t <= len LS by A198,FINSEQ_3:27;
      then 1 < t by A29,A199,REAL_1:def 5;
      then Index(Gij,LS)+1 = t by A199,A200,JORDAN3:45;
      then
A201: len L_Cut(LS,Gij) = len LS-Index(Gij,LS) by A6,A199,JORDAN3:61;
      set tt = Index(p,do)+(Gij..LS)-'1;
A202: 1<=Index(Gij,LS) & 0+Index(Gij,LS) < len LS by A6,JORDAN3:41;
      then
A203: len LS-Index(Gij,LS) > 0 by XREAL_1:22;
      Index(p,do) < len LS-'Index(Gij,LS) by A196,A201,BINARITH:def 3;
      then Index(p,do)+1 <= len LS-'Index(Gij,LS) by NAT_1:13;
      then Index(p,do) <= len LS-'Index(Gij,LS)-1 by XREAL_1:21;
      then Index(p,do) <= len LS-Index(Gij,LS)-1 by A203,BINARITH:def 3;
      then Index(p,do) <= len LS-Gij..LS by A197;
      then Index(p,do) <= len LS-'Gij..LS by BINARITH:def 3;
      then Index(p,do) < len LS-'(Gij..LS)+1 by NAT_1:13;
      then
A204: LSeg(mid(LS,Gij..LS,len LS),Index(p,do)) =
      LSeg(LS,Index(p,do)+(Gij..LS)-'1) by A194,A195,A196,JORDAN4:31;
A205: 1+1 <= Gij..LS by A197,A202,XREAL_1:9;
      then Index(p,do)+Gij..LS >= 1+1+1 by A196,XREAL_1:9;
      then Index(p,do)+Gij..LS-1 >= 1+1+1-1 by XREAL_1:11;
      then
A206: tt >= 1+1 by BINARITH:def 3;
A207: 2 in dom LS by A164,FINSEQ_3:27;
      now per cases by A206,REAL_1:def 5;
        suppose tt > 1+1;
          then LSeg(LS,1) misses LSeg(LS,tt) by TOPREAL1:def 9;
          hence contradiction by A191,A192,A193,A204,XBOOLE_0:3;
        end;
        suppose
A208:     tt = 1+1;
          then LSeg(LS,1) /\ LSeg(LS,tt) = {LS/.2} by A21,TOPREAL1:def 8;
          then p in {LS/.2} by A191,A192,A193,A204,XBOOLE_0:def 3;
          then
A209:     p = LS/.2 by TARSKI:def 1;
          then
A210:     p..LS = 2 by A207,FINSEQ_5:44;
          1+1 = Index(p,do)+(Gij..LS)-1 by A208,BINARITH:def 3;
          then 1+1+1 = Index(p,do)+(Gij..LS);
          then
A211:     Gij..LS = 2 by A196,A205,JORDAN1E:10;
          p in rng LS by A207,A209,PARTFUN2:4;
          then p = Gij by A34,A210,A211,FINSEQ_5:10;
          then Gij`1 = Ebo by A209,JORDAN1G:40;
          then Gij`1 = Ga*(len Ga,j)`1 by A8,A10,JORDAN1A:92;
          hence contradiction by A1,A12,A61,JORDAN1G:7;
        end;
      end;
      hence contradiction;
    end;
  end;
  then east_halfline E-max C c= (L~godo)` by SUBSET_1:43;
  then consider W be Subset of TOP-REAL 2 such that
A212: W is_a_component_of (L~godo)` and
A213: east_halfline E-max C c= W by GOBOARD9:5;
  W is not Bounded by A213,JORDAN2C:16,129;
  then W is_outside_component_of L~godo by A212,JORDAN2C:def 4;
  then W c= UBD L~godo by JORDAN2C:27;
  then
A214: east_halfline E-max C c= UBD L~godo by A213,XBOOLE_1:1;
  E-max C in east_halfline E-max C by TOPREAL1:45;
  then E-max C in UBD L~godo by A214;
  then E-max C in LeftComp godo by GOBRD14:46;
  then UA meets L~godo by A116,A117,A125,A133,JORDAN1J:36;
  then
A215: UA meets (L~go \/ L~pion1) or UA meets L~do by A126,XBOOLE_1:70;
A216: UA c= C by JORDAN6:76;
  per cases by A215,XBOOLE_1:70;
  suppose UA meets L~go;
    then UA meets L~Cage(C,n) by A43,A127,XBOOLE_1:1,63;
    hence contradiction by A216,JORDAN10:5,XBOOLE_1:63;
  end;
  suppose UA meets L~pion1;
    hence contradiction by A5,A75,A130;
  end;
  suppose UA meets L~do;
    then UA meets L~Cage(C,n) by A50,A127,XBOOLE_1:1,63;
    hence contradiction by A216,JORDAN10:5,XBOOLE_1:63;
  end;
end;

theorem Th32:
  for C be Simple_closed_curve for i,j,k be Element of NAT st
  1 < j & j <= k & k < len Gauge(C,n) & 1 <= i & i <= width Gauge(C,n) &
  Gauge(C,n)*(k,i) in L~Upper_Seq(C,n) &
  Gauge(C,n)*(j,i) in L~Lower_Seq(C,n) holds
  LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) meets Lower_Arc C
proof
  let C be Simple_closed_curve;
  let i,j,k be Element of NAT;
  assume that
A1: 1 < j & j <= k & k < len Gauge(C,n) and
A2: 1 <= i & i <= width Gauge(C,n) and
A3: Gauge(C,n)*(k,i) in L~Upper_Seq(C,n) and
A4: Gauge(C,n)*(j,i) in L~Lower_Seq(C,n);
  consider j1,k1 be Element of NAT such that
A5: j <= j1 and
A6: j1 <= k1 and
A7: k1 <= k and
A8: LSeg(Gauge(C,n)*(j1,i),Gauge(C,n)*(k1,i)) /\ L~Lower_Seq(C,n) =
  {Gauge(C,n)*(j1,i)} and
A9: LSeg(Gauge(C,n)*(j1,i),Gauge(C,n)*(k1,i)) /\ L~Upper_Seq(C,n) =
  {Gauge(C,n)*(k1,i)} by A1,A2,A3,A4,Th16;
  1 < j1 & k1 < len Gauge(C,n) by A1,A5,A7,XXREAL_0:2;
  then LSeg(Gauge(C,n)*(j1,i),Gauge(C,n)*(k1,i)) meets Lower_Arc C
  by A2,A6,A8,A9,Th30;
  hence LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) meets Lower_Arc C
  by A1,A2,A5,A6,A7,Th8,XBOOLE_1:63;
end;

theorem Th33:
  for C be Simple_closed_curve for i,j,k be Element of NAT st
  1 < j & j <= k & k < len Gauge(C,n) & 1 <= i & i <= width Gauge(C,n) &
  Gauge(C,n)*(k,i) in L~Upper_Seq(C,n) &
  Gauge(C,n)*(j,i) in L~Lower_Seq(C,n) holds
  LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) meets Upper_Arc C
proof
  let C be Simple_closed_curve;
  let i,j,k be Element of NAT;
  assume that
A1: 1 < j & j <= k & k < len Gauge(C,n) and
A2: 1 <= i & i <= width Gauge(C,n) and
A3: Gauge(C,n)*(k,i) in L~Upper_Seq(C,n) and
A4: Gauge(C,n)*(j,i) in L~Lower_Seq(C,n);
  consider j1,k1 be Element of NAT such that
A5: j <= j1 and
A6: j1 <= k1 and
A7: k1 <= k and
A8: LSeg(Gauge(C,n)*(j1,i),Gauge(C,n)*(k1,i)) /\ L~Lower_Seq(C,n) =
  {Gauge(C,n)*(j1,i)} and
A9: LSeg(Gauge(C,n)*(j1,i),Gauge(C,n)*(k1,i)) /\ L~Upper_Seq(C,n) =
  {Gauge(C,n)*(k1,i)} by A1,A2,A3,A4,Th16;
  1 < j1 & k1 < len Gauge(C,n) by A1,A5,A7,XXREAL_0:2;
  then LSeg(Gauge(C,n)*(j1,i),Gauge(C,n)*(k1,i)) meets Upper_Arc C
  by A2,A6,A8,A9,Th31;
  hence LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) meets Upper_Arc C
  by A1,A2,A5,A6,A7,Th8,XBOOLE_1:63;
end;

theorem Th34:
  for C be Simple_closed_curve for i,j,k be Element of NAT st
  1 < j & j <= k & k < len Gauge(C,n) &
  1 <= i & i <= width Gauge(C,n) & n > 0 &
  Gauge(C,n)*(k,i) in Upper_Arc L~Cage(C,n) &
  Gauge(C,n)*(j,i) in Lower_Arc L~Cage(C,n) holds
  LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) meets Lower_Arc C
proof
  let C be Simple_closed_curve;
  let i,j,k be Element of NAT;
  assume that
A1: 1 < j & j <= k & k < len Gauge(C,n) and
A2: 1 <= i & i <= width Gauge(C,n) and
A3: n > 0 and
A4: Gauge(C,n)*(k,i) in Upper_Arc L~Cage(C,n) and
A5: Gauge(C,n)*(j,i) in Lower_Arc L~Cage(C,n);
A6: L~Upper_Seq(C,n) = Upper_Arc L~Cage(C,n) by A3,JORDAN1G:63;
  L~Lower_Seq(C,n) = Lower_Arc L~Cage(C,n) by A3,JORDAN1G:64;
  hence LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) meets Lower_Arc C
  by A1,A2,A4,A5,A6,Th32;
end;

theorem Th35:
  for C be Simple_closed_curve for i,j,k be Element of NAT st
  1 < j & j <= k & k < len Gauge(C,n) &
  1 <= i & i <= width Gauge(C,n) & n > 0 &
  Gauge(C,n)*(k,i) in Upper_Arc L~Cage(C,n) &
  Gauge(C,n)*(j,i) in Lower_Arc L~Cage(C,n) holds
  LSeg(Gauge(C,n)*(j,i),Gauge(C,n)*(k,i)) meets Upper_Arc C
proof
  let C be Simple_closed_curve;
