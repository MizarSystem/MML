    'or' ('not'( (a).x 'or' (c).x) 'or' ((a).x 'or' (c).x))) by XBOOLEAN:102
      .=TRUE '&' ('not'( (a).x 'or' (b).x) 'or' TRUE) by XBOOLEAN:102
      .=TRUE;
