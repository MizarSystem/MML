    thus thesis by A1;
  end;
  uniqueness
  proof
    let P1,P2;
    assume that
A2: [x,y] in P1 iff x in X & x=y and
A3: [x,y] in P2 iff x in X & x=y;    
    let x,y;
    [x,y] in P1 iff x in X & x=y by A2;
    hence [x,y] in P1 iff [x,y] in P2 by A3;
  end;
end;

