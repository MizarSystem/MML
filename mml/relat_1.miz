  let y;
  assume
A2: y in rng R /\ Y;
  then y in rng R by XBOOLE_0:def 4;
  then consider x such that
A3: [x,y] in R by Def5;
  y in Y by A2,XBOOLE_0:def 4;
  then [x,y] in Y|R by A3,Def12;
  hence thesis by Def5;
