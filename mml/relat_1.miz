    suppose y in R.:Y;
      then consider x such that
A3:   [x,y] in R and
A4:   x in Y by Def13;
      x in X \/ Y by A4,XBOOLE_0:def 3;
      hence y in R.:(X \/ Y) by A3,Def13;
    end;
    suppose y in R.:X;
      then consider x such that
A6:   [x,y] in R and
A7:   x in X by Def13;
      x in X \/ Y by A7,XBOOLE_0:def 3;
