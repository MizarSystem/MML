A5: [x,y] in (P*R)|X;
    then [x,y] in P*R by Def11;
    then consider a such that
A6: [x,a] in P and
A7: [a,y] in R by Def8;
    x in X by A5,Def11;
    then [x,a] in P|X by A6,Def11;
    hence [x,y] in (P|X)*R by A7,Def8;
  end;
  assume [x,y] in (P|X)*R;
  then consider a such that
A2: [x,a] in P|X and
A3: [a,y] in R by Def8;
  [x,a] in P by A2,Def11;
  then
A4: [x,y] in P*R by A3,Def8;
  x in X by A2,Def11;
  hence [x,y] in (P*R)|X by A4,Def11;
