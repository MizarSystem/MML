environ

 vocabularies VECTSP_1, REALSET2, FUNCT_1, BOOLE, BINOP_1, REALSET3, RLVECT_1,
      ARYTM_1, GROUP_1;
 notations TARSKI, ZFMISC_1, SUBSET_1, DOMAIN_1, FUNCT_1, FUNCT_2, BINOP_1,
      STRUCT_0, GROUP_1, RLVECT_1, VECTSP_1, REALSET2;
 constructors BINOP_1, REALSET2;
 registrations SUBSET_1, RELSET_1, REALSET1, STRUCT_0, REALSET2;
 requirements SUBSET, BOOLE;
 definitions BINOP_1, RLVECT_1, REALSET2, GROUP_1;
