  let t be Element of dom T;
  thus succ t c= { t^<*i*>: Q[i,T.t]}
  proof
    let x;
    assume
A195: x in succ t;
    consider l such that
A196: x = t^<*l*> and
A197: t^<*l*> in dom T by A195;
A198: [x,T.x] in T by A196,A197,FUNCT_1:8;
    consider X such that
A199: [x,T.x] in X and
A200: X in rng f by A198,TARSKI:def 4;
    consider y such that
A201: y in NAT and
A202: X = f.y by A125,A200,FUNCT_1:def 5;
    consider T1 being DecoratedTree of D(), k1 such that
A203: y = k1 & f.y = T1 & T1.{} = d() &
    for t being Element of dom T1 holds len t <= k1 &
    (len t < k1 implies succ t = { t^<*i*>: Q[i,T1.t]} &
    for n,x st x = T1.t & Q[n,x] holds T1.(t^<*n*>) = S().(x,n)) by A125,A201;
A204: t^<*l*> in dom T1 by A196,A199,A202,A203,FUNCT_1:8;
    reconsider t9 = t, p = t^<*l*> as Element of dom T1 by A204,TREES_1:46;
A205: len p <= k1 by A203;
A206: len t + 1 <= k1 by A205,FINSEQ_2:19;
A207: len t9 < k1 by A206,NAT_1:13;
A208: succ t9 = { t9^<*i*>: Q[i,T1.t9]} by A203,A207;
A209: T1.t = T.t by A190,A200,A202,A203,A207;
