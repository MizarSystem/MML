A103: x = t^<*n*> and
A104: Q[n,T9.t] by A102;
A105: x = t9^<*n*> by A103;
A106: x in { s^<*l*> where s is Element of dom T: Q[l,T.s] } by A87,A104
      ,A105;
A107: x in dom T9 by A45,A106,XBOOLE_0:def 3;
      thus thesis by A103,A107;
    end;
    let n,x;
    assume that
A108: x = T9.t and
A109: Q[n,x];
A110: t^<*n*> in { t^<*i*>: Q[i,T9.t]} by A108,A109;
A111: t^<*n*> in succ t by A101,A110;
A112: now per cases;
      suppose
A113:   t^<*n*> in dom T;
