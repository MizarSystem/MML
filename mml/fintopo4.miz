theorem
  for n being Nat st n>0 holds (FTSL1 n) is filled
proof
  let n be Nat;assume n>0;then
  A1: (FTSL1 n)=FT_Space_Str(# Seg n,Nbdl1 n #) by AA100;
  for x being Element of (FTSL1 n) holds x in U_FT x
  proof let x be Element of (FTSL1 n);
    x in Seg n by A1;then
    reconsider i=x as Nat;
    (the Nbd of (FTSL1 n)).x={i,max(i-'1,1),min(i+1,n)} by AA2010,A1;then
    U_FT x= {i,max(i-'1,1),min(i+1,n)} by FIN_TOPO:def 1;
    hence x in U_FT x by ENUMSET1:def 1;
  end;
  hence (FTSL1 n) is filled by FIN_TOPO:def 3;
end;

theorem
  for n being Nat st n>0 holds (FTSL1 n) is symmetric
proof
  let n be Nat;
  assume n>0;then
A1: (FTSL1 n)=FT_Space_Str(# Seg n,Nbdl1 n #) by AA100;
  for x, y being Element of (FTSL1 n) holds
    y in U_FT x implies x in U_FT y
  proof
    let x, y be Element of (FTSL1 n);
A2: x in Seg n by A1;then
    reconsider i=x as Nat;
A3: 1<=i & i<=n by A2,FINSEQ_1:3;
    y in Seg n by A1;then
    reconsider j=y as Nat;
    (the Nbd of (FTSL1 n)).x={i,max(i-'1,1),min(i+1,n)} by AA2010,A1;then
A6: U_FT x= {i,max(i-'1,1),min(i+1,n)} by FIN_TOPO:def 1;
    (the Nbd of (FTSL1 n)).y={j,max(j-'1,1),min(j+1,n)}
                                 by A1,AA2010;then
A7: U_FT y= {j,max(j-'1,1),min(j+1,n)} by FIN_TOPO:def 1;
    now assume A8: y in U_FT x;then
A10: y=i or y=max(i-'1,1) or y=min(i+1,n) by ENUMSET1:def 1,A6;
A12: now assume A13: y=max(i-'1,1);
      now per cases;
        case A14: i-'1>=1;then
A15:      y=i-'1 by A13,SQUARE_1:def 2;
          now per cases;
            case i-1>=0;then 
              j=i-1 by A15,BINARITH:def 3;
              hence x =j or x=max(j-'1,1) or x= min(j+1,n)
                                     by SQUARE_1:def 1,A3;
              end;
              case i-1<0;then i-'1=0 by BINARITH:def 3;
                hence contradiction by A14;
              end;
            end;
            hence x =j or x=max(j-'1,1) or x= min(j+1,n);
          end;
          case A17: i-'1<1;then
A18:        y=1 by A13,SQUARE_1:def 2;
            now assume i>1;then
A19:          i-1>0 by SQUARE_1:11;then
              i-'1=i-1 by BINARITH:def 3;then
              i-'1>=0+1 by A19,NAT_1:38;
              hence contradiction by A17;
            end;
            hence x =j or x=max(j-'1,1) or x= min(j+1,n) by A18,A3,AXIOMS:21;
          end;
        end;
        hence x in U_FT y by A7,ENUMSET1:def 1;
      end;
      now assume y=min(i+1,n);
A20:    y=i or y=max(i-'1,1) or y=min(i+1,n) by ENUMSET1:def 1,A6,A8;
        now per cases by A20;
          case y=i;
            hence x =j or x=max(j-'1,1) or x= min(j+1,n);
          end;
          case A21: y=max(i-'1,1);
            now per cases;
              case A22: i-'1>=1;then
A23:            y=i-'1 by A21,SQUARE_1:def 2;
                now per cases;
                  case i-1>=0;then
                   j=i-1 by A23,BINARITH:def 3;
                   hence x =j or x=max(j-'1,1) or x= min(j+1,n)
                                           by SQUARE_1:def 1,A3;
                  end;
                  case i-1<0;then i-'1=0 by BINARITH:def 3;
                    hence contradiction by A22;
                  end;
                end;
                hence x =j or x=max(j-'1,1) or x= min(j+1,n);
              end;
              case A24: i-'1<1;then
A25:            y=1 by A21,SQUARE_1:def 2;
                now assume i>1;then
A26:               i-1>0 by SQUARE_1:11;then
                  i-'1=i-1 by BINARITH:def 3;then
                  i-'1>=0+1 by A26,NAT_1:38;
                  hence contradiction by A24;
                end;
                hence x =j or x=max(j-'1,1) or x= min(j+1,n)
                                           by A25,A3,AXIOMS:21;
              end;
            end;
            hence x =j or x=max(j-'1,1) or x= min(j+1,n);
          end;
          case A27: y=min(i+1,n);
            now per cases;
              case i+1<=n;then
                y=i+1 by A27,SQUARE_1:def 1;then
A28:            j-1=i by XCMPLX_1:26;then
                j-1>0 by A3,AXIOMS:22;then
A29:            j-1=j-'1 by BINARITH:def 3;
                now per cases;
                  case j-1>=1;
                    hence x =j or x=max(j-'1,1) or x= min(j+1,n)
                                    by A29,A28,SQUARE_1:def 2;
                  end;
                  case j-1<1;
                    hence contradiction by A3,A28;
                  end;
                end;
                hence x =j or x=max(j-'1,1) or x= min(j+1,n);
              end;
              case A30: i+1>n;then
                y=n by A27,SQUARE_1:def 1;then
                j+1>n by NAT_1:38;then
A31:            min(j+1,n)=n by SQUARE_1:def 1;
                i>=n by NAT_1:38,A30;
                hence x =j or x=max(j-'1,1) or x= min(j+1,n)
                                           by A31,A3,AXIOMS:21;
              end;
            end;
            hence x =j or x=max(j-'1,1) or x= min(j+1,n);
          end;
        end;
        hence x in U_FT y by A7,ENUMSET1:def 1;
      end;
      hence x in U_FT y by A10,A8,A12;
    end;
    hence y in U_FT x implies x in U_FT y;
  end;
  hence (FTSL1 n) is symmetric by FIN_TOPO:def 14;
end;

