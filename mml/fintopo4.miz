      CONNSP_1, SQUARE_1, FINTOPO3, FINTOPO4;
 notation TARSKI, XBOOLE_0, ZFMISC_1, SUBSET_1, ORDINAL1, NUMBERS, XCMPLX_0,
      XREAL_0, NAT_1, STRUCT_0, RELSET_1, RELAT_1, FUNCT_1, FUNCT_2, CQC_LANG,
      DOMAIN_1, GROUP_1, FINSEQ_1, FINSEQ_4, FIN_TOPO, FINTOPO3, SQUARE_1,
      BINARITH, ENUMSET1, LIMFUNC1, RFUNCT_3;
 constructors NAT_1, CQC_LANG, DOMAIN_1, GROUP_1, FINSEQ_4, MEMBERED, XBOOLE_0,
      FIN_TOPO, FINTOPO3, SQUARE_1, BINARITH, ENUMSET1, LIMFUNC1, RFUNCT_3;
 clusters SUBSET_1, INT_1, RELSET_1, STRUCT_0, CQC_LANG, FINSEQ_1, NAT_1,
      XREAL_0, MEMBERED, ZFMISC_1, XBOOLE_0, NUMBERS, ORDINAL2, FIN_TOPO;
 requirements REAL, NUMERALS, SUBSET, BOOLE, ARITHM;
 definitions TARSKI, GROUP_1, STRUCT_0, ORDINAL1, XBOOLE_0;
 theorems TARSKI, FINSEQ_1, NAT_1, AXIOMS, REAL_1, FUNCT_1, FUNCT_2, XBOOLE_0,
      XBOOLE_1, XCMPLX_1, FIN_TOPO, FINTOPO3, RELAT_1, SQUARE_1, BINARITH,
      ENUMSET1, GOBOARD9, STRUCT_0;
 schemes NAT_1, FUNCT_2;

begin

definition
  let FT be non empty FT_Space_Str,A,B be Subset of FT;
  pred A,B are_separated means
:AA002: A^b misses B & A misses (B^b);
end;

theorem AA004:
  for FT being filled (non empty FT_Space_Str), 
      A being Subset of FT, n,m being Nat st
    n<=m holds Finf(A,n) c= Finf(A,m)
proof
  let FT be filled (non empty FT_Space_Str), A be Subset of FT, n,m be Nat;
  assume n<=m;then A1: m-n>=0 by SQUARE_1:12;
  defpred P[Nat] means Finf(A,n) c= Finf(A,n+$1);
A2: P[0];
A3: for k being Nat st P[k] holds P[k+1]
  proof
    let k be Nat;
    assume A4:P[k];
    Finf(A,n+k) c= Finf(A,n+k+1) by FINTOPO3:37;
    hence P[k+1] by A4,XBOOLE_1:1;
  end;
A6: for m2 being Nat holds P[m2] from NAT_1:sch 1(A2,A3);
  n+(m-'n)=n+(m-n) by A1,BINARITH:def 3
         .=m;
  hence Finf(A,n) c= Finf(A,m) by A6;
end;

theorem
  for FT being filled (non empty FT_Space_Str),
      A being Subset of FT, n,m being Nat st
    n<=m holds Fcl(A,n) c= Fcl(A,m)
proof
  let FT be filled (non empty FT_Space_Str),A be Subset of FT, n,m be Nat;
  assume n<=m;then
A1: m-n>=0 by SQUARE_1:12;
  defpred P[Nat] means Fcl(A,n) c= Fcl(A,n+$1);
A2: P[0];
A3: for k being Nat st P[k] holds P[k+1]
  proof
    let k be Nat;
    assume P[k];then
A4: Fcl(A,n) c= Fcl(A,n+k);
    Fcl(A,n+k) c= Fcl(A,n+k+1) by FINTOPO3:25;
    hence P[k+1] by A4,XBOOLE_1:1;
  end;
  for m2 being Nat holds P[m2] from NAT_1:sch 1(A2,A3);then
A6: P[m-'n];
  n+(m-'n)=n+(m-n) by A1,BINARITH:def 3
         .=m;
  hence Fcl(A,n) c= Fcl(A,m) by A6;
end;

theorem
  for FT being filled (non empty FT_Space_Str),
      A being Subset of FT, n,m being Nat st
    n<=m holds Fdfl(A,m) c= Fdfl(A,n)
proof
  let FT be filled (non empty FT_Space_Str),A be Subset of FT, n,m be Nat;
  assume n<=m;then
A1: m-n>=0 by SQUARE_1:12;
  defpred P[Nat] means Fdfl(A,n+$1) c= Fdfl(A,n);
A2: P[0];
A3: for k being Nat st P[k] holds P[k+1]
  proof let k be Nat;
    assume P[k];then
A4: Fdfl(A,n+k) c= Fdfl(A,n);
    Fdfl(A,n+k+1) c= Fdfl(A,n+k) by FINTOPO3:44;
    hence P[k+1] by A4,XBOOLE_1:1;
  end;
  for m2 being Nat holds P[m2] from NAT_1:sch 1(A2,A3);then
A6: P[m-'n];
  n+(m-'n)=n+(m-n) by A1,BINARITH:def 3
         .=m;
  hence Fdfl(A,m) c= Fdfl(A,n) by A6;
end;

theorem
  for FT being filled (non empty FT_Space_Str), A being Subset of FT,
      n,m being Nat st
    n<=m holds Fint(A,m) c= Fint(A,n)
proof
  let FT be filled (non empty FT_Space_Str),A be Subset of FT, n,m be Nat;
  assume n<=m;then
A1: m-n>=0 by SQUARE_1:12;
  defpred P[Nat] means Fint(A,n+$1) c= Fint(A,n);
A2: P[0];
A3: for k being Nat st P[k] holds P[k+1]
  proof
    let k be Nat;
    assume P[k];then
A4: Fint(A,n+k) c= Fint(A,n);
    Fint(A,n+k+1) c= Fint(A,n+k) by FINTOPO3:26;
    hence P[k+1] by A4,XBOOLE_1:1;
  end;
  for m2 being Nat holds P[m2] from NAT_1:sch 1(A2,A3);then
A6: P[m-'n];
  n+(m-'n)=n+(m-n) by A1,BINARITH:def 3
         .=m;
  hence Fint(A,m) c= Fint(A,n) by A6;
end;

theorem
  for FT being non empty FT_Space_Str,A,B being Subset of FT st
    A,B are_separated holds B,A are_separated
proof
  let FT be non empty FT_Space_Str,A,B be Subset of FT;
  assume A,B are_separated;then
  A^b misses B & A misses (B^b) by AA002;
  hence B,A are_separated by AA002;
end;

theorem AA20:
  for FT being filled (non empty FT_Space_Str),
      A,B being Subset of FT st
    A,B are_separated holds A misses B
proof
  let FT be filled (non empty FT_Space_Str),
  A,B be Subset of FT;
  assume A,B are_separated;then
A1: A^b misses B & A misses (B^b) by AA002;
  A c= A^b by FIN_TOPO:18;
  hence A misses B by A1,XBOOLE_1:63;
end;

theorem 
  for FT being non empty FT_Space_Str,
      A,B being Subset of FT st
    FT is symmetric holds 
    A,B are_separated iff A^f misses B & A misses (B^f)
proof
  let FT be non empty FT_Space_Str,
  A,B be Subset of FT;
  assume A1: FT is symmetric;then
A2: A^b=A^f by FIN_TOPO:17;
    B^b=B^f by FIN_TOPO:17,A1;
  hence thesis by A2,AA002;
end;

theorem AA60:
  for FT being filled (non empty FT_Space_Str),
      A,B being Subset of FT st
    FT is symmetric & A^b misses B holds A misses (B^b)
proof
  let FT be filled (non empty FT_Space_Str),
  A,B be Subset of FT;
  assume A1: FT is symmetric & A^b misses B;
  now assume A2: A meets (B^b);
    consider x being set such that
A3: x in A & x in B^b by XBOOLE_0:3,A2;
A4: A^b= {x2 where x2 is Element of FT:U_FT x2 meets A}
                                        by FIN_TOPO:def 10;
    B^b= {x2 where x2 is Element of FT:U_FT x2 meets B}
                                    by FIN_TOPO:def 10;then
    consider x2 being Element of FT such that
A5: x2=x & U_FT x2 meets B by A3;
A6: U_FT x2 /\ B <>{} by XBOOLE_0:def 7,A5;
    consider y being Element of U_FT x2 /\ B;
A7: y in U_FT x2 & y in B by XBOOLE_0:def 3,A6;then
    reconsider y2=y as Element of FT;
    x2 in U_FT y2 by A1,A7,FIN_TOPO:def 14;then
    x2 in U_FT y2 /\ A by A3,A5,XBOOLE_0:def 3;then
    U_FT y2 meets A by XBOOLE_0:def 7;then
    y2 in A^b by A4;
    hence contradiction by A1,A7,XBOOLE_0:3;
  end;
  hence thesis;
end;

theorem AA70:
  for FT being filled (non empty FT_Space_Str),
      A,B being Subset of FT st
    FT is symmetric & A misses (B^b) holds A^b misses B by AA60;

theorem
  for FT being filled (non empty FT_Space_Str),
      A,B being Subset of FT st
    FT is symmetric holds
    A,B are_separated iff A^b misses B
proof
  let FT be filled (non empty FT_Space_Str),
  A,B be Subset of FT;
  assume A1: FT is symmetric;
  thus A,B are_separated implies A^b misses B by AA002;
  thus A^b misses B implies A,B are_separated
  proof assume A2: A^b misses B;then
    A misses (B^b) by AA60,A1;
    hence A,B are_separated by AA002,A2;
  end;
end;

theorem
  for FT being filled (non empty FT_Space_Str),
      A,B being Subset of FT st
    FT is symmetric holds
    A,B are_separated iff A misses (B^b)
proof
  let FT be filled (non empty FT_Space_Str),
  A,B be Subset of FT;
  assume A1: FT is symmetric;
  thus A,B are_separated implies A misses (B^b) by AA002;
  thus A misses (B^b) implies A,B are_separated
  proof
    assume A2: A misses (B^b);then
    A^b misses B by AA70,A1;
    hence A,B are_separated by AA002,A2;
  end;
end;

theorem AA80:
  for FT being filled (non empty FT_Space_Str),
      IT being Subset of FT st
    FT is symmetric holds IT is connected iff
   (for A, B being Subset of FT st
      IT = A \/ B & A,B are_separated holds
      A = IT or B = IT)
proof
  let FT be filled(non empty FT_Space_Str),IT be Subset of FT;
  assume A1: FT is symmetric;
A2: now assume IT is connected;then
A3: for A,B being Subset of FT st
      IT = A \/ B & A <> {} & B <> {} & A misses B holds
      A^b meets B by FIN_TOPO:def 17;
    thus
    (for A, B being Subset of FT st
       IT = A \/ B & A,B are_separated holds A = IT or B = IT)
    proof
      let A, B be Subset of FT;
      assume A4: IT = A \/ B & A,B are_separated;then
A5:   A misses B by AA20;
      now assume A7: A<>{};
        now assume B<>{};then
A8:       A^b meets B by A3,A7,A5,A4;
          hence contradiction by AA002,A4;
        end;
        hence A = IT or B = IT by A4;
      end;
      hence A = IT or B = IT by A4;
    end;
  end;
  now assume
A9: (for A, B being Subset of FT st
       IT = A \/ B & A,B are_separated holds
       A = IT or B = IT);
    for A,B being Subset of FT st
      IT = A \/ B & A <> {} & B <> {} & A misses B holds
      A^b meets B
    proof
      let A,B be Subset of FT;
      assume A10: IT = A \/ B & A <> {} & B <> {} & A misses B;
      now assume A11:A^b misses B; then
a11:    A^b /\ B={} by XBOOLE_0:def 7;
        A c= A^b by FIN_TOPO:18;then
        A /\ B c= A^b /\ B by XBOOLE_1:26;then
A12:    A /\ B ={} by a11,XBOOLE_1:3;
        now assume A13: A meets (B^b);
          consider x being set such that
A14:      x in A & x in B^b by XBOOLE_0:3,A13;
A15:      A^b= {x2 where x2 is Element of FT:U_FT x2 meets A}
                                     by FIN_TOPO:def 10;
          B^b= {x2 where x2 is Element of FT:U_FT x2 meets B}
                                     by FIN_TOPO:def 10;then
          consider x2 being Element of FT such that
A16:      x2=x & U_FT x2 meets B by A14;
A17:      U_FT x2 /\ B <>{} by XBOOLE_0:def 7,A16;
          consider y being Element of U_FT x2 /\ B;
A18:      y in U_FT x2 & y in B by XBOOLE_0:def 3,A17;then
          reconsider y2=y as Element of FT;
          x2 in U_FT y2 by A1,A18,FIN_TOPO:def 14;then
          x2 in U_FT y2 /\ A by A14,A16,XBOOLE_0:def 3;then
          U_FT y2 meets A by XBOOLE_0:def 7;then
          y2 in A^b by A15;
          hence contradiction by A11,A18,XBOOLE_0:3;
        end;then
        A,B are_separated by AA002,A11;then
        A=IT or B=IT by A9,A10;
        hence contradiction by A10,A12,XBOOLE_1:21;
      end;
      hence A^b meets B;
    end;
    hence IT is connected by FIN_TOPO:def 17;
  end;
  hence thesis by A2;
end;

theorem
  for FT being filled (non empty FT_Space_Str),
      B being Subset of FT st FT is symmetric holds
    B is connected iff
    not (ex C being Subset of FT st
    C<>{} & B\C <>{} & C c= B & (C^b) misses (B\C))
proof
  let FT be filled (non empty FT_Space_Str),
  B be Subset of FT;
  assume A1: FT is symmetric;
  thus B is connected implies not (ex C being Subset of FT st
    C<>{} & B\C <>{} & C c= B & (C^b) misses (B\C))
  proof
    assume A2: B is connected;
    for C being Subset of FT st C c= B & (C^b) misses (B\C) holds
      C={} or B\C={}
    proof
      let C be Subset of FT;
      assume A3: C c= B & (C^b) misses (B\C);
A4:   C \/ (B\C)=C \/ B by XBOOLE_1:39
               .=B by A3,XBOOLE_1:12;
      C^b misses (B\C) & C misses ((B\C)^b) by A3,AA60,A1;then
      C,B\C are_separated by AA002;then
      C = B or B\C = B by AA80,A2,A4,A1;
      hence C={} or B\C={} by A3,XBOOLE_1:38,XBOOLE_1:37;
    end;
    hence not (ex C being Subset of FT st C<>{} & B\C <>{} &
    C c= B & (C^b) misses (B\C));
  end;
  thus not (ex C being Subset of FT st C<>{} & B\C <>{} &
   C c= B & (C^b) misses (B\C)) implies B is connected
  proof
    assume not (ex C being Subset of FT st
      C<>{} & B\C <>{} & C c= B & (C^b) misses (B\C));then
A5: for C being Subset of FT st
      C c= B & (C^b) misses (B\C) holds C={} or B\C={};
    for A, B2 being Subset of FT st
      B = A \/ B2 & A,B2 are_separated holds
    A = B or B2 = B
    proof
      let A, B2 be Subset of FT;
      assume A6: B = A \/ B2 & A,B2 are_separated;then
A7:   A c= B by XBOOLE_1:7;
A8:   A^b misses B2 & A misses (B2^b) by A6,AA002;
A9:   (A \/ B2) \A =B2\A by XBOOLE_1:40;
      B\A c= B2 by A9,A6,XBOOLE_1:36;then
      A^b misses (B\A) by A8,XBOOLE_1:63;then
      A={} or B\A={} by A5,A7;then
      B=B2 or B c= A by XBOOLE_1:37,A6;
      hence A = B or B2 = B by A7,XBOOLE_0:def 10;
    end;
    hence B is connected by AA80,A1;
  end;
end;

definition
  let FT1,FT2 be non empty FT_Space_Str,
  f be Function of (the carrier of FT1),(the carrier of FT2),n be Nat;
  pred f is_continuous n means
:AA600: for x being Element of FT1,y being Element of FT2 st
   x in the carrier of FT1 & y=f.x holds
  f.:( U_FT(x,0)) c= U_FT(y,n);
end;

theorem
  for FT1 being non empty FT_Space_Str,
      FT2 being filled (non empty FT_Space_Str), n being Nat,
      f being Function of (the carrier of FT1),(the carrier of FT2) st
    f is_continuous 0 holds f is_continuous n
proof
  let FT1 be non empty FT_Space_Str,
  FT2 be filled (non empty FT_Space_Str),n be Nat,
  f be Function of (the carrier of FT1),(the carrier of FT2);
  assume f is_continuous 0;then
A1: for x being Element of FT1,y being Element of FT2 st
      x in the carrier of FT1 & y=f.x holds
      f.:( U_FT(x,0)) c= U_FT(y,0) by AA600;
  for x being Element of FT1,y being Element of FT2 st
      x in the carrier of FT1 & y=f.x holds
    f.:( U_FT(x,0)) c= U_FT(y,n)
  proof
    let x be Element of FT1,y be Element of FT2;
    assume A2: x in the carrier of FT1 & y=f.x;
A3: U_FT y =U_FT(y,0) by FINTOPO3:47;
    U_FT(y,n)=Finf((U_FT y),n) by FINTOPO3:def 10;then
A4: U_FT(y,0) c= U_FT(y,n) by A3,FINTOPO3:36;
    f.:( U_FT(x,0)) c= U_FT(y,0) by A1,A2;
    hence f.:( U_FT(x,0)) c= U_FT(y,n) by A4,XBOOLE_1:1;
  end;
  hence f is_continuous n by AA600;
end;

theorem
  for FT1 being non empty FT_Space_Str,
      FT2 being filled (non empty FT_Space_Str), n0,n being Nat,
      f being Function of (the carrier of FT1),(the carrier of FT2) st
    f is_continuous n0 & n0<=n holds f is_continuous n
proof
  let FT1 be non empty FT_Space_Str,
  FT2 be filled (non empty FT_Space_Str),n0,n be Nat,
  f be Function of (the carrier of FT1),(the carrier of FT2);
  assume A1: f is_continuous n0 & n0<=n;then
A2: for x being Element of FT1,y being Element of FT2 st
      x in the carrier of FT1 & y=f.x holds
   f.:( U_FT(x,0)) c= U_FT(y,n0) by AA600;
  for x being Element of FT1,y being Element of FT2 st
    x in the carrier of FT1 & y=f.x holds
    f.:( U_FT(x,0)) c= U_FT(y,n)
  proof
    let x be Element of FT1,y be Element of FT2;
    assume A3: x in the carrier of FT1 & y=f.x;
A4: U_FT(y,n0)=Finf((U_FT y),n0) by FINTOPO3:def 10;
    U_FT(y,n)=Finf((U_FT y),n) by FINTOPO3:def 10;then
A5: U_FT(y,n0) c= U_FT(y,n) by A4,AA004,A1;
    f.:( U_FT(x,0)) c= U_FT(y,n0) by A2,A3;
    hence f.:( U_FT(x,0)) c= U_FT(y,n) by A5,XBOOLE_1:1;
  end;
  hence f is_continuous n by AA600;
end;

theorem AA900:
  for FT1,FT2 being non empty FT_Space_Str,
      A being Subset of FT1,B being Subset of FT2,
      f being Function of (the carrier of FT1),(the carrier of FT2) st
    f is_continuous 0 & B=f.:A holds
    f.:(A^b) c= B^b
proof
  let FT1,FT2 be non empty FT_Space_Str,A be Subset of FT1,
  B be Subset of FT2,
  f be Function of (the carrier of FT1),(the carrier of FT2);
  assume A1: f is_continuous 0 & B=f.:A;
  thus f.:(A^b) c= B^b
  proof
    let y be set;assume y in f.:(A^b);then
    consider x being set such that
A2: x in dom f & x in A^b & y=f.x by FUNCT_1:def 12;
    reconsider x0=x as Element of FT1 by A2;
    reconsider y0=y as Element of FT2 by FUNCT_2:7,A2;
    x in {z where z is Element of FT1:U_FT z meets A}
                           by FIN_TOPO:def 10,A2;then
    consider z being Element of FT1 such that
A3: z=x & U_FT z meets A;
A4: U_FT x0 /\ A<>{} by A3,XBOOLE_0:def 7;
    f.:( U_FT(x0,0)) c= U_FT(y0,0) by A2,A1,AA600;then
    f.:(U_FT x0) c=  U_FT(y0,0) by FINTOPO3:47;then
A5: f.:(U_FT x0) c=  U_FT y0 by FINTOPO3:47;
    dom f=the carrier of FT1 by FUNCT_2:def 1;then
A6: f.:((U_FT x0) /\ A)<>{} by A4,RELAT_1:152;
A7: f.:((U_FT x0) /\ A) c= (f.:((U_FT x0)))/\ (f.:A) by RELAT_1:154;
    (f.:((U_FT x0)))/\ (f.:A) c= (U_FT y0) /\ (f.:A) by A5,XBOOLE_1:26;then
    f.:((U_FT x0) /\ A) c= (U_FT y0) /\ (f.:A) by A7,XBOOLE_1:1;then
    (U_FT y0) /\ (f.:A)<>{} by A6,XBOOLE_1:3;then
    U_FT y0 meets B by A1,XBOOLE_0:def 7;then
    y in {z2 where z2 is Element of FT2:U_FT z2 meets B};
    hence y in B^b by FIN_TOPO:def 10;
  end;
end;

theorem
  for FT1,FT2 being non empty FT_Space_Str,A being Subset of FT1,
      B being Subset of FT2,
      f being Function of (the carrier of FT1),(the carrier of FT2) st
    A is connected & f is_continuous 0 & B=f.:A holds B is connected
proof
  let FT1,FT2 be non empty FT_Space_Str,A be Subset of FT1,
  B be Subset of FT2,
  f be Function of (the carrier of FT1),(the carrier of FT2);
  assume A1: A is connected & f is_continuous 0 & B=f.:A;then
A2: for B1,C1 being Subset of FT1 st
    A = B1 \/ C1 & B1 <> {} & C1 <> {} & B1 misses C1 holds
   B1^b meets C1 by FIN_TOPO:def 17;
  for B2,C2 being Subset of FT2 st
    B = B2 \/ C2 & B2 <> {} & C2 <> {} & B2 misses C2 holds
   B2^b meets C2
  proof let B2,C2 be Subset of FT2;
    assume A4: B = B2 \/ C2 & B2 <> {} & C2 <> {} & B2 misses C2;
    reconsider B1 = f"B2 as Subset of FT1;
A5: f.:B1 c= B2
    proof
      let y be set;
      assume y in f.:B1;then
      consider x2 being set such that
A6:   x2 in dom f & x2 in B1 & y=f.x2 by FUNCT_1:def 12;
      thus y in B2 by A6,FUNCT_1:def 13;
    end;
    reconsider B20 = f.:B1 as Subset of FT2;
A7: B20^b c= B2^b by A5,FIN_TOPO:19;
    reconsider B10=B1/\A as Subset of FT1;
    reconsider C1=f"C2 as Subset of FT1;
    reconsider C10=C1 /\ A as Subset of FT1;
    A c= the carrier of FT1;then
A8: A c= dom f by FUNCT_2:def 1;
    (f"B2) \/ (f"C2)=f"(f.:A) by A1,A4,RELAT_1:175;then
    A c= B1 \/ C1 by A8,FUNCT_1:146;then
