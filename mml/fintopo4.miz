    (dom f1 =Seg n & for i being Nat st i in Seg n holds
     (1<i & i<n implies f1.i={i,i-'1,i+1})&
     (i=1 & i<n implies f1.i={i,n,i+1}) &
     (1<i & i=n implies f1.i={i,i-'1,1}) &
