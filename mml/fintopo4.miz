definition let n be Nat;
  func Nbdl1 n -> Function of Seg n,bool (Seg n) means
:AA2010: dom it =Seg n & for i being Nat st
   i in Seg n holds it.i={i,max(i-'1,1),min(i+1,n)};
existence
  proof
    defpred P[set,set] means for i being Nat st i=$1 holds
    $2={i,max(i-'1,1),min(i+1,n)};
A1: for x being set st x in Seg n 
        ex y being set st y in bool (Seg n) & P[x,y]
    proof
      let x being set;
      assume A2: x in Seg n;then
      reconsider i=x as Nat;
      n>0 by NAT_1:19,FINSEQ_1:4,A2;then
A3:   0+1<=n by NAT_1:38;
      set y0={i,max(i-'1,1),min(i+1,n)};
      y0 c=Seg n
      proof
        let z1 being set;
        assume z1 in y0;then
A4:     z1=i or z1=max(i-'1,1) or z1=min(i+1,n) by ENUMSET1:def 1;
A6:     now assume A7: z1=max(i-'1,1);
A9:       i-'1<=i by GOBOARD9:2;
          i<=n by A2,FINSEQ_1:3;then
          i-'1<=n by A9,AXIOMS:22;then
          1<= max(i-'1,1) & max(i-'1,1)<=n by SQUARE_1:50,A3;
          hence z1 in Seg n by A7,FINSEQ_1:3;
        end;
        now assume A12: z1=min(i+1,n);
          1<=i+1 by NAT_1:37;then
A13:      1<=min(i+1,n) by A3,SQUARE_1:39;
          min(i+1,n)<=n by SQUARE_1:35;
          hence z1 in Seg n by A12,FINSEQ_1:3,A13;
        end;
        hence z1 in Seg n by A2,A4,A6;
      end;then
A14:  y0 in bool (Seg n);
      P[x,y0];
      hence ex y being set st y in bool (Seg n) & P[x,y] by A14;
    end;
    consider f2 being Function of Seg n,bool(Seg n) such that
A15: for x3 being set st
    x3 in Seg n holds P[x3,f2.x3] from FUNCT_2:sch 1(A1);
A16: for i being Nat st i in Seg n holds
    f2.i={i,max(i-'1,1),min(i+1,n)} by A15;
    dom f2=Seg n by FUNCT_2:def 1;
