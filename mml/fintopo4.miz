        U_FT x= {i,n,i+1} by FIN_TOPO:def 1,A3,A7;
        hence x in U_FT x by ENUMSET1:def 1;
      end;
      case 1<i & i=n;then
        U_FT x= {i,i-'1,1} by FIN_TOPO:def 1,A3,A7;
        hence x in U_FT x by ENUMSET1:def 1;
      end;
      case i=1 & i=n;then
        U_FT x= {i} by FIN_TOPO:def 1,A3,A7;
        hence x in U_FT x by TARSKI:def 1;
      end;
    end;
    hence x in U_FT x;
  end;
  hence (FTSC1 n) is filled by FIN_TOPO:def 3;
end;

theorem
  for n being Nat st n>0 holds (FTSC1 n) is symmetric
proof
  let n be Nat;
  assume n>0;then
A1: (FTSC1 n)=FT_Space_Str(# Seg n,Nbdc1 n #) by AA200;
  for x, y being Element of (FTSC1 n) holds
    y in U_FT x implies x in U_FT y
  proof
    let x, y be Element of (FTSC1 n);
A2: x in Seg n by A1;then
    reconsider i=x as Nat;
A3: y in Seg n by A1;then
    reconsider j=y as Nat;
A4: 1<=j & j<=n by A3,FINSEQ_1:3;
A5: the Nbd of (FTSC1 n)=Nbdc1 n by A1;
    set f=Nbdc1 n;
A6: 1<=i & i<=n by A2,FINSEQ_1:3;then
A7: 1<i or i=1 by REAL_1:def 5;
A8: i<n or i=n by A6,REAL_1:def 5;
A9: (1<i & i<n implies f.i={i,i-'1,i+1})&
    (i=1 & i<n implies f.i={i,n,i+1}) &
    (1<i & i=n implies f.i={i,i-'1,1}) &
    (i=1 & i=n implies f.i={i}) by A2,AA2010;
    now assume A10: y in U_FT x;
      now per cases by A7,A8;
        case A11: 1<i & i<n;then
          U_FT x= {i,i-'1,i+1} by FIN_TOPO:def 1,A5,A9;then
A14:      y=i or y=i-'1 or y=i+1 by ENUMSET1:def 1,A10;
A17:      now assume A18: y=i-'1;
            i-1>0 by A11,SQUARE_1:11;then
            i-'1=i-1 by BINARITH:def 3;then
A19:        j+1=i by A18;
A20:        x=j or x=j-'1 or x=j+1 by A19;
            now per cases by A20;
              case A21: x=j;then
                (the Nbd of (FTSC1 n)).y={j,j-'1,j+1}
                            by A5,A11,AA2010,A3;then
                U_FT y = {j,j-'1,j+1} by FIN_TOPO:def 1;
                hence x in U_FT y by A21,ENUMSET1:def 1;
              end;
              case x=j-'1;then
A22:            i=(i-'1)-'1 by A18;
                now assume i<>0;then i>0 by NAT_1:19;then
A24:              i>=0+1 by NAT_1:38;then
                  i-1>=1-1 by REAL_1:49;then
A25:              i-1=i-'1 by BINARITH:def 3;
                  now assume A26: i=1;then
A27:                i-1=0;
                    i-'1-1<0 by A27,A25;
                    hence contradiction by A26,A22,BINARITH:def 3;
                  end;then
                  i>1 by A24,REAL_1:def 5;then
                  i-1>1-1 by REAL_1:54;then
                  i-'1>=0+1 by NAT_1:38,A25;then
                  i-'1-1>=0 by SQUARE_1:12;then
                  i-'1-'1=i-'1-1 by BINARITH:def 3;then
                  i+1=i-1 by A22,A25;then
A28:              i+1+1=i;
                  i<i+1 by NAT_1:38;
                  hence contradiction by A28,NAT_1:38;
                end;
                hence x in U_FT y by A11;
              end;
              case x=j+1;then
                i>0 by NAT_1:19;then
                i>=0+1 by NAT_1:38;then
                i-1>=0 by SQUARE_1:12;then
                y=i-1 by A18,BINARITH:def 3;then
A33:            j+1=x by XCMPLX_1:27;then
A34:            j<n by A6,NAT_1:38;
A35:            now assume j=1;then
                  (the Nbd of (FTSC1 n)).y={j,n,j+1}
                           by A5,AA2010,A3,A34;then
                  U_FT y = {j,n,j+1} by FIN_TOPO:def 1;
                  hence x in U_FT y by A33,ENUMSET1:def 1;
                end;
                now assume j<>1;then
                  j>1 by REAL_1:def 5,A4;then
                  (the Nbd of (FTSC1 n)).y={j,j-'1,j+1}
                              by A34,A5,AA2010,A3;then
                  U_FT y = {j,j-'1,j+1} by FIN_TOPO:def 1;
                  hence x in U_FT y by A33,ENUMSET1:def 1;
                end;
                hence x in U_FT y by A35;
              end;
            end;
            hence x in U_FT y;
          end;
          now assume 
A36:        y=i+1;then
A37:        j-1=x by XCMPLX_1:26;then 
            j-1>0 by A11,AXIOMS:22;then
A38:        x=j or x=j-'1 or x=j+1 by A37,BINARITH:def 3;
            now per cases by A38;
              case A39: x=j;then
                (the Nbd of (FTSC1 n)).y={j,j-'1,j+1}
                          by A5,A11,AA2010,A3;then
                U_FT y = {j,j-'1,j+1} by FIN_TOPO:def 1;
                hence x in U_FT y by A39,ENUMSET1:def 1;
              end;
              case A40: x=j-'1;
                now assume j=1;then
                  j-1=0;then
                  x=0 by A40,BINARITH:def 3;
                  hence contradiction by A11;
                end;then
A42:            j>1 by REAL_1:def 5,A4;
A43:            now assume j=n;then
                  (the Nbd of (FTSC1 n)).y={j,j-'1,1}
                         by A5,A42,AA2010,A3;then
                  U_FT y = {j,j-'1,1} by FIN_TOPO:def 1;
                  hence x in U_FT y by A40,ENUMSET1:def 1;
                end;
                now assume j<>n;then
                  j<n by REAL_1:def 5,A4;then
                  (the Nbd of (FTSC1 n)).y={j,j-'1,j+1}
                             by A5,A42,AA2010,A3;then
                  U_FT y = {j,j-'1,j+1} by FIN_TOPO:def 1;
                  hence x in U_FT y by A40,ENUMSET1:def 1;
                end;
                hence x in U_FT y by A43;
              end;
              case x=j+1;then
A44:            i=(i+1)+1 by A36;
                i<i+1 by NAT_1:38;
                hence contradiction by A44,NAT_1:38;
              end;
            end;
            hence x in U_FT y;
          end;
          hence x in U_FT y by A14,A10,A17;
        end;
        case  A46: i=1 & i<n;then
          U_FT x= {i,n,i+1} by FIN_TOPO:def 1,A5,A9;then
A47:      y=i & y<>n or y=n or y=i+1& y<>n by ENUMSET1:def 1,A10;
          now per cases by A47;
            case A48: y=i & y<>n;then
              U_FT y= {j,n,j+1} by FIN_TOPO:def 1,A5,A9,A46;
              hence x in U_FT y by A48,ENUMSET1:def 1;
            end;
            case y=n;then
             (the Nbd of (FTSC1 n)).y={j,j-'1,1} by A46,A3,AA2010,A5;then
              U_FT y= {j,j-'1,1} by FIN_TOPO:def 1;
              hence x in U_FT y by A46,ENUMSET1:def 1;
            end;
            case A50: y=i+1 & y<>n;then
A51:          j<n by A4,REAL_1:def 5;
              j-1=i by A50,XCMPLX_1:26;then
A53:          j-'1=i by BINARITH:def 3,A46;
              (the Nbd of (FTSC1 n)).y={j,j-'1,j+1}
                             by A46,A51,A3,AA2010,A5,A50;then
              U_FT y= {j,j-'1,j+1} by FIN_TOPO:def 1;
              hence x in U_FT y by A53,ENUMSET1:def 1;
            end;
          end;
          hence x in U_FT y;
        end;
        case A54: 1<i & i=n;then
          U_FT x= {i,i-'1,1} by FIN_TOPO:def 1,A5,A9;then
A55:      y=i or y=i-'1 or y=1 by ENUMSET1:def 1,A10;
          now per cases by A55;
            case A56: y=i & y<>1;then
              U_FT y= {j,j-'1,1} by FIN_TOPO:def 1,A5,A9,A54;
              hence x in U_FT y by A56,ENUMSET1:def 1;
            end;
            case A57: y=1;
              (the Nbd of (FTSC1 n)).y={j,n,j+1}
                  by A57,A3,AA2010,A5,A54;then
              U_FT y= {j,n,j+1} by FIN_TOPO:def 1;
              hence x in U_FT y by A54,ENUMSET1:def 1;
            end;
            case A58: y=i-'1 & y<>1;then
A59:          1<j by A4,REAL_1:def 5;
A60:          j=n-'1 by A58,A54;
              n-1>0 by SQUARE_1:11,A54;then
A61:          n-1=n-'1 by BINARITH:def 3;
A62:          n-1+1=n;then
A63:          j+1=i by A61,A60,A54;
              j<n by A61,A62,A60,NAT_1:38;then
              (the Nbd of (FTSC1 n)).y={j,j-'1,j+1} by A59,A3,AA2010,A5;then
              U_FT y= {j,j-'1,j+1} by FIN_TOPO:def 1;
              hence x in U_FT y by A63,ENUMSET1:def 1;
            end;
          end;
          hence x in U_FT y;
        end;
        case i=1 & i=n;then
          U_FT x= {i} by FIN_TOPO:def 1,A5,A9;then
          j=i by A10,TARSKI:def 1;
          hence x in U_FT y by A10;
        end;
      end;
      hence x in U_FT y;
    end;
    hence y in U_FT x implies x in U_FT y;
  end;
  hence (FTSC1 n) is symmetric by FIN_TOPO:def 14;
end;
