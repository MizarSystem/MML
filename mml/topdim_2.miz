  let TM such that
A1: TM is second-countable finite-ind and
A2: ind TM<=I;
  -1<=ind[#]TM by A1,TOPDIM_1:5;
  then -1<=I by A2,XXREAL_0:2;
  then -1+1<=I+1 by XREAL_1:8;
