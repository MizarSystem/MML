        then
A56:    p`1/p`2/p`2 <=1/p`2 & -(1/p`2)<= p`1/p`2/p`2 or
        p`1/p`2/p`2 >=1/p`2 & p`1/p`2/p`2<= -(1/p`2)
         by A53,A55,XREAL_1:74;
        p9`2=1/p`2 & p9`1=p`1/p`2/p`2 by EUCLID:56;
        hence y in K1 by A43,A51,A52,A56;
      end;
      case
A57:    p`2<0;

then p`1<=p`2 & (-1 *p`2)<=p`1 or p`1/p`2<=p`2/p`2 & p`1/p`2>=(-1 *p`2)/ p`2
        by A44,XREAL_1:75;
        then
B58:     p`1<=p`2 & (-1 *p`2)<=p`1 or p`1/p`2<=1 & p`1/p`2>=(-1)*p`2/p`2
        by A57,XCMPLX_1:60;
        then
        p`1/p`2>=1 & (-1)*p`2/p`2>=p`1/p`2 or p`1/p`2<=1 & p`1/p`2 >=-1
        by A57,XCMPLX_1:90;
        then (-1)/p`2>= p`1/p`2/p`2 by A57,XREAL_1:75;
        then
A59:    p`1/p`2/p`2 <=1/p`2 & -(1/p`2)<= p`1/p`2/p`2 or
        p`1/p`2/p`2 >=1/p`2 & p`1/p`2/p`2<= -(1/p`2)
         by B58,A57,XREAL_1:75;
        p9`2=1/p`2 & p9`1=p`1/p`2/p`2 by EUCLID:56;
        hence y in K1 by A43,A51,A52,A59;
      end;
    end;
    then y in [#](((TOP-REAL 2)|D)|K1) by PRE_TOPC:def 10;
    hence thesis;
  end;
  then reconsider g=Out_In_Sq|K1 as Function of ((TOP-REAL 2)|D)|K1,
  ((TOP-REAL 2)|D) by A38,A39,FUNCT_2:4,XBOOLE_1:1;
A60: K0=[#](((TOP-REAL 2)|D)|K0) by PRE_TOPC:def 10;
A61: K1=[#](((TOP-REAL 2)|D)|K1) by PRE_TOPC:def 10;
A62: D= [#]((TOP-REAL 2)|D) by PRE_TOPC:def 10;
  D c= K0 \/ K1
  proof
    let x be set;
    assume
A67: x in D;
    then
A68: x in (the carrier of TOP-REAL 2) & not x in {0.TOP-REAL 2}
    by A2,XBOOLE_0:def 5;
    reconsider px=x as Point of TOP-REAL 2 by A67;
    (px`2<=px`1 & -px`1<=px`2 or px`2>=px`1 & px`2<=-px`1) & px<>0.TOP-REAL 2
     or
    (px`1<=px`2 & -px`2<=px`1 or px`1>=px`2 & px`1<=-px`2)
    & px<>0.TOP-REAL 2 by A68,TARSKI:def 1,XREAL_1:28;
    then x in K0 or x in K1;
    hence thesis by XBOOLE_0:def 3;
  end;
  then
A69: ([#](((TOP-REAL 2)|D)|K0)) \/ ([#](((TOP-REAL 2)|D)|K1))
  = [#]((TOP-REAL 2)|D) by A60,A61,A62,XBOOLE_0:def 10;
  f=Out_In_Sq|K0 & D=NonZero TOP-REAL 2
  & K0={p:(p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1) & p<>0.TOP-REAL 2}
   by A2;
  then
A70: f is continuous & K0 is closed by Th48;
  g=Out_In_Sq|K1 & D=NonZero TOP-REAL 2
  & K1={p:(p`1<=p`2 & -p`2<=p`1 or p`1>=p`2 & p`1<=-p`2) & p<>0.TOP-REAL 2}
   by A2;
  then
A71: g is continuous & K1 is closed by Th49;
A72: for x be set st x in ([#]((((TOP-REAL 2)|D)|K0)))
  /\ ([#] ((((TOP-REAL 2)|D)|K1))) holds f.x = g.x
  proof
    let x be set;
    assume
Z:   x in ([#]((((TOP-REAL 2)|D)|K0))) /\ ([#] ((((TOP-REAL 2)|D)|K1)));
    then x in K0 & x in K1 by A60,A61,XBOOLE_0:def 4;
    then f.x=Out_In_Sq.x by FUNCT_1:72;
    hence thesis by Z,A61,FUNCT_1:72;
  end;
  then consider h being Function of (TOP-REAL 2)|D,(TOP-REAL 2)|D such that
A74: h= f+*g & h is continuous by A60,A61,A69,A70,A71,Th9;
A75: dom h=the carrier of ((TOP-REAL 2)|D) by FUNCT_2:def 1;
A76: the carrier of ((TOP-REAL 2)|D) =[#](((TOP-REAL 2)|D))
    .=(NonZero TOP-REAL 2) by A2,PRE_TOPC:def 10;
  then
A77: dom Out_In_Sq=the carrier of ((TOP-REAL 2)|D) by FUNCT_2:def 1;
A78: dom f=K0 by A12,FUNCT_2:def 1;
A79: K0=[#](((TOP-REAL 2)|D)|K0) by PRE_TOPC:def 10;
A80: dom g=K1 by A39,FUNCT_2:def 1;
  K1=[#](((TOP-REAL 2)|D)|K1) by PRE_TOPC:def 10;
  then
A81: f tolerates g by A72,A78,A79,A80,PARTFUN1:def 6;
  for x being set st x in dom h holds h.x=Out_In_Sq.x
  proof
    let x be set;
    assume
A82: x in dom h;
    then x in (the carrier of TOP-REAL 2) & not x in {0.TOP-REAL 2}
    by A76,XBOOLE_0:def 5;
    then
A83: x <>0.TOP-REAL 2 by TARSKI:def 1;
    reconsider p=x as Point of TOP-REAL 2 by A76,A82,XBOOLE_0:def 5;
    now per cases;
      case
A84:    x in K0;
        h.p=(g+*f).p by A74,A81,FUNCT_4:35
          .=f.p by A78,A84,FUNCT_4:14;
        hence thesis by A84,FUNCT_1:72;
      end;
      case not x in K0;
        then not (p`2<=p`1 & -p`1<=p`2 or p`2>=p`1 & p`2<=-p`1) by A83;
        then (p`1<=p`2 & -p`2<=p`1 or p`1>=p`2 & p`1<=-p`2)by XREAL_1:28;
        then
A85:    x in K1 by A83;
        then Out_In_Sq.p=g.p by FUNCT_1:72;
        hence thesis by A74,A80,A85,FUNCT_4:14;
      end;
    end;
    hence thesis;
  end;
  then f+*g=Out_In_Sq by A74,A75,A77,FUNCT_1:9;
  hence thesis by A60,A61,A69,A70,A71,A72,Th9;
end;

theorem Th51:
  for B,K0,Kb being Subset of TOP-REAL 2 st B={0.TOP-REAL 2}
  & K0={p: -1<p`1 & p`1<1 & -1<p`2 & p`2<1}
  & Kb={q: -1=q`1 & -1<=q`2 & q`2<=1 or q`1=1 & -1<=q`2 & q`2<=1
  or -1=q`2 & -1<=q`1 & q`1<=1 or 1=q`2 & -1<=q`1 & q`1<=1}
  ex f being Function of (TOP-REAL 2)|B`,(TOP-REAL 2)|B` st
  f is continuous & f is one-to-one &
  (for t being Point of TOP-REAL 2 st t in K0 & t<>0.TOP-REAL 2 holds
  not f.t in K0 \/ Kb)
  &(for r being Point of TOP-REAL 2 st not r in K0 \/ Kb holds f.r in K0)
  &(for s being Point of TOP-REAL 2 st s in Kb holds f.s=s)
proof
  let B,K0,Kb be Subset of TOP-REAL 2;
  assume
A1: B={0.TOP-REAL 2} & K0={p: -1<p`1 & p`1<1 & -1<p`2 & p`2<1}
  & Kb={q: -1=q`1 & -1<=q`2 & q`2<=1 or q`1=1 & -1<=q`2 & q`2<=1
  or -1=q`2 & -1<=q`1 & q`1<=1 or 1=q`2 & -1<=q`1 & q`1<=1};
  then
A2: B`=NonZero TOP-REAL 2 by SUBSET_1:def 5;
  reconsider D=B` as non empty Subset of TOP-REAL 2 by A1,Th19;
A3: D`={0.TOP-REAL 2} by A1;
  then consider h being Function of (TOP-REAL 2)|D,(TOP-REAL 2)|D such that
A4: h=Out_In_Sq & h is continuous by Th50;
A5: D =(NonZero TOP-REAL 2) by A1,SUBSET_1:def 5;
  set K0a={p8 where p8 is Point of TOP-REAL 2:
  (p8`2<=p8`1 & -p8`1<=p8`2 or p8`2>=p8`1 & p8`2<=-p8`1) & p8<>0.TOP-REAL 2};
  set K1a={p8 where p8 is Point of TOP-REAL 2:
  (p8`1<=p8`2 & -p8`2<=p8`1 or p8`1>=p8`2 & p8`1<=-p8`2) & p8<>0.TOP-REAL 2 };
  for x1,x2 being set st x1 in dom Out_In_Sq & x2 in dom Out_In_Sq &
  Out_In_Sq.x1=Out_In_Sq.x2 holds x1=x2
  proof
    let x1,x2 be set;
    assume that
A6: x1 in dom Out_In_Sq & x2 in dom Out_In_Sq and
A7: Out_In_Sq.x1=Out_In_Sq.x2;
    NonZero TOP-REAL 2<>{} by Th19;
    then
A8: dom Out_In_Sq=NonZero TOP-REAL 2
by FUNCT_2:def 1;
    reconsider p1=x1,p2=x2 as Point of TOP-REAL 2 by A6,XBOOLE_0:def 5;
A9: the carrier of (TOP-REAL 2)|D=[#]((TOP-REAL 2)|D)
      .=D by PRE_TOPC:def 10;
    reconsider K01=K0a as non empty Subset of ((TOP-REAL 2)|D) by A3,Th27;
    ((1.REAL 2)`1<=(1.REAL 2)`2 & -(1.REAL 2)`2<=(1.REAL 2)`1 or
    (1.REAL 2)`1>=(1.REAL 2)`2 & (1.REAL 2)`1<=-(1.REAL 2)`2) &
    (1.REAL 2)<>0.TOP-REAL 2 by Th13,REVROT_1:19,XX;
    then
A10: 1.REAL 2 in K1a;
    K1a c= D
    proof
      let x be set;
      assume x in K1a;
      then consider p8 being Point of TOP-REAL 2 such that
A11:  x=p8 & (
      (p8`1<=p8`2 & -p8`2<=p8`1 or p8`1>=p8`2 & p8`1<=-p8`2) &
      p8<>0.TOP-REAL 2);
      not x in {0.TOP-REAL 2} by A11,TARSKI:def 1;
      hence thesis by A2,A11,XBOOLE_0:def 5;
    end;
    then reconsider K11=K1a as non empty Subset of ((TOP-REAL 2)|D) by A9,A10;
A12: D c= K01 \/ K11
    proof
      let x be set;
      assume
A13:  x in D;
      then
A14:  x in (the carrier of TOP-REAL 2) & not x in {0.TOP-REAL 2}
      by A5,XBOOLE_0:def 5;
      reconsider px=x as Point of TOP-REAL 2 by A13;
      (px`2<=px`1 & -px`1<=px`2 or px`2>=px`1 & px`2<=-px`1) &
      px<>0.TOP-REAL 2 or
      (px`1<=px`2 & -px`2<=px`1 or px`1>=px`2 & px`1<=-px`2)
      & px<>0.TOP-REAL 2 by A14,TARSKI:def 1,XREAL_1:27;
      then x in K01 or x in K11;
      hence thesis by XBOOLE_0:def 3;
    end;
A15: x1 in D & x2 in D by A1,A6,A8,SUBSET_1:def 5;
    now per cases by A12,A15,XBOOLE_0:def 3;
      case x1 in K01;
        then consider p7 being Point of TOP-REAL 2 such that
A16:    p1=p7 & (
        (p7`2<=p7`1 & -p7`1<=p7`2 or p7`2>=p7`1 & p7`2<=-p7`1) &
        p7<>0.TOP-REAL 2);
A17:    Out_In_Sq.p1=|[1/p1`1,p1`2/p1`1/p1`1]| by A16,Def1;
        now per cases by A12,A15,XBOOLE_0:def 3;
          case x2 in K0a;
            then consider p8 being Point of (TOP-REAL 2) such that
A18:        p2=p8 & ( (p8`2<=p8`1 & -p8`1<=p8`2 or p8`2>=p8`1 & p8`2<=-p8`1)
            & p8<>0.TOP-REAL 2);
A19:        |[1/p2`1,p2`2/p2`1/p2`1]|
            =|[1/p1`1,p1`2/p1`1/p1`1]| by A7,A17,A18,Def1;
            set qq=|[1/p2`1,p2`2/p2`1/p2`1]|;
            qq`1=1/p2`1 & qq`2=p2`2/p2`1/p2`1 by EUCLID:56;
            then
A20:        1/p1`1= 1/p2`1 & p1`2/p1`1/p1`1 = p2`2/p2`1/p2`1 by A19,EUCLID:56;
A21:        (1/p1`1)"=(p1`1)""
              .=p1`1;
A23:        now
              assume
A24:          p1`1=0;
              then p1`2=0 by A16;
              hence contradiction by A16,A24,EUCLID:57,58;
            end;
            then p1`2/p1`1= p2`2/p1`1 by A20,A21,XCMPLX_1:53;
            then
A25:        p1`2=p2`2 by A23,XCMPLX_1:53;
            p1=|[p1`1,p1`2]| by EUCLID:57;
            hence thesis by A20,A21,A25,EUCLID:57;
          end;
          case
A26:        x2 in K1a & not x2 in K0a;
            then consider p8 being Point of (TOP-REAL 2) such that
A27:        p2=p8 & ((p8`1<=p8`2 & -p8`2<=p8`1 or p8`1>=p8`2 & p8`1<=-p8`2)
            & p8<>0.TOP-REAL 2);
            not((p2`2<=p2`1 & -p2`1<=p2`2 or p2`2>=p2`1 & p2`2<=-p2`1)
            & p2 <> 0.TOP-REAL 2) by A26;
            then Out_In_Sq.p2=|[p2`1/p2`2/p2`2,1/p2`2]| by A27,Def1;
            then
A28:        1/p1`1=p2`1/p2`2/p2`2 & p1`2/p1`1/p1`1=1/p2`2 by A7,A17,SPPOL_2:1;
A29:        now
              assume
A30:          p2`2=0;
              then p2`1=0 by A27;
              hence contradiction by A27,A30,EUCLID:57,58;
            end;
A31:        now
              assume
A32:          p1`1=0;
              then p1`2=0 by A16;
              hence contradiction by A16,A32,EUCLID:57,58;
            end;
A33:        p2`1/p2`2=1/p1`1*p2`2 by A28,A29,XCMPLX_1:88
              .= p2`2/p1`1;
A34:        p1`2/p1`1=1/p2`2*p1`1 by A28,A31,XCMPLX_1:88
              .= p1`1/p2`2;
            then
A35:        (p2`1/p2`2)* (p1`2/p1`1)=1 by A29,A31,A33,XCMPLX_1:113;
A36:        (p2`1/p2`2)* (p1`2/p1`1)*p1`1=1 *p1`1 by A29,A31,A33,A34,
XCMPLX_1:113;
            then (p2`1/p2`2)* ((p1`2/p1`1)*p1`1)=p1`1;
            then
A37:        (p2`1/p2`2)*p1`2=p1`1 by A31,XCMPLX_1:88;
A38:        p2`1<>0 & p1`2<>0 by A31,A36;
            then
A39:        (p2`1/p2`2)=p1`1/p1`2 by A37,XCMPLX_1:90;
            consider p9 being Point of (TOP-REAL 2) such that
A40:        p2=p9 & ( (p9`1<=p9`2 & -p9`2<=p9`1 or p9`1>=p9`2 & p9`1<=-p9`2)
            & p9<>0.TOP-REAL 2) by A26;
A41:        now per cases by A40;
              case
A42:            p2`1<=p2`2 & -p2`2<=p2`1;
                then p2`2>=0;
                then p2`1/p2`2<=p2`2/p2`2 by A42,XREAL_1:74;
                hence p2`1/p2`2<=1 by A29,XCMPLX_1:60;
              end;
              case
A43:            p2`1>=p2`2 & p2`1<=-p2`2;
                p2`2 <= 0 by A43;
                then p2`1/p2`2<=p2`2/p2`2 by A43,XREAL_1:75;
                hence p2`1/p2`2<=1 by A29,XCMPLX_1:60;
              end;
            end;
A45:        now per cases by A16;
              case
A46:            p1`2<=p1`1 & -p1`1<=p1`2;
                then p1`1>=0;
                then p1`2/p1`1<=p1`1/p1`1 by A46,XREAL_1:74;
                hence p1`2/p1`1<=1 by A31,XCMPLX_1:60;
              end;
              case
A47:            p1`2>=p1`1 & p1`2<=-p1`1;
                p1`1 <= 0 by A47;
                then p1`2/p1`1<=p1`1/p1`1 by A47,XREAL_1:75;
                hence p1`2/p1`1<=1 by A31,XCMPLX_1:60;
              end;
            end;
A49:        now per cases by A40;
              case
A50:            p2`1<=p2`2 & -p2`2<=p2`1;
                then p2`2>=0;
                then (-p2`2)/p2`2<=p2`1/p2`2 by A50,XREAL_1:74;
                hence -1<=p2`1/p2`2 by A29,XCMPLX_1:198;
              end;
              case
A51:            p2`1>=p2`2 & p2`1<=-p2`2;
                then
A52:            -p2`1>=--p2`2 by XREAL_1:26;
                p2`2 <= 0 by A51;
                then (-p2`1)/(-p2`2)>=p2`2/(-p2`2) by A52,XREAL_1:74;
                then (-p2`1)/(-p2`2)>= -1 by A29,XCMPLX_1:199;
                hence -1<=p2`1/p2`2 by XCMPLX_1:192;
              end;
            end;
A54:        now per cases by A16;
              case
A55:            p1`2<=p1`1 & -p1`1<=p1`2;
                then p1`1>=0;
                then (-p1`1)/p1`1<=p1`2/p1`1 by A55,XREAL_1:74;
                hence -1<=p1`2/p1`1 by A31,XCMPLX_1:198;
              end;
              case
A56:            p1`2>=p1`1 & p1`2<=-p1`1;
                then
A57:            -p1`2>=--p1`1 by XREAL_1:26;
                p1`1 <= 0 by A56;
                then (-p1`2)/(-p1`1)>=p1`1/(-p1`1) by A57,XREAL_1:74;
                then (-p1`2)/(-p1`1)>= -1 by A31,XCMPLX_1:199;
                hence -1<=p1`2/p1`1 by XCMPLX_1:192;
              end;
            end;
            now per cases;
              case
A59:            0<=p2`1/p2`2;

then
B60: p1`2>0 & p1`1>=0 or p1`2<0 & p1`1<=0 by A31,A37;
                now
                  assume p1`2/p1`1<>1;
                  then p1`2/p1`1<1 by A45,XXREAL_0:1;
                  hence contradiction by A35,A41,A59,B60,XREAL_1:164;
                end;
                then p1`2=(1)*p1`1 by A31,XCMPLX_1:88;
                then (p2`1/p2`2)*p2`2=(1)*p2`2 by A31,A39,XCMPLX_1:60
                  .=p2`2;
                then p2`1=p2`2 by A29,XCMPLX_1:88;
                hence contradiction by A26,A40;
              end;
              case
A61:            0>p2`1/p2`2;
                then
B62:              p1`2<0 & p1`1>0 or p1`2>0 & p1`1<0 by A39,XREAL_1:145;
                now
                  assume p1`2/p1`1<>-1;
                  then -1<p1`2/p1`1 by A54,XXREAL_0:1;
                  hence contradiction by A35,A49,A61,B62,XREAL_1:168;
                end;
                then p1`2=(-1)*p1`1 by A31,XCMPLX_1:88
                  .= -p1`1;
                then -p1`2 =p1`1;
                then p2`1/p2`2=-1 by A38,A39,XCMPLX_1:198;
                then p2`1=(-1)*p2`2 by A29,XCMPLX_1:88;
                then -p2`1=p2`2;
                hence contradiction by A26,A40;
              end;
            end;
            hence contradiction;
          end;
        end;
        hence thesis;
      end;
      case x1 in K1a;
        then consider p7 being Point of TOP-REAL 2 such that
A63:    p1=p7 & ( (p7`1<=p7`2 & -p7`2<=p7`1 or p7`1>=p7`2 & p7`1<=-p7`2)
        & p7<>0.TOP-REAL 2);
A64:    Out_In_Sq.p1=|[p1`1/p1`2/p1`2,1/p1`2]| by A63,Th24;
        now per cases by A12,A15,XBOOLE_0:def 3;
          case x2 in K1a;
            then consider p8 being Point of (TOP-REAL 2) such that
A65:        p2=p8 & ( (p8`1<=p8`2 & -p8`2<=p8`1 or p8`1>=p8`2 & p8`1<=-p8`2)
            & p8<>0.TOP-REAL 2);
A66:        |[p2`1/p2`2/p2`2,1/p2`2]|
            =|[p1`1/p1`2/p1`2,1/p1`2]| by A7,A64,A65,Th24;
            set qq=|[p2`1/p2`2/p2`2,1/p2`2]|;
            qq`2=1/p2`2 & qq`1=p2`1/p2`2/p2`2 by EUCLID:56;
            then
A67:        1/p1`2= 1/p2`2 & p1`1/p1`2/p1`2 = p2`1/p2`2/p2`2 by A66,EUCLID:56;
A68:        (1/p1`2)"=(p1`2)""
              .=p1`2;
A70:        now
              assume
A71:          p1`2=0;
              then p1`1=0 by A63;
              hence contradiction by A63,A71,EUCLID:57,58;
            end;
            then p1`1/p1`2= p2`1/p1`2 by A67,A68,XCMPLX_1:53;
            then
A72:        p1`1=p2`1 by A70,XCMPLX_1:53;
            p1=|[p1`1,p1`2]| by EUCLID:57;
            hence thesis by A67,A68,A72,EUCLID:57;
          end;
          case
A73:        x2 in K0a & not x2 in K1a;
