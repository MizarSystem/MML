    (p8`2<=p8`1 & -p8`1<=p8`2 or p8`2>=p8`1 & p8`2<=-p8`1) & p8<>0.TOP-REAL 2)
    by A1;
    not x in {0.TOP-REAL 2} by A4,TARSKI:def 1;
