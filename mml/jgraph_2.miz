    end;
    set p9=|[p`1/p`2/p`2,1/p`2]|;
A51: now
      assume p9=0.TOP-REAL 2;
      then p9`2=0 & p9`1=0 by EUCLID:56,58;
      then 0 *p`2=1/p`2*p`2 by EUCLID:56;
