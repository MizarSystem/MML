A570:   n1 in dom v & v1.1 = v.n1 by A569,PARTFUN1:26;
A571:   n1 in Seg len D by A544,A570,FINSEQ_1:def 3;
then A572:   v1.1 = vol(divset(D,n1)) by A544,A570;
           n1 in dom D by A571,FINSEQ_1:def 3;
        hence thesis by A562,A572;
       end;
          v1.k = min rng upper_volume(chi(A,A),D)
       proof
A573:    k=1
        proof
         assume A574:k <> 1;
A575:    len v1 = len v by A545,RFINSEQ:16;
A576:    k in Seg len v1 by A549,FINSEQ_1:def 3;
            k in Seg len v by A549,A575,FINSEQ_1:def 3;
