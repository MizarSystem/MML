A601:   n1 in dom v and
A602:   v1.1 = v.n1 by A600,PARTFUN1:26;
        n1 in Seg len D by A576,A601,FINSEQ_1:def 3;
        then
A603:   n1 in dom D by FINSEQ_1:def 3;
        v1.1 = vol(divset(D,n1)) by A576,A601,A602;
