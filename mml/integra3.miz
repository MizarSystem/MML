    .=Sum(mid(UVg(D1),1,j1)^mid(UVg(D1),j1+1,j)) by A19,JORDAN3:25
    .=Sum(mid(UVg(D1),1,j)) by A19,A135,A137,INTEGRA2:4
    .=Sum(UVg(D1)|j) by A135,JORDAN3:25;
