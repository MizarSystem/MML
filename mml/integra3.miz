          D.(len D) <= upper_bound divset(D1,j) by A549,INTEGRA2:1;
          hence contradiction by A554,INTEGRA1:def 2;
        end;
        indx(D2,D1,len D1) in dom D2 by A13,A546,INTEGRA1:def 21;
        then indx(D2,D1,len D1)=len D2 by A15,A547,SEQ_4:155;
        then PLf(D2,len D2)-lower_sum(f,D1)<=(len D)* (upper_bound(rng f)-
