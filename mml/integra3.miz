,A279,A295,A281,A289,INTEGRA2:1;
                    then
A297:               D2.(indx(D2,D1,n1)+2) in rng D /\ divset(D1,j) by A294,
XBOOLE_0:def 4;
A298:               indx(D2,D1,n1)+1 < indx(D2,D1,j) by A284,A286,XXREAL_0:2;
