A580: v=v1*H by A577,CLASSES1:85;
      consider k such that
A581: k in dom D and
A582: vol(divset(D,k)) > 0 by A3,Th1;
A583: dom D = Seg len v by A576,FINSEQ_1:def 3;
      then H.k in dom v1 by A576,A578,A580,A581,FUNCT_1:21;
      then reconsider Hk = H.k as Nat;
      v.k > 0 by A576,A578,A581,A582,A583;
      then P[Hk] by A576,A578,A580,A581,A583,FUNCT_1:21,22;
      hence thesis;
    end;
    consider k be Nat such that
A584: P[k] & for n be Nat st P[n] holds k<=n from NAT_1:sch 5(A579);
A585: 2*p >0 by XREAL_1:131;
    then
A586: 2*p*(H-h+1) > 0 by A568,XREAL_1:131;
    min(v1.k,e/(2*p*(H-h+1))) > 0
    proof
      per cases by XXREAL_0:15;
      suppose
        min(v1.k,e/(2*p*(H-h+1))) = v1.k;
        hence thesis by A584;
      end;
      suppose
        min(v1.k,e/(2*p*(H-h+1))) = e/(2*p*(H-h+1));
        hence thesis by A566,A586,XREAL_1:141;
      end;
    end;
    then consider n such that
A587: for m st n<=m holds 0 < (delta(T)).m & (delta(T)).m < min(v1.k,
    e/(2*p*(H-h+1))) by A557;
    take n;
A588: y=lower_sum(f,D) by A573,A574,INTEGRA1:def 12;
    for m st n<=m holds abs((lower_sum(f,T)).m-lower_integral(f))<e
    proof
A589: v1.1 > 0
      proof
A590:   for n1 be Element of NAT st n1 in dom D holds vol(divset(D,n1))
        >0
        proof
          let n1 be Element of NAT;
