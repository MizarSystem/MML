A599:   k <= len v1 by A584,FINSEQ_3:27;
        1 <= k by A584,FINSEQ_3:27;
        then 1 <= len v1 by A599,XXREAL_0:2;
        then 1 in dom v1 by FINSEQ_3:27;
        then
A600:   v1.1 in rng v1 by FUNCT_1:def 5;
