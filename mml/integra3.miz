            hence thesis by A206,INTEGRA1:def 5;
            end;
            suppose n1<>1;
            hence thesis by A206,INTEGRA1:def 5;
          end;
          end;
          then D.i < D.n2 by A418,A419,AXIOMS:22;
          then i < n2 by A205,A417,GOBOARD2:18;
then A420:     i+1 <= n2 by NAT_1:38;
A421:     D.n2 < D1.(n1+1) by A227,A412,A413,A417,GOBOARD1:def 1;
A422:     j-1 in dom D1 & j-1 in NAT by A202,A209,A223,INTEGRA1:9;
             n1+1 +1 <= j by A299,NAT_1:38;
          then n1+1 <= j-1 by REAL_1:84;
then A423:     D1.(n1+1) <= D1.(j-1) by A226,A422,GOBOARD2:18;
A424:     inf divset(D1,j) <= D.(i+1) by A202,INTEGRA2:1;
             inf divset(D1,j) = D1.(j-1) by A202,A209,A223,INTEGRA1:def 5;
          then D1.(n1+1) <= D.(i+1) by A423,A424,AXIOMS:22;
          then D.n2 < D.(i+1) by A421,AXIOMS:22;
          hence contradiction by A201,A417,A420,GOBOARD2:18;
         end;
         end;
         hence contradiction;
        end;
A425: Sum upper_volume(g,MD2)=Sum mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j))
        proof
            upper_volume(g,MD2)=mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j))
         proof
A426:      len upper_volume(g,MD2)=indx(D2,D1,j)-(indx(D2,D1,n1)+1)+1
             by A324,A411,INTEGRA1:def 7;
A427:      indx(D2,D1,j) <= len UVf(D2) by A231,INTEGRA1:def 7;
A428:      indx(D2,D1,n1)+1 <= indx(D2,D1,j) by A229,A408,AXIOMS:22;
then len mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j))
          = indx(D2,D1,j)-'(indx(D2,D1,n1)+1)+1
          by A231,A407,A410,A427,JORDAN3:27;
then A429:      len upper_volume(g,MD2)
          =len mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j))
          by A426,A428,SCMFSA_7:3;
             for k st 1<=k & k<=len upper_volume(g,MD2) holds
          upper_volume(g,MD2).k=mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j)).k
          proof
           let k; assume A430:1 <= k & k <= len upper_volume(g,MD2);
           then k in Seg len upper_volume(g,MD2) by FINSEQ_1:3;
then A431:       k in Seg len MD2 by INTEGRA1:def 7;
then A432:       upper_volume(g,MD2).k
           =(sup rng(g|divset(MD2,k)))*vol(divset(MD2,k)) by INTEGRA1:def 7;
A433:      divset(MD2,k)=[.inf divset(MD2,k),sup divset(MD2,k).] by INTEGRA1:5;
A434:       mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j)).k
           =UVf(D2).(k+(indx(D2,D1,n1)+1)-1)
           by A407,A426,A427,A428,A430,JORDAN3:31;
              k <= indx(D2,D1,j)-((indx(D2,D1,n1)+1)-1) by A324,A411,A430,
INTEGRA1:def 7;
           then k+((indx(D2,D1,n1)+1)-1) <= indx(D2,D1,j) by REAL_1:84;
then A435:       k+(indx(D2,D1,n1)+1)-1 <= len UVf(D2) by A427,AXIOMS:22;
              1<=indx(D2,D1,n1)+1 by NAT_1:37;
           then 1+1<=k+(indx(D2,D1,n1)+1) by A430,REAL_1:55;
then A436:       1 <= k+(indx(D2,D1,n1)+1)-1 by REAL_1:84;
           consider k2 being Nat such that
A437:       indx(D2,D1,n1)+1=1+k2;
A438:       k2=(indx(D2,D1,n1)+1)-1 by A437;
            k+k2 in Seg len UVf(D2) by A435,A436,A437,FINSEQ_1:3;
then A439:       k+k2 in Seg len D2 by INTEGRA1:def 7;
then A440:       mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j)).k
           =(sup rng(f|divset(D2,k+k2)))*vol(divset(D2,k+k2))
           by A434,A437,INTEGRA1:def 7;
              inf divset(MD2,k)=inf divset(D2,k+k2)
           & sup divset(MD2,k)=sup divset(D2,k+k2)
           proof
             k+k2>=1+1 by A208,A430,A438,REAL_1:55;
then A441:        k+k2>1 by NAT_1:38;
A442:        k in dom MD2 by A431,FINSEQ_1:def 3;
A443:        k+k2 in dom D2 by A439,FINSEQ_1:def 3;
               now per cases;
             suppose A444:k=1;
then A445:        inf divset(MD2,k)=inf B & sup divset(MD2,k)=MD2.k
             by A442,INTEGRA1:def 5;
then A446:         sup divset(MD2,k)=D2.(1+indx(D2,D1,n1+1)-1)
             by A229,A231,A302,A407,A411,A426,A430,A444,JORDAN3:31
             .=D1.(n1+1) by A20,A226,INTEGRA1:def 21;
                inf divset(D2,k+k2)=D2.(1+k2-1) & sup divset(D2,k+k2)=D2.(1+k2)
             by A441,A443,A444,INTEGRA1:def 5;
             hence inf divset(MD2,k)=inf divset(D2,k+k2)
             & sup divset(MD2,k)=sup divset(D2,k+k2)
             by A20,A207,A226,A302,A411,A437,A445,A446,INTEGRA1:def 21,XCMPLX_1
:26;
             end;
             suppose A447:k<>1;
then A448:        k-1 in dom MD2 & k-1 in NAT by A442,INTEGRA1:9;
then A449:        1 <= k-1 by FINSEQ_3:27;
                k-1 <= k by Lm1;
then A450:        k-1 <= indx(D2,D1,j)-(indx(D2,D1,n1)+1)+1 by A426,A430,AXIOMS
:22;
                inf divset(MD2,k)=MD2.(k-1) & sup divset(MD2,k)=MD2.k
             by A442,A447,INTEGRA1:def 5;
then         inf divset(MD2,k)=D2.(k-1+(indx(D2,D1,n1)+1)-1)
             & sup divset(MD2,k)=D2.(k+(indx(D2,D1,n1)+1)-1)
             by A229,A231,A302,A407,A411,A426,A430,A448,A449,A450,JORDAN3:31;
             hence inf divset(MD2,k)=inf divset(D2,k+k2)
             & sup divset(MD2,k)=sup divset(D2,k+k2) by A437,A441,A443,INTEGRA1
:def 5;
            end;
            end;
            hence thesis;
           end;
then A451:       divset(MD2,k)=divset(D2,k+k2) by A433,INTEGRA1:5;
              k in dom MD2 by A431,FINSEQ_1:def 3;
           then divset(D2,k+k2) c= B by A451,INTEGRA1:10;
           hence thesis by A432,A440,A451,FUNCT_1:82;
          end;
          hence thesis by A429,FINSEQ_1:18;
         end;
         hence thesis;
        end;
           Sum upper_volume(g,MD1)=Sum mid(UVf(D1),n1+1,j)
        proof
            upper_volume(g,MD1)=mid(UVf(D1),n1+1,j)
         proof
A452:      j-'(n1+1)+1=j-(n1+1)+1 by A221,SCMFSA_7:3;
A453:      len upper_volume(g,MD1)=len MD1 by INTEGRA1:def 7
          .=j-(n1+1)+1 by A221,A224,A225,A302,A452,JORDAN3:27;
A454:      n1+1 <= len UVf(D1) by A225,INTEGRA1:def 7;
A455:      j <= len UVf(D1) by A224,INTEGRA1:def 7;
then A456:      len upper_volume(g,MD1)=len mid(UVf(D1),n1+1,j)
             by A221,A224,A225,A452,A453,A454,JORDAN3:27;
             for k st 1<=k & k<=len upper_volume(g,MD1) holds
          upper_volume(g,MD1).k=mid(UVf(D1),n1+1,j).k
          proof
           let k; assume A457:1 <= k & k <= len upper_volume(g,MD1);
           then k in Seg len upper_volume(g,MD1) by FINSEQ_1:3;
then A458:      k in Seg len MD1 by INTEGRA1:def 7;
then A459:      upper_volume(g,MD1).k
           =(sup rng(g|divset(MD1,k)))*vol(divset(MD1,k)) by INTEGRA1:def 7;
              k <= j-((n1+1)-1) by A453,A457;
then A460:      k+((n1+1)-1) <= j by REAL_1:84;
           consider k2 being Nat such that
A461:       n1+1=1+k2;
A462:      k2=(n1+1)-1 by A461;
A463:      1 <= k+k2 by A457,NAT_1:37;
              k+k2 <= len D1 by A224,A460,A461,AXIOMS:22;
then A464:      k+k2 in Seg len D1 by A463,FINSEQ_1:3;
A465:      mid(UVf(D1),n1+1,j).k
           =UVf(D1).(k+(n1+1)-1) by A221,A225,A453,A455,A457,JORDAN3:31
           .=(sup rng(f|divset(D1,k+k2)))*vol(divset(D1,k+k2))
           by A461,A464,INTEGRA1:def 7;
A466:      divset(D1,k+k2)=divset(MD1,k)
           proof
             1+1 <= k+k2 by A224,A457,A462,REAL_1:55;
then A467:       1 < k+k2 by NAT_1:38;
A468:       divset(MD1,k)=[.inf divset(MD1,k),sup divset(MD1,k).]
            by INTEGRA1:5;
A469:       k in dom MD1 by A458,FINSEQ_1:def 3;
              A470: k+k2 in dom D1 by A464,FINSEQ_1:def 3;
               inf divset(D1,k+k2)=inf divset(MD1,k)
            & sup divset(D1,k+k2)=sup divset(MD1,k)
            proof
             per cases;
              suppose A471:k=1;
              then inf divset(MD1,k)=inf B & sup divset(MD1,k)=MD1.k
              by A469,INTEGRA1:def 5;
              then inf divset(MD1,k)=D1.n1 & sup divset(MD1,k)=D1.(k+(n1+1)-1)
              by A221,A224,A225,A302,A453,A457,JORDAN3:31;
              hence thesis by A462,A467,A470,A471,INTEGRA1:def 5;
              end;
              suppose A472:k<>1;
then A473:         k-1 in dom MD1 & k-1 in NAT by A469,INTEGRA1:9;
then A474:         1 <= k-1 by FINSEQ_3:27;
                 k-1 <= k by Lm1;
then A475:         k-1 <= j-(n1+1)+1 by A453,A457,AXIOMS:22;
                 inf divset(MD1,k)=MD1.(k-1) & sup divset(MD1,k)=MD1.k
              by A469,A472,INTEGRA1:def 5;
              then inf divset(MD1,k)=D1.(k-1+(n1+1)-1)
              & sup divset(MD1,k)=D1.(k+(n1+1)-1)
              by A221,A224,A225,A302,A453,A457,A473,A474,A475,JORDAN3:31;
              hence thesis by A461,A467,A470,INTEGRA1:def 5;
            end;
            end;
            hence thesis by A468,INTEGRA1:5;
           end;
              k in dom MD1 by A458,FINSEQ_1:def 3;
           then divset(D1,k+k2) c= B by A466,INTEGRA1:10;
           hence thesis by A459,A465,A466,FUNCT_1:82;
          end;
          hence thesis by A456,FINSEQ_1:18;
         end;
         hence thesis;
        end;
        hence thesis by A388,A405,A425,AXIOMS:22;
       end;
       end;

A476:  PUf(D2,indx(D2,D1,j))=
        PUf(D2,indx(D2,D1,n1))+Sum mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j))
        proof
A477:    indx(D2,D1,n1)<indx(D2,D1,j) by A229,A232,AXIOMS:22;
            indx(D2,D1,j) in Seg len D2 by A228,FINSEQ_1:def 3;
then A478:    indx(D2,D1,j) in Seg len UVf(D2) by INTEGRA1:def 7;
A479:    indx(D2,D1,j) <= len UVf(D2) by A231,INTEGRA1:def 7;
            indx(D2,D1,n1) in Seg len D2 by A207,FINSEQ_1:def 3;
         then indx(D2,D1,n1) in Seg len UVf(D2) by INTEGRA1:def 7;
         then PUf(D2,indx(D2,D1,n1))=Sum(UVf(D2)|indx(D2,D1,n1)) by INTEGRA1:
def 22
         .=Sum mid(UVf(D2),1,indx(D2,D1,n1)) by A208,JORDAN3:25;
         then PUf(D2,indx(D2,D1,n1))+Sum mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,
D1,j))
         =Sum(mid(UVf(D2),1,indx(D2,D1,n1))^
            mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j))) by RVSUM_1:105
         .=Sum mid(UVf(D2),1,indx(D2,D1,j)) by A208,A477,A479,INTEGRA2:4
         .=Sum(UVf(D2)|indx(D2,D1,j)) by A231,JORDAN3:25;
         hence thesis by A478,INTEGRA1:def 22;
        end;
A480:    j <= len UVf(D1) by A224,INTEGRA1:def 7;
then A481:    j in Seg len UVf(D1) by A224,FINSEQ_1:3;
            n1 in Seg len D1 by A206,FINSEQ_1:def 3;
         then n1 in Seg len UVf(D1) by INTEGRA1:def 7;
         then PUf(D1,n1)=Sum(UVf(D1)|n1) by INTEGRA1:def 22
         .=Sum mid(UVf(D1),1,n1) by A224,JORDAN3:25;
         then PUf(D1,n1)+Sum mid(UVf(D1),n1+1,j)
          =Sum(mid(UVf(D1),1,n1)^mid(UVf(D1),n1+1,j)) by RVSUM_1:105
         .=Sum mid(UVf(D1),1,j) by A209,A224,A480,INTEGRA2:4
         .=Sum(UVf(D1)|j) by A224,JORDAN3:25;
then A482:    PUf(D1,j)=PUf(D1,n1)+Sum mid(UVf(D1),n1+1,j) by A481,INTEGRA1:def
22;
A483:    (PUf(D1,n1)-PUf(D2,indx(D2,D1,n1)))+
   (Sum mid(UVf(D1),n1+1,j)-Sum mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j)))
        <=i*(sup rng f-inf rng f)*delta(D1)+(sup rng f-inf rng f)*delta(D1)
        by A206,A234,REAL_1:55;
           (PUf(D1,n1)-PUf(D2,indx(D2,D1,n1)))+
   (Sum mid(UVf(D1),n1+1,j)-Sum mid(UVf(D2),indx(D2,D1,n1)+1,indx(D2,D1,j)))
         =PUf(D1,j)-PUf(D2,indx(D2,D1,j)) by A476,A482;
        hence thesis by A202,A483;
       end;
       hence thesis;
      end;
A484: for k being non empty Nat holds P[k] from BINARITH:sch 1(A43,A198);
         i in Seg len D by A22,FINSEQ_1:def 3;
      then reconsider i as non empty Nat by BINARITH:5;
         P[i] by A484;
      hence thesis by A22;
     end;
        len D in dom D by SCMFSA_7:12;
     then consider j such that
A485:  j in dom D1 & D.(len D) in divset(D1,j) &
       PUf(D1,j)-PUf(D2,indx(D2,D1,j))
     <= (len D)*(sup(rng f)-inf(rng f))*delta(D1) by A21;
A486:len D1 in dom D1 by SCMFSA_7:12;
A487:j = len D1
     proof
      assume A488:j<>len D1;
      A489: D.(len D) <= sup divset(D1,j) by A485,INTEGRA2:1;
         j in Seg len D1 by A485,FINSEQ_1:def 3;
      then j <= len D1 by FINSEQ_1:3;
      then j < len D1 by A488,REAL_1:def 5;
      then D1.j < D1.(len D1) by A485,A486,GOBOARD1:def 1;
then A490: D1.j < sup A by INTEGRA1:def 2;
         sup divset(D1,j) < sup A
      proof
       per cases;
       suppose j=1;
       hence thesis by A485,A490,INTEGRA1:def 5;
       end;
       suppose j<>1;
       hence thesis by A485,A490,INTEGRA1:def 5;
      end;
      end;
      hence contradiction by A489,INTEGRA1:def 2;
     end;
A491: len D2 in dom D2 by SCMFSA_7:12;
   indx(D2,D1,len D1)=len D2
     proof
A492: indx(D2,D1,len D1) in dom D2 &
      D1.(len D1) = D2.indx(D2,D1,len D1) by A20,A486,INTEGRA1:def 21;
      then sup A = D2.indx(D2,D1,len D1) by INTEGRA1:def 2;
      then D2.(len D2) = D2.indx(D2,D1,len D1) by INTEGRA1:def 2;
      hence thesis by A491,A492,GOBOARD2:19;
     end;
     then upper_sum(f,D1)-PUf(D2,len D2)<=(len D)*(sup(rng f)-inf(rng f))*
delta(D1)
     by A485,A487,INTEGRA1:44;
     hence thesis by INTEGRA1:44;
    end;
    hence thesis by A20;
   end;
   hence thesis;
  end;

A493:for e be real number st e>0 ex n st for m st n<=m holds
   abs((upper_sum(f,T)).m-upper_integral(f))<e
   proof
     let e be real number;
     assume A494:e>0;
then A495: e/2>0 by REAL_2:127;
     reconsider e as Real by XREAL_0:def 1;
A496: upper_integral(f) = inf rng upper_sum_set(f) by INTEGRA1:def 15;
A497: rng upper_sum_set(f) is bounded_below by A1,INTEGRA2:35;
        dom upper_sum_set(f) is non empty by INTEGRA1:def 11;
     then rng upper_sum_set(f) is non empty by RELAT_1:65;
     then consider y be real number such that
A498: y in rng upper_sum_set(f) & upper_integral(f)+e/2>y
      by A495,A496,A497,SEQ_4:def 5;
        ex D being Element of divs A st D in dom upper_sum_set(f) &
     y=(upper_sum_set(f)).D & D.1 > inf A
     proof
      consider D3 being Element of divs A such that
A499:   D3 in dom upper_sum_set(f) & y=(upper_sum_set(f)).D3 by A498,PARTFUN1:
26;
         len D3 <> 0 by FINSEQ_1:25;
then A500: len D3 in Seg len D3 by FINSEQ_1:5;
      then 1 <= len D3 by FINSEQ_1:3;
then A501: 1 in Seg len D3 by FINSEQ_1:3;
then A502: 1 in dom D3 by FINSEQ_1:def 3;
         now per cases;
       suppose A503:D3.1 <> inf A;
          D3.1 in A by A502,INTEGRA1:8;
       then inf A <= D3.1 by INTEGRA2:1;
       then D3.1 > inf A by A503,REAL_1:def 5;
       hence ex D being Element of divs A st D in dom upper_sum_set(f) &
       y=(upper_sum_set(f)).D & D.1 > inf A by A499;
       end;
       suppose A504:D3.1 = inf A;
          ex D being Element of divs A st D in dom upper_sum_set(f) &
       y=(upper_sum_set(f)).D & D.1 > inf A
       proof
A505:   D3.(len D3) = sup A by INTEGRA1:def 2;
A506:   len D3 in dom D3 by A500,FINSEQ_1:def 3;
           vol(A) >= 0 by INTEGRA1:11;
        then sup A - inf A > 0 by A3,INTEGRA1:def 6;
        then sup A > inf A by REAL_2:106;
then A507:   len D3 > 1 by A502,A504,A505,A506,GOBOARD2:18;
        then reconsider D=D3/^1 as increasing FinSequence of REAL
         by INTEGRA1:36;
A508:   len D = len D3 - 1 & for m be Nat st m in dom D holds D.m = D3.(m+1)
        by A507,RFINSEQ:def 2;
then A509:   len D <> 0 by A507,SQUARE_1:11;
        then reconsider D as non empty increasing FinSequence of REAL
        by FINSEQ_1:25;
           rng D c= rng D3 & rng D3 c= A by FINSEQ_5:36,INTEGRA1:def 2;
then A510:   rng D c= A by XBOOLE_1:1;
A511:   len D+1=len D3 by A508,XCMPLX_1:27;
A512:   len D in Seg len D by A509,FINSEQ_1:5;
           len D in dom D by SCMFSA_7:12;
        then D.(len D)=sup A by A505,A507,A511,RFINSEQ:def 2;
        then D is DivisionPoint of A by A510,INTEGRA1:def 2;
        then reconsider D as Element of divs A by INTEGRA1:def 4;
           D in divs A;
then A513:   D in dom upper_sum_set(f) by INTEGRA1:def 11;
A514:   y=(upper_sum_set(f)).D
        proof
A515:    y=upper_sum(f,D3) by A499,INTEGRA1:def 11
         .=Sum(upper_volume(f,D3)) by INTEGRA1:def 9
         .=Sum((upper_volume(f,D3)|1)^(upper_volume(f,D3)/^1)) by RFINSEQ:21;
A516:    1 <= len upper_volume(f,D3) by A507,INTEGRA1:def 7;
then A517:    len (upper_volume(f,D3)|1)=1 by FINSEQ_1:80;
A518:    1 in dom upper_volume(f,D3) by A516,FINSEQ_3:27;
            1 in Seg 1 by FINSEQ_1:3;
         then (upper_volume(f,D3)|1).1 = upper_volume(f,D3).1 by A518,RFINSEQ:
19;
         then A519:    upper_volume(f,D3)|1 = <*upper_volume(f,D3).1*> by A517,
FINSEQ_1:57;
A520:    vol(divset(D3,1))=sup divset(D3,1)-inf divset(D3,1) by INTEGRA1:def 6
          .=sup divset(D3,1) - inf A by A502,INTEGRA1:def 5
          .=D3.1 - inf A by A502,INTEGRA1:def 5
          .=0 by A504;
         A521: upper_volume(f,D3).1=(sup rng(f|divset(D3,1)))*vol(divset(D3,1))
           by A501,INTEGRA1:def 7;
            upper_volume(f,D3)/^1 = upper_volume(f,D)
         proof
A522:      2-'1=2-1 by SCMFSA_7:3 .= 1;
A523:      len D3 >= 1+1 by A507,NAT_1:38;
          then len upper_volume(f,D3) >= 2 by INTEGRA1:def 7;
then A524:      mid(upper_volume(f,D3),2,len upper_volume(f,D3))
