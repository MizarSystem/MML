  then
A42: {r} + P2 c= P1 by TARSKI:def 3;
  now
    let z be set;
    assume
    z in R1;
    then consider n be set such that
A43: n in NAT and
A44: (inferior_realsequence seq1).n = z by FUNCT_2:17;
