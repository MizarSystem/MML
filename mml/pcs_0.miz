 definitions SUBSET_1, RELAT_1, PARTFUN1, RELAT_2, EQREL_1, CARD_3, ORDERS_2,
      PARTIT_2, STRUCT_0, TSEP_1, MSUALG_4, PRALG_1, YELLOW_1, WAYBEL_3,
