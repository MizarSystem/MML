      .= R14.(z-z0)+(R19.(z-z0)+R18.(z-z0))+(L.(z-z0)+R13.(z-z0))
      .= L.(z-z0)+R13.(z-z0)+(R14.(z-z0)+R20.(z-z0)) by A12,RFUNCT_1:72
      .= L.(z-z0)+(R13.(z-z0)+R14.(z-z0)+R20.(z-z0))
      .= L.(z-z0)+(R15.(z-z0)+R20.(z-z0)) by A12,RFUNCT_1:72
      .= L.(z-z0)+R.(z-z0) by A12,RFUNCT_1:72;
