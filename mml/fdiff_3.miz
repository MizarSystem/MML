      end;
     then A34: f*c is convergent by SEQ_2:def 6;
     then A35: lim(f*c) = f.x0 by A32,SEQ_2:def 7;
     A36: h(#)(h"(#)(f*(h + c) - f*c)) is convergent by A22,A24,SEQ_2:28;
