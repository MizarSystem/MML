    lim(h"(#) ((f^)/*(h+c) - (f^)/*c)) = lim(-(h"(#) ((f/*(h+c))- (f/*c))
    ))/(f.x0)^2 by A38,A47,A48,A31,A50,SEQ_2:38
      .=(- Rdiff(f,x0)) /(f.x0)^2 by A30,A29,SEQ_2:24
      .=- Rdiff(f,x0) /(f.x0)^2 by XCMPLX_1:188;
