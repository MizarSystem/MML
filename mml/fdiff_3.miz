  /*c)) = - Rdiff(f,x0)/(f.x0)^2
  proof
    let h,c;
    assume that
A22: rng c = {x0} and
A23: rng (h+c) c= dom (f^) and
A24: for n holds h.n > 0;
A25: lim h = 0 by FDIFF_1:def 1;
A26: for m holds c.m = x0
    proof
