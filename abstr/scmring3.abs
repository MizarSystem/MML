environ

 vocabularies NUMBERS, SCMFSA7B, FUNCSDOM, SUBSET_1, AMI_3, AMI_1, FSM_1,
      ORDINAL1, INT_3, FINSET_1, STRUCT_0, AMI_2, TARSKI, ZFMISC_1, RELAT_1,
