  P1 = {pa where pa is Point of TOP-REAL 2:
                 s1<pa`1 & pa`1<s2 & t1 <pa`2 & pa`2 < t2} holds
P1 c= [#] ((TOP-REAL 2)|P`);

theorem :: JORDAN1:44
     for s1,s2,t1,t2,P,P1 st P = { p where p is Point of TOP-REAL 2:
