theorem :: JORDAN1:46
     for s1,s2,t1,t2,P,P2 st s1<s2 & t1< t2 &
   P = { p where p is Point of TOP-REAL 2:
                        p`1 = s1 & p`2 <= t2 & p`2 >= t1 or
                        p`1 <= s2 & p`1 >= s1 & p`2 = t2 or
                        p`1 <= s2 & p`1 >= s1 & p`2 = t1 or
