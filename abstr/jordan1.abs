reserve s1a,t1a,s2a,t2a,s3a,t3a,sb,tb,sc,tc for Real;

canceled 2;

theorem :: JORDAN1:12
{|[ s,t ]|: s1<s & s<s2 & t1<t & t<t2} =
{|[ s3,t3 ]|:s1<s3} /\ {|[s4,t4]|:s4<s2} /\ {|[s5,t5]|:t1<t5} /\
 {|[s6,t6]|:t6<t2};

theorem :: JORDAN1:13
{|[ s,t ]|:not (s1<=s & s<=s2 & t1<=t & t<=t2)} =
{|[ s3,t3 ]|:s3<s1} \/ {|[s4,t4]|:t4<t1} \/ {|[s5,t5]|:s2<s5} \/
 {|[s6,t6]|:t2<t6};

