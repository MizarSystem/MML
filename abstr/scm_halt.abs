  InitHalting Program of SCM+FSA;
  cluster I ';' j -> InitHalting keepInt0_1;
end;

registration
  let i be keeping_0 parahalting Instruction of SCM+FSA, J be keepInt0_1
  InitHalting Program of SCM+FSA;
  cluster i ';' J -> InitHalting keepInt0_1;
end;

registration
