theorem :: FUZZY_2:22
for c holds max(min(f,g),min(f,1_minus g)).c <= f.c;

theorem :: FUZZY_2:23
for c holds max(f,min(g,1_minus f)).c <= max(f,g).c;

theorem :: FUZZY_2:24
   A \/ (B\A) c= A \/ B;

theorem :: FUZZY_2:25
