for c holds (min(f,g)*min(f,h)).c <= min(f,(g*h)).c;

theorem :: FUZZY_2:104
   (A \/ B)*(A \/ D) c= A \/ (B*D) & (A /\ B)*(A /\ D) c= A /\ (B*D);

theorem :: FUZZY_2:105
for c be Element of C,f,g be Membership_Func of C holds
(f ++ g).c = 1 - ((1 - f.c)*(1 - g.c));

