theorem :: FUZZY_2:94
   for a,b,c be Real st 0 <= c holds
c*max(a,b) = max(c*a,c*b) & c*min(a,b) = min(c*a,c*b);

theorem :: FUZZY_2:95
   for a,b,c be Real holds
c + max(a,b) = max(c+a,c+b) & c + min(a,b) = min(c+a,c+b);

theorem :: FUZZY_2:96
f*max(g,h) = max(f*g,f*h);

theorem :: FUZZY_2:97
f*min(g,h) = min(f*g,f*h);

theorem :: FUZZY_2:98
   A*(B /\ D) = (A*B) /\ (A*D) & A*(B \/ D) = (A*B) \/ (A*D);

theorem :: FUZZY_2:99
f ++ max(g,h) = max((f ++ g),(f ++ h));

theorem :: FUZZY_2:100
