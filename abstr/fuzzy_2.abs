theorem :: FUZZY_2:14
for c holds min(f,1_minus g).c <= f.c;

theorem :: FUZZY_2:15
   A\B c= A;

theorem :: FUZZY_2:16
for c holds min(f,1_minus g).c <= max(min(f,1_minus(g)),min(1_minus(f),g)).c;

theorem :: FUZZY_2:17
