theorem :: FUZZY_2:26
min(f,1_minus min(g,1_minus h)) = max(min(f,1_minus g),min(f,h));

theorem :: FUZZY_2:27
   A\(B\D) = (A\B) \/ (A /\ D);

theorem :: FUZZY_2:28
for c holds min(f,g).c <= min(f,1_minus(min(f,1_minus g))).c;

theorem :: FUZZY_2:29
