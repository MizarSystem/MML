for c holds
min(max(f,g),1_minus min(f,g)).c >= max(min(f,1_minus g),min(1_minus f,g)).c;

theorem :: FUZZY_2:48
   A \+\ B c= (A \/ B)\(A /\ B);

theorem :: FUZZY_2:49
for c holds
max(min(f,g),1_minus max(f,g)).c
<= (1_minus max(min(f,1_minus g),min(1_minus f,g))).c;

