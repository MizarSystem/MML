for c holds min(f,1_minus g).c <= min(max(f,g),1_minus g).c;

theorem :: FUZZY_2:31
   A\B c= (A \/ B)\B;

theorem :: FUZZY_2:32
min(f,1_minus max(g,h)) = min(min(f,1_minus g),min(f,1_minus h));

theorem :: FUZZY_2:33
   A\(B \/ D) = (A\B) /\ (A\D);

theorem :: FUZZY_2:34
min(f,1_minus min(g,h)) = max(min(f,1_minus g),min(f,1_minus h));

theorem :: FUZZY_2:35
   A\(B /\ D) = (A\B) \/ (A\D);

