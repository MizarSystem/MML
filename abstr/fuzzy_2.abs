theorem :: FUZZY_2:101
   A ++ (B \/ D) = (A ++ B) \/ (A ++ D) & A ++ (B /\ D) = (A ++ B) /\ (A ++ D);

theorem :: FUZZY_2:102
for c holds (max(f,g)*max(f,h)).c <= max(f,(g*h)).c;

