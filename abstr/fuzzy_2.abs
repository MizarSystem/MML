min(min(f,1_minus g),1_minus h) = min(f,1_minus max(g,h));

theorem :: FUZZY_2:37
   (A\B)\D = A\(B \/ D);

theorem :: FUZZY_2:38
for c holds
min(max(f,g),1_minus min(f,g)).c >= max(min(f,1_minus g),min(g,1_minus f)).c;

theorem :: FUZZY_2:39
   (A\B) \/ (B\A) c= (A \/ B)\(A /\ B);

theorem :: FUZZY_2:40
min(max(f,g),1_minus h) = max(min(f,1_minus h),min(g,1_minus h));

theorem :: FUZZY_2:41
   (A \/ B)\D = (A\D) \/ (B\D);

