begin :: Preliminaries

 reserve n, k, r, m, i, j for Element of NAT;

theorem :: FIB_NUM2:1
   for n being non empty Element of NAT holds
    n -' 1 + 2 = n + 1;

theorem :: FIB_NUM2:2
  for n being odd Integer,
      m being non empty real number holds
    (-m) to_power n = -(m to_power n);

theorem :: FIB_NUM2:3
  for n being odd Integer holds
    (-1) to_power n = -1;

theorem :: FIB_NUM2:4
  for n being even Integer,
      m being non empty real number holds
    (-m) to_power n = m to_power n;

theorem :: FIB_NUM2:5
  for n being even Integer holds
    (-1) to_power n = 1;

theorem :: FIB_NUM2:6
  for m being non empty real number, n being Integer holds
    ((-1) * m) to_power n = ((-1) to_power n) * (m to_power n);

theorem :: FIB_NUM2:7
  for a being non empty real number holds
    a to_power (k+m) = (a to_power k) * (a to_power m);

theorem :: FIB_NUM2:8
  for k being non empty real number,
      m being odd Integer holds
    k to_power m to_power n = k to_power (m * n);

theorem :: FIB_NUM2:9
  ((-1) to_power (-n)) ^2 = 1;

theorem :: FIB_NUM2:10
  for a being non empty real number holds
    (a to_power (-k)) * (a to_power (-m)) = a to_power (-k-m);

theorem :: FIB_NUM2:11
  (-1) to_power (-2 * n) = 1;

theorem :: FIB_NUM2:12
  for a being non empty real number holds
    (a to_power k) * (a to_power (-k)) = 1;

registration let n be odd Integer;
  cluster -n -> odd;
end;

registration let n be even Integer;
  cluster -n -> even;
end;

theorem :: FIB_NUM2:13
  (-1) to_power (-n) = (-1) to_power n;

theorem :: FIB_NUM2:14
  for k, m, m1, n1 being Element of NAT st k divides m & k divides n holds
    k divides m * m1 + n * n1;

registration
  cluster finite non empty natural-membered with_non-empty_elements set;
end;

registration let f be Function of NAT, NAT;
             let A be finite natural-membered with_non-empty_elements set;
  cluster f | A -> FinSubsequence-like;
end;

theorem :: FIB_NUM2:15
  for p being FinSubsequence holds
    rng Seq p c= rng p;

definition let f be Function of NAT, NAT;
           let A be finite with_non-empty_elements natural-membered set;
  func Prefix (f, A) -> FinSequence of NAT equals
:: FIB_NUM2:def 1
      Seq (f | A);
end;

theorem :: FIB_NUM2:16
  for k being Element of NAT st k <> 0 holds
    k + m <= n implies m < n;

registration
  cluster NAT -> bounded_below;
end;

theorem :: FIB_NUM2:17
  for x, y being set st 0 < i & i < j holds {[i,x], [j,y]} is FinSubsequence;

theorem :: FIB_NUM2:18
  for x, y being set,
      q being FinSubsequence st i < j & q = {[i,x], [j,y]} holds
    Seq q = <*x,y*>;

registration let n be Element of NAT;
  cluster Seg n -> with_non-empty_elements;
end;

registration let A be with_non-empty_elements set;
  cluster -> with_non-empty_elements Subset of A;
end;

registration let A be with_non-empty_elements set;
             let B be set;
  cluster A /\ B -> with_non-empty_elements;
  cluster B /\ A -> with_non-empty_elements;
end;

theorem :: FIB_NUM2:19
  for k being Element of NAT, a being set st k >= 1 holds
    {[k, a]} is FinSubsequence;

theorem :: FIB_NUM2:20
  for i being Element of NAT,
      y being set,
      f being FinSubsequence st f = { [1,y] } holds
    i Shift f = { [1+i,y] };

theorem :: FIB_NUM2:21
  for q being FinSubsequence,
      k, n being Element of NAT st dom q c= Seg k & n > k
   ex p being FinSequence st q c= p & dom p = Seg n;

theorem :: FIB_NUM2:22
  for q being FinSubsequence holds
    ex p being FinSequence st q c= p;

begin :: Fibonacci Numbers

scheme :: FIB_NUM2:sch 1
 FibInd1 {P[set] } :
  for k being non empty Element of NAT holds P[k] provided
  P[1] and
  P[2] and
  for k being non empty Element of NAT st P[k] & P[k+1] holds P[k+2];

scheme :: FIB_NUM2:sch 2
 FibInd2 {P[set] } :
  for k be non trivial Element of NAT holds P[k] provided
  P[2] and
  P[3] and
  for k be non trivial Element of NAT st P[k] & P[k+1] holds P[k+2];

theorem :: FIB_NUM2:23
  Fib (2) = 1;

theorem :: FIB_NUM2:24
  Fib (3) = 2;

theorem :: FIB_NUM2:25
  Fib (4) = 3;

theorem :: FIB_NUM2:26
  Fib (n + 2) = Fib (n) + Fib (n + 1);

theorem :: FIB_NUM2:27
  Fib (n + 3) = Fib (n + 2) + Fib (n + 1);

theorem :: FIB_NUM2:28
  Fib (n + 4) = Fib (n + 2) + Fib (n + 3);

theorem :: FIB_NUM2:29
  Fib (n + 5) = Fib (n + 3) + Fib (n + 4);

theorem :: FIB_NUM2:30
  Fib (n + 2) = Fib (n + 3) - Fib (n + 1);

theorem :: FIB_NUM2:31
  Fib (n + 1) = Fib (n + 2) - Fib (n);

theorem :: FIB_NUM2:32
  Fib (n) = Fib (n+2) - Fib (n+1);

begin :: Cassini's and Catalan's Identities

theorem :: FIB_NUM2:33
  Fib (n) * Fib (n+2) - (Fib (n+1)) ^2 = (-1) |^ (n+1);

theorem :: FIB_NUM2:34
   for n being non empty Element of NAT holds
    Fib (n-'1) * Fib (n+1) - (Fib (n)) ^2 = (-1) |^n;

theorem :: FIB_NUM2:35
  tau > 0;

theorem :: FIB_NUM2:36
  tau_bar = (- tau) to_power (-1);

theorem :: FIB_NUM2:37
  (-tau) to_power ((-1) * n) = (-tau) to_power (-1) to_power n;

theorem :: FIB_NUM2:38
  - 1 / tau = tau_bar;

theorem :: FIB_NUM2:39
  (tau to_power r) ^2 - 2 * ((-1) to_power r) + (tau to_power (-r)) ^2 =
    ((tau to_power r) - (tau_bar to_power r)) ^2;

theorem :: FIB_NUM2:40
   for n,r being non empty Element of NAT st r <= n holds
    (Fib (n)) ^2 - Fib (n+r) * Fib (n-'r) = ((-1) |^(n-'r)) * (Fib (r)) ^2;

theorem :: FIB_NUM2:41
   (Fib (n)) ^2 + (Fib (n+1)) ^2 = Fib (2*n + 1);

theorem :: FIB_NUM2:42
  for k being non empty Element of NAT holds
    Fib (n+k) = Fib (k) * Fib (n+1) + Fib (k-'1) * Fib (n);

theorem :: FIB_NUM2:43
  for n being non empty Element of NAT holds
    Fib (n) divides Fib (n*k);

theorem :: FIB_NUM2:44
  for k being non empty Element of NAT holds
    k divides n implies Fib (k) divides Fib (n);

theorem :: FIB_NUM2:45
  Fib (n) <= Fib (n + 1);

theorem :: FIB_NUM2:46
  for n being Element of NAT st n > 1 holds
    Fib (n) < Fib (n+1);

theorem :: FIB_NUM2:47
   for m, n being Element of NAT st m >= n holds Fib(m) >= Fib(n);

theorem :: FIB_NUM2:48
  for k being Element of NAT st k > 1 holds
    k < n implies Fib (k) < Fib (n);

theorem :: FIB_NUM2:49
  Fib (k) = 1 iff k = 1 or k = 2;

theorem :: FIB_NUM2:50
  for k,n being Element of NAT st n > 1 & k <> 0 & k <> 1 &
    ((k <> 1 & n <> 2) or (k <> 2 & n <> 1)) holds
      Fib (k) = Fib (n) iff k = n;

theorem :: FIB_NUM2:51
  for n being Element of NAT st n > 1 & n <> 4 holds
  n is non prime implies ex k being non empty Element of NAT st
    k <> 1 & k <> 2 & k <> n & k divides n;

theorem :: FIB_NUM2:52
   for n being Element of NAT st n > 1 & n <> 4 holds
    Fib (n) is prime implies n is prime;

begin :: Sequence of Fibonacci Numbers

definition
  func FIB -> Function of NAT, NAT means
:: FIB_NUM2:def 2
    for k being Element of NAT holds it.k = Fib(k);
end;

definition
  func EvenNAT -> Subset of NAT equals
:: FIB_NUM2:def 3
     { 2 * k where k is Element of NAT : not contradiction };
   func OddNAT -> Subset of NAT equals
:: FIB_NUM2:def 4
      { 2 * k + 1 where k is Element of NAT : not contradiction };
 end;

theorem :: FIB_NUM2:53
  for k being Element of NAT holds 2 * k in EvenNAT & not 2 * k + 1 in EvenNAT;

theorem :: FIB_NUM2:54
  for k being Element of NAT holds 2 * k + 1 in OddNAT & not 2 * k in OddNAT;

definition let n be Element of NAT;
  func EvenFibs (n) -> FinSequence of NAT equals
:: FIB_NUM2:def 5
     Prefix (FIB, EvenNAT /\ Seg n);
  func OddFibs (n) -> FinSequence of NAT equals
:: FIB_NUM2:def 6
     Prefix (FIB, OddNAT /\ Seg n);
end;

theorem :: FIB_NUM2:55
  EvenFibs (0) = {};

theorem :: FIB_NUM2:56
   Seq (FIB | {2}) = <*1*>;

theorem :: FIB_NUM2:57
  EvenFibs (2) = <*1*>;

theorem :: FIB_NUM2:58
   EvenFibs (4) = <*1,3*>;

theorem :: FIB_NUM2:59
  for k being Element of NAT holds
    (EvenNAT /\ Seg (2 * k + 2)) \/ {2 * k + 4} = EvenNAT /\ Seg (2 * k + 4);

theorem :: FIB_NUM2:60
  for k being Element of NAT holds
    FIB | (EvenNAT /\ Seg (2 * k + 2)) \/ {[2*k+4,FIB.(2 * k + 4)]} =
    FIB | (EvenNAT /\ Seg (2 * k + 4));

theorem :: FIB_NUM2:61
  for n being Element of NAT holds
    EvenFibs (2 * n + 2) = EvenFibs (2 * n) ^ <* Fib (2 * n + 2) *>;

theorem :: FIB_NUM2:62
  OddFibs (1) = <*1*>;

theorem :: FIB_NUM2:63
  OddFibs (3) = <*1,2*>;

theorem :: FIB_NUM2:64
  for k being Element of NAT holds
    OddNAT /\ Seg (2 * k + 3) \/ {2 * k + 5} = OddNAT /\ Seg (2 * k + 5);

theorem :: FIB_NUM2:65
  for k being Element of NAT holds
    (FIB | (OddNAT /\ Seg (2 * k + 3))) \/ {[2*k+5,FIB.(2 * k + 5)]} =
      (FIB | (OddNAT /\ Seg (2 * k + 5)));

theorem :: FIB_NUM2:66
  for n being Element of NAT holds
    OddFibs (2 * n + 3) = OddFibs (2 * n + 1) ^ <* Fib (2 * n + 3) *>;

theorem :: FIB_NUM2:67
   for n being Element of NAT holds
    Sum EvenFibs (2 * n + 2) = Fib (2 * n + 3) - 1;

theorem :: FIB_NUM2:68
   for n being Element of NAT holds
    Sum OddFibs (2 * n + 1) = Fib (2 * n + 2);

begin :: Carmichael's Theorem on Prime Divisors

theorem :: FIB_NUM2:69
  for n being Element of NAT holds Fib (n), Fib (n+1) are_relative_prime;

theorem :: FIB_NUM2:70
  for n being non empty Element of NAT, m being Element of NAT st m <> 1 holds
    m divides Fib (n) implies not m divides Fib (n-'1);

theorem :: FIB_NUM2:71
   for n being non empty Element of NAT holds
    m is prime & n is prime & m divides Fib (n) implies
      for r being Element of NAT st r < n & r <> 0 holds not m divides Fib (r);

begin :: Fibonacci Numbers and Pythagorean Triples

theorem :: FIB_NUM2:72
    for n being non empty Element of NAT holds
     {Fib (n) * Fib (n+3), 2 * Fib (n+1) * Fib (n+2),
       (Fib (n+1)) ^2 + (Fib (n+2)) ^2} is Pythagorean_triple;
