definition
  let X1,X2,X3,X4,X5,X6;
  assume
 X1<>{} & X2<>{} & X3<>{} & X4<>{} & X5<>{} & X6<>{};
  let x be Element of [:X1,X2,X3,X4,X5,X6:];
  func x`1 -> Element of X1 means
