theorem :: JORDAN6:22
  for P being Subset of TOP-REAL 2,
  p1,p2 being Point of TOP-REAL 2 st P is_an_arc_of p1,p2 holds
  L_Segment(P,p1,p2,p1)={p1};

canceled 2;

theorem :: JORDAN6:25
  for P being Subset of TOP-REAL 2,
  p1,p2 being Point of TOP-REAL 2 st P is_an_arc_of p1,p2 holds
  L_Segment(P,p1,p2,p2)=P;

theorem :: JORDAN6:26
  for P being Subset of TOP-REAL 2,
